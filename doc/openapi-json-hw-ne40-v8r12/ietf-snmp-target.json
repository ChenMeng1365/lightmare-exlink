    "definitions": {
        "snmp_schema": {
            "description": "Top-level container for SNMP-related configuration and\nstatus objects.",
            "properties": {
                "target": {
                    "description": "List of targets.",
                    "type": "array",
                    "items": {
                        "properties": {
                            "name": {
                                "type": "string",
                                "description": "Identifies the target."
                            },
                            "transport": {
                                "description": "Transport address of the target.\nThe snmpTargetAddrTDomain and snmpTargetAddrTAddress\nobjects are mapped to transport-specific YANG nodes.  Each\ntransport is configured as a separate case in this\nchoice.  Submodules providing configuration for additional\ntransports are expected to augment this choice.",
                                "properties": {
                                    "udp": {
                                        "properties": {
                                            "udp": {
                                                "properties": {
                                                    "ip": {
                                                        "type": "string"
                                                    },
                                                    "port": {
                                                        "type": "string",
                                                        "description": "UDP port number."
                                                    },
                                                    "prefix-length": {
                                                        "type": "string",
                                                        "description": "The value of this leaf must match the value of\n../snmp:ip.  If ../snmp:ip contains an IPv4 address,\nthis leaf must be less than or equal to 32.  If it\ncontains an IPv6 address, it must be less than or\nequal to 128.\nNote that the prefix-length is currently only used\nby the Community-based Security Model to filter\nincoming messages.  Furthermore, the prefix-length\nfiltering does not cover all possible filters\nsupported by the corresponding MIB object."
                                                    }
                                                }
                                            }
                                        }
                                    }
                                }
                            },
                            "tag": {
                                "type": "array",
                                "items": {
                                    "type": "string",
                                    "description": "List of tag values used to select target addresses."
                                }
                            },
                            "timeout": {
                                "type": "string",
                                "description": "Needed only if this target can receive\nInformRequest-PDUs."
                            },
                            "retries": {
                                "type": "string",
                                "description": "Needed only if this target can receive\nInformRequest-PDUs."
                            },
                            "target-params": {
                                "type": "string"
                            }
                        }
                    },
