  <augment target-node="/qos:qos/qos:global-config">
    <description>
      <text>Global acl enable.</text>
    </description>
    <container name="local-id-enable">
      <presence value="create qos-local-id-enable"/>
      <description>
        <text>Enable/disable QoS-local-id.</text>
      </description>
      <leaf name="mode">
        <type name="traffic-mode"/>
        <mandatory value="true"/>
        <description>
          <text>The mode of traffic policy QoS-local-id enable.</text>
        </description>
      </leaf>
    </container>
  </augment>
  <augment target-node="/ifm:ifm/ifm:interfaces/ifm:interface/qos:qos">
    <description>
      <text>QPPB.</text>
    </description>
    <container name="local-id-enable">
      <description>
        <text>Configure QoS-local-id enable.</text>
      </description>
      <leaf name="mode">
        <type name="qppb-mode"/>
        <must condition="not(../../qppb:qppb/qppb:policys/qppb:policy[qppb:direction='inbound'])"/>
        <description>
          <text>The mode of traffic policy and QPPB QoS-local-id enable.</text>
        </description>
      </leaf>
      <leaf name="both">
        <type name="empty"/>
        <description>
          <text>The mode of traffic policy QoS-local-id both enable. Both has a higher priority than mode.</text>
        </description>
      </leaf>
    </container>
    <container name="qppb">
      <description>
        <text>Configure QPPB.</text>
      </description>
      <container name="policys">
        <description>
          <text>List of QPPB policies application.</text>
        </description>
        <list name="policy">
          <must condition="(./direction='inbound' and ./policy-name and ./mode and not(../../../local-id-enable/mode)) or (./direction='outbound' and ./policy-name and not(./mode))"/>
          <key value="direction"/>
          <description>
            <text>Configure application of QPPB policy.</text>
          </description>
          <leaf name="direction">
            <type name="qppb-direction-type"/>
            <description>
              <text>QPPB policy direction.</text>
            </description>
          </leaf>
          <leaf name="policy-name">
            <type name="leafref">
              <path value="/qos:qos/qppb:qppb/qppb:local-policys/qppb:local-policy/qppb:policy-name"/>
            </type>
            <mandatory value="true"/>
            <description>
              <text>QPPB local policy name.</text>
            </description>
          </leaf>
          <leaf name="mode">
            <when condition="../direction='inbound'"/>
            <type name="qppb-mode"/>
            <mandatory value="true"/>
            <description>
              <text>QPPB policy mode.</text>
            </description>
          </leaf>
          <leaf name="share-mode">
            <type name="boolean"/>
            <default value="true"/>
            <description>
              <text>Enable/disable share mode.</text>
            </description>
          </leaf>
        </list>
      </container>
      <container name="ip-precedence">
        <presence value="create ip-precedence"/>
        <description>
          <text>Enable/disable QPPB policy ip-precedence.</text>
        </description>
        <leaf name="mode">
          <type name="qppb-mode"/>
          <mandatory value="true"/>
          <description>
            <text>QPPB policy mode.</text>
          </description>
        </leaf>
      </container>
    </container>
  </augment>
  <ext:task-name value="qos"/>
</module>
