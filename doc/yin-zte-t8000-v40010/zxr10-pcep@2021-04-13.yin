  <container name="configuration">
    <description>
      <text>The branch of configuration model.</text>
    </description>
    <container name="pcep">
      <description>
        <text>The configuration of PCEP(Path Compute Element Protocol).</text>
      </description>
      <leaf name="pcep-enable">
        <description>
          <text>The global switch of PCEP.</text>
        </description>
        <type name="enumeration">
          <enum name="enable">
            <value value="1"/>
            <description>
              <text>The PCEP is enable.</text>
            </description>
          </enum>
          <enum name="disable">
            <value value="0"/>
            <description>
              <text>The PCEP is disable.</text>
            </description>
          </enum>
        </type>
        <default value="disable"/>
      </leaf>
      <leaf name="router-id">
        <description>
          <text>The PCEP IPv4 router ID configuration.</text>
        </description>
        <type name="ncd:ipv4-address">
          <pattern value="([1-9]|[1-9][0-9]|1[0-9][0-9]|2[0-4][0-9]|25[0-5])(.([0-9]|[1-9][0-9]|1[0-9][0-9]|2[0-4][0-9]|25[0-5])){3}">
            <description>
              <text>To filter the invalid address.</text>
            </description>
          </pattern>
        </type>
      </leaf>
      <leaf name="router-id-v6">
        <description>
          <text>The PCEP IPv6 route ID configuration.</text>
        </description>
        <type name="ncd:ipv6-address">
          <pattern value="([0-9a-fA-F]{1,4}:){7}([0-9a-fA-F]{0,4})$|^([0-9a-fA-F]{1,4}:){1,6}(:[0-9a-fA-F]{0,4}){1,6}$|^([:]{2})(([0-9a-fA-F]{1,4}:){0,6})([0-9a-fA-F]{1,4})$|^([0-9a-fA-F]{1,4}:){6}([0-9]{1,3}.){3}([0-9]{1,3})$|^([0-9a-fA-F]{1,4}:){1,5}(:[0-9a-fA-F]{0,4}){1,5}([0-9]{1,3}.){3}([0-9]{1,3})$|^([:]{2})(([0-9a-fA-F]{1,4}:){0,5})([0-9]{1,3}.){3}([0-9]{1,3})"/>
        </type>
        <ncd:revise-date date="2018-11-23"/>
      </leaf>
      <leaf name="role">
        <description>
          <text>The role of PCEP.</text>
        </description>
        <type name="enumeration">
          <enum name="pcc">
            <value value="1"/>
            <description>
              <text>The role is PCC.</text>
            </description>
          </enum>
          <enum name="pce">
            <value value="2"/>
            <description>
              <text>The role is PCE.</text>
            </description>
            <status value="obsolete"/>
          </enum>
          <enum name="invalid">
            <value value="4"/>
            <description>
              <text>The role is invalid.</text>
            </description>
          </enum>
        </type>
        <default value="invalid"/>
      </leaf>
      <leaf name="vrf-name">
        <description>
          <text>Configure VRF name</text>
        </description>
        <type name="string">
          <length value="1..32"/>
        </type>
        <ncd:revise-date date="2019-03-07"/>
      </leaf>
      <container name="pcc">
        <description>
          <text>The configuration of PCC(Path Computation Client).</text>
        </description>
        <leaf name="pcc-enable">
          <description>
            <text>Enable the PCC.</text>
          </description>
          <type name="enumeration">
            <enum name="disable">
              <value value="0"/>
              <description>
                <text>The PCC is disable.</text>
              </description>
            </enum>
            <enum name="enable">
              <value value="1"/>
              <description>
                <text>The PCC is enable.</text>
              </description>
            </enum>
          </type>
          <default value="disable"/>
        </leaf>
        <container name="peers">
          <description>
            <text>PCC Peer Configuration.</text>
          </description>
          <list name="peer">
            <description>
              <text>The peer configuration list of PCC.</text>
            </description>
            <max-elements value="65535"/>
            <key value="peer-id"/>
            <leaf name="peer-id">
              <description>
                <text>The instance id of peer.</text>
              </description>
              <type name="uint16">
                <range value="1..65535"/>
              </type>
            </leaf>
            <choice name="peer-router-id-type">
              <case name="ipv4">
                <description>
                  <text>IPv4 type</text>
                </description>
                <leaf name="peer-router-id">
                  <description>
                    <text>The IPv4 router id of peer.</text>
                  </description>
                  <type name="ncd:ipv4-address">
                    <pattern value="([1-9]|[1-9][0-9]|1[0-9][0-9]|2[0-4][0-9]|25[0-5])(.([0-9]|[1-9][0-9]|1[0-9][0-9]|2[0-4][0-9]|25[0-5])){3}">
                      <description>
                        <text>To filter the invalid address.</text>
                      </description>
                    </pattern>
                  </type>
                </leaf>
              </case>
              <case name="ipv6">
                <description>
                  <text>IPv6 type</text>
                </description>
                <leaf name="peer-router-id-v6">
                  <description>
                    <text>The IPv6 router id of peer.</text>
                  </description>
                  <type name="ncd:ipv6-address">
                    <pattern value="([0-9a-fA-F]{1,4}:){7}([0-9a-fA-F]{0,4})$|^([0-9a-fA-F]{1,4}:){1,6}(:[0-9a-fA-F]{0,4}){1,6}$|^([:]{2})(([0-9a-fA-F]{1,4}:){0,6})([0-9a-fA-F]{1,4})$|^([0-9a-fA-F]{1,4}:){6}([0-9]{1,3}.){3}([0-9]{1,3})$|^([0-9a-fA-F]{1,4}:){1,5}(:[0-9a-fA-F]{0,4}){1,5}([0-9]{1,3}.){3}([0-9]{1,3})$|^([:]{2})(([0-9a-fA-F]{1,4}:){0,5})([0-9]{1,3}.){3}([0-9]{1,3})"/>
                  </type>
                </leaf>
                <ncd:revise-date date="2018-11-23"/>
              </case>
              <case name="peer-router-id-type-choice-clear">
                <description>
                  <text>Remove value of any entities which belong to any case within this choice.</text>
                </description>
                <container name="peer-router-id-type-all-clear">
                  <description>
                    <text>Remove this entitiy means to remove value of any entities which belong to any case within this choice.</text>
                  </description>
                  <presence value="peer-router-id-type-all-clear"/>
                </container>
                <ncd:revise-date date="2020-07-03"/>
              </case>
            </choice>
            <leaf name="master-ship">
              <description>
                <text>The master ship of peer.</text>
              </description>
              <type name="enumeration">
                <enum name="master">
                  <value value="1"/>
                  <description>
                    <text>The peer is master.</text>
                  </description>
                </enum>
                <enum name="slave">
                  <value value="2"/>
                  <description>
                    <text>The peer is slave.</text>
                  </description>
                </enum>
              </type>
              <default value="master"/>
            </leaf>
            <choice name="slave-peer-router-id-type">
              <case name="ipv4">
                <description>
                  <text>IPv4 type</text>
                </description>
                <leaf name="slave-peer-router-id">
                  <description>
                    <text>The IPv4 router id of slave peer.</text>
                  </description>
                  <type name="ncd:ipv4-address">
                    <pattern value="([1-9]|[1-9][0-9]|1[0-9][0-9]|2[0-4][0-9]|25[0-5])(.([0-9]|[1-9][0-9]|1[0-9][0-9]|2[0-4][0-9]|25[0-5])){3}">
                      <description>
                        <text>To filter the invalid address.</text>
                      </description>
                    </pattern>
                  </type>
                </leaf>
              </case>
              <case name="ipv6">
                <description>
                  <text>IPv6 type</text>
                </description>
                <leaf name="slave-peer-router-id-v6">
                  <description>
                    <text>The IPv6 router id of slave peer.</text>
                  </description>
                  <type name="ncd:ipv6-address">
                    <pattern value="([0-9a-fA-F]{1,4}:){7}([0-9a-fA-F]{0,4})$|^([0-9a-fA-F]{1,4}:){1,6}(:[0-9a-fA-F]{0,4}){1,6}$|^([:]{2})(([0-9a-fA-F]{1,4}:){0,6})([0-9a-fA-F]{1,4})$|^([0-9a-fA-F]{1,4}:){6}([0-9]{1,3}.){3}([0-9]{1,3})$|^([0-9a-fA-F]{1,4}:){1,5}(:[0-9a-fA-F]{0,4}){1,5}([0-9]{1,3}.){3}([0-9]{1,3})$|^([:]{2})(([0-9a-fA-F]{1,4}:){0,5})([0-9]{1,3}.){3}([0-9]{1,3})"/>
                  </type>
                </leaf>
                <ncd:revise-date date="2018-11-23"/>
              </case>
              <case name="slave-peer-router-id-type-choice-clear">
                <description>
                  <text>Remove value of any entities which belong to any case within this choice.</text>
                </description>
                <container name="slave-peer-router-id-type-all-clear">
                  <description>
                    <text>Remove this entitiy means to remove value of any entities which belong to any case within this choice.</text>
                  </description>
                  <presence value="slave-peer-router-id-type-all-clear"/>
                </container>
                <ncd:revise-date date="2020-07-03"/>
              </case>
            </choice>
            <leaf name="keepalive-time">
              <description>
                <text>The interval of keepalive message.</text>
              </description>
              <type name="uint8">
                <range value="0..255"/>
              </type>
              <default value="30"/>
            </leaf>
            <leaf name="dead-time">
              <description>
                <text>The timeout time for the peer.</text>
              </description>
              <type name="uint8">
                <range value="0..255"/>
              </type>
              <default value="120"/>
            </leaf>
            <leaf name="request-response-wait">
              <description>
                <text>The time waiting for the response of request.</text>
              </description>
              <type name="uint32">
                <range value="0..300"/>
              </type>
              <default value="0"/>
            </leaf>
            <leaf name="keepalive-min">
              <description>
                <text>The minimal interval of keepalive message whitch accepted from peer node.</text>
              </description>
              <type name="uint8">
                <range value="0..255"/>
              </type>
              <default value="0"/>
              <ncd:revise-date date="2016-04-26"/>
            </leaf>
            <leaf name="precedence">
              <description>
                <text>The precedence of PCE.</text>
              </description>
              <type name="uint8">
                <range value="0..63"/>
              </type>
              <default value="63"/>
              <ncd:revise-date date="2016-04-26"/>
            </leaf>
            <leaf name="keychain">
              <description>
                <text>Configure keychain name</text>
              </description>
              <type name="string">
                <length value="1..31"/>
              </type>
              <ncd:revise-date date="2018-04-10"/>
            </leaf>
            <leaf name="description">
              <description>
                <text>Configure description</text>
              </description>
              <type name="string">
                <length value="1..80"/>
              </type>
              <ncd:revise-date date="2019-05-17"/>
            </leaf>
            <leaf name="dscp">
              <description>
                <text>Configure dscp</text>
              </description>
              <type name="uint8">
                <range value="0..63"/>
              </type>
              <ncd:revise-date date="2021-03-16"/>
            </leaf>
          </list>
        </container>
        <container name="lsp-names">
          <description>
            <text>Lsp name configuration for te tunnel.</text>
          </description>
          <list name="lsp-name">
            <description>
              <text>The lsp-name configuration list.</text>
            </description>
            <max-elements value="131070"/>
            <key value="te-tunnel-interface"/>
            <leaf name="te-tunnel-interface">
              <description>
                <text>The te tunnel interface.</text>
              </description>
              <type name="ncd:ifname"/>
            </leaf>
            <leaf name="te-lsp-name">
              <description>
                <text>The redefined tunnel name.</text>
              </description>
              <type name="string">
                <length value="1..63"/>
              </type>
              <mandatory value="true"/>
            </leaf>
          </list>
        </container>
        <leaf name="redelegation-time">
          <description>
            <text>LSP redelegation timeout interval.</text>
          </description>
          <type name="uint16">
            <range value="0..3600"/>
