  <feature name="fixed-flag">
    <description>
      <text>This flag identifies fixed mode is or not supported.</text>
    </description>
  </feature>
  <feature name="utc-trace-flag">
    <description>
      <text>This flag identifies the utc-trace is or not supported.</text>
    </description>
  </feature>
  <feature name="nomode-flag">
    <description>
      <text>This flag identifies the port mode is or not supported.</text>
    </description>
  </feature>
  <feature name="free-flag">
    <description>
      <text>This flag identifies free mode is or not supported.</text>
    </description>
  </feature>
  <feature name="time-source-flag">
    <description>
      <text>This flag identifies the time source is or not supported.</text>
    </description>
  </feature>
  <feature name="ip-list-flag">
    <description>
      <text>This flag identifies the IP list is or not supported.</text>
    </description>
  </feature>
  <feature name="free-port-vlan">
    <description>
      <text>This flag identifies the port-vlanid is or not supported.</text>
    </description>
  </feature>
  <feature name="time-source-select"/>
  <feature name="unicast-request-tlv"/>
  <feature name="accepted-slave-ip"/>
