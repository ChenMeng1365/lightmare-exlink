<?xml version="1.0" encoding="UTF-8"?>
<module name="zxr10-pm-flexe"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:pm-flexe="http://www.zte.com.cn/zxr10/netconf/schema/rosng/pm-flexe"
        xmlns:ncd="http://www.zte.com.cn/zxr10/netconf/schema/rosng/netconf-defs">
  <namespace uri="http://www.zte.com.cn/zxr10/netconf/schema/rosng/pm-flexe"/>
  <prefix value="pm-flexe"/>
  <yang-version value="1"/>
  <import module="zxr10-netconf-defs">
    <prefix value="ncd"/>
  </import>
  <organization>
    <text>ZTE Corporation</text>
  </organization>
  <contact>
    <text>ZTE Corporation  
   Nanjing Institute of ZTE Corporation 
   No.68 Zijinghua Rd. Yuhuatai District, Nanjing, China 
   Tel: +86-25-52870000</text>
  </contact>
  <revision date="2020-11-05">
    <description>
      <text>add ptn-config.</text>
    </description>
  </revision>
  <revision date="2020-08-11">
    <description>
      <text>add flexe client minavailrate percent</text>
    </description>
  </revision>
  <revision date="2020-04-27">
    <description>
      <text>add flexe_subclient's timeslot</text>
    </description>
  </revision>
  <revision date="2020-02-17">
    <description>
      <text>add show flexegroup-overhead</text>
    </description>
  </revision>
  <revision date="2019-11-13">
    <description>
      <text>add container flexe_subclients</text>
    </description>
  </revision>
  <revision date="2019-07-30">
    <description>
      <text>add tx-neg-timeout</text>
    </description>
  </revision>
  <revision date="2019-07-24">
    <description>
      <text>The maximum value of group-number is decide by 570687642</text>
    </description>
  </revision>
  <revision date="2019-01-18">
    <description>
      <text>add filter node to flexe_group&amp;flexe_client</text>
    </description>
  </revision>
  <revision date="2018-11-21">
    <description>
      <text>modify length of timeslots</text>
    </description>
  </revision>
  <revision date="2018-03-16">
    <description>
      <text>remove create-if container, add leaf node reply-ca-mode</text>
    </description>
  </revision>
  <revision date="2017-09-30">
    <description>
      <text>create the module</text>
    </description>
  </revision>
  <feature name="ptn-config"/>
  <container name="configuration">
    <container name="flexe_groups">
      <list name="flexe_group">
        <description>
          <text>flexe_group attribute</text>
        </description>
        <key value="ifname"/>
        <leaf name="ifname">
          <description>
            <text>flexegroup interface name</text>
          </description>
          <type name="ncd:ifname"/>
        </leaf>
        <container name="ifattribute">
          <leaf name="group-number">
            <type name="uint32">
              <range value="1..1048575"/>
            </type>
          </leaf>
          <leaf name="receive-mode">
            <type name="enumeration">
              <enum name="auto">
                <value value="1"/>
              </enum>
              <enum name="static">
                <value value="2"/>
              </enum>
            </type>
          </leaf>
          <leaf name="skew">
            <type name="uint32">
              <range value="1..1000"/>
            </type>
          </leaf>
          <leaf name="alarm-insertion">
            <type name="enumeration">
              <enum name="standard">
                <value value="1"/>
              </enum>
              <enum name="extended">
                <value value="2"/>
              </enum>
            </type>
          </leaf>
          <leaf name="rx-calendar">
            <type name="enumeration">
              <enum name="A">
                <value value="1"/>
              </enum>
              <enum name="B">
                <value value="2"/>
              </enum>
            </type>
          </leaf>
          <leaf name="tx-calendar">
            <type name="enumeration">
              <enum name="A">
                <value value="1"/>
              </enum>
              <enum name="B">
                <value value="2"/>
              </enum>
            </type>
          </leaf>
          <leaf name="tx-calendar-neg">
            <type name="enumeration">
              <enum name="manual">
                <value value="1"/>
              </enum>
              <enum name="protocol-force">
                <value value="2"/>
              </enum>
              <enum name="protocol-normal">
                <value value="3"/>
              </enum>
            </type>
          </leaf>
          <leaf name="reply-ca-mode">
            <type name="enumeration">
              <enum name="never">
                <value value="1"/>
                <description>
                  <text>never reply ca</text>
                </description>
              </enum>
              <enum name="immediately">
                <value value="2"/>
                <description>
                  <text>immediately reply ca</text>
                </description>
              </enum>
              <enum name="ask-controller">
                <value value="3"/>
                <description>
                  <text>ask-controller if need reply ca</text>
                </description>
              </enum>
            </type>
          </leaf>
          <leaf name="tx-calendar-neg-timeout">
            <type name="uint32">
              <range value="10..600000"/>
            </type>
            <ncd:revise-date date="2019-07-30"/>
          </leaf>
          <ncd:revise-date date="2018-03-16"/>
        </container>
        <container name="bind-timeslots">
          <list name="bind">
            <key value="phy-interface"/>
            <leaf name="phy-interface">
              <type name="string"/>
            </leaf>
            <leaf name="phy-number">
              <type name="uint32">
                <range value="1..254"/>
              </type>
              <mandatory value="true"/>
            </leaf>
            <leaf name="timeslots">
              <type name="ncd:range"/>
            </leaf>
            <leaf name="timeslots-all">
              <type name="boolean"/>
            </leaf>
          </list>
        </container>
      </list>
    </container>
    <container name="flexe_clients">
      <list name="flexe_client">
        <description>
          <text>flexe_client attribute</text>
        </description>
        <key value="ifname"/>
        <leaf name="ifname">
          <description>
            <text>flexe_client interfacename</text>
          </description>
          <type name="ncd:ifname"/>
        </leaf>
        <container name="ifattribute">
          <leaf name="client-number">
            <type name="uint32">
              <range value="1..65534"/>
            </type>
          </leaf>
          <leaf name="terminal">
            <type name="enumeration">
              <enum name="enable">
                <value value="1"/>
              </enum>
              <enum name="disable">
                <value value="2"/>
              </enum>
            </type>
          </leaf>
          <leaf name="subclient-enable">
            <type name="enumeration">
              <enum name="enable">
                <value value="1"/>
              </enum>
              <enum name="disable">
                <value value="0"/>
              </enum>
            </type>
            <ncd:revise-date date="2019-11-13"/>
          </leaf>
          <leaf name="minavailrate">
            <type name="uint8">
              <range value="0..100"/>
            </type>
            <ncd:revise-date date="2020-08-11"/>
          </leaf>
          <ncd:revise-date date="2018-03-16"/>
        </container>
        <container name="bind-timeslots">
          <list name="bind">
            <key value="calendar phy-interface"/>
            <leaf name="calendar">
              <type name="enumeration">
                <enum name="A">
                  <value value="1"/>
                </enum>
                <enum name="B">
                  <value value="2"/>
                </enum>
              </type>
            </leaf>
            <leaf name="phy-interface">
              <type name="string"/>
            </leaf>
            <leaf name="flexe_group">
              <type name="string"/>
            </leaf>
            <leaf name="timeslots">
              <type name="ncd:range"/>
              <mandatory value="true"/>
            </leaf>
          </list>
        </container>
        <container name="background">
          <leaf name="flexe_group-ifname">
            <type name="string"/>
          </leaf>
        </container>
      </list>
    </container>
    <container name="veis">
      <list name="vei">
        <description>
          <text>vei bind flexe_client</text>
        </description>
        <key value="ifname"/>
        <leaf name="ifname">
          <type name="string"/>
        </leaf>
        <container name="bind">
          <leaf name="flexe_client-ifname">
            <type name="string"/>
          </leaf>
          <ncd:revise-date date="2018-03-16"/>
        </container>
      </list>
    </container>
    <container name="flexe_subclients">
      <list name="flexe_subclient">
        <key value="ifname"/>
        <leaf name="ifname">
          <description>
            <text>flexe_subclient ifname</text>
          </description>
          <type name="ncd:ifname"/>
        </leaf>
        <container name="ifattribute">
          <leaf name="subclient-number">
            <type name="uint32">
              <range value="1..4000"/>
            </type>
          </leaf>
          <leaf name="terminal">
            <type name="enumeration">
              <enum name="enable">
                <value value="1"/>
              </enum>
              <enum name="disable">
                <value value="2"/>
              </enum>
            </type>
          </leaf>
        </container>
        <container name="bind-client">
          <leaf name="parent-client">
            <type name="string"/>
          </leaf>
          <choice name="timeslot">
            <case name="bandwidth">
              <leaf name="bandwidth">
                <type name="uint32">
                  <range value="1..800"/>
                </type>
              </leaf>
            </case>
            <case name="cells">
              <leaf name="timeslots">
                <type name="ncd:range"/>
              </leaf>
            </case>
            <ncd:revise-date date="2020-04-27"/>
          </choice>
        </container>
      </list>
      <ncd:revise-date date="2019-11-13"/>
    </container>
  </container>
  <container name="state">
    <config value="false"/>
    <container name="flexegroup_ohs">
      <list name="flexegroup_oh">
        <key value="ifname"/>
        <leaf name="ifname">
          <description>
            <text>flexegroup interface name</text>
          </description>
          <type name="ncd:ifname"/>
        </leaf>
        <leaf name="rx-calendar-bit">
          <description>
            <text>rx calendar</text>
          </description>
          <type name="string"/>
        </leaf>
      </list>
    </container>
    <ncd:revise-date date="2020-02-17"/>
  </container>
</module>
