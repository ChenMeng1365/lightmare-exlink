  <grouping name="algorithm-id">
    <description>
      <text>Algorithm ID.</text>
    </description>
    <leaf name="algorithm-id">
      <type name="uint8"/>
      <description>
        <text>Algorithm ID.</text>
      </description>
    </leaf>
  </grouping>
  <grouping name="fingerprint-data">
    <description>
      <text>Fingerprints data.</text>
    </description>
    <container name="fingerprints">
      <description>
        <text>List of fingerprint.</text>
      </description>
      <uses name="algorithm-id"/>
      <list name="fingerprint">
        <key value="fingerprint-group-id"/>
        <description>
          <text>Fingerprint data.</text>
        </description>
        <leaf name="fingerprint-group-id">
          <type name="uint8">
            <range value="1..15"/>
          </type>
          <description>
            <text>Group index of a group of fingerprints.</text>
          </description>
        </leaf>
        <container name="features">
          <description>
            <text>List of feature.</text>
          </description>
          <list name="feature">
            <key value="feature-id"/>
            <description>
              <text>Feature info.</text>
            </description>
            <leaf name="feature-id">
              <type name="uint8"/>
              <description>
                <text>Feature index.</text>
              </description>
            </leaf>
            <leaf name="feature-value">
              <type name="string">
                <length value="1..256"/>
                <pattern value="[0-9]*(.[0-9]+)?"/>
              </type>
              <description>
                <text>Feature value string.</text>
              </description>
            </leaf>
          </list>
        </container>
      </list>
    </container>
  </grouping>
  <container name="eai-service">
    <description>
      <text>EAI service management.</text>
    </description>
    <container name="global">
      <description>
        <text>Configure global attributes.</text>
      </description>
      <leaf name="anomaly-detection-enable">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable EAI anomaly detection.</text>
        </description>
      </leaf>
      <leaf name="anomaly-identify-enable">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable EAI anomaly identify.</text>
        </description>
      </leaf>
      <leaf name="resource-prediction-enable">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable EAI resource prediction.</text>
        </description>
      </leaf>
    </container>
    <container name="anomaly-identify-datas">
      <config value="false"/>
      <description>
        <text>List of anomaly identify data.</text>
      </description>
      <list name="anomaly-identify-data">
        <key value="class sub-class module-id chassis-id slot-id object-id attribute-id sequence-id"/>
        <description>
          <text>Statistics of anomaly identify data.</text>
        </description>
        <leaf name="class">
          <type name="class-type"/>
          <description>
            <text>KPI main class.</text>
          </description>
        </leaf>
        <leaf name="sub-class">
          <type name="sub-class-type"/>
          <description>
            <text>KPI sub class.</text>
          </description>
        </leaf>
        <leaf name="module-id">
          <type name="uint32"/>
          <description>
            <text>KPI module ID.</text>
          </description>
        </leaf>
        <leaf name="chassis-id">
          <type name="uint8"/>
          <description>
            <text>Chassis ID.</text>
          </description>
        </leaf>
        <leaf name="slot-id">
          <type name="uint16"/>
          <description>
            <text>Slot ID.</text>
          </description>
        </leaf>
        <leaf name="object-id">
          <type name="string">
            <length value="1..128"/>
          </type>
          <description>
            <text>Object name.</text>
          </description>
        </leaf>
        <leaf name="attribute-id">
          <type name="string">
            <length value="1..128"/>
          </type>
          <description>
            <text>Attribute name.</text>
          </description>
        </leaf>
        <leaf name="sequence-id">
          <type name="uint32"/>
          <config value="false"/>
          <description>
            <text>KPI data sequence ID.</text>
          </description>
        </leaf>
        <leaf name="state">
          <type name="kpi-state"/>
          <description>
            <text>KPI state.</text>
          </description>
        </leaf>
        <leaf name="upload-time">
          <type name="uint32"/>
          <units name="s"/>
          <description>
            <text>Number of seconds from 00:00 on January 1, 1970 to the time when the event was generated, in UTC.</text>
          </description>
        </leaf>
        <leaf name="collect-interval">
          <type name="uint16"/>
          <units name="s"/>
          <description>
            <text>Collect interval.</text>
          </description>
        </leaf>
        <leaf name="fault-point-count">
          <type name="uint8"/>
          <description>
            <text>Fault point count.</text>
          </description>
        </leaf>
        <leaf name="abnormal-degree">
          <type name="uint16"/>
          <description>
            <text>Auxiliary parameters for determining the abnormality severity.</text>
          </description>
        </leaf>
      </list>
    </container>
    <container name="resource-prediction-datas">
      <config value="false"/>
      <description>
        <text>List of resource prediction data.</text>
      </description>
      <list name="resource-prediction-data">
        <key value="class sub-class module-id chassis-id slot-id object-id attribute-id sequence-id"/>
        <description>
          <text>Statistics of resource prediction data.</text>
        </description>
        <leaf name="class">
          <type name="class-type"/>
          <description>
            <text>KPI main class.</text>
          </description>
        </leaf>
        <leaf name="sub-class">
          <type name="sub-class-type"/>
          <description>
            <text>KPI sub class.</text>
          </description>
        </leaf>
        <leaf name="module-id">
          <type name="uint32"/>
          <description>
            <text>KPI module ID.</text>
          </description>
        </leaf>
        <leaf name="chassis-id">
          <type name="uint8"/>
          <description>
            <text>Chassis ID.</text>
          </description>
        </leaf>
        <leaf name="slot-id">
          <type name="uint16"/>
          <description>
            <text>Slot ID.</text>
          </description>
        </leaf>
        <leaf name="object-id">
          <type name="string">
            <length value="1..128"/>
          </type>
          <description>
            <text>Object name.</text>
          </description>
        </leaf>
        <leaf name="attribute-id">
          <type name="string">
            <length value="1..128"/>
          </type>
          <description>
            <text>Attribute name.</text>
          </description>
        </leaf>
        <leaf name="sequence-id">
          <type name="uint32"/>
          <config value="false"/>
          <description>
            <text>KPI data sequence ID.</text>
          </description>
        </leaf>
        <leaf name="state">
          <type name="kpi-state"/>
          <description>
            <text>KPI state. 0: alarm;</text>
          </description>
        </leaf>
        <leaf name="upload-time">
          <type name="uint32"/>
          <units name="s"/>
          <description>
            <text>Number of seconds from 00:00 on January 1, 1970 to the time when the event was generated, in UTC.</text>
          </description>
        </leaf>
        <leaf name="collect-interval">
          <type name="uint16"/>
          <units name="s"/>
          <description>
            <text>Collect interval.</text>
          </description>
        </leaf>
        <leaf name="over-threshold-days">
          <type name="uint16"/>
          <units name="d"/>
          <description>
            <text>Prediction days over the threshold.</text>
          </description>
        </leaf>
        <leaf name="period">
          <type name="uint8">
            <range value="1..72"/>
          </type>
          <units name="h"/>
          <description>
            <text>Period time of prediction.</text>
          </description>
        </leaf>
        <leaf name="value-type">
          <type name="kpi-value-type"/>
          <description>
            <text>Value type of KPI.</text>
          </description>
        </leaf>
        <leaf name="threshold">
          <type name="uint64"/>
          <description>
            <text>KPI threshold.</text>
          </description>
        </leaf>
        <leaf name="predict-num">
          <type name="uint16"/>
          <description>
            <text>Predict number in the event.</text>
          </description>
        </leaf>
        <leaf name="predict-value">
          <type name="string">
            <length value="1..10000"/>
          </type>
          <description>
            <text>Predict indcator value, seperated by comma.</text>
          </description>
        </leaf>
        <leaf name="predict-std-value">
          <type name="string">
            <length value="1..10000"/>
          </type>
          <description>
            <text>Predict standard deviation value, seperated by comma.</text>
          </description>
        </leaf>
        <leaf name="history-value">
          <type name="string">
            <length value="1..10000"/>
          </type>
          <description>
            <text>History value, seperated by comma.</text>
          </description>
        </leaf>
      </list>
    </container>
  </container>
  <rpc name="recv-algorithm-tagging">
    <description>
      <text>EAI receive algorithm and tagging data from controller.</text>
    </description>
    <input>
      <container name="algorithm-tagging-datas">
        <description>
          <text>List of algorithm and tagging data.</text>
        </description>
        <list name="algorithm-tagging-data">
          <key value="recv-data-index"/>
          <description>
            <text>Algorithm and tagging data.</text>
          </description>
          <leaf name="recv-data-index">
            <type name="uint8"/>
            <description>
              <text>Index of received data.</text>
            </description>
          </leaf>
          <choice name="data-type">
            <description>
              <text>Algorithm or tagging.</text>
            </description>
            <case name="algorithm">
              <description>
                <text>Algorithm data.</text>
              </description>
              <container name="algorithm">
                <description>
                  <text>Algorithm-selection data.</text>
                </description>
                <uses name="algorithm-id"/>
                <uses name="fingerprint-data"/>
              </container>
            </case>
            <case name="tagging">
              <description>
                <text>Tagging data.</text>
              </description>
              <container name="tagging">
                <description>
                  <text>Tagging data.</text>
                </description>
                <leaf name="start-time">
                  <type name="uint32"/>
                  <mandatory value="true"/>
                  <description>
                    <text>UTC time, time for data to start tagging.</text>
                  </description>
                </leaf>
                <leaf name="end-time">
                  <type name="uint32"/>
                  <mandatory value="true"/>
                  <description>
                    <text>UTC time, time for data to stop tagging.</text>
                  </description>
                </leaf>
                <leaf name="tagging-type">
                  <type name="uint8"/>
                  <mandatory value="true"/>
                  <description>
                    <text>Tagging type, e.g. white, black.</text>
                  </description>
                </leaf>
                <uses name="fingerprint-data"/>
              </container>
            </case>
          </choice>
          <container name="module">
            <description>
              <text>Module info.</text>
            </description>
            <leaf name="module-id">
              <type name="uint32"/>
              <description>
                <text>Index of module.</text>
              </description>
            </leaf>
            <container name="objects">
              <description>
                <text>List of object.</text>
              </description>
              <list name="object">
                <key value="object-group-id object-id chassis-id slot-id"/>
                <description>
                  <text>Object info.</text>
                </description>
                <leaf name="object-group-id">
                  <type name="uint8">
                    <range value="1..50"/>
                  </type>
                  <description>
                    <text>Group index of a group of objects.</text>
                  </description>
                </leaf>
                <leaf name="object-id">
                  <type name="string">
                    <length value="1..128"/>
                  </type>
                  <description>
                    <text>Object index: ID or name.</text>
                  </description>
                </leaf>
                <leaf name="chassis-id">
                  <type name="uint8"/>
                  <description>
                    <text>Chassis ID of this object.</text>
                  </description>
                </leaf>
                <leaf name="slot-id">
                  <type name="uint16"/>
                  <description>
                    <text>Slot ID of this object.</text>
                  </description>
                </leaf>
                <container name="attributes">
                  <description>
                    <text>List of attribute.</text>
                  </description>
                  <list name="attribute">
                    <key value="attribute-group-id attribute-id"/>
                    <description>
                      <text>Attribute info.</text>
                    </description>
                    <leaf name="attribute-group-id">
                      <type name="uint8">
                        <range value="1..50"/>
                      </type>
                      <description>
                        <text>Group index of a group of attributes.</text>
                      </description>
                    </leaf>
                    <leaf name="attribute-id">
                      <type name="string">
                        <length value="1..128"/>
                      </type>
                      <description>
                        <text>Attribute index: ID or name.</text>
                      </description>
                    </leaf>
                  </list>
                </container>
              </list>
            </container>
          </container>
        </list>
      </container>
    </input>
