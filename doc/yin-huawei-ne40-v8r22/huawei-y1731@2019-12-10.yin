      <enum name="zero-with-crc">
        <value value="1"/>
        <description>
          <text>Null signal with CRC-32.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Test pattern.</text>
    </description>
  </typedef>
  <container name="y1731">
    <description>
      <text>Configure Y.1731 attributes. Y.1731 is developed by ITU-T for operations, administration and maintenance (OAM) of Ethernet, which providing fault management and performance management solutions for Ethernet.</text>
    </description>
    <container name="global-attribute">
      <description>
        <text>Configure global Y.1731 attributes.</text>
      </description>
      <leaf name="cfm-version">
        <type name="version"/>
        <default value="standard"/>
        <description>
          <text>Version of Y.1731, which can be standard or draft7.</text>
        </description>
      </leaf>
      <leaf name="mp-address-model">
        <type name="mp-address-model"/>
        <default value="bridge"/>
        <description>
          <text>MAC model of Y.1731, which can be bridge or individual.</text>
        </description>
      </leaf>
      <leaf name="pm-mode-enable">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable Y.1731 PM.</text>
        </description>
      </leaf>
      <leaf name="portid-tlv">
        <type name="portid-tlv-type"/>
        <default value="interface-name"/>
        <description>
          <text>Set the type of port ID TLV in MAC trace packet.</text>
        </description>
      </leaf>
      <leaf name="idle-test-id-count">
        <type name="uint16"/>
        <default value="1"/>
        <description>
          <text>Set the number of idle test ID for query.</text>
        </description>
      </leaf>
    </container>
    <container name="idle-test-ids">
      <config value="false"/>
      <description>
        <text>List of all idle test IDs.</text>
      </description>
      <list name="idle-test-id">
        <key value="test-id"/>
        <description>
          <text>Operational state of idle test ID.</text>
        </description>
        <leaf name="test-id">
          <type name="uint32">
            <range value="1..4294967295"/>
          </type>
          <description>
            <text>Idle ID of a test instance.</text>
          </description>
        </leaf>
      </list>
    </container>
    <container name="global-enable">
      <when condition="(../global-attribute/cfm-version='standard')"/>
      <presence value="Enable/disable Y.1731."/>
      <description>
        <text>Configure global-enable Y.1731 attributes. Y.1731 is developed by ITU-T for operations, administration and maintenance (OAM) of Ethernet, which providing fault management and performance management solutions for Ethernet.</text>
      </description>
      <leaf name="clear-mac-vlan">
        <type name="pub-type:vlan-range"/>
        <description>
          <text>Configure the VLAN ID of clear MAC address.</text>
        </description>
      </leaf>
      <container name="mds">
        <description>
          <text>List of all maintenance domains.</text>
        </description>
        <list name="md">
          <key value="name"/>
          <description>
            <text>Configure a maintenance domain.</text>
          </description>
          <leaf name="name">
            <type name="string">
              <length value="1..43"/>
              <pattern value="[^\?]*"/>
            </type>
            <description>
              <text>Configure the name of a maintenance domain (MD).</text>
            </description>
          </leaf>
          <leaf name="level">
            <ext:operation-exclude value="update|delete"/>
            <type name="uint8">
              <range value="0..7"/>
            </type>
            <must condition="not(../../../default-md/level) or (../../../default-md/level&gt; ../level)"/>
            <default value="0"/>
            <description>
              <text>Configure the level of the maintenance domain.</text>
            </description>
          </leaf>
          <leaf name="format">
            <ext:operation-exclude value="update|delete"/>
            <type name="md-format"/>
            <must condition="((../format='string') or ((../format='none') and (not(../format-name))) or ((../format!='none') and (../format-name)))"/>
            <default value="string"/>
            <description>
              <text>Configure the format of the maintenance domain name. By default, the format is a string. If the format is MAC-address, the format name must be in form of H-H-H:U&lt;0-65535&gt;, for example, 0001-0001-0001:1.</text>
            </description>
          </leaf>
          <leaf name="format-name">
            <ext:operation-exclude value="update|delete"/>
            <when condition="../format!='none'"/>
            <type name="string">
              <length value="1..43"/>
              <pattern value="[^\?]*"/>
            </type>
            <ext:dynamic-default>
              <ext:default-value value="../name">
                <when condition="../format='string'"/>
                <description>
                  <text>The default value equals to the value of name when the format is string.</text>
                </description>
              </ext:default-value>
            </ext:dynamic-default>
            <description>
              <text>Configure the name of the maintenance domain which will be formatted. By default, it is the same as the name of the maintenance domain.</text>
            </description>
          </leaf>
          <leaf name="mip-create-type">
            <type name="mip-create-type"/>
            <default value="none"/>
            <description>
              <text>Configure the MIP create type of maintenance domain.</text>
            </description>
          </leaf>
          <leaf name="sender-tlv-type">
            <type name="sender-tlv-type"/>
            <default value="defer"/>
            <description>
              <text>Configure the sender ID TLV type of the default maintenance domain.</text>
            </description>
          </leaf>
          <leaf name="chassis-sub-type">
            <when condition="(../sender-tlv-type='chassis') or (../sender-tlv-type='chassis-manage')"/>
            <type name="chassis-sub-type"/>
            <default value="mac-address"/>
            <description>
              <text>Configure the chassis ID sub type of the maintenance domain.</text>
            </description>
          </leaf>
          <container name="mas">
            <description>
              <text>List of all maintenance associations.</text>
            </description>
            <list name="ma">
              <key value="name"/>
              <description>
                <text>Configure a maintenance association.</text>
              </description>
              <leaf name="name">
                <type name="string">
                  <length value="1..43"/>
                  <pattern value="[^\?]*"/>
                </type>
                <description>
                  <text>Configure the name of a maintenance association (MA).</text>
                </description>
              </leaf>
              <leaf name="format">
                <ext:operation-exclude value="update|delete"/>
                <type name="ma-format"/>
                <must condition="((../format='icc-based' and ../../../format='none' and ../format-name) or (../format!='icc-based'))"/>
                <default value="string"/>
                <description>
                  <text>Configure the format of the maintenance association's name.</text>
                </description>
              </leaf>
              <leaf name="format-name">
                <ext:operation-exclude value="update|delete"/>
                <type name="string">
                  <length value="1..43"/>
                  <pattern value="[^\?]*"/>
                </type>
                <ext:dynamic-default>
                  <ext:default-value value="../name">
                    <when condition="../format='string'"/>
                    <description>
                      <text>The default value equals to the value of name when the format is string.</text>
                    </description>
                  </ext:default-value>
                </ext:dynamic-default>
                <description>
                  <text>Configure the name of the maintenance association (MA) which will be formatted. By default, as the same with the name of the MA. The length of MA format description is [1,13] when the format is ICC-based, and [1,43] when it is string.</text>
                </description>
              </leaf>
              <choice name="map-service">
                <default value="no-bind"/>
                <description>
                  <text>Configure the type of a maintenance association's service.</text>
                </description>
                <case name="no-bind">
                  <description>
                    <text>MA map no service.</text>
                  </description>
                  <leaf name="no-bind">
                    <type name="empty"/>
                    <description>
                      <text>MA map no service.</text>
                    </description>
                  </leaf>
                </case>
                <case name="bind-vlan">
                  <description>
                    <text>Map a VLAN.</text>
                  </description>
                  <leaf name="vlan-id">
                    <ext:operation-exclude value="update">
                      <when condition="../meps/mep or ../rmeps/rmep"/>
                      <description>
                        <text>The value cannot be updated, when mep or rmep exists.</text>
                      </description>
                    </ext:operation-exclude>
                    <type name="uint16">
                      <range value="1..4094"/>
                    </type>
                    <mandatory value="true"/>
                    <description>
                      <text>Configure the VLAN ID associated with the maintenance association.</text>
                    </description>
                  </leaf>
                  <leaf name="one-way-threshold-value">
                    <ext:operation-exclude value="update"/>
                    <type name="uint32">
                      <range value="1..4294967295"/>
                    </type>
                    <units name="ms"/>
                    <description>
                      <text>Configure the threshold of one-way delay measurement.</text>
                    </description>
                  </leaf>
                  <leaf name="two-way-threshold-value">
                    <ext:operation-exclude value="update"/>
                    <type name="uint32">
                      <range value="1..4294967295"/>
                    </type>
                    <units name="ms"/>
                    <description>
                      <text>Configure the threshold of two-way delay measurement.</text>
                    </description>
                  </leaf>
                </case>
                <case name="bind-l2vc">
                  <description>
                    <text>Map a L2VC.</text>
                  </description>
                  <leaf name="l2vc-id">
                    <ext:operation-exclude value="update">
                      <when condition="../meps/mep or ../rmeps/rmep"/>
                      <description>
                        <text>The 'l2vc-id' cannot be updated, when mep or rmep exists.</text>
                      </description>
                    </ext:operation-exclude>
                    <type name="leafref">
                      <path value="/l2vpn:l2vpn/l2vpn:instances/l2vpn:instance/l2vpn:vpws-ldp/l2vpn:pws/l2vpn:pw/l2vpn:pw-id"/>
                    </type>
                    <must condition="count(../meps/mep)&lt;=1"/>
                    <mandatory value="true"/>
                    <description>
                      <text>Configure the ID of the L2VC associated with the maintenance association.</text>
                    </description>
                    <ext:support-filter value="true"/>
                  </leaf>
                  <leaf name="l2vc-type">
                    <ext:operation-exclude value="update">
                      <when condition="../meps/mep or ../rmeps/rmep"/>
                      <description>
                        <text>The 'l2vc-type' cannot be updated, when mep or rmep exists.</text>
                      </description>
                    </ext:operation-exclude>
                    <type name="l2vc-type"/>
                    <mandatory value="true"/>
                    <description>
                      <text>Configure the type of the L2VC associated with the maintenance association.</text>
                    </description>
                    <ext:support-filter value="true"/>
                  </leaf>
                  <leaf name="l2vc-ip">
                    <type name="leafref">
                      <path value="/l2vpn:l2vpn/l2vpn:instances/l2vpn:instance/l2vpn:vpws-ldp/l2vpn:pws/l2vpn:pw/l2vpn:peer-ip"/>
                    </type>
                    <description>
                      <text>Configure the peer IP of the L2VC associated with the maintenance association.</text>
                    </description>
                  </leaf>
                </case>
                <case name="bind-vsi">
                  <description>
                    <text>Map a VSI.</text>
                  </description>
                  <leaf name="vsi-name">
                    <ext:operation-exclude value="update">
                      <when condition="../meps/mep or ../rmeps/rmep"/>
                      <description>
                        <text>The 'vsi-name' cannot be updated, when mep or rmep exists.</text>
                      </description>
                    </ext:operation-exclude>
                    <type name="leafref">
                      <path value="/l2vpn:l2vpn/l2vpn:instances/l2vpn:instance/l2vpn:name"/>
                    </type>
                    <must condition="/l2vpn:l2vpn/l2vpn:instances/l2vpn:instance[l2vpn:name = current()][l2vpn:type='vpls']"/>
                    <description>
                      <text>Configure the name of the VSI associated with the maintenance association.</text>
                    </description>
                    <ext:support-filter value="true"/>
                  </leaf>
                </case>
                <case name="bind-ccc">
                  <description>
                    <text>Map a CCC.</text>
                  </description>
                  <leaf name="ccc-name">
                    <type name="leafref">
                      <path value="/l2vpn:l2vpn/l2vpn:instances/l2vpn:instance/l2vpn:name"/>
                    </type>
                    <must condition="/l2vpn:l2vpn/l2vpn:instances/l2vpn:instance[l2vpn:name = current()][l2vpn:type='vpws-local-ccc']"/>
                    <description>
                      <text>Configure the CCC names associated with the maintenance association.</text>
                    </description>
                    <ext:support-filter value="true"/>
                  </leaf>
                </case>
                <case name="bind-l2tpv3-pw">
                  <description>
                    <text>Map the tunnel of a l2tpv3.</text>
                  </description>
                  <leaf name="tnl-name">
                    <type name="string">
                      <length value="1..19"/>
                    </type>
                    <description>
                      <text>Configures the L2TPV3 tunnel name that associated with the maintenance association.</text>
                    </description>
                  </leaf>
                </case>
                <case name="bind-l2tpv3-ccc">
                  <description>
                    <text>Map the local connection of a L2TPV3.</text>
                  </description>
                  <leaf name="l2tp-ccc-name">
                    <type name="string">
                      <length value="1..31"/>
                    </type>
                    <description>
                      <text>Configures the L2TPv3 local connection name that associated with the maintenance association.</text>
                    </description>
                  </leaf>
                </case>
                <case name="bind-bgp-pw">
                  <description>
                    <text>Map a BGP PW.</text>
                  </description>
                  <leaf name="vpn-name">
                    <type name="leafref">
                      <path value="/l2vpn:l2vpn/l2vpn:common/l2vpn:vpws-bgp-vpns/l2vpn:vpws-bgp-vpn/l2vpn:name"/>
                    </type>
                    <mandatory value="true"/>
                    <description>
                      <text>Configure the name of the L2VPN associated with the maintenance association.</text>
                    </description>
                    <ext:support-filter value="true"/>
                  </leaf>
                  <leaf name="ce-id">
                    <type name="leafref">
                      <path value="/l2vpn:l2vpn/l2vpn:common/l2vpn:vpws-bgp-vpns/l2vpn:vpws-bgp-vpn[l2vpn:name=current()/../vpn-name]/l2vpn:sites/l2vpn:site/l2vpn:id"/>
                    </type>
                    <mandatory value="true"/>
                    <description>
                      <text>Configure the ID of the CE associated with the maintenance association.</text>
                    </description>
                    <ext:support-filter value="true"/>
                  </leaf>
                  <leaf name="ce-off-set">
                    <type name="leafref">
                      <path value="/l2vpn:l2vpn/l2vpn:instances/l2vpn:instance/l2vpn:vpws-bgp/l2vpn:pws/l2vpn:pw/l2vpn:remote-site-id"/>
                    </type>
                    <mandatory value="true"/>
                    <description>
                      <text>Configure the ID of the remote CE associated with the maintenance association.</text>
                    </description>
                    <ext:support-filter value="true"/>
                  </leaf>
                </case>
                <case name="bind-evpn">
                  <description>
                    <text>Map an EVPN.</text>
                  </description>
                  <leaf name="evpn-name">
                    <type name="leafref">
                      <path value="/evpn:evpn/evpn:instances/evpn:instance/evpn:name"/>
                    </type>
                    <must condition="/evpn:evpn/evpn:instances/evpn:instance[evpn:name=current()]/evpn:type='vpws-evpn' or /evpn:evpn/evpn:instances/evpn:instance[evpn:name=current()]/evpn:type='normal'"/>
                    <description>
                      <text>Configure the EVPN name associated with the maintenance association.</text>
                    </description>
                  </leaf>
                </case>
                <case name="bind-bd">
                  <description>
                    <text>Map a BD.</text>
                  </description>
                  <leaf name="bd-id">
                    <type name="leafref">
                      <path value="/bd:bd/bd:instances/bd:instance/bd:id"/>
                    </type>
                    <description>
                      <text>Configure the BD ID associated with the maintenance association.</text>
                    </description>
                  </leaf>
                </case>
              </choice>
              <leaf name="packet-priority">
                <type name="uint8">
                  <range value="0..7"/>
                </type>
                <default value="7"/>
                <description>
                  <text>Configure the priority of the packet which is sent in the maintenance association.</text>
                </description>
              </leaf>
              <leaf name="ccm-interval">
                <type name="ccm-interval"/>
                <default value="1s"/>
                <description>
                  <text>Configure the interval at which CCMs are sent in a maintenance association.</text>
                </description>
              </leaf>
              <leaf name="alarm-occur-time">
                <type name="uint16">
                  <range value="0..30000"/>
                </type>
                <units name="ms"/>
                <must condition="(../alarm-occur-time mod 500)=0"/>
                <default value="2500"/>
                <description>
                  <text>Configure the delay for reporting an alarm.</text>
                </description>
              </leaf>
              <leaf name="alarm-finish-time">
                <type name="uint16">
                  <range value="0..30000"/>
                </type>
                <units name="ms"/>
                <must condition="(../alarm-finish-time mod 500)=0"/>
                <default value="10000"/>
                <description>
                  <text>Configure the delay for clearing an alarm.</text>
                </description>
              </leaf>
              <leaf name="active-time">
                <type name="uint16">
                  <range value="0..600"/>
                </type>
                <units name="s"/>
                <default value="0"/>
                <description>
                  <text>Configure the time to activate a RMEP.</text>
                </description>
              </leaf>
              <leaf name="interface-tlv">
                <type name="boolean"/>
                <default value="false"/>
                <description>
                  <text>Enable/disable interface status TLV in CCM.</text>
                </description>
              </leaf>
              <leaf name="sender-id-tlv">
                <type name="boolean"/>
                <default value="false"/>
                <description>
                  <text>Enable/disable sender ID TLV in CCM.</text>
                </description>
              </leaf>
              <leaf name="port-status-tlv">
                <type name="boolean"/>
                <default value="false"/>
                <description>
                  <text>Enable/disable port status TLV in CCM.</text>
                </description>
              </leaf>
              <leaf name="mip-create-type">
                <type name="ma-mip-create-type"/>
                <default value="defer"/>
                <description>
                  <text>Configures the MIP create type of maintenance association.</text>
                </description>
              </leaf>
              <leaf name="unexpected-mep-alarm">
                <type name="boolean"/>
                <default value="false"/>
                <description>
                  <text>Enable/disable unexpectedMep alarm.</text>
                </description>
              </leaf>
              <container name="ais">
                <presence value="Enable/disable AIS."/>
                <description>
                  <text>Configure the function of all AIS instances.</text>
                </description>
                <leaf name="interval">
                  <type name="ais-interval"/>
                  <default value="1s"/>
                  <description>
                    <text>Configure the interval at which AIS packets are sent.</text>
                  </description>
                </leaf>
                <leaf name="level">
                  <type name="uint8">
                    <range value="0..7"/>
                  </type>
                  <must condition="../level&gt;../../../../level"/>
                  <description>
                    <text>Configure the level of AIS packets.</text>
                  </description>
                </leaf>
                <leaf name="suppress-alarm-enable">
                  <type name="boolean"/>
                  <default value="false"/>
                  <description>
                    <text>Enable/disable suppressing alarms.</text>
                  </description>
                </leaf>
                <container name="link-interfaces">
                  <description>
                    <text>List of AIS on interfaces.</text>
                  </description>
                  <list name="link-interface">
                    <key value="if-name"/>
                    <description>
                      <text>Configure the interface instance of AIS.</text>
                    </description>
                    <leaf name="if-name">
                      <type name="leafref">
                        <path value="/ifm:ifm/ifm:interfaces/ifm:interface/ifm:name"/>
                      </type>
                      <description>
                        <text>Configure the interface name of AIS.</text>
                      </description>
                    </leaf>
                  </list>
                </container>
              </container>
              <container name="meps">
                <description>
                  <text>List of all MEPs.</text>
                </description>
                <list name="mep">
                  <must condition="(pe-vlan and ce-vlan) or (not(pe-vlan) and not(ce-vlan))"/>
                  <must condition="(direction and (if-name or peer-ip)) or (not(direction) and not(if-name) and not(peer-ip))"/>
                  <key value="id"/>
                  <description>
                    <text>Configure a MEP point.</text>
                  </description>
                  <leaf name="id">
                    <type name="uint16">
                      <range value="1..8191"/>
                    </type>
                    <description>
                      <text>Configure a MEP ID.</text>
                    </description>
                  </leaf>
                  <choice name="type">
                    <mandatory value="true"/>
                    <description>
                      <text>MEP type.</text>
                    </description>
                    <case name="if-mep">
                      <leaf name="if-name">
                        <ext:operation-exclude value="update"/>
                        <type name="leafref">
                          <path value="/ifm:ifm/ifm:interfaces/ifm:interface/ifm:name"/>
                        </type>
                        <must condition="not(../../../meps/mep[if-name!=current()])"/>
                        <mandatory value="true"/>
                        <description>
                          <text>Configure a MEP interface.</text>
                        </description>
                      </leaf>
                      <choice name="tags">
                        <description>
                          <text>Configure the tags of a MEP.</text>
                        </description>
                        <case name="two-tags">
                          <leaf name="pe-vlan">
                            <ext:operation-exclude value="update|delete"/>
                            <type name="uint16">
                              <range value="1..4094"/>
                            </type>
                            <description>
                              <text>Configure the PE VLAN of a MEP.</text>
                            </description>
                          </leaf>
                          <leaf name="ce-vlan">
                            <ext:operation-exclude value="update|delete"/>
                            <type name="uint16">
                              <range value="1..4094"/>
                            </type>
                            <description>
                              <text>Configure the CE VLAN of a MEP.</text>
                            </description>
                          </leaf>
                          <description>
                            <text>Configure the two tags of a MEP.</text>
                          </description>
                        </case>
                        <case name="one-tag">
                          <description>
                            <text>Configure the dot1q VLAN of a MEP.</text>
                          </description>
                          <leaf name="dot1q-vlan">
                            <ext:operation-exclude value="update|delete"/>
                            <type name="uint16">
                              <range value="1..4094"/>
                            </type>
                            <description>
                              <text>Configure the dot1q VLAN of a MEP.</text>
                            </description>
                          </leaf>
                        </case>
                      </choice>
                      <leaf name="measure-point">
                        <when condition="../../../l2vc-id and ../direction='inward'"/>
                        <type name="measure-point"/>
                        <default value="pw"/>
                        <description>
                          <text>Configure the measure point of a MEP.</text>
                        </description>
                      </leaf>
                      <leaf name="clear-arp">
                        <when condition="../../../vlan-id"/>
                        <type name="boolean"/>
                        <must condition="../direction='outward'"/>
                        <default value="false"/>
                        <description>
                          <text>Enable/disable clear ARP.</text>
                        </description>
                      </leaf>
                      <leaf name="test-enable">
                        <type name="boolean"/>
                        <must condition="../test-enable='false' or (not(../../../l2tp-ccc-name) and not(../../../tnl-name))"/>
                        <default value="false"/>
                        <description>
                          <text>Enable/disable test.</text>
                        </description>
                      </leaf>
                      <container name="lck">
                        <presence value="Enable/disable LCK."/>
                        <description>
                          <text>Configure the LCK.</text>
                        </description>
                        <leaf name="level">
                          <type name="uint8">
                            <range value="0..7"/>
                          </type>
                          <must condition="../level &gt; ../../../../../../level"/>
                          <mandatory value="true"/>
                          <description>
                            <text>Configure the LCK message level.</text>
                          </description>
                        </leaf>
                      </container>
                      <container name="eth-bn-receive">
                        <when condition="(../../../vlan-id or ../../../no-bind) and (not (../pe-vlan)) and ../direction='outward'"/>
                        <presence value="Enable the ETN-BN receive function."/>
                        <description>
                          <text>Configure ETH-BN receive.</text>
                        </description>
                        <leaf name="min-bw">
                          <type name="uint8">
                            <range value="1..4"/>
                          </type>
                          <units name="Mbit/s"/>
                          <default value="1"/>
                          <description>
                            <text>The min bandwidth.</text>
                          </description>
                        </leaf>
                        <leaf name="mode">
                          <type name="eth-bn-mode"/>
                          <must condition="not((../../../../vlan-id) and (../mode='port-vlan'))"/>
                          <default value="port"/>
                          <description>
                            <text>ETH-BN mode.</text>
