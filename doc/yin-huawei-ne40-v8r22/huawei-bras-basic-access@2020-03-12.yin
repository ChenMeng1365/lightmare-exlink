  <grouping name="subscriber-base">
    <description>
      <text>Subscriber base.</text>
    </description>
    <leaf name="default-domain-type">
      <when condition="../default-domain-name"/>
      <type name="bras-basic-access-default-domain-type"/>
      <description>
        <text>Default domain type.</text>
      </description>
    </leaf>
    <leaf name="default-domain-name">
      <type name="leafref">
        <path value="/aaa:aaa/aaa:domains/aaa:domain/aaa:name"/>
      </type>
      <description>
        <text>Default domain name.</text>
      </description>
    </leaf>
    <leaf name="pre-domain-name">
      <type name="leafref">
        <path value="/aaa:aaa/aaa:domains/aaa:domain/aaa:name"/>
      </type>
      <description>
        <text>Pre domain name.</text>
      </description>
    </leaf>
  </grouping>
  <grouping name="accounting-radius-server">
    <description>
      <text>Accounting radius server.</text>
    </description>
    <leaf name="radius-server-name">
      <type name="leafref">
        <path value="/radius:radius/radius:radius-server-groups/radius:radius-server-group/radius:name"/>
      </type>
      <description>
        <text>RADIUS server name.</text>
      </description>
    </leaf>
  </grouping>
  <grouping name="bas-interface-name">
    <description>
      <text>Bas interface name.</text>
    </description>
    <leaf name="bas-interface-name">
      <type name="string">
        <length value="1..32"/>
        <pattern value="([^A-Z]{1,32})"/>
      </type>
      <description>
        <text>Bas interface name.</text>
      </description>
      <ext:case-sensitivity value="lower-only"/>
    </leaf>
  </grouping>
  <grouping name="default-domain-name">
    <description>
      <text>Bas interface default domain name.</text>
    </description>
    <leaf name="default-domain-name">
      <type name="leafref">
        <path value="/aaa:aaa/aaa:domains/aaa:domain/aaa:name"/>
      </type>
      <description>
        <text>Default domain name.</text>
      </description>
    </leaf>
  </grouping>
  <grouping name="leaseline-access">
    <description>
      <text>Leaseline access type base.</text>
    </description>
    <leaf name="password">
      <type name="pub-type:password-extend">
        <length value="1..268"/>
      </type>
      <mandatory value="true"/>
      <description>
        <text>Password. The simple password length ranges from 1 to 128, and the cipher password length ranges from 1 to 268.</text>
      </description>
    </leaf>
    <leaf name="password-type">
      <type name="bras-basic-access-password-type"/>
      <mandatory value="true"/>
      <description>
        <text>Password type.</text>
      </description>
    </leaf>
    <container name="leaseline-base">
      <description>
        <text>Configure leaseline base.</text>
      </description>
      <uses name="default-domain-name"/>
      <uses name="bas-interface-name"/>
      <uses name="accounting-radius-server"/>
    </container>
  </grouping>
  <grouping name="nas-port-type">
    <description>
      <text>Configures the type of the interface that accesses a BAS interface.</text>
    </description>
    <container name="nas-port-type">
      <description>
        <text>Configure the type of the interface that accesses a BAS interface.</text>
      </description>
      <leaf name="type">
        <type name="bras-basic-access-nas-port-type"/>
        <default value="ethernet"/>
        <description>
          <text>Nas port type.</text>
        </description>
      </leaf>
    </container>
  </grouping>
  <grouping name="trust-access-line-id-insert">
    <description>
      <text>Trust access line id insert.</text>
    </description>
    <container name="trust-access-line-id-insert">
      <description>
        <text>Configure the the access-line-id information, including the following commands:client-access-line-id and client-option82.</text>
      </description>
      <leaf name="insert-type">
        <type name="bras-basic-access-access-line-id-type"/>
        <description>
          <text>Inserts information about the BAS interface in the telecom format.</text>
        </description>
      </leaf>
      <leaf name="insert-version">
        <when condition="../insert-type"/>
        <type name="bras-basic-access-trust-access-line-id-insert-version"/>
        <description>
          <text>Specifies encapsulating the access-line-id information into a DHCP packet in version2 format.</text>
        </description>
      </leaf>
    </container>
  </grouping>
  <grouping name="access-line-id-insert">
    <description>
      <text>Access line id insert grouping.</text>
    </description>
    <container name="access-line-id-insert">
      <description>
        <text>Configure the access line ID.</text>
      </description>
      <choice name="access-line-id-insert">
        <description>
          <text>Access line ID insert choice.</text>
        </description>
        <case name="trust">
          <description>
            <text>Configures the the access-line-id information, including the following commands:basinfo-insert, client-access-line-id and client-option82.</text>
          </description>
          <leaf name="trust-insert-type">
            <type name="bras-basic-access-access-line-id-type"/>
            <description>
              <text>Inserts information about the BAS interface in the telecom format.</text>
            </description>
          </leaf>
          <leaf name="trust-insert-version">
            <when condition="../trust-insert-type"/>
            <type name="bras-basic-access-trust-access-line-id-insert-version"/>
            <description>
              <text>Specifies encapsulating the access-line-id information into a DHCP packet.</text>
            </description>
          </leaf>
        </case>
        <case name="distrust">
          <description>
            <text>Configures the the access-line-id information, including the following commands:basinfo-insert, client-access-line-id and client-option82.</text>
          </description>
          <leaf name="distrust-insert-version">
            <type name="bras-basic-access-distrust-access-line-id-insert-version"/>
            <description>
              <text>Specifies encapsulating the access-line-id information into a DHCP packet.</text>
            </description>
          </leaf>
        </case>
      </choice>
    </container>
  </grouping>
  <grouping name="access-delay">
    <description>
      <text>Access delay grouping.</text>
    </description>
    <container name="access-delay">
      <description>
        <text>Configure a response delay policy for access users on the BAS interface.</text>
      </description>
      <choice name="access-delay">
        <description>
          <text>Access delay choice.</text>
        </description>
        <case name="global">
          <description>
            <text>Access delay global.</text>
          </description>
          <leaf name="delay-time">
            <type name="uint32">
              <range value="1..2550"/>
            </type>
            <units name="10 ms"/>
            <description>
              <text>Specifies encapsulating the access-line-id information into a DHCP packet in version3 format.</text>
            </description>
          </leaf>
        </case>
        <case name="mac">
          <description>
            <text>Access delay by mac.</text>
          </description>
          <leaf name="odd-delay-time">
            <type name="uint32">
              <range value="1..2550"/>
            </type>
            <units name="10 ms"/>
            <description>
              <text>Specifies encapsulating the access-line-id information into a DHCP packet in version3 format.</text>
            </description>
          </leaf>
          <leaf name="even-delay-time">
            <type name="uint32">
              <range value="1..2550"/>
            </type>
            <units name="10 ms"/>
            <description>
              <text>Specifies encapsulating the access-line-id information into a DHCP packet in version3 format.</text>
            </description>
          </leaf>
        </case>
        <case name="circuit-id">
          <description>
            <text>Access delay by circuit id.</text>
          </description>
          <leaf name="circuit-id-delay-time">
            <type name="uint32">
              <range value="1..2550"/>
            </type>
            <units name="10 ms"/>
            <mandatory value="true"/>
            <description>
              <text>Specifies the response delay for access users.</text>
            </description>
          </leaf>
          <leaf name="circuit-id">
            <type name="string">
              <length value="1..200"/>
            </type>
            <mandatory value="true"/>
            <description>
              <text>Specifies the ID of an access node.</text>
            </description>
          </leaf>
        </case>
        <case name="load-balance-group">
          <description>
            <text>Configures a response delay policy for access users on a Broadband Access Server (BAS) interface.</text>
          </description>
          <leaf name="load-balance-group-delay-time">
            <type name="uint32">
              <range value="1..2550"/>
            </type>
            <units name="10 ms"/>
            <mandatory value="true"/>
            <description>
              <text>Specifies the response delay for access users.</text>
            </description>
          </leaf>
          <leaf name="load-balance-group">
            <type name="leafref">
              <path value="/bras-basic-access:bras-basic-access/bras-basic-access:load-balance-groups/bras-basic-access:load-balance-group/bras-basic-access:name"/>
            </type>
            <mandatory value="true"/>
            <description>
              <text>Specifies a name for a load balancing group.</text>
            </description>
          </leaf>
        </case>
      </choice>
    </container>
  </grouping>
  <grouping name="client-vendor-class">
    <description>
      <text>Client vendor class grouping.</text>
    </description>
    <container name="client-option60">
      <description>
        <text>Configure the client-option60 or vendor-class identifier of Option 60.</text>
      </description>
      <leaf name="client-vendor-class">
        <type name="bras-basic-access-vendor-class-type"/>
        <description>
          <text>Specifies the client-option60 or vendor-class identifier of Option 60.</text>
        </description>
      </leaf>
    </container>
  </grouping>
  <grouping name="client-option18">
    <description>
      <text>Client option18 grouping.</text>
    </description>
    <container name="client-option18">
      <description>
        <text>Configure the client option18.</text>
      </description>
      <leaf name="option18-enable">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable the server to trust the information in the Option 18 field of DHCPv6 messages sent by the client.</text>
        </description>
      </leaf>
    </container>
