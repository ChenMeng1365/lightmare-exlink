  <typedef name="syslog-log-switch">
    <type name="enumeration">
      <enum name="off">
        <value value="0"/>
        <description>
          <text>Do not send log to syslog server.</text>
        </description>
      </enum>
      <enum name="on">
        <value value="1"/>
        <description>
          <text>Send log to syslog server.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Swtich to send log to syslog server.</text>
    </description>
  </typedef>
  <typedef name="syslog-format">
    <type name="enumeration">
      <enum name="rfc3164">
        <value value="1"/>
        <description>
          <text>RFC3164.</text>
        </description>
      </enum>
      <enum name="rfc3164-with-tag">
        <value value="2"/>
        <description>
          <text>RFC3164 with TAG.</text>
        </description>
      </enum>
      <enum name="rfc5424">
        <value value="3"/>
        <description>
          <text>RFC5424.</text>
        </description>
      </enum>
    </type>
    <default value="rfc3164"/>
    <description>
      <text>Syslog format.</text>
    </description>
  </typedef>
  <container name="configuration">
    <description>
      <text>Configuration info.</text>
    </description>
    <container name="syslog-server">
      <description>
        <text>Specify syslog-server.</text>
      </description>
      <container name="sources">
        <description>
          <text>Specify source address in syslog transactions.</text>
        </description>
        <container name="ipv4">
          <description>
            <text>Specify IPv4 source address.</text>
          </description>
          <presence value="ipv4"/>
          <leaf name="ipv4-address">
            <status value="obsolete">
              <ncd:revise-date date="2018-06-28"/>
            </status>
            <description>
              <text>IPv4 address.</text>
            </description>
            <type name="ncd:ipv4-address"/>
          </leaf>
          <leaf-list name="ipv4-addr">
            <description>
              <text>IPv4 address.</text>
            </description>
            <type name="ncd:ipv4-address"/>
            <max-elements value="1"/>
            <ncd:revise-date date="2018-06-28"/>
          </leaf-list>
        </container>
        <container name="ipv6">
          <description>
            <text>Specify IPv6 source address.</text>
          </description>
          <presence value="ipv6"/>
          <leaf name="ipv6-address">
            <status value="obsolete">
              <ncd:revise-date date="2018-06-28"/>
            </status>
            <description>
              <text>IPv6 address.</text>
            </description>
            <type name="ncd:ipv6-address"/>
          </leaf>
          <leaf-list name="ipv6-addr">
            <description>
              <text>IPv6 address.</text>
            </description>
            <type name="ncd:ipv6-address"/>
            <max-elements value="1"/>
            <ncd:revise-date date="2018-06-28"/>
          </leaf-list>
        </container>
        <container name="interface">
          <description>
            <text>Specify the name of the interface.</text>
          </description>
          <presence value="interface"/>
          <leaf name="interface-name">
            <status value="obsolete">
              <ncd:revise-date date="2018-06-28"/>
            </status>
            <description>
              <text>Interface name.</text>
            </description>
            <type name="ncd:ifname">
              <length value="0..31"/>
            </type>
          </leaf>
          <leaf-list name="if-name">
            <description>
              <text>Interface name.</text>
            </description>
            <type name="ncd:ifname"/>
            <max-elements value="1"/>
            <ncd:revise-date date="2018-06-28"/>
          </leaf-list>
        </container>
      </container>
      <container name="hosts-vrf">
        <description>
          <text>Specify syslog-server host with vrf.</text>
        </description>
        <container name="ipv4-vrf">
          <description>
            <text>Specify syslog-server host of ipv4 with vrf.</text>
          </description>
          <list name="ipv4">
            <description>
              <text>Specify syslog-server host of ipv4.</text>
            </description>
            <key value="vrf-name ipv4-address"/>
            <leaf name="vrf-name">
              <description>
                <text>VPN routing/forwarding instance (VRF) name.</text>
              </description>
              <type name="string">
                <length value="1..32"/>
              </type>
            </leaf>
            <leaf name="ipv4-address">
              <description>
                <text>IPv4 address.</text>
              </description>
              <type name="ncd:ipv4-address"/>
            </leaf>
            <leaf name="src-ipv4">
              <description>
                <text>Source IPv4 address.</text>
              </description>
              <type name="ncd:ipv4-address-none-zero"/>
              <ncd:revise-date date="2018-01-04"/>
            </leaf>
            <leaf name="fport">
              <description>
                <text>The remote port.</text>
              </description>
              <type name="uint16">
                <range value="1..65535"/>
              </type>
              <default value="514"/>
            </leaf>
            <leaf name="lport">
              <description>
                <text>The local port.</text>
              </description>
              <type name="uint16">
                <range value="514 | 1024..65535"/>
              </type>
              <default value="514"/>
            </leaf>
            <leaf name="alarmlog">
              <description>
                <text>Send alarm log to syslog server.</text>
              </description>
              <type name="syslog-log-switch"/>
              <default value="off"/>
            </leaf>
            <leaf name="debugmsg">
              <description>
                <text>Send debug message to syslog server.</text>
              </description>
              <type name="syslog-log-switch"/>
              <default value="off"/>
            </leaf>
            <leaf name="cmdlog">
              <description>
                <text>Send command log to syslog server.</text>
              </description>
              <type name="syslog-log-switch"/>
              <default value="off"/>
            </leaf>
            <leaf name="servicelog">
              <description>
                <text>Send service log to syslog server.</text>
              </description>
              <type name="syslog-log-switch"/>
              <default value="off"/>
            </leaf>
            <leaf name="netconflog">
              <description>
                <text>Send NETCONF log to syslog server.</text>
              </description>
              <type name="syslog-log-switch"/>
              <default value="off"/>
              <ncd:revise-date date="2016-02-23"/>
            </leaf>
            <leaf name="braslog">
              <description>
                <text>Send bras log to syslog server.</text>
              </description>
              <type name="syslog-log-switch"/>
              <default value="off"/>
            </leaf>
            <leaf name="natlog">
              <description>
                <text>Send NAT log to syslog server.</text>
              </description>
              <type name="syslog-log-switch"/>
              <default value="off"/>
            </leaf>
            <leaf name="urllog">
              <description>
                <text>Send URL log to syslog server.</text>
              </description>
              <type name="syslog-log-switch"/>
              <default value="off"/>
            </leaf>
            <leaf name="nat-translationlog">
              <description>
                <text>Send NAT translation log to syslog server.</text>
              </description>
              <type name="syslog-log-switch"/>
              <default value="off"/>
              <ncd:revise-date date="2016-02-01"/>
            </leaf>
            <leaf name="nat-portrangelog">
              <description>
                <text>Send NAT portrange log to syslog server.</text>
              </description>
              <type name="syslog-log-switch"/>
              <default value="off"/>
              <ncd:revise-date date="2016-02-01"/>
            </leaf>
            <leaf name="haglog">
              <description>
                <text>Send Hybrid Access Gateway log to syslog server.</text>
              </description>
              <type name="syslog-log-switch"/>
              <default value="off"/>
              <ncd:revise-date date="2017-03-03"/>
            </leaf>
            <leaf name="snmplog">
              <description>
                <text>Send SNMP log to syslog server.</text>
              </description>
              <type name="syslog-log-switch"/>
              <default value="off"/>
              <ncd:revise-date date="2017-07-28"/>
            </leaf>
            <leaf name="weblog">
              <description>
                <text>Send WEB log to syslog server.</text>
              </description>
              <type name="syslog-log-switch"/>
              <default value="off"/>
              <ncd:revise-date date="2018-12-26"/>
            </leaf>
            <leaf name="traplog">
              <description>
                <text>Send TRAP log to syslog server.</text>
              </description>
              <type name="syslog-log-switch"/>
              <default value="off"/>
              <ncd:revise-date date="2021-01-18"/>
            </leaf>
          </list>
        </container>
        <container name="ipv6-linklocal-vrf">
          <description>
            <text>Specify syslog-server host of IPv6(linklocal) with vrf.</text>
          </description>
          <list name="ipv6-linklocal">
            <description>
              <text>Specify syslog-server host of IPv6 with linklocal.</text>
            </description>
            <key value="vrf-name ipv6-address interface-name"/>
            <leaf name="vrf-name">
              <description>
                <text>VPN routing/forwarding instance (VRF) name.</text>
              </description>
              <type name="string">
                <length value="1..32"/>
              </type>
            </leaf>
            <leaf name="ipv6-address">
              <description>
                <text>IPv6 address.</text>
              </description>
              <type name="ncd:ipv6-address">
                <pattern value="^[Ff][Ee][8-9a-bA-B][0-9a-fA-F]"/>
              </type>
            </leaf>
            <leaf name="interface-name">
              <description>
                <text>Interface name.</text>
              </description>
              <type name="ncd:ifname">
                <length value="1..31"/>
              </type>
            </leaf>
            <container name="src-ip">
              <description>
                <text>Source IPv6 address.</text>
              </description>
              <leaf name="src-ipv6">
                <description>
                  <text>Source IPv6 address.</text>
                </description>
                <type name="ncd:ipv6-address"/>
              </leaf>
              <ncd:revise-date date="2018-01-04"/>
            </container>
            <leaf name="fport">
              <description>
                <text>The remote port.</text>
              </description>
              <type name="uint16">
                <range value="1..65535"/>
              </type>
              <default value="514"/>
            </leaf>
            <leaf name="lport">
              <description>
                <text>The local port.</text>
              </description>
              <type name="uint16">
                <range value="514 | 1024..65535"/>
              </type>
              <default value="514"/>
            </leaf>
            <leaf name="alarmlog">
              <description>
                <text>Send alarm log to syslog server.</text>
              </description>
              <type name="syslog-log-switch"/>
              <default value="off"/>
            </leaf>
            <leaf name="debugmsg">
              <description>
                <text>Send debug message to syslog server.</text>
              </description>
              <type name="syslog-log-switch"/>
              <default value="off"/>
            </leaf>
            <leaf name="cmdlog">
              <description>
                <text>Send command log to syslog server.</text>
              </description>
              <type name="syslog-log-switch"/>
              <default value="off"/>
            </leaf>
            <leaf name="servicelog">
              <description>
                <text>Send service log to syslog server.</text>
              </description>
              <type name="syslog-log-switch"/>
              <default value="off"/>
            </leaf>
            <leaf name="netconflog">
              <description>
