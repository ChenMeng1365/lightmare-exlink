        <text>List of switch mode supported by port group.</text>
      </description>
      <list name="port-group-mode">
        <ext:generated-by value="system"/>
        <ext:operation-exclude value="create"/>
        <key value="port-groupid"/>
        <description>
          <text>Configure switch mode supported by port group.</text>
        </description>
        <leaf name="port-groupid">
          <type name="uint32">
            <range value="1..10000"/>
          </type>
          <description>
            <text>Index of Port Group.</text>
          </description>
        </leaf>
        <leaf name="port-list">
          <type name="string">
            <length value="0..64"/>
          </type>
          <config value="false"/>
          <description>
            <text>Port list in group.</text>
          </description>
        </leaf>
        <leaf name="mode-type">
          <type name="enumeration">
            <enum name="25GE-10GE">
              <value value="1"/>
              <description>
                <text>Configure 25GE-10GE.</text>
              </description>
            </enum>
            <enum name="10GE-1GE">
              <value value="2"/>
              <description>
                <text>Configure 10GE-1GE.</text>
              </description>
            </enum>
          </type>
          <default value="25GE-10GE"/>
          <description>
            <text>Switch mode.</text>
          </description>
        </leaf>
      </list>
    </container>
    <container name="queue-priority">
      <description>
        <text>Configure MAC Queue Priority Enable.</text>
      </description>
      <leaf name="queue-pri-enable">
        <type name="enumeration">
          <enum name="disable">
            <value value="0"/>
            <description>
              <text>Disable.</text>
            </description>
          </enum>
          <enum name="enable">
            <value value="1"/>
            <description>
              <text>Enable.</text>
            </description>
          </enum>
        </type>
        <default value="enable"/>
        <description>
          <text>MAC Queue Priority Enable.</text>
        </description>
      </leaf>
    </container>
    <container name="frame-format">
      <description>
        <text>Configure card frame format.</text>
      </description>
      <leaf name="frame-format">
        <type name="enumeration">
          <enum name="SDH">
            <value value="0"/>
            <description>
              <text>Synchronous Digital Hierarchy.</text>
            </description>
          </enum>
          <enum name="SONET">
            <value value="1"/>
            <description>
              <text>Synchronous Optical Network.</text>
            </description>
          </enum>
        </type>
        <default value="SDH"/>
        <description>
          <text>FrameFormat.</text>
        </description>
      </leaf>
    </container>
    <container name="process-mode">
      <must condition="(shield-mode = 'disabled' and process-mode = 'reset') or (shield-mode = 'enabled') or (shield-mode = 'disabled' and not(process-mode))"/>
      <description>
        <text>Configure subcard fault process mode.</text>
      </description>
      <leaf name="process-mode">
        <type name="fault-process-mode-type"/>
        <default value="reset"/>
        <description>
          <text>Card fault process mode.</text>
        </description>
      </leaf>
      <leaf name="shield-mode">
        <type name="enumeration">
          <enum name="disabled">
            <value value="0"/>
            <description>
              <text>Disabled.</text>
            </description>
          </enum>
          <enum name="enabled">
            <value value="1"/>
            <description>
              <text>Enabled.</text>
            </description>
          </enum>
        </type>
        <default value="disabled"/>
        <description>
          <text>Shield Card-Fault Process-Mode Global-Config.</text>
        </description>
      </leaf>
      <leaf name="fault-process-mode-status">
        <type name="fault-process-mode-type"/>
        <config value="false"/>
        <description>
          <text>Configure subcard fault process mode.</text>
        </description>
      </leaf>
    </container>
    <container name="em-set">
      <description>
        <text>Configure E&amp;M voice signaling.</text>
      </description>
      <leaf name="single-mode">
        <type name="enumeration">
          <enum name="bell-1">
            <value value="0"/>
            <description>
              <text>Bell-1 mode.</text>
            </description>
          </enum>
          <enum name="bell-2">
            <value value="1"/>
            <description>
              <text>Bell-2 mode.</text>
            </description>
          </enum>
          <enum name="bell-3">
            <value value="2"/>
            <description>
              <text>Bell-3 mode.</text>
            </description>
          </enum>
          <enum name="bell-4">
            <value value="3"/>
            <description>
              <text>Bell-4 mode.</text>
            </description>
          </enum>
          <enum name="bell-5">
            <value value="4"/>
            <description>
              <text>Bell-5 mode.</text>
            </description>
          </enum>
        </type>
        <default value="bell-5"/>
        <description>
          <text>E&amp;M signaling mode.</text>
        </description>
      </leaf>
      <leaf name="userline-mode">
        <type name="enumeration">
          <enum name="4wire">
            <value value="1"/>
            <description>
              <text>Configure 4 wire mode.</text>
            </description>
          </enum>
          <enum name="2wire">
            <value value="0"/>
            <description>
              <text>Configure 2 wire mode.</text>
            </description>
          </enum>
        </type>
        <default value="4wire"/>
        <description>
          <text>E&amp;M user line mode.</text>
        </description>
      </leaf>
    </container>
    <container name="service-switch">
      <description>
        <text>Configure service switch.</text>
      </description>
      <leaf name="service-switch-mode">
        <type name="enumeration">
          <enum name="p2mp">
            <value value="0"/>
            <description>
              <text>Point to multipoint mode.</text>
            </description>
          </enum>
          <enum name="time-delay">
            <value value="1"/>
            <description>
              <text>Time delay mode.</text>
            </description>
          </enum>
        </type>
        <default value="p2mp"/>
        <description>
          <text>Service mode switch.</text>
        </description>
      </leaf>
    </container>
    <container name="panel-slots">
      <config value="false"/>
      <description>
        <text>Operational state of card panel extend slots.</text>
      </description>
      <leaf name="slot">
        <type name="string">
          <length value="0..32"/>
        </type>
        <config value="false"/>
        <description>
          <text>Card Panel Slots.</text>
        </description>
      </leaf>
    </container>
  </augment>
  <augment target-node="/devm:devm/devm:mpu-boards/devm:mpu-board">
    <description>
      <text>Mac set.</text>
    </description>
    <container name="cmac-padding">
      <description>
        <text>Configure chip global filling function.</text>
      </description>
      <leaf name="padding">
        <type name="enumeration">
          <enum name="disable">
            <value value="0"/>
            <description>
              <text>Enable Chip Global fill function.</text>
            </description>
          </enum>
          <enum name="enable">
            <value value="1"/>
            <description>
              <text>Disable Chip Global fill function.</text>
            </description>
          </enum>
        </type>
        <default value="disable"/>
        <description>
          <text>Chip global filling function.</text>
        </description>
      </leaf>
    </container>
  </augment>
  <augment target-node="/devm:devm/devm:lpu-boards/devm:lpu-board">
    <description>
      <text>Mac set.</text>
    </description>
    <container name="cmac-padding">
      <description>
        <text>Configure chip global filling function.</text>
      </description>
      <leaf name="padding">
        <type name="enumeration">
          <enum name="disable">
            <value value="0"/>
            <description>
              <text>Enable Chip Global fill function.</text>
            </description>
          </enum>
          <enum name="enable">
            <value value="1"/>
            <description>
              <text>Disable Chip Global fill function.</text>
            </description>
          </enum>
        </type>
        <default value="disable"/>
        <description>
          <text>Chip global filling function.</text>
        </description>
      </leaf>
    </container>
  </augment>
  <ext:task-name value="device-mgr"/>
</module>
