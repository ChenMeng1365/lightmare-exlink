                    </description>
                    <leaf name="start-delay-end-date-time">
                      <type name="yang:date-and-time"/>
                      <description>
                        <text>The test instance end time.</text>
                      </description>
                    </leaf>
                  </case>
                  <case name="delay">
                    <description>
                      <text>Stop the test after the specified delay time.</text>
                    </description>
                    <leaf name="start-delay-end-delay-seconds">
                      <type name="uint32">
                        <range value="6..86399"/>
                      </type>
                      <units name="s"/>
                      <description>
                        <text>The test instance end delay seconds.</text>
