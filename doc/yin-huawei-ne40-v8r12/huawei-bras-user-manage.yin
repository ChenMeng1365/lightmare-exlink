        <pattern value='[^"?aAcCdDgGiImMoOpPsSuUvV]'/>
      </type>
      <description>
        <text>Configures the user-defined format of the attribute seperator.</text>
      </description>
    </leaf>
  </grouping>
  <grouping name="template-default-user-name-group">
    <description>
      <text>List of the creation mode for the pure user names of IPoX or pppoe users.</text>
    </description>
    <container name="include-types">
      <description>
        <text>List of the creation mode for the pure user names of IPoX or pppoe users.</text>
      </description>
      <list name="include-type">
        <key value="type"/>
        <min-elements value="1"/>
        <ordered-by value="user"/>
        <description>
          <text>Configure the creation mode for the pure user names of IPoX or pppoe users.</text>
        </description>
        <uses name="include-type-group"/>
      </list>
    </container>
  </grouping>
  <grouping name="default-user-name-template">
    <description>
      <text>Configure a user name template to a Broadband Access Server (BAS) interface.</text>
    </description>
    <container name="default-user-name-template">
      <description>
        <text>Configure a user name template to a Broadband Access Server (BAS) interface.</text>
      </description>
      <leaf name="name">
        <type name="leafref">
          <path value="/bras-user-manage:bras-user-manage/bras-user-manage:default-user-name-templates/bras-user-manage:default-user-name-template/bras-user-manage:template-name"/>
        </type>
        <description>
          <text>Specifies a user name template.</text>
        </description>
      </leaf>
    </container>
  </grouping>
  <container name="bras-user-manage">
    <description>
      <text>Bras user management data model define.</text>
    </description>
    <container name="service-identify-policys">
      <description>
        <text>List of the service identification policy and enter the service identification policy.</text>
      </description>
      <list name="service-identify-policy">
        <key value="name"/>
        <description>
          <text>Configure a service identification policy and enter the service identification policy.</text>
        </description>
        <leaf name="name">
          <type name="string">
            <length value="1..32"/>
            <pattern value="[a-z0-9._-]{1,32}"/>
          </type>
          <must condition="../name != '-' and ../name != '--'"/>
          <description>
            <text>Specifies the name of a service identification policy.</text>
          </description>
          <ext:case-sensitivity value="lower-only"/>
        </leaf>
        <container name="mode">
          <description>
            <text>Configure the service identification mode.</text>
          </description>
          <leaf name="mode">
            <type name="bras-user-manage-service-identify-type"/>
            <description>
              <text>Configures the service identification mode.</text>
            </description>
          </leaf>
        </container>
        <container name="dhcp-option60">
          <when condition="../mode/mode='dhcp-option60'"/>
          <description>
            <text>Configure option60 attribute.</text>
          </description>
          <uses name="option60-vendorclass-cfg"/>
        </container>
        <container name="vendor-class">
          <when condition="../mode/mode='vendor-class'"/>
          <description>
            <text>Configure vendor-class attribute.</text>
          </description>
          <uses name="option60-vendorclass-cfg"/>
        </container>
      </list>
    </container>
    <container name="domain-map-lists">
      <description>
        <text>List of the domain map.</text>
      </description>
      <list name="domain-map-list">
        <key value="name"/>
        <max-elements value="4"/>
        <description>
          <text>Configure a domain mapping list.</text>
        </description>
        <leaf name="name">
          <type name="string">
            <length value="1..32"/>
            <pattern value="[!-~]{1,32}"/>
            <pattern value="([^A-Z&quot;']{1,32})"/>
          </type>
          <must condition="../name != '-' and ../name != '--'"/>
          <description>
            <text>Domain map list name.</text>
          </description>
          <ext:case-sensitivity value="lower-only"/>
        </leaf>
        <container name="domain-mappings">
          <description>
            <text>List of the domain mappings in the domain mapping list.</text>
          </description>
          <list name="domain-mapping">
            <key value="source-domain"/>
            <max-elements value="128"/>
            <description>
              <text>Configure a domain mapping in the domain mapping list.</text>
            </description>
            <leaf name="source-domain">
              <type name="leafref">
                <path value="/aaa:aaa/aaa:domains/aaa:domain/aaa:name"/>
              </type>
              <description>
                <text>Source domain name.</text>
              </description>
            </leaf>
            <leaf name="destination-domain">
              <type name="leafref">
                <path value="/aaa:aaa/aaa:domains/aaa:domain/aaa:name"/>
              </type>
              <mandatory value="true"/>
              <description>
                <text>Destination domain name.</text>
              </description>
            </leaf>
            <leaf name="extend">
              <type name="boolean"/>
              <default value="false"/>
              <description>
                <text>Enable/disable extending mapping.</text>
              </description>
            </leaf>
          </list>
        </container>
      </list>
    </container>
    <container name="domain-lists">
      <description>
        <text>List of the domain.</text>
      </description>
      <list name="domain-list">
        <key value="name"/>
        <max-elements value="128"/>
        <description>
          <text>Configure a domain list.</text>
        </description>
        <leaf name="name">
          <type name="string">
            <length value="1..32"/>
            <pattern value="[!-~]{1,32}"/>
            <pattern value="([^A-Z&quot;%'*/:&lt;&gt;\?@|]{1,32})"/>
          </type>
          <must condition="../name != '-' and ../name != '--'"/>
          <description>
            <text>Domain list name.</text>
          </description>
          <ext:case-sensitivity value="lower-only"/>
        </leaf>
        <leaf-list name="domain-list-domain">
          <type name="leafref">
            <path value="/aaa:aaa/aaa:domains/aaa:domain/aaa:name"/>
          </type>
          <max-elements value="16384"/>
          <description>
            <text>Configure a domain to a domain list when the same operation needs to be performed for the domains.</text>
          </description>
        </leaf-list>
      </list>
    </container>
    <container name="ip-userlog">
      <presence value="ip-userlog"/>
      <description>
        <text>Configure the NE40E to generate user login and logout logs and send the logs to a log server.</text>
      </description>
      <container name="ip-userlog-access">
        <description>
          <text>Configure user login and logout logs.</text>
        </description>
        <leaf name="ip-address">
          <type name="inet:ipv4-address-no-zone"/>
          <description>
            <text>Sets an IP address for a log server.</text>
          </description>
        </leaf>
        <leaf name="udp-port">
          <when condition="../ip-address"/>
          <type name="uint32">
            <range value="1..65535"/>
          </type>
          <mandatory value="true"/>
          <description>
            <text>Sets a UDP port number for a log server.</text>
          </description>
        </leaf>
        <leaf name="version">
          <type name="uint32">
            <range value="1..4"/>
          </type>
          <description>
            <text>Specifies the version of packets used to send user login and logout logs.</text>
          </description>
        </leaf>
        <leaf name="enable">
          <when condition="../ip-address and ../version"/>
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable the NE40E to generate user login and logout logs and send the logs to a log server.</text>
          </description>
        </leaf>
        <leaf name="send-format-syslog">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable the device to use the syslog format to send user online and offline logs.</text>
          </description>
        </leaf>
      </container>
      <container name="ip-userlog-call-status">
        <description>
          <text>Configure user online result logs.</text>
        </description>
        <leaf name="ip-address">
          <type name="inet:ipv4-address-no-zone"/>
          <description>
            <text>Sets calling status log ip.</text>
          </description>
        </leaf>
        <leaf name="udp-port">
          <when condition="../ip-address"/>
          <type name="uint32">
            <range value="1..65535"/>
          </type>
          <mandatory value="true"/>
          <description>
            <text>Sets user calling status log port.</text>
          </description>
        </leaf>
        <leaf name="call-status-enable">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable the user calling status log information.</text>
          </description>
        </leaf>
      </container>
    </container>
    <container name="trace">
      <description>
        <text>Configure service trace.</text>
      </description>
      <leaf name="online-fail-auto-enable">
        <type name="boolean"/>
        <default value="true"/>
        <description>
          <text>Enable/disable the automatic service trace function.</text>
        </description>
      </leaf>
    </container>
    <container name="user-authorization">
      <description>
        <text>Configure the user authorization.</text>
      </description>
      <leaf name="reauthorize-enable">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable command-line reauthorization.</text>
        </description>
      </leaf>
    </container>
    <container name="qoe-accounting">
      <description>
        <text>Configure a QoE accounting copy server.</text>
      </description>
      <leaf name="radius-server-group-name">
        <type name="leafref">
          <path value="/radius:radius/radius:radius-server-groups/radius:radius-server-group/radius:name"/>
        </type>
        <description>
          <text>Configure a QoE accounting copy server.</text>
        </description>
      </leaf>
      <leaf name="interval">
        <type name="uint32">
          <range value="30..120"/>
        </type>
        <units name="min"/>
        <description>
          <text>Configure a real-time accounting interval for the QoE.</text>
        </description>
      </leaf>
    </container>
    <container name="access-policy">
      <description>
        <text>Configure a policy of users are allowed or to go online.</text>
      </description>
      <leaf name="pure-user-name-no-exist-online">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable users are allowed to go online if the pure user name is empty.</text>
        </description>
      </leaf>
    </container>
    <container name="realtime-accounting-backup">
      <description>
        <text>Configure a real-time accounting backup between the master and slave MPUs.</text>
      </description>
      <leaf name="enable">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable real-time accounting backup between the master and slave MPUs. By default, real-time accounting backup is disabled between the master and slave MPUs. After a master/slave MPU switchover, the real-time accounting interval is reset.</text>
        </description>
      </leaf>
    </container>
    <container name="accounting-time-calibration">
      <description>
        <text>Configure accounting time calibration for PPPoE users.</text>
      </description>
      <leaf name="enable">
        <type name="boolean"/>
        <default value="true"/>
        <description>
          <text>Enable/disable accounting time calibration for PPPoE users.</text>
        </description>
      </leaf>
    </container>
    <container name="acct-session-id">
      <description>
        <text>Configure the version for generating the accounting session ID.</text>
      </description>
      <leaf name="version">
        <type name="enumeration">
          <enum name="version1">
            <value value="1"/>
            <description>
              <text>Indicates that the accounting session ID of the user is generated by version 1.</text>
            </description>
          </enum>
          <enum name="version2">
            <value value="2"/>
            <description>
              <text>Indicates that the accounting session ID of the user is generated by version 2.</text>
            </description>
          </enum>
          <enum name="version3">
            <value value="3"/>
            <description>
              <text>Indicates that the accounting session ID of the user is generated by version 3.</text>
            </description>
          </enum>
          <enum name="version4">
            <value value="4"/>
            <description>
              <text>Indicates that the accounting session ID of the user is generated by version 4.</text>
            </description>
          </enum>
          <enum name="version5">
            <value value="5"/>
            <description>
              <text>Indicates that the accounting session ID of the user is generated by version 5.</text>
            </description>
          </enum>
          <enum name="version6">
            <value value="6"/>
            <description>
              <text>Indicates that the accounting session ID of the user is generated by version 6.</text>
            </description>
          </enum>
        </type>
        <default value="version1"/>
        <description>
          <text>Sets the version for generating the accounting session ID.</text>
        </description>
      </leaf>
      <leaf name="nas-logic-sysname">
        <when condition="../version='version1' or ../version='version4' or ../version='version5'"/>
        <type name="string">
          <length value="1..7"/>
        </type>
        <description>
          <text>Indicates the host name which is used to generate accounting session ID of the user.</text>
        </description>
      </leaf>
    </container>
    <container name="access-speed">
      <description>
        <text>Configure the user access speed.</text>
      </description>
      <container name="adjust-system-state">
        <description>
          <text>Configure the NE40E to adjust the user access rate based on the system status.</text>
        </description>
        <leaf name="enable">
          <type name="boolean"/>
          <default value="true"/>
          <description>
            <text>Enable/disable the user access rate according to the cpu state.</text>
          </description>
        </leaf>
      </container>
      <container name="access-rate-limitation">
        <must condition="(access-number and access-period) or (not(access-number) and not(access-period))"/>
        <description>
          <text>Configure the access rate limitation on the NE40E. By default, users access the device at a lower rate between that limited by the CPU usage and that limited by the memory usage.</text>
        </description>
        <leaf name="access-number">
          <type name="uint32">
            <range value="1..65535"/>
          </type>
          <description>
            <text>Specifies the number of access users during the statistics period.</text>
          </description>
        </leaf>
        <leaf name="access-period">
          <type name="uint32">
            <range value="1..65535"/>
          </type>
          <units name="s"/>
          <description>
            <text>Specifies the period of access user statistics.</text>
          </description>
        </leaf>
        <leaf name="type">
          <when condition="../access-number and ../access-period"/>
          <type name="enumeration">
            <enum name="forced">
              <value value="1"/>
              <description>
                <text>Indicates that users access the device at the configured access rate, regardless of the CPU or memory usage.</text>
              </description>
            </enum>
            <enum name="level1">
              <value value="2"/>
              <description>
                <text>Indicates that the router adjusts the user access rate based on the system status.</text>
              </description>
            </enum>
          </type>
          <description>
            <text>Sets the access rate limitation type.</text>
          </description>
        </leaf>
        <leaf name="trigger-packet-enable">
          <type name="boolean"/>
          <default value="true"/>
          <description>
            <text>Enable/disable a device to adjust the access rate of users who go online by sending ARP/IP/IPv6/ND packets based on the system status.</text>
          </description>
        </leaf>
      </container>
      <container name="access-queue-threshold">
        <description>
          <text>Configure a size threshold for the UM message queue to control the usage of the UM message queue. When the usage of the UM message queue reaches the threshold, the system decreases or increases the CPCAR value.</text>
        </description>
        <leaf name="msg-queue-length">
          <type name="uint32">
            <range value="1..48"/>
          </type>
          <default value="24"/>
          <description>
            <text>Set a size threshold for the access queue.</text>
          </description>
        </leaf>
      </container>
      <container name="offline-speed">
        <description>
          <text>Configure the user offline speed, specifically, the number of users that go offline within a second. If the number of users to go offline within a second exceeds the configured value, the users that cannot go offline enter the offline queue.</text>
        </description>
        <leaf name="offline-number">
          <type name="uint32">
            <range value="50..256"/>
          </type>
          <default value="256"/>
          <description>
            <text>Specifies the number of users that go offline within a second.</text>
          </description>
        </leaf>
      </container>
    </container>
    <container name="default-password-templates">
      <description>
        <text>List of IPoX user passwords.</text>
      </description>
      <list name="default-password-template">
        <key value="template-name"/>
        <description>
          <text>Configure the password of an IPoX user.</text>
        </description>
        <leaf name="template-name">
          <type name="string">
            <length value="1..31"/>
          </type>
          <description>
            <text>Specifies the name of the template used to determine the user password generation mode.</text>
          </description>
        </leaf>
        <uses name="default-password"/>
      </list>
    </container>
    <container name="access-trigger-loose">
      <description>
        <text>Configure online in loose mode after the system restarts due to an exception reduces client restarts.</text>
      </description>
      <leaf name="loose-time">
        <when condition="../all-time='false'"/>
        <type name="uint32">
          <range value="0..1440"/>
        </type>
        <units name="min"/>
        <default value="120"/>
        <description>
          <text>Period during which users can get online in loose mode after the system restarts. 0 indicates that users cannot get online in loose mode after the system restarts.</text>
        </description>
      </leaf>
      <leaf name="all-time">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable loose mode for getting online after the system restarts.</text>
        </description>
      </leaf>
      <leaf name="infinite-lease">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable infiniting lease users can get online in loose mode after the system restarts.</text>
        </description>
      </leaf>
    </container>
    <container name="local-aaa-server-users">
      <description>
        <text>List of the local AAA server users.</text>
      </description>
      <list name="local-aaa-server-user">
        <key value="user-name"/>
        <description>
          <text>Configure a local AAA server user.</text>
        </description>
        <leaf name="user-name">
          <type name="string">
            <length value="1..253"/>
            <pattern value="([^A-Z&quot;%'*/:&lt;&gt;\?@|]{1,253})"/>
          </type>
          <description>
            <text>Specifies a user name that is case-insensitive and supports the asterisk (*). Special characters including slash (/), backslash (\), colon (:), question mark (?), and quotation marks (') are not supported.</text>
          </description>
          <ext:case-sensitivity value="lower-only"/>
        </leaf>
        <leaf name="password-type">
          <type name="enumeration">
            <enum name="cipher-password">
              <value value="1"/>
              <description>
                <text>Specifies a ciphertext password, the new password is at least eight characters long and contains at least two of upper-case letters, lower-case letters, digits, and special characters.</text>
              </description>
            </enum>
            <enum name="irreversible-password">
              <value value="2"/>
              <description>
                <text>Specifies an irreversible password.</text>
              </description>
            </enum>
          </type>
          <mandatory value="true"/>
          <description>
            <text>Specifies the password type of a user.</text>
          </description>
        </leaf>
        <leaf name="password">
          <type name="pub-type:password-extend"/>
          <mandatory value="true"/>
          <description>
            <text>Specifies the password of a user.</text>
          </description>
        </leaf>
        <leaf name="user-state">
          <type name="enumeration">
            <enum name="block">
              <value value="0"/>
              <description>
                <text>Indicates that a user is in the blocked state. The device denies any authentication request from a blocked user.</text>
              </description>
            </enum>
            <enum name="active">
              <value value="1"/>
              <description>
                <text>Indicates that a user is in the active state. The device receives and handles the authentication request from an active user.</text>
              </description>
            </enum>
            <enum name="lock-when-authen-fail">
              <value value="2"/>
              <description>
                <text>Indicates that a user is locked due to authentication failures. When the number of consecutive authentication failures of a user reaches the limit, the user is locked. The device denies any authentication request from a locked users within the specified time.</text>
              </description>
            </enum>
          </type>
          <default value="lock-when-authen-fail"/>
          <description>
            <text>Specifies the state of a user.</text>
          </description>
        </leaf>
        <leaf name="user-level">
          <type name="uint16">
            <range value="0..15"/>
          </type>
          <description>
            <text>Specifies the level of a user. If the command level adjustment function is disabled, the value of userLevel ranges from 0 to 3. If the command level adjustment function is enabled, the value of userLevel ranges from 0 to 15.</text>
          </description>
        </leaf>
        <leaf name="ftp-directory">
          <type name="string">
            <length value="1..255"/>
          </type>
          <description>
            <text>Specifies an FTP path.</text>
          </description>
        </leaf>
        <leaf name="idle-cut">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable the idle-cut function.</text>
          </description>
        </leaf>
        <leaf name="fail-times">
          <when condition="../user-state='lock-when-authen-fail'"/>
          <type name="uint32">
            <range value="1..10"/>
          </type>
          <default value="3"/>
          <description>
            <text>Specifies the maximum number of allowed authentication failures. When the number of consecutive authentication failures of a user reaches the limit, the user is locked.</text>
          </description>
        </leaf>
        <leaf name="fail-interval">
          <when condition="../user-state='lock-when-authen-fail'"/>
          <type name="uint32">
            <range value="1..65535"/>
          </type>
          <units name="min"/>
          <default value="5"/>
          <description>
            <text>Specifies the interval at which a locked user is performed to be re-authenticated.</text>
          </description>
        </leaf>
        <leaf name="callback-nocheck">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable the callback signals of a modem are not checked.</text>
          </description>
        </leaf>
        <leaf name="callback-number">
          <type name="string">
            <length value="1..64"/>
          </type>
          <description>
            <text>Specifies a string of characters for callback user dial-up.</text>
          </description>
        </leaf>
        <leaf name="qos-profile-name">
          <type name="leafref">
            <path value="/qos:qos/qos:queue-scheduler-template/qos:profiles/qos:profile/qos:name"/>
          </type>
          <must condition="/qos:qos/qos:queue-scheduler-template/qos:profiles/qos:profile[qos:name=current()]/qos:mode='normal'"/>
          <description>
            <text>Specifies the name of the QoS profile.</text>
          </description>
        </leaf>
        <leaf name="ip-address">
          <type name="inet:ipv4-address-no-zone"/>
          <description>
            <text>Specifies the IP address of a local user account, the IP address must be configured in the ip pool when it is not disabled or it belongs to the address segment of excluded-ip-address.</text>
          </description>
        </leaf>
        <leaf name="vpn-name">
          <when condition="../ip-address"/>
          <type name="leafref">
            <path value="/ni:network-instance/ni:instances/ni:instance/ni:name"/>
          </type>
          <must condition="/ni:network-instance/ni:instances/ni:instance[ni:name=current()]/l3vpn:afs/l3vpn:af[l3vpn:type='ipv4-unicast']"/>
          <description>
            <text>Specifies a VPN name, when the vpn-name is not delivered, the ip pool is matched based on the ip-address. When vpn-name is delivered, ip-address and vpn-name must be matched to match the ip pool.</text>
          </description>
        </leaf>
        <leaf name="user-group-name">
          <type name="leafref">
            <path value="/aaa:aaa/aaa:user-groups/aaa:user-group/aaa:name"/>
          </type>
          <description>
            <text>Specifies the name of a user group.</text>
          </description>
        </leaf>
        <leaf name="terminal">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable login using a terminal.</text>
          </description>
        </leaf>
        <leaf name="telnet">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable login using Telnet.</text>
          </description>
        </leaf>
        <leaf name="ftp">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable login using FTP.</text>
          </description>
        </leaf>
        <leaf name="ppp">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable login using PPP.</text>
