        value 1 ;
        description RFC3164. ;
      }
      enum rfc3164-with-tag {
        value 2 ;
        description 'RFC3164 with TAG.'
                    ;
      }
      enum rfc5424 {
        value 3 ;
        description RFC5424. ;
      }
    }
    default rfc3164 ;
    description 'Syslog format.'
