            <leaf name="echo-reply">
              <description>
                <text>The number of sending echo-reply packets.</text>
              </description>
              <type name="uint32"/>
            </leaf>
            <leaf name="redirect">
              <description>
                <text>The number of rsending redirect packets.</text>
              </description>
              <type name="uint32"/>
            </leaf>
            <leaf name="group-query">
              <description>
                <text>The number of sending group-query packets.</text>
              </description>
              <type name="uint32"/>
            </leaf>
            <leaf name="group-report-v1">
              <description>
                <text>The number of sending group-report-v1 packets.</text>
              </description>
