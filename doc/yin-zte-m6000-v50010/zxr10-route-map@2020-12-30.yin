                <text>Set l2-srv6-equivalent-sid.</text>
              </description>
              <presence value="l2-srv6-equivalent-sid"/>
              <leaf name="equivalent-sid">
                <description>
                  <text>Set equivalent sid.</text>
                </description>
                <type name="empty"/>
                <mandatory value="true"/>
              </leaf>
              <ncd:revise-date date="2020-12-04"/>
            </container>
            <container name="cpu">
              <description>
                <text>Set cpu flag.</text>
              </description>
              <presence value="cpu is not Mandatory"/>
              <leaf name="cpu">
                <description>
                  <text>Set cpu flag.</text>
                </description>
                <type name="empty"/>
                <mandatory value="true"/>
              </leaf>
              <ncd:revise-date date="2020-12-30"/>
            </container>
          </container>
          <container name="continues">
            <description>
              <text>Continue match action.</text>
            </description>
            <choice name="continue">
              <description>
                <text>Continue match action.</text>
              </description>
              <case name="continue-next">
                <description>
                  <text>Continue to match next sequence.</text>
                </description>
                <leaf name="next">
                  <description>
                    <text>Continue to match next sequence.</text>
                  </description>
                  <type name="empty"/>
                </leaf>
              </case>
              <case name="continue-sequence">
                <description>
                  <text>Continue to match specify sequence.</text>
                </description>
                <leaf name="sequence">
                  <description>
                    <text>Continue to match specify sequence.</text>
                  </description>
                  <type name="uint32">
                    <range value="0..65535"/>
                  </type>
                </leaf>
              </case>
            </choice>
            <ncd:revise-date date="2015-09-01"/>
          </container>
          <container name="time-range">
            <description>
              <text>Sequence of route-map binds time-range.</text>
            </description>
            <presence value="time range"/>
            <leaf name="time-range">
              <description>
                <text>Time range.</text>
              </description>
              <type name="string">
                <length value="1..31"/>
              </type>
              <mandatory value="true"/>
            </leaf>
            <ncd:revise-date date="2016-05-24"/>
          </container>
          <container name="load-share">
            <description>
              <text>Forward mode enable load-share.</text>
            </description>
            <leaf name="load-share">
              <description>
                <text>Forward mode enable load-share.</text>
              </description>
              <type name="empty"/>
            </leaf>
            <ncd:revise-date date="2016-05-24"/>
          </container>
          <container name="default-route">
            <description>
              <text>Default route.</text>
            </description>
            <leaf name="default-route-enable">
              <description>
                <text>Default route enable.</text>
              </description>
              <type name="empty"/>
            </leaf>
            <ncd:revise-date date="2016-07-18"/>
          </container>
          <container name="blackhole-route">
            <description>
              <text>Blackhole route.</text>
            </description>
            <leaf name="blackhole-route-enable">
              <description>
                <text>Blackhole route enable.</text>
              </description>
              <type name="empty"/>
            </leaf>
            <ncd:revise-date date="2016-07-18"/>
          </container>
          <container name="strict-policy">
            <description>
              <text>Enable strict policy.</text>
            </description>
            <leaf name="strict-policy">
              <description>
                <text>Enable strict policy.</text>
              </description>
              <type name="empty"/>
            </leaf>
            <ncd:revise-date date="2016-08-18"/>
