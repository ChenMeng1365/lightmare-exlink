  <grouping name="f-interface-state">
    <description>
      <text>the operational state of interface.</text>
    </description>
    <leaf name="phy-status">
      <type name="enumeration">
        <enum name="UP">
          <value value="1"/>
          <description>
            <text>Ready to pass packets.</text>
          </description>
        </enum>
        <enum name="DOWN">
          <value value="2"/>
          <description>
            <text>The interface does not pass any packets.</text>
          </description>
        </enum>
      </type>
      <description>
        <text>The current physical state of the interface.</text>
      </description>
    </leaf>
    <leaf name="ipv4-oper-status">
      <type name="enumeration">
        <enum name="UP">
          <value value="1"/>
          <description>
            <text>Ready to pass packets.</text>
          </description>
        </enum>
        <enum name="DOWN">
          <value value="2"/>
          <description>
            <text>The interface does not pass any packets.</text>
          </description>
        </enum>
      </type>
      <description>
        <text>The current ipv4 operational state of the interface.</text>
      </description>
    </leaf>
  </grouping>
  <grouping name="s-interface-state">
    <description>
      <text>the operational state of subinterface.</text>
    </description>
    <leaf name="ipv4-oper-status">
      <type name="enumeration">
        <enum name="UP">
          <value value="1"/>
          <description>
            <text>Ready to pass packets.</text>
          </description>
        </enum>
        <enum name="DOWN">
          <value value="2"/>
          <description>
            <text>The interface does not pass any packets.</text>
          </description>
        </enum>
      </type>
      <description>
        <text>The current ipv4 operational state of the interface.</text>
      </description>
    </leaf>
  </grouping>
  <grouping name="interface-statistics-state">
    <description>
      <text>the operational state of interface statistics.</text>
    </description>
    <leaf name="input-utilization">
      <type name="decimal64">
        <fraction-digits value="6"/>
        <range value="0..1.000000"/>
      </type>
      <description>
        <text>The input utilization of bandwidith. The maximum decimal precision is 6 and
the specific reporting accuracy shall be subject to project customization.</text>
      </description>
    </leaf>
    <leaf name="output-utilization">
      <type name="decimal64">
        <fraction-digits value="6"/>
        <range value="0..1.000000"/>
      </type>
      <description>
        <text>The output utilization of bandwidith. The maximum decimal precision is 6 and
the specific reporting accuracy shall be subject to project customization.</text>
      </description>
    </leaf>
    <leaf name="in-traffic-rate">
      <type name="decimal64">
        <fraction-digits value="4"/>
      </type>
      <description>
        <text>The input traffic rate(unit:Mbps).</text>
      </description>
    </leaf>
    <leaf name="in-packet-rate">
      <type name="decimal64">
        <fraction-digits value="4"/>
      </type>
      <description>
        <text>The input packet rate(unit:Kfps).</text>
      </description>
    </leaf>
    <leaf name="out-traffic-rate">
      <type name="decimal64">
        <fraction-digits value="4"/>
      </type>
      <description>
        <text>The output traffic rate(unit:Mbps).</text>
      </description>
    </leaf>
    <leaf name="out-packet-rate">
      <type name="decimal64">
        <fraction-digits value="4"/>
      </type>
      <description>
        <text>The output packet rate(unit:Kfps).</text>
      </description>
    </leaf>
    <leaf name="in-v4-octets">
      <type name="oc-yang:counter64"/>
      <description>
        <text>The total number of v4 octets received on the interface.</text>
      </description>
    </leaf>
    <leaf name="out-v4-octets">
      <type name="oc-yang:counter64"/>
      <description>
        <text>The total number of v4 octets sent on the interface.</text>
      </description>
    </leaf>
    <leaf name="in-v4-pkts">
      <type name="oc-yang:counter64"/>
      <description>
        <text>Total v4 packets received.</text>
      </description>
    </leaf>
    <leaf name="out-v4-pkts">
      <type name="oc-yang:counter64"/>
      <description>
        <text>Total v4 packets sent.</text>
      </description>
    </leaf>
    <leaf name="in-v6-octets">
      <type name="oc-yang:counter64"/>
      <description>
        <text>The total number of v6 octets received on the interface.</text>
      </description>
    </leaf>
    <leaf name="out-v6-octets">
      <type name="oc-yang:counter64"/>
      <description>
        <text>The total number of v6 octets sent on the interface.</text>
      </description>
    </leaf>
    <leaf name="in-v6-pkts">
      <type name="oc-yang:counter64"/>
      <description>
        <text>Total v6 packets received.</text>
      </description>
    </leaf>
    <leaf name="out-v6-pkts">
      <type name="oc-yang:counter64"/>
      <description>
        <text>Total v6 packets sent.</text>
      </description>
    </leaf>
    <leaf name="in-v4-traffic-rate">
      <type name="decimal64">
        <fraction-digits value="4"/>
      </type>
      <description>
        <text>The input v4 traffic rate(unit:Mbps).</text>
      </description>
    </leaf>
    <leaf name="in-v4-packet-rate">
      <type name="decimal64">
        <fraction-digits value="4"/>
      </type>
      <description>
        <text>The input v4 packet rate(unit:Kfps).</text>
      </description>
    </leaf>
    <leaf name="out-v4-traffic-rate">
      <type name="decimal64">
        <fraction-digits value="4"/>
      </type>
      <description>
        <text>The output v4 traffic rate(unit:Mbps).</text>
      </description>
    </leaf>
    <leaf name="out-v4-packet-rate">
      <type name="decimal64">
        <fraction-digits value="4"/>
      </type>
      <description>
        <text>The output v4 packet rate(unit:Kfps).</text>
      </description>
    </leaf>
    <leaf name="in-v6-traffic-rate">
      <type name="decimal64">
        <fraction-digits value="4"/>
      </type>
      <description>
        <text>The input v6 traffic rate(unit:Mbps).</text>
      </description>
    </leaf>
    <leaf name="in-v6-packet-rate">
      <type name="decimal64">
        <fraction-digits value="4"/>
      </type>
      <description>
        <text>The input v6 packet rate(unit:Kfps).</text>
      </description>
    </leaf>
    <leaf name="out-v6-traffic-rate">
      <type name="decimal64">
        <fraction-digits value="4"/>
      </type>
      <description>
        <text>The output v6 traffic rate(unit:Mbps).</text>
      </description>
    </leaf>
    <leaf name="out-v6-packet-rate">
      <type name="decimal64">
        <fraction-digits value="4"/>
      </type>
      <description>
        <text>The output v6 packet rate(unit:Kfps).</text>
      </description>
    </leaf>
    <leaf name="input-v4-utilization">
      <type name="decimal64">
        <fraction-digits value="6"/>
        <range value="0..1.000000"/>
      </type>
      <description>
        <text>The input v4 utilization of bandwidith. The maximum decimal precision is 6 and
the specific reporting accuracy shall be subject to project customization.</text>
      </description>
    </leaf>
    <leaf name="output-v4-utilization">
      <type name="decimal64">
        <fraction-digits value="6"/>
        <range value="0..1.000000"/>
      </type>
      <description>
        <text>The output v4 utilization of bandwidith. The maximum decimal precision is 6 and
the specific reporting accuracy shall be subject to project customization.</text>
      </description>
    </leaf>
    <leaf name="input-v6-utilization">
      <type name="decimal64">
        <fraction-digits value="6"/>
        <range value="0..1.000000"/>
      </type>
      <description>
        <text>The input v6 utilization of bandwidith. The maximum decimal precision is 6 and
the specific reporting accuracy shall be subject to project customization.</text>
      </description>
    </leaf>
    <leaf name="output-v6-utilization">
      <type name="decimal64">
        <fraction-digits value="6"/>
        <range value="0..1.000000"/>
      </type>
      <description>
        <text>The output v6 utilization of bandwidith. The maximum decimal precision is 6 and
the specific reporting accuracy shall be subject to project customization.</text>
      </description>
    </leaf>
  </grouping>
  <augment target-node="/oc-if:interfaces/oc-if:interface/oc-if:state">
    <description>
      <text>Additional data for interfaces</text>
    </description>
    <uses name="f-interface-state"/>
  </augment>
  <augment target-node="/oc-if:interfaces/oc-if:interface/oc-if:state/oc-if:counters">
    <description>
      <text>Additional statistics data for interfaces</text>
    </description>
    <uses name="interface-statistics-state"/>
  </augment>
  <augment target-node="/oc-if:interfaces/oc-if:interface/oc-if:subinterfaces/oc-if:subinterface/oc-if:state">
    <description>
      <text>Additional data for subinterfaces</text>
    </description>
    <uses name="s-interface-state"/>
  </augment>
  <augment target-node="/oc-if:interfaces/oc-if:interface/oc-if:subinterfaces/oc-if:subinterface/oc-if:state/oc-if:counters">
    <description>
      <text>Additional statistics data for interfaces</text>
    </description>
    <uses name="interface-statistics-state"/>
  </augment>
</module>
