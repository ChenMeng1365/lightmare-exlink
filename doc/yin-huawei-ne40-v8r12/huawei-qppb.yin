  <rpc name="reset-local-policy-statistics">
    <description>
      <text>To clear local policy statistics.</text>
    </description>
    <ext:node-ref value="/qos:qos/qppb:qppb/qppb:query-statistics"/>
    <input>
      <leaf name="interface-name">
        <type name="leafref">
          <path value="/ifm:ifm/ifm:interfaces/ifm:interface/ifm:name"/>
        </type>
        <mandatory value="true"/>
        <description>
          <text>Interface name.</text>
        </description>
      </leaf>
      <leaf name="direction">
        <type name="qppb-direction-type"/>
        <mandatory value="true"/>
        <description>
          <text>Direction.</text>
        </description>
      </leaf>
      <leaf name="local-id">
        <type name="uint32">
          <range value="1..255"/>
        </type>
        <description>
          <text>Local ID.</text>
        </description>
      </leaf>
    </input>
  </rpc>
  <augment target-node="/qos:qos">
    <description>
      <text>QPPB.</text>
    </description>
    <container name="qppb">
      <description>
        <text>Configure QPPB.</text>
      </description>
      <container name="local-policys">
        <description>
          <text>List of QPPB local policy.</text>
        </description>
        <list name="local-policy">
          <key value="policy-name"/>
          <description>
            <text>Configure QPPB local policy.</text>
          </description>
          <leaf name="policy-name">
            <type name="string">
              <length value="1..31"/>
            </type>
            <description>
              <text>Local-policy name.</text>
            </description>
          </leaf>
          <leaf name="statistic-enable">
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable statistics flag.</text>
            </description>
          </leaf>
          <leaf name="service-class-outbound">
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable outbound service-class flag.</text>
            </description>
          </leaf>
          <container name="qos-local-ids">
            <description>
              <text>List of QoS local ID.</text>
            </description>
            <list name="qos-local-id">
              <key value="local-id"/>
              <max-elements value="255"/>
              <description>
                <text>Configure local ID.</text>
              </description>
              <leaf name="local-id">
                <type name="uint32">
                  <range value="1..255"/>
                </type>
                <description>
                  <text>Local ID value.</text>
                </description>
              </leaf>
              <leaf name="behavior-name">
                <type name="leafref">
                  <path value="/qos:qos/qos:classifier-template/qos:traffic-behaviors/qos:traffic-behavior/qos:name"/>
                </type>
                <mandatory value="true"/>
                <description>
                  <text>Behavior name.</text>
                </description>
              </leaf>
            </list>
          </container>
        </list>
      </container>
      <container name="query-policys">
        <config value="false"/>
        <description>
          <text>List of interface QPPB.</text>
        </description>
        <list name="query-policy">
          <key value="if-name direction"/>
          <config value="false"/>
          <max-elements value="2"/>
          <description>
            <text>Interface QPPB state.</text>
          </description>
          <leaf name="if-name">
            <type name="leafref">
              <path value="/ifm:ifm/ifm:interfaces/ifm:interface/ifm:name"/>
            </type>
            <config value="false"/>
            <description>
              <text>Name of an interface.</text>
            </description>
          </leaf>
          <leaf name="direction">
            <type name="direction-type"/>
            <config value="false"/>
            <description>
              <text>Direction.</text>
            </description>
          </leaf>
          <leaf name="policy-name">
            <type name="leafref">
              <path value="/qos:qos/qppb:qppb/qppb:local-policys/qppb:local-policy/qppb:policy-name"/>
            </type>
            <config value="false"/>
            <description>
              <text>Local-policy name.</text>
            </description>
          </leaf>
          <leaf name="local-id">
            <type name="uint32">
              <range value="1..255"/>
            </type>
            <config value="false"/>
            <description>
              <text>Local ID.</text>
            </description>
          </leaf>
          <leaf name="behavior-name">
            <type name="leafref">
              <path value="/qos:qos/qos:classifier-template/qos:traffic-behaviors/qos:traffic-behavior/qos:name"/>
            </type>
            <config value="false"/>
            <description>
              <text>Behavior name.</text>
            </description>
          </leaf>
          <leaf name="state">
            <type name="behavior-state"/>
            <config value="false"/>
            <description>
              <text>QPPB Policy state.</text>
            </description>
          </leaf>
        </list>
      </container>
      <container name="query-statistics">
        <config value="false"/>
        <description>
          <text>List of QPPB statistics.</text>
        </description>
        <list name="query-statistic">
          <key value="if-name slot-id direction"/>
          <config value="false"/>
          <description>
            <text>QPPB statistics.</text>
          </description>
          <leaf name="if-name">
            <type name="leafref">
              <path value="/ifm:ifm/ifm:interfaces/ifm:interface/ifm:name"/>
            </type>
            <description>
              <text>Outbound interface name.</text>
            </description>
          </leaf>
          <leaf name="slot-id">
            <type name="string">
              <length value="1..127"/>
              <pattern value="(c[cl]c\d+/)?\d+"/>
            </type>
            <config value="false"/>
            <description>
              <text>Slot ID.</text>
            </description>
          </leaf>
          <leaf name="direction">
            <type name="qppb-direction-type"/>
            <config value="false"/>
            <description>
              <text>Direction.</text>
            </description>
          </leaf>
          <leaf name="local-id">
            <type name="uint32">
              <range value="1..255"/>
            </type>
            <config value="false"/>
            <description>
              <text>Local ID.</text>
            </description>
          </leaf>
