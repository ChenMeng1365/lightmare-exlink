                <description>
                  <text>Set l3-VNI label.</text>
                </description>
                <mandatory value="true"/>
                <case name="label">
                  <description>
                    <text>Set the label value.</text>
                  </description>
                  <leaf name="label">
                    <description>
                      <text>Set the label value.</text>
                    </description>
                    <type name="uint32">
                      <range value="0..16777215">
                        <description>
                          <text>The range of l3-vni label is from 0 to 16777215.</text>
                        </description>
                      </range>
                    </type>
                    <mandatory value="true"/>
                  </leaf>
                </case>
                <case name="automatic">
                  <description>
                    <text>Automatically found out VNI-label value.</text>
                  </description>
                  <leaf name="automatic">
                    <description>
                      <text>Automatically found out VNI-label value.</text>
                    </description>
                    <type name="empty"/>
                    <mandatory value="true"/>
                  </leaf>
                  <ncd:revise-date date="2019-09-26"/>
                </case>
