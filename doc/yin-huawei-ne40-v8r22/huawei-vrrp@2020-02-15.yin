  <ext:task-name value="vrrp"/>
  <container name="vrrp">
    <description>
      <text>Virtual router redundancy protocol.</text>
    </description>
    <container name="global">
      <description>
        <text>Configure global VRRP configuration.</text>
      </description>
      <leaf name="gratuitous-arp-enable">
        <type name="boolean"/>
        <default value="true"/>
        <description>
          <text>Enable/disable a device to periodically send gratuitous ARP packets.</text>
        </description>
      </leaf>
      <leaf name="gratuitous-timeout">
        <when condition="../gratuitous-arp-enable='true'"/>
        <type name="uint16">
          <range value="30..1200"/>
        </type>
        <units name="s"/>
        <default value="30"/>
        <description>
          <text>Interval at which gratuitous ARP packets are sent.</text>
        </description>
      </leaf>
      <leaf name="adv-time-learn">
        <type name="boolean"/>
        <default value="true"/>
        <description>
          <text>Enable/disable learning the interval at which VRRP packets are sent.</text>
        </description>
      </leaf>
      <leaf name="ping-enable">
        <type name="boolean"/>
        <default value="true"/>
        <description>
          <text>Enable/disable ping virtual IP address.</text>
        </description>
      </leaf>
      <leaf name="recover-delay">
        <type name="uint16">
          <range value="0..3600"/>
        </type>
        <units name="s"/>
        <default value="0"/>
        <description>
          <text>Delay in recovering after an interface goes up. The delay is used for interface flapping suppression.</text>
        </description>
      </leaf>
      <leaf name="version">
        <type name="vrrp-version"/>
        <default value="v2"/>
        <description>
          <text>Global VRRP version.</text>
        </description>
      </leaf>
      <leaf name="virtual-iproute-enable">
        <type name="boolean"/>
        <default value="true"/>
        <description>
          <text>Enable/disable virtual IP routing function.</text>
        </description>
      </leaf>
      <leaf name="virtual-ospf-enable">
        <when condition="not(../virtual-iproute-enable='false')"/>
        <type name="boolean"/>
        <default value="true"/>
        <description>
          <text>Enable/disable advertising the virtual IP route by OSPF.</text>
        </description>
      </leaf>
      <leaf name="virtual-isis-enable">
        <when condition="not(../virtual-iproute-enable='false')"/>
        <type name="boolean"/>
        <default value="true"/>
        <description>
          <text>Enable/disable advertising the virtual IP route by ISIS.</text>
        </description>
      </leaf>
      <leaf name="virtual-rip-enable">
        <when condition="not(../virtual-iproute-enable='false')"/>
        <type name="boolean"/>
        <default value="true"/>
        <description>
          <text>Enable/disable advertising the virtual IP route by RIP.</text>
        </description>
      </leaf>
      <leaf name="v6-cks-mode">
        <type name="vrrp6cksm-mode"/>
        <default value="include-pseudo-header"/>
        <description>
          <text>VRRP6 checksum mode.</text>
        </description>
      </leaf>
      <leaf name="smooth-switch-enable">
        <when condition="not(../adv-time-learn='false')"/>
        <type name="boolean"/>
        <default value="true"/>
        <description>
          <text>Enable/disable smoothing VRRP switching packets.</text>
        </description>
      </leaf>
      <leaf name="smooth-switch-time">
        <when condition="../smooth-switch-enable='true'"/>
        <type name="uint8">
          <range value="1..255"/>
        </type>
        <units name="s"/>
        <default value="100"/>
        <description>
          <text>Interval carried in smooth VRRP switching packets.</text>
        </description>
      </leaf>
      <leaf name="unicast-port">
        <type name="uint32">
          <range value="1025..65535"/>
        </type>
        <default value="3077"/>
        <description>
          <text>Number of a UDP interface for sending and receiving unicast VRRP packets.</text>
        </description>
      </leaf>
      <leaf name="gratuitous-enhance-enable">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable gratuitous ARP enhance.</text>
        </description>
      </leaf>
      <leaf name="na-enhance-enable">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable vrrp6 na enhance.</text>
        </description>
      </leaf>
      <container name="vrrp-session-car">
        <must condition="pir &gt;= cir and pbs &gt;= cbs"/>
        <description>
          <text>Configure session CAR.</text>
        </description>
        <leaf name="enable">
          <type name="boolean"/>
          <default value="true"/>
          <description>
            <text>Enable/disable session CAR.</text>
          </description>
        </leaf>
        <leaf name="cir">
          <type name="uint32">
            <range value="0..325"/>
          </type>
          <units name="kbit/s"/>
          <default value="4"/>
          <description>
            <text>Specify the session CAR CIR value.</text>
          </description>
        </leaf>
        <leaf name="cbs">
          <type name="uint32">
            <range value="0..30000"/>
          </type>
          <units name="Byte"/>
          <default value="9000"/>
          <description>
            <text>Specify the session CAR CBS value.</text>
          </description>
        </leaf>
        <leaf name="pir">
          <type name="uint32">
            <range value="0..325"/>
          </type>
          <units name="kbit/s"/>
          <default value="5"/>
          <description>
            <text>Specify the session CAR PIR value.</text>
          </description>
        </leaf>
        <leaf name="pbs">
          <type name="uint32">
            <range value="0..30000"/>
          </type>
          <units name="Byte"/>
          <default value="9000"/>
          <description>
            <text>Specify the session CAR PBS value.</text>
          </description>
        </leaf>
      </container>
      <container name="vrrp6-session-car">
        <must condition="pir &gt;= cir and pbs &gt;= cbs"/>
        <description>
          <text>Configure session CAR.</text>
        </description>
        <leaf name="enable">
          <type name="boolean"/>
          <default value="true"/>
          <description>
            <text>Enable/disable session CAR.</text>
          </description>
        </leaf>
        <leaf name="cir">
          <type name="uint32">
            <range value="0..2600"/>
          </type>
          <units name="kbit/s"/>
          <default value="32"/>
          <description>
            <text>Specify the session CAR CIR value.</text>
          </description>
        </leaf>
        <leaf name="cbs">
          <type name="uint32">
            <range value="0..1600000"/>
          </type>
          <units name="Byte"/>
          <default value="72000"/>
          <description>
            <text>Specify the session CAR CBS value.</text>
          </description>
        </leaf>
        <leaf name="pir">
          <type name="uint32">
            <range value="0..2600"/>
          </type>
          <units name="kbit/s"/>
          <default value="40"/>
          <description>
            <text>Specify the session CAR PIR value.</text>
          </description>
        </leaf>
        <leaf name="pbs">
          <type name="uint32">
            <range value="0..1600000"/>
          </type>
          <units name="Byte"/>
          <default value="72000"/>
          <description>
            <text>Specify the session CAR PBS value.</text>
          </description>
        </leaf>
      </container>
    </container>
    <container name="err-packets">
      <config value="false"/>
      <description>
        <text>List of error packets.</text>
      </description>
      <list name="err-packet">
        <key value="index"/>
        <description>
          <text>Statistics of error packet.</text>
        </description>
        <leaf name="index">
          <type name="uint32"/>
          <description>
            <text>Index.</text>
          </description>
        </leaf>
        <leaf name="ifname">
          <type name="pub-type:if-name"/>
          <description>
            <text>Name of the interface.</text>
          </description>
        </leaf>
        <leaf name="receive-time">
          <type name="yang:date-and-time"/>
          <description>
            <text>Receive time.</text>
          </description>
        </leaf>
        <leaf name="reason">
          <type name="string">
            <length value="0..48"/>
          </type>
          <description>
            <text>Error reason.</text>
          </description>
        </leaf>
        <leaf name="buffer">
          <type name="string">
            <length value="0..512"/>
          </type>
          <description>
            <text>Error buffer.</text>
          </description>
        </leaf>
      </list>
    </container>
  </container>
  <augment target-node="/ni:network-instance/ni:instances/ni:instance">
    <description>
      <text>VRRP configurations on L3VPN.</text>
    </description>
    <container name="vrrp">
      <status value="deprecated"/>
      <description>
        <text>Configure VRRP backup groups under network-instance.</text>
      </description>
      <container name="groups">
        <status value="deprecated"/>
        <description>
          <text>List of all VRRP backup groups.</text>
        </description>
        <list name="group">
          <key value="id if-name"/>
          <status value="deprecated"/>
          <description>
            <text>Configure VRRP operations on a device. It lists a series of VRRP operation entries. You can use this table to query the VRRP configuration on the device. In addition, you can create VRRP groups and modify the configuration.</text>
          </description>
          <leaf name="id">
            <type name="uint8">
              <range value="1..255"/>
            </type>
            <status value="deprecated"/>
            <description>
              <text>VRRP backup group ID.</text>
            </description>
          </leaf>
          <leaf name="if-name">
            <type name="leafref">
              <path value="/ifm:ifm/ifm:interfaces/ifm:interface/ifm:name"/>
            </type>
            <must condition="../../../../ni:name=/ifm:ifm/ifm:interfaces/ifm:interface[ifm:name=current()]/ifm:vrf-name"/>
            <status value="deprecated"/>
            <description>
              <text>Name of the outbound interface of a BFD session. This parameter is available only when the type of the detected link is IP or LDP LSP. If only the destination IP address is specified, multi-hop routes are detected. If both the destination IP address and outbound interface name are specified, single-hop links are detected. That is, a fixed route is detected, with the interface where the BFD session resides as the outbound interface and the destination IP address as the next hop address.</text>
            </description>
          </leaf>
          <leaf name="priority">
            <type name="uint8">
              <range value="1..254"/>
            </type>
            <default value="100"/>
            <status value="deprecated"/>
            <description>
              <text>Configured VRRP priority. A larger value indicates a higher priority.</text>
            </description>
          </leaf>
          <leaf name="preempt-mode">
            <type name="boolean"/>
            <default value="true"/>
            <status value="deprecated"/>
            <description>
              <text>Enable/disable whether a high-priority VRRP group can preempt the master state.</text>
            </description>
          </leaf>
          <leaf name="delay-time">
            <when condition="../preempt-mode='true'"/>
            <type name="uint16">
              <range value="0..10800"/>
            </type>
            <units name="s"/>
            <default value="0"/>
            <status value="deprecated"/>
            <description>
              <text>Preemption delay.</text>
            </description>
          </leaf>
          <leaf name="adver-interval">
            <type name="uint32">
              <range value="100..255000"/>
            </type>
            <units name="ms"/>
            <default value="1000"/>
            <status value="deprecated"/>
            <description>
              <text>Configure an interval (in milliseconds) at which the master sends VRRP advertisement packets. This interval must be a multiple of 1000. If the device supports fast advertisement sending, this interval can be set to 100ms 200ms or 500ms.</text>
            </description>
          </leaf>
          <leaf name="isplain">
            <when condition="../authentication-mode='simple'"/>
            <type name="boolean"/>
            <must condition="../authentication-mode='simple' and ../authentication-key"/>
            <default value="false"/>
            <status value="deprecated"/>
            <description>
              <text>Enable/disable selecting the display mode of an authentication key.</text>
            </description>
          </leaf>
          <leaf name="authentication-key">
            <when condition="not(../authentication-mode='none')"/>
            <type name="pub-type:password">
              <length value="1..128"/>
              <pattern value="(\S+.*\S+|\S*)"/>
            </type>
            <must condition="(../authentication-mode='none' and not(../authentication-key)) or (not(../authentication-mode='none') and ../authentication-key)"/>
            <status value="deprecated"/>
            <description>
              <text>Authentication key, which is set based on the authentication type. Value description: If non-authentication is specified, the value is empty. If simple authentication, MD5 authentication, or HMAC-SHA256 authentication is specified, the value is a string with the length ranging from 1 to 8 bytes or a ciphertext password with the corresponding length.</text>
            </description>
          </leaf>
          <leaf name="authentication-mode">
            <type name="vrrpauthen-mode"/>
            <must condition="((not(/ifm:ifm/ifm:interfaces/ifm:interface[ifm:name = current()/../if-name]/ifm:type='LoopBack') and not(../authentication-mode='hmac-sha256')) or ((/ifm:ifm/ifm:interfaces/ifm:interface[ifm:name = current()/../if-name]/ifm:type='LoopBack') and not(../authentication-mode='simple'))) and ((../authentication-mode='none' and not(../authentication-key)) or (not(../authentication-mode='none') and ../authentication-key))"/>
            <default value="none"/>
            <status value="deprecated"/>
            <description>
              <text>Authentication type used for VRRP packet exchanges between VRRP groups. The values are as follows: noAuthentication, simpleTextPassword, md5Authentication. The default value is noAuthentication.</text>
            </description>
          </leaf>
          <leaf name="hold-multi">
            <type name="uint8">
              <range value="3..10"/>
            </type>
            <default value="3"/>
            <status value="deprecated"/>
            <description>
              <text>Set the hello packet sending interval multiplier.</text>
            </description>
          </leaf>
          <leaf name="backup-forward">
            <type name="boolean"/>
            <default value="false"/>
            <status value="deprecated"/>
            <description>
              <text>Enable/disable VRRP backup forward mode.</text>
            </description>
          </leaf>
          <leaf name="fast-resume">
            <when condition="../admin-flag"/>
            <type name="boolean"/>
            <default value="false"/>
            <status value="deprecated"/>
            <description>
              <text>Enable/disable VRRP fast resume mode.</text>
            </description>
          </leaf>
          <leaf name="v3-sendmode">
            <when condition="not(/vrrp:vrrp/vrrp:global/vrrp:version='v2')"/>
            <type name="vrrp-v3-sendmode"/>
            <default value="v3-only"/>
            <status value="deprecated"/>
            <description>
              <text>VRRP advertisement packet mode on the interface.</text>
            </description>
          </leaf>
          <choice name="vrrp-type">
            <status value="deprecated"/>
            <description>
              <text>Type of VRRP.</text>
            </description>
            <case name="member-vrrp">
              <status value="deprecated"/>
              <description>
                <text>Member VRRP.</text>
              </description>
              <leaf name="admin-id">
                <type name="uint8">
                  <range value="1..255"/>
                </type>
                <must condition="../admin-if-name"/>
                <status value="deprecated"/>
                <description>
                  <text>Tracked MVRRP ID.</text>
                </description>
              </leaf>
              <leaf name="admin-if-name">
                <type name="leafref">
                  <path value="/ifm:ifm/ifm:interfaces/ifm:interface/ifm:name"/>
                </type>
                <must condition="../admin-id"/>
                <status value="deprecated"/>
                <description>
                  <text>Tracked MVRRP interface name.</text>
                </description>
              </leaf>
            </case>
            <case name="admin-vrrp">
              <status value="deprecated"/>
              <description>
                <text>Admin VRRP.</text>
              </description>
              <leaf name="admin-flag">
                <type name="empty"/>
                <status value="deprecated"/>
                <description>
                  <text>Management VRRP.</text>
                </description>
              </leaf>
              <leaf name="admin-ignore-ifdown">
                <when condition="../admin-flag"/>
                <type name="boolean"/>
                <default value="false"/>
                <status value="deprecated"/>
                <description>
                  <text>Enable/disable ignoring an interface down event.</text>
                </description>
              </leaf>
            </case>
          </choice>
          <leaf name="type">
            <type name="vrrp-type"/>
            <default value="normal"/>
            <config value="false"/>
            <status value="deprecated"/>
            <description>
              <text>Type of a VRRP backup group.</text>
            </description>
          </leaf>
          <leaf name="state">
            <type name="vrrp-oper-state"/>
            <config value="false"/>
            <status value="deprecated"/>
            <description>
              <text>Running status of the VRRP group. This object has three defined values.
Initialize: The VRRP group is waiting for a startup event.
Backup: The VRRP backup group is monitoring the availability of the master device.
Master: The VRRP backup group is forwarding packets for IP addresses that are associated with this device.</text>
            </description>
          </leaf>
          <leaf name="create-time">
            <type name="yang:date-and-time"/>
            <config value="false"/>
            <status value="deprecated"/>
            <description>
              <text>Time when the VRRP backup group was created.</text>
            </description>
          </leaf>
          <leaf name="last-change-time">
            <type name="yang:date-and-time"/>
            <config value="false"/>
            <status value="deprecated"/>
            <description>
              <text>Time when the status of the VRRP backup group last changed.</text>
            </description>
          </leaf>
          <container name="virtual-ips">
            <status value="deprecated"/>
            <description>
              <text>List of IP address table related to VRRP backup groups. A VRRP backup group can have 0 to 16 extended virtual IP addresses.</text>
            </description>
            <list name="virtual-ip">
              <key value="address"/>
              <max-elements value="16"/>
              <status value="deprecated"/>
              <description>
                <text>Configure virtual IP address configuration. A VRRP backup group can have 0 to 16 extended virtual IP addresses.</text>
              </description>
              <leaf name="address">
                <type name="inet:ipv4-address-no-zone"/>
                <status value="deprecated"/>
                <description>
                  <text>Virtual IP address.</text>
                </description>
              </leaf>
            </list>
          </container>
          <container name="track-interfaces">
            <when condition="not(../admin-id)"/>
            <status value="deprecated"/>
            <description>
              <text>List of interfaces tracked by a VRRP backup group. A VRRP backup group can track a maximum of 16 interfaces.</text>
            </description>
            <list name="track-interface">
              <key value="ifname"/>
              <max-elements value="16"/>
              <status value="deprecated"/>
              <description>
                <text>Configure VRRP backup group tracking interface.</text>
              </description>
              <leaf name="ifname">
                <type name="leafref">
                  <path value="/ifm:ifm/ifm:interfaces/ifm:interface/ifm:name"/>
                </type>
                <status value="deprecated"/>
                <description>
                  <text>Name of the interface tracked by the VRRP backup group.</text>
                </description>
              </leaf>
              <leaf name="track-mode">
                <type name="vrrptrack-mode"/>
                <default value="reduce"/>
                <status value="deprecated"/>
                <description>
                  <text>Mode in which the VRRP backup group tracks the interface.</text>
                </description>
              </leaf>
              <leaf name="priority-change">
                <type name="uint8">
                  <range value="1..255"/>
                </type>
                <default value="10"/>
                <status value="deprecated"/>
                <description>
                  <text>Value by which the priority increases or reduces when the VRRP backup group tracks the interface.</text>
                </description>
              </leaf>
            </list>
          </container>
          <container name="track-bfds">
            <when condition="not(../admin-id)"/>
            <status value="deprecated"/>
            <description>
              <text>List of BFD sessions tracked by the VRRP backup group.</text>
            </description>
            <list name="track-bfd">
              <key value="session-name session-type"/>
              <max-elements value="17"/>
              <status value="deprecated"/>
              <description>
                <text>Configure VRRP backup group tracking BFD session.</text>
              </description>
              <leaf name="session-name">
                <type name="leafref">
                  <path value="/bfd:bfd/bfd:sessions/bfd:session/bfd:name"/>
                </type>
                <status value="deprecated"/>
                <description>
                  <text>Name of the BFD session tracked by the VRRP backup group.</text>
                </description>
              </leaf>
              <leaf name="session-type">
                <type name="vrrp-bfd-type"/>
                <status value="deprecated"/>
                <description>
                  <text>Type of the BFD session tracked by the VRRP backup group.</text>
                </description>
              </leaf>
              <leaf name="track-mode">
                <when condition="../session-type='normal'"/>
                <type name="vrrp-track-mode"/>
                <default value="reduce"/>
                <status value="deprecated"/>
                <description>
                  <text>Mode in which the VRRP backup group tracks the BFD session.</text>
                </description>
              </leaf>
              <leaf name="priority-change">
                <when condition="../session-type='normal'"/>
                <type name="uint8">
                  <range value="1..255"/>
                </type>
                <default value="10"/>
                <status value="deprecated"/>
                <description>
                  <text>Value by which the priority increases or reduces when the VRRP backup group tracks the common BFD session.</text>
                </description>
              </leaf>
              <leaf name="session-id">
                <type name="uint32">
                  <range value="1..32768"/>
                </type>
                <config value="false"/>
                <status value="deprecated"/>
                <description>
                  <text>ID of the BFD session tracked by the VRRP backup group.</text>
                </description>
              </leaf>
              <leaf name="session-state">
                <type name="vrrp-track-bfd-state"/>
                <config value="false"/>
                <status value="deprecated"/>
                <description>
                  <text>State of the BFD session tracked by the VRRP backup group.</text>
                </description>
              </leaf>
            </list>
          </container>
        </list>
      </container>
    </container>
  </augment>
  <augment target-node="/ifm:ifm/ifm:interfaces/ifm:interface">
    <description>
      <text>VRRP configurations on interface.</text>
    </description>
    <container name="vrrp">
      <description>
        <text>Configure virtual router redundancy protocol.</text>
      </description>
      <container name="backup-groups">
        <when condition="../../ip:ipv4/vrrp:vrrp/vrrp:groups/vrrp:group/vrrp:id"/>
        <description>
          <text>Configure VRRP backup groups under interface. The configured attributes apply to all backup groups on the interface.</text>
        </description>
        <leaf name="ttlcheck-flag">
          <type name="boolean"/>
          <default value="true"/>
          <description>
            <text>Enable/disable TTL check which is performed for a received VRRP advertisement packet. The check is successful only when the TTL value is 255. By default, a TTL check is performed.</text>
          </description>
        </leaf>
        <leaf name="recover-delay">
          <type name="uint16">
            <range value="0..3600"/>
          </type>
          <units name="s"/>
          <default value="0"/>
          <description>
            <text>Delay in recovering after an interface goes Up. The delay is used for interface flapping suppression.</text>
          </description>
        </leaf>
        <leaf name="arpsend-simple">
          <when condition="/ifm:ifm/ifm:interfaces/ifm:interface/ethernet:ethernet/ethernet:l3-sub-interface/ethernet:qinq-termination"/>
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable simple send mode for gratuitous ARP packets.</text>
          </description>
        </leaf>
        <choice name="advertise-send-mode">
          <when condition="../../ifm:type='Vlanif'"/>
          <description>
            <text>VRRP backup groups send packets mode.</text>
          </description>
          <case name="all-subvlans">
            <description>
              <text>Packets are sent in all sub-VLANs of the super VLAN.</text>
            </description>
            <leaf name="sendmode-all">
              <type name="empty"/>
              <description>
                <text>Packets are sent in all sub-VLANs of the super VLAN.</text>
              </description>
            </leaf>
          </case>
          <case name="one-subvlan">
            <description>
              <text>Packets are sent in one sub-VLAN of the super VLAN.</text>
            </description>
            <leaf name="send-subvlanid">
              <type name="uint16">
                <range value="1..4094"/>
              </type>
              <description>
                <text>Specifies the ID of a sub-VLAN in which packets are sent. By default, packets are sent from the active VLAN with the minimum ID. This configuration is valid only for a super VLAN.</text>
              </description>
            </leaf>
          </case>
        </choice>
        <container name="statistics">
          <config value="false"/>
          <description>
            <text>Statistics of VRRP on the interface.</text>
          </description>
          <leaf name="ip-protocol-errs">
            <type name="uint32"/>
            <config value="false"/>
            <description>
              <text>Total number of VRRP packets received with an invalid IP protocol value.</text>
            </description>
          </leaf>
          <leaf name="dest-ip-errs">
            <type name="uint32"/>
            <config value="false"/>
            <description>
              <text>Total number of VRRP packets received with an incorrect destination IP address.</text>
            </description>
          </leaf>
          <leaf name="check-sum-errs">
            <type name="uint32"/>
            <config value="false"/>
            <description>
              <text>Total number of VRRP packets received with an invalid VRRP checksum value.</text>
            </description>
          </leaf>
          <leaf name="version-errs">
            <type name="uint32"/>
            <config value="false"/>
            <description>
              <text>Total number of VRRP packets received with an unknown or unsupported version number.</text>
            </description>
          </leaf>
          <leaf name="vrrpid-errs">
            <type name="uint32"/>
            <config value="false"/>
            <description>
              <text>Total number of VRRP packets received with an invalid VRRP ID for this device.</text>
            </description>
          </leaf>
        </container>
      </container>
      <container name="backup-group6s">
        <when condition="../../ip:ipv6/vrrp:vrrp6/vrrp:group6s/vrrp:group6/vrrp:id"/>
        <description>
          <text>Configure VRRP6 backup groups on interfaces, the configured attributes apply to all backup groups on the interface.</text>
        </description>
        <leaf name="hoplimit-check">
          <type name="boolean"/>
          <default value="true"/>
          <description>
            <text>Enable/disable to control whether a hop limit check is performed for a received VRRP6 advertisement packet. The check is successful only when the hop limit value is 255.</text>
          </description>
        </leaf>
        <leaf name="recover-delay">
          <type name="uint16">
            <range value="0..3600"/>
          </type>
          <units name="s"/>
          <default value="0"/>
          <description>
            <text>Delay in recovering after an interface goes Up. The delay is used for interface flapping suppression.</text>
          </description>
        </leaf>
        <leaf name="nd-send-simple">
          <when condition="/ifm:ifm/ifm:interfaces/ifm:interface/ethernet:ethernet/ethernet:l3-sub-interface/ethernet:qinq-termination"/>
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable simple send mode for ND packets.</text>
          </description>
        </leaf>
      </container>
      <container name="track-admins">
        <description>
          <text>List of service interfaces.</text>
        </description>
        <list name="track-admin">
          <key value="id if-name"/>
          <max-elements value="1"/>
          <description>
            <text>Configure service interface.</text>
          </description>
          <leaf name="id">
            <type name="uint8">
              <range value="1..255"/>
            </type>
            <description>
              <text>Tracked MVRRP ID.</text>
            </description>
          </leaf>
          <leaf name="if-name">
            <type name="leafref">
              <path value="/ifm:ifm/ifm:interfaces/ifm:interface/ifm:name"/>
            </type>
            <description>
              <text>Tracked MVRRP interface name.</text>
            </description>
          </leaf>
        </list>
      </container>
    </container>
  </augment>
  <augment target-node="/ifm:ifm/ifm:interfaces/ifm:interface/ip:ipv4">
    <description>
      <text>VRRP configurations on interface.</text>
    </description>
    <container name="vrrp">
      <description>
        <text>Configure all VRRP backup groups.</text>
      </description>
      <container name="groups">
        <description>
          <text>List of all VRRP backup groups.</text>
        </description>
        <list name="group">
          <key value="id"/>
          <description>
            <text>Configure VRRP operations on a device. It lists a series of VRRP operation entries. You can use this table to query the VRRP configuration on the device. In addition, you can create VRRP groups and modify the configuration.</text>
          </description>
          <leaf name="id">
            <type name="uint8">
              <range value="1..255"/>
            </type>
            <description>
              <text>VRRP backup group ID.</text>
            </description>
          </leaf>
          <leaf name="priority">
            <type name="uint8">
              <range value="1..254"/>
            </type>
            <default value="100"/>
            <description>
              <text>Configured VRRP priority. The value ranges from 1 to 254. A larger value indicates a higher priority.</text>
            </description>
          </leaf>
          <container name="preempt">
            <description>
              <text>Configure a higher priority VRRP backup group to preempt a lower priority VRRP master.</text>
            </description>
            <leaf name="preempt-enable">
              <type name="boolean"/>
              <default value="true"/>
              <description>
                <text>Enable/disable whether a high priority VRRP backup group can preempt the master state.</text>
              </description>
            </leaf>
            <leaf name="delay-time">
              <when condition="../preempt-enable='true'"/>
              <type name="uint16">
                <range value="0..10800"/>
              </type>
              <units name="s"/>
              <default value="0"/>
              <description>
                <text>Preemption delay.</text>
              </description>
            </leaf>
          </container>
          <leaf name="adver-interval">
            <type name="uint32">
              <range value="100..255000"/>
            </type>
            <units name="ms"/>
            <default value="1000"/>
            <description>
              <text>Configure an interval at which the master sends VRRP advertisement packets. This interval must be a multiple of 1000. If the device supports fast advertisement sending, this interval can be set to 100ms 200ms or 500 ms.</text>
            </description>
          </leaf>
          <leaf name="authentication-mode">
            <type name="vrrpauthen-mode"/>
            <must condition="((not(../../../../../ifm:type='LoopBack') and not(../authentication-mode='hmac-sha256')) or ((../../../../../ifm:type='LoopBack') and not(../authentication-mode='simple'))) and ((../authentication-mode='none' and not(../authentication-key)) or (not(../authentication-mode='none') and ../authentication-key))"/>
            <default value="none"/>
            <description>
              <text>Authentication type used for VRRP packet exchanges between VRRP groups. The values are as follows: noAuthentication, simpleTextPassword, md5Authentication.</text>
            </description>
          </leaf>
          <leaf name="isplain">
            <when condition="../authentication-mode='simple'"/>
            <type name="boolean"/>
            <must condition="../authentication-mode='simple' and ../authentication-key"/>
            <default value="false"/>
            <description>
              <text>Enable/disable selecting the display mode of an authentication key.</text>
            </description>
          </leaf>
          <leaf name="authentication-key">
            <when condition="not(../authentication-mode='none')"/>
            <type name="pub-type:password">
              <length value="1..128"/>
              <pattern value="(\S+.*\S+|\S*)"/>
            </type>
            <must condition="(../authentication-mode='none' and not(../authentication-key)) or (not(../authentication-mode='none') and ../authentication-key)"/>
            <description>
              <text>Authentication key, which is set based on the authentication type. Value description: If non-authentication is specified, the value is empty. If simple authentication, MD5 authentication, or HMAC-SHA256 authentication is specified, the value is a string with the length ranging from 1 to 8 bytes or a ciphertext password with the corresponding length.</text>
            </description>
          </leaf>
          <leaf name="link-bfddown-num">
            <type name="uint8">
              <range value="1..8"/>
            </type>
            <description>
              <text>Specify the threshold for the number of link BFD sessions in the Down state tracked by the MVRRP backup group.</text>
            </description>
          </leaf>
          <leaf name="hold-multiper">
            <type name="uint8">
              <range value="3..10"/>
            </type>
            <default value="3"/>
            <description>
              <text>Set the hello packet sending interval multiplier.</text>
            </description>
          </leaf>
          <leaf name="backup-forward">
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable VRRP backup forward mode.</text>
            </description>
          </leaf>
          <leaf name="fast-resume">
            <when condition="../admin-flag"/>
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable VRRP fast resume mode.</text>
            </description>
          </leaf>
          <leaf name="v3-sendmode">
            <when condition="/vrrp:vrrp/vrrp:global/vrrp:version='v3'"/>
            <type name="vrrp-v3-sendmode"/>
            <default value="v3-only"/>
            <description>
              <text>VRRP advertisement packet mode on the interface.</text>
            </description>
          </leaf>
          <choice name="vrrp-type">
            <description>
              <text>Type of VRRP.</text>
            </description>
            <case name="member-vrrp">
              <description>
                <text>Member VRRP.</text>
              </description>
              <leaf name="admin-id">
                <type name="uint8">
                  <range value="1..255"/>
                </type>
                <must condition="/ifm:ifm/ifm:interfaces/ifm:interface[ifm:name=current()/../vrrp:admin-if-name]/ip:ipv4/vrrp:vrrp/vrrp:groups/vrrp:group[vrrp:id=current()]/vrrp:admin-flag"/>
                <description>
                  <text>Tracked MVRRP ID.</text>
                </description>
              </leaf>
              <leaf name="admin-if-name">
                <type name="leafref">
                  <path value="/ifm:ifm/ifm:interfaces/ifm:interface/ifm:name"/>
                </type>
                <must condition="/ifm:ifm/ifm:interfaces/ifm:interface[ifm:name=current()]/ip:ipv4/vrrp:vrrp/vrrp:groups/vrrp:group[vrrp:id=current()/../vrrp:admin-id]/vrrp:admin-flag"/>
                <description>
                  <text>Tracked MVRRP interface name.</text>
                </description>
              </leaf>
              <leaf name="unflowdown">
                <when condition="../admin-id and ../admin-if-name"/>
                <type name="boolean"/>
                <default value="false"/>
                <description>
                  <text>Enable/disable the flowdown function for service VRRP.</text>
                </description>
              </leaf>
            </case>
            <case name="admin-vrrp">
              <description>
                <text>Admin VRRP.</text>
              </description>
              <leaf name="admin-flag">
                <type name="empty"/>
                <description>
                  <text>Management VRRP.</text>
                </description>
              </leaf>
              <leaf name="admin-ignore-ifdown">
                <when condition="../admin-flag"/>
                <type name="boolean"/>
                <default value="false"/>
                <description>
                  <text>Enable/disable ignoring an interface down event.</text>
                </description>
              </leaf>
              <leaf name="init-state-holdtime">
                <when condition="../admin-ignore-ifdown='true'"/>
                <type name="uint32">
                  <range value="0..3600"/>
                </type>
                <units name="s"/>
                <description>
                  <text>Hold time of the Initialize state after the management VRRP backup group is configured to ignore an interface down event.</text>
                </description>
              </leaf>
            </case>
          </choice>
          <leaf name="type">
            <type name="vrrp-type"/>
            <default value="normal"/>
            <config value="false"/>
            <description>
              <text>Type of a VRRP backup group.</text>
            </description>
          </leaf>
          <leaf name="virtual-mac">
            <type name="pub-type:mac-address"/>
            <config value="false"/>
            <description>
              <text>Virtual MAC address of the VRRP group.</text>
            </description>
          </leaf>
          <leaf name="state">
            <type name="vrrp-oper-state"/>
            <config value="false"/>
            <description>
              <text>Running status of the VRRP group. This object has three defined values.
Initialize: The VRRP group is waiting for a startup event.
Backup: The VRRP group is monitoring the availability of the master VRRP group.
Master: The VRRP group is forwarding packets for IP addresses that are associated with this device.</text>
            </description>
          </leaf>
          <leaf name="run-priority">
            <type name="uint8">
              <range value="1..255"/>
            </type>
            <config value="false"/>
            <description>
              <text>This object specifies the priority for master VRRP group election. A larger value indicates a higher priority. A priority of 0, although not settable, is sent by the master VRRP group to indicate that this VRRP group has ceased to participate in VRRP and a backup VRRP group should change to a new master. A priority of 255 is used for the device that owns the associated IP addresses.</text>
            </description>
          </leaf>
          <leaf name="master-priority">
            <type name="uint8">
              <range value="0..255"/>
            </type>
            <config value="false"/>
            <description>
              <text>Priority of the master VRRP group in the VRRP backup group.</text>
            </description>
          </leaf>
          <leaf name="master-ip">
            <type name="inet:ipv4-address-no-zone"/>
            <config value="false"/>
            <description>
              <text>IP address of the master router in the VRRP backup group.</text>
            </description>
          </leaf>
          <leaf name="local-if-ip">
            <type name="inet:ipv4-address-no-zone"/>
            <config value="false"/>
            <description>
              <text>Local IP address of the interface in the VRRP backup group.</text>
            </description>
          </leaf>
          <leaf name="create-time">
            <type name="yang:date-and-time"/>
            <config value="false"/>
            <description>
              <text>Time when the VRRP backup group was created.</text>
            </description>
          </leaf>
          <leaf name="last-change-time">
            <type name="yang:date-and-time"/>
            <config value="false"/>
            <description>
              <text>Time when the status of the VRRP backup group last changed.</text>
            </description>
          </leaf>
          <container name="virtual-ips">
            <description>
              <text>List of IP address table related to VRRP groups. A VRRP backup group can have 0 to 16 extended virtual IP addresses.</text>
            </description>
            <list name="virtual-ip">
              <key value="address"/>
              <max-elements value="16"/>
              <description>
                <text>Configure virtual IP address configuration. A VRRP backup group can have 0 to 16 extended virtual IP addresses.</text>
              </description>
              <leaf name="address">
                <type name="inet:ipv4-address-no-zone"/>
                <must condition="/ifm:ifm/ifm:interfaces/ifm:interface/ip:ipv4/vrrp:vrrp/vrrp:groups/vrrp:group/vrrp:virtual-ips/vrrp:virtual-ip[vrrp:address!=current()]"/>
                <description>
                  <text>Virtual IP address.</text>
                </description>
              </leaf>
              <leaf name="mask">
                <type name="inet:ipv4-address-no-zone"/>
                <description>
                  <text>Virtual IP address mask.</text>
                </description>
              </leaf>
              <leaf name="vgmp-role">
                <type name="vgmp-type"/>
                <default value="active"/>
                <description>
                  <text>The configuration state of VRRP.</text>
                </description>
              </leaf>
            </list>
          </container>
          <container name="track-interfaces">
            <when condition="not(../admin-id)"/>
            <description>
              <text>List of interfaces tracked by a VRRP backup group. A VRRP backup group can track a maximum of 16 interfaces.</text>
            </description>
            <list name="track-interface">
              <key value="if-name"/>
              <max-elements value="16"/>
              <description>
                <text>Configure interface tracked by a VRRP backup group.</text>
              </description>
              <leaf name="if-name">
                <type name="leafref">
                  <path value="/ifm:ifm/ifm:interfaces/ifm:interface/ifm:name"/>
                </type>
                <description>
                  <text>Name of the interface tracked by the VRRP backup group.</text>
                </description>
              </leaf>
              <leaf name="priority-change">
                <type name="uint8">
                  <range value="1..255"/>
                </type>
                <mandatory value="true"/>
                <description>
                  <text>Value by which the priority increases or reduces when the VRRP backup group tracks the interface.</text>
                </description>
              </leaf>
              <leaf name="track-mode">
                <type name="vrrptrack-mode"/>
                <default value="reduce"/>
                <description>
                  <text>Mode in which the VRRP backup group tracks the interface.</text>
                </description>
              </leaf>
            </list>
          </container>
          <container name="track-3ahs">
            <when condition="not(../admin-id)"/>
            <description>
              <text>List of EFM sessions tracked by the VRRP backup group.</text>
            </description>
            <list name="track-3ah">
              <key value="track-if-name"/>
              <max-elements value="8"/>
              <description>
                <text>Configure EFM session tracked by the VRRP backup group.</text>
              </description>
              <leaf name="track-if-name">
                <type name="leafref">
                  <path value="/ifm:ifm/ifm:interfaces/ifm:interface/ifm:name"/>
                </type>
                <description>
                  <text>Name of the interface of the EFM session tracked by the VRRP backup group.</text>
                </description>
              </leaf>
            </list>
          </container>
          <container name="track-bfds">
            <when condition="not(../admin-id)"/>
            <description>
              <text>List of BFD sessions tracked by the VRRP backup group.</text>
            </description>
            <list name="track-bfd">
              <key value="session-name session-type"/>
              <max-elements value="17"/>
              <description>
                <text>Configure BFD session tracked by the VRRP backup group.</text>
              </description>
              <leaf name="session-name">
                <type name="leafref">
                  <path value="/bfd:bfd/bfd:sessions/bfd:session/bfd:name"/>
                </type>
                <description>
                  <text>Name of the BFD session tracked by the VRRP backup group.</text>
                </description>
              </leaf>
              <leaf name="session-type">
                <type name="vrrp-bfd-type"/>
                <description>
                  <text>Type of the BFD session tracked by the VRRP backup group.</text>
                </description>
              </leaf>
              <leaf name="priority-change">
                <when condition="../session-type='normal'"/>
                <type name="uint8">
                  <range value="1..255"/>
                </type>
                <mandatory value="true"/>
                <description>
                  <text>Value by which the priority increases or reduces when the VRRP backup group tracks the common BFD session.</text>
                </description>
              </leaf>
              <leaf name="track-mode">
                <when condition="../session-type='normal'"/>
                <type name="vrrp-track-mode"/>
                <default value="reduce"/>
                <description>
                  <text>Mode in which the VRRP backup group tracks the BFD session.</text>
                </description>
              </leaf>
              <leaf name="session-id">
                <type name="uint32">
                  <range value="1..32768"/>
                </type>
                <config value="false"/>
                <description>
                  <text>ID of the BFD session tracked by the VRRP backup group.</text>
                </description>
              </leaf>
              <leaf name="session-state">
                <type name="vrrp-track-bfd-state"/>
                <config value="false"/>
                <description>
                  <text>State of the BFD session tracked by the VRRP backup group.</text>
                </description>
              </leaf>
            </list>
          </container>
          <container name="track-mtgrps">
            <description>
              <text>List of VRRP backup groups to track interface monitoring groups.</text>
            </description>
            <list name="track-mtgrp">
              <key value="monitor-group-name"/>
              <max-elements value="3"/>
              <description>
                <text>Configure VRRP backup groups to track interface monitoring groups.</text>
              </description>
              <leaf name="monitor-group-name">
                <type name="leafref">
                  <path value="/monitor-group:monitor-group/monitor-group:groups/monitor-group:group/monitor-group:name"/>
                </type>
                <description>
                  <text>Name of the interface monitoring group tracked by a VRRP backup group.</text>
                </description>
              </leaf>
              <leaf name="failure-ratio">
                <type name="uint8">
                  <range value="1..100"/>
                </type>
                <mandatory value="true"/>
                <description>
                  <text>Link failure ratio threshold for an interface monitoring group.</text>
                </description>
              </leaf>
              <leaf name="priority-change">
                <when condition="../track-mode='reduce'"/>
                <type name="uint8">
                  <range value="1..255"/>
                </type>
                <mandatory value="true"/>
                <description>
                  <text>Value by which the priority of the VRRP backup group reduces if the status of the tracked interface monitoring group changes to Down.</text>
                </description>
              </leaf>
              <leaf name="track-mode">
                <type name="mtgrptrack-mode"/>
                <default value="reduce"/>
                <description>
                  <text>Mode in which a VRRP backup group tracks an interface monitoring group.</text>
                </description>
              </leaf>
            </list>
          </container>
          <container name="track-nqas">
            <description>
              <text>List of NQA sessions tracked by the VRRP backup group.</text>
            </description>
            <list name="track-nqa">
              <key value="admin-name test-name"/>
              <max-elements value="8"/>
              <description>
                <text>Configure a VRRP backup group to track NQA test instances.</text>
              </description>
              <leaf name="admin-name">
                <type name="leafref">
                  <path value="/nqa:nqa/nqa:instances/nqa:instance/nqa:admin-name"/>
                </type>
                <description>
                  <text>Name of the tracked NQA administrator.</text>
                </description>
              </leaf>
              <leaf name="test-name">
                <type name="leafref">
                  <path value="/nqa:nqa/nqa:instances/nqa:instance/nqa:test-name"/>
                </type>
                <must condition="(/nqa:nqa/nqa:instances/nqa:instance[nqa:test-name=current()]/nqa:icmp-input/nqa:ni-name) and (/nqa:nqa/nqa:instances/nqa:instance[nqa:admin-name=current()/../vrrp:admin-name]/nqa:icmp-input/nqa:ni-name)"/>
                <description>
                  <text>Name of the tracked NQA test instance.</text>
                </description>
              </leaf>
              <leaf name="priority-change">
                <type name="uint8">
                  <range value="1..255"/>
                </type>
                <default value="10"/>
                <description>
                  <text>Value by which the priority reduces when the VRRP backup group tracks NQA.</text>
                </description>
              </leaf>
            </list>
          </container>
          <container name="track-ip-routes">
            <description>
              <text>List of VRRP backup groups to track IP routes.</text>
            </description>
            <list name="track-ip-route">
              <key value="vpn-name ip-address mask-length"/>
              <description>
                <text>Configure VRRP backup groups to track IP route.</text>
              </description>
              <leaf name="vpn-name">
                <type name="leafref">
                  <path value="/ni:network-instance/ni:instances/ni:instance/ni:name"/>
                </type>
                <description>
                  <text>VPN instance name.</text>
                </description>
              </leaf>
              <leaf name="ip-address">
                <type name="inet:ipv4-address-no-zone"/>
                <description>
                  <text>Specifies the destination IP address of a route to be tracked.</text>
                </description>
              </leaf>
              <leaf name="mask-length">
                <type name="uint8">
                  <range value="0..32"/>
                </type>
                <must condition="not(../ip-address!='0.0.0.0' and ../mask-length=0)"/>
                <description>
                  <text>Specifies the destination IP address mask length of a route to be tracked.</text>
                </description>
              </leaf>
              <leaf name="priority-change">
                <type name="uint8">
                  <range value="1..255"/>
                </type>
                <mandatory value="true"/>
                <description>
                  <text>Value by which the priority increases or reduces when the VRRP backup group tracks IP route.</text>
                </description>
              </leaf>
              <leaf name="track-mode">
                <type name="vrrp-track-mode"/>
                <default value="reduce"/>
                <description>
                  <text>Mode in which the VRRP backup group tracks IP route.</text>
                </description>
              </leaf>
            </list>
          </container>
          <container name="statistics">
            <config value="false"/>
            <description>
              <text>This is a table statistics for virtual routers. This table lists statistics of the virtual routers, including the number of times that the status changes to Master and the number of received broadcast packets.</text>
            </description>
            <leaf name="change-master-num">
              <type name="uint32"/>
              <config value="false"/>
              <description>
                <text>Total number of times that this virtual router's state has changed to master.</text>
              </description>
            </leaf>
            <leaf name="send-packets-num">
              <type name="uint32"/>
              <config value="false"/>
              <description>
                <text>Total number of VRRP advertisement packets sent by this virtual router.</text>
              </description>
            </leaf>
            <leaf name="receive-packets-num">
              <type name="uint32"/>
              <config value="false"/>
              <description>
                <text>Total number of VRRP advertisement packets received by this virtual router.</text>
              </description>
            </leaf>
            <leaf name="adver-diff-num">
              <type name="uint32"/>
              <config value="false"/>
              <description>
                <text>Total number of received VRRP advertisement packets with the advertisement interval different from the locally configured one.</text>
              </description>
            </leaf>
            <leaf name="authentication-failures">
              <type name="uint32"/>
              <config value="false"/>
              <description>
                <text>Total number of VRRP packets that fail the authentication check.</text>
              </description>
            </leaf>
            <leaf name="ip-ttl-errors">
              <type name="uint32"/>
              <config value="false"/>
              <description>
                <text>Total number of received VRRP packets with the IP TTL not equal to 255.</text>
              </description>
            </leaf>
            <leaf name="receive-priority-zero">
              <type name="uint32"/>
              <config value="false"/>
              <description>
                <text>Total number of received VRRP packets with a priority of 0.</text>
              </description>
            </leaf>
            <leaf name="send-priority-zero">
              <type name="uint32"/>
              <config value="false"/>
              <description>
                <text>Total number of sent VRRP packets with a priority of 0.</text>
              </description>
            </leaf>
            <leaf name="invalid-type-packets">
              <type name="uint32"/>
              <config value="false"/>
              <description>
                <text>Number of received VRRP packets with an invalid value in the type field.</text>
              </description>
            </leaf>
            <leaf name="address-list-errs">
              <type name="uint32"/>
              <config value="false"/>
              <description>
                <text>Total number of packets received for which the address list does not match the locally configured list for the virtual router.</text>
              </description>
            </leaf>
            <leaf name="invalid-authen-type">
              <type name="uint32"/>
              <config value="false"/>
              <description>
                <text>Total number of packets received with an unknown authentication type.</text>
              </description>
            </leaf>
            <leaf name="authen-type-num">
              <type name="uint32"/>
              <config value="false"/>
              <description>
                <text>Total number of packets received with the authentication type not equal to the locally configured one.</text>
              </description>
            </leaf>
            <leaf name="receive-length-num">
              <type name="uint32"/>
              <config value="false"/>
              <description>
                <text>Total number of packets received with a packet length less than the length of the VRRP header.</text>
              </description>
            </leaf>
            <leaf name="receive-self-num">
              <type name="uint32"/>
              <config value="false"/>
              <description>
                <text>Total number of received packets that were sent by itself.</text>
              </description>
            </leaf>
            <leaf name="send-timeerr-num">
              <type name="uint32"/>
              <config value="false"/>
              <description>
                <text>Total number of sent packets with time error request.</text>
              </description>
            </leaf>
            <leaf name="receive-timeerr-num">
              <type name="uint32"/>
              <config value="false"/>
              <description>
                <text>Total number of received packets with time error request.</text>
              </description>
            </leaf>
            <leaf name="send-reply-num">
              <type name="uint32"/>
              <config value="false"/>
              <description>
                <text>Total number of sent packets with time error reply.</text>
              </description>
            </leaf>
            <leaf name="receive-reply-num">
              <type name="uint32"/>
              <config value="false"/>
              <description>
                <text>Total number of received packets with time error reply.</text>
              </description>
            </leaf>
            <leaf name="receive-attack-num">
              <type name="uint32"/>
              <config value="false"/>
              <description>
                <text>Total number of received attacking packets.</text>
              </description>
            </leaf>
            <leaf name="receive-faillearn-num">
              <type name="uint32"/>
              <config value="false"/>
              <description>
                <text>Total number of received packets and failed to learn interval.</text>
              </description>
            </leaf>
          </container>
        </list>
      </container>
    </container>
  </augment>
  <augment target-node="/ifm:ifm/ifm:interfaces/ifm:interface/ip:ipv6">
    <description>
      <text>VRRP6 configurations on interface.</text>
    </description>
    <container name="vrrp6">
      <description>
        <text>Configure all VRRP6 backup groups.</text>
      </description>
      <container name="group6s">
        <description>
          <text>List of all VRRP6 backup groups.</text>
        </description>
        <list name="group6">
          <must condition="/ifm:ifm/ifm:interfaces/ifm:interface/ip:ipv6/ip:addresses/ip:address/ip:ip"/>
          <key value="id"/>
          <unique tag="link-local-ip"/>
          <description>
            <text>Configure this table for VRRP6 operations on a device. It lists a series of VRRP6 operation entries. You can use this table to query the VRRP6 configuration on the device. In addition, you can create VRRP6 groups and modify the configuration.</text>
          </description>
          <leaf name="id">
            <type name="uint8">
              <range value="1..255"/>
            </type>
            <description>
              <text>VRRP6 backup group ID.</text>
            </description>
          </leaf>
          <leaf name="priority">
            <type name="uint8">
              <range value="1..254"/>
            </type>
            <default value="100"/>
            <description>
              <text>Configured VRRP6 priority. A larger value indicates a higher priority.</text>
            </description>
          </leaf>
          <container name="preempt">
            <description>
              <text>Configure a higher priority VRRP6 backup VRRP6 group to preempt a lower priority VRRP6 master.</text>
            </description>
            <leaf name="preempt-enable">
              <type name="boolean"/>
              <default value="true"/>
              <description>
                <text>Enable/disable the value of this object to control whether a high-priority VRRP6 group can preempt the master state.</text>
              </description>
            </leaf>
            <leaf name="delay-time">
              <when condition="../preempt-enable='true'"/>
              <type name="uint16">
                <range value="0..3600"/>
              </type>
              <units name="s"/>
              <default value="0"/>
              <description>
                <text>Preemption delay.</text>
              </description>
            </leaf>
          </container>
          <leaf name="adver-interval">
            <type name="uint32">
              <range value="1000..40950"/>
            </type>
            <units name="ms"/>
            <default value="1000"/>
            <description>
              <text>Configure an interval (in milliseconds) at which the master sends VRRP advertisement packets. This interval must be a multiple of 10 that is no less than 1000ms. If the device supports fast advertisement sending, this interval can be set to 500ms.</text>
            </description>
          </leaf>
          <leaf name="link-local-ip">
            <type name="inet:ipv6-address-no-zone"/>
            <description>
              <text>Link local IP address of the interface in the VRRP6 backup group.</text>
            </description>
            <ext:operation-exclude value="update">
              <description>
                <text>The link-local-ip can not be modified.</text>
              </description>
            </ext:operation-exclude>
          </leaf>
          <leaf name="vgmp-role">
            <when condition="../link-local-ip"/>
            <type name="vgmp-type"/>
            <default value="active"/>
            <description>
              <text>The configuration state of VRRP6.</text>
            </description>
          </leaf>
          <choice name="vrrp6-type">
            <description>
              <text>Type of a VRRP6 backup group.</text>
            </description>
            <case name="admin">
              <description>
                <text>Admin type of a VRRP6 backup group.</text>
              </description>
              <leaf name="adminvrrp6">
                <type name="empty"/>
                <description>
                  <text>Config admin VRRP6 backup groups.</text>
                </description>
              </leaf>
              <leaf name="admin-ignore-ifdown">
                <when condition="../adminvrrp6"/>
                <type name="boolean"/>
                <default value="false"/>
                <description>
                  <text>Enable/disable MVRRP6 to ignore an interface down event.</text>
                </description>
              </leaf>
              <leaf name="init-state-holdtime">
                <when condition="../admin-ignore-ifdown='true'"/>
                <type name="uint32">
                  <range value="0..3600"/>
                </type>
                <description>
                  <text>Hold time of the initialize state after the management VRRP6 backup group is configured to ignore an interface down event.</text>
                </description>
              </leaf>
            </case>
            <case name="member">
              <description>
                <text>Member type of a VRRP6 backup group.</text>
              </description>
              <leaf name="admin-id">
                <type name="uint8">
                  <range value="1..255"/>
                </type>
                <must condition="/ifm:ifm/ifm:interfaces/ifm:interface[ifm:name=current()/../vrrp:admin-if-name]/ip:ipv6/vrrp:vrrp6/vrrp:group6s/vrrp:group6[vrrp:id=current()]/vrrp:adminvrrp6"/>
                <description>
                  <text>Tracked MVRRP6 ID.</text>
                </description>
              </leaf>
              <leaf name="admin-if-name">
                <type name="leafref">
                  <path value="/ifm:ifm/ifm:interfaces/ifm:interface/ifm:name"/>
                </type>
                <must condition="/ifm:ifm/ifm:interfaces/ifm:interface[ifm:name=current()]/ip:ipv6/vrrp:vrrp6/vrrp:group6s/vrrp:group6[vrrp:id=current()/../vrrp:admin-id]/vrrp:adminvrrp6"/>
                <description>
                  <text>Tracked MVRRP6 interface name.</text>
                </description>
              </leaf>
              <leaf name="unflowdown">
                <when condition="../admin-id and ../admin-if-name"/>
                <type name="boolean"/>
                <default value="false"/>
                <description>
                  <text>Enable/disable the flowdown function for service VRRP6.</text>
                </description>
              </leaf>
              <leaf name="na-copy-to-member">
                <when condition="../unflowdown='true' and ../../../../../ifm:type='Eth-Trunk'"/>
                <type name="boolean"/>
                <default value="false"/>
                <description>
                  <text>Enable/disable VRRP6 NA copy to member mode.</text>
                </description>
              </leaf>
            </case>
          </choice>
          <leaf name="hold-multiper">
            <type name="uint8">
              <range value="3..10"/>
            </type>
            <default value="3"/>
            <description>
              <text>Set the hello packet sending interval multiplier.</text>
            </description>
          </leaf>
          <leaf name="backup-forward">
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable VRRP6 backup forward mode.</text>
            </description>
          </leaf>
          <leaf name="state">
            <type name="vrrp-oper-state"/>
            <config value="false"/>
            <description>
              <text>Running status of the virtual router. This object has three defined values.
Initialize: The virtual router is waiting for a startup event.
Backup: The virtual router is monitoring the availability of the master router.
Master: The virtual router is forwarding packets for IP addresses that are associated with this router.</text>
            </description>
          </leaf>
          <leaf name="run-priority">
            <type name="uint8">
              <range value="1..255"/>
            </type>
            <config value="false"/>
            <description>
              <text>This object specifies the priority for master router election. A larger value indicates a higher priority. A priority of 0, although not settable, is sent by the master router to indicate that this router has ceased to participate in VRRP6 and a backup router should change to a new master. A priority of 255 is used for the device that owns the associated IP addresses.</text>
            </description>
          </leaf>
          <leaf name="master-priority">
            <type name="uint8">
              <range value="1..255"/>
            </type>
            <config value="false"/>
            <description>
              <text>Priority of the master router in the VRRP6 backup group.</text>
            </description>
          </leaf>
          <leaf name="master-ip">
            <type name="inet:ipv6-address-no-zone"/>
            <config value="false"/>
            <description>
              <text>IP address of the master router in the VRRP6 backup group.</text>
            </description>
          </leaf>
          <leaf name="local-ifip">
            <type name="inet:ipv6-address-no-zone"/>
            <config value="false"/>
            <description>
              <text>Local IP address of the interface in the VRRP6 backup group.</text>
            </description>
          </leaf>
          <leaf name="create-time">
            <type name="yang:date-and-time"/>
            <config value="false"/>
            <description>
              <text>Time when the VRRP6 backup group was created.</text>
            </description>
          </leaf>
          <leaf name="last-chage-time">
            <type name="yang:date-and-time"/>
            <config value="false"/>
            <description>
              <text>Time when the status of the VRRP6 backup group last changed.</text>
            </description>
          </leaf>
          <leaf name="run-adver-interval">
            <type name="uint32">
              <range value="100..40950"/>
            </type>
            <units name="ms"/>
            <default value="1000"/>
            <config value="false"/>
            <description>
              <text>Running interval between sending advertisements. Only the master router sends VRRP6 advertisements.</text>
            </description>
          </leaf>
          <leaf name="remain-time">
            <type name="uint16">
              <range value="0..3600"/>
            </type>
            <units name="s"/>
            <default value="0"/>
            <config value="false"/>
            <description>
              <text>Remaining time for preemption.</text>
            </description>
          </leaf>
          <leaf name="virtual-mac">
            <type name="pub-type:mac-address"/>
            <config value="false"/>
            <description>
              <text>Virtual MAC address of the virtual router.</text>
            </description>
          </leaf>
          <leaf name="type">
            <type name="vrrp-type"/>
            <default value="normal"/>
            <config value="false"/>
            <description>
              <text>Type of a VRRP6 backup group.</text>
            </description>
          </leaf>
          <container name="virtual-ips">
            <when condition="../link-local-ip"/>
            <description>
              <text>List of IP addresses related to VRRP6 groups. A VRRP6 backup group can have 0 to 15 extended virtual IP addresses.</text>
            </description>
            <list name="virtual-ip">
              <key value="ip-address"/>
              <max-elements value="15"/>
              <description>
                <text>Configure virtual IP address.</text>
              </description>
              <leaf name="ip-address">
                <type name="inet:ipv6-address-no-zone"/>
                <must condition="/ifm:ifm/ifm:interfaces/ifm:interface/ip:ipv6/vrrp:vrrp6/vrrp:group6s/vrrp:group6/vrrp:virtual-ips/vrrp:virtual-ip[vrrp:ip-address!=current()]"/>
                <description>
                  <text>The link local address must be configured first and the virtual IP address must be in the same network segment as the IP address of the interface.</text>
                </description>
              </leaf>
            </list>
          </container>
          <container name="track-interfaces">
            <when condition="not(../admin-id)"/>
            <description>
              <text>List of interfaces tracked by a VRRP6 backup group. A VRRP6 backup group can track a maximum of 16 interfaces.</text>
            </description>
            <list name="track-interface">
              <key value="track-ifname"/>
              <max-elements value="16"/>
              <description>
                <text>Configure interface tracked by a VRRP6 backup group.</text>
              </description>
              <leaf name="track-ifname">
                <type name="leafref">
                  <path value="/ifm:ifm/ifm:interfaces/ifm:interface/ifm:name"/>
                </type>
                <description>
                  <text>Name of the interface tracked by the VRRP6 backup group.</text>
                </description>
              </leaf>
              <leaf name="priority-change">
                <type name="uint8">
                  <range value="1..255"/>
                </type>
                <mandatory value="true"/>
                <description>
                  <text>Value by which the priority increases or reduces when the VRRP6 backup group tracks the interface.</text>
                </description>
              </leaf>
              <leaf name="track-mode">
                <type name="vrrp-track-mode"/>
                <default value="reduce"/>
                <description>
                  <text>Mode in which the VRRP6 backup group tracks the interface.</text>
                </description>
              </leaf>
            </list>
          </container>
          <container name="track-bfds">
            <description>
              <text>List of BFD sessions tracked by the VRRP6 backup group.</text>
            </description>
            <list name="track-bfd">
              <key value="bfd-sessname"/>
              <max-elements value="17"/>
              <description>
                <text>Configure BFD session tracked by the VRRP6 backup group.</text>
              </description>
              <leaf name="bfd-sessname">
                <type name="leafref">
                  <path value="/bfd:bfd/bfd:sessions/bfd:session/bfd:name"/>
                </type>
                <description>
                  <text>Name of the BFD session tracked by the VRRP6 backup group.</text>
                </description>
              </leaf>
              <leaf name="bfd-type">
                <type name="vrrp-bfd-type"/>
                <mandatory value="true"/>
                <description>
                  <text>Type of the BFD session tracked by the VRRP6 backup group.</text>
                </description>
              </leaf>
              <leaf name="priority-change">
                <when condition="../bfd-type='normal'"/>
                <type name="uint8">
                  <range value="1..255"/>
                </type>
                <mandatory value="true"/>
                <description>
                  <text>Value by which the priority increases or reduces when the VRRP6 backup group tracks the common BFD session.</text>
                </description>
              </leaf>
              <leaf name="track-mode">
                <when condition="../bfd-type='normal'"/>
                <type name="vrrp-track-mode"/>
                <default value="reduce"/>
                <description>
                  <text>Mode in which the VRRP6 backup group tracks the BFD session.</text>
                </description>
              </leaf>
              <leaf name="bfd-sessid">
                <type name="uint32">
                  <range value="1..32768"/>
                </type>
                <config value="false"/>
                <description>
                  <text>ID of the BFD session tracked by the VRRP6 backup group.</text>
                </description>
              </leaf>
            </list>
          </container>
          <container name="statistics">
            <config value="false"/>
            <description>
              <text>This is a table statistics for virtual routers. This table lists statistics of the virtual routers, including the number of times that the status changes to master and the number of received broadcast packets.</text>
            </description>
            <leaf name="change-master-num">
              <type name="uint32"/>
              <config value="false"/>
              <description>
                <text>Total number of times that this virtual router's state has changed to master.</text>
              </description>
            </leaf>
            <leaf name="send-adver">
              <type name="uint32"/>
              <config value="false"/>
              <description>
                <text>Total number of VRRP6 advertisement packets sent by this virtual router.</text>
              </description>
            </leaf>
            <leaf name="send-packets-num">
              <type name="uint32"/>
              <config value="false"/>
              <description>
                <text>Total number of VRRP6 advertisement packets received by this virtual router.</text>
              </description>
            </leaf>
            <leaf name="adver-diff-num">
              <type name="uint32"/>
              <config value="false"/>
              <description>
                <text>Total number of received VRRP6 advertisement packets with the advertisement interval different from the locally configured one.</text>
              </description>
            </leaf>
            <leaf name="authentication-failures">
              <type name="uint32"/>
              <config value="false"/>
              <description>
                <text>Total number of VRRP6 packets that fail the authentication check.</text>
              </description>
            </leaf>
            <leaf name="ip-ttl-errors">
              <type name="uint32"/>
              <config value="false"/>
              <description>
                <text>Total number of received VRRP6 packets with the IP TTL not equal to 255.</text>
              </description>
            </leaf>
            <leaf name="receive-priority-zero">
              <type name="uint32"/>
              <config value="false"/>
              <description>
                <text>Total number of received VRRP6 packets with a priority of 0.</text>
              </description>
            </leaf>
            <leaf name="send-priority-zero">
              <type name="uint32"/>
              <config value="false"/>
              <description>
                <text>Total number of sent VRRP6 packets with a priority of 0.</text>
              </description>
            </leaf>
            <leaf name="invalid-type-packets">
              <type name="uint32"/>
              <config value="false"/>
              <description>
                <text>Number of received VRRP6 packets with an invalid value in the type field.</text>
              </description>
            </leaf>
            <leaf name="address-list-errs">
              <type name="uint32"/>
              <config value="false"/>
              <description>
                <text>Total number of packets received for which the address list does not match the locally configured list for the virtual router.</text>
              </description>
            </leaf>
            <leaf name="invalid-authen-type">
              <type name="uint32"/>
              <config value="false"/>
              <description>
                <text>Total number of packets received with an unknown authentication type.</text>
              </description>
            </leaf>
            <leaf name="authen-type-num">
              <type name="uint32"/>
              <config value="false"/>
              <description>
                <text>Total number of packets received with the authentication type not equal to the locally configured one.</text>
              </description>
            </leaf>
            <leaf name="receive-length-num">
              <type name="uint32"/>
              <config value="false"/>
              <description>
                <text>Total number of packets received with a packet length less than the length of the VRRP6 header.</text>
              </description>
            </leaf>
            <leaf name="receive-self-num">
              <type name="uint32"/>
              <config value="false"/>
              <description>
                <text>Total number of received packets that were sent by itself.</text>
              </description>
            </leaf>
            <leaf name="receive-attack-num">
              <type name="uint32"/>
              <config value="false"/>
              <description>
                <text>Total number of received attacking packets.</text>
              </description>
            </leaf>
            <leaf name="receive-faillearn-num">
              <type name="uint32"/>
              <config value="false"/>
              <description>
                <text>Total number of received packets and failed to learn interval.</text>
              </description>
            </leaf>
          </container>
        </list>
      </container>
    </container>
  </augment>
</module>
