        xmlns:ext="urn:huawei:yang:huawei-extension"
        xmlns:yang="urn:ietf:params:xml:ns:yang:ietf-yang-types">
  <yang-version value="1"/>
  <namespace uri="urn:huawei:yang:huawei-wdm"/>
  <prefix value="wdm"/>
  <import module="huawei-extension">
    <prefix value="ext"/>
  </import>
  <import module="ietf-yang-types">
    <prefix value="yang"/>
  </import>
  <organization>
    <text>Huawei Technologies Co., Ltd.</text>
  </organization>
  <contact>
    <text>Huawei Industrial Base
Bantian, Longgang
Shenzhen 518129
People's Republic of China
Website: http://www.huawei.com
Email: support@huawei.com</text>
  </contact>
  <description>
    <text>Configuration of WDM port.</text>
  </description>
  <revision date="2019-07-20">
    <description>
      <text>Initial revision.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <typedef name="wdm-transalarm-type">
    <type name="enumeration">
      <enum name="ODU-AIS">
        <value value="22"/>
        <description>
          <text>ODU-AIS.</text>
        </description>
      </enum>
      <enum name="ODU-LCK">
        <value value="23"/>
        <description>
          <text>ODU-LCK.</text>
        </description>
      </enum>
      <enum name="ODU-OCI">
        <value value="24"/>
        <description>
          <text>ODU-OCI.</text>
        </description>
      </enum>
      <enum name="OTU-AIS">
        <value value="25"/>
        <description>
          <text>OTU-AIS.</text>
        </description>
      </enum>
      <enum name="OTU-LOM">
        <value value="26"/>
        <description>
          <text>OTU-LOM.</text>
        </description>
      </enum>
      <enum name="OTU-SD-BER">
        <value value="27"/>
        <description>
          <text>OTU-SD-BER.</text>
        </description>
      </enum>
      <enum name="OTU-SF-BER">
        <value value="28"/>
        <description>
          <text>OTU-SF-BER.</text>
        </description>
      </enum>
      <enum name="PM-BDI">
        <value value="29"/>
        <description>
          <text>PM-BDI.</text>
        </description>
      </enum>
      <enum name="PM-TIM">
        <value value="30"/>
        <description>
          <text>PM-TIM.</text>
        </description>
      </enum>
      <enum name="R-LOF">
        <value value="31"/>
        <description>
          <text>R-LOF.</text>
        </description>
      </enum>
      <enum name="R-LOS">
        <value value="32"/>
        <description>
          <text>R-LOS.</text>
        </description>
      </enum>
      <enum name="R-OOF">
        <value value="33"/>
        <description>
          <text>R-OOF.</text>
        </description>
      </enum>
      <enum name="SM-BDI">
        <value value="34"/>
        <description>
          <text>SM-BDI.</text>
        </description>
      </enum>
      <enum name="SM-IAE">
        <value value="35"/>
        <description>
          <text>SM-IAE.</text>
        </description>
      </enum>
      <enum name="SM-TIM">
        <value value="36"/>
        <description>
          <text>SM-TIM.</text>
        </description>
      </enum>
      <enum name="PREFEC-TCA">
        <value value="37"/>
        <description>
          <text>PREFEC-TCA.</text>
        </description>
      </enum>
      <enum name="ODU-SD-BER">
        <value value="45"/>
        <description>
          <text>ODU-SD-BER.</text>
        </description>
      </enum>
