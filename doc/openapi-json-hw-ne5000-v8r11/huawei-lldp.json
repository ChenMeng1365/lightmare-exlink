    "definitions": {
        "lldp_schema": {
            "description": "Link layer discovery protocol.",
            "properties": {
                "global-attribute": {
                    "description": "Configure LLDP global attribute.",
                    "properties": {
                        "message-transmission-interval": {
                            "type": "integer",
                            "format": "int32",
                            "description": "Interval at which LLDP messages are transmitted."
                        },
                        "message-transmission-delay": {
                            "type": "integer",
                            "format": "int32",
                            "description": "Time between two successive LLDP messages."
                        },
                        "message-transmission-hold-multiplier": {
                            "type": "integer",
                            "format": "int32",
                            "description": "Multiplier on the message transmission interval that determines the actual TTL value used in an LLDPDU."
                        },
                        "restart-delay": {
                            "type": "integer",
                            "format": "int32",
                            "description": "Delay from when LLDP interface status becomes disabled until re-initialization will be attempted."
                        },
                        "trap-interval": {
                            "type": "integer",
                            "format": "int32",
                            "description": "LLDP trap notification interval."
                        },
                        "fast-count": {
                            "type": "integer",
                            "format": "int32",
                            "description": "Number of rapidly transmitted LLDP messages."
                        },
                        "managment-address": {
                            "type": "string",
                            "x-path": "/ifm:ifm/ifm:interfaces/ifm:interface/ip:ipv4/ip:addresses/ip:address/ip:ip",
                            "description": "IP address for network management, which must exist in the device."
                        },
                        "mdn-status": {
                            "type": "string",
                            "enum": [
                                "rx-only",
                                "disabled"
                            ],
                            "description": "MDN status."
                        },
                        "trap-enable": {
                            "type": "string",
                            "enum": [
                                "enabled",
                                "disabled"
                            ],
                            "description": "Enable the LLDP neighbor change trap function."
                        }
                    }
                }
            }
        },
