<?xml version="1.0" encoding="UTF-8"?>
<module name="zxr10-monitor"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:monitor="http://www.zte.com.cn/zxr10/netconf/schema/rosng/monitor"
        xmlns:ncd="http://www.zte.com.cn/zxr10/netconf/schema/rosng/netconf-defs">
  <namespace uri="http://www.zte.com.cn/zxr10/netconf/schema/rosng/monitor"/>
  <prefix value="monitor"/>
  <yang-version value="1"/>
  <import module="zxr10-netconf-defs">
    <prefix value="ncd"/>
  </import>
  <organization>
    <text>ZTE Corporation</text>
  </organization>
  <contact>
    <text>ZTE Corporation  
   Nanjing Institute of ZTE Corporation 
   No.68 Zijinghua Rd. Yuhuatai District, Nanjing, China 
   Tel: +86-25-52870000</text>
  </contact>
  <revision date="2021-01-05">
    <description>
      <text>1 Add default-vxlans with feature default-vxlan-feature to support destination VXLAN:
/configuration/monitor/sessions/session/default-vxlans;
2 Add remote-monitor with feature remote-monitor-feature to support RSPAN:
/configuration/monitor/sessions/session/default-interfaces/default-interface/remote-monitor;
3 Add encap-remote-monitor with feature encap-remote-monitor-feature to support ERSPAN:
/configuration/monitor/sessions/session/default-gres/default-gre/encap-remote-monitor;
4 Add switchvlan-sources with feature switchvlan-source-feature to support switchvlan as monitor source:
/configuration/monitor/sessions/session/sources/switchvlan-sources.
5 Add cpu-sources with feature cpu-source-feature to support CPU monitor source:
/configuration/monitor/sessions/session/sources/cpu-sources.</text>
    </description>
  </revision>
  <revision date="2020-06-15">
    <description>
      <text>Support description for seesion:
/configuration/monitor/sessions/session/description.</text>
    </description>
  </revision>
  <revision date="2020-04-13">
    <description>
      <text>1. Support multi-acl-destinations with feature multi-acl-feature:
/configuration/monitor/sessions/session/multi-acl-rules;
2. Support default-interfaces with feature default-interface-feature:
/configuration/monitor/sessions/session/default-interfaces.
3. Add enumeration value ipv4-mixed-acl:
/configuration/monitor/sessions/session/acl-rules/acl-rule/acl-type</text>
    </description>
  </revision>
  <revision date="2017-05-16">
    <description>
      <text>add destination of gre.</text>
    </description>
  </revision>
  <revision date="2017-04-14">
    <description>
      <text>1.add vpws case to /configuration/monitor/sessions/session/acl-rules/acl-rule/destination-type/.
2.add default-vpwss container to /configuration/monitor/sessions/session/. 
3.add default-observers container to /configuration/monitor/sessions/session/. 
4.add rule-observers container to /configuration/monitor/sessions/session/.
5.add flow-type leaf to /configuration/monitor/sessions/session/sources/interface-source/.
6.add vpws-source list to /configuration/monitor/sessions/session/sources/.</text>
    </description>
  </revision>
  <revision date="2015-07-13">
    <description>
      <text>Initial version</text>
    </description>
  </revision>
  <feature name="multi-acl-feature">
    <description>
      <text>Mark multiple ACL destination.</text>
    </description>
  </feature>
  <feature name="default-interface-feature">
    <description>
      <text>Mark multiple default destination.</text>
    </description>
  </feature>
  <feature name="default-vxlan-feature">
    <description>
      <text>Mark default destination VXLAN.</text>
    </description>
  </feature>
  <feature name="switchvlan-source-feature">
    <description>
      <text>Mark switchvlan source.</text>
    </description>
  </feature>
  <feature name="remote-monitor-feature">
    <description>
      <text>Mark remote monitor.</text>
    </description>
  </feature>
  <feature name="encap-remote-monitor-feature">
    <description>
      <text>Mark encapsulated remote monitor.</text>
    </description>
  </feature>
  <feature name="cpu-source-feature">
    <description>
      <text>Mark CPU source.</text>
    </description>
  </feature>
