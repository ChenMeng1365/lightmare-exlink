    <container name="accounting-copy-groups">
      <description>
        <text>List of an accounting copy group and enter the accounting copy group.</text>
      </description>
      <list name="accounting-copy-group">
        <key value="name"/>
        <max-elements value="1000"/>
        <description>
          <text>Configure an accounting copy group and enter the accounting copy group.</text>
        </description>
        <leaf name="name">
          <type name="string">
            <length value="1..32"/>
            <pattern value="[a-zA-Z0-9._-]{1,32}"/>
          </type>
          <must condition="../name != '-' and ../name != '--'"/>
          <description>
            <text>Specifies the name of an accounting copy group.</text>
          </description>
        </leaf>
        <container name="accounting-copy-servers">
          <description>
            <text>List of an accounting copy RADIUS server and accounting interval.</text>
          </description>
          <list name="accounting-copy-server">
            <key value="radius-server-group-name"/>
            <max-elements value="8"/>
            <description>
              <text>Configure an accounting copy RADIUS server and accounting interval.</text>
            </description>
            <leaf name="radius-server-group-name">
              <type name="leafref">
                <path value="/radius:radius/radius:radius-server-groups/radius:radius-server-group/radius:name"/>
              </type>
              <description>
                <text>Specifies the name of an accounting copy RADIUS server.</text>
              </description>
            </leaf>
            <leaf name="accounting-interval">
              <type name="uint32">
                <range value="1..65535"/>
              </type>
              <units name="min"/>
              <description>
                <text>Specifies the interval of RADIUS server group send accounting packets. If the accounting interval is less than 1 hour, the system pressure may increase, causing the CPU usage to increase.</text>
              </description>
            </leaf>
          </list>
        </container>
      </list>
    </container>
