<?xml version="1.0" encoding="UTF-8"?>
<submodule name="huawei-l3vpn-type"
           xmlns="urn:ietf:params:xml:ns:yang:yin:1"
           xmlns:l3vpn="urn:huawei:yang:huawei-l3vpn">
  <belongs-to module="huawei-l3vpn">
    <prefix value="l3vpn"/>
  </belongs-to>
  <organization>
    <text>Huawei Technologies Co., Ltd.</text>
  </organization>
  <contact>
    <text>Huawei Industrial Base
Bantian, Longgang
Shenzhen 518129
People's Republic of China
Website: http://www.huawei.com
Email: support@huawei.com</text>
  </contact>
  <description>
    <text>This YANG module defines a YANG data model to configure and manage
Layer3 VPNs. It augments the huawei-network-instance yang model
to add L3VPN specific configuration.</text>
  </description>
  <revision date="2020-03-13">
    <description>
      <text>The YANG descriptions are modified according to new standards.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <revision date="2019-04-27">
    <description>
      <text>Initial revision.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <typedef name="vpn-target-type">
    <type name="enumeration">
      <enum name="export-extcommunity">
        <value value="2"/>
        <description>
          <text>Specifies the value of the extended community attribute
of the route from an outbound interface to the destination
VPN.</text>
        </description>
      </enum>
      <enum name="import-extcommunity">
        <value value="3"/>
        <description>
          <text>Receives routes that carry the specified extended community attribute value.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>The type of vpn target.</text>
    </description>
  </typedef>
  <typedef name="label-mode-type">
    <type name="enumeration">
      <enum name="per-instance">
        <value value="1"/>
        <description>
          <text>Labels are assigned to VPN instances in 'one label per
instance' mode, that is, all routes in a VPN instance
are assigned with the same label.</text>
        </description>
      </enum>
      <enum name="per-route">
        <value value="0"/>
        <description>
          <text>Labels are assigned to routes in the 'one label per route' mode,
that is, each route is assigned with one label.</text>
        </description>
      </enum>
      <enum name="per-nexthop">
        <value value="2"/>
        <description>
          <text>Labels are assigned to routes in the 'one label per nexthop'
mode, that is, all routes with the same nexthop are assigned
with the same label.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>The mode of l3vpncommonLabel.</text>
    </description>
  </typedef>
  <typedef name="af-type">
    <type name="enumeration">
      <enum name="ipv4-unicast">
        <value value="1"/>
        <description>
          <text>IPv4 unicast address family.</text>
        </description>
      </enum>
      <enum name="ipv6-unicast">
        <value value="5"/>
        <description>
          <text>IPv6 unicast address family.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>The type of l3vpncommonL3vpnPrefix.</text>
    </description>
  </typedef>
  <typedef name="vpn-status-type">
    <type name="enumeration">
      <enum name="up">
        <value value="1"/>
        <description>
          <text>VPN Instance status Up.</text>
        </description>
      </enum>
      <enum name="down">
        <value value="2"/>
        <description>
          <text>VPN Instance status Down.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>The status of the vpn-instance.</text>
    </description>
  </typedef>
  <typedef name="lsp-operation-type">
    <type name="enumeration">
      <enum name="pop">
        <value value="0"/>
        <description>
          <text>Label Operation Mode: POP.</text>
        </description>
      </enum>
      <enum name="pop-go">
        <value value="1"/>
        <description>
          <text>Label Operation Mode: POPGO.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>The label operation mode of the l3vpncommon.</text>
    </description>
  </typedef>
</submodule>
