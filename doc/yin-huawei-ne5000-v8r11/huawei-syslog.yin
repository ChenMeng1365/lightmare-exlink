  <ext:task-name value="log"/>
  <container name="syslog">
    <description>
      <text>System log.</text>
    </description>
    <container name="servers">
      <description>
        <text>List of syslog servers.</text>
      </description>
      <list name="server">
        <key value="ip-type ipaddress is-default-vpn vrf-name"/>
        <max-elements value="8"/>
        <description>
          <text>Configure attributes of log server.</text>
        </description>
        <leaf name="ip-type">
          <type name="address-family-type"/>
          <description>
            <text>Log server address type, IPv4 or IPv6.</text>
          </description>
        </leaf>
        <leaf name="ipaddress">
          <type name="inet:ip-address-no-zone"/>
          <description>
            <text>Log server address, IPv4 or IPv6 type.</text>
          </description>
        </leaf>
        <leaf name="is-default-vpn">
          <type name="boolean"/>
          <description>
            <text>Enable/disable the default VPN.</text>
          </description>
        </leaf>
        <leaf name="vrf-name">
          <type name="leafref">
            <path value="/ni:network-instance/ni:instances/ni:instance/ni:name"/>
          </type>
          <must condition="not(../is-default-vpn='true') or (../is-default-vpn='true' and ../vrf-name='_public_')"/>
          <description>
            <text>VPN name on a log server.</text>
          </description>
        </leaf>
        <leaf name="host-name">
          <type name="string">
            <length value="1..31"/>
          </type>
          <mandatory value="true"/>
          <description>
            <text>Configure host name of log server.</text>
          </description>
        </leaf>
        <leaf name="is-activated">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable the log host.</text>
          </description>
        </leaf>
        <leaf name="level">
          <type name="log-level"/>
          <default value="debugging"/>
          <description>
            <text>Level of logs saved on a log server.</text>
          </description>
          <ext:support-filter value="true"/>
        </leaf>
        <leaf name="port">
          <type name="uint32">
            <range value="1..65535"/>
          </type>
          <default value="514"/>
          <description>
            <text>Number of a port recving logs.</text>
          </description>
          <ext:support-filter value="true"/>
        </leaf>
        <leaf name="facility">
          <type name="loghost-facility"/>
          <default value="local7"/>
          <description>
            <text>Log record tool.</text>
          </description>
          <ext:support-filter value="true"/>
        </leaf>
        <leaf name="channel-id">
          <type name="leafref">
            <path value="/syslog:syslog/syslog:info-center-channels/syslog:info-center-channel/syslog:channel-id"/>
          </type>
          <default value="2"/>
          <description>
            <text>Channel number.</text>
          </description>
          <ext:support-filter value="true"/>
        </leaf>
        <leaf name="channel-name">
          <type name="leafref">
            <path value="/syslog:syslog/syslog:info-center-channels/syslog:info-center-channel/syslog:channel-name"/>
          </type>
          <default value="loghost"/>
          <description>
            <text>Channel name.</text>
          </description>
        </leaf>
        <leaf name="timestamp">
          <type name="timestamp-format"/>
          <default value="UTC"/>
          <description>
            <text>Log server timestamp.</text>
          </description>
          <ext:support-filter value="true"/>
        </leaf>
        <leaf name="transport-mode">
          <type name="transport-mode-type"/>
          <default value="udp"/>
          <description>
            <text>Transport mode.</text>
          </description>
          <ext:support-filter value="true"/>
        </leaf>
        <leaf name="ssl-policy-name">
          <type name="leafref">
            <path value="/ssl:ssl/ssl:ssl-policys/ssl:ssl-policy/ssl:policy-name"/>
          </type>
          <must condition="../transport-mode='tcp'"/>
          <description>
            <text>SSL policy name.</text>
          </description>
        </leaf>
        <leaf name="source-ip">
          <ext:support-filter value="true"/>
          <type name="inet:ip-address-no-zone"/>
          <description>
            <text>Syslog source IP address.</text>
          </description>
        </leaf>
        <leaf name="dns-identifier-name">
          <type name="string">
            <length value="1..255"/>
          </type>
          <must condition="../transport-mode='tcp' and ../ssl-policy-name"/>
          <description>
            <text>DNS Identifier Name.</text>
          </description>
        </leaf>
        <leaf name="is-brief-format">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable syslog Brief Format.</text>
          </description>
        </leaf>
        <leaf name="is-security-only">
          <type name="boolean"/>
          <must condition="../is-security-only='false' or (../is-security-only='true' and ../ssl-policy-name)"/>
          <default value="false"/>
          <description>
            <text>Enable/disable security log server.</text>
          </description>
        </leaf>
        <leaf name="facility-mask">
          <type name="bits">
            <bit name="local0">
              <description>
                <text>The bit can be used to configure local0 facility.</text>
              </description>
            </bit>
            <bit name="local1">
              <description>
                <text>The bit can be used to configure local1 facility.</text>
              </description>
            </bit>
            <bit name="local2">
              <description>
                <text>The bit can be used to configure local2 facility.</text>
              </description>
            </bit>
            <bit name="local3">
              <description>
                <text>The bit can be used to configure local3 facility.</text>
              </description>
            </bit>
            <bit name="local4">
              <description>
                <text>The bit can be used to configure local4 facility.</text>
              </description>
            </bit>
            <bit name="local5">
              <description>
                <text>The bit can be used to configure local5 facility.</text>
              </description>
            </bit>
            <bit name="local6">
              <description>
                <text>The bit can be used to configure local6 facility.</text>
              </description>
            </bit>
            <bit name="local7">
              <description>
                <text>The bit can be used to configure local7 facility.</text>
              </description>
            </bit>
          </type>
          <default value="local0 local1 local2 local3 local4 local5 local6 local7"/>
          <description>
            <text>Configure facility of the syslog host.</text>
          </description>
        </leaf>
        <leaf name="severity-mask">
          <type name="bits">
            <bit name="emergency">
              <description>
                <text>The bit can be used to configure emergency severity.</text>
              </description>
            </bit>
            <bit name="alert">
              <description>
                <text>The bit can be used to configure alert severity.</text>
              </description>
            </bit>
            <bit name="critical">
              <description>
                <text>The bit can be used to configure critical severity.</text>
              </description>
            </bit>
            <bit name="error">
              <description>
                <text>The bit can be used to configure error severity.</text>
              </description>
            </bit>
            <bit name="warning">
              <description>
                <text>The bit can be used to configure warning severity.</text>
              </description>
            </bit>
            <bit name="notice">
              <description>
                <text>The bit can be used to configure notice severity.</text>
              </description>
            </bit>
            <bit name="informational">
              <description>
                <text>The bit can be used to configure informational severity.</text>
              </description>
            </bit>
            <bit name="debug">
              <description>
                <text>The bit can be used to configure debug severity.</text>
              </description>
            </bit>
          </type>
          <default value="emergency alert critical error warning notice informational debug"/>
          <description>
            <text>Configure severity of logs received by the syslog host.</text>
          </description>
        </leaf>
      </list>
    </container>
    <container name="global-parameter">
      <description>
        <text>Configure global parameter.</text>
      </description>
      <leaf name="info-center-enable">
        <type name="boolean"/>
        <default value="true"/>
        <description>
          <text>Enable/disable the info-center function.</text>
        </description>
      </leaf>
      <leaf name="buffer-size">
        <type name="int32">
