     |     +--ro ptp-port-pkt-stat
     |     |  +--ro port-recv-announce?            uint32
     |     |  +--ro port-recv-sync?                uint32
     |     |  +--ro port-recv-req?                 uint32
     |     |  +--ro port-recv-resp?                uint32
     |     |  +--ro port-recv-pdresp-followup?     uint32
     |     |  +--ro port-recv-followup?            uint32
     |     |  +--ro port-send-announce?            uint32
     |     |  +--ro port-send-sync?                uint32
     |     |  +--ro port-send-req?                 uint32
     |     |  +--ro port-send-resp?                uint32
     |     |  +--ro port-send-followup?            uint32
     |     |  +--ro port-send-pdresp-followup?     uint32
