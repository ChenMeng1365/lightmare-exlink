                      <description>
                        <text>Select PHB value as match criteria.</text>
                      </description>
                      <leaf name="service-class">
                        <description>
                          <text>Select service class of PHB info, see below:
  be  Best effort
  af1 Assured forwarding class 1
  af2 Assured forwarding class 2
  af3 Assured forwarding class 3
  af4 Assured forwarding class 4
  ef  Expedited forward
  cs6 Internetwork control service class
  cs7 Network control service class</text>
                        </description>
                        <type name="qos-service-class"/>
                        <mandatory value="true"/>
                      </leaf>
                    </case>
