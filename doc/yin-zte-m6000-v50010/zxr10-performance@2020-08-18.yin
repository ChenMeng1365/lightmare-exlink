  <container name="operation">
    <config value="false"/>
    <container name="performance">
      <description>
        <text>Performance operation.</text>
      </description>
      <container name="clear">
        <description>
          <text>Cleared performance data.</text>
        </description>
        <container name="currentdata">
          <description>
            <text>Cleared current performance data.</text>
          </description>
          <list name="currentdatum">
            <description>
              <text>Used to clear current performance value.</text>
            </description>
            <key value="cycle checkpoint-type tpid-matcher tpid-mask perfcode tpid-class"/>
            <leaf name="cycle">
              <description>
                <text>Performance cycle.</text>
              </description>
              <type name="enumeration">
                <enum name="fifteen-minutes">
                  <value value="1"/>
                  <description>
                    <text>Minute cycle. The actual cycle value depends on the minute-cycle configuration.</text>
                  </description>
                </enum>
                <enum name="twenty-four-hours">
                  <value value="2"/>
                </enum>
              </type>
            </leaf>
            <leaf name="checkpoint-type">
              <description>
                <text>TPID type. 0xFFFF for all.</text>
              </description>
              <type name="uint16">
                <range value="1..65535"/>
              </type>
            </leaf>
            <leaf name="tpid-matcher">
              <description>
                <text>As a conditions tpid, used with tpid-mask.</text>
              </description>
              <type name="binary">
                <length value="62..128"/>
              </type>
            </leaf>
            <leaf name="tpid-mask">
              <description>
                <text>Mask bit of attribute 'tpid-matcher'. Length is always 8 bytes.</text>
              </description>
              <type name="binary">
                <length value="8..16"/>
              </type>
            </leaf>
            <leaf name="perfcode">
              <description>
                <text>The specified performance code. 0xFFFF for all.</text>
              </description>
              <type name="uint16">
                <range value="1..65535"/>
              </type>
            </leaf>
            <leaf name="tpid-class">
              <description>
                <text>The class of TPID.</text>
              </description>
              <type name="enumeration">
                <enum name="board">
                  <value value="1"/>
                </enum>
                <enum name="CPU">
                  <value value="2"/>
                </enum>
                <enum name="system">
                  <value value="3"/>
                </enum>
                <enum name="all">
                  <value value="255"/>
                </enum>
              </type>
            </leaf>
            <leaf name="tpid-length">
              <description>
                <text>The length of TPID.</text>
              </description>
              <type name="uint8">
                <range value="62..128"/>
              </type>
            </leaf>
          </list>
        </container>
      </container>
      <container name="monitor">
        <description>
          <text>The real-time performance monitoring task operation.</text>
        </description>
        <container name="start-up">
          <description>
            <text>Start (i.e., create or generate) real-time performance monitoring tasks.</text>
          </description>
          <list name="task">
            <description>
              <text>Real-time monitor task.</text>
            </description>
            <key value="taskid"/>
            <leaf name="taskid">
              <description>
                <text>Monitor task ID.</text>
              </description>
              <type name="uint32"/>
            </leaf>
            <leaf name="cycle">
              <description>
                <text>Sampling period.</text>
              </description>
              <type name="uint16">
                <range value="5..900"/>
              </type>
              <units name="second"/>
              <default value="30"/>
            </leaf>
            <leaf name="total-time">
              <description>
                <text>Total duration of monitoring tasks. When filling 0xFFFFFFFF that the total duration of uncertainty triggered by the user manually stopped.</text>
              </description>
              <type name="uint32"/>
              <units name="second"/>
            </leaf>
            <leaf name="auto-report">
              <description>
                <text>Whether to automatically reported. Only support false.</text>
              </description>
              <type name="boolean"/>
              <default value="false"/>
            </leaf>
            <container name="checkpoints">
              <description>
                <text>Performance Item List.</text>
              </description>
              <list name="checkpoint">
                <description>
                  <text>The performance code of this tpid.</text>
                </description>
                <min-elements value="1"/>
                <key value="type tpid perfcode"/>
                <leaf name="type">
                  <description>
                    <text>TPID type.</text>
                  </description>
                  <type name="uint16"/>
                </leaf>
                <leaf name="tpid">
                  <description>
                    <text>The tpid specified.</text>
                  </description>
                  <type name="binary">
                    <length value="62..128"/>
                  </type>
                </leaf>
                <leaf name="perfcode">
                  <description>
                    <text>Performance item code.</text>
                  </description>
                  <type name="uint16">
                    <range value="1..65535"/>
                  </type>
                </leaf>
                <leaf name="tpid-length">
                  <description>
                    <text>The length of tpid.</text>
                  </description>
                  <type name="uint8">
                    <range value="62..128"/>
                  </type>
                </leaf>
              </list>
            </container>
          </list>
        </container>
        <container name="stop">
          <description>
            <text>Stop (i.e., delete or cancel) real-time performance monitoring tasks.</text>
          </description>
          <list name="task">
            <description>
              <text>Real-time monitor tasks.</text>
            </description>
            <key value="taskid"/>
            <leaf name="taskid">
              <description>
                <text>Monitor task ID. 0xFFFFFFFF indicates that stop all monitor task.</text>
              </description>
              <type name="uint32"/>
            </leaf>
          </list>
        </container>
      </container>
    </container>
  </container>
</module>
