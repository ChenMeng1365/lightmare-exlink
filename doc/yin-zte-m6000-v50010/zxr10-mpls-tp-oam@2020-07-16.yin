  <container name="configuration">
    <description>
      <text>Configuration model info.</text>
    </description>
    <container name="mpls-tp-oam">
      <description>
        <text>MPLS-TP OAM instences configuration</text>
      </description>
      <container name="global">
        <description>
          <text>Configure MPLS-TP OAM of global info.</text>
        </description>
        <leaf name="status">
          <description>
            <text>The switch for turning on/off MPLS-TP OAM function(default: enable).</text>
          </description>
          <type name="enumeration">
            <enum name="disable">
              <value value="0"/>
              <description>
                <text>Turn off MPLS-TP OAM function.</text>
              </description>
            </enum>
            <enum name="enable">
              <value value="1"/>
              <description>
                <text>Turn on MPLS-TP OAM function.</text>
              </description>
            </enum>
          </type>
          <default value="enable"/>
        </leaf>
        <leaf name="mel">
          <description>
            <text>The value of MEL field in OAM packets, ranges from 0 to 7(default: 7).</text>
          </description>
          <type name="uint8">
            <range value="0..7">
              <description>
                <text>The value of MEL.</text>
              </description>
            </range>
          </type>
          <default value="7"/>
        </leaf>
        <leaf name="channel-type">
          <description>
            <text>The identify of Y.1731 packet in G-ACH Label, ranges from 1 to 65535(default: 32762).</text>
          </description>
          <type name="uint16">
            <range value="1..65535">
              <description>
                <text>The value of channel type.</text>
              </description>
            </range>
          </type>
          <default value="32762"/>
        </leaf>
        <leaf name="tc">
          <description>
            <text>The value of TC field in the label of GAL, ranges from 0 to 7(default: 7).</text>
          </description>
          <type name="uint8">
            <range value="0..7">
              <description>
                <text>The value of GAL TC.</text>
              </description>
            </range>
          </type>
          <default value="7"/>
        </leaf>
        <leaf name="rdi-to-sf">
          <description>
            <text>This switch used to transform the RDI alarm to SF alarm.</text>
          </description>
          <type name="enumeration">
            <enum name="disable">
              <value value="0"/>
              <description>
                <text>Turn off RDI to SF function.</text>
              </description>
            </enum>
            <enum name="enable">
              <value value="1"/>
              <description>
                <text>Turn on RDI to SF function.</text>
              </description>
            </enum>
          </type>
        </leaf>
        <leaf name="statistic-time">
          <description>
            <text>The value of reporting interval of statistic functionality, ranges from 5s to 60s(default: 5s).</text>
          </description>
          <type name="uint8">
            <range value="5..60">
              <description>
                <text>The value of reporting interval of statistic functionality.</text>
              </description>
            </range>
          </type>
          <default value="5"/>
        </leaf>
        <leaf name="ais-speed">
          <description>
            <text>AIS fast or slow packets config.</text>
          </description>
          <type name="enumeration">
            <enum name="fast">
              <value value="1"/>
              <description>
                <text>Fast packets.</text>
              </description>
            </enum>
            <enum name="slow">
              <value value="2"/>
              <description>
                <text>Slow packets.</text>
              </description>
            </enum>
          </type>
        </leaf>
        <leaf name="csf-speed">
          <description>
            <text>CSF fast or slow packets config.</text>
          </description>
          <type name="enumeration">
            <enum name="fast">
              <value value="1"/>
              <description>
                <text>Fast packets</text>
              </description>
            </enum>
            <enum name="slow">
              <value value="2"/>
              <description>
                <text>Slow packets</text>
              </description>
            </enum>
          </type>
        </leaf>
        <leaf name="ais-period">
          <description>
            <text>The period carried in the AIS frame,including 1s,and 1min (defaut: 1s).</text>
          </description>
          <type name="enumeration">
            <enum name="1s">
              <value value="0"/>
              <description>
                <text>1 second, it means 1 packet per second.</text>
              </description>
            </enum>
            <enum name="1min">
              <value value="1"/>
              <description>
                <text>1 minute, it means 1 packet per minute.</text>
              </description>
            </enum>
          </type>
          <default value="1s"/>
        </leaf>
        <leaf name="fei-period">
          <description>
            <text>The sending period of FEI packets, ranges from 1 to 5(units:s, default: 1).</text>
          </description>
          <type name="uint8">
            <range value="1..5">
              <description>
                <text>FEI packet send period</text>
              </description>
            </range>
          </type>
          <units name="s"/>
          <default value="1"/>
        </leaf>
        <leaf name="node-id">
          <description>
            <text>The node ID is used to identify the MIP.</text>
          </description>
          <type name="ncd:ipv4-address"/>
          <ncd:revise-date date="2017-08-07"/>
        </leaf>
        <leaf name="delay">
          <description>
            <text>The delay time, ranges from 0 to 60(units:min, default: 0).</text>
          </description>
          <type name="uint8">
            <range value="0..60">
              <description>
                <text>The value of delay time.</text>
              </description>
            </range>
          </type>
          <default value="0"/>
          <ncd:revise-date date="2018-05-04"/>
        </leaf>
      </container>
      <container name="megs">
        <description>
          <text>Configure MEG parameters.</text>
        </description>
        <list name="meg">
          <description>
            <text>Configure MEG parameters.</text>
          </description>
          <key value="meg-index"/>
          <leaf name="meg-index">
            <description>
              <text>The value of MEG index,the upper limit of this parameter depends on the device.</text>
            </description>
            <type name="uint32">
              <range value="1..4294967295">
                <description>
                  <text>The value of MEG index.</text>
                </description>
              </range>
            </type>
          </leaf>
          <choice name="instance-type">
            <description>
              <text>Configure Instance type.</text>
            </description>
            <mandatory value="true"/>
            <case name="section">
              <description>
                <text>Configure MPLS-TP OAM of section.</text>
              </description>
              <container name="section">
                <description>
                  <text>Configure MPLS-TP OAM of section.</text>
                </description>
                <leaf name="section-id">
                  <description>
                    <text>Section ID used to identify the section entity.</text>
                  </description>
                  <type name="uint32">
                    <range value="1..4000">
                      <description>
                        <text>The value of section ID.</text>
                      </description>
                    </range>
                  </type>
                  <mandatory value="true"/>
                </leaf>
              </container>
            </case>
            <case name="sta-tunnel">
              <description>
                <text>Configure MPLS-TP OAM of static tunnel.</text>
              </description>
              <container name="sta-tunnel">
                <description>
                  <text>Configure MPLS-TP OAM of static tunnel.</text>
                </description>
                <leaf name="tunnel-id">
                  <description>
                    <text>The identity of static tunnel,the upper limit of this parameter depend on the device.</text>
                  </description>
                  <type name="uint32">
                    <range value="1..4294967295">
                      <description>
                        <text>The value of static tunnel number,and the upper limit of this parameter depend on the device.</text>
                      </description>
