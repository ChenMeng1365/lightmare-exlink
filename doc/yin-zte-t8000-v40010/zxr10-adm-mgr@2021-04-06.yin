  <feature name="clear-online-user">
    <description>
      <text>Clear the online user.</text>
    </description>
  </feature>
  <feature name="root-user">
    <description>
      <text>Configure root user password.</text>
    </description>
  </feature>
  <feature name="date-check">
    <description>
      <text>Set password date-related check</text>
    </description>
  </feature>
  <feature name="activate-user">
    <description>
      <text>Activate local user.</text>
    </description>
  </feature>
  <feature name="global-user-aging">
    <description>
      <text>The configuration of global user-aging.</text>
    </description>
  </feature>
  <feature name="user-aging">
    <description>
      <text>The configuration of user's user-aging.</text>
    </description>
  </feature>
  <feature name="max-length">
    <description>
      <text>Set the maximum length of the password</text>
    </description>
  </feature>
  <feature name="remind-before-expire"/>
  <feature name="password-recover-remind">
    <description>
      <text>Configure the question and answer for password recovery.</text>
    </description>
  </feature>
  <feature name="user-authen-restriction">
    <description>
      <text>The configuration of user's user-authen-restriction.</text>
    </description>
  </feature>
