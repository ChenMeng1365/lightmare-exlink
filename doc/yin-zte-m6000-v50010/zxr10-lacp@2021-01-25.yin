  <feature name="lacp-protocol-packet">
    <description>
      <text>Set the rule of the control plane sending protocol packet for smartgroup.</text>
    </description>
  </feature>
  <container name="configuration">
    <description>
      <text>Describe the configuration of Link Aggregation Control Protocol.</text>
    </description>
    <container name="lacp">
      <description>
        <text>Describe the parameters of LACP to be configured.</text>
      </description>
      <container name="global">
        <description>
          <text>Describe the global parameters to be configured.</text>
        </description>
        <leaf name="system-priority">
          <description>
            <text>The global system priority. In the case of the smartgroup-priority leaf of smartgroup list is not configured, this value will be used.</text>
          </description>
          <type name="uint16">
            <range value="1..65535"/>
          </type>
          <default value="32768"/>
        </leaf>
        <leaf name="system-active-number">
          <status value="obsolete">
            <ncd:revise-date date="2018-12-21"/>
          </status>
          <description>
            <text>The global system active number, its maximum value depends on the performance parameters. In the case of the active-number leaf of smartgroup list is not configured, this value will be used.</text>
          </description>
          <type name="uint32">
            <range value="1..1024">
              <description>
                <text>The maximum value depends on the performance parameters.</text>
              </description>
            </range>
          </type>
          <default value="1"/>
        </leaf>
        <leaf name="alarm-to-notification">
          <description>
            <text>The flag that turns alarm to notification, its default value depends on the performance parameters.</text>
          </description>
          <type name="enumeration">
            <enum name="disable">
              <value value="0"/>
              <description>
                <text>Disable the alarm to notification function.</text>
              </description>
            </enum>
            <enum name="enable">
              <value value="1"/>
              <description>
                <text>Enable the alarm to notification function.</text>
              </description>
            </enum>
          </type>
          <ncd:revise-date date="2018-06-12"/>
        </leaf>
        <leaf name="service-switch">
          <description>
            <text>LACP service switch, its default value depends on the performance parameters.</text>
          </description>
          <type name="enumeration">
            <enum name="disable">
              <value value="0"/>
              <description>
                <text>Disable LACP service.</text>
              </description>
            </enum>
            <enum name="enable">
              <value value="1"/>
              <description>
                <text>Enable LACP service.</text>
              </description>
            </enum>
          </type>
          <ncd:revise-date date="2018-07-18"/>
        </leaf>
        <container name="mclag">
          <description>
            <text>Describe the parameters of global MC-LAG to be configured.</text>
          </description>
          <container name="mclag-backoff">
            <description>
              <text>Describe the parameters of global MC-LAG backoff to be configured.</text>
            </description>
            <leaf name="backoff">
              <description>
                <text>The backoff of global MC-LAG.</text>
              </description>
              <type name="enumeration">
                <enum name="disable">
                  <value value="1"/>
                  <description>
                    <text>Disable global MC-LAG backoff .</text>
                  </description>
                </enum>
                <enum name="enable">
                  <value value="0"/>
                  <description>
                    <text>Enable global MC-LAG backoff.</text>
                  </description>
                </enum>
              </type>
              <default value="enable"/>
            </leaf>
          </container>
          <container name="mclag-ccheck-critical">
            <description>
              <text>Describe the parameters of global MC-LAG consistency check to be configured.</text>
            </description>
            <presence value="critical-type should be mandatory"/>
            <leaf name="critical-type">
              <description>
                <text>The consistency check type of global MC-LAG is critical.</text>
              </description>
              <type name="empty"/>
              <mandatory value="true"/>
            </leaf>
            <leaf name="mode">
              <description>
                <text>The consistency check mode of global MC-LAG.</text>
              </description>
              <type name="enumeration">
                <enum name="loose">
                  <value value="0"/>
                  <description>
                    <text>The consistency check mode of global MC-LAG is loose.</text>
                  </description>
                </enum>
                <enum name="strict">
                  <value value="1"/>
                  <description>
                    <text>The consistency check mode of global MC-LAG is strict.</text>
                  </description>
                </enum>
              </type>
              <default value="loose"/>
            </leaf>
          </container>
          <container name="mclag-ccheck-normal">
            <description>
              <text>Describe the parameters of global MC-LAG consistency check to be configured.</text>
            </description>
            <presence value="normal-type should be mandatory"/>
            <leaf name="normal-type">
              <description>
                <text>The consistency check type of global MC-LAG is normal.</text>
              </description>
              <type name="empty"/>
              <mandatory value="true"/>
            </leaf>
          </container>
          <container name="mclag-delay-time">
            <description>
              <text>Describe the parameters of global MC-LAG delay time to be configured.</text>
            </description>
            <leaf name="delay-down-time">
              <description>
                <text>The delay down time to global MC-LAG, its default value depends on the performance parameters.</text>
              </description>
              <type name="uint16">
                <range value="0..180"/>
              </type>
              <units name="second"/>
              <default value="3"/>
            </leaf>
            <leaf name="delay-up-time">
              <description>
                <text>The delay up time to global MC-LAG, its default value depends on the performance parameters.</text>
              </description>
              <type name="uint16">
                <range value="0..3600"/>
              </type>
              <units name="second"/>
              <default value="480"/>
            </leaf>
            <leaf name="reload-delay-up-time">
              <description>
                <text>The reload delay up time to global MC-LAG, its default value depends on the performance parameters.</text>
              </description>
              <type name="uint16">
                <range value="0..3600"/>
              </type>
              <units name="second"/>
              <default value="480"/>
            </leaf>
          </container>
          <container name="mclag-dual-malfunction-protect">
            <description>
              <text>Describe the parameters of global MC-LAG dual malfunction protect to be configured.</text>
            </description>
            <leaf name="dual-malfunction-protect">
              <description>
                <text>The dual malfunction protect of global MC-LAG,its default value depends on the performance parameters.</text>
              </description>
              <type name="enumeration">
                <enum name="disable">
                  <value value="0"/>
                  <description>
                    <text>The dual malfunction protect of global MC-LAG is disable.</text>
                  </description>
                </enum>
                <enum name="enable">
                  <value value="1"/>
                  <description>
                    <text>The dual malfunction protect of global MC-LAG is enable.</text>
                  </description>
                </enum>
              </type>
              <default value="enable"/>
            </leaf>
          </container>
          <container name="mclag-exempts">
            <description>
              <text>Describe the parameters of global MC-LAG exempt interface to be configured.</text>
            </description>
            <list name="mclag-exempt">
              <description>
                <text>Describe the parameters of global MC-LAG exempt interface to be configured.</text>
              </description>
              <key value="interface-name"/>
              <leaf name="interface-name">
                <description>
                  <text>The exempt interface of global MC-LAG.</text>
                </description>
                <type name="ncd:ifname"/>
              </leaf>
            </list>
          </container>
          <container name="mclag-ipv4-address">
            <description>
              <text>Describe the parameters of global MC-LAG IPv4 source and destination address to be configured.</text>
            </description>
            <presence value="source-address and destination-address should be mandatory"/>
            <leaf name="source-address">
              <description>
                <text>The IPv4 source address of global MC-LAG.</text>
              </description>
              <type name="ncd:ipv4-address"/>
              <mandatory value="true"/>
            </leaf>
            <leaf name="destination-address">
              <description>
                <text>The IPv4 destination address of global MC-LAG.</text>
              </description>
              <type name="ncd:ipv4-address"/>
              <mandatory value="true"/>
            </leaf>
            <container name="vrf">
              <description>
                <text>Describe the parameters of global MC-LAG VRF name to be configured.</text>
              </description>
              <choice name="vrf-choice">
                <description>
                  <text>The VRF name of global MC-LAG.</text>
                </description>
                <case name="vrf-name-case">
                  <description>
                    <text>The VRF name of global MC-LAG, whose name cannot be mng.</text>
                  </description>
                  <leaf name="vrf-name">
                    <description>
                      <text>The VRF name of global MC-LAG, whose name cannot be mng.</text>
                    </description>
                    <type name="string">
                      <length value="1..32"/>
                    </type>
                  </leaf>
                </case>
                <case name="vrf-mng-case">
                  <description>
                    <text>The VRF mng instance of global MC-LAG.</text>
                  </description>
                  <leaf name="vrf-mng">
                    <description>
                      <text>The VRF mng instance of global MC-LAG.</text>
                    </description>
                    <type name="empty"/>
                  </leaf>
                </case>
              </choice>
            </container>
          </container>
          <container name="mclag-ipv6-address">
            <description>
              <text>Describe the parameters of global MC-LAG IPv6 source and destination address to be configured.</text>
            </description>
            <presence value="source-address and destination-address should be mandatory"/>
            <leaf name="source-address">
              <description>
                <text>The IPv6 source address of global MC-LAG.</text>
              </description>
              <type name="ncd:ipv6-address-none-zero"/>
              <mandatory value="true"/>
            </leaf>
            <leaf name="destination-address">
              <description>
                <text>The IPv6 destination address of global MC-LAG.</text>
              </description>
              <type name="ncd:ipv6-address-none-zero"/>
              <mandatory value="true"/>
            </leaf>
            <container name="vrf">
              <description>
                <text>Describe the parameters of global MC-LAG VRF name to be configured.</text>
              </description>
              <choice name="vrf-choice">
                <description>
                  <text>The VRF name of global MC-LAG.</text>
                </description>
                <case name="vrf-name-case">
                  <description>
                    <text>The VRF name of global MC-LAG, whose name cannot be mng.</text>
                  </description>
                  <leaf name="vrf-name">
                    <description>
                      <text>The VRF name of global MC-LAG, whose name cannot be mng.</text>
                    </description>
                    <type name="string">
                      <length value="1..32"/>
                    </type>
                  </leaf>
                </case>
                <case name="vrf-mng-case">
                  <description>
                    <text>The VRF mng instance of global MC-LAG.</text>
                  </description>
                  <leaf name="vrf-mng">
                    <description>
                      <text>The VRF mng instance of global MC-LAG.</text>
                    </description>
                    <type name="empty"/>
                  </leaf>
                </case>
              </choice>
            </container>
          </container>
          <container name="mclag-priority">
            <description>
