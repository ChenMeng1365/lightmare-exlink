    "definitions": {
        "software_schema": {
            "description": "Specify software package upgrade management.",
            "properties": {
                "global": {
                    "description": "Configure of auto save checkpoint switch.",
                    "properties": {
                        "auto-save": {
                            "description": "Configure of enable auto save checkpoint.",
                            "properties": {
                                "enable": {
                                    "type": "boolean",
                                    "description": "Enable/disable auto save checkpoint when upgrade software package."
                                }
                            }
                        }
                    }
                },
                "versions": {
                    "description": "List of operational state of software package versions. The node versions is deprecated. You are advised to use the node /system/system-info.",
                    "properties": {
                        "version": {
                            "description": "Operational state of a software package version. The node version is deprecated. You are advised to use the node /system/system-info.",
                            "type": "array",
                            "items": {
                                "properties": {
                                    "base": {
                                        "type": "string",
                                        "description": "Current software package version. The node base is deprecated. You are advised to use the node /system/system-info/product-version."
                                    },
                                    "patch": {
                                        "type": "string",
                                        "description": "Current patch package version. The node patch is deprecated. You are advised to use the node /system/system-info/patch-version."
                                    }
                                }
                            },
                            "x-key": "base"
                        }
                    }
                },
                "startup-packages": {
                    "description": "List of operational state of startup software packages.",
                    "properties": {
                        "startup-package": {
                            "description": "Operational state of startup software package.",
                            "type": "array",
                            "items": {
                                "properties": {
                                    "slot-id": {
                                        "type": "string",
                                        "description": "Slot ID."
                                    },
                                    "current-package": {
                                        "type": "string",
                                        "description": "The name of a software package used at the current startup."
                                    },
                                    "next-package": {
                                        "type": "string",
                                        "description": "The name of a software package used at the next startup."
                                    }
                                }
                            },
                            "x-key": "slot-id"
                        }
                    }
                },
                "packages": {
                    "description": "List of operational state of software packages on the device.",
                    "properties": {
                        "package": {
                            "description": "Operational state of a software package on the device.",
                            "type": "array",
                            "items": {
                                "properties": {
                                    "name": {
                                        "type": "string",
                                        "description": "The name of a software package on the device. There may exist user-created software packages with names longer than 127 on the device, however, such packages cannot be used for the next startup."
                                    },
                                    "package-type": {
                                        "type": "string",
                                        "enum": [
                                            "system",
                                            "feature"
                                        ],
                                        "description": "The type of a software package on the device."
                                    }
                                }
                            },
                            "x-key": "name"
                        }
                    }
                },
                "upgrade-rollback-timer": {
                    "description": "Operational data of upgrade rollback timer.",
                    "properties": {
                        "timer-enable": {
                            "type": "boolean",
                            "description": "When the rollback timer is closed, the time leaf will not be shown."
                        },
                        "time": {
                            "type": "string",
                            "description": "Rollback time."
                        }
                    }
                },
                "checkpoint-infos": {
                    "description": "List of checkpoint base information.",
                    "properties": {
                        "checkpoint-info": {
                            "description": "Statistics of a checkpoint info.",
                            "type": "array",
                            "items": {
                                "properties": {
                                    "name": {
                                        "type": "string",
                                        "description": "Checkpoint name."
                                    },
                                    "create-time": {
                                        "type": "string",
                                        "description": "Checkpoint create time."
                                    },
                                    "create-mode": {
                                        "type": "string",
                                        "enum": [
                                            "manual",
                                            "auto"
                                        ],
                                        "description": "Checkpoint create mode."
                                    },
                                    "base-package": {
                                        "type": "string",
                                        "description": "Checkpoint software package."
                                    },
                                    "feature-package": {
                                        "type": "string",
                                        "description": "Checkpoint feature packages."
                                    },
                                    "patch-package": {
                                        "type": "string",
                                        "description": "Checkpoint patch packages."
                                    },
                                    "module-package": {
                                        "type": "string",
                                        "description": "Checkpoint mod patchs."
                                    }
                                }
                            },
                            "x-key": "name"
                        }
                    }
                }
            }
        },
        "global_schema": {
            "description": "Configure of auto save checkpoint switch.",
            "properties": {
                "auto-save": {
                    "description": "Configure of enable auto save checkpoint.",
                    "properties": {
                        "enable": {
                            "type": "boolean",
                            "description": "Enable/disable auto save checkpoint when upgrade software package."
                        }
                    }
                }
            }
        },
        "auto-save_schema": {
            "description": "Configure of enable auto save checkpoint.",
            "properties": {
