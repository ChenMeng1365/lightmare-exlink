                            "type": "array",
                            "items": {
                                "properties": {
                                    "name": {
                                        "type": "string",
                                        "description": "Specify the name of the DHCPv6 server group."
                                    },
                                    "server-source": {
                                        "description": "Configure the source address for packets to be sent to the DHCPv6 server group.",
                                        "properties": {
                                            "source-address": {
                                                "description": "Configure the source address for packets to be sent to the DHCPv6 server group.",
                                                "properties": {
                                                    "link-address": {
                                                        "description": "Configures the source address, which is a link address of the prefix pool bound to a remote address pool.",
                                                        "properties": {
                                                            "link-address": {
                                                                "type": "string",
                                                                "description": "Indicate that the source address is a link address of the prefix pool bound to a remote address pool."
                                                            }
                                                        }
                                                    },
                                                    "if-name": {
                                                        "description": "Configures the type and number of the interface whose address is the source address.",
