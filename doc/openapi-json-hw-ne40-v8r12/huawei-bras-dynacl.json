    "definitions": {
        "bras-dynacl_schema": {
            "description": "Bras dynacl.",
            "properties": {
                "remote-download-acl": {
                    "description": "Configure the RADIUS server to create dynamic ACLs.",
                    "properties": {
                        "enable": {
                            "type": "boolean",
                            "description": "Enable/disable the RADIUS server to create dynamic ACLs."
                        }
                    }
                },
                "remote-download-user-group": {
                    "description": "Configure the RADIUS server to create dynamic user groups.",
                    "properties": {
                        "enable": {
                            "type": "boolean",
                            "description": "Enable/disable the RADIUS server to create dynamic user groups."
                        }
                    }
                },
                "remote-user-group-check-interval": {
                    "description": "Configure an interval at which a device checks whether dynamic user groups created by the RADIUS server are used by online users or dynamic ACLs.",
                    "properties": {
                        "interval-value": {
                            "type": "string",
                            "description": "Specifies an interval at which the device checks whether a dynamic user group is in use."
                        }
                    }
                },
                "remote-download-acl-alarm-threshold": {
                    "description": "Configure the alarm threshold for the usage of classifier-behavior pairs dynamically delivered by the RADIUS server.",
                    "properties": {
                        "threshold-value": {
                            "type": "string",
                            "description": "Specifies an alarm threshold."
                        }
                    }
                },
                "support-all-operate": {
                    "description": "Configure a BRAS to support all operation types of a dynamic ACL delivered using a RADIUS authentication response packet when a user goes online.",
                    "properties": {
                        "enable": {
                            "type": "boolean",
                            "description": "Enable/disable a BRAS to support all operation types of a dynamic ACL delivered using a RADIUS authentication response packet when a user goes online."
                        }
                    }
                }
            }
        },
        "remote-download-acl_schema": {
            "description": "Configure the RADIUS server to create dynamic ACLs.",
            "properties": {
                "enable": {
                    "type": "boolean",
                    "description": "Enable/disable the RADIUS server to create dynamic ACLs."
                }
            }
        },
        "remote-download-user-group_schema": {
