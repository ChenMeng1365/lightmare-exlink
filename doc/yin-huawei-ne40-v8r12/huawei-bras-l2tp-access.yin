  <grouping name="lastest-received-message-backup">
    <description>
      <text>Lastest received message backup info.</text>
    </description>
    <container name="lastest-received-message">
      <description>
        <text>Statistics of backup.</text>
      </description>
      <leaf name="tunnel-drop">
        <type name="uint32"/>
        <description>
          <text>The number of last tunnel drop message.</text>
        </description>
      </leaf>
      <leaf name="tunnel-update">
        <type name="uint32"/>
        <description>
          <text>The number of last tunnel update message.</text>
        </description>
      </leaf>
      <leaf name="tunnel-create">
        <type name="uint32"/>
        <description>
          <text>The number of last tunnel create message.</text>
