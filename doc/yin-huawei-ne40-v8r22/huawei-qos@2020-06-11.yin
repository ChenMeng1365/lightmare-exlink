            </type>
            <description>
              <text>Description.</text>
            </description>
          </leaf>
          <leaf name="step">
            <type name="uint16">
              <range value="1..20"/>
            </type>
            <default value="1"/>
            <description>
              <text>Step.</text>
            </description>
          </leaf>
          <leaf name="share-mode">
            <type name="switch-flag"/>
            <default value="enable"/>
            <description>
              <text>Sharing mode. After the policy is applied, the attribute of this node cannot be changed.</text>
            </description>
