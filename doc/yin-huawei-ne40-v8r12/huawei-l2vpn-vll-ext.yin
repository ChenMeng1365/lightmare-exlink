<?xml version="1.0" encoding="UTF-8"?>
<module name="huawei-l2vpn-vll-ext"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:l2vpn-vll-ext="urn:huawei:yang:huawei-l2vpn-vll-ext"
        xmlns:qos="urn:huawei:yang:huawei-qos"
        xmlns:ifm="urn:huawei:yang:huawei-ifm"
        xmlns:ext="urn:huawei:yang:huawei-extension">
  <namespace uri="urn:huawei:yang:huawei-l2vpn-vll-ext"/>
  <prefix value="l2vpn-vll-ext"/>
  <import module="huawei-qos">
    <prefix value="qos"/>
  </import>
  <import module="huawei-ifm">
    <prefix value="ifm"/>
  </import>
  <import module="huawei-extension">
    <prefix value="ext"/>
  </import>
  <organization>
    <text>Huawei Technologies Co., Ltd.</text>
  </organization>
  <contact>
    <text>Huawei Industrial Base
Bantian, Longgang
Shenzhen 518129
People's Republic of China
Website: http://www.huawei.com
Email: support@huawei.com</text>
  </contact>
  <description>
    <text>L2VPN vll ext.</text>
  </description>
  <revision date="2020-03-06">
    <description>
      <text>Add pipe type.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <revision date="2020-01-21">
    <description>
      <text>Add vll-pw-loop-check.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <revision date="2019-10-17">
    <description>
      <text>Modify typedef defination.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <revision date="2019-04-23">
    <description>
      <text>Initial revision.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <typedef name="l2vpn-pipe-mode">
    <type name="enumeration">
      <enum name="pipe">
        <value value="1"/>
        <description>
          <text>Specify the L2VPN pipe mode as pipe.</text>
        </description>
      </enum>
      <enum name="shortPipe">
        <value value="2"/>
        <description>
          <text>Specify the L2VPN pipe mode as short pipe.</text>
        </description>
      </enum>
      <enum name="uniform">
        <value value="3"/>
        <description>
          <text>Specify the L2VPN pipe mode as uniform.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>L2vpn pipe mode.</text>
    </description>
  </typedef>
  <typedef name="l2vpn-service-class">
    <type name="enumeration">
      <enum name="be">
        <value value="0"/>
        <description>
          <text>Specify the L2VPN CoS as BE.</text>
        </description>
      </enum>
      <enum name="af1">
        <value value="1"/>
        <description>
          <text>Specify the L2VPN CoS as AF1.</text>
        </description>
      </enum>
      <enum name="af2">
        <value value="2"/>
        <description>
          <text>Specify the L2VPN CoS as AF2.</text>
        </description>
      </enum>
      <enum name="af3">
        <value value="3"/>
        <description>
          <text>Specify the L2VPN CoS as AF3.</text>
        </description>
      </enum>
      <enum name="af4">
        <value value="4"/>
        <description>
          <text>Specify the L2VPN CoS as AF4.</text>
        </description>
      </enum>
      <enum name="ef">
        <value value="5"/>
        <description>
          <text>Specify the L2VPN CoS as EF.</text>
        </description>
      </enum>
      <enum name="cs6">
        <value value="6"/>
        <description>
          <text>Specify the L2VPN CoS as CS6.</text>
        </description>
      </enum>
      <enum name="cs7">
        <value value="7"/>
        <description>
          <text>Specify the L2VPN CoS as CS7.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Description of CoS.</text>
    </description>
  </typedef>
  <typedef name="l2vpn-color">
    <type name="enumeration">
      <enum name="green">
        <value value="0"/>
        <description>
          <text>Specify the packet color as green.</text>
        </description>
      </enum>
      <enum name="yellow">
        <value value="1"/>
        <description>
          <text>Specify the packet color as yellow.</text>
        </description>
      </enum>
      <enum name="red">
        <value value="2"/>
        <description>
          <text>Specify the packet color as red.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Description of packet color.</text>
    </description>
  </typedef>
  <typedef name="l2vpn-trust-type">
    <type name="enumeration">
      <enum name="outpri">
        <value value="0"/>
        <description>
          <text>Specify the trust type as OUTTER-VLAN-8021P.</text>
        </description>
      </enum>
      <enum name="inpri">
        <value value="1"/>
        <description>
          <text>Specify the trust type as INNER-VLAN-8021P.</text>
        </description>
      </enum>
      <enum name="ipdscp">
        <value value="2"/>
        <description>
          <text>Specify the trust type as IP-DSCP.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Description of trust type.</text>
    </description>
  </typedef>
  <ext:task-name value="forwarding"/>
  <augment target-node="/ifm:ifm/ifm:interfaces/ifm:interface/qos:qos">
    <description>
      <text>Interface QOS.</text>
    </description>
    <container name="qos-vll-pipe">
      <description>
        <text>Configure the diffserv mode for L2VPN.</text>
      </description>
      <leaf name="split-mode">
        <type name="boolean"/>
        <must condition="((../split-mode='false' and not(../ingress-pipe-mode) and not(../egress-pipe-mode) and ../pipe-mode) or (../split-mode='true' and not(../pipe-mode) and ../ingress-pipe-mode and ../egress-pipe-mode and not(../ingress-pipe-mode='uniform' and ../egress-pipe-mode='uniform')))"/>
        <default value="false"/>
        <description>
          <text>Enable/disable split mode.</text>
        </description>
      </leaf>
      <leaf name="pipe-mode">
        <when condition="../split-mode='false'"/>
        <type name="l2vpn-pipe-mode"/>
        <must condition="(../split-mode='false') and ((../pipe-mode='pipe' and (../service-class) and (../color)) or (../pipe-mode='shortPipe' and (../service-class) and (../color) and (../domain-name)) or (../pipe-mode='uniform'))"/>
        <default value="uniform"/>
        <description>
          <text>Specify the pipe mode.</text>
        </description>
      </leaf>
      <leaf name="ingress-pipe-mode">
        <when condition="../split-mode='true'"/>
        <type name="l2vpn-pipe-mode"/>
        <must condition="((../split-mode='true') and (((../service-class) and (../color) and (../ingress-pipe-mode='shortPipe' or ../ingress-pipe-mode='pipe')) or (../ingress-pipe-mode='uniform' and ../egress-pipe-mode!='uniform')))"/>
        <default value="uniform"/>
        <description>
          <text>Specify the ingress pipe mode.</text>
        </description>
      </leaf>
      <leaf name="egress-pipe-mode">
        <when condition="../split-mode='true'"/>
        <type name="l2vpn-pipe-mode"/>
        <must condition="((../split-mode='true') and (((../domain-name) and (../trust-type) and (../egress-pipe-mode='shortPipe')) or (../egress-pipe-mode='pipe') or (../egress-pipe-mode='uniform' and ../ingress-pipe-mode!='uniform')))"/>
        <default value="uniform"/>
        <description>
          <text>Specify the egress pipe mode.</text>
        </description>
      </leaf>
      <leaf name="service-class">
        <when condition="(../pipe-mode='pipe' or ../pipe-mode='shortPipe' or ../ingress-pipe-mode='pipe' or ../ingress-pipe-mode='shortPipe')"/>
        <type name="l2vpn-service-class"/>
        <must condition="(../pipe-mode='pipe' or ../pipe-mode='shortPipe' or ../ingress-pipe-mode='pipe' or ../ingress-pipe-mode='shortPipe')"/>
        <default value="be"/>
        <description>
          <text>Specify the CoS.</text>
        </description>
      </leaf>
      <leaf name="color">
        <when condition="(../pipe-mode='pipe' or ../pipe-mode='shortPipe'  or ../ingress-pipe-mode='pipe' or ../ingress-pipe-mode='shortPipe')"/>
        <type name="l2vpn-color"/>
        <must condition="(../pipe-mode='pipe' or ../pipe-mode='shortPipe' or ../ingress-pipe-mode='pipe' or ../ingress-pipe-mode='shortPipe')"/>
        <default value="green"/>
        <description>
          <text>Specify the color of packets.</text>
        </description>
      </leaf>
      <leaf name="domain-name">
        <when condition="(../pipe-mode='shortPipe'  or ../egress-pipe-mode='shortPipe')"/>
        <type name="leafref">
          <path value="/qos:qos/qos:classifier-template/qos:diffserv-domains/qos:diffserv-domain/qos:name"/>
        </type>
        <must condition="(../pipe-mode='shortPipe' or ../egress-pipe-mode='shortPipe')"/>
        <default value="default"/>
        <description>
          <text>DS domain name.</text>
        </description>
      </leaf>
      <leaf name="trust-type">
        <when condition="../egress-pipe-mode='shortPipe'"/>
        <type name="l2vpn-trust-type"/>
        <must condition="../egress-pipe-mode='shortPipe'"/>
        <default value="outpri"/>
        <description>
          <text>Specify the trust type.</text>
        </description>
      </leaf>
    </container>
  </augment>
  <augment target-node="/ifm:ifm/ifm:interfaces/ifm:interface">
    <description>
      <text>PW loop check.</text>
    </description>
    <container name="vll-pw-loop-check">
      <when condition="../ifm:type='Ethernet' or&#10;../ifm:type='GigabitEthernet' or&#10;../ifm:type='Eth-Trunk' or&#10;../ifm:type='400GE' or&#10;../ifm:type='100GE' or&#10;../ifm:type='50GE' or&#10;../ifm:type='40GE' or&#10;../ifm:type='25GE' or&#10;../ifm:type='10GE' or&#10;../ifm:type='XGigabitEthernet' or&#10;../ifm:type='GEBrief' or&#10;../ifm:type='50|100GE' or&#10;../ifm:type='FlexE'"/>
      <presence value="Enable PW loop check."/>
      <description>
        <text>Enable/disable PW loop check.</text>
      </description>
      <leaf name="time">
        <type name="uint32">
          <range value="1..65535"/>
        </type>
        <units name="s"/>
        <default value="300"/>
        <description>
          <text>PW loop check time.</text>
        </description>
      </leaf>
    </container>
  </augment>
  <augment target-node="/ifm:ifm/ifm:interfaces/ifm:interface">
    <description>
      <text>Configure the encapsulation VLAN pass of a PW.</text>
    </description>
    <container name="encapsulation-vlan">
      <when condition="../ifm:class='sub-interface' and ../ifm:l2-mode-enable='true' and ../ifm:type!='Virtual-Ethernet'"/>
      <description>
        <text>Configure the encapsulation VLAN pass of a PW.</text>
      </description>
      <leaf name="vlan-pass">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable the VLAN pass.</text>
        </description>
      </leaf>
    </container>
  </augment>
</module>
