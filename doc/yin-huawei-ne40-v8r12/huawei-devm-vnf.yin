<?xml version="1.0" encoding="UTF-8"?>
<module name="huawei-devm-vnf"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:devm-vnf="urn:huawei:yang:huawei-devm-vnf"
        xmlns:ext="urn:huawei:yang:huawei-extension"
        xmlns:pub-type="urn:huawei:yang:huawei-pub-type">
  <namespace uri="urn:huawei:yang:huawei-devm-vnf"/>
  <prefix value="devm-vnf"/>
  <import module="huawei-extension">
    <prefix value="ext"/>
  </import>
  <import module="huawei-pub-type">
    <prefix value="pub-type"/>
  </import>
  <organization>
    <text>Huawei Technologies Co., Ltd.</text>
  </organization>
  <contact>
    <text>Huawei Industrial Base
Bantian, Longgang
Shenzhen 518129
People's Republic of China
Website: http://www.huawei.com
Email: support@huawei.com</text>
  </contact>
  <description>
    <text>Configuration management module.</text>
  </description>
  <revision date="2020-04-10">
    <description>
      <text>Modify description warnings.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <revision date="2019-07-01">
    <description>
      <text>Initial revision.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <typedef name="run-state-type">
    <type name="enumeration">
      <enum name="abnormal">
        <value value="0"/>
        <description>
          <text>Abnormal.</text>
        </description>
      </enum>
      <enum name="normal">
        <value value="1"/>
        <description>
          <text>Normal.</text>
        </description>
      </enum>
      <enum name="isolated">
        <value value="2"/>
        <description>
          <text>Isolated.</text>
        </description>
      </enum>
      <enum name="notinstall">
        <value value="3"/>
        <description>
          <text>Not install.</text>
        </description>
      </enum>
      <enum name="wrongtype">
        <value value="4"/>
        <description>
          <text>Wrong type.</text>
        </description>
      </enum>
      <enum name="active">
        <value value="5"/>
        <description>
          <text>Active.</text>
        </description>
      </enum>
      <enum name="inactive">
        <value value="6"/>
        <description>
          <text>Inactive.</text>
        </description>
      </enum>
      <enum name="offline">
        <value value="7"/>
        <description>
          <text>Offline.</text>
        </description>
      </enum>
      <enum name="stop">
        <value value="8"/>
        <description>
          <text>Stop.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Run state.</text>
    </description>
  </typedef>
  <typedef name="resource-type">
    <type name="enumeration">
      <enum name="vm">
        <value value="0"/>
        <description>
          <text>Virtual machine.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Resource source type.</text>
    </description>
  </typedef>
  <typedef name="standby-state-type">
    <type name="enumeration">
      <enum name="not-supported">
        <value value="0"/>
        <description>
          <text>No standby state.</text>
        </description>
      </enum>
      <enum name="master">
        <value value="1"/>
        <description>
          <text>Master board.</text>
        </description>
      </enum>
      <enum name="slave">
        <value value="2"/>
        <description>
          <text>Slave board.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Board standby state.</text>
    </description>
  </typedef>
  <typedef name="board-role-type">
    <type name="enumeration">
      <enum name="unknown">
        <value value="0"/>
        <description>
          <text>Unknown.</text>
        </description>
      </enum>
      <enum name="mmb">
        <value value="1"/>
        <description>
          <text>MMB.</text>
        </description>
      </enum>
      <enum name="mb">
        <value value="4"/>
        <description>
          <text>MB.</text>
        </description>
      </enum>
      <enum name="lc">
        <value value="5"/>
        <description>
          <text>LC.</text>
        </description>
      </enum>
      <enum name="other">
        <value value="200"/>
        <description>
          <text>OTHER.</text>
        </description>
      </enum>
      <enum name="na">
        <value value="255"/>
        <description>
          <text>NA.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Board role.</text>
    </description>
  </typedef>
  <ext:task-name value="device-mgr"/>
  <container name="devm-vnf">
    <description>
      <text>Virtualized network function (VNF) infomation.</text>
    </description>
    <container name="vnf-boards">
      <description>
        <text>List of virtualized network function (VNF) board.</text>
      </description>
      <list name="vnf-board">
        <key value="vnfc-name id"/>
        <description>
          <text>Configure virtualized network function (VNF) board.</text>
        </description>
        <leaf name="vnfc-name">
          <type name="string">
            <length value="1..31"/>
          </type>
          <description>
            <text>Virtualized network function component (VNFC) name.</text>
          </description>
        </leaf>
        <leaf name="id">
          <type name="uint32"/>
          <description>
            <text>Board ID.</text>
          </description>
        </leaf>
        <leaf name="type-name">
          <ext:operation-exclude value="update"/>
          <type name="string">
            <length value="1..63"/>
          </type>
          <mandatory value="true"/>
          <description>
            <text>Type name.</text>
          </description>
        </leaf>
        <leaf name="name">
          <type name="string">
            <length value="1..63"/>
          </type>
          <config value="false"/>
          <description>
            <text>Board name.</text>
          </description>
        </leaf>
        <leaf name="run-state">
          <type name="run-state-type"/>
          <config value="false"/>
          <description>
            <text>Running status.</text>
          </description>
        </leaf>
        <leaf name="resource-type">
          <type name="resource-type"/>
          <config value="false"/>
          <description>
            <text>Resource Type.</text>
          </description>
        </leaf>
        <leaf name="resource-name">
          <type name="string">
            <length value="1..63"/>
          </type>
          <config value="false"/>
          <description>
            <text>Name of a resource source.</text>
          </description>
        </leaf>
        <leaf name="standby-state">
          <type name="standby-state-type"/>
          <config value="false"/>
          <description>
            <text>Standby state of a VNF board.</text>
          </description>
        </leaf>
        <leaf name="role">
          <type name="board-role-type"/>
          <config value="false"/>
          <description>
            <text>MPU role.</text>
          </description>
        </leaf>
        <leaf name="up-time">
          <type name="pub-type:time-tick"/>
          <config value="false"/>
          <description>
            <text>Running period.</text>
          </description>
        </leaf>
        <leaf name="physical-index">
          <type name="uint32"/>
          <config value="false"/>
          <description>
            <text>Entity index.</text>
          </description>
        </leaf>
      </list>
    </container>
  </container>
</module>
