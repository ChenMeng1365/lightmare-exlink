    "definitions": {
        "route-monitor-group_schema": {
            "description": "A route monitoring group monitors the status of its member routes, each of which has a down-weight. The down-weight sum of the route monitoring group changes with the number of routes that are Down and indicates the link quality. If a route in the route monitoring group goes Down, its down-weight is added to the down-weight sum of the route monitoring group. If a route in the route monitoring group goes Up again, its down-weight is subtracted from the down-weight sum of the route monitoring group.",
            "properties": {
                "monitor-groups": {
                    "description": "List of route monitoring groups.",
                    "properties": {
                        "monitor-group": {
                            "description": "Configure a route monitoring group.",
                            "type": "array",
                            "items": {
                                "properties": {
                                    "group-name": {
                                        "type": "string",
                                        "description": "Name of a route monitoring group."
                                    },
                                    "enable-status": {
                                        "type": "boolean",
                                        "description": "Enable/disable a route monitoring group."
                                    },
                                    "operator-and": {
                                        "type": "boolean",
                                        "description": "Enable/disable the AND relationship for routes in a route monitoring group."
                                    },
                                    "trigger-up-delay": {
                                        "type": "string",
                                        "description": "Restoration delay."
                                    },
                                    "trigger-down-delay": {
                                        "type": "string",
                                        "description": "Notification delay."
                                    },
                                    "weight-sum": {
                                        "type": "string",
                                        "description": "Down-weight sum of the route monitoring group."
                                    },
                                    "down-weight": {
                                        "type": "string",
                                        "description": "Down-weight of the routes in the route monitoring group."
                                    },
                                    "track-routes": {
                                        "description": "List of routes added to a route monitoring group.",
                                        "properties": {
                                            "track-route": {
                                                "description": "Configure routes to be added to a route monitoring group.",
                                                "type": "array",
                                                "items": {
                                                    "properties": {
                                                        "destination-ni-name": {
                                                            "type": "string",
                                                            "x-path": "/ni:network-instance/ni:instances/ni:instance/ni:name",
                                                            "description": "Name of a VPN instance."
                                                        },
                                                        "destination": {
                                                            "type": "string",
                                                            "description": "Destination IP address."
                                                        },
                                                        "mask-length": {
                                                            "type": "string",
                                                            "description": "Mask length."
                                                        },
                                                        "down-weight": {
                                                            "type": "string",
                                                            "description": "Down-weight of a route in a route monitoring group."
                                                        },
                                                        "route-status": {
                                                            "type": "string",
                                                            "enum": [
                                                                "active",
                                                                "inactive",
                                                                "invalid"
                                                            ],
                                                            "description": "Displays the route status."
                                                        }
                                                    }
                                                },
                                                "x-key": "destination-ni-name destination mask-length"
                                            }
                                        }
                                    }
                                }
                            },
                            "x-key": "group-name"
                        }
                    }
                }
            }
        },
        "monitor-groups_schema": {
            "description": "List of route monitoring groups.",
            "properties": {
                "monitor-group": {
                    "description": "Configure a route monitoring group.",
                    "type": "array",
                    "items": {
                        "properties": {
                            "group-name": {
                                "type": "string",
                                "description": "Name of a route monitoring group."
                            },
