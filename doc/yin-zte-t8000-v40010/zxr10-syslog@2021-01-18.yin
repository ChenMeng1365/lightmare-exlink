    <description>
      <text>send URL log to syslog.</text>
    </description>
  </feature>
  <feature name="syslog-source-interface">
    <description>
      <text>syslog source interface feature.</text>
    </description>
  </feature>
  <feature name="syslog-format">
    <description>
      <text>1.Syslog format.</text>
    </description>
  </feature>
  <feature name="syslog-tcp">
    <description>
      <text>SYSLOG TCP.</text>
    </description>
