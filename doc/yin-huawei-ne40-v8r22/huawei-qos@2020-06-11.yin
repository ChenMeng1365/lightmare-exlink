                  <path value="../../../../../qos:traffic-policys/qos:traffic-policy/qos:name"/>
                </type>
                <mandatory value="true"/>
                <description>
                  <text>Policy name.</text>
                </description>
              </leaf>
              <leaf name="hierarchical-car">
                <type name="enable-type"/>
                <description>
                  <text>Hierarchical car.</text>
                </description>
              </leaf>
              <leaf name="layer">
                <type name="qos-policy-layer"/>
                <description>
                  <text>Ip-layer.</text>
                </description>
              </leaf>
            </container>
          </container>
          <container name="suppression-actions">
            <description>
              <text>List of L2 suppression.</text>
            </description>
            <list name="suppression-action">
              <must condition="(./green-service-class and ./green-color) or (not(./green-service-class) and not(./green-color))"/>
              <must condition="(./red-service-class and ./red-color) or (not(./green-service-class) and not(./green-color))"/>
              <key value="suppress-type"/>
              <max-elements value="3"/>
              <description>
                <text>Configure L2 suppression.</text>
              </description>
              <leaf name="suppress-type">
                <type name="suppress-type"/>
                <description>
                  <text>Suppression type.</text>
                </description>
              </leaf>
              <leaf name="cir">
                <type name="uint32">
                  <range value="0..4294967295"/>
                </type>
                <units name="kbit/s"/>
                <mandatory value="true"/>
                <description>
                  <text>Committed information rate.</text>
                </description>
              </leaf>
              <leaf name="cbs">
                <type name="uint32">
                  <range value="0..4294967295"/>
                </type>
                <units name="Byte"/>
                <mandatory value="true"/>
                <description>
                  <text>Committed burst size.</text>
                </description>
              </leaf>
              <leaf name="green-action">
                <type name="color-action-type"/>
                <mandatory value="true"/>
                <description>
                  <text>Action performed to the green packets.</text>
                </description>
              </leaf>
              <leaf name="green-service-class">
                <when condition="not(../green-action='discard') or ../green-action='pass'"/>
                <type name="qos-service-class"/>
                <must condition="../green-color"/>
                <description>
                  <text>Service class marked to the green packets.</text>
                </description>
              </leaf>
              <leaf name="green-color">
                <when condition="../green-service-class"/>
                <type name="qos-color"/>
                <description>
                  <text>Color marked to the green packets.</text>
                </description>
              </leaf>
              <leaf name="red-action">
                <type name="color-action-type"/>
                <mandatory value="true"/>
                <description>
                  <text>Action performed to the red packets.</text>
                </description>
              </leaf>
              <leaf name="red-service-class">
                <when condition="not(../red-action='discard') or ../red-action='pass'"/>
                <type name="qos-service-class"/>
                <must condition="../red-color"/>
                <description>
                  <text>Service class marked to the red packets.</text>
                </description>
              </leaf>
              <leaf name="red-color">
                <when condition="../red-service-class"/>
                <type name="qos-color"/>
                <description>
                  <text>Color marked to the red packets.</text>
                </description>
              </leaf>
            </list>
          </container>
          <container name="load-action">
            <description>
              <text>Configure load balancing action.</text>
            </description>
            <leaf name="balance-type">
              <type name="load-balance-type"/>
              <description>
                <text>Load balancing type.</text>
              </description>
            </leaf>
          </container>
          <container name="match-term-action">
            <description>
              <text>Configure match termination action.</text>
            </description>
            <leaf name="match-term">
              <type name="priority-aware-type"/>
              <description>
                <text>Match termination type.</text>
              </description>
            </leaf>
          </container>
          <container name="netstream-sampler-actions">
            <description>
              <text>List of netstream sampling action.</text>
            </description>
            <list name="netstream-sampler-action">
              <key value="flow-type"/>
              <max-elements value="2"/>
              <description>
                <text>Configure netstream sampling action.</text>
              </description>
              <leaf name="flow-type">
                <type name="ip-type"/>
                <description>
                  <text>Flow type.</text>
                </description>
              </leaf>
              <leaf name="sample-type">
                <type name="sample-type"/>
                <mandatory value="true"/>
                <description>
                  <text>Sampling type.</text>
                </description>
              </leaf>
              <leaf name="sample-value">
                <type name="uint16">
                  <range value="1..65535"/>
                </type>
                <must condition="(../sample-type='fix-time' and ../sample-value&gt;=5 and ../sample-value&lt;=30000) or not(../sample-type='fix-time')"/>
                <mandatory value="true"/>
                <description>
                  <text>Sampling parameter. When sampling type is netstream fix packets sampler feature or netstream random packets sampler feature the sampling parameter is 1~65535, when sampling type is netstream fix time sampler feature the sampling parameter is 5~30000.</text>
                </description>
              </leaf>
            </list>
          </container>
          <container name="urpf-action">
            <presence value="create urpf-action"/>
            <description>
              <text>Enable/disable urpf action.</text>
            </description>
            <leaf name="check-type">
              <type name="check-type"/>
              <mandatory value="true"/>
              <description>
                <text>Type of the urpf check.</text>
              </description>
            </leaf>
            <leaf name="allow-default">
              <type name="priority-aware-type"/>
              <description>
                <text>To check the default route.</text>
              </description>
            </leaf>
            <leaf name="ignore-default">
              <type name="ignore-default-type"/>
              <description>
                <text>Indicates a mode in which the default route is excluded.</text>
              </description>
            </leaf>
          </container>
          <container name="except-deny-action">
            <description>
              <text>Configure IP netstream sampler except deny-action.</text>
            </description>
            <leaf name="deny-action">
              <type name="priority-aware-type"/>
              <description>
                <text>DenyAction type.</text>
              </description>
            </leaf>
          </container>
          <container name="user-queue-action">
            <presence value="create user-queue-action"/>
            <description>
              <text>Enable/disable user queue action.</text>
            </description>
            <leaf name="cir">
              <type name="uint32">
                <range value="0 | 16..4294967294"/>
              </type>
              <units name="kbit/s"/>
              <mandatory value="true"/>
              <description>
                <text>Committed information rate.</text>
              </description>
            </leaf>
            <leaf name="pir">
              <type name="uint32">
                <range value="0 | 16..4294967294"/>
              </type>
              <units name="kbit/s"/>
              <must condition="../pir&gt;=../cir"/>
              <mandatory value="true"/>
              <description>
                <text>Peak information rate.</text>
              </description>
            </leaf>
            <leaf name="flow-queue-name">
              <type name="leafref">
                <path value="../../../../../queue-scheduler-template/flow-queues/flow-queue/name"/>
              </type>
              <must condition="../../../../../qos:queue-scheduler-template/qos:flow-queues/qos:flow-queue[qos:name=current()]/qos:mode='normal'"/>
              <description>
                <text>Flow queue.</text>
              </description>
            </leaf>
            <leaf name="flow-mapping-name">
              <type name="leafref">
                <path value="../../../../../queue-scheduler-template/flow-mappings/flow-mapping/name"/>
              </type>
              <description>
                <text>Flow mapping.</text>
              </description>
            </leaf>
            <leaf name="user-group-queue-name">
              <type name="leafref">
                <path value="../../../../../queue-scheduler-template/user-group-queues/user-group-queue/name"/>
              </type>
              <must condition="/qos:qos/qos:queue-scheduler-template/qos:user-group-queues/qos:user-group-queue[qos:name=current()]/qos:mode='normal'"/>
              <description>
                <text>User group queue.</text>
              </description>
            </leaf>
            <leaf name="service-template-name">
              <type name="leafref">
                <path value="../../../../../queue-scheduler-template/service-templates/service-template/name"/>
              </type>
              <description>
                <text>Service template.</text>
              </description>
            </leaf>
          </container>
          <container name="system-log-action">
            <presence value="create system-log-action"/>
            <description>
              <text>Enable/disable system log.</text>
            </description>
            <leaf name="threshold">
              <type name="uint32">
                <range value="1..4294967295"/>
              </type>
              <mandatory value="true"/>
              <description>
                <text>Log threshold.</text>
              </description>
            </leaf>
            <leaf name="statistics-type">
              <type name="statistics-type"/>
              <mandatory value="true"/>
              <description>
                <text>Statistics type.</text>
              </description>
            </leaf>
          </container>
        </list>
      </container>
      <container name="traffic-policys">
        <description>
          <text>List of traffic policies.</text>
        </description>
        <list name="traffic-policy">
          <key value="name"/>
          <description>
            <text>Configure the traffic policy.</text>
          </description>
          <leaf name="name">
            <type name="string">
