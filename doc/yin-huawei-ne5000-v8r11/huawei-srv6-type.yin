  <typedef name="protocol-type">
    <type name="enumeration">
      <enum name="null">
        <value value="0"/>
        <description>
          <text>NULL.</text>
        </description>
      </enum>
      <enum name="evpn">
        <value value="1"/>
        <description>
          <text>EVPN.</text>
        </description>
      </enum>
      <enum name="evpn-l3vpn">
        <value value="2"/>
        <description>
          <text>EVPN L3VPN.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Protocol Type.</text>
    </description>
  </typedef>
  <typedef name="mode-type">
    <type name="enumeration">
      <enum name="uniform">
        <value value="1"/>
        <description>
          <text>Uniform mode.</text>
        </description>
      </enum>
      <enum name="pipe">
        <value value="2"/>
        <description>
          <text>Pipe mode.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Pipe mode types.</text>
    </description>
  </typedef>
  <typedef name="service-class">
    <type name="enumeration">
      <enum name="be">
        <value value="0"/>
        <description>
          <text>Service name is BE.</text>
        </description>
      </enum>
      <enum name="af1">
        <value value="1"/>
        <description>
          <text>Service name is AF1.</text>
        </description>
      </enum>
      <enum name="af2">
        <value value="2"/>
        <description>
          <text>Service name is AF2.</text>
        </description>
      </enum>
      <enum name="af3">
        <value value="3"/>
        <description>
          <text>Service name is AF3.</text>
        </description>
      </enum>
      <enum name="af4">
        <value value="4"/>
        <description>
          <text>Service name is AF4.</text>
        </description>
      </enum>
      <enum name="ef">
        <value value="5"/>
        <description>
          <text>Service name is EF.</text>
        </description>
      </enum>
      <enum name="cs6">
        <value value="6"/>
        <description>
          <text>Service name is CS6.</text>
        </description>
      </enum>
      <enum name="cs7">
        <value value="7"/>
        <description>
          <text>Service name is CS7.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Service type for the packet.</text>
    </description>
  </typedef>
  <typedef name="color">
    <type name="enumeration">
      <enum name="green">
        <value value="0"/>
        <description>
          <text>Green Color.</text>
        </description>
      </enum>
      <enum name="yellow">
        <value value="1"/>
        <description>
          <text>Yellow Color.</text>
        </description>
      </enum>
      <enum name="red">
        <value value="2"/>
        <description>
          <text>Red Color.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Color type for the packet.</text>
    </description>
  </typedef>
  <typedef name="inner-type">
    <type name="enumeration">
      <enum name="null">
        <value value="0"/>
        <description>
          <text>Indicates that the inner type is not configured.</text>
        </description>
      </enum>
      <enum name="ipv4">
        <value value="1"/>
        <description>
          <text>The inner type is IPv4.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Indicates the type of the original packet sent from the SFF to the SF.</text>
    </description>
  </typedef>
  <typedef name="match-type">
    <type name="enumeration">
      <enum name="none">
        <value value="0"/>
        <description>
          <text>The mapping policy type is none.</text>
        </description>
      </enum>
      <enum name="sr-policy-dscp">
        <value value="1"/>
        <description>
          <text>SR Policy DSCP.</text>
        </description>
      </enum>
    </type>
    <description>
