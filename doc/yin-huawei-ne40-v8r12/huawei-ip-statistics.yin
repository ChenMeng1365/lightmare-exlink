        <description>
          <text>Total number of IPv6 multicast packet bytes received on an interface.</text>
        </description>
      </leaf>
      <leaf name="out-ucast-pkts">
        <type name="uint64"/>
        <units name="packet"/>
        <description>
          <text>Total number of IPv6 unicast packets sent on an interface.</text>
        </description>
      </leaf>
      <leaf name="out-ucast-octets">
        <type name="uint64"/>
        <units name="Octet"/>
        <description>
          <text>Total number of IPv6 unicast packet bytes sent on an interface.</text>
        </description>
      </leaf>
      <leaf name="out-mcast-pkts">
        <type name="uint64"/>
        <units name="packet"/>
        <description>
          <text>Total number of IPv6 multicast packets sent on an interface.</text>
        </description>
      </leaf>
      <leaf name="out-mcast-octets">
        <type name="uint64"/>
        <units name="Octet"/>
        <description>
          <text>Total number of IPv6 multicast packet bytes sent on an interface.</text>
        </description>
      </leaf>
      <leaf name="out-rate-pkts">
        <type name="uint64"/>
        <units name="packet"/>
        <description>
          <text>Rate for sending IPv6 packets on an interface.</text>
        </description>
      </leaf>
      <leaf name="out-rate-bits">
        <type name="uint64"/>
        <units name="bit/s"/>
        <description>
          <text>Rate for sending IPv6 packet bits on an interface.</text>
        </description>
      </leaf>
      <leaf name="out-rate-byte-bandwidth-usage">
        <type name="string">
          <length value="1..7"/>
        </type>
        <description>
          <text>Bandwidth usage for an interface to send IPv6 packets.</text>
        </description>
      </leaf>
      <leaf name="out-transmits">
        <type name="uint64"/>
        <units name="packet"/>
        <description>
          <text>Total number of IPv6 packets sent on an interface.</text>
        </description>
      </leaf>
      <leaf name="out-octets">
