  <typedef name="pce-client-fsm-state">
    <type name="enumeration">
      <enum name="idle">
        <value value="0"/>
        <description>
          <text>The FSM status of a PCE client is idle.</text>
        </description>
      </enum>
      <enum name="tcp-pending">
        <value value="1"/>
        <description>
          <text>The FSM status of a PCE client is pending.</text>
        </description>
      </enum>
      <enum name="open-wait">
        <value value="2"/>
        <description>
          <text>The FSM status of a PCE client is open-wait.</text>
        </description>
      </enum>
      <enum name="keep-wait">
        <value value="3"/>
        <description>
          <text>The FSM status of a PCE client is keep-wait.</text>
        </description>
      </enum>
      <enum name="up">
        <value value="4"/>
        <description>
          <text>The FSM status of a PCE client is Up.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>PCE FSM state.</text>
    </description>
  </typedef>
  <typedef name="pce-client-ssn-down-reason-type">
    <type name="enumeration">
      <enum name="none">
        <value value="0"/>
