      <enum name="ospf">
        <value value="3"/>
        <description>
          <text>OSPF.</text>
        </description>
      </enum>
      <enum name="direct">
        <value value="4"/>
        <description>
          <text>Direct.</text>
        </description>
      </enum>
      <enum name="static">
        <value value="5"/>
        <description>
          <text>Static.</text>
        </description>
      </enum>
      <enum name="ospfv3">
        <value value="6"/>
        <description>
          <text>OSPFv3.</text>
        </description>
      </enum>
      <enum name="bgp">
        <value value="7"/>
        <description>
          <text>BGP protocol.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>The type of the bgpcommLsp protocol.</text>
    </description>
  </typedef>
  <grouping name="route-key-element-type">
