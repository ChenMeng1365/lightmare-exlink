          <leaf name="port-send-announce">
            <type name="uint32"/>
            <units name="packet"/>
            <default value="0"/>
            <description>
              <text>Number of Announce packets sent on a PTP port.</text>
            </description>
          </leaf>
          <leaf name="port-send-sync">
            <type name="uint32"/>
            <units name="packet"/>
            <default value="0"/>
            <description>
              <text>Number of Sync packets sent on a PTP port.</text>
            </description>
          </leaf>
          <leaf name="port-send-req">
            <type name="uint32"/>
            <units name="packet"/>
            <default value="0"/>
            <description>
              <text>Number of Request packets sent on a PTP port.</text>
            </description>
          </leaf>
          <leaf name="port-send-resp">
            <type name="uint32"/>
            <units name="packet"/>
            <default value="0"/>
            <description>
              <text>Number of Response packets sent on a PTP port.</text>
            </description>
          </leaf>
          <leaf name="port-send-followup">
            <type name="uint32"/>
            <units name="packet"/>
            <default value="0"/>
            <description>
              <text>Number of FollowUp packets sent on a PTP port.</text>
            </description>
          </leaf>
          <leaf name="port-send-pdresp-followup">
            <type name="uint32"/>
            <units name="packet"/>
            <default value="0"/>
            <description>
              <text>Number of Pdelay Response FollowUp packets sent on a PTP port.</text>
            </description>
          </leaf>
          <leaf name="port-discard-announce">
            <type name="uint32"/>
            <units name="packet"/>
            <default value="0"/>
            <description>
              <text>Number of Announce packets discarded on a PTP port.</text>
            </description>
          </leaf>
          <leaf name="port-discard-sync">
            <type name="uint32"/>
            <units name="packet"/>
            <default value="0"/>
            <description>
              <text>Number of Sync packets discarded on a PTP port.</text>
            </description>
          </leaf>
          <leaf name="port-discard-delayreq">
