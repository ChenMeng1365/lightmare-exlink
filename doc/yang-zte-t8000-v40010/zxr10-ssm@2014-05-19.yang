  container operation {
    description 'The operation of SSM.' ;
    container ssm {
      description 'The node of SSM.' ;
      container wtr-clear-action {
        description 'Clear wait to restore action.' ;
        choice wtr-clear {
          description 'To clear wait to restore state.' ;
          case clear-oneclock {
            description 'To clear one clock wait to restore state.' ;
            leaf if-name {
              description 'The interface name of clock,inner-clk or PTP1588-clk'
                        + ' is NULL.'
                          ;
              type string {
                length 0..31 ;
              }
            }
            leaf clock-type {
              description 'The clock type.' ;
              type enumeration {
                enum external-clk {
                  value 1 ;
                  description 'External clock.' ;
                }
                enum line-clk {
                  value 2 ;
                  description 'Line clock.' ;
                }
                enum inner-clk {
                  value 3 ;
                  description 'Internal clock.' ;
                }
                enum tributeline-clk {
                  value 5 ;
                  description 'Tribute line clock.' ;
                }
                enum PTP1588-clk {
                  value 6 ;
                  description 'PTP clock.' ;
                }
                enum gps-clk {
                  value 7 ;
                  description 'GPS clock.' ;
                }
                enum stm-clk {
                  value 8 ;
                  description 'Stm-n clock.' ;
                }
              }
            }
          }
          case clear-allclock {
            description 'To clear all clock wait to restore state.' ;
            leaf allclock {
              description 'The flag of all clock ports.' ;
              type uint8 {
                range 1 ;
              }
            }
          }
        }
      }
    }
  }
