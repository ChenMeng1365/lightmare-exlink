                <text>PIM enable mode.</text>
              </description>
            </leaf>
            <leaf name="state-refresh-capable">
              <when condition="../../../../../ni:name='_public_' and ../type='ipv4unicast'"/>
              <type name="boolean"/>
              <default value="true"/>
              <description>
                <text>Enable/disable state-refresh.</text>
              </description>
            </leaf>
            <leaf name="graft-retry">
              <when condition="../../../../../ni:name='_public_' and ../type='ipv4unicast'"/>
              <type name="uint32">
                <range value="1..65535"/>
              </type>
              <default value="3"/>
