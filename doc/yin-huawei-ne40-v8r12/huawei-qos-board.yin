          <text>Configure over ride outbound threshold adjust.</text>
        </description>
        <leaf name="upgrade-threshold">
          <type name="uint8">
            <range value="2..99"/>
          </type>
          <default value="80"/>
          <description>
            <text>Outbound upgrade threshold.</text>
          </description>
        </leaf>
        <leaf name="degrade-threshold">
          <type name="uint8">
            <range value="1..98"/>
          </type>
          <default value="60"/>
          <description>
            <text>Outbound degrade threshold.</text>
          </description>
