<?xml version="1.0" encoding="UTF-8"?>
<module name="huawei-eai-service-notification"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:eai-service-notification="urn:huawei:yang:huawei-eai-service-notification"
        xmlns:ext="urn:huawei:yang:huawei-extension">
  <yang-version value="1"/>
  <namespace uri="urn:huawei:yang:huawei-eai-service-notification"/>
  <prefix value="eai-service-notification"/>
  <import module="huawei-extension">
    <prefix value="ext"/>
  </import>
  <organization>
    <text>Huawei Technologies Co., Ltd.</text>
  </organization>
  <contact>
    <text>Huawei Industrial Base
Bantian, Longgang
Shenzhen 518129
People's Republic of China
Website: http://www.huawei.com
Email: support@huawei.com</text>
  </contact>
  <description>
    <text>Notification of eai-service-notification.</text>
  </description>
  <revision date="2020-05-28">
    <description>
      <text>Initial revision.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <ext:task-name value="eai"/>
  <notification name="anomaly-packet-loss">
    <description>
      <text>Interface flow sudden change and forwarding chip packets loss is detected.</text>
    </description>
    <leaf name="interface-names">
      <type name="string">
        <length value="1..665"/>
      </type>
      <description>
        <text>Interface name.</text>
      </description>
    </leaf>
    <leaf name="forwarding-chip-info">
      <type name="string">
        <length value="1..800"/>
      </type>
      <description>
        <text>Forwarding chip info.</text>
      </description>
    </leaf>
  </notification>
  <notification name="anomaly-packet-loss-resume">
    <description>
      <text>Interface flow sudden change and forwarding chip packets loss is resumed.</text>
    </description>
    <leaf name="interface-names">
      <type name="string">
        <length value="1..665"/>
      </type>
      <description>
        <text>Interface name.</text>
      </description>
    </leaf>
    <leaf name="forwarding-chip-info">
      <type name="string">
        <length value="1..800"/>
      </type>
      <description>
        <text>Forwarding chip info.</text>
      </description>
    </leaf>
  </notification>
  <notification name="resource-predict-over-threshold">
    <description>
      <text>Resource trend predict will exceed threshold under current circumstance.</text>
    </description>
    <leaf name="chassis-id">
      <type name="uint32"/>
      <description>
        <text>Chassis id.</text>
      </description>
    </leaf>
    <leaf name="slot-id">
      <type name="uint32"/>
      <description>
        <text>Slot id.</text>
      </description>
    </leaf>
    <leaf name="object">
      <type name="string">
        <length value="1..128"/>
      </type>
      <description>
        <text>Object name.</text>
      </description>
    </leaf>
    <leaf name="reason-id">
      <type name="int32"/>
      <description>
        <text>Reason id.</text>
      </description>
    </leaf>
    <leaf name="reason-description">
      <type name="string">
        <length value="1..256"/>
      </type>
      <description>
        <text>Reason description.</text>
      </description>
    </leaf>
  </notification>
  <notification name="resource-predict-over-threshold-resume">
    <description>
      <text>Resource trend predict over threshold resume.</text>
    </description>
    <leaf name="chassis-id">
      <type name="uint32"/>
      <description>
        <text>Chassis id.</text>
      </description>
    </leaf>
    <leaf name="slot-id">
      <type name="uint32"/>
      <description>
        <text>Slot id.</text>
      </description>
    </leaf>
    <leaf name="object">
      <type name="string">
        <length value="1..128"/>
      </type>
      <description>
        <text>Object name.</text>
      </description>
    </leaf>
    <leaf name="reason-id">
      <type name="int32"/>
      <description>
        <text>Reason id.</text>
      </description>
    </leaf>
    <leaf name="reason-description">
      <type name="string">
        <length value="1..256"/>
      </type>
      <description>
        <text>Reason description.</text>
      </description>
    </leaf>
  </notification>
</module>
