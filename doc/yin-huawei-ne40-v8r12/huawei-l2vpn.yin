            </leaf>
            <leaf name="in-bytes">
              <type name="uint64"/>
              <units name="Byte"/>
              <description>
                <text>Number of bytes received by the PW.</text>
              </description>
            </leaf>
            <leaf name="out-bytes">
              <type name="uint64"/>
              <units name="Byte"/>
              <description>
                <text>Number of bytes sent by the PW.</text>
              </description>
            </leaf>
            <leaf name="in-frames">
              <type name="uint64"/>
              <units name="packet"/>
              <description>
                <text>Number of frames received by the PW.</text>
              </description>
            </leaf>
            <leaf name="out-frames">
              <type name="uint64"/>
              <units name="packet"/>
              <description>
                <text>Number of frames sent by the PW.</text>
              </description>
            </leaf>
            <leaf name="in-unicast-frames">
              <type name="uint64"/>
              <units name="packet"/>
              <description>
                <text>Number of unicast frames received by the PW.</text>
              </description>
            </leaf>
            <leaf name="in-unicast-bytes">
              <type name="uint64"/>
              <units name="Byte"/>
              <description>
                <text>Number of unicast bytes received by the PW.</text>
              </description>
            </leaf>
            <leaf name="in-multicast-frames">
              <type name="uint64"/>
              <units name="packet"/>
              <description>
                <text>Number of multicast frames received by the PW.</text>
              </description>
            </leaf>
            <leaf name="in-multicast-bytes">
              <type name="uint64"/>
              <units name="Byte"/>
              <description>
                <text>Number of multicast bytes received by the PW.</text>
              </description>
            </leaf>
            <leaf name="in-broadcast-frames">
              <type name="uint64"/>
              <units name="packet"/>
              <description>
                <text>Number of broadcast frames received by the PW.</text>
              </description>
            </leaf>
            <leaf name="in-broadcast-bytes">
              <type name="uint64"/>
              <units name="Byte"/>
              <description>
                <text>Number of broadcast bytes received by the PW.</text>
              </description>
            </leaf>
            <leaf name="out-unicast-frames">
              <type name="uint64"/>
              <units name="packet"/>
              <description>
                <text>Number of unicast frames sent by the PW.</text>
              </description>
            </leaf>
            <leaf name="out-unicast-bytes">
              <type name="uint64"/>
              <units name="Byte"/>
              <description>
                <text>Number of unicast bytes sent by the PW.</text>
              </description>
            </leaf>
            <leaf name="out-multicast-frames">
              <type name="uint64"/>
              <units name="packet"/>
              <description>
                <text>Number of multicast frames sent by the PW.</text>
              </description>
            </leaf>
            <leaf name="out-multicast-bytes">
              <type name="uint64"/>
              <units name="Byte"/>
              <description>
                <text>Number of multicast bytes sent by the PW.</text>
              </description>
            </leaf>
            <leaf name="out-broadcast-frames">
              <type name="uint64"/>
              <units name="packet"/>
              <description>
                <text>Number of broadcast frames sent by the PW.</text>
              </description>
            </leaf>
            <leaf name="out-broadcast-bytes">
              <type name="uint64"/>
              <units name="Byte"/>
              <description>
                <text>Number of broadcast bytes sent by the PW.</text>
              </description>
            </leaf>
            <leaf name="in-discard-frames">
              <type name="uint64"/>
              <units name="packet"/>
              <description>
                <text>Number of frames received and then dropped by the PW.</text>
              </description>
            </leaf>
            <leaf name="reset-time">
              <type name="yang:date-and-time"/>
              <description>
                <text>Time when statistics were cleared the last time.</text>
              </description>
