<?xml version="1.0" encoding="UTF-8"?>
<module name="huawei-warranty"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:warranty="urn:huawei:yang:huawei-warranty"
        xmlns:pub-type="urn:huawei:yang:huawei-pub-type"
        xmlns:ext="urn:huawei:yang:huawei-extension">
  <namespace uri="urn:huawei:yang:huawei-warranty"/>
  <prefix value="warranty"/>
  <import module="huawei-pub-type">
    <prefix value="pub-type"/>
  </import>
  <import module="huawei-extension">
    <prefix value="ext"/>
  </import>
  <organization>
    <text>Huawei Technologies Co., Ltd.</text>
  </organization>
  <contact>
    <text>Huawei Industrial Base
Bantian, Longgang
Shenzhen 518129
People's Republic of China
Website: http://www.huawei.com
Email: support@huawei.com</text>
  </contact>
  <description>
    <text>Electronic warranty management module.</text>
  </description>
  <revision date="2020-04-24">
    <description>
      <text>Init revision.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <ext:task-name value="warranty"/>
  <typedef name="warranty-status">
    <type name="enumeration">
      <enum name="normal">
        <value value="1"/>
        <description>
          <text>Normal.</text>
        </description>
      </enum>
      <enum name="to-be-expired">
        <value value="2"/>
        <description>
          <text>To be expired.</text>
        </description>
      </enum>
      <enum name="expired">
        <value value="3"/>
        <description>
          <text>Expired.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Warranty status.</text>
    </description>
  </typedef>
  <typedef name="unit-type">
    <type name="enumeration">
      <enum name="device">
        <value value="1"/>
        <description>
          <text>The warranty type is device.</text>
        </description>
      </enum>
      <enum name="part">
        <value value="2"/>
        <description>
          <text>The warranty type is part.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Unit type.</text>
    </description>
  </typedef>
  <container name="warranty">
    <description>
      <text>Electronic warranty management module.</text>
    </description>
    <container name="global">
      <description>
        <text>Configure the electronic warranty management alarm feature.</text>
      </description>
      <leaf name="alarm-enable">
        <type name="boolean"/>
        <default value="true"/>
        <description>
          <text>Enable/disable function to report the alarms of electronic warranty management.</text>
        </description>
      </leaf>
    </container>
    <container name="warranty-infos">
      <config value="false"/>
      <description>
        <text>List of information about electronic warranty Informations.</text>
      </description>
      <list name="warranty-info">
        <key value="slot type serial-number"/>
        <description>
          <text>Operational state of information about a electronic warranty.</text>
        </description>
        <leaf name="slot">
          <type name="pub-type:position"/>
          <description>
            <text>Specify the slot path.</text>
          </description>
        </leaf>
        <leaf name="type">
          <type name="unit-type"/>
          <description>
            <text>Specify the type of the electronic warranty.</text>
          </description>
        </leaf>
        <leaf name="serial-number">
          <type name="string">
            <length value="1..100"/>
          </type>
          <description>
            <text>Serial number of the electronic warranty.</text>
          </description>
        </leaf>
        <leaf name="start-date">
          <type name="pub-type:date"/>
          <description>
            <text>Starting point date for the electronic warranty.</text>
          </description>
        </leaf>
        <leaf name="life-span">
          <type name="uint32">
            <range value="1..600"/>
          </type>
          <description>
            <text>Life span for the electronic warranty.</text>
          </description>
        </leaf>
        <leaf name="status">
          <type name="warranty-status"/>
          <description>
            <text>Status for the electronic warranty.</text>
          </description>
        </leaf>
      </list>
    </container>
  </container>
  <rpc name="activate-warranty">
    <description>
      <text>Activate the electronic warranty on a device.</text>
    </description>
    <input>
      <container name="warrantys">
        <description>
          <text>List of information about electronic warranty.</text>
        </description>
        <list name="warranty">
          <key value="serial-number"/>
          <description>
            <text>The list of warranty information to be activated.</text>
          </description>
          <leaf name="serial-number">
            <type name="string">
              <length value="1..100"/>
            </type>
            <mandatory value="true"/>
            <description>
              <text>Serial number of the electronic warranty.</text>
            </description>
          </leaf>
          <leaf name="start-date">
            <type name="pub-type:date"/>
            <description>
              <text>Starting date for the electronic warranty.</text>
            </description>
          </leaf>
          <leaf name="life-span">
            <type name="uint32">
              <range value="1..600"/>
            </type>
            <units name="month"/>
            <description>
              <text>Life span for the electronic warranty.</text>
            </description>
          </leaf>
        </list>
      </container>
    </input>
  </rpc>
</module>
