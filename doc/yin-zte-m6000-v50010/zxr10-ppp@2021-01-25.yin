<?xml version="1.0" encoding="UTF-8"?>
<module name="zxr10-ppp"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:ppp="http://www.zte.com.cn/zxr10/netconf/schema/rosng/ppp"
        xmlns:ncd="http://www.zte.com.cn/zxr10/netconf/schema/rosng/netconf-defs">
  <namespace uri="http://www.zte.com.cn/zxr10/netconf/schema/rosng/ppp"/>
  <prefix value="ppp"/>
  <yang-version value="1"/>
  <import module="zxr10-netconf-defs">
    <prefix value="ncd"/>
  </import>
  <organization>
    <text>ZTE Corporation</text>
  </organization>
  <contact>
    <text>ZTE Corporation  
   Nanjing Institute of ZTE Corporation 
   No.68 Zijinghua Rd. Yuhuatai District, Nanjing, China 
   Tel: +86-25-52870000</text>
  </contact>
  <description>
    <text>The netconf module to describe Point-to-Point Protocol(PPP)</text>
  </description>
