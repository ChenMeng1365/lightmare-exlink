  <feature name="vpls">
    <description>
      <text>MLD snooping over VPLS.</text>
    </description>
  </feature>
  <feature name="vlan">
    <description>
      <text>MLD snooping over VLAN.</text>
    </description>
  </feature>
  <feature name="ring-tree">
    <description>
      <text>MLD snooping over Ring Tree.</text>
    </description>
  </feature>
  <feature name="vbui">
    <description>
      <text>MLD snooping over VBUI.</text>
    </description>
  </feature>
  <feature name="ip-global">
    <description>
      <text>MLD snooping over IP-global.</text>
    </description>
  </feature>
  <feature name="vrf">
    <description>
      <text>MLD snooping over VRF.</text>
    </description>
  </feature>
  <feature name="host-aging-closedown">
    <description>
      <text>MLD snooping support host-aging-closedown.</text>
    </description>
  </feature>
  <feature name="general-state-mrouter">
    <description>
      <text>MLD snooping support eneral state mrouter.</text>
    </description>
  </feature>
  <typedef name="switch-type">
    <type name="enumeration">
      <enum name="disable">
        <value value="0"/>
        <description>
          <text>Disable this switch.</text>
        </description>
      </enum>
      <enum name="enable">
        <value value="1"/>
        <description>
          <text>Enable this switch.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Type of switch status.</text>
    </description>
  </typedef>
  <typedef name="work-mode-type">
    <type name="enumeration">
      <enum name="proxy">
        <value value="0"/>
        <description>
          <text>Proxy mode.</text>
        </description>
      </enum>
      <enum name="route">
        <value value="1"/>
        <description>
          <text>Route mode.</text>
        </description>
      </enum>
      <enum name="transparent">
        <value value="2"/>
        <description>
          <text>Transparent mode.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Type of work mode.</text>
    </description>
  </typedef>
  <typedef name="version-type">
    <type name="enumeration">
      <enum name="v1">
        <value value="1"/>
        <description>
          <text>MLD version1.</text>
        </description>
      </enum>
      <enum name="v2">
        <value value="2"/>
        <description>
          <text>MLD version2.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>The version of MLD.</text>
    </description>
  </typedef>
  <typedef name="mode-type">
    <type name="enumeration">
      <enum name="dynamic">
        <value value="0"/>
        <description>
          <text>Dynamic mode.</text>
        </description>
      </enum>
      <enum name="static">
        <value value="1"/>
        <description>
          <text>Static mode.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Type of mode. include dynamic mode and static mode.</text>
    </description>
  </typedef>
  <container name="configuration">
    <description>
      <text>Configuration model.</text>
    </description>
    <container name="mld-snooping">
      <description>
        <text>Configure MLD snooping parameters.</text>
      </description>
      <container name="global">
        <description>
          <text>MLD snooping global configure.</text>
        </description>
        <leaf name="global-enable">
          <description>
            <text>Enable/disable MLD snooping in this device.Its default value depends on the product.</text>
          </description>
          <type name="switch-type"/>
          <default value="disable"/>
        </leaf>
        <leaf name="global-query-enable">
          <description>
            <text>Enable/disable MLD queries in this device.</text>
          </description>
          <type name="switch-type"/>
          <default value="disable"/>
        </leaf>
        <leaf name="query-interval">
          <description>
            <text>Configure query interval.The query interval is the interval between general queries sent by the querier.</text>
          </description>
          <type name="uint16">
            <range value="30..65535"/>
          </type>
          <units name="second"/>
          <default value="125"/>
        </leaf>
        <leaf name="query-rsp-interval">
          <description>
            <text>Configure query response interval. The response interval inserted into the periodic general queries.</text>
          </description>
          <type name="uint8">
            <range value="1..255"/>
          </type>
          <units name="tick (1 tick = 100ms)"/>
          <default value="100"/>
        </leaf>
        <leaf name="host-aging-closedown">
          <description>
            <text>Configure host aging closedown.</text>
          </description>
          <type name="switch-type"/>
          <default value="disable"/>
          <ncd:revise-date date="2019-12-12"/>
        </leaf>
      </container>
      <container name="vpls">
        <description>
          <text>MLD snooping VPLS configure.User can't config MLD snooping inVPLS if the VPLS instance not exist.If MLD snooping configuration exist in VPLS,the VPLS instance can't be deleted.</text>
        </description>
        <container name="vpls-configs">
          <description>
            <text>The config of instance VPLS.</text>
          </description>
          <list name="vpls-config">
            <description>
              <text>The config of instance VPLS.</text>
            </description>
            <key value="vpls-name"/>
            <leaf name="vpls-name">
              <description>
                <text>Submode instance name of VPLS.</text>
              </description>
              <type name="string">
                <length value="1..32"/>
              </type>
            </leaf>
            <leaf name="snooping-switch">
              <description>
                <text>Enable/disable MLD snooping in this instance.</text>
              </description>
              <type name="switch-type"/>
              <default value="disable"/>
            </leaf>
            <leaf name="member-fast-leave">
              <description>
                <text>Enable/disable group membership fast leave in this instance.</text>
              </description>
              <type name="switch-type"/>
              <default value="disable"/>
            </leaf>
            <leaf name="acl-name">
              <description>
                <text>Configure an ACL rule in this instance.</text>
              </description>
              <type name="string">
                <length value="0..31"/>
              </type>
            </leaf>
            <leaf name="member-time-out">
              <description>
                <text>Member aging time in this instance.</text>
              </description>
              <type name="uint16">
                <range value="30..65535"/>
              </type>
              <units name="second"/>
              <default value="260"/>
            </leaf>
            <leaf name="mrouter-time-out">
              <description>
                <text>Multicast router port aging time in this instance.</text>
              </description>
              <type name="uint16">
                <range value="30..65535"/>
              </type>
              <units name="second"/>
              <default value="260"/>
            </leaf>
            <leaf name="work-mode">
              <description>
                <text>Work mode in this instance: proxy, transparent or router.</text>
              </description>
              <type name="work-mode-type"/>
              <default value="proxy"/>
            </leaf>
            <leaf name="max-group-num">
              <description>
                <text>Max number of multicast group can access in this instance.Its default value and maximum value depends on the product.</text>
              </description>
              <type name="uint32">
                <range value="1..4294967295"/>
              </type>
            </leaf>
            <leaf name="last-mem-qry-interval">
              <description>
                <text>The last member query interval is the max response time inserted into group-specific queries sent in response to leave group messages, and is also the amount of time between group-specific query messages.</text>
              </description>
              <type name="uint16">
                <range value="1..25"/>
              </type>
              <units name="second"/>
              <default value="1"/>
            </leaf>
            <leaf name="query-version">
              <description>
                <text>Querier version: V1 or V2.</text>
              </description>
              <type name="version-type"/>
              <default value="v1"/>
            </leaf>
            <leaf name="query-enable">
              <description>
                <text>Enable/disable querier in this instance.</text>
              </description>
              <type name="switch-type"/>
              <default value="disable"/>
            </leaf>
            <leaf name="ndl-enable">
              <description>
                <text>Enable/disable all ports are not allowed to learn dynamic mrouter port(s).</text>
              </description>
              <type name="switch-type"/>
              <default value="disable"/>
              <ncd:revise-date date="2018-04-02"/>
            </leaf>
          </list>
        </container>
        <container name="drops">
          <description>
            <text>The config of group with drop in instance VPLS.</text>
          </description>
          <list name="drop">
            <description>
              <text>The config of group with drop in instance VPLS.The max-elements depends on the product.</text>
            </description>
            <key value="group-ip vpls-name"/>
            <leaf name="group-ip">
              <description>
                <text>Group IPv6 address.</text>
              </description>
              <type name="ncd:ipv6-address"/>
            </leaf>
            <leaf name="vpls-name">
              <description>
                <text>Submode instance name of VPLS.</text>
              </description>
              <type name="string">
                <length value="1..32"/>
              </type>
            </leaf>
          </list>
        </container>
        <container name="max-member-nums">
          <description>
            <text>The config of group with max-member-num in instance VPLS.</text>
          </description>
          <list name="max-member-num">
            <description>
              <text>The config of group with max-member-num in instance VPLS.The max-elements depends on the product.</text>
            </description>
            <key value="group-ip vpls-name"/>
            <leaf name="group-ip">
              <description>
                <text>Group IPv6 address.</text>
              </description>
              <type name="ncd:ipv6-address"/>
            </leaf>
            <leaf name="vpls-name">
              <description>
                <text>Submode instance name of VPLS.</text>
              </description>
              <type name="string">
                <length value="1..32"/>
              </type>
            </leaf>
            <leaf name="max-member-num">
              <description>
                <text>Max number of MLD member can access to this group.Its default value and maximum value depends on the product.</text>
              </description>
              <type name="uint32">
                <range value="1..4000"/>
              </type>
              <default value="4000"/>
            </leaf>
          </list>
        </container>
        <container name="members">
          <description>
            <text>The config of static member in instance VPLS.</text>
          </description>
          <list name="member">
            <description>
              <text>The config of static member in instance VPLS.The max-elements depends on the product.</text>
            </description>
            <key value="interface-name group-ip vpls-name"/>
            <leaf name="interface-name">
              <description>
                <text>Interface name.</text>
              </description>
              <type name="ncd:ifname"/>
            </leaf>
            <leaf name="group-ip">
              <description>
                <text>Group IPv6 address.</text>
              </description>
              <type name="ncd:ipv6-address"/>
            </leaf>
            <leaf name="vpls-name">
              <description>
                <text>Submode instance name of VPLS.</text>
              </description>
              <type name="string">
                <length value="1..32"/>
              </type>
            </leaf>
          </list>
        </container>
        <container name="mrouters">
          <description>
            <text>The config of static mrouter in instance VPLS.</text>
          </description>
          <list name="mrouter">
            <description>
              <text>The config of static mrouter in instance VPLS.The max-elements depends on the product.</text>
            </description>
            <key value="interface-name vpls-name"/>
            <leaf name="interface-name">
              <description>
                <text>Interface name.</text>
              </description>
              <type name="ncd:ifname"/>
            </leaf>
            <leaf name="vpls-name">
              <description>
                <text>Submode instance name of VPLS.</text>
              </description>
              <type name="string">
                <length value="1..32"/>
              </type>
            </leaf>
          </list>
        </container>
        <container name="m-vpls">
          <description>
            <text>The config of multicast VPLS in instance VPLS.The max-elements depends on the product.</text>
          </description>
          <container name="defaults">
            <description>
              <text>The config of default multicast VPLS in instance VPLS.</text>
            </description>
            <list name="default">
              <description>
                <text>The config of default multicast VPLS in instance VPLS.</text>
              </description>
              <key value="u-vpls-name"/>
              <leaf name="u-vpls-name">
                <description>
                  <text>User VPLS name.</text>
                </description>
                <type name="string">
                  <length value="1..32"/>
                </type>
              </leaf>
              <leaf name="m-vpls-name">
                <description>
                  <text>Multicast VPLS name.</text>
                </description>
                <type name="string">
                  <length value="1..32"/>
                </type>
                <mandatory value="true"/>
              </leaf>
            </list>
          </container>
          <container name="groups">
            <description>
              <text>The specific group of multicast VPLS in instance VPLS.</text>
            </description>
            <list name="group">
              <description>
                <text>The specific group of multicast VPLS in instance VPLS.</text>
              </description>
              <key value="u-vpls-name group-ip"/>
              <leaf name="u-vpls-name">
                <description>
                  <text>User VPLS name.</text>
                </description>
                <type name="string">
                  <length value="1..32"/>
                </type>
              </leaf>
              <leaf name="group-ip">
                <description>
                  <text>Group IPv6 address.</text>
                </description>
                <type name="ncd:ipv6-address"/>
              </leaf>
              <leaf name="m-vpls-name">
                <description>
                  <text>Multicast VPLS name.</text>
                </description>
                <type name="string">
                  <length value="1..32"/>
                </type>
                <mandatory value="true"/>
              </leaf>
            </list>
          </container>
        </container>
        <container name="prejoins">
          <description>
            <text>The config of group with prejoin in instance VPLS.</text>
          </description>
          <list name="prejoin">
            <description>
              <text>The config of group with prejoin in instance VPLS.The max-elements depends on the product.</text>
            </description>
            <key value="group-ip vpls-name"/>
            <leaf name="group-ip">
              <description>
                <text>Group IPv6 address.</text>
              </description>
              <type name="ncd:ipv6-address"/>
            </leaf>
            <leaf name="vpls-name">
              <description>
                <text>Submode instance name of VPLS.</text>
              </description>
              <type name="string">
                <length value="1..32"/>
              </type>
            </leaf>
          </list>
          <ncd:revise-date date="2017-11-20"/>
        </container>
        <container name="ndls">
          <description>
            <text>The config of port not allowed to learn dynamic mrouter in instance VPLS.</text>
          </description>
          <list name="ndl">
            <description>
              <text>The config of port not allowed to learn dynamic mrouter in instance VPLS.</text>
            </description>
            <key value="interface-name vpls-name"/>
            <leaf name="interface-name">
              <description>
                <text>Interface name.</text>
              </description>
              <type name="ncd:ifname"/>
            </leaf>
            <leaf name="vpls-name">
              <description>
                <text>Submode instance name of VPLS.</text>
              </description>
              <type name="string">
                <length value="1..32"/>
              </type>
            </leaf>
          </list>
          <ncd:revise-date date="2018-04-02"/>
        </container>
        <container name="interfaces">
          <description>
            <text>The config of interface in instance VPLS.</text>
          </description>
          <list name="interface">
            <description>
              <text>The config of interface in instance VPLS.</text>
            </description>
            <key value="interface-name vpls-name"/>
            <leaf name="interface-name">
              <description>
                <text>Interface name.</text>
              </description>
              <type name="ncd:ifname"/>
            </leaf>
            <leaf name="vpls-name">
              <description>
                <text>Submode instance name of VPLS.</text>
              </description>
              <type name="string">
                <length value="1..32"/>
              </type>
            </leaf>
            <leaf name="mrouter-guard">
              <description>
                <text>Interface multicast router guard function is enabled.</text>
              </description>
              <type name="switch-type"/>
              <default value="disable"/>
            </leaf>
          </list>
          <ncd:revise-date date="2018-04-02"/>
        </container>
      </container>
      <container name="vlan">
        <description>
          <text>MLD snooping VLAN configure.Can't config MLD snooping in VLANif the VLAN instance not exist.If MLD snooping configuration exist in VLAN,the VLAN instance can't be deleted.</text>
        </description>
        <container name="vlan-configs">
          <description>
            <text>The config of instance VLAN.</text>
          </description>
          <list name="vlan-config">
            <description>
              <text>The config of instance VLAN.</text>
            </description>
            <key value="id"/>
            <leaf name="id">
              <description>
                <text>Submode instance ID of VLAN.</text>
              </description>
              <type name="uint16">
                <range value="1..4094"/>
              </type>
            </leaf>
            <leaf name="snooping-switch">
              <description>
                <text>Enable/disable MLD snooping in this instance.</text>
              </description>
              <type name="switch-type"/>
              <default value="disable"/>
            </leaf>
            <leaf name="member-fast-leave">
              <description>
                <text>Enable/disable group membership fast leave in this instance.</text>
              </description>
              <type name="switch-type"/>
              <default value="disable"/>
            </leaf>
            <leaf name="acl-name">
              <description>
                <text>Configure an ACL rule in this instance.</text>
              </description>
              <type name="string">
                <length value="0..31"/>
              </type>
            </leaf>
            <leaf name="member-time-out">
              <description>
                <text>Member aging time in this instance.</text>
              </description>
              <type name="uint16">
                <range value="30..65535"/>
              </type>
              <units name="second"/>
              <default value="260"/>
            </leaf>
            <leaf name="mrouter-time-out">
              <description>
                <text>Multicast router port aging time in this instance.</text>
              </description>
              <type name="uint16">
                <range value="30..65535"/>
              </type>
              <units name="second"/>
              <default value="260"/>
            </leaf>
            <leaf name="work-mode">
              <description>
                <text>Work mode in this instance: proxy, transparent or router.</text>
              </description>
              <type name="work-mode-type"/>
              <default value="proxy"/>
            </leaf>
