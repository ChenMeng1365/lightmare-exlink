module zxr10-rip {
  namespace 'http://www.zte.com.cn/zxr10/netconf/schema/rosng/rip'
            ;
  prefix rip ;
  yang-version 1 ;
  import zxr10-netconf-defs {
    prefix ncd ;
  }
  organization 'ZTE Corporation'
               ;
  contact 'ZTE Corporation  '+"\n"
        + '   Nanjing Institute of ZTE Corporation '+"\n"
        + '   No.68 Zijinghua Rd. Yuhuatai District, Nanjing, China '+"\n"
        + '   Tel: +86-25-52870000'
          ;
  revision 2020-09-21 {
    description 'Modify the length of route policy name to 200 characters.'
                ;
  }
  revision 2020-07-06 {
    description 'Modify the length of encrypted text.'
                ;
  }
  revision 2020-06-09 {
    description 'Modify the range of route-map, the paths are configuration/rip'
              + '/public-instance/redistributes and configuration/rip/vrf-redis'
              + 'tributes.'
                ;
  }
  revision 2017-09-26 {
    description 'Initial version.'
                ;
  }
  typedef enable-or-disable {
    type enumeration {
      enum disable {
        value 0 ;
        description 'The function is disabled.'
                    ;
      }
      enum enable {
        value 1 ;
        description 'The function is enabled.'
                    ;
      }
    }
    description 'Define the state of function switch.'
                ;
  }
  typedef select-mode-def {
    type enumeration {
      enum global {
        value 1 ;
        description 'The public network.'
                    ;
      }
      enum vpn {
        value 2 ;
        description 'The VPN network.'
                    ;
      }
    }
    description 'The type of network. The "global" means the public network, th'
              + 'e "vpn" means private network.'
                ;
  }
  typedef route-status-def {
    type enumeration {
      enum invalid {
        value 0 ;
        description 'The route is invalid.'
                    ;
      }
      enum invalid-and-best {
        value 1 ;
        description 'The route is invalid but is the best.'
                    ;
      }
      enum valid {
        value 2 ;
        description 'The route is valid but not best.'
                    ;
      }
      enum valid-and-best {
        value 3 ;
        description 'The route is valid and best.'
                    ;
      }
    }
    description 'The definition of route status.'
                ;
  }
  typedef route-time-status-def {
    type enumeration {
      enum normal {
        value 0 ;
        description 'The route is normal.'
                    ;
      }
      enum holddown {
        value 1 ;
        description 'The route is in holddown time.'
                    ;
      }
      enum flush {
        value 2 ;
        description 'The route is out holddown time, possibly to be flushed.'
                    ;
      }
    }
    description 'The definition of route time status.'
                ;
  }
  typedef route-owner-def {
    type enumeration {
      enum unknown {
        value 0 ;
        description 'The route owner is unknown.'
                    ;
      }
      enum connected {
        value 1 ;
        description 'Connected route.'
                    ;
      }
      enum static {
        value 2 ;
        description 'Static route.'
                    ;
      }
      enum ospf-int {
        value 3 ;
        description 'OSPF-INT route.'
                    ;
      }
      enum ospf-ext {
        value 4 ;
        description 'OSPF-EXT route.'
                    ;
      }
      enum isis-l1 {
        value 5 ;
        description 'ISIS-L1 route.'
                    ;
      }
      enum isis-l2 {
        value 6 ;
        description 'ISIS-L2 route.'
                    ;
      }
      enum bgp-int {
        value 7 ;
        description 'BGP-INT route.'
                    ;
      }
      enum bgp-ext {
        value 8 ;
        description 'BGP-EXT route.'
                    ;
      }
      enum rip-connected {
        value 9 ;
        description 'RIP connected route.'
                    ;
      }
      enum rip-discard {
        value 10 ;
        description 'RIP discard route.'
                    ;
      }
      enum rip {
        value 11 ;
        description 'RIP route.'
                    ;
      }
      enum nat {
        value 12 ;
        description 'NAT route.'
                    ;
      }
      enum pat {
        value 13 ;
        description 'PAT route.'
                    ;
      }
      enum user-ipaddr {
        value 14 ;
        description 'User IP address route.'
                    ;
      }
      enum user-network {
        value 15 ;
        description 'User network route.'
                    ;
      }
      enum user-special {
        value 16 ;
        description 'User special route.'
                    ;
      }
      enum ps-busi-addr {
        value 17 ;
        description 'PS-BUSI-address route.'
                    ;
      }
      enum ps-user-addr {
        value 18 ;
        description 'PS-user-address route.'
                    ;
      }
      enum ipsec {
        value 19 ;
        description 'IPSec route.'
                    ;
      }
    }
    description 'The definition of route owner.'
                ;
  }
  typedef rip-version-def {
    type enumeration {
      enum v1 {
        value 1 ;
        description 'RIP version 1.'
                    ;
      }
      enum v2 {
        value 2 ;
        description 'RIP version 2.'
                    ;
      }
    }
    description 'The definition of RIP version.'
                ;
  }
  container configuration {
    description 'The configuration information processing of RIP module.'
                ;
    container rip {
      description 'The configuration of RIP module.'
                  ;
      container public-instance {
        description 'The configuration of RIP public instance.'
                    ;
        leaf version {
          description 'Set routing protocol version.'
                      ;
          type rip-version-def ;
          default v2 ;
        }
        leaf max-paths {
          description 'Maximum number of parallel routes RIP can support.'
                      ;
          type uint8 {
            range 1..32 ;
          }
          default 1 ;
        }
        leaf default-metric {
          description 'Default metric of redistributed routes.'
                      ;
          type uint8 {
            range 1..16 ;
          }
          default 1 ;
        }
        leaf distance {
          description 'Define an administrative distance.'
                      ;
          type uint8 {
            range 1..255 ;
          }
          default 120 ;
        }
        leaf auto-summary {
          description 'Enable automatic network number summarization.'
                      ;
          type enable-or-disable ;
          default enable ;
        }
        leaf validate-update-source {
          description 'Perform sanity checks against source address of routing '
                    + 'updates.'
                      ;
          type enable-or-disable ;
          default enable ;
        }
        leaf flash-update-threshold {
          description 'Specify flash update threshold in second.'
                      ;
          type uint8 {
            range 1..30 ;
          }
          units second ;
          default 5 ;
        }
        leaf bfd {
          description 'Configure all interfaces BFD capability.'
                      ;
          type enable-or-disable ;
          default disable ;
        }
        leaf fast-reroute {
          description 'Enable RIP fast-reroute function.'
                      ;
          type enable-or-disable ;
          default disable ;
        }
        leaf track-name {
          description 'Event tracking.'
                      ;
          type string {
            length 1..31 ;
          }
        }
        container timer-basic {
          description 'Adjust routing timers.'
                      ;
          presence timer-basic ;
          leaf update-time {
            description 'Interval between updates, default is 30(seconds).'
                        ;
            type uint32 {
              range 1..65535 ;
            }
            units second ;
            mandatory true ;
          }
          leaf invalid-time {
            description 'Invalid time, default is 180(seconds).'
                        ;
            type uint32 {
              range 1..65535 ;
            }
            units second ;
            mandatory true ;
          }
          leaf holddown-time {
            description 'Holddown time, default is 180(seconds).'
                        ;
            type uint32 {
              range 0..65535 ;
            }
            units second ;
            mandatory true ;
          }
          leaf flush-time {
            description 'Flush time, default is 240(seconds).'
                        ;
            type uint32 {
              range 1..65535 ;
            }
            units second ;
            mandatory true ;
          }
        }
        container output-delay {
          description 'Interpacket delay of RIP updates.'
                      ;
          presence output-delay ;
          leaf packets {
            description 'Packet number.'
                        ;
            type uint32 {
              range 1..4294967295 ;
            }
            mandatory true ;
          }
          leaf delay-time {
            description 'Delay in milliseconds.'
                        ;
            type uint8 {
              range 1..100 ;
            }
            units millisecond ;
            mandatory true ;
          }
        }
        container networks {
          description 'The configuration of RIP networks in public instance.'
                      ;
          list network {
            description 'Enable routing on an IP network.'
                        ;
            key 'network-addr network-wildmask'
                ;
            leaf network-addr {
              description 'Network address.'
                          ;
              type ncd:ipv4-address ;
            }
            leaf network-wildmask {
              description 'Network wild mask.'
                          ;
              type ncd:ipv4-imask ;
            }
          }
        }
        container neighbors {
          description 'The configuration of RIP neighbors in public instance.'
                      ;
          list neighbor {
            description 'Specify a neighbor router.'
                        ;
            key neighbor-addr ;
            leaf neighbor-addr {
              description 'Neighbor IP address.'
                          ;
              type ncd:ipv4-address ;
            }
          }
        }
        container redistributes {
          description 'Redistribute information from another routing protocol.'
                      ;
          container redistribute-isiss {
            description 'Redistribute information from ISIS.'
                        ;
            list redistribute-isis {
              description 'Redistribute information from ISIS.'
                          ;
              key 'protocol process-id'
                  ;
              leaf protocol {
                description 'Routing protocol type.'
                            ;
                type enumeration {
                  enum isis-1 {
                    value 6 ;
                    description 'IS-IS level-1 routes.'
                                ;
                  }
                  enum isis-2 {
                    value 7 ;
                    description 'IS-IS level-2 routes.'
                                ;
                  }
                }
              }
              leaf process-id {
                description 'Process ID of ISIS.'
                            ;
                type uint32 {
                  range 0..65535 ;
                }
              }
              leaf metric {
                description 'Metric for redistributed routes.'
                            ;
                type uint8 {
                  range 1..16 ;
                }
              }
              leaf route-map {
                description 'Apply route-map to redistribute.'
                            ;
                type string {
                  length 1..200 ;
                }
              }
              leaf route-policy {
                description 'Apply route-policy to redistribute.'
                            ;
                type string {
                  length 1..200 ;
                }
              }
            }
          }
          container redistribute-ospfs {
            description 'Redistribute information from OSPF.'
                        ;
            list redistribute-ospf {
              description 'Redistribute information from OSPF.'
                          ;
              key 'protocol process-id'
                  ;
              leaf protocol {
                description 'Routing protocol type.'
                            ;
                type enumeration {
                  enum ospf-int {
                    value 2 ;
                    description 'Open shortest path first(OSPF) internal routes.'
                                ;
                  }
                  enum ospf-ext {
                    value 3 ;
                    description 'Open shortest path first(OSPF) external routes.'
                                ;
                  }
                }
              }
              leaf process-id {
                description 'Process ID of OSPF.'
                            ;
                type uint32 {
                  range 1..65535 ;
                }
              }
              leaf metric {
                description 'Metric for redistributed routes.'
                            ;
                type uint8 {
                  range 1..16 ;
                }
              }
              leaf route-map {
                description 'Apply route-map to redistribute.'
                            ;
                type string {
                  length 1..200 ;
                }
              }
              leaf route-policy {
                description 'Apply route-policy to redistribute.'
                            ;
                type string {
                  length 1..200 ;
                }
              }
            }
          }
          container redistribute-pss {
            description 'Redistribute information from PS.'
                        ;
            list redistribute-ps {
              description 'Redistribute information from PS.'
                          ;
              key protocol ;
              leaf protocol {
                description 'Routing protocol type.'
                            ;
                type enumeration {
                  enum ps-busi-addr {
                    value 12 ;
                    description 'Ps-busi-addr routes.'
                                ;
                  }
                  enum ps-user-addr {
                    value 13 ;
                    description 'Ps-user-addr routes.'
                                ;
                  }
                }
              }
              leaf metric {
                description 'Configure metric for redistributed routes.'
                            ;
                type uint8 {
                  range 1..16 ;
                }
              }
              leaf with-originate-metric {
                description 'Use original metric of redistributed route.'
                            ;
                type empty ;
              }
              leaf route-map {
                description 'Apply route-map to redistribute.'
                            ;
                type string {
                  length 1..200 ;
                }
              }
              leaf route-policy {
                description 'Apply route-policy to redistribute.'
                            ;
                type string {
                  length 1..200 ;
                }
              }
            }
          }
          container redistribute-others {
            description 'Redistribute information from another routing protocol.'
                        ;
            list redistribute-other {
              description 'Redistribute information from another routing protoc'
                        + 'ol.'
                          ;
              key protocol ;
              leaf protocol {
                description 'Routing protocol type.'
                            ;
                type enumeration {
                  enum connected {
                    value 0 ;
                    description 'Connected routes.'
                                ;
                  }
                  enum static {
                    value 1 ;
                    description 'Static routes.'
                                ;
                  }
                  enum bgp-ext {
                    value 4 ;
                    description 'External border gateway protocol (EBGP) routes.'
                                ;
                  }
                  enum bgp-int {
                    value 5 ;
                    description 'Internal border gateway protocol (IBGP) routes.'
                                ;
                  }
                  enum nat {
                    value 8 ;
                    description 'NAT routes.'
                                ;
                  }
                  enum natpt {
                    value 9 ;
                    description 'Port-address Translation routes.'
                                ;
                  }
                  enum subscriber-host {
                    value 10 ;
                    description "Bras subscriber's host routes."
                                ;
                  }
                  enum subscriber-aggregation {
                    value 11 ;
                    description "Bras subscriber's aggregation routes."
                                ;
                  }
                  enum user-special {
                    value 14 ;
                    description 'User-special routes.'
                                ;
                  }
                  enum ipsec {
                    value 15 ;
                    description 'IPsec routes.'
                                ;
                  }
                }
              }
              leaf metric {
                description 'Metric for redistributed routes.'
                            ;
                type uint8 {
                  range 1..16 ;
                }
              }
              leaf route-map {
                description 'Apply route-map to redistribute.'
                            ;
                type string {
                  length 1..200 ;
                }
              }
              leaf route-policy {
                description 'Apply route-policy to redistribute.'
                            ;
                type string {
                  length 1..200 ;
                }
              }
            }
          }
        }
        container offset-lists {
          description 'Apply offset lists to RIP.'
                      ;
          container for-insts {
            description 'Apply offset lists to RIP instance.'
                        ;
            presence for-insts ;
            list for-inst {
              description 'Apply offset lists to RIP instance.'
                          ;
              key in-or-out ;
              leaf in-or-out {
                description 'Specify the direction of offset list.'
                            ;
                type enumeration {
                  enum in {
                    value 1 ;
                    description 'Inbound packets.'
                                ;
                  }
                  enum out {
                    value 2 ;
                    description 'Outbound packets.'
                                ;
                  }
                }
              }
              leaf acl-name {
                description 'Access list of networks to apply offset list(1-31 '
                          + 'characters).'
                            ;
                type string {
                  length 1..31 ;
                }
                mandatory true ;
              }
              leaf metric {
                description 'Offset list metric.'
                            ;
                type uint8 {
                  range 0..16 ;
                }
                mandatory true ;
              }
            }
          }
          container for-ifs {
            description 'Apply offset lists to RIP interface.'
                        ;
            presence for-ifs ;
            list for-if {
              description 'Apply offset lists to RIP interface.'
                          ;
              key 'in-or-out interface'
                  ;
              leaf in-or-out {
                description 'Specify the direction of offset list.'
                            ;
                type enumeration {
                  enum in {
                    value 1 ;
                    description 'Inbound packets.'
                                ;
                  }
                  enum out {
                    value 2 ;
                    description 'Outbound packets.'
                                ;
                  }
                }
              }
              leaf interface {
                description 'Interface name.'
                            ;
                type ncd:ifname {
                  length 1..31 ;
                }
              }
              leaf acl-name {
                description 'Access list of networks to apply offset (1-31 char'
                          + 'acters).'
                            ;
                type string {
                  length 1..31 ;
                }
                mandatory true ;
              }
              leaf metric {
                description 'Offset list metric.'
                            ;
                type uint8 {
                  range 0..16 ;
                }
                mandatory true ;
              }
            }
          }
        }
        container interfaces {
          description 'Configuration of RIP interfaces.'
                      ;
          list interface {
            description 'Configuration of RIP interface.'
                        ;
            key if-name ;
            leaf if-name {
              description 'Interface name.'
                          ;
              type ncd:ifname {
                length 0..31 ;
              }
            }
            leaf poison-reverse {
              description 'Configure poison reverse.'
                          ;
              type enable-or-disable ;
              default enable ;
            }
            leaf split-horizen {
              description 'Configure split horizon.'
                          ;
              type enable-or-disable ;
              default enable ;
            }
            leaf if-mode {
              description 'The mode of the interface,include active(only send),'
                        + 'passive(only recieve) and normal.'
                          ;
              type enumeration {
                enum normal {
                  value 0 ;
                  description 'This RIP interface both send and receive.'
                              ;
                }
                enum active {
                  value 1 ;
                  description 'This RIP interface only send.'
                              ;
                }
                enum passive {
                  value 2 ;
                  description 'This RIP interface only receive.'
                              ;
                }
              }
              default normal ;
            }
            leaf only-send-subnet {
              description 'Only send subnet information about the interface.'
                          ;
              type enable-or-disable ;
              default disable ;
            }
            leaf originate-default-route {
              description 'Originate default route on specified interface.'
                          ;
              type enumeration {
                enum disable {
                  value 0 ;
                  description 'Can not originate default route on this interfac'
                            + 'e.'
                              ;
                }
                enum enable {
                  value 1 ;
                  description 'Originate default route and send it on this inte'
                            + 'rface.'
                              ;
                }
                enum only {
                  value 2 ;
                  description 'Originate default route and only send on at this'
                            + ' interface.'
                              ;
                }
              }
              default disable ;
            }
            leaf notify-default-route {
              description 'Enable an interface to notify default route.'
                          ;
              type enumeration {
                enum disable {
                  value 0 ;
                  description 'Can not notify default route on this interface.'
                              ;
                }
                enum enable {
                  value 1 ;
                  description 'Notify default route on this interface.'
                              ;
                }
                enum only {
                  value 2 ;
                  description 'Only notify default route on this interface.'
                              ;
                }
              }
              default disable ;
            }
            leaf originate-default-rpl {
              description 'Use route-policy when originate default route.'
                          ;
              type string {
                length 1..200 ;
              }
            }
            leaf neighbor-restrict {
              description 'Enable the interface restrict mode of communicate wi'
                        + 'th neighbors.'
                          ;
              type enable-or-disable ;
              default disable ;
            }
            leaf auth-mode {
              description 'Specify the authentication type.'
                          ;
              type enumeration {
                enum none {
                  value 1 ;
                  description 'No authentication.'
                              ;
                }
                enum text {
                  value 2 ;
                  description 'Simple text authentication.'
                              ;
                }
                enum md5 {
                  value 3 ;
                  description 'MD5 authentication.'
                              ;
                }
              }
              default none ;
            }
            leaf bfd {
              description 'Configure BFD for the interface.'
                          ;
              type enumeration {
                enum not-config {
                  value 0 ;
                  description 'BFD is not configured.'
                              ;
                }
                enum enable {
                  value 1 ;
                  description 'BFD is enabled.'
                              ;
                }
                enum disable {
                  value 2 ;
                  description 'BFD is disabled.'
                              ;
                }
              }
              default not-config ;
            }
            leaf keychain-name {
              description 'Specify a keychain of authentication.'
                          ;
              type string {
                length 1..31 ;
              }
            }
            leaf send-version {
              description 'Specify RIP version for transmission.'
                          ;
              type enumeration {
                enum v1 {
                  value 1 ;
                  description 'Use RIP version 1 to transmit advertisement.'
                              ;
                }
                enum v2 {
                  value 2 ;
                  description 'Use RIP version 2 to transmit advertisement.'
                              ;
                }
                enum not-config {
                  value 3 ;
                  description 'Send version is not configured on this interface.'
                              ;
                }
              }
              default not-config ;
            }
            leaf send-type {
              description 'Specify send type.'
                          ;
              type enumeration {
                enum not-config {
                  value 0 ;
                  description 'Transmission type is not configured.'
                              ;
                }
                enum broadcast {
                  value 1 ;
                  description 'Transmission type is broadcast.'
                              ;
                }
                enum multicast {
                  value 2 ;
                  description 'Transmission type is multicast.'
                              ;
                }
              }
              default not-config ;
            }
            choice auth-key {
              description 'Specify the authentication key.'
                          ;
              case auth-key-clear {
                description 'Set the authentication key in clear text.'
                            ;
                container auth-key-clear {
                  description 'Set the authentication key in clear text.'
                              ;
                  leaf key-clear {
                    description 'Set the authentication key in clear text.'
                                ;
                    type string {
                      length 1..16 ;
                    }
                  }
                }
              }
              case auth-key-encrypted {
                description 'Set the authentication key in encrypted text.'
                            ;
                container auth-key-encrypted {
                  description 'Set the authentication key in encrypted text.'
                              ;
                  leaf key-encrypted {
                    description 'Set the authentication key in encrypted text.'
                                ;
                    type string {
                      length 1..84 ;
                    }
                  }
                }
              }
            }
            container receive-version {
              description 'Specify RIP version for reception.'
                          ;
              presence receive-version ;
              leaf receive-v1 {
                description 'RIP version number.'
                            ;
                type rip-version-def ;
                mandatory true ;
              }
              leaf receive-v2 {
                description 'RIP version number.'
                            ;
                type enumeration {
                  enum none {
                    value 0 ;
                    description 'No config.'
                                ;
                  }
                  enum v1 {
                    value 1 ;
                    description 'RIP version 1.'
                                ;
                  }
                  enum v2 {
                    value 2 ;
                    description 'RIP version 2.'
                                ;
                  }
                }
                mandatory true ;
              }
            }
          }
        }
      }
      container vrf-instances {
        description 'The configuration of RIP VRF instances.'
                    ;
        list vrf-instance {
          description 'The configuration of RIP VRF instance.'
                      ;
          key vrf-name ;
          leaf vrf-name {
            description 'VPN Routing/Forwarding instance name (1-32 characters).'
                        ;
            type string {
              length 1..32 ;
            }
          }
          leaf version {
            description 'Set routing protocol version.'
                        ;
            type rip-version-def ;
            default v2 ;
          }
          leaf max-paths {
            description 'Maximum number of parallel routes RIP can support.'
                        ;
            type uint8 {
              range 1..32 ;
            }
            default 1 ;
          }
          leaf default-metric {
            description 'Default metric of redistributed routes.'
                        ;
            type uint8 {
              range 1..16 ;
            }
            default 1 ;
          }
          leaf distance {
            description 'Define an administrative distance.'
                        ;
            type uint8 {
              range 1..255 ;
            }
            default 120 ;
          }
          leaf auto-summary {
            description 'Enable automatic network number summarization.'
                        ;
            type enable-or-disable ;
            default enable ;
          }
          leaf validate-update-source {
            description 'Perform sanity checks against source address of routin'
                      + 'g updates.'
                        ;
            type enable-or-disable ;
            default enable ;
          }
          leaf bfd {
            description 'Configure all interfaces BFD capability.'
                        ;
            type enable-or-disable ;
            default disable ;
          }
          leaf fast-reroute {
            description 'Enable RIP fast-reroute function.'
                        ;
            type enable-or-disable ;
            default disable ;
          }
          leaf track-name {
            description 'Event tracking.'
                        ;
            type string {
              length 1..31 ;
            }
          }
        }
      }
      container vrf-networks {
        description 'The configuration of RIP networks in VRF instance.'
                    ;
        list vrf-network {
          description 'Enable routing on an IP network.'
                      ;
          key 'vrf-name network-addr network-wildmask'
              ;
          leaf vrf-name {
            description 'VPN Routing/Forwarding instance name (1-32 characters).'
                        ;
            type string {
              length 1..32 ;
            }
          }
          leaf network-addr {
            description 'Network address.'
                        ;
            type ncd:ipv4-address ;
          }
          leaf network-wildmask {
            description 'Network wild mask.'
                        ;
            type ncd:ipv4-imask ;
          }
        }
      }
      container vrf-neighbors {
        description 'The configuration of RIP neighbors in VRF instance.'
                    ;
        list vrf-neighbor {
          description 'Specify a neighbor router.'
                      ;
          key 'vrf-name neighbor-addr'
              ;
          leaf vrf-name {
            description 'VPN Routing/Forwarding instance name (1-32 characters).'
                        ;
            type string {
              length 1..32 ;
            }
          }
          leaf neighbor-addr {
