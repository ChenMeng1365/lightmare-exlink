module: huawei-port-group
  +--rw port-group
     +--rw groups
        +--rw group* [name]
           +--rw name       string
           +--rw members
              +--rw member* [if-name]
                 +--rw if-name    -> /ifm:ifm/interfaces/interface/name

  rpcs:
    +---x port-group-shutdown
       +---w input
          +---w name        -> /port-group/groups/group/name
          +---w shutdown?   boolean
