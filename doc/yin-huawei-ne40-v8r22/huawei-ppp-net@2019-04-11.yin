<?xml version="1.0" encoding="UTF-8"?>
<module name="huawei-ppp-net"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:ppp-net="urn:huawei:yang:huawei-ppp-net"
        xmlns:ifm="urn:huawei:yang:huawei-ifm"
        xmlns:devm="urn:huawei:yang:huawei-devm"
        xmlns:inet="urn:ietf:params:xml:ns:yang:ietf-inet-types"
        xmlns:ext="urn:huawei:yang:huawei-extension"
        xmlns:pub-type="urn:huawei:yang:huawei-pub-type"
        xmlns:yang="urn:ietf:params:xml:ns:yang:ietf-yang-types">
  <namespace uri="urn:huawei:yang:huawei-ppp-net"/>
  <prefix value="ppp-net"/>
  <import module="huawei-ifm">
    <prefix value="ifm"/>
  </import>
  <import module="huawei-devm">
    <prefix value="devm"/>
  </import>
  <import module="ietf-inet-types">
    <prefix value="inet"/>
  </import>
  <import module="huawei-extension">
    <prefix value="ext"/>
  </import>
  <import module="huawei-pub-type">
    <prefix value="pub-type"/>
  </import>
  <import module="ietf-yang-types">
    <prefix value="yang"/>
  </import>
  <organization>
    <text>Huawei Technologies Co., Ltd.</text>
  </organization>
  <contact>
    <text>Huawei Industrial Base
Bantian, Longgang
Shenzhen 518129
People's Republic of China
Website: http://www.huawei.com
Email: support@huawei.com</text>
  </contact>
  <description>
    <text>PPP net.</text>
  </description>
  <revision date="2019-04-11">
    <description>
      <text>Initial revision.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <ext:task-name value="ppp"/>
  <typedef name="ppp-damp-level-type">
    <type name="enumeration">
      <enum name="light">
        <value value="0"/>
        <description>
          <text>Light weight.</text>
        </description>
      </enum>
      <enum name="middle">
        <value value="1"/>
        <description>
          <text>Middle weight.</text>
        </description>
      </enum>
      <enum name="heavy">
        <value value="2"/>
        <description>
          <text>Heavy weight.</text>
        </description>
      </enum>
      <enum name="manual">
        <value value="3"/>
        <description>
          <text>Manually configure the parameters.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>PPP damp level type is light/middle/heavy/manual.</text>
    </description>
  </typedef>
  <typedef name="ppp-net-mru-negotiation-type">
    <type name="enumeration">
      <enum name="ipv4">
        <value value="0"/>
        <description>
          <text>IPv4 MRU negotiation on PPP MRU.</text>
        </description>
      </enum>
      <enum name="ipv6">
        <value value="1"/>
        <description>
          <text>IPv6 MRU negotiation on PPP MRU.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>PPP LCP MRU negotiation type.</text>
    </description>
  </typedef>
  <typedef name="ppp-enable">
    <type name="enumeration">
      <enum name="disable">
        <value value="0"/>
        <description>
          <text>Disabling.</text>
        </description>
      </enum>
      <enum name="enable">
        <value value="1"/>
        <description>
          <text>Enabling.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Description of enable type.</text>
    </description>
  </typedef>
  <typedef name="ppp-net-link-type">
    <type name="enumeration">
      <enum name="lcp">
        <value value="0"/>
        <description>
          <text>LCP.</text>
        </description>
      </enum>
      <enum name="ipcp">
        <value value="1"/>
        <description>
          <text>IPCP.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Link Type.</text>
    </description>
  </typedef>
  <typedef name="ppp-net-fsm-state">
    <type name="enumeration">
      <enum name="initial">
        <value value="0"/>
        <description>
          <text>Initial.</text>
        </description>
      </enum>
      <enum name="starting">
        <value value="1"/>
        <description>
          <text>Starting.</text>
        </description>
      </enum>
      <enum name="closed">
        <value value="2"/>
        <description>
          <text>Closed.</text>
        </description>
      </enum>
      <enum name="stopped">
        <value value="3"/>
        <description>
          <text>Stopped.</text>
        </description>
      </enum>
      <enum name="closing">
        <value value="4"/>
        <description>
          <text>Closing.</text>
        </description>
      </enum>
      <enum name="stopping">
        <value value="5"/>
        <description>
          <text>Stopping.</text>
        </description>
      </enum>
      <enum name="request-sent">
        <value value="6"/>
        <description>
          <text>Request sent.</text>
        </description>
      </enum>
      <enum name="ack-received">
        <value value="7"/>
        <description>
          <text>Ack-received.</text>
        </description>
      </enum>
      <enum name="ack-sent">
        <value value="8"/>
        <description>
          <text>Ack-sent.</text>
        </description>
      </enum>
      <enum name="opened">
        <value value="9"/>
        <description>
          <text>Opened.</text>
        </description>
      </enum>
      <enum name="none">
        <value value="10"/>
        <description>
          <text>None.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>PPP fsm state.</text>
    </description>
  </typedef>
  <typedef name="mp-fragment-size">
    <type name="enumeration">
      <enum name="fragment-128">
        <value value="0"/>
        <description>
          <text>Fragment size 128 bytes.</text>
        </description>
      </enum>
      <enum name="fragment-256">
        <value value="1"/>
        <description>
          <text>Fragment size 256 bytes.</text>
        </description>
      </enum>
      <enum name="fragment-512">
        <value value="2"/>
        <description>
          <text>Fragment size 512 bytes.</text>
        </description>
      </enum>
      <enum name="no-fragment">
        <value value="3"/>
        <description>
          <text>No fragment.</text>
        </description>
      </enum>
      <enum name="fragment-64">
        <value value="4"/>
        <description>
          <text>Fragment size 64 bytes.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>MP fragment size.</text>
    </description>
  </typedef>
  <typedef name="mp-member-physical-state">
    <type name="enumeration">
      <enum name="down">
        <value value="0"/>
        <description>
          <text>The interface is in the down state.</text>
        </description>
      </enum>
      <enum name="up">
        <value value="1"/>
        <description>
          <text>The interface is in the up state.</text>
        </description>
      </enum>
      <enum name="admin-shutdown">
        <value value="2"/>
        <description>
          <text>The interface is in the admin shutdown state.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>MP group member physical state.</text>
    </description>
  </typedef>
  <typedef name="mp-member-protocol-state">
    <type name="enumeration">
      <enum name="down">
        <value value="0"/>
        <description>
          <text>The interface is in the down state.</text>
        </description>
      </enum>
      <enum name="up">
        <value value="1"/>
        <description>
          <text>The interface is in the up state.</text>
        </description>
      </enum>
      <enum name="damping">
        <value value="2"/>
        <description>
          <text>The interface is in the damping state.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>MP group member protocol state.</text>
    </description>
  </typedef>
  <container name="ppp-net">
    <description>
      <text>PPP net.</text>
    </description>
    <container name="ppp-dampening">
      <presence value="PPP protocol status damping enable."/>
      <description>
        <text>Enable/disable type of PPP damp. If dampen-level is set to manual, the settings of the other parameters need to meet the following requirement: suppress-value/1000 &lt;= (reuse-value/1000) * 2^ (max-suppress-time/half-life-period).</text>
      </description>
      <ext:generated-by value="system">
        <ext:can-be-deleted/>
        <description>
          <text>The PPP dampening whose level is light is generated by system automatically.</text>
        </description>
      </ext:generated-by>
      <leaf name="dampen-level">
        <type name="ppp-damp-level-type"/>
        <default value="light"/>
        <description>
          <text>PPP protocol status damping level.</text>
        </description>
      </leaf>
      <leaf name="half-life-period">
        <when condition="../dampen-level='manual'"/>
        <type name="uint8">
          <range value="1..60"/>
        </type>
        <units name="s"/>
        <mandatory value="true"/>
        <description>
          <text>PPP protocol status damping half life time (seconds)</text>
        </description>
      </leaf>
      <leaf name="suppress-value">
        <when condition="../dampen-level='manual'"/>
        <type name="uint16">
          <range value="1..20000"/>
        </type>
        <must condition="../suppress-value &gt;= ../reuse-value"/>
        <mandatory value="true"/>
        <description>
          <text>1000 times of PPP protocol status damping suppress threshold.</text>
        </description>
      </leaf>
      <leaf name="reuse-value">
        <when condition="../dampen-level='manual'"/>
        <type name="uint16">
          <range value="1..20000"/>
        </type>
        <must condition="../suppress-value &gt;= ../reuse-value"/>
        <mandatory value="true"/>
        <description>
          <text>1000 times of PPP protocol status damping reuse threshold.</text>
        </description>
      </leaf>
      <leaf name="max-suppress-time">
        <when condition="../dampen-level='manual'"/>
        <type name="uint8">
          <range value="1..255"/>
        </type>
        <units name="s"/>
        <mandatory value="true"/>
        <description>
          <text>PPP protocol status damping max suppress time.</text>
        </description>
      </leaf>
    </container>
    <container name="load-balance-identify-pppoe">
      <description>
        <text>Configure load balance identify PPPoE.</text>
      </description>
      <leaf name="load-balance-enable">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable a device to identify the IP.</text>
        </description>
      </leaf>
    </container>
  </container>
  <augment target-node="/devm:devm/devm:lpu-boards/devm:lpu-board">
    <description>
      <text>Add PPP net configuration to devm model.</text>
    </description>
    <container name="ppp-net">
      <description>
        <text>Configure PPP net to devm.</text>
      </description>
      <container name="ppp-min-packet-length">
        <description>
          <text>Configure PPP minimum packet length.</text>
        </description>
        <leaf name="packet-length">
          <type name="uint32">
            <range value="8..64"/>
          </type>
          <description>
            <text>PPP minimum packet length.</text>
          </description>
        </leaf>
      </container>
      <container name="load-balance-identify-pppoe">
        <description>
          <text>Configure load balance identify PPPoE.</text>
        </description>
        <leaf name="load-balance-enable">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable a device to identify the IP.</text>
          </description>
        </leaf>
      </container>
    </container>
  </augment>
  <augment target-node="/devm:devm/devm:mpu-boards/devm:mpu-board">
    <description>
      <text>Add PPP net configuration to devm model.</text>
    </description>
    <container name="ppp-net">
      <description>
        <text>Configure PPP net to devm.</text>
      </description>
      <container name="load-balance-identify-pppoe">
        <description>
          <text>Configure load balance identify PPPoE.</text>
        </description>
        <leaf name="load-balance-enable">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable a device to identify the IP.</text>
          </description>
        </leaf>
      </container>
    </container>
  </augment>
  <augment target-node="/ifm:ifm/ifm:interfaces/ifm:interface">
    <description>
      <text>PPP configuration of interface.</text>
    </description>
    <container name="ppp-net">
      <description>
        <text>Configure PPP configuration of interface.</text>
      </description>
      <container name="ppp">
        <when condition="../../ifm:link-protocol='ppp'"/>
        <description>
          <text>Configure PPP of interface.</text>
        </description>
        <container name="ppp-base">
          <description>
            <text>Configure PPP base.</text>
          </description>
          <leaf name="negotiation-timeout">
            <when condition="../../../../ifm:type = 'Serial' or ../../../../ifm:type = 'Pos' or ../../../../ifm:type = 'Pos-Trunk' or ../../../../ifm:type = 'Trunk-Serial' or ../../../../ifm:type = 'Lmpif'"/>
            <type name="uint32">
              <range value="1..10"/>
            </type>
            <units name="s"/>
            <default value="3"/>
            <description>
              <text>Timeout duration of PPP negotiation.</text>
            </description>
          </leaf>
          <leaf name="dead-link-time">
            <when condition="../../../../ifm:type = 'Serial' or ../../../../ifm:type = 'Trunk-Serial'"/>
            <type name="uint32">
              <range value="5..30"/>
            </type>
            <units name="s"/>
            <default value="30"/>
            <description>
              <text>Specify the Link Dead timer value.</text>
            </description>
          </leaf>
          <leaf name="peer-ip-address">
            <when condition="../../../../ifm:type = 'Serial' or ../../../../ifm:type = 'Pos' or ../../../../ifm:type = 'Pos-Trunk' or ../../../../ifm:type = 'Trunk-Serial' or ../../../../ifm:type = 'Mp-group' or ../../../../ifm:type = 'Global-Mp-Group' or ../../../../ifm:type = 'Lmpif'"/>
            <type name="inet:ipv4-address-no-zone"/>
            <must condition="../peer-ip-address != '0.0.0.0'"/>
            <description>
              <text>IP address assigned for the peer.</text>
            </description>
          </leaf>
          <leaf name="mru-negotiation-type">
            <when condition="../../../../ifm:type = 'Serial' or ../../../../ifm:type = 'Pos' or ../../../../ifm:type = 'Lmpif'"/>
            <type name="ppp-net-mru-negotiation-type"/>
            <default value="ipv4"/>
            <description>
              <text>Specify MRU negotiation.</text>
            </description>
          </leaf>
          <leaf name="magic-number-check-flag">
            <when condition="../../../../ifm:type = 'Serial' or ../../../../ifm:type = 'Pos' or ../../../../ifm:type = 'Pos-Trunk' or ../../../../ifm:type = 'Trunk-Serial' or ../../../../ifm:type = 'Lmpif'"/>
            <type name="ppp-enable"/>
            <default value="disable"/>
            <description>
              <text>Enable/disable the function of magic numebr check.</text>
            </description>
          </leaf>
          <leaf name="osicp-negotiation-flag">
            <when condition="../../../../ifm:type = 'Serial' or ../../../../ifm:type = 'Pos' or ../../../../ifm:type = 'Pos-Trunk' or ../../../../ifm:type = 'Trunk-Serial' or ../../../../ifm:type = 'Mp-group' or ../../../../ifm:type = 'Global-Mp-Group'"/>
            <type name="ppp-enable"/>
            <default value="enable"/>
            <description>
              <text>Enable OSICP negotiate.</text>
            </description>
          </leaf>
          <leaf name="mplscp-negotiation-flag">
            <when condition="../../../../ifm:type = 'Serial' or ../../../../ifm:type = 'Pos' or ../../../../ifm:type = 'Pos-Trunk' or ../../../../ifm:type = 'Trunk-Serial' or ../../../../ifm:type = 'Mp-group' or ../../../../ifm:type = 'Global-Mp-Group'"/>
            <type name="ppp-enable"/>
            <default value="enable"/>
            <description>
              <text>Enable MPLSCP negotiate.</text>
            </description>
          </leaf>
          <leaf name="suppress-hostroute-flag">
            <when condition="../../../../ifm:type = 'Serial' or ../../../../ifm:type = 'Pos' or ../../../../ifm:type = 'Pos-Trunk' or ../../../../ifm:type = 'Trunk-Serial' or ../../../../ifm:type = 'Mp-group' or ../../../../ifm:type = 'Global-Mp-Group' or ../../../../ifm:type = 'Lmpif'"/>
            <type name="ppp-enable"/>
            <default value="disable"/>
            <description>
              <text>Enable/disable the function of suppressing the adding of peer host routes to the local direct route table.</text>
            </description>
          </leaf>
          <leaf name="admit-any-dns">
            <when condition="../../../../ifm:type = 'Serial' or ../../../../ifm:type = 'Pos' or ../../../../ifm:type = 'Mp-group' or ../../../../ifm:type = 'Global-Mp-Group' or ../../../../ifm:type = 'Lmpif'"/>
            <type name="ppp-enable"/>
            <default value="disable"/>
            <description>
              <text>Enable/disable receiving any DNS address sent by the peer.</text>
            </description>
          </leaf>
          <leaf name="primary-dns-address">
            <when condition="../../../../ifm:type = 'Serial' or ../../../../ifm:type = 'Pos' or ../../../../ifm:type = 'Mp-group' or ../../../../ifm:type = 'Global-Mp-Group' or ../../../../ifm:type = 'Lmpif'"/>
            <type name="inet:ipv4-address-no-zone"/>
            <must condition="../primary-dns-address != '0.0.0.0'"/>
            <description>
              <text>IP address of the master DNS server.</text>
            </description>
          </leaf>
          <leaf name="second-dns-address">
            <when condition="../../../../ifm:type = 'Serial' or ../../../../ifm:type = 'Pos' or ../../../../ifm:type = 'Mp-group' or ../../../../ifm:type = 'Global-Mp-Group' or ../../../../ifm:type = 'Lmpif'"/>
            <type name="inet:ipv4-address-no-zone"/>
            <must condition="../primary-dns-address and ../second-dns-address != '0.0.0.0'"/>
            <description>
              <text>IP address of the slave DNS server.</text>
            </description>
          </leaf>
          <leaf name="pap-authen-flag">
            <when condition="../../../../ifm:type = 'Serial' or ../../../../ifm:type = 'Pos' or ../../../../ifm:type = 'Pos-Trunk' or ../../../../ifm:type = 'Trunk-Serial' or ../../../../ifm:type = 'Lmpif'"/>
            <type name="ppp-enable"/>
            <default value="disable"/>
            <description>
              <text>Enable/disable the PAP authentication mode.</text>
            </description>
          </leaf>
          <leaf name="chap-authen-flag">
            <when condition="../../../../ifm:type = 'Serial' or ../../../../ifm:type = 'Pos' or ../../../../ifm:type = 'Pos-Trunk' or ../../../../ifm:type = 'Trunk-Serial' or ../../../../ifm:type = 'Lmpif'"/>
            <type name="ppp-enable"/>
            <default value="disable"/>
            <description>
              <text>Enable/disable the CHAP authentication mode.</text>
            </description>
          </leaf>
          <leaf name="pap-user-name">
            <when condition="../../../../ifm:type = 'Serial' or ../../../../ifm:type = 'Pos' or ../../../../ifm:type = 'Pos-Trunk' or ../../../../ifm:type = 'Trunk-Serial' or ../../../../ifm:type = 'Lmpif'"/>
            <type name="string">
              <length value="1..253"/>
            </type>
            <must condition="../pap-user-name and ../pap-password"/>
            <description>
              <text>PAP authentication on user names.</text>
            </description>
          </leaf>
          <leaf name="pap-password">
            <when condition="../../../../ifm:type = 'Serial' or ../../../../ifm:type = 'Pos' or ../../../../ifm:type = 'Pos-Trunk' or ../../../../ifm:type = 'Trunk-Serial' or ../../../../ifm:type = 'Lmpif'"/>
            <type name="pub-type:password-extend">
              <length value="1..268"/>
            </type>
            <must condition="../pap-user-name and ../pap-password"/>
            <description>
              <text>The password is a string ranging from 1 to 128 characters for a simple password and 1 to 268 characters for a ciphertext password.</text>
            </description>
          </leaf>
          <leaf name="chap-user-name">
            <when condition="../../../../ifm:type = 'Serial' or ../../../../ifm:type = 'Pos' or ../../../../ifm:type = 'Pos-Trunk' or ../../../../ifm:type = 'Trunk-Serial' or ../../../../ifm:type = 'Lmpif'"/>
            <type name="string">
              <length value="1..253"/>
            </type>
            <description>
              <text>CHAP authentication on user names.</text>
            </description>
          </leaf>
          <leaf name="chap-password">
            <when condition="../../../../ifm:type = 'Serial' or ../../../../ifm:type = 'Pos' or ../../../../ifm:type = 'Pos-Trunk' or ../../../../ifm:type = 'Trunk-Serial' or ../../../../ifm:type = 'Lmpif'"/>
            <type name="pub-type:password-extend">
              <length value="1..268"/>
            </type>
            <description>
              <text>The password is a string ranging from 1 to 128 characters for a simple password and 1 to 268 characters for a ciphertext password.</text>
            </description>
          </leaf>
        </container>
        <container name="interface-link">
          <description>
            <text>Configure PPP polling interval.</text>
          </description>
          <leaf name="time-interval">
            <when condition="../../../../ifm:type != 'Pos-Trunk' and ../../../../ifm:type != 'Mp-group' and ../../../../ifm:type != 'Global-Mp-Group'"/>
            <type name="uint32">
              <range value="0..32767"/>
            </type>
            <units name="s"/>
            <default value="10"/>
            <description>
              <text>Polling interval.</text>
            </description>
          </leaf>
        </container>
        <container name="ppp-link-info">
          <config value="false"/>
          <description>
            <text>Statistics of PPP link.</text>
          </description>
          <leaf name="lcp-state">
            <type name="ppp-net-fsm-state"/>
            <description>
              <text>LCP state.</text>
            </description>
          </leaf>
          <leaf name="ipcp-state">
            <type name="ppp-net-fsm-state"/>
            <description>
              <text>IPCP state.</text>
            </description>
          </leaf>
          <leaf name="local-mru">
            <type name="uint32"/>
            <description>
              <text>Negotiated MRU value of the local device.</text>
            </description>
          </leaf>
          <leaf name="peer-mru">
            <type name="uint32"/>
            <description>
              <text>Negotiated MRU value of the peer device.</text>
            </description>
          </leaf>
          <leaf name="magic-number">
            <type name="uint32"/>
            <description>
              <text>Negotiated magic number.</text>
            </description>
          </leaf>
          <leaf name="negotiation-timeout">
            <type name="uint32"/>
            <units name="s"/>
            <description>
              <text>Timeout period of PPP negotiation.</text>
            </description>
          </leaf>
          <leaf name="peer-ip-address">
            <type name="inet:ipv4-address-no-zone"/>
            <description>
              <text>Negotiated 32-bit-mask IP address of the peer device.</text>
            </description>
          </leaf>
          <leaf name="mplscp-state">
            <type name="ppp-net-fsm-state"/>
            <description>
              <text>MPLSCP state.</text>
            </description>
          </leaf>
          <leaf name="osicp-state">
            <type name="ppp-net-fsm-state"/>
            <description>
              <text>OSICP state.</text>
            </description>
          </leaf>
          <leaf name="ip6cp-state">
            <type name="ppp-net-fsm-state"/>
            <description>
              <text>IP6CP state.</text>
            </description>
          </leaf>
          <leaf name="reuse-time">
            <type name="uint16"/>
            <units name="s"/>
            <description>
              <text>Dampening reuse time.</text>
            </description>
          </leaf>
          <container name="down-events">
            <description>
              <text>List of down events.</text>
            </description>
            <list name="down-event">
              <key value="link-type"/>
              <description>
                <text>Statistics of down events.</text>
              </description>
              <leaf name="link-type">
                <type name="ppp-net-link-type"/>
                <description>
                  <text>Link type.</text>
                </description>
              </leaf>
              <leaf name="down-time1">
                <type name="yang:date-and-time"/>
                <description>
                  <text>The 1st latest down time.</text>
                </description>
              </leaf>
              <leaf name="down-reason1">
                <type name="string">
                  <length value="1..100"/>
                </type>
                <description>
