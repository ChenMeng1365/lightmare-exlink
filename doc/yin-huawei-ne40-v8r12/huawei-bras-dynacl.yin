<?xml version="1.0" encoding="UTF-8"?>
<module name="huawei-bras-dynacl"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:bras-dynacl="urn:huawei:yang:huawei-bras-dynacl"
        xmlns:ext="urn:huawei:yang:huawei-extension">
  <namespace uri="urn:huawei:yang:huawei-bras-dynacl"/>
  <prefix value="bras-dynacl"/>
  <import module="huawei-extension">
    <prefix value="ext"/>
  </import>
  <organization>
    <text>Huawei Technologies Co., Ltd.</text>
  </organization>
  <contact>
    <text>Huawei Industrial Base
Bantian, Longgang
Shenzhen 518129
People's Republic of China
Website: http://www.huawei.com
Email: support@huawei.com</text>
  </contact>
  <description>
    <text>Bras dynacl.</text>
  </description>
  <revision date="2020-03-02">
    <description>
      <text>Modify description.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <revision date="2019-04-10">
    <description>
      <text>Init revision.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <ext:task-name value="aaa-access"/>
  <container name="bras-dynacl">
    <description>
      <text>Bras dynacl.</text>
    </description>
    <container name="remote-download-acl">
      <description>
        <text>Configure the RADIUS server to create dynamic ACLs.</text>
      </description>
      <leaf name="enable">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable the RADIUS server to create dynamic ACLs.</text>
        </description>
      </leaf>
    </container>
    <container name="remote-download-user-group">
      <description>
        <text>Configure the RADIUS server to create dynamic user groups.</text>
      </description>
      <leaf name="enable">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable the RADIUS server to create dynamic user groups.</text>
        </description>
      </leaf>
    </container>
    <container name="remote-user-group-check-interval">
      <description>
        <text>Configure an interval at which a device checks whether dynamic user groups created by the RADIUS server are used by online users or dynamic ACLs.</text>
      </description>
      <leaf name="interval-value">
        <type name="uint32">
          <range value="1..65535"/>
        </type>
        <units name="min"/>
        <default value="10"/>
        <description>
          <text>Specifies an interval at which the device checks whether a dynamic user group is in use.</text>
        </description>
      </leaf>
    </container>
    <container name="remote-download-acl-alarm-threshold">
      <description>
        <text>Configure the alarm threshold for the usage of classifier-behavior pairs dynamically delivered by the RADIUS server.</text>
      </description>
      <leaf name="threshold-value">
        <type name="uint32">
          <range value="1..100"/>
        </type>
        <units name="%"/>
        <default value="80"/>
        <description>
          <text>Specifies an alarm threshold.</text>
        </description>
      </leaf>
    </container>
    <container name="support-all-operate">
      <description>
        <text>Configure a BRAS to support all operation types of a dynamic ACL delivered using a RADIUS authentication response packet when a user goes online.</text>
      </description>
      <leaf name="enable">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable a BRAS to support all operation types of a dynamic ACL delivered using a RADIUS authentication response packet when a user goes online.</text>
        </description>
      </leaf>
    </container>
  </container>
</module>
