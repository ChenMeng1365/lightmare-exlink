  <feature name="relay-user_connect_statistic">
    <description>
      <text>DHCPv6 relay user connect statistics on the board.</text>
    </description>
  </feature>
  <container name="configuration">
    <description>
      <text>Configuration.</text>
    </description>
    <container name="dhcpv6">
      <description>
        <text>DHCPv6 config.</text>
      </description>
      <container name="global">
        <description>
          <text>DHCPv6 global common config.</text>
        </description>
        <leaf name="enable-state">
          <description>
            <text>Enable DHCPv6 process.</text>
          </description>
          <type name="enumeration">
            <enum name="disable">
              <value value="0"/>
              <description>
                <text>DHCPv6 disable.</text>
              </description>
            </enum>
            <enum name="enable">
              <value value="1"/>
              <description>
                <text>DHCPv6 enable.</text>
              </description>
            </enum>
          </type>
          <default value="disable"/>
        </leaf>
        <container name="client">
          <description>
            <text>DHCPv6 client global config.</text>
          </description>
          <leaf name="enable-state">
            <description>
              <text>Enable DHCPv6 client process.</text>
            </description>
            <type name="enumeration">
              <enum name="enable">
                <value value="0"/>
                <description>
                  <text>DHCPv6 client enable.</text>
                </description>
              </enum>
              <enum name="disable">
                <value value="1"/>
                <description>
                  <text>DHCPv6 client disable.</text>
                </description>
              </enum>
            </type>
            <default value="enable"/>
          </leaf>
        </container>
        <leaf name="ramble">
          <description>
            <text>Enable DHCPv6 user ramble.</text>
          </description>
          <type name="enumeration">
            <enum name="ramble-disable">
              <value value="0"/>
              <description>
                <text>Turn off DHCPv6 ramble.</text>
              </description>
            </enum>
            <enum name="ramble-enable">
              <value value="1"/>
              <description>
                <text>Turn on DHCPv6 ramble.</text>
              </description>
            </enum>
          </type>
          <default value="ramble-enable"/>
          <ncd:revise-date date="2019-05-31"/>
        </leaf>
        <leaf name="online-user-solicit-reonline">
          <description>
            <text>Configure the suppression time of DHCPv6 online user re-online receciving solicit packet.

It's toplimit depends on ability of the device.</text>
          </description>
          <type name="uint32">
            <range value="0..3600">
              <description>
                <text>The range of the value is 0 to 3600.</text>
              </description>
            </range>
          </type>
          <ncd:revise-date date="2019-12-23"/>
        </leaf>
        <leaf name="upgrade-lease-time">
          <description>
            <text>Upgrade lease time.</text>
          </description>
          <type name="uint32">
            <range value="60..4294967295">
              <description>
                <text>Upgrade lease time(second).</text>
              </description>
            </range>
          </type>
          <ncd:revise-date date="2020-04-09"/>
        </leaf>
        <leaf name="check-invalid-option">
          <description>
            <text>Configure check or not check invalid options in client packets.</text>
          </description>
          <type name="enumeration">
            <enum name="off">
              <value value="0"/>
              <description>
                <text>Not check invalid options.</text>
              </description>
            </enum>
            <enum name="on">
              <value value="1"/>
              <description>
                <text>Check invalid options.</text>
              </description>
            </enum>
          </type>
          <default value="off"/>
          <ncd:revise-date date="2020-05-26"/>
        </leaf>
        <container name="suppress-packet">
          <description>
            <text>DHCPV6 suppress packet</text>
          </description>
          <presence value="suppress-packet"/>
          <leaf name="packet-type">
            <description>
              <text>DHCPV6 suppress packet type.</text>
            </description>
            <type name="enumeration">
              <enum name="request-packet">
                <value value="1"/>
                <description>
                  <text>All request packet.</text>
                </description>
              </enum>
            </type>
            <mandatory value="true"/>
          </leaf>
          <leaf name="packet-count">
            <description>
              <text>The received packet count before packet suppression.</text>
            </description>
            <type name="uint32">
              <range value="1..10000">
                <description>
                  <text>Packet count.[1,10000].</text>
                </description>
              </range>
            </type>
            <mandatory value="true"/>
          </leaf>
          <leaf name="check-period">
            <description>
              <text>Check period.</text>
            </description>
            <type name="uint32">
              <range value="1..3600">
                <description>
                  <text>Check period.[1,3600].</text>
                </description>
              </range>
            </type>
            <mandatory value="true"/>
          </leaf>
          <leaf name="suppress-period">
            <description>
              <text>Suppress period.</text>
            </description>
            <type name="uint32">
              <range value="1..3600">
                <description>
                  <text>Suppress period.[1,3600].</text>
                </description>
              </range>
            </type>
            <mandatory value="true"/>
          </leaf>
          <ncd:revise-date date="2020-08-11"/>
        </container>
        <container name="authen-templates">
          <description>
            <text>DHCPv6 authentication template configuration.</text>
          </description>
          <list name="authen-template">
            <description>
              <text>DHCPv6 authentication template configuration.</text>
            </description>
            <key value="template-name"/>
            <leaf name="template-name">
              <description>
                <text>DHCPv6 authentication template name.</text>
              </description>
              <type name="string">
                <length value="1..64">
                  <description>
                    <text>DHCPv6 authentication template name.</text>
                  </description>
                </length>
              </type>
            </leaf>
            <leaf name="radius-group-name">
              <description>
                <text>DHCPv6 authentication radius group name.</text>
              </description>
              <type name="string">
                <length value="1..31">
                  <description>
                    <text>Authentication radius group name.</text>
                  </description>
                </length>
              </type>
            </leaf>
            <container name="radius-attributes">
              <description>
                <text>DHCPv6 authentication radius attributes configuration.</text>
              </description>
              <list name="radius-attribute">
                <description>
                  <text>DHCPv6 authentication radius attributes configuration.</text>
                </description>
                <key value="attribute-type"/>
                <leaf name="attribute-type">
                  <description>
                    <text>DHCPv6 authentication radius attributes type.</text>
                  </description>
                  <type name="enumeration">
                    <enum name="user-name">
                      <value value="1"/>
                      <description>
                        <text>Radius attribute is user-name.</text>
                      </description>
                    </enum>
                    <enum name="user-password">
                      <value value="2"/>
                      <description>
                        <text>Radius attribute is user-password.</text>
                      </description>
                    </enum>
                    <enum name="nas-port-id">
                      <value value="3"/>
                      <description>
                        <text>Radius attribute is nas-port-id.</text>
                      </description>
                    </enum>
                  </type>
                </leaf>
                <leaf name="attribute-source">
                  <description>
                    <text>DHCPv6 authentication radius attributes source.</text>
                  </description>
                  <type name="enumeration">
                    <enum name="mac">
                      <value value="1"/>
                      <description>
                        <text>Radius source is from MAC.</text>
                      </description>
                    </enum>
                    <enum name="china-tel-option18">
                      <value value="2"/>
                      <description>
                        <text>Radius source is from China-tel-option18.</text>
                      </description>
                    </enum>
                  </type>
                  <mandatory value="true"/>
                </leaf>
              </list>
            </container>
          </list>
          <ncd:revise-date date="2020-11-20"/>
        </container>
        <container name="option-templates">
          <description>
            <text>DHCPv6 option template configuration.</text>
          </description>
          <list name="option-template">
            <description>
              <text>DHCPv6 option template configuration.</text>
            </description>
            <key value="template-name"/>
            <leaf name="template-name">
              <description>
                <text>DHCPv6 option template name.</text>
              </description>
              <type name="string">
                <length value="1..64">
                  <description>
                    <text>DHCPv6 option template name.</text>
                  </description>
                </length>
              </type>
            </leaf>
            <container name="options">
              <description>
                <text>Configure option.</text>
              </description>
              <list name="option">
                <description>
                  <text>Configure option.</text>
                </description>
                <key value="option-id"/>
                <leaf name="option-id">
                  <description>
                    <text>Configure option ID.</text>
                  </description>
                  <type name="uint16">
                    <range value="1..65535">
                      <description>
                        <text>The option code.</text>
                      </description>
                    </range>
                  </type>
                </leaf>
                <choice name="type">
                  <description>
                    <text>DHCPv6 option type of option template.</text>
                  </description>
                  <mandatory value="true"/>
                  <case name="ascii">
                    <description>
                      <text>DHCPv6 option type is ASCII string.</text>
                    </description>
                    <leaf name="ascii-string">
                      <description>
                        <text>ASCII string.</text>
                      </description>
                      <type name="string">
                        <length value="1..64">
                          <description>
                            <text>The length of ASCII string.</text>
                          </description>
                        </length>
                      </type>
                      <mandatory value="true"/>
                    </leaf>
                  </case>
                  <case name="hex">
                    <description>
                      <text>DHCPv6 option type is HEX string.</text>
                    </description>
                    <leaf name="hex-string">
                      <description>
                        <text>HEX string.</text>
                      </description>
                      <type name="string">
                        <length value="1..128">
                          <description>
                            <text>The length of HEX string.</text>
                          </description>
                        </length>
                      </type>
                      <mandatory value="true"/>
                    </leaf>
                  </case>
                  <case name="ip">
                    <description>
                      <text>DHCPv6 option type is IP.</text>
                    </description>
