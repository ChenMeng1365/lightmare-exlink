<?xml version="1.0" encoding="UTF-8"?>
<module name="zxr10-telnet"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:telnet="http://www.zte.com.cn/zxr10/netconf/schema/rosng/TELNET"
        xmlns:ncd="http://www.zte.com.cn/zxr10/netconf/schema/rosng/netconf-defs">
  <namespace uri="http://www.zte.com.cn/zxr10/netconf/schema/rosng/TELNET"/>
  <prefix value="telnet"/>
  <yang-version value="1"/>
  <import module="zxr10-netconf-defs">
    <prefix value="ncd"/>
  </import>
  <organization>
    <text>ZTE Corporation</text>
  </organization>
  <contact>
    <text>ZTE Corporation

   Nanjing Institute of ZTE Corporation

   No.68 Zijinghua Rd. Yuhuatai District, Nanjing, China

   Tel: +86-25-52870000</text>
  </contact>
  <description>
    <text>The netconf module to describe the TELNET.</text>
  </description>
  <revision date="2015-04-14">
    <description>
      <text>Modify the type of history-size, modify Max-elements of users and terminal,modify range of sty-no and vty-no.</text>
    </description>
  </revision>
  <revision date="2015-03-13">
    <description>
      <text>Delete the leaf node of max-link, add a leaf node of max-connection.</text>
    </description>
  </revision>
  <revision date="2014-05-06">
    <description>
      <text>Modify the default value of dscp.</text>
    </description>
  </revision>
  <revision date="2014-02-24">
    <description>
      <text>Modify length of line and username of users.</text>
    </description>
  </revision>
  <revision date="2013-12-12">
    <description>
      <text>Modify version of import defs file; Modify the type of line,type,login-time,from string to string-with-blank</text>
    </description>
  </revision>
  <revision date="2013-10-10">
    <description>
      <text>Initial version.</text>
    </description>
  </revision>
  <container name="configuration">
    <description>
      <text>The configuration model of TELNET.</text>
    </description>
    <container name="telnet">
      <description>
        <text>Configure the TELNET parameters.</text>
      </description>
      <container name="telnet-server">
        <description>
          <text>Configure the TELNET server parameters.</text>
        </description>
        <choice name="status">
          <description>
            <text>To configure the TELNET server enable or disable.</text>
          </description>
          <default value="enable"/>
          <case name="disable">
            <description>
              <text>To configure the TELNET server disable.</text>
            </description>
            <leaf name="disable">
              <description>
                <text>Disable the TELNET server.</text>
              </description>
              <type name="empty"/>
            </leaf>
          </case>
          <case name="enable">
            <description>
              <text>To configure the TELNET server enable.</text>
            </description>
            <leaf name="enable">
              <description>
                <text>Enable TELNET server.</text>
              </description>
              <type name="empty"/>
            </leaf>
            <leaf name="listen-port">
              <description>
                <text>The listenning port of the TELNET server. It can be configured as 23 or any integer between 49152 and 65535. It cannot be changed while TELNET server has been enabled.</text>
              </description>
              <type name="uint16">
                <range value="23 | 49152..65535"/>
              </type>
              <default value="23"/>
            </leaf>
          </case>
        </choice>
        <leaf name="dscp">
          <description>
            <text>The DSCP value is used in packets of the TELNET server. It can be configured any integer between 0 and 63.</text>
          </description>
          <type name="uint8">
            <range value="0..63"/>
          </type>
        </leaf>
        <leaf name="idle-timeout">
          <description>
            <text>Telnet maximum idle time in minutes(0-1000).Distinguish project,support performance parameter.</text>
          </description>
          <type name="uint16">
            <range value="0..1000"/>
          </type>
          <units name="minute"/>
          <default value="120"/>
        </leaf>
        <leaf name="absolute-timeout">
          <description>
            <text>Telnet maximum online time in minutes(0-10000).Distinguish project,support performance parameter.</text>
          </description>
          <type name="uint16">
            <range value="0..10000"/>
          </type>
          <units name="minute"/>
          <default value="1440"/>
        </leaf>
        <leaf name="max-link">
          <status value="obsolete"/>
          <description>
            <text>Maximum number of telnet links.</text>
          </description>
          <type name="uint8">
            <range value="1..15"/>
          </type>
          <default value="15"/>
        </leaf>
        <leaf name="ipv4-acl">
          <description>
            <text>The IPv4 ACL name which is binded to the TELNET server.</text>
          </description>
          <type name="string">
            <length value="1..31"/>
          </type>
        </leaf>
        <leaf name="ipv6-acl">
          <description>
            <text>The IPv6 ACL name which is binded to the TELNET server.</text>
          </description>
          <type name="string">
            <length value="1..31"/>
          </type>
        </leaf>
        <leaf name="max-connection">
          <description>
            <text>Maximum number of telnet connections.</text>
          </description>
          <type name="uint32">
            <range value="1..32">
              <description>
                <text>Maximum value support performance parameter.</text>
              </description>
            </range>
          </type>
          <default value="15"/>
        </leaf>
      </container>
      <container name="console">
        <description>
          <text>Configure the console parameters.</text>
        </description>
        <leaf name="idle-timeout">
          <description>
            <text>Console maximum idle time in minutes(0-1000).

Distinguish project,support performance parameter.</text>
          </description>
          <type name="uint16">
            <range value="0..1000"/>
          </type>
          <units name="minute"/>
          <default value="30"/>
        </leaf>
        <leaf name="absolute-timeout">
          <description>
            <text>Console maximum absolute time in minutes(0-10000).

Distinguish project,support performance parameter.</text>
          </description>
          <type name="uint16">
            <range value="0..10000"/>
          </type>
          <units name="minute"/>
          <default value="1440"/>
        </leaf>
      </container>
    </container>
  </container>
  <container name="state">
    <config value="false"/>
    <description>
      <text>State model of TELNET.</text>
    </description>
    <container name="telnet">
      <description>
        <text>State of terminals and login users.</text>
      </description>
      <list name="users">
        <description>
          <text>Information of login users.</text>
        </description>
        <max-elements value="33"/>
        <key value="sty-no"/>
        <leaf name="sty-no">
          <description>
            <text>Sty number.</text>
          </description>
          <type name="uint8">
            <range value="0..32"/>
          </type>
        </leaf>
        <leaf name="line">
          <description>
            <text>Sty virtual information.</text>
          </description>
          <type name="ncd:string-with-blank">
            <length value="0..15"/>
          </type>
        </leaf>
        <leaf name="username">
          <description>
            <text>User name.</text>
          </description>
          <type name="string">
            <length value="0..65"/>
          </type>
        </leaf>
        <leaf name="remote-addr">
          <description>
            <text>Address of remote server.</text>
          </description>
          <type name="string">
            <length value="0..45"/>
          </type>
        </leaf>
        <leaf name="local-addr">
          <description>
            <text>Client address.</text>
          </description>
          <type name="string">
            <length value="0..45"/>
          </type>
        </leaf>
        <leaf name="idle-time">
          <description>
            <text>Idle time in minutes.</text>
          </description>
          <type name="string">
            <length value="0..32"/>
          </type>
        </leaf>
      </list>
      <list name="terminal">
        <description>
          <text>Information of terminal.</text>
        </description>
        <max-elements value="33"/>
        <key value="sty-no"/>
        <leaf name="sty-no">
          <description>
            <text>Sty number.</text>
          </description>
          <type name="uint8">
            <range value="0..32"/>
          </type>
        </leaf>
        <leaf name="location">
          <description>
            <text>Client address.</text>
          </description>
          <type name="string">
            <length value="0..45"/>
          </type>
        </leaf>
        <leaf name="type">
          <description>
            <text>Type of terminal.</text>
          </description>
          <type name="ncd:string-with-blank">
            <length value="0..32"/>
          </type>
        </leaf>
        <leaf name="baudrate">
          <description>
            <text>Baudrate of terminal.</text>
          </description>
          <type name="uint32"/>
        </leaf>
        <leaf name="capability">
          <description>
            <text>Capability of teminal.</text>
          </description>
          <type name="string">
            <length value="0..32"/>
          </type>
        </leaf>
        <leaf name="online-time">
          <description>
            <text>Online time of terminal.</text>
          </description>
          <type name="string">
            <length value="0..32"/>
          </type>
        </leaf>
        <leaf name="editing-state">
          <description>
            <text>Editing state of terminal.</text>
          </description>
          <type name="string">
            <length value="0..32"/>
          </type>
        </leaf>
        <leaf name="history-state">
          <description>
            <text>History state of terminal.</text>
          </description>
          <type name="string">
            <length value="0..32"/>
          </type>
        </leaf>
        <leaf name="history-size">
          <description>
            <text>History size of terminal.</text>
          </description>
          <type name="uint16">
            <range value="5..256"/>
          </type>
        </leaf>
        <leaf name="length">
          <description>
            <text>Screen height of teminal.</text>
          </description>
          <type name="uint16">
            <range value="0..512"/>
          </type>
          <default value="24"/>
        </leaf>
        <leaf name="width">
          <description>
            <text>Screen width of terminal.</text>
          </description>
          <type name="uint16">
            <range value="80..512"/>
          </type>
          <default value="80"/>
        </leaf>
        <leaf name="login-time">
          <description>
            <text>Login time of terminal.</text>
          </description>
          <type name="ncd:string-with-blank">
            <length value="0..32"/>
          </type>
        </leaf>
      </list>
    </container>
  </container>
  <container name="operation">
    <config value="false"/>
    <description>
      <text>Operation model of TELNET.</text>
    </description>
    <container name="telnet">
      <description>
        <text>Operation of TELNET.</text>
      </description>
      <container name="clear-vty">
        <description>
          <text>Reset a terminal line.</text>
        </description>
        <leaf name="vty-no">
          <description>
            <text>Vty number.</text>
          </description>
          <type name="uint8">
            <range value="0..31"/>
          </type>
        </leaf>
      </container>
    </container>
  </container>
</module>
