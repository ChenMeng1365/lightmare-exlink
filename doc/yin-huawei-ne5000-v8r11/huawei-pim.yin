              </description>
            </leaf>
            <leaf name="is-silent">
              <type name="boolean"/>
              <default value="false"/>
              <description>
                <text>Enable/disable the PIM silent function on an interface. By default, PIM silent is disabled on an interface.</text>
              </description>
            </leaf>
            <leaf name="dr-swt-delay-enable">
              <type name="boolean"/>
              <default value="false"/>
              <description>
                <text>Enable/disable DR switchover delay on an interface. By default, this function is disabled on an interface.</text>
              </description>
            </leaf>
            <leaf name="dr-swt-delay-interval">
              <when condition="../dr-swt-delay-enable='true'"/>
              <type name="uint32">
                <range value="10..3600"/>
              </type>
              <units name="s"/>
              <default value="10"/>
