<?xml version="1.0" encoding="UTF-8"?>
<module name="huawei-system-resources-usage"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:system-resources-usage="urn:huawei:yang:huawei-system-resources-usage"
        xmlns:ext="urn:huawei:yang:huawei-extension">
  <namespace uri="urn:huawei:yang:huawei-system-resources-usage"/>
  <prefix value="system-resources-usage"/>
  <import module="huawei-extension">
    <prefix value="ext"/>
  </import>
  <organization>
    <text>Huawei Technologies Co., Ltd.</text>
  </organization>
  <contact>
    <text>Huawei Industrial Base
Bantian, Longgang
Shenzhen 518129
People's Republic of China
Website: http://www.huawei.com
Email: support@huawei.com</text>
  </contact>
  <description>
    <text>Display system resources usage.</text>
  </description>
  <revision date="2020-02-28">
    <description>
      <text>Init revision.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <ext:task-name value="forward-system"/>
  <container name="system-resources-usage">
    <config value="false"/>
    <description>
      <text>System resources usage.</text>
    </description>
    <container name="resources">
      <description>
        <text>List of resource informations.</text>
      </description>
      <list name="resource">
        <key value="slot resource-name"/>
        <description>
          <text>Statistics of resource information.</text>
        </description>
        <leaf name="slot">
          <type name="string">
            <length value="1..16"/>
          </type>
          <description>
            <text>Slot ID.</text>
          </description>
        </leaf>
        <leaf name="resource-name">
          <type name="string">
            <length value="1..64"/>
          </type>
          <description>
            <text>Resource name.</text>
          </description>
        </leaf>
        <leaf name="total">
          <type name="uint32"/>
          <description>
            <text>Total resource count.</text>
          </description>
        </leaf>
        <leaf name="allocated">
          <type name="uint32"/>
          <description>
            <text>Allocated resource count.</text>
          </description>
        </leaf>
        <leaf name="free">
          <type name="uint32"/>
          <description>
            <text>Free resource count.</text>
          </description>
        </leaf>
      </list>
    </container>
  </container>
</module>
