                  <value value="0"/>
                  <description>
                    <text>Route is valid.</text>
                  </description>
                </enum>
                <enum name="INVALID">
                  <value value="1"/>
                  <description>
                    <text>Route is invalid.</text>
                  </description>
                </enum>
                <enum name="NOTEXIST">
                  <value value="2"/>
                  <description>
                    <text>Route is not exist.</text>
                  </description>
                </enum>
              </type>
              <default value="VALID"/>
            </leaf>
            <leaf name="route-tag">
              <description>
                <text>The route tag, and the default value is 0. The range is 0-4294967295</text>
              </description>
              <type name="uint32">
                <range value="0..4294967295"/>
              </type>
              <default value="0"/>
            </leaf>
            <leaf name="is-used-backup">
              <description>
                <text>The route is backup route or not. The "YES" means yes, and the "NO" means no.</text>
              </description>
              <type name="enumeration">
                <enum name="NO">
                  <value value="0"/>
                  <description>
                    <text>The route is not backup route.</text>
                  </description>
                </enum>
                <enum name="YES">
                  <value value="1"/>
                  <description>
                    <text>The route is backup route.</text>
                  </description>
                </enum>
              </type>
              <default value="NO"/>
            </leaf>
            <leaf name="ipv6-nexthop">
              <description>
                <text>The route ipv6 nexthop.</text>
              </description>
              <type name="ncd:ipv6-address"/>
              <ncd:revise-date date="2019-04-01"/>
            </leaf>
          </list>
        </container>
      </container>
    </container>
  </container>
</module>
