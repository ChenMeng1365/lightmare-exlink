          <description>
            <text>Configure a delegation IPv6 address pool.</text>
          </description>
          <container name="bind-prefix">
            <description>
              <text>Configure binding an IPv6 prefix pool to an IPv6 address pool.</text>
            </description>
            <leaf name="prefix-name">
              <type name="leafref">
                <path value="/bras-ipv6-address-manage:bras-ipv6-address-manage/bras-ipv6-address-manage:ipv6-prefixs/bras-ipv6-address-manage:ipv6-prefix/bras-ipv6-address-manage:name"/>
              </type>
              <must condition="/bras-ipv6-address-manage:bras-ipv6-address-manage/bras-ipv6-address-manage:ipv6-prefixs/bras-ipv6-address-manage:ipv6-prefix[name=current()]/bras-ipv6-address-manage:type ='delegation'"/>
              <description>
                <text>Bind an IPv6 prefix pool to an IPv6 address pool.</text>
              </description>
            </leaf>
          </container>
          <uses name="remote-not-support"/>
          <uses name="rapid-commit-config"/>
          <container name="weight">
            <when condition="../../rui-slave = 'true'"/>
            <description>
              <text>Configure a weight for an IPv6 address pool.</text>
            </description>
            <uses name="weight"/>
          </container>
          <uses name="ipv6-pool-comm"/>
          <uses name="aftr-config"/>
          <uses name="export-host-route"/>
          <uses name="ipv6-constant-index"/>
        </container>
        <container name="remote">
          <when condition="../type = 'remote'"/>
          <description>
            <text>Configure a remote IPv6 address pool.</text>
          </description>
          <container name="bind-prefix">
            <description>
              <text>Configure binding an IPv6 prefix pool to an IPv6 address pool.</text>
