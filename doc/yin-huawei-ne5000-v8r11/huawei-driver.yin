            <type name="string">
              <length value="1..32"/>
            </type>
            <description>
              <text>Slot.</text>
            </description>
          </leaf>
          <leaf name="board-type">
            <type name="string">
              <length value="1..32"/>
            </type>
            <description>
              <text>Board type.</text>
            </description>
          </leaf>
