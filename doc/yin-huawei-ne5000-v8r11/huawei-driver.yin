  <rpc name="check-system-software">
    <description>
      <text>Check system software.</text>
    </description>
    <input>
      <choice name="startup-file">
        <mandatory value="true"/>
        <description>
          <text>Choose startup or file.</text>
        </description>
        <case name="startup">
          <description>
            <text>Startup file.</text>
          </description>
          <leaf name="startup">
            <type name="empty"/>
            <description>
              <text>The startup file.</text>
            </description>
          </leaf>
        </case>
        <case name="file">
          <description>
            <text>Packet file.</text>
          </description>
          <leaf name="file-name">
            <type name="string">
              <length value="4..127"/>
            </type>
            <description>
              <text>Packet file name in master MPU cfcard, for example:cfcard:/B02D.cc.</text>
            </description>
          </leaf>
        </case>
      </choice>
    </input>
    <output>
      <leaf name="sha256-hash-value">
        <type name="string">
          <length value="1..255"/>
        </type>
        <description>
          <text>SHA256 hash value of the system software.</text>
        </description>
      </leaf>
      <leaf name="signature-result">
        <type name="driver-check-result-type"/>
        <description>
          <text>Result of system software signature check.</text>
        </description>
      </leaf>
      <leaf name="crc-result">
        <type name="driver-check-result-type"/>
        <description>
          <text>Result of system software CRC check.</text>
        </description>
      </leaf>
      <leaf name="match-result">
        <type name="driver-check-result-type"/>
        <description>
          <text>Result of system software match check.</text>
        </description>
      </leaf>
    </output>
  </rpc>
  <rpc name="check-hardware-version">
    <description>
      <text>Check whether the system software package version matches the software versions, such as the EPLD and CANBUS version, running on the hardware.</text>
    </description>
    <input>
      <leaf name="chassis-id">
        <type name="string">
          <length value="1..32"/>
        </type>
        <description>
          <text>Chassis ID of device.</text>
        </description>
      </leaf>
      <choice name="check-type">
        <mandatory value="true"/>
        <description>
          <text>Choose startup or file.</text>
        </description>
        <case name="startup-package">
          <description>
            <text>Startup.</text>
          </description>
          <leaf name="startup">
            <type name="empty"/>
            <description>
              <text>The startup file.</text>
            </description>
          </leaf>
        </case>
        <case name="package-file">
          <description>
            <text>Packet file name.</text>
          </description>
          <leaf name="file-name">
            <type name="string">
              <length value="4..127"/>
            </type>
            <description>
              <text>Packet file name in master MPU.</text>
            </description>
          </leaf>
        </case>
      </choice>
    </input>
    <output>
      <leaf name="signature-result">
        <type name="driver-check-result-type"/>
        <description>
          <text>Result of system software signature check.</text>
        </description>
      </leaf>
      <leaf name="match-result">
        <type name="driver-check-result-type"/>
        <description>
          <text>Result of system software version check.</text>
        </description>
      </leaf>
      <container name="version-infos">
        <description>
          <text>List of version information.</text>
        </description>
        <list name="version-info">
          <key value="slot"/>
          <description>
            <text>Information of version.</text>
          </description>
          <leaf name="slot">
            <type name="string">
              <length value="1..32"/>
            </type>
            <description>
              <text>Slot ID of device.</text>
            </description>
          </leaf>
          <leaf name="hardware-type">
            <type name="driver-slot-infos-type"/>
            <description>
              <text>Hardware type.</text>
            </description>
          </leaf>
          <leaf name="content">
            <type name="driver-hardware-version-type"/>
            <description>
              <text>Content of system software.</text>
            </description>
          </leaf>
          <leaf name="current-version">
            <type name="uint32"/>
            <description>
              <text>Current version.</text>
            </description>
          </leaf>
          <leaf name="package-version">
            <type name="uint32"/>
            <description>
              <text>Package version.</text>
            </description>
          </leaf>
          <leaf name="compatibility-state">
            <type name="driver-compatibility-state-type"/>
            <description>
              <text>Compatibility state of system software.</text>
            </description>
          </leaf>
          <leaf name="upgrade-suggestion">
            <type name="driver-upgrade-suggestion-type"/>
            <description>
              <text>Upgrade suggestion.</text>
            </description>
          </leaf>
        </list>
      </container>
    </output>
  </rpc>
