  <rpc name="reset-ptp-pkt">
    <description>
      <text>Clear statistics, including packet and timestamp statistics.</text>
    </description>
    <ext:node-ref value="/clocksync-1588v2:clocksync-1588v2/clocksync-1588v2:ptp-port-stats/clocksync-1588v2:ptp-port-stat"/>
    <input>
      <leaf name="port-name">
        <type name="pub-type:if-name"/>
        <description>
          <text>If interface-name, Clear specific interface statistics, if all, clears all interfaces statistics, including packet and timestamp statistics.</text>
        </description>
      </leaf>
    </input>
  </rpc>
  <rpc name="recover-ptsf-switch">
    <description>
      <text>Ptsf switching of the time source to be recovered.</text>
    </description>
    <ext:node-ref value="/clocksync-1588v2:clocksync-1588v2/clocksync-1588v2:ptp-port-stats/clocksync-1588v2:ptp-port-stat"/>
    <input>
      <leaf name="recover-ptsf-src-name">
        <type name="pub-type:if-name"/>
        <description>
          <text>Interface name of the time source to be recovered.</text>
        </description>
      </leaf>
    </input>
  </rpc>
  <augment target-node="/ifm:ifm/ifm:interfaces/ifm:interface">
    <description>
      <text>Interface.</text>
    </description>
    <container name="ptp-port-srcs">
      <when condition="(/clocksync-1588v2:clocksync-1588v2/clocksync-1588v2:ptp-common/clocksync-1588v2:device-type and /clocksync-1588v2:clocksync-1588v2/clocksync-1588v2:ptp-common/clocksync-1588v2:device-type!='none')"/>
      <description>
        <text>Configure PTP interface clock sources.</text>
      </description>
      <leaf name="port-enable">
        <type name="boolean"/>
        <must condition="((../port-enable='true') and not((/clocksync-1588v2:clocksync-1588v2/clocksync-1588v2:ptp-common/clocksync-1588v2:device-type='tcandbc') and (../ptp-port-advance/port-type-set='none'))) or (../port-enable='false')"/>
        <default value="false"/>
        <description>
          <text>Enable/disable PTP for PTP interface clock sources.</text>
        </description>
      </leaf>
      <leaf name="port-num">
        <type name="int32">
          <range value="0..65535"/>
        </type>
        <config value="false"/>
        <description>
          <text>Port number of PTP interface clock sources.</text>
        </description>
      </leaf>
      <leaf name="port-state">
        <type name="ptp-port-state"/>
        <config value="false"/>
        <description>
          <text>Interface status of a PTP clock source.</text>
        </description>
      </leaf>
      <leaf name="permit-dmac">
        <type name="pub-type:mac-address"/>
        <description>
          <text>Configure the destination MAC address of the packets that can be received and processed by 1588v2 interfaces.</text>
        </description>
      </leaf>
      <container name="ptp-port-advance">
        <description>
          <text>Configure advanced attributes of a PTP interface clock source.</text>
        </description>
        <leaf name="port-domain">
          <when condition="(../port-type-set='tc')"/>
          <type name="uint32">
            <range value="0..255"/>
          </type>
          <must condition="((/clocksync-1588v2:clocksync-1588v2/clocksync-1588v2:ptp-common/clocksync-1588v2:profile='IEEE1588V2') and (../port-domain&gt;=0 and ../port-domain&lt;=255)) or ((/clocksync-1588v2:clocksync-1588v2/clocksync-1588v2:ptp-common/clocksync-1588v2:profile = 'SMPTE2059-2') and (../port-domain&gt;=0 and ../port-domain&lt;=127)) or ((/clocksync-1588v2:clocksync-1588v2/clocksync-1588v2:ptp-common/clocksync-1588v2:profile='G8275.1' or /clocksync-1588v2:clocksync-1588v2/clocksync-1588v2:ptp-common/clocksync-1588v2:profile='CU106') and ../port-domain=24)"/>
          <ext:dynamic-default>
            <ext:default-value value="0">
              <when condition="/clocksync-1588v2:clocksync-1588v2/clocksync-1588v2:ptp-common/clocksync-1588v2:profile='IEEE1588V2'"/>
              <description>
                <text>The default value is 0 when profile is IEEE1588V2.</text>
              </description>
            </ext:default-value>
            <ext:default-value value="127">
              <when condition="/clocksync-1588v2:clocksync-1588v2/clocksync-1588v2:ptp-common/clocksync-1588v2:profile='SMPTE2059-2'"/>
              <description>
                <text>The default value is 127 when profile is SMPTE2059-2.</text>
              </description>
            </ext:default-value>
            <ext:default-value value="24">
              <when condition="/clocksync-1588v2:clocksync-1588v2/clocksync-1588v2:ptp-common/clocksync-1588v2:profile='G8275.1' or /clocksync-1588v2:clocksync-1588v2/clocksync-1588v2:ptp-common/clocksync-1588v2:profile='CU106'"/>
              <description>
                <text>The default value is 24 when profile is G8275.1 or CU106.</text>
              </description>
            </ext:default-value>
          </ext:dynamic-default>
          <description>
            <text>Clock domain of PTP interface clock sources. The clock domain can be configured only if the PTP device type is TCandBC and the PTP interface type is TC.</text>
          </description>
        </leaf>
        <leaf name="port-type-set">
          <when condition="(/clocksync-1588v2:clocksync-1588v2/clocksync-1588v2:ptp-common/clocksync-1588v2:device-type='tcandbc')"/>
          <type name="ptp-port-type"/>
          <must condition="(../port-type-set='bc' or ../port-type-set='tc') or (../port-type-set='none')"/>
          <default value="none"/>
          <description>
            <text>Type of the PTP interface clock source.</text>
          </description>
        </leaf>
        <leaf name="port-type">
          <type name="ptp-port-type"/>
          <default value="none"/>
          <config value="false"/>
          <description>
            <text>Type of the PTP interface clock source.</text>
          </description>
        </leaf>
        <leaf name="delay-mechanism">
          <type name="ptp-delay-mechanism"/>
          <must condition="((((/clocksync-1588v2:clocksync-1588v2/clocksync-1588v2:ptp-common/clocksync-1588v2:device-type='bc') or (/clocksync-1588v2:clocksync-1588v2/clocksync-1588v2:ptp-common/clocksync-1588v2:device-type='oc') or (/clocksync-1588v2:clocksync-1588v2/clocksync-1588v2:ptp-common/clocksync-1588v2:device-type='tcandbc')) and (../../port-enable='false')) or (../delay-mechanism='delay'))"/>
          <default value="delay"/>
          <description>
            <text>Delay measurement mechanism for PTP packets.</text>
          </description>
        </leaf>
        <leaf name="port-cfg-state">
          <type name="ptp-port-state"/>
          <default value="initializing"/>
          <description>
            <text>In static BMC selection, the status of an interface clock source can be manually configured, such as master or slave.</text>
          </description>
        </leaf>
        <leaf name="clk-step">
          <type name="ptp-port-step-type"/>
          <default value="onestep"/>
          <description>
            <text>Number of devices that an Announce packet of an interface clock source passes through before the packet reaches a local device.</text>
          </description>
        </leaf>
        <leaf name="not-slave">
          <type name="boolean"/>
          <must condition="((/clocksync-1588v2:clocksync-1588v2/clocksync-1588v2:ptp-common/clocksync-1588v2:profile='G8275.1') or (/clocksync-1588v2:clocksync-1588v2/clocksync-1588v2:ptp-common/clocksync-1588v2:profile='CU106')) or (../not-slave='true')"/>
          <default value="true"/>
          <description>
            <text>Enable/disable notslave attribute.</text>
          </description>
        </leaf>
        <leaf name="port-local-priority">
          <type name="int32">
            <range value="1..255"/>
          </type>
          <must condition="((/clocksync-1588v2:clocksync-1588v2/clocksync-1588v2:ptp-common/clocksync-1588v2:profile='G8275.1') or (/clocksync-1588v2:clocksync-1588v2/clocksync-1588v2:ptp-common/clocksync-1588v2:profile='CU106')) or (../port-local-priority='128')"/>
          <default value="128"/>
          <description>
            <text>Configure local-priority attribute.</text>
          </description>
        </leaf>
      </container>
      <container name="ptp-pkt-pack">
        <description>
          <text>Configure PTP packet encapsulation mode. A PTP packet can be encapsulated in a Layer 2 frame or in a Layer 3 UDP packet.</text>
        </description>
        <leaf name="pkt-pack-type">
          <type name="ptp-pack-type"/>
          <must condition="((/clocksync-1588v2:clocksync-1588v2/clocksync-1588v2:ptp-common/clocksync-1588v2:profile='IEEE1588V2' or /clocksync-1588v2:clocksync-1588v2/clocksync-1588v2:ptp-common/clocksync-1588v2:profile='G8275.1' or /clocksync-1588v2:clocksync-1588v2/clocksync-1588v2:ptp-common/clocksync-1588v2:profile='CU106') and (../pkt-pack-type='mac')) or ((/clocksync-1588v2:clocksync-1588v2/clocksync-1588v2:ptp-common/clocksync-1588v2:profile='IEEE1588V2' or /clocksync-1588v2:clocksync-1588v2/clocksync-1588v2:ptp-common/clocksync-1588v2:profile='SMPTE2059-2') and (../pkt-pack-type='udp' or ../pkt-pack-type='none'))"/>
          <ext:dynamic-default>
            <ext:default-value value="mac">
              <when condition="(/clocksync-1588v2:clocksync-1588v2/clocksync-1588v2:ptp-common/clocksync-1588v2:profile='IEEE1588V2' or /clocksync-1588v2:clocksync-1588v2/clocksync-1588v2:ptp-common/clocksync-1588v2:profile='G8275.1' or /clocksync-1588v2:clocksync-1588v2/clocksync-1588v2:ptp-common/clocksync-1588v2:profile='CU106')"/>
              <description>
                <text>The default value is MAC when profile is IEEE1588V2 or G8275.1 or CU106.</text>
              </description>
            </ext:default-value>
            <ext:default-value value="none">
              <when condition="/clocksync-1588v2:clocksync-1588v2/clocksync-1588v2:ptp-common/clocksync-1588v2:profile='SMPTE2059-2'"/>
              <description>
                <text>The default value is none when profile is SMPTE2059-2.</text>
              </description>
            </ext:default-value>
          </ext:dynamic-default>
          <description>
            <text>PTP packet encapsulation mode. A PTP packet can be encapsulated in a Layer 2 frame or in a Layer 3 UDP packet.</text>
          </description>
        </leaf>
        <container name="ptp-pkt-mac-pack">
          <when condition="../pkt-pack-type='mac'"/>
          <description>
            <text>Configure layer 2 MAC encapsulation for PTP packets.</text>
          </description>
          <leaf name="mac-egress-dest-mac">
            <type name="pub-type:mac-address">
              <pattern value="([0-9a-h][0|2|4|6|8|a|c|e][0-9a-h]{2}-[0-9a-h]{4}-[0-9a-h]{4})|(011b-1900-0000)|(0180-c200-000e)"/>
            </type>
            <must condition="((/clocksync-1588v2:clocksync-1588v2/clocksync-1588v2:ptp-common/clocksync-1588v2:profile='G8275.1' or /clocksync-1588v2:clocksync-1588v2/clocksync-1588v2:ptp-common/clocksync-1588v2:profile='CU106') and (../mac-egress-dest-mac='011b-1900-0000' or ../mac-egress-dest-mac='0180-c200-000e')) or ((/clocksync-1588v2:clocksync-1588v2/clocksync-1588v2:ptp-common/clocksync-1588v2:profile='IEEE1588V2') and ((not(../mac-egress-dest-mac='0180-c200-000e') and ../../../ptp-port-advance/delay-mechanism='delay') or (not(../mac-egress-dest-mac='011b-1900-0000') and ../../../ptp-port-advance/delay-mechanism='pdelay')))"/>
            <ext:dynamic-default>
              <ext:default-value value="011b-1900-0000">
                <when condition="(../../../ptp-port-advance/delay-mechanism='delay')"/>
                <description>
                  <text>The default value is 011b-1900-0000 when delay-mechanism is delay.</text>
                </description>
              </ext:default-value>
              <ext:default-value value="0180-c200-000e">
                <when condition="(../../../ptp-port-advance/delay-mechanism='pdelay')"/>
                <description>
                  <text>The default value is 0180-c200-000e when delay-mechanism is pdelay.</text>
                </description>
              </ext:default-value>
            </ext:dynamic-default>
            <description>
              <text>Destination MAC address for a PTP packet encapsulated in a Layer 2 frame.</text>
            </description>
          </leaf>
          <leaf name="mac-egress-vlan-id">
            <type name="int32">
              <range value="0..4094"/>
            </type>
            <must condition="(/clocksync-1588v2:clocksync-1588v2/clocksync-1588v2:ptp-common/clocksync-1588v2:profile='IEEE1588V2') or (../mac-egress-vlan-id=0)"/>
            <default value="0"/>
            <description>
              <text>VLAN ID for a PTP packet encapsulated in a Layer 2 frame. 0 means packet without VLAN-ID.</text>
            </description>
          </leaf>
          <leaf name="mac-egress-pkt-priority">
            <when condition="(../mac-egress-vlan-id and ../mac-egress-vlan-id!=0)"/>
            <type name="int32">
              <range value="0..7"/>
            </type>
            <must condition="(/clocksync-1588v2:clocksync-1588v2/clocksync-1588v2:ptp-common/clocksync-1588v2:profile='IEEE1588V2')"/>
            <default value="7"/>
            <description>
              <text>Packet priority of a PTP packet encapsulated in a Layer 2 frame.</text>
            </description>
          </leaf>
        </container>
        <container name="ptp-pkt-udp-pack">
          <when condition="../pkt-pack-type='udp'"/>
          <description>
            <text>Configure layer 3 UDP encapsulation for PTP packets.</text>
          </description>
          <leaf name="udp-egress-src-ip">
            <type name="inet:ipv4-address-no-zone"/>
            <must condition="(../udp-egress-src-ip!='0.0.0.0')"/>
            <description>
              <text>Source IP address for a PTP packet encapsulated in a Layer 3 UDP packet.</text>
            </description>
          </leaf>
          <leaf name="udp-egress-dest-ip">
            <when condition="(../udp-egress-src-ip)"/>
            <type name="inet:ipv4-address-no-zone"/>
            <must condition="(../udp-egress-dest-ip!='0.0.0.0')"/>
            <description>
              <text>Destination IP address for a PTP packet encapsulated in a Layer 3 UDP packet.</text>
            </description>
          </leaf>
          <leaf name="udp-egress-dscp">
            <when condition="(../udp-egress-src-ip)"/>
            <type name="int32">
              <range value="0..63"/>
            </type>
            <default value="56"/>
            <description>
              <text>DSCP value for a PTP packet encapsulated in a Layer 3 UDP packet.</text>
            </description>
          </leaf>
          <leaf name="udp-egress-vlan-id">
            <when condition="(../udp-egress-src-ip)"/>
            <type name="int32">
              <range value="0..4094"/>
            </type>
            <default value="0"/>
            <description>
              <text>VLAN ID for a PTP packet encapsulated in a Layer 3 UDP packet. 0 means packet without VLAN-ID.</text>
            </description>
          </leaf>
          <leaf name="udp-egress-pkt-priority">
            <when condition="(../udp-egress-vlan-id and ../udp-egress-vlan-id!=0)"/>
            <type name="int32">
              <range value="0..7"/>
            </type>
            <default value="7"/>
            <description>
              <text>Priority of a PTP packet encapsulated in a Layer 3 UDP packet.</text>
            </description>
          </leaf>
          <leaf name="udp-egress-dest-mac">
            <when condition="(../udp-egress-src-ip)"/>
            <type name="pub-type:mac-address">
              <pattern value="([0-9a-h][0|2|4|6|8|a|c|e][0-9a-h]{2}-[0-9a-h]{4}-[0-9a-h]{4})|(0100-5e00-0181)|(0100-5e00-006b)"/>
            </type>
            <must condition="((not(../udp-egress-dest-mac='0100-5e00-006b') and ../../../ptp-port-advance/delay-mechanism='delay') or (not(../udp-egress-dest-mac='0100-5e00-0181') and ../../../ptp-port-advance/delay-mechanism='pdelay'))"/>
            <ext:dynamic-default>
              <ext:default-value value="0100-5e00-0181">
                <when condition="(../../../ptp-port-advance/delay-mechanism='delay')"/>
                <description>
                  <text>The default value is 0100-5e00-0181 when delay-mechanism is delay.</text>
                </description>
              </ext:default-value>
              <ext:default-value value="0100-5e00-006b">
                <when condition="(../../../ptp-port-advance/delay-mechanism='pdelay')"/>
                <description>
                  <text>The default value is 0100-5e00-006b when delay-mechanism is pdelay.</text>
                </description>
              </ext:default-value>
            </ext:dynamic-default>
            <description>
              <text>Destination MAC address for a PTP packet encapsulated in a Layer 3 UDP packet.</text>
            </description>
          </leaf>
        </container>
      </container>
      <container name="ptp-peer-clock">
        <config value="false"/>
        <description>
          <text>Statistics of information about the upstream PTP device.</text>
        </description>
        <leaf name="peer-port-clk-id">
          <type name="string">
            <length value="16"/>
          </type>
          <default value="0000000000000000"/>
          <description>
            <text>Clock number of an upstream PTP device.</text>
          </description>
        </leaf>
        <leaf name="peer-port-num">
          <type name="int32">
            <range value="0..65535"/>
          </type>
          <description>
            <text>Number of the interface on the upstream device that is connected to a local device.</text>
          </description>
        </leaf>
        <leaf name="peer-announce-send-interval">
          <type name="int32">
            <range value="0..20"/>
          </type>
          <description>
            <text>Intervals at which Announce packets are sent on the interface of the upstream PTP device that is connected to a local device. the calculation formula for the interval in seconds is (2 to the power of interval/1024)s.</text>
          </description>
        </leaf>
      </container>
      <container name="ptp-pkt-control">
        <description>
          <text>Configure PTP packet control. Including discarding the received Announce packets and setting packet transmission intervals.</text>
        </description>
        <leaf name="announce-drop">
          <type name="boolean"/>
          <must condition="(not(/clocksync-1588v2:clocksync-1588v2/clocksync-1588v2:ptp-common/clocksync-1588v2:device-type='t-bc') and not(/clocksync-1588v2:clocksync-1588v2/clocksync-1588v2:ptp-common/clocksync-1588v2:device-type='t-tc')) or (../announce-drop='false')"/>
          <default value="false"/>
          <description>
            <text>Enable/disable an interface to discard the received Announce packets.</text>
          </description>
        </leaf>
        <leaf name="delay-req-send-disable">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable an interface to send Delay_Req message.</text>
          </description>
        </leaf>
        <leaf name="announce-interval">
          <type name="int32">
            <range value="0..20"/>
          </type>
          <must condition="((/clocksync-1588v2:clocksync-1588v2/clocksync-1588v2:ptp-common/clocksync-1588v2:profile='IEEE1588V2' or /clocksync-1588v2:clocksync-1588v2/clocksync-1588v2:ptp-common/clocksync-1588v2:profile='CU106' or /clocksync-1588v2:clocksync-1588v2/clocksync-1588v2:ptp-common/clocksync-1588v2:profile='G8275.1') and (../announce-interval&gt;=0 and ../announce-interval&lt;=20)) or ((/clocksync-1588v2:clocksync-1588v2/clocksync-1588v2:ptp-common/clocksync-1588v2:profile='SMPTE2059-2') and (../announce-interval&gt;=7 and ../announce-interval&lt;=11))"/>
          <ext:dynamic-default>
            <ext:default-value value="8">
              <when condition="/clocksync-1588v2:clocksync-1588v2/clocksync-1588v2:ptp-common/clocksync-1588v2:profile='SMPTE2059-2'"/>
              <description>
                <text>The default value is 8 when profile is SMPTE2059-2.</text>
              </description>
            </ext:default-value>
            <ext:default-value value="7">
              <when condition="/clocksync-1588v2:clocksync-1588v2/clocksync-1588v2:ptp-common/clocksync-1588v2:profile='G8275.1' or /clocksync-1588v2:clocksync-1588v2/clocksync-1588v2:ptp-common/clocksync-1588v2:profile='CU106' or /clocksync-1588v2:clocksync-1588v2/clocksync-1588v2:ptp-common/clocksync-1588v2:profile='IEEE1588V2'"/>
              <description>
                <text>The default value is 7 when profile is G8275.1 or CU106 or IEEE1588V2.</text>
              </description>
            </ext:default-value>
          </ext:dynamic-default>
          <description>
            <text>Set an interval at which Announce packets are sent on a PTP interface. the calculation formula for the interval in seconds is (2 to the power of interval/1024)s.</text>
          </description>
        </leaf>
        <leaf name="announce-receipt-timeout">
          <type name="int32">
            <range value="2..255"/>
          </type>
          <must condition="((/clocksync-1588v2:clocksync-1588v2/clocksync-1588v2:ptp-common/clocksync-1588v2:profile='CU106' or /clocksync-1588v2:clocksync-1588v2/clocksync-1588v2:ptp-common/clocksync-1588v2:profile='G8275.1') and (../announce-receipt-timeout&gt;=3 and ../announce-receipt-timeout&lt;=255)) or ((/clocksync-1588v2:clocksync-1588v2/clocksync-1588v2:ptp-common/clocksync-1588v2:profile='IEEE1588V2') and (../announce-receipt-timeout&gt;=2 and ../announce-receipt-timeout&lt;=255)) or ((/clocksync-1588v2:clocksync-1588v2/clocksync-1588v2:ptp-common/clocksync-1588v2:profile='SMPTE2059-2') and (../announce-receipt-timeout&gt;=2 and ../announce-receipt-timeout&lt;=10))"/>
          <default value="3"/>
          <description>
            <text>Timeout occurrences of announce packet receiving on the PTP interface. If the number of timeouts exceeds the set value, the newly received Announce packets will enter the timeout processing procedure.</text>
          </description>
        </leaf>
        <leaf name="sync-interval">
          <type name="int32">
            <range value="0..20"/>
          </type>
          <must condition="((/clocksync-1588v2:clocksync-1588v2/clocksync-1588v2:ptp-common/clocksync-1588v2:profile='IEEE1588V2' or /clocksync-1588v2:clocksync-1588v2/clocksync-1588v2:ptp-common/clocksync-1588v2:profile='G8275.1' or /clocksync-1588v2:clocksync-1588v2/clocksync-1588v2:ptp-common/clocksync-1588v2:profile='CU106') and (../sync-interval&gt;=0 and ../sync-interval&lt;=20)) or ((/clocksync-1588v2:clocksync-1588v2/clocksync-1588v2:ptp-common/clocksync-1588v2:profile='SMPTE2059-2') and (../sync-interval&gt;=3 and ../sync-interval&lt;=9))"/>
          <ext:dynamic-default>
            <ext:default-value value="3">
              <when condition="/clocksync-1588v2:clocksync-1588v2/clocksync-1588v2:ptp-common/clocksync-1588v2:profile='IEEE1588V2'"/>
              <description>
                <text>The default value is 3 when profile is IEEE1588V2.</text>
              </description>
            </ext:default-value>
            <ext:default-value value="7">
              <when condition="/clocksync-1588v2:clocksync-1588v2/clocksync-1588v2:ptp-common/clocksync-1588v2:profile='SMPTE2059-2'"/>
              <description>
                <text>The default value is 7 when profile is SMPTE2059-2.</text>
              </description>
            </ext:default-value>
            <ext:default-value value="6">
              <when condition="/clocksync-1588v2:clocksync-1588v2/clocksync-1588v2:ptp-common/clocksync-1588v2:profile='G8275.1' or /clocksync-1588v2:clocksync-1588v2/clocksync-1588v2:ptp-common/clocksync-1588v2:profile='CU106'"/>
              <description>
                <text>The default value is 6 when profile is G8275.1 or CU106.</text>
              </description>
            </ext:default-value>
          </ext:dynamic-default>
          <description>
            <text>Interval at which Sync packets are sent on a PTP interface.the calculation formula for the interval in seconds is (2 to the power of interval/1024)s.</text>
          </description>
        </leaf>
        <leaf name="min-delay-req-interval">
          <type name="int32">
            <range value="0..20"/>
          </type>
          <must condition="((/clocksync-1588v2:clocksync-1588v2/clocksync-1588v2:ptp-common/clocksync-1588v2:profile='IEEE1588V2' or /clocksync-1588v2:clocksync-1588v2/clocksync-1588v2:ptp-common/clocksync-1588v2:profile='CU106' or /clocksync-1588v2:clocksync-1588v2/clocksync-1588v2:ptp-common/clocksync-1588v2:profile='G8275.1') and (../min-delay-req-interval&gt;=0 and ../min-delay-req-interval&lt;=20)) or ((/clocksync-1588v2:clocksync-1588v2/clocksync-1588v2:ptp-common/clocksync-1588v2:profile='SMPTE2059-2') and (../min-delay-req-interval&gt;=3 and ../min-delay-req-interval&lt;=14))"/>
          <ext:dynamic-default>
            <ext:default-value value="7">
              <when condition="/clocksync-1588v2:clocksync-1588v2/clocksync-1588v2:ptp-common/clocksync-1588v2:profile='SMPTE2059-2' or /clocksync-1588v2:clocksync-1588v2/clocksync-1588v2:ptp-common/clocksync-1588v2:profile='IEEE1588V2'"/>
              <description>
                <text>The default value is 7 when profile is SMPTE2059-2 or IEEE1588V2.</text>
              </description>
            </ext:default-value>
            <ext:default-value value="6">
              <when condition="/clocksync-1588v2:clocksync-1588v2/clocksync-1588v2:ptp-common/clocksync-1588v2:profile='G8275.1' or /clocksync-1588v2:clocksync-1588v2/clocksync-1588v2:ptp-common/clocksync-1588v2:profile='CU106'"/>
              <description>
                <text>The default value is 6 when profile is G8275.1 or CU106.</text>
              </description>
            </ext:default-value>
          </ext:dynamic-default>
          <description>
            <text>Set the minimum interval at which Delay_Req packets are sent on a PTP interface.the calculation formula for the interval in seconds is (2 to the power of interval/1024)s.</text>
          </description>
        </leaf>
        <leaf name="min-pdelay-req-interval">
          <type name="int32">
            <range value="0..20"/>
          </type>
          <must condition="((not(/clocksync-1588v2:clocksync-1588v2/clocksync-1588v2:ptp-common/clocksync-1588v2:device-type='t-bc') and not(/clocksync-1588v2:clocksync-1588v2/clocksync-1588v2:ptp-common/clocksync-1588v2:device-type='t-tc')) or (../min-pdelay-req-interval=7))"/>
          <default value="7"/>
          <description>
            <text>Set the minimum interval at which Pdelay_Req packets are sent on a PTP interface.the calculation formula for the interval in seconds is (2 to the power of interval/1024)s.</text>
          </description>
        </leaf>
      </container>
      <container name="ptp-asymmetry-correction">
        <description>
          <text>Configure asymmetric delay correction for port fibers. Calculate the correction value after auto-measurement of ring-network asymmetry delay is enabled and asymmetry delay of optical fibers is tested, including positive and negative flags as well as the value to be corrected, and set the value on the interface.</text>
        </description>
        <leaf name="correction-flag">
          <when condition="not(../correction-value='0')"/>
          <type name="ptp-sign-flag"/>
          <default value="positive"/>
          <description>
            <text>Signal flag of correction value.</text>
          </description>
        </leaf>
        <leaf name="correction-value">
          <type name="int32">
            <range value="0..2000000"/>
          </type>
          <default value="0"/>
          <description>
            <text>Correction value.</text>
          </description>
        </leaf>
      </container>
      <container name="ptp-tcoc-static-clock-src">
        <description>
          <text>Configure clock ID and port number of a TCOC static clock source.</text>
        </description>
        <leaf name="tcoc-static-clk-id">
          <type name="string">
            <length value="16"/>
            <pattern value="[0-9a-f]{16}"/>
          </type>
          <must condition="((/clocksync-1588v2:clocksync-1588v2/clocksync-1588v2:ptp-common/clocksync-1588v2:device-type='p2ptcoc' or /clocksync-1588v2:clocksync-1588v2/clocksync-1588v2:ptp-common/clocksync-1588v2:device-type='e2etcoc') or (../tcoc-static-clk-id='0000000000000000'))"/>
          <default value="0000000000000000"/>
          <description>
            <text>When the TCOC device type is used, TCOC static Clock ID and interface number can be specified for PTP interface clock sources, so that a local device can trace a specified upstream PTP device.</text>
          </description>
        </leaf>
        <leaf name="tcoc-static-clk-port-num">
          <type name="int32">
            <range value="0..65535"/>
          </type>
          <must condition="(not(../tcoc-static-clk-id='0000000000000000') and (../tcoc-static-clk-id)) or (../tcoc-static-clk-port-num=0)"/>
          <default value="0"/>
          <description>
            <text>When the TCOC device type is used, TCOC static Clock ID and interface number can be specified for PTP interface clock sources, so that a local device can trace a specified upstream PTP device.</text>
          </description>
        </leaf>
      </container>
      <container name="ptp-ring-fiber-length-change">
        <config value="false"/>
        <description>
          <text>Statistics of ring-network asymmetry delay. If auto-measurement of ring-network asymmetry delay is enabled, the device can automatically calculate the correction value, including positive and negative flags as well as the value to be corrected.</text>
        </description>
        <leaf name="change-value-flag">
          <type name="ptp-sign-flag"/>
          <description>
            <text>If auto-measurement of ring-network asymmetry delay is enabled, the device can automatically calculate the correction value, including positive and negative flags as well as the value to be corrected.</text>
          </description>
        </leaf>
        <leaf name="change-value">
          <type name="uint32"/>
          <description>
            <text>If auto-measurement of ring-network asymmetry delay is enabled, the device can automatically calculate the correction value, including positive and negative flags as well as the value to be corrected.</text>
          </description>
        </leaf>
      </container>
    </container>
  </augment>
</module>
