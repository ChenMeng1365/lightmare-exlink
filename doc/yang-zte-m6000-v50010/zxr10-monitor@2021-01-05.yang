module zxr10-monitor {
  namespace 'http://www.zte.com.cn/zxr10/netconf/schema/rosng/monitor'
            ;
  prefix monitor ;
  yang-version 1 ;
  import zxr10-netconf-defs {
    prefix ncd ;
  }
  organization 'ZTE Corporation'
               ;
  contact 'ZTE Corporation  '+"\n"
        + '   Nanjing Institute of ZTE Corporation '+"\n"
        + '   No.68 Zijinghua Rd. Yuhuatai District, Nanjing, China '+"\n"
        + '   Tel: +86-25-52870000'
          ;
  revision 2021-01-05 {
    description '1 Add default-vxlans with feature default-vxlan-feature to sup'
              + 'port destination VXLAN:'+"\n"
              + '/configuration/monitor/sessions/session/default-vxlans;'+"\n"
              + '2 Add remote-monitor with feature remote-monitor-feature to su'
              + 'pport RSPAN:'+"\n"
              + '/configuration/monitor/sessions/session/default-interfaces/def'
              + 'ault-interface/remote-monitor;'+"\n"
              + '3 Add encap-remote-monitor with feature encap-remote-monitor-f'
              + 'eature to support ERSPAN:'+"\n"
              + '/configuration/monitor/sessions/session/default-gres/default-g'
              + 're/encap-remote-monitor;'+"\n"
              + '4 Add switchvlan-sources with feature switchvlan-source-featur'
              + 'e to support switchvlan as monitor source:'+"\n"
              + '/configuration/monitor/sessions/session/sources/switchvlan-sou'
              + 'rces.'+"\n"
              + '5 Add cpu-sources with feature cpu-source-feature to support C'
              + 'PU monitor source:'+"\n"
              + '/configuration/monitor/sessions/session/sources/cpu-sources.'
                ;
  }
  revision 2020-06-15 {
    description 'Support description for seesion:'+"\n"
              + '/configuration/monitor/sessions/session/description.'
                ;
  }
  revision 2020-04-13 {
    description '1. Support multi-acl-destinations with feature multi-acl-featu'
              + 're:'+"\n"
              + '/configuration/monitor/sessions/session/multi-acl-rules;'+"\n"
              + '2. Support default-interfaces with feature default-interface-f'
              + 'eature:'+"\n"
              + '/configuration/monitor/sessions/session/default-interfaces.'+"\n"
              + '3. Add enumeration value ipv4-mixed-acl:'+"\n"
              + '/configuration/monitor/sessions/session/acl-rules/acl-rule/acl'
              + '-type'
                ;
  }
  revision 2017-05-16 {
    description 'add destination of gre.'
                ;
  }
  revision 2017-04-14 {
    description '1.add vpws case to /configuration/monitor/sessions/session/acl'
              + '-rules/acl-rule/destination-type/.'+"\n"
              + '2.add default-vpwss container to /configuration/monitor/sessio'
              + 'ns/session/. '+"\n"
              + '3.add default-observers container to /configuration/monitor/se'
              + 'ssions/session/. '+"\n"
              + '4.add rule-observers container to /configuration/monitor/sessi'
              + 'ons/session/.'+"\n"
              + '5.add flow-type leaf to /configuration/monitor/sessions/sessio'
              + 'n/sources/interface-source/.'+"\n"
              + '6.add vpws-source list to /configuration/monitor/sessions/sess'
              + 'ion/sources/.'
                ;
  }
  revision 2015-07-13 {
    description 'Initial version'
                ;
  }
  feature multi-acl-feature {
    description 'Mark multiple ACL destination.'
                ;
  }
  feature default-interface-feature {
    description 'Mark multiple default destination.'
                ;
  }
  feature default-vxlan-feature {
    description 'Mark default destination VXLAN.'
                ;
  }
  feature switchvlan-source-feature {
    description 'Mark switchvlan source.'
                ;
  }
  feature remote-monitor-feature {
    description 'Mark remote monitor.'
                ;
  }
  feature encap-remote-monitor-feature {
    description 'Mark encapsulated remote monitor.'
                ;
  }
  feature cpu-source-feature {
    description 'Mark CPU source.'
                ;
  }
