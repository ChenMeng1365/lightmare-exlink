                <description>
                  <text>Tag value.</text>
                </description>
                <type name="uint32">
                  <range value="0..4294967295">
                    <description>
                      <text>The range of tag-value is from 0 to 4294967295.</text>
                    </description>
                  </range>
                </type>
                <mandatory value="true"/>
              </leaf>
              <ncd:revise-date date="2019-04-17"/>
            </container>
            <container name="tunnel-encapsulation">
              <description>
                <text>Tunnel encapsulation.</text>
              </description>
              <presence value="tunnel-encapsulation"/>
