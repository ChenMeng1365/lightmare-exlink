              <enum name="v1">
                <value value="1"/>
              </enum>
              <enum name="v2">
                <value value="2"/>
              </enum>
              <enum name="all">
                <value value="0"/>
              </enum>
            </type>
          </leaf>
          <leaf name="send-version">
            <description>
              <text>The version of RIP message sent on the interface,include the send mode in RIP-2.</text>
            </description>
