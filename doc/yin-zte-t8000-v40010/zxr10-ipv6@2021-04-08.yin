            <leaf name="group-report-v2">
              <description>
                <text>The number of sending group-report-v2 packets.</text>
              </description>
              <type name="uint32"/>
            </leaf>
            <leaf name="rs">
              <description>
                <text>The number of sending router solicit packets.</text>
              </description>
              <type name="uint32"/>
            </leaf>
            <leaf name="ra">
              <description>
                <text>The number of sending router advert packets.</text>
              </description>
              <type name="uint32"/>
            </leaf>
            <leaf name="ns">
              <description>
                <text>The number of sending neighour solicit packets.</text>
              </description>
              <type name="uint32"/>
            </leaf>
            <leaf name="na">
              <description>
                <text>The number of sending neighour advert packets.</text>
              </description>
              <type name="uint32"/>
            </leaf>
          </container>
          <container name="send">
            <description>
              <text>ICMP sending paragrams.</text>
            </description>
            <leaf name="error-message-out">
              <description>
                <text>The number of sending error message packets.</text>
              </description>
              <type name="uint32"/>
            </leaf>
            <leaf name="rate-discard">
              <description>
                <text>The number of discard packets due to rate.</text>
              </description>
              <type name="uint32"/>
            </leaf>
            <leaf name="dest-unreach">
