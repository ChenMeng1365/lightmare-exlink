          <case name="damp">
            <description>
              <text>Single interface status suppression.</text>
            </description>
            <container name="damp">
              <presence value="Enable/disable suppression on an interface's physical status flappings."/>
              <description>
                <text>Enable/disable suppression on an interface's physical status flappings.</text>
              </description>
              <leaf name="tx-off">
