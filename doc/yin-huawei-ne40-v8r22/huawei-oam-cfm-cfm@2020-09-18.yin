<?xml version="1.0" encoding="UTF-8"?>
<module name="huawei-oam-cfm-cfm"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:oam-cfm-cfm="urn:huawei:yang:huawei-oam-cfm-cfm"
        xmlns:ext="urn:huawei:yang:huawei-extension"
        xmlns:oammgr="urn:huawei:yang:huawei-oammgr"
        xmlns:y1731="urn:huawei:yang:huawei-y1731">
  <namespace uri="urn:huawei:yang:huawei-oam-cfm-cfm"/>
  <prefix value="oam-cfm-cfm"/>
  <import module="huawei-extension">
    <prefix value="ext"/>
  </import>
  <import module="huawei-oammgr">
    <prefix value="oammgr"/>
  </import>
  <import module="huawei-y1731">
    <prefix value="y1731"/>
  </import>
  <organization>
    <text>Huawei Technologies Co., Ltd.</text>
  </organization>
  <contact>
    <text>Huawei Industrial Base
Bantian, Longgang
Shenzhen 518129
People's Republic of China
Website: http://www.huawei.com
Email: support@huawei.com</text>
  </contact>
  <description>
    <text>Configure the operation, administration and maintenance (OAM) association function.</text>
  </description>
  <revision date="2020-09-18">
    <description>
      <text>Initial revision.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <ext:task-name value="oammgr"/>
  <typedef name="cfm-bind-cfm-type">
    <type name="enumeration">
      <enum name="CFM1-CFM2">
        <value value="1"/>
        <description>
          <text>Type that CFM1 transfers fault information to CFM2.</text>
        </description>
      </enum>
      <enum name="CFM2-CFM1">
        <value value="2"/>
        <description>
          <text>Type that CFM2 transfers fault information to CFM1.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Binding type.</text>
    </description>
  </typedef>
  <augment target-node="/oammgr:oammgr">
    <description>
      <text>Configure the operation, administration and maintenance (OAM) association function.</text>
    </description>
    <container name="cfm-bind-cfms">
      <description>
        <text>List of associations between CFM and CFM.</text>
      </description>
      <list name="cfm-bind-cfm">
        <must condition="not((md-name1=md-name2) and (ma-name1=ma-name2))"/>
        <key value="md-name1 ma-name1 md-name2 ma-name2 bind-type"/>
        <unique tag="md-name1 ma-name1 bind-type"/>
        <unique tag="md-name2 ma-name2 bind-type"/>
        <description>
          <text>Configure an instance of an association between CFM and CFM.</text>
        </description>
        <leaf name="md-name1">
          <type name="leafref">
            <path value="/y1731:y1731/y1731:global-enable/y1731:mds/y1731:md/y1731:name"/>
          </type>
          <description>
            <text>Configure the name of the first MD.</text>
          </description>
        </leaf>
        <leaf name="ma-name1">
          <type name="leafref">
            <path value="/y1731:y1731/y1731:global-enable/y1731:mds/y1731:md[y1731:name=current()/../md-name1]/y1731:mas/y1731:ma/y1731:name"/>
          </type>
          <description>
            <text>Configure the name of the first MA.</text>
          </description>
        </leaf>
        <leaf name="md-name2">
          <type name="leafref">
            <path value="/y1731:y1731/y1731:global-enable/y1731:mds/y1731:md/y1731:name"/>
          </type>
          <description>
            <text>Configure the name of the second MD.</text>
          </description>
        </leaf>
        <leaf name="ma-name2">
          <type name="leafref">
            <path value="/y1731:y1731/y1731:global-enable/y1731:mds/y1731:md[y1731:name=current()/../md-name2]/y1731:mas/y1731:ma/y1731:name"/>
          </type>
          <description>
            <text>Configure the name of the second MA.</text>
          </description>
        </leaf>
        <leaf name="bind-type">
          <type name="cfm-bind-cfm-type"/>
          <description>
            <text>Configure a binding type.</text>
          </description>
        </leaf>
      </list>
    </container>
  </augment>
</module>
