<?xml version="1.0" encoding="UTF-8"?>
<module name="huawei-mpls-forward-devm"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:mpls-forward-devm="urn:huawei:yang:huawei-mpls-forward-devm"
        xmlns:devm="urn:huawei:yang:huawei-devm"
        xmlns:loadbalance="urn:huawei:yang:huawei-loadbalance"
        xmlns:ext="urn:huawei:yang:huawei-extension">
  <yang-version value="1"/>
  <namespace uri="urn:huawei:yang:huawei-mpls-forward-devm"/>
  <prefix value="mpls-forward-devm"/>
  <import module="huawei-devm">
    <prefix value="devm"/>
  </import>
  <import module="huawei-loadbalance">
    <prefix value="loadbalance"/>
  </import>
  <import module="huawei-extension">
    <prefix value="ext"/>
  </import>
  <organization>
    <text>Huawei Technologies Co., Ltd.</text>
  </organization>
  <contact>
    <text>Huawei Industrial Base
Bantian, Longgang
Shenzhen 518129
People's Republic of China
Website: http://www.huawei.com
Email: support@huawei.com</text>
  </contact>
  <description>
    <text>MPLS forward for DEVM.</text>
  </description>
  <revision date="2020-02-27">
    <description>
      <text>The type of some nodes is fixed.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <revision date="2019-03-27">
    <description>
      <text>Init revision.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <typedef name="hash-cfg-flag">
    <type name="enumeration">
      <enum name="payload-header">
        <value value="64"/>
        <description>
          <text>Hashed based on payload-header.</text>
        </description>
      </enum>
      <enum name="label">
        <value value="192"/>
        <description>
          <text>Hashed based on label.</text>
        </description>
      </enum>
      <enum name="ip">
        <value value="160"/>
        <description>
          <text>Hashed based on label or payload-header.</text>
        </description>
      </enum>
      <enum name="ip-tos">
        <value value="193"/>
        <description>
          <text>Hashed based on label or TOS.</text>
        </description>
      </enum>
      <enum name="mac">
        <value value="196"/>
        <description>
          <text>Hashed based on label or MAC.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Hash configuration flag.</text>
    </description>
  </typedef>
  <typedef name="enable-flag">
    <type name="enumeration">
      <enum name="enable">
        <value value="1"/>
        <description>
          <text>The CFG is enabled.</text>
        </description>
      </enum>
      <enum name="disable">
        <value value="0"/>
        <description>
          <text>The CFG is disabled.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Enable flag type.</text>
    </description>
  </typedef>
  <typedef name="check-mode">
    <type name="enumeration">
      <enum name="ip">
        <value value="291"/>
        <description>
          <text>The check mode is IP.</text>
        </description>
      </enum>
      <enum name="label-contained-length">
        <value value="292"/>
        <description>
          <text>The check mode is label-contained-length.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Check mode.</text>
    </description>
  </typedef>
  <typedef name="hash-type">
    <type name="enumeration">
      <enum name="flow">
        <value value="194"/>
        <description>
          <text>Load balance based on flow.</text>
        </description>
      </enum>
      <enum name="packet">
        <value value="195"/>
        <description>
          <text>Load balance based on packet.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Hash type.</text>
    </description>
  </typedef>
  <augment target-node="/devm:devm/devm:lpu-boards/devm:lpu-board">
    <description>
      <text>MPLS forward configuration to the DEVM module.</text>
    </description>
    <container name="mpls-forward">
      <description>
        <text>Configure MPLS forward.</text>
      </description>
      <container name="mpls-hash-mode">
        <description>
          <text>Configure MPLS hash mode.</text>
        </description>
        <leaf name="hashcfg">
          <type name="hash-cfg-flag"/>
          <default value="ip"/>
          <description>
            <text>MPLS hash mode value.</text>
          </description>
        </leaf>
      </container>
      <container name="mplsfrags">
        <description>
          <text>Configure dynamic load balance.</text>
        </description>
        <leaf name="mplsfrag-enable">
          <type name="enable-flag"/>
          <default value="disable"/>
          <description>
            <text>MPLS fragment enable value.</text>
          </description>
        </leaf>
      </container>
      <container name="mpls-load-ctrwords">
        <description>
          <text>Configure MPLS load balance control word disable.</text>
        </description>
        <leaf name="mplsload-ctrword-disable">
          <type name="enable-flag"/>
          <default value="disable"/>
          <description>
            <text>MPLS load balance control word disable value.</text>
          </description>
        </leaf>
      </container>
    </container>
  </augment>
  <augment target-node="/devm:devm/devm:mpu-boards/devm:mpu-board">
    <description>
      <text>MPLS forward configuration to the DEVM module.</text>
    </description>
    <container name="mpls-forward">
      <description>
        <text>Configure MPLS forward.</text>
      </description>
      <container name="mpls-hash-mode">
        <description>
          <text>Configure MPLS hash mode.</text>
        </description>
        <leaf name="hashcfg">
          <type name="hash-cfg-flag"/>
          <default value="ip"/>
          <description>
            <text>MPLS hash mode value.</text>
          </description>
        </leaf>
      </container>
      <container name="mplsfrags">
        <description>
          <text>Configure dynamic load balance configuration.</text>
        </description>
        <leaf name="mplsfrag-enable">
          <type name="enable-flag"/>
          <default value="disable"/>
          <description>
            <text>MPLS fragment enable value.</text>
          </description>
        </leaf>
      </container>
      <container name="mpls-load-ctrwords">
        <description>
          <text>Configure MPLS load balance control word disable.</text>
        </description>
        <leaf name="mplsload-ctrword-disable">
          <type name="enable-flag"/>
          <default value="disable"/>
          <description>
            <text>MPLS load balance control word disable value.</text>
          </description>
        </leaf>
      </container>
    </container>
  </augment>
  <augment target-node="/loadbalance:loadbalance">
    <description>
      <text>MPLS forward configuration to the load balance module.</text>
    </description>
    <container name="mpls-hashpacket">
      <description>
        <text>Configure load balance based on packet.</text>
      </description>
      <list name="hashpacket">
        <key value="slotid"/>
        <description>
          <text>Configure load balance based on packet.</text>
        </description>
        <ext:generated-by value="system">
          <description>
            <text>The all instances are generated automatically.</text>
          </description>
        </ext:generated-by>
        <ext:operation-exclude value="create">
          <description>
            <text>The all instances cannot be created.</text>
          </description>
        </ext:operation-exclude>
        <leaf name="slotid">
          <type name="string">
            <length value="1..32"/>
          </type>
          <description>
            <text>Slot ID.</text>
          </description>
        </leaf>
        <leaf name="hash-type">
          <type name="hash-type"/>
          <default value="flow"/>
          <description>
            <text>Load balance mode.</text>
          </description>
          <ext:support-filter value="true"/>
        </leaf>
      </list>
    </container>
    <container name="mpls-intfmtu">
      <description>
        <text>Configure interface MTU check mode.</text>
      </description>
      <list name="intfmtu">
        <key value="slotid"/>
        <description>
          <text>Configure interface MTU check mode.</text>
        </description>
        <ext:generated-by value="system">
          <description>
            <text>The all instances are generated automatically.</text>
          </description>
        </ext:generated-by>
        <ext:operation-exclude value="create">
          <description>
            <text>The all instances cannot be created.</text>
          </description>
        </ext:operation-exclude>
        <leaf name="slotid">
          <type name="string">
            <length value="1..32"/>
          </type>
          <description>
            <text>Slot ID.</text>
          </description>
        </leaf>
        <leaf name="chkmode">
          <type name="check-mode"/>
          <mandatory value="true"/>
          <description>
            <text>Check mode configuration.</text>
          </description>
          <ext:support-filter value="true"/>
        </leaf>
      </list>
    </container>
  </augment>
  <ext:task-name value="forwarding"/>
</module>
