              + 'e'
                ;
  }
  revision 2015-06-04 {
    description '1.remove two leaf nodes vrf-name from :/state/mld/private/grou'
              + 'p-summary/intfs/intf/vrf-name and /state/mld/private/group-sum'
              + 'mary/all-intfs/vrf-name to one leaf to:/state/mld/private/grou'
              + 'p-summary/summary/vrf-name.'+"\n"
              + '2. remove two leaf nodes vrf-name from :/state/mld/private/pac'
              + 'kets/intfs/intf/vrf-name and /state/mld/private/packets/all-in'
              + 'tfs/vrf-name to one leaf to:/state/mld/private/packets/packet/'
              + 'vrf-name.'+"\n"
              + '3.modify the name of leaf nodes last-report :/state/mld/privat'
              + 'e/groups/group/last-report to last-reporter  :/state/mld/priva'
              + 'te/groups/group/last-reporter,rec-leave :/state/mld/private/pa'
              + 'ckets/packet/all-intf/rec-leave to rec-done  :/state/mld/priva'
              + 'te/packets/packet/all-intf/rec-done, sent-leave :/state/mld/pr'
              + 'ivate/packets/packet/all-intf/sent-leave to sent-done  :/state'
              + '/mld/private/packets/packet/all-intf/sent-done,rec-leave :/sta'
              + 'te/mld/private/packets/packet/intf/rec-leave to rec-done  :/st'
              + 'ate/mld/private/packets/packet/intf/rec-done, sent-leave :/sta'
              + 'te/mld/private/packets/packet/intf/sent-leave to sent-done :/s'
              + 'tate/mld/private/packets/packet/intf/sent-done,rec-leave :/sta'
              + 'te/mld/public/packet/all-intf/rec-leave to rec-done :/state/ml'
              + 'd/public/packet/all-intf/rec-done, sent-leave :/state/mld/publ'
              + 'ic/packet/all-intf/sent-leave to sent-done :/state/mld/public/'
              + 'packet/all-intf/sent-done,rec-leave :/state/mld/public/packet/'
              + 'intfs/intf/rec-leave to rec-done :/state/mld/public/packet/int'
              + 'fs/intf/rec-done, sent-leave :/state/mld/public/packet/intfs/i'
              + 'ntf/sent-leave to sent-done :/state/mld/public/packet/intfs/in'
              + 'tf//sent-done.'
                ;
  }
  revision 2015-04-15 {
    description '1. add state model :/state/.'+"\n"
              + '2. add operation model :/operation/.'+"\n"
              + '3. modify description :/configuration/router-mld/mlds/global'
                ;
  }
  revision 2014-04-10 {
    description 'Modify the range of vrf-name from (1..31) to (1..32).'
                ;
  }
  revision 2014-03-19 {
    description 'Modify the default value of immediate-leaves and the range of '
              + 'acl from(1..31) to (0..31).'
                ;
  }
  revision 2014-03-11 {
    description 'Initial version.'
                ;
  }
  container configuration {
    description 'Configuration model.'
                ;
    container router-mld {
      description 'Router mld'
                  ;
      container mlds {
        description 'MLD instance model'
                    ;
        presence mld ;
        container global {
          description 'Configure MLD instance.'
                      ;
          leaf last-member-query {
            description 'MLD last member query interval'
                        ;
            type uint32 {
              range 1..25 {
                description 'Last member query interval(second)'
                            ;
              }
            }
            units second ;
            default 1 ;
          }
          leaf querier-timeout {
            description 'MLD previous querier timeout'
                        ;
            type uint32 {
              range 60..300 {
                description 'Timeout value(second)'
                            ;
              }
            }
            units second ;
            default 255 ;
          }
          leaf query-interval {
            description 'MLD host query interval'
                        ;
            type uint32 {
              range 1..65535 {
                description 'Query interval(second)'
                            ;
              }
            }
            units second ;
            default 125 ;
          }
          leaf query-max-response {
            description 'MLD max query response value'
                        ;
            type uint32 {
              range 1..25 {
                description 'Query response value(second)'
                            ;
              }
            }
            units second ;
            default 10 ;
          }
          leaf require-alert-options {
            description 'Require MLD message with router alert options'
                        ;
            type empty ;
          }
          leaf robustness-variable {
            description 'MLD robustness count'
                        ;
            type uint32 {
              range 2..7 {
                description 'Robustness count'
                            ;
              }
            }
            default 2 ;
          }
          leaf version {
            description 'MLD version'
                        ;
            type uint32 {
              range 1..2 {
                description '1  Version 1
2  Version 2'
                            ;
              }
            }
            default 2 ;
          }
          leaf ip-source-unspecified {
            description 'Permit unspecified source address(::/128) of MLD repor'
                      + 't message'
                        ;
            type empty ;
            ncd:revise-date 2021-01-25 ;
          }
        }
        container group-intf-nums {
          description 'Max host limit commands'
                      ;
          list num {
            description 'Max host limit commands'
                        ;
            max-elements 2048 ;
            key group-addr ;
            leaf group-addr {
              description 'Group address'
                          ;
              type ncd:ipv6-address ;
            }
            leaf intf-num {
              description 'The number of interface'
                          ;
              type uint32 {
                range 1..64 {
                  description 'Interface number'
                              ;
                }
              }
              mandatory true ;
            }
          }
        }
        container ssmmaps {
          description 'SSM mapping commands'
                      ;
          container acls {
            description 'Access list to specify groups'
                        ;
            list acl {
              description 'Access list to specify groups'
                          ;
              max-elements 1000 ;
              key 'acl-name src-addr'
                  ;
              leaf acl-name {
                description 'Acl name'
                            ;
                type string {
                  length 1..31 {
                    description 'Access list to specify groups'
                                ;
                  }
                }
              }
              leaf src-addr {
                description 'Source address'
                            ;
                type ncd:ipv6-address ;
              }
            }
          }
          container defaults {
            description 'Default SSM group'
                        ;
            list default {
              description 'Default SSM group'
                          ;
              max-elements 1000 ;
              key 'src-addr default'
                  ;
              leaf src-addr {
                description 'Source address'
                            ;
                type ncd:ipv6-address ;
              }
              leaf default {
                description 'Default ssm group'
                            ;
                type uint8 {
                  range 1 {
                    description 'Default ssm group'
                                ;
                  }
                }
              }
            }
          }
        }
        container interfaces {
          description 'MLD interface model'
                      ;
          list interface {
            description 'MLD interface model'
                        ;
            key if-name ;
            leaf if-name {
              description 'Interface name'
                          ;
              type ncd:ifname {
                length 1..31 {
                  description 'Interface name'
                              ;
                }
              }
            }
            leaf access-group {
              description 'MLD access group'
                          ;
              type string {
                length 1..31 {
                  description 'Access list name (1-31 characters)'
                              ;
                }
              }
            }
            leaf last-member-query-interval {
              description 'MLD last member query interval'
                          ;
              type uint32 {
                range 1..25 {
                  description 'Last member query interval(second)'
                              ;
                }
              }
              units second ;
              default 1 ;
            }
            leaf querier-timeout {
              description 'MLD previous querier timeout'
                          ;
              type uint32 {
                range 60..300 {
                  description 'Timeout value'
                              ;
                }
              }
              units second ;
              default 255 ;
            }
            leaf query-interval {
              description 'MLD host query interval'
                          ;
              type uint32 {
                range 1..65535 {
                  description 'Timeout value(second)'
                              ;
                }
              }
              units second ;
              default 125 ;
            }
            leaf query-max-response-time {
              description 'MLD max query response value'
                          ;
              type uint32 {
                range 1..25 {
                  description 'Query response value(second)'
                              ;
                }
              }
              units second ;
              default 10 ;
            }
            leaf robustness-count {
              description 'MLD robustness count'
                          ;
              type uint32 {
                range 2..7 {
                  description 'Robustness count'
                              ;
                }
              }
              default 2 ;
            }
            leaf version {
              description 'MLD version'
                          ;
              type uint32 {
                range 1..2 {
                  description '1  Version 1
2  Version 2'
                              ;
                }
              }
              default 2 ;
            }
            leaf querier-disable {
              description 'MLD querier election disable command'
                          ;
              type empty ;
            }
            leaf maximum-joins {
              description 'MLD maximum joins'
                          ;
              type uint32 {
                range 1..40000 {
                  description 'Max allowed joins on this interface'
                              ;
                }
              }
              ncd:revise-date 2019-07-15 ;
            }
            leaf specific-query-ignore {
              description 'Specific query ignore.'
                          ;
              type empty ;
              ncd:revise-date 2019-07-15 ;
            }
            leaf access-ip-source {
              description 'Access ip source filter.'
                          ;
              type string {
                length 1..31 ;
              }
              ncd:revise-date 2019-07-15 ;
            }
            leaf startup-query-interval {
              description 'Startup query interval.'
                          ;
              type uint32 {
                range 1..18000 ;
              }
              units second ;
              default 31 ;
              ncd:revise-date 2019-07-15 ;
            }
            leaf startup-query-count {
              description 'Startup query count.'
                          ;
              type uint32 {
                range 1..10 ;
              }
              default 2 ;
              ncd:revise-date 2019-07-15 ;
            }
            leaf sibtp-service {
              description 'MLD enable SIBTP service'
                          ;
              type empty ;
              ncd:revise-date 2021-04-24 ;
            }
            container mld-groups {
              status obsolete {
                ncd:revise-date 2015-07-01 ;
              }
              description 'MLD static multicast group'
                          ;
              list mld-group {
                description 'MLD static multicast group'
                            ;
                max-elements 65535 ;
                key 'group source ssm-map static-flag'
                    ;
                leaf group {
                  description 'Group address'
                              ;
                  type ncd:ipv6-address ;
                }
                leaf source {
                  description 'Source address'
                              ;
                  type ncd:ipv6-address ;
                }
                leaf ssm-map {
                  description 'SSM static group to source mapping'
                              ;
                  type uint32 {
                    range 0..1 {
                      description 'SSM static group to source mapping'
                                  ;
                    }
                  }
                }
                leaf static-flag {
                  description Hiddenpara ;
                  type uint32 {
                    range 1..2 {
                      description '1 static-group
2 join-group'
                                  ;
                    }
                  }
                }
                leaf filter-mode {
                  description 'Filter mode'
                              ;
                  type uint32 {
                    range 0..4 {
                      description 'Filter mode'
                                  ;
                    }
                  }
                }
              }
            }
            container immediate-leave {
              description 'Leave groups immediately without sending last member'
                        + ' query'
                          ;
              choice immediate {
                description 'Leave groups immediately without sending last memb'
                          + 'er query'
                            ;
                case acls {
                  description 'Immediate acl'
                              ;
                  leaf acl {
                    description 'Leave groups immediately without sending last '
                              + ' member query'
                                ;
                    type string {
                      length 1..31 {
                        description 'Immediate acl'
                                    ;
                      }
                    }
                  }
                }
                case alls {
                  description 'Immediate all'
                              ;
                  leaf all {
                    description 'Leave groups immediately without sending last '
                              + 'member query'
                                ;
                    type empty ;
                  }
                }
              }
            }
            container mld-group {
              description 'MLD static multicast group'
                          ;
              list groups {
                description 'MLD static multicast group'
                            ;
                max-elements 65535 ;
                key 'group source ssm-map static-flag inc-mask'
                    ;
                leaf group {
                  description 'Group address'
                              ;
                  type ncd:ipv6-address ;
                }
                leaf source {
                  description 'Source address'
                              ;
                  type ncd:ipv6-address ;
                }
                leaf ssm-map {
                  description 'SSM static group to source mapping'
                              ;
                  type uint32 {
                    range 0..1 {
                      description 'SSM static group to source mapping'
                                  ;
                    }
                  }
                }
                leaf static-flag {
                  description 'MLD static multicast group or join multicast gro'
                            + 'up'
                              ;
                  type enumeration {
                    enum static-group {
                      value 1 ;
                    }
                    enum join-group {
                      value 2 ;
                    }
                  }
                }
                leaf inc-mask {
                  description 'Increment mask for group address'
                              ;
                  type ncd:ipv6-address ;
                }
                leaf filter-mode {
                  description 'Filter mode'
                              ;
                  type enumeration {
                    enum no-mode {
                      value 0 ;
                    }
                    enum include {
                      value 3 ;
                    }
                    enum exclude {
                      value 4 ;
                    }
                  }
                }
                leaf count {
                  description 'Number of group addresses to join'
                              ;
                  type uint32 {
                    range 1..512 ;
                  }
                }
              }
              ncd:revise-date 2015-07-01 ;
            }
          }
        }
      }
      container mlds-vrf {
        description 'Configure MLD VRF parameters.'
                    ;
        list mld-vrf {
          description 'Configure MLD VRF parameters.'
                      ;
          key vrf-name ;
          leaf vrf-name {
            description 'Vrf name'
                        ;
            type string {
              length 1..32 {
                description 'Vrf name'
                            ;
              }
            }
          }
          leaf last-member-query {
            description 'MLD last member query interval'
                        ;
            type uint32 {
              range 1..25 {
                description 'Last member query interval(second)'
                            ;
              }
            }
            units second ;
            default 1 ;
          }
          leaf querier-timeout {
            description 'MLD previous querier timeout'
                        ;
            type uint32 {
              range 60..300 {
                description 'Timeout value(second)'
                            ;
              }
            }
            units second ;
            default 255 ;
          }
          leaf query-interval {
            description 'MLD host query interval'
                        ;
            type uint32 {
              range 1..65535 {
                description 'Query interval(second)'
                            ;
              }
            }
            units second ;
            default 125 ;
          }
          leaf require-alert-options {
            description 'Require MLD message with router alert options'
                        ;
            type empty ;
          }
          leaf query-max-response {
            description 'MLD max query response value'
                        ;
            type uint32 {
              range 1..25 {
                description 'Query response value(second)'
                            ;
              }
            }
            units second ;
            default 10 ;
          }
          leaf robustness-variable {
            description 'MLD robustness count'
                        ;
            type uint32 {
              range 2..7 {
                description 'Robustness count'
                            ;
              }
            }
            default 2 ;
          }
          leaf version {
            description 'MLD version'
                        ;
            type uint32 {
              range 1..2 {
                description '1  Version 1
2  Version 2'
                            ;
              }
            }
            default 2 ;
          }
          leaf ip-source-unspecified {
            description 'Permit unspecified source address(::/128) of MLD repor'
                      + 't message'
                        ;
            type empty ;
            ncd:revise-date 2021-01-25 ;
          }
          container group-intf-nums {
            description 'Max host limit commands'
                        ;
            list num {
              description 'Max host limit commands'
                          ;
              max-elements 2048 ;
              key group-addr ;
              leaf group-addr {
                description 'Group address'
                            ;
                type ncd:ipv6-address ;
              }
              leaf intf-num {
                description 'The number of interface'
                            ;
                type uint32 {
                  range 1..64 {
                    description 'Interface number'
                                ;
                  }
                }
                mandatory true ;
              }
            }
          }
          container ssmmaps {
            description 'SSM mapping commands'
                        ;
            container acls {
              description 'Access list to specify groups'
                          ;
              list acl {
                description 'Access list to specify groups'
                            ;
                max-elements 1000 ;
                key 'acl-name src-addr'
                    ;
                leaf acl-name {
                  description 'Acl name'
                              ;
                  type string {
                    length 1..31 {
                      description 'Access list to specify groups'
                                  ;
                    }
                  }
                }
                leaf src-addr {
                  description 'Source address'
                              ;
                  type ncd:ipv6-address ;
                }
              }
            }
            container defaults {
              description 'Default SSM group'
                          ;
              list default {
                description 'Default SSM group'
                            ;
                max-elements 1000 ;
                key 'src-addr default'
                    ;
                leaf src-addr {
                  description 'Source address'
                              ;
                  type ncd:ipv6-address ;
                }
                leaf default {
                  description 'Default ssm group'
                              ;
                  type uint8 {
                    range 1 {
                      description 'Default ssm group'
                                  ;
                    }
                  }
                }
              }
            }
          }
          container interfaces {
            description 'MLD interface model'
                        ;
            list interface {
              description 'MLD interface model'
                          ;
              key if-name ;
              leaf if-name {
                description 'Interface name'
                            ;
                type ncd:ifname {
                  length 1..31 {
                    description 'Interface name'
                                ;
                  }
                }
              }
              leaf access-group {
                description 'MLD access group'
                            ;
                type string {
                  length 1..31 {
                    description 'Access list name (1-31 characters)'
                                ;
                  }
                }
              }
              leaf last-member-query-interval {
                description 'MLD last member query interval'
                            ;
                type uint32 {
                  range 1..25 {
                    description 'Last member query interval(second)'
                                ;
                  }
                }
                units second ;
                default 1 ;
              }
              leaf querier-timeout {
                description 'MLD previous querier timeout'
                            ;
                type uint32 {
                  range 60..300 {
                    description 'Timeout value'
                                ;
                  }
                }
                units second ;
                default 255 ;
              }
              leaf query-interval {
                description 'MLD host query interval'
                            ;
                type uint32 {
                  range 1..65535 {
                    description 'Timeout value(second)'
                                ;
                  }
                }
                units second ;
                default 125 ;
              }
              leaf query-max-response-time {
                description 'MLD max query response value'
                            ;
                type uint32 {
                  range 1..25 {
                    description 'Query response value(second)'
                                ;
                  }
                }
                units second ;
                default 10 ;
              }
              leaf robustness-count {
                description 'MLD robustness count'
                            ;
                type uint32 {
                  range 2..7 {
                    description 'Robustness count'
                                ;
                  }
                }
                default 2 ;
              }
              leaf version {
                description 'MLD version'
                            ;
                type uint32 {
                  range 1..2 {
                    description '1  Version 1
2  Version 2'
                                ;
                  }
                }
                default 2 ;
              }
              leaf querier-disable {
                description 'MLD querier election disable command'
                            ;
                type empty ;
              }
              leaf maximum-joins {
                description 'MLD maximum joins'
                            ;
                type uint32 {
                  range 1..40000 {
                    description 'Max allowed joins on this interface'
                                ;
                  }
                }
                ncd:revise-date 2019-07-15 ;
              }
              leaf specific-query-ignore {
                description 'Specific query ignore.'
                            ;
                type empty ;
                ncd:revise-date 2019-07-15 ;
              }
              leaf access-ip-source {
                description 'Access ip source filter.'
                            ;
                type string {
                  length 1..31 ;
                }
                ncd:revise-date 2019-07-15 ;
              }
              leaf startup-query-interval {
                description 'Startup query interval.'
                            ;
                type uint32 {
                  range 1..18000 ;
                }
                units second ;
                default 31 ;
                ncd:revise-date 2019-07-15 ;
              }
              leaf startup-query-count {
                description 'Startup query count.'
                            ;
                type uint32 {
                  range 1..10 ;
                }
                default 2 ;
                ncd:revise-date 2019-07-15 ;
              }
              leaf sibtp-service {
                description 'MLD enable SIBTP service'
                            ;
                type empty ;
                ncd:revise-date 2021-04-24 ;
              }
