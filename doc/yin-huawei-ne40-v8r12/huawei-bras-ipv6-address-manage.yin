      <enum name="mac">
        <value value="1"/>
        <description>
          <text>Reserves IPv6 address or IPv6 prefix by mac.</text>
        </description>
      </enum>
      <enum name="duid">
        <value value="2"/>
        <description>
          <text>Reserves IPv6 address or IPv6 prefix by duid.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Reserve IPv6 address or IPv6 prefix type.</text>
    </description>
  </typedef>
  <typedef name="pool-type">
    <type name="enumeration">
      <enum name="local">
        <value value="1"/>
        <description>
          <text>The type of local IPv6 address pool.</text>
        </description>
      </enum>
      <enum name="delegation">
        <value value="2"/>
        <description>
          <text>The type of delegation IPv6 address pool.</text>
        </description>
      </enum>
      <enum name="remote">
        <value value="3"/>
        <description>
          <text>The type of remote IPv6 address pool.</text>
        </description>
      </enum>
      <enum name="relay">
        <value value="4"/>
        <description>
          <text>The type of relay IPv6 address pool.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>IPv6 pool type.</text>
    </description>
  </typedef>
  <typedef name="prefix-type">
    <type name="enumeration">
      <enum name="local">
        <value value="1"/>
        <description>
          <text>Local type.</text>
        </description>
      </enum>
      <enum name="delegation">
        <value value="2"/>
        <description>
          <text>Delegation type.</text>
        </description>
      </enum>
      <enum name="remote">
        <value value="3"/>
        <description>
          <text>Remote type.</text>
        </description>
      </enum>
      <enum name="dynamic">
        <value value="4"/>
        <description>
          <text>Dynamic type.</text>
        </description>
      </enum>
