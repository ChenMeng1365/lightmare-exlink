  <typedef name="interface-status">
    <type name="enumeration">
      <enum name="up">
        <value value="1"/>
        <description>
          <text>The interface is in the up state..</text>
        </description>
      </enum>
      <enum name="down">
        <value value="2"/>
        <description>
          <text>The interface is in the down state.</text>
        </description>
      </enum>
      <enum name="test">
        <value value="3"/>
        <description>
          <text>The interface is in the test state..</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Interface status.</text>
    </description>
