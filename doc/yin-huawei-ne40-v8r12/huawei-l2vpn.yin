            </type>
            <description>
              <text>PW ID used by the remote end for PW negotiation.</text>
            </description>
          </leaf>
          <leaf name="peer-ip">
            <type name="inet:ipv4-address-no-zone"/>
            <description>
              <text>IP address of the remote end.</text>
            </description>
          </leaf>
          <leaf name="encapsulation-type">
            <type name="encapsulation-type"/>
            <description>
              <text>Encapsulate type on the remote end.</text>
            </description>
          </leaf>
          <leaf name="remote-label">
            <type name="uint32"/>
            <description>
              <text>Label information on the remote end.</text>
            </description>
          </leaf>
          <leaf name="remote-group-id">
            <type name="uint32"/>
            <description>
              <text>Group ID on the remote end.</text>
            </description>
          </leaf>
          <leaf name="remote-mtu">
            <type name="uint32"/>
            <description>
              <text>MTU information on the remote end.</text>
            </description>
          </leaf>
          <leaf name="remote-state-code">
            <type name="l2vpn-vpls-ldp-pw-state-code"/>
            <description>
              <text>Status code information on the remote end.</text>
