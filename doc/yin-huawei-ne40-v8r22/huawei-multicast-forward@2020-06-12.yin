  <rpc name="get-multicast-speed-statistics">
    <description>
      <text>Query multicast speed statistics.</text>
    </description>
    <input>
      <leaf name="source-address">
        <type name="inet:ipv4-address-no-zone"/>
        <mandatory value="true"/>
        <description>
          <text>Multicast source address.</text>
        </description>
      </leaf>
      <leaf name="group-address">
        <type name="inet:ipv4-address-no-zone"/>
        <mandatory value="true"/>
        <description>
          <text>Multicast group address.</text>
        </description>
      </leaf>
      <leaf name="interface-name">
        <type name="leafref">
          <path value="/ifm:ifm/ifm:interfaces/ifm:interface/ifm:name"/>
        </type>
        <mandatory value="true"/>
        <description>
          <text>Specify interface-name.</text>
        </description>
      </leaf>
      <leaf name="flow-direction">
        <type name="rule-direction"/>
        <mandatory value="true"/>
        <description>
          <text>Data flow direction of interface.</text>
        </description>
      </leaf>
      <choice name="query-type">
        <description>
          <text>Multicast speed query type.</text>
        </description>
        <case name="vlan">
          <description>
            <text>Query by VLAN.</text>
          </description>
          <leaf name="vlan-id">
            <type name="uint32">
              <range value="1..4094"/>
            </type>
            <mandatory value="true"/>
            <description>
              <text>VLAN ID.</text>
            </description>
          </leaf>
        </case>
        <case name="pe-ce">
          <description>
            <text>Query by outer VLAN and inner VLAN.</text>
          </description>
          <leaf name="pe-vid">
            <type name="uint32">
              <range value="1..4094"/>
            </type>
            <mandatory value="true"/>
            <description>
              <text>Outer VLAN ID.</text>
            </description>
          </leaf>
          <leaf name="ce-vid">
            <type name="uint32">
              <range value="1..4094"/>
            </type>
            <mandatory value="true"/>
            <description>
              <text>Inner VLAN ID.</text>
            </description>
          </leaf>
        </case>
      </choice>
    </input>
    <output>
      <leaf name="receive-byte">
        <type name="uint64"/>
        <units name="Byte"/>
        <description>
          <text>Bytes received in 2 seconds.</text>
        </description>
      </leaf>
      <leaf name="receive-byte-rate">
        <type name="uint64"/>
        <units name="Byte/s"/>
        <description>
          <text>Byte-rate received in 2 seconds.</text>
        </description>
      </leaf>
      <leaf name="receive-packet">
        <type name="uint64"/>
        <description>
          <text>Packets received in 2 seconds.</text>
        </description>
      </leaf>
      <leaf name="receive-packet-rate">
        <type name="uint64"/>
        <units name="pps"/>
        <description>
          <text>Packet-rate received in 2 seconds.</text>
        </description>
      </leaf>
    </output>
  </rpc>
  <augment target-node="/ifm:ifm/ifm:interfaces/ifm:interface">
    <description>
      <text>Multicast forward configuration to the ifm module.</text>
    </description>
    <container name="multicast-forward">
      <description>
        <text>Configure multicast forward list.</text>
      </description>
      <container name="shaping">
        <description>
          <text>Configure multicast shaping of interface.</text>
        </description>
        <leaf name="shaping-enable">
          <when condition="../../../ifm:type='GigabitEthernet' or ../../../ifm:type='Eth-Trunk' or ../../../ifm:type='Ip-Trunk' or ../../../ifm:type='Pos' or ../../../ifm:type='100GE' or ../../../ifm:type='200GE' or ../../../ifm:type='40GE' or ../../../ifm:type='10GE' or ../../../ifm:type='4x10GE' or ../../../ifm:type='10x10GE' or ../../../ifm:type='3x40GE' or ../../../ifm:type='4x25GE' or ../../../ifm:type='25GE' or ../../../ifm:type='ATM' or ../../../ifm:type='XGigabitEthernet' or ../../../ifm:type='FlexE' or ../../../ifm:type='50|100GE' or ../../../ifm:type='50GE' or ../../../ifm:type='400GE'"/>
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable multicast shaping on an interface.</text>
          </description>
        </leaf>
      </container>
      <container name="hold-up">
        <description>
          <text>Configure the trunk member interface that changes from Down to Up to forward multicast traffic.</text>
        </description>
        <leaf name="time">
          <when condition="../../../ifm:type='Eth-Trunk' or ../../../ifm:type='Ip-Trunk'"/>
          <type name="uint32">
            <range value="2..300"/>
          </type>
          <units name="s"/>
          <default value="2"/>
          <description>
            <text>Delay for a trunk interface to go Up.</text>
          </description>
        </leaf>
      </container>
      <container name="backup">
        <description>
          <text>Configure master/backup protection for trunk member interfaces.</text>
        </description>
        <leaf name="ipv4-enable">
          <when condition="../../../ifm:type='Eth-Trunk' or ../../../ifm:type='Global-VE'"/>
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable master/backup protection for trunk member interfaces in Layer 3 IPv4 multicast scenarios.</text>
          </description>
        </leaf>
        <leaf name="ipv6-enable">
          <when condition="../../../ifm:type='Eth-Trunk' or ../../../ifm:type='Global-VE'"/>
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable master/backup protection for trunk member interfaces in Layer 3 IPv6 multicast scenarios.</text>
          </description>
        </leaf>
      </container>
    </container>
  </augment>
  <augment target-node="/qos:qos/qos:classifier-template/qos:traffic-behaviors/qos:traffic-behavior">
    <description>
      <text>Multicast forward configuration to the QoS module.</text>
    </description>
    <container name="behavior-bind-multicast-nat-instance">
      <when condition="/multicast-forward:multicast-forward/multicast-forward:nat"/>
      <description>
        <text>Configure multicast NAT instances and bind them to the traffic behavior.</text>
      </description>
      <list name="behavior-bind-multicast-nat">
        <key value="instance-id"/>
        <max-elements value="1"/>
        <description>
          <text>Configure a multicast NAT instance and bind it to the traffic behavior.</text>
        </description>
        <leaf name="instance-id">
          <type name="leafref">
            <path value="/multicast-forward:multicast-forward/multicast-forward:nat/multicast-forward:instances/multicast-forward:instance/multicast-forward:instance-id"/>
          </type>
          <description>
            <text>ID of a multicast NAT instance.</text>
          </description>
        </leaf>
        <leaf name="instance-name">
          <type name="leafref">
            <path value="/multicast-forward:multicast-forward/multicast-forward:nat/multicast-forward:instances/multicast-forward:instance[multicast-forward:instance-id=current()/../multicast-forward:instance-id]/multicast-forward:instance-name"/>
          </type>
          <description>
            <text>Name of a multicast NAT instance.</text>
          </description>
        </leaf>
      </list>
    </container>
  </augment>
</module>
