  <revision date="2014-12-12">
    <description>
      <text>Add PING netconf notification report function.</text>
    </description>
  </revision>
  <revision date="2014-05-06">
    <description>
      <text>Initial version.</text>
    </description>
  </revision>
  <feature name="vxlan"/>
  <feature name="srv6"/>
  <feature name="uni-nni-ping">
    <description>
      <text>UNI and NNI ping</text>
    </description>
  </feature>
  <feature name="uni-nni-oam-cfg">
    <description>
      <text>UNI and NNI OAM configuration</text>
    </description>
  </feature>
  <feature name="forward-ping-for-all">
    <description>
      <text>forward-ping configuration.</text>
    </description>
  </feature>
  <feature name="forward-ping-send-paras">
    <description>
      <text>forward-ping configuration for next-hop.</text>
    </description>
  </feature>
  <feature name="forward-ping-vlan">
    <description>
      <text>forward-ping configuration for vlan and label-stack-list.</text>
    </description>
  </feature>
  <container name="operation">
    <config value="false"/>
    <description>
      <text>PING operation model info.</text>
    </description>
    <container name="ping">
      <description>
        <text>PING operation parameters.</text>
      </description>
      <presence value="ping"/>
      <choice name="ip-type">
        <description>
          <text>The type of PING operation, IPV4 or IPV6.</text>
        </description>
        <mandatory value="true"/>
        <case name="ipv4">
          <description>
            <text>IPv4 PING operation parameters.</text>
          </description>
          <container name="ping4s">
            <description>
              <text>IPv4 PING operation parameters.</text>
            </description>
            <list name="ping4">
              <description>
                <text>IPv4 PING operation parameters.</text>
              </description>
              <key value="serial-num"/>
              <leaf name="serial-num">
                <description>
                  <text>IPv4 PING serial number.</text>
                </description>
                <type name="uint64"/>
              </leaf>
              <choice name="status">
                <description>
                  <text>The switch for turning on/off IPv4 PING.</text>
                </description>
                <mandatory value="true"/>
                <case name="enable">
                  <description>
                    <text>Start IPv4 PING function.</text>
                  </description>
                  <leaf name="ping-enable">
                    <description>
                      <text>Start IPv4 PING function.</text>
                    </description>
                    <type name="empty"/>
                    <mandatory value="true"/>
                  </leaf>
                  <container name="ping-data">
                    <description>
                      <text>Parameters for IPv4 PING.</text>
                    </description>
                    <choice name="ping4-type">
                      <description>
                        <text>The type of IPv4 PING.</text>
                      </description>
                      <mandatory value="true"/>
                      <case name="normal">
                        <description>
                          <text>Normal IP PING operation.</text>
                        </description>
                        <container name="normals">
                          <description>
                            <text>Normal IP PING operation.</text>
                          </description>
                          <choice name="dest-type">
                            <description>
                              <text>The type of normal PING.</text>
                            </description>
                            <mandatory value="true"/>
                            <case name="ip">
                              <description>
                                <text>The destination of PING is IP address.</text>
                              </description>
                              <leaf name="dst-ip">
                                <description>
                                  <text>The destination IPv4 address.</text>
                                </description>
                                <type name="ncd:ipv4-address"/>
                                <mandatory value="true"/>
                              </leaf>
                            </case>
                            <case name="domain">
                              <description>
                                <text>The destination of PING is domain name.</text>
                              </description>
                              <leaf name="domain-name">
                                <description>
                                  <text>The domain name.</text>
                                </description>
                                <type name="string">
                                  <length value="1..128">
                                    <description>
                                      <text>The domain name.</text>
                                    </description>
                                  </length>
                                </type>
                                <mandatory value="true"/>
                              </leaf>
                            </case>
                          </choice>
                        </container>
                        <container name="normal-option">
                          <description>
                            <text>Option parameters for normal PING.</text>
                          </description>
                          <choice name="vrf">
                            <description>
                              <text>The VRF type.</text>
                            </description>
                            <case name="normal-vrf">
                              <description>
                                <text>The normal VRF.</text>
                              </description>
                              <leaf name="vrf-name">
                                <description>
                                  <text>The VRF name.</text>
                                </description>
                                <type name="string">
                                  <length value="1..32">
                                    <description>
                                      <text>The VRF name.</text>
                                    </description>
                                  </length>
                                </type>
                              </leaf>
                            </case>
                            <case name="dcn-vrf">
                              <description>
                                <text>The DCN VRF.</text>
                              </description>
                              <leaf name="dcn">
                                <description>
                                  <text>Whether DCN VRF or not.</text>
                                </description>
                                <type name="empty"/>
                              </leaf>
                            </case>
                          </choice>
                          <leaf name="interface">
                            <description>
                              <text>The interface of sending packets.</text>
                            </description>
                            <type name="ncd:ifname"/>
                          </leaf>
                          <leaf name="pattern">
                            <description>
                              <text>The pad bytes.</text>
                            </description>
                            <type name="string">
                              <length value="4">
                                <description>
                                  <text>The pattern string.</text>
                                </description>
                              </length>
                            </type>
                          </leaf>
                          <leaf name="source">
                            <description>
                              <text>The source IP address of the packet.</text>
                            </description>
                            <type name="ncd:ipv4-address"/>
                          </leaf>
                          <container name="ip-option">
                            <description>
                              <text>The IP option of packet.</text>
                            </description>
                            <choice name="ip-options">
                              <description>
                                <text>IP options.</text>
                              </description>
                              <case name="source-route">
                                <description>
                                  <text>Source route option.</text>
                                </description>
                                <container name="route-option">
                                  <description>
                                    <text>Source route options.</text>
                                  </description>
                                  <leaf name="route-type">
                                    <description>
                                      <text>The source route type.</text>
                                    </description>
                                    <type name="enumeration">
                                      <enum name="loose">
                                        <value value="1"/>
                                        <description>
                                          <text>Loose source route.</text>
                                        </description>
                                      </enum>
                                      <enum name="strict">
                                        <value value="2"/>
                                        <description>
                                          <text>Strict source route.</text>
                                        </description>
                                      </enum>
                                    </type>
                                  </leaf>
                                  <list name="route-ips">
                                    <description>
                                      <text>Route IP.</text>
                                    </description>
                                    <key value="route-ip"/>
                                    <leaf name="route-ip">
                                      <description>
                                        <text>Route IP address.</text>
                                      </description>
                                      <type name="ncd:ipv4-address"/>
                                    </leaf>
                                  </list>
                                </container>
                              </case>
                              <case name="route-record">
                                <description>
                                  <text>Route record option.</text>
                                </description>
                                <leaf name="record-num">
                                  <description>
                                    <text>The number of recording route.</text>
                                  </description>
                                  <type name="uint8">
                                    <range value="1..9">
                                      <description>
                                        <text>The number of recording route.</text>
                                      </description>
                                    </range>
                                  </type>
                                </leaf>
                              </case>
                              <case name="timestamp">
                                <description>
                                  <text>Timestamp option.</text>
                                </description>
                                <leaf name="timestamp-num">
                                  <description>
                                    <text>The number of timestamp.</text>
                                  </description>
                                  <type name="uint8">
                                    <range value="1..9">
                                      <description>
                                        <text>The number of timestamp.</text>
                                      </description>
                                    </range>
                                  </type>
                                </leaf>
                              </case>
                            </choice>
                          </container>
                        </container>
                      </case>
                      <case name="uni">
                        <description>
                          <text>UNI-PING.</text>
                        </description>
                        <container name="unis">
                          <description>
                            <text>UNI-PING.</text>
                          </description>
                          <leaf name="dst-ip">
                            <description>
                              <text>The destination IPv4 address.</text>
                            </description>
                            <type name="ncd:ipv4-address"/>
                            <mandatory value="true"/>
                          </leaf>
                          <leaf name="source">
                            <description>
                              <text>The source IP address of the packet.</text>
                            </description>
                            <type name="ncd:ipv4-address"/>
                          </leaf>
                          <leaf name="vrf-name">
                            <description>
                              <text>The VRF name.</text>
                            </description>
                            <type name="string">
                              <length value="1..32">
                                <description>
                                  <text>The VRF name.</text>
                                </description>
                              </length>
                            </type>
                            <ncd:revise-date date="2016-06-29"/>
                          </leaf>
                          <leaf name="pattern">
                            <description>
                              <text>The pad bytes.</text>
                            </description>
                            <type name="string">
                              <length value="4">
                                <description>
                                  <text>The pattern string.</text>
                                </description>
                              </length>
                            </type>
                            <ncd:revise-date date="2019-07-10"/>
                          </leaf>
                        </container>
                      </case>
                      <case name="nni">
                        <description>
                          <text>NNI-PING.</text>
                        </description>
                        <container name="nnis">
                          <description>
                            <text>NNI-PING.</text>
                          </description>
                          <leaf name="dst-ip">
                            <description>
                              <text>The destination IPv4 address.</text>
                            </description>
                            <type name="ncd:ipv4-address"/>
                            <mandatory value="true"/>
                          </leaf>
                          <leaf name="next-hop">
                            <description>
                              <text>The next hop address for NNI-PING.</text>
                            </description>
                            <type name="ncd:ipv4-address"/>
                            <mandatory value="true"/>
                          </leaf>
                          <leaf name="vrf-name">
                            <description>
                              <text>The VRF name.</text>
                            </description>
                            <type name="string">
                              <length value="1..32">
                                <description>
                                  <text>The VRF name.</text>
                                </description>
                              </length>
                            </type>
                            <ncd:revise-date date="2016-06-29"/>
                          </leaf>
                          <leaf name="pattern">
                            <description>
                              <text>The pad bytes.</text>
                            </description>
                            <type name="string">
                              <length value="4">
                                <description>
                                  <text>The pattern string.</text>
                                </description>
                              </length>
                            </type>
                            <ncd:revise-date date="2019-07-10"/>
                          </leaf>
                        </container>
                      </case>
                      <case name="vxlan">
                        <description>
                          <text>VXLAN-PING.</text>
                        </description>
                        <container name="vxlans">
                          <description>
                            <text>VXLAN PING.</text>
                          </description>
                          <leaf name="dst-ip">
                            <description>
                              <text>The destination IPv4 address.</text>
                            </description>
                            <type name="ncd:ipv4-address"/>
                            <mandatory value="true"/>
                          </leaf>
                          <leaf name="vrf-name">
                            <description>
                              <text>The VRF name.</text>
                            </description>
                            <type name="string">
                              <length value="1..32">
                                <description>
                                  <text>The VRF name.</text>
                                </description>
                              </length>
                            </type>
                            <mandatory value="true"/>
                          </leaf>
                        </container>
                        <container name="vxlan-option">
                          <description>
                            <text>Optional parameters of VXLAN PING.</text>
                          </description>
                          <leaf name="source">
                            <description>
                              <text>The source IP address of the packet.</text>
                            </description>
                            <type name="ncd:ipv4-address"/>
                          </leaf>
                          <choice name="underlay">
                            <description>
                              <text>underlay VXLAN information.</text>
                            </description>
                            <case name="dynamic">
                              <description>
                                <text>Dynamic VXLAN Tunnel.</text>
                              </description>
                              <leaf name="local-ip">
                                <description>
                                  <text>The local IPv4 address.</text>
                                </description>
                                <type name="ncd:ipv4-address"/>
                                <mandatory value="true"/>
                              </leaf>
                              <leaf name="remote-ip">
                                <description>
                                  <text>The remote IPv4 address.</text>
                                </description>
                                <type name="ncd:ipv4-address"/>
                                <mandatory value="true"/>
                              </leaf>
                            </case>
                            <case name="static">
                              <description>
                                <text>Static VXLAN Tunnel.</text>
                              </description>
                              <leaf name="vxlan-interface">
                                <description>
                                  <text>The VXLAN interface of sending packets.</text>
                                </description>
                                <type name="ncd:ifname"/>
                              </leaf>
                            </case>
                            <case name="ipv6-dynamic">
                              <description>
                                <text>Dynamic VXLAN IP address.</text>
                              </description>
                              <container name="ipv6-dynamics">
                                <description>
                                  <text>Dynamic VXLAN IP address.</text>
                                </description>
                                <leaf name="ipv6-remote">
                                  <description>
                                    <text>The remote IPv6 address.</text>
                                  </description>
                                  <type name="ncd:ipv6-address"/>
                                  <mandatory value="true"/>
                                </leaf>
                                <leaf name="ipv6-local">
                                  <description>
                                    <text>The local IPv6 address.</text>
                                  </description>
                                  <type name="ncd:ipv6-address"/>
                                  <mandatory value="true"/>
                                </leaf>
                              </container>
                              <ncd:revise-date date="2020-04-13"/>
                            </case>
                          </choice>
                          <leaf name="pattern">
                            <description>
                              <text>The pad bytes.</text>
                            </description>
                            <type name="string">
                              <length value="4">
                                <description>
                                  <text>The pattern string.</text>
                                </description>
                              </length>
                            </type>
                            <ncd:revise-date date="2020-04-13"/>
                          </leaf>
                        </container>
                        <ncd:revise-date date="2019-08-30"/>
                      </case>
                    </choice>
                  </container>
                  <container name="common-option">
                    <description>
                      <text>Option parameters for IPv4 PING.</text>
                    </description>
                    <choice name="speed">
                      <description>
                        <text>The mode of sending packets.</text>
                      </description>
                      <case name="limit-mode">
                        <description>
                          <text>The mode which controls how many packets sent in a second.</text>
                        </description>
                        <leaf name="limit">
                          <description>
                            <text>The number of sending packets in a second, 0 means no limit.</text>
                          </description>
                          <type name="uint8">
                            <range value="0..100">
                              <description>
                                <text>The number of sending packets in a second, 0 means no limit.</text>
                              </description>
                            </range>
                          </type>
                        </leaf>
                      </case>
                      <case name="interval-mode">
                        <description>
                          <text>The mode which controls how long between two sending packets.</text>
                        </description>
                        <leaf name="interval">
                          <description>
                            <text>The interval between two sending packets.</text>
                          </description>
                          <type name="uint8">
                            <range value="2..10">
                              <description>
                                <text>The interval between two sending packets.</text>
                              </description>
                            </range>
                          </type>
                          <units name="s"/>
                        </leaf>
                      </case>
                    </choice>
                    <leaf name="df-bit">
                      <description>
                        <text>Do not frag bit flag.</text>
                      </description>
                      <type name="uint8">
                        <range value="0..1">
                          <description>
                            <text>Do not frag bit flag.</text>
                          </description>
                        </range>
                      </type>
                    </leaf>
                    <leaf name="repeat">
                      <description>
                        <text>The repeat times of sending packets.</text>
                      </description>
                      <type name="uint16">
                        <range value="1..65535">
                          <description>
                            <text>The repeat times of sending packets.</text>
                          </description>
                        </range>
                      </type>
                      <default value="5"/>
                    </leaf>
                    <leaf name="size">
                      <description>
                        <text>The packet size, include IP header.</text>
                      </description>
                      <type name="uint16">
                        <range value="36..65507">
                          <description>
                            <text>The packet size, include IP header.</text>
                          </description>
                        </range>
                      </type>
                      <units name="bytes"/>
                      <default value="100"/>
                    </leaf>
                    <leaf name="timeout">
                      <description>
                        <text>Timeout.</text>
                      </description>
                      <type name="uint8">
                        <range value="1..60">
                          <description>
                            <text>Timeout.</text>
                          </description>
                        </range>
                      </type>
                      <units name="s"/>
                      <default value="2"/>
                    </leaf>
                    <leaf name="ttl">
                      <description>
                        <text>Time to live.</text>
                      </description>
                      <type name="uint8">
                        <range value="1..255">
                          <description>
                            <text>Time to live.</text>
                          </description>
                        </range>
                      </type>
                    </leaf>
                    <choice name="pkt-priority">
                      <description>
                        <text>The priority of sending packets.</text>
