  <grouping name="ipv4-adj-rib-common">
    <description>
      <text>Common structural grouping for each IPv4 adj-RIB table</text>
    </description>
    <uses name="bgp-common-table-attrs-top"/>
    <container name="routes">
      <config value="false"/>
      <description>
        <text>Enclosing container for list of routes in the routing
table.</text>
      </description>
      <list name="route">
        <key value="prefix path-id"/>
        <description>
          <text>List of routes in the table, keyed by a combination of
the route prefix and path-id to distinguish multiple
routes received from a neighbor for the same prefix,
e.g., when BGP add-paths is enabled.</text>
        </description>
        <uses name="bgp-adj-rib-key-refs"/>
        <container name="state">
          <description>
            <text>Operational state data for BGP Adj-RIB entries</text>
          </description>
          <leaf name="prefix">
            <type name="oc-inet:ipv4-prefix"/>
            <description>
              <text>Prefix for the route</text>
            </description>
          </leaf>
          <uses name="bgp-adj-rib-attr-state"/>
          <uses name="bgp-adj-rib-common-attr-refs"/>
          <uses name="bgp-common-route-annotations-state"/>
        </container>
        <uses name="bgp-unknown-attr-top"/>
      </list>
    </container>
  </grouping>
