                <path value="/qos:qos/qos:queue-scheduler-template/qos:user-group-queues/qos:user-group-queue/qos:name"/>
              </type>
              <must condition="/qos:qos/qos:queue-scheduler-template/qos:user-group-queues/qos:user-group-queue[qos:name=current()]/qos:mode='normal'"/>
              <description>
                <text>User-group-queue name for outbound.</text>
              </description>
            </leaf>
          </container>
          <container name="other-function-sets">
            <description>
              <text>Configure other function.</text>
            </description>
            <leaf name="alarm-enable">
              <type name="boolean"/>
              <default value="true"/>
              <description>
                <text>Enable/disable tunnel alarm.</text>
              </description>
            </leaf>
            <leaf name="avp-nas-port-enable">
              <type name="empty"/>
              <description>
                <text>Enable the L2TP attribute AVP100.</text>
              </description>
            </leaf>
            <leaf name="avp46-enable">
              <type name="empty"/>
              <description>
                <text>Avp46 enable.</text>
              </description>
            </leaf>
            <leaf name="avp-cncrypte-enable">
              <type name="empty"/>
              <description>
                <text>Whether AVP data is transmitted in encrypted text.</text>
              </description>
            </leaf>
            <leaf name="peer-hello-check-enable">
              <type name="empty"/>
              <description>
                <text>Peer hello check.</text>
              </description>
            </leaf>
          </container>
        </container>
        <container name="l2tp-lac">
          <description>
            <text>Configure LAC service.</text>
          </description>
          <container name="tunnel-mode">
            <description>
              <text>Configure tunnel load sharing mode.</text>
            </description>
            <choice name="mode">
              <description>
                <text>Mode.</text>
              </description>
              <case name="load-share">
                <description>
                  <text>Load-share.</text>
                </description>
                <leaf name="load-share-enable">
                  <type name="empty"/>
                  <description>
                    <text>Tunnel load Share enable.</text>
                  </description>
                </leaf>
              </case>
              <case name="priority">
                <description>
                  <text>The tunnel is divided according to the priority load.</text>
                </description>
                <leaf name="priority-enable">
                  <type name="empty"/>
                  <description>
                    <text>Tunnel priority enable.</text>
                  </description>
                </leaf>
              </case>
              <case name="per-user">
                <description>
                  <text>Per-user.</text>
                </description>
                <leaf name="per-user-enable">
                  <type name="empty"/>
                  <description>
                    <text>Tunnel per user enable.</text>
                  </description>
                </leaf>
              </case>
            </choice>
          </container>
          <container name="source-interface">
            <presence value="Tunnel bind source interface."/>
            <description>
              <text>Enable/disable tunnel bind source interface.</text>
            </description>
            <leaf name="name">
              <type name="leafref">
                <path value="/ifm:ifm/ifm:interfaces/ifm:interface/ifm:name"/>
              </type>
              <must condition="/ifm:ifm/ifm:interfaces/ifm:interface[ifm:name=current()]/ifm:type='Ethernet' or&#10;/ifm:ifm/ifm:interfaces/ifm:interface[ifm:name=current()]/ifm:type='GigabitEthernet' or&#10;/ifm:ifm/ifm:interfaces/ifm:interface[ifm:name=current()]/ifm:type='Eth-Trunk' or&#10;/ifm:ifm/ifm:interfaces/ifm:interface[ifm:name=current()]/ifm:type='10GE' or&#10;/ifm:ifm/ifm:interfaces/ifm:interface[ifm:name=current()]/ifm:type='LoopBack' or&#10;/ifm:ifm/ifm:interfaces/ifm:interface[ifm:name=current()]/ifm:type='XGigabitEthernet' or&#10;/ifm:ifm/ifm:interfaces/ifm:interface[ifm:name=current()]/ifm:type='100GE' or&#10;/ifm:ifm/ifm:interfaces/ifm:interface[ifm:name=current()]/ifm:type='50GE' or&#10;/ifm:ifm/ifm:interfaces/ifm:interface[ifm:name=current()]/ifm:type='50|100GE' or&#10;/ifm:ifm/ifm:interfaces/ifm:interface[ifm:name=current()]/ifm:type='FlexE' or&#10;/ifm:ifm/ifm:interfaces/ifm:interface[ifm:name=current()]/ifm:type='25GE' or&#10;/ifm:ifm/ifm:interfaces/ifm:interface[ifm:name=current()]/ifm:type='Pos' or&#10;/ifm:ifm/ifm:interfaces/ifm:interface[ifm:name=current()]/ifm:type='Ip-Trunk' or&#10;/ifm:ifm/ifm:interfaces/ifm:interface[ifm:name=current()]/ifm:type='ATM' or&#10;/ifm:ifm/ifm:interfaces/ifm:interface[ifm:name=current()]/ifm:type='MEth'"/>
              <mandatory value="true"/>
              <description>
                <text>Interface name.</text>
              </description>
            </leaf>
            <leaf name="rui-enable">
              <type name="boolean"/>
              <must condition="../rui-enable='false' or (../rui-enable='true' and /ifm:ifm/ifm:interfaces/ifm:interface[ifm:name=current()/../name]/ifm:type='LoopBack')"/>
              <default value="false"/>
              <description>
                <text>Enable/disable backup user, can only be configured on loopBack interface.</text>
              </description>
            </leaf>
          </container>
          <container name="start-l2tp">
            <must condition="not(../../l2tp-lns/allow/virtual-template-number)"/>
            <presence value="Start l2tp enable."/>
            <description>
              <text>Enable/disable start l2tp.</text>
            </description>
            <container name="lns-ips">
              <description>
                <text>List of L2tp start IPs.</text>
              </description>
              <list name="lns-ip">
                <key value="identifier-name"/>
                <max-elements value="8"/>
                <description>
                  <text>Configure L2tp start IP.</text>
                </description>
                <leaf name="identifier-name">
                  <type name="string">
                    <length value="1..4"/>
                  </type>
                  <description>
                    <text>Identifier name.</text>
                  </description>
                </leaf>
                <leaf name="ip-address">
                  <type name="inet:ipv4-address-no-zone"/>
                  <mandatory value="true"/>
                  <description>
                    <text>Ip address.</text>
                  </description>
                </leaf>
                <leaf name="weight">
                  <type name="uint32">
                    <range value="1..10"/>
                  </type>
                  <default value="5"/>
                  <description>
                    <text>Weight.</text>
                  </description>
                </leaf>
                <leaf name="preference">
                  <type name="uint32">
                    <range value="1..255"/>
                  </type>
                  <default value="1"/>
                  <description>
                    <text>Preference.</text>
                  </description>
                </leaf>
                <leaf name="remote-name">
                  <type name="string">
                    <length value="1..253"/>
                  </type>
                  <description>
                    <text>Remote name.</text>
                  </description>
                </leaf>
              </list>
            </container>
          </container>
          <container name="local-name-lns-ips">
            <description>
              <text>List of specify configuration information of the L2TP tunnel.</text>
            </description>
            <list name="local-name-lns-ip">
              <key value="ip-address"/>
              <max-elements value="8"/>
              <description>
                <text>Configure specify information of the L2TP tunnel.</text>
              </description>
              <leaf name="ip-address">
                <type name="inet:ipv4-address-no-zone"/>
                <description>
                  <text>Sets specify lns ip address.</text>
                </description>
              </leaf>
              <leaf name="local-name">
                <type name="string">
                  <length value="1..253"/>
                </type>
                <mandatory value="true"/>
                <description>
                  <text>Sets specify local name of the L2TP tunnel.</text>
                </description>
              </leaf>
            </list>
          </container>
          <container name="block-lns-ip">
            <when condition="../tunnel-mode/priority-enable"/>
            <description>
              <text>Configure block L2TP LNS IP address.</text>
            </description>
            <leaf-list name="ip-address">
              <type name="inet:ipv4-address-no-zone"/>
              <description>
                <text>Configure block L2TP LNS IP address.</text>
              </description>
            </leaf-list>
          </container>
          <container name="password-lns-ips">
            <description>
              <text>List of lns tunnel password.</text>
            </description>
            <list name="password-lns-ip">
              <key value="ip-address"/>
              <max-elements value="8"/>
              <description>
                <text>Configure tunnel password and lns-ip.</text>
              </description>
              <leaf name="ip-address">
                <type name="inet:ipv4-address-no-zone"/>
                <description>
                  <text>Sets lns ip address.</text>
                </description>
              </leaf>
              <leaf name="password">
                <type name="pub-type:password-extend">
                  <length value="1..408"/>
                </type>
                <mandatory value="true"/>
                <description>
                  <text>The password is a string ranging from 1 to 255 characters for a plaintext password and 48 to 408 characters for a ciphertext password.</text>
                </description>
              </leaf>
            </list>
          </container>
          <container name="avp-calling-number">
            <description>
              <text>Configure avp alling-number parameters.</text>
            </description>
            <container name="avp-enable">
              <description>
                <text>Configure avp alling-number.</text>
              </description>
              <leaf name="status">
                <type name="enumeration">
                  <enum name="enable">
                    <value value="1"/>
                    <description>
                      <text>Enable.</text>
                    </description>
                  </enum>
                  <enum name="disable">
                    <value value="2"/>
                    <description>
                      <text>Disable.</text>
                    </description>
                  </enum>
                </type>
                <description>
                  <text>Avp alling-number enable.</text>
                </description>
              </leaf>
            </container>
            <container name="calling-number-format">
              <description>
                <text>Configure number format.</text>
              </description>
              <choice name="module">
                <description>
                  <text>Module.</text>
                </description>
                <case name="l2tp">
                  <description>
                    <text>L2TP.</text>
                  </description>
                  <container name="l2tp-format">
                    <description>
                      <text>Configure number format from l2tp.</text>
                    </description>
                    <choice name="format">
                      <description>
                        <text>Format.</text>
                      </description>
                      <case name="version1">
                        <description>
                          <text>Version1.</text>
                        </description>
                        <leaf name="version1-enable">
                          <type name="empty"/>
                          <description>
                            <text>Version1 Enable.</text>
                          </description>
                        </leaf>
                        <leaf name="include-option82">
                          <when condition="../version1-enable"/>
                          <type name="empty"/>
                          <description>
                            <text>Include option82.</text>
                          </description>
                        </leaf>
                      </case>
                      <case name="version2">
                        <description>
                          <text>Version2.</text>
                        </description>
                        <container name="include-attributes">
                          <description>
                            <text>List of the user-defined format of the attributes.</text>
                          </description>
                          <list name="include-attribute">
                            <key value="attribute-type"/>
                            <max-elements value="10"/>
                            <ordered-by value="user"/>
                            <description>
                              <text>Configure the user-defined format of the attribute.</text>
                            </description>
                            <leaf name="attribute-type">
                              <type name="enumeration">
                                <enum name="option82">
                                  <value value="1"/>
                                  <description>
                                    <text>Option82.</text>
                                  </description>
                                </enum>
                                <enum name="user-mac">
                                  <value value="2"/>
                                  <description>
                                    <text>User mac address.</text>
                                  </description>
                                </enum>
                                <enum name="interface">
                                  <value value="3"/>
                                  <description>
                                    <text>Interface.</text>
                                  </description>
                                </enum>
                                <enum name="domain">
                                  <value value="4"/>
                                  <description>
                                    <text>Domain.</text>
                                  </description>
                                </enum>
                                <enum name="system-name">
                                  <value value="5"/>
                                  <description>
                                    <text>System name.</text>
                                  </description>
                                </enum>
                                <enum name="vlan">
                                  <value value="6"/>
                                  <description>
                                    <text>VLAN.</text>
                                  </description>
                                </enum>
                                <enum name="agent-circuit-id">
                                  <value value="7"/>
                                  <description>
                                    <text>Agent circuit id.</text>
                                  </description>
                                </enum>
                                <enum name="agent-remote-id">
                                  <value value="8"/>
                                  <description>
                                    <text>Agent remote id.</text>
                                  </description>
                                </enum>
                                <enum name="pe-vlan">
                                  <value value="9"/>
                                  <description>
                                    <text>Pe-VLAN.</text>
                                  </description>
                                </enum>
                                <enum name="ce-vlan">
                                  <value value="10"/>
                                  <description>
                                    <text>Ce-VLAN.</text>
                                  </description>
                                </enum>
                              </type>
                              <description>
                                <text>Configures the user-defined format of the nas-port-id attribute type.</text>
                              </description>
                            </leaf>
                            <leaf name="delimiter">
                              <type name="string">
                                <length value="1"/>
                                <pattern value="[b\*\-\\#$@&amp;:]"/>
                              </type>
                              <description>
                                <text>Configures the user-defined format of the attribute delimiter one.</text>
                              </description>
                            </leaf>
                          </list>
                        </container>
                        <leaf name="include-delimiter">
                          <type name="string">
                            <length value="1"/>
                            <pattern value="[b\*\-\\#$@&amp;:]"/>
                          </type>
                          <must condition="count(../include-attributes/include-attribute) &gt;= 1"/>
                          <description>
                            <text>The value of the includeDelimiter field must be one of the following values: b, asterisk, hyphen, backslash, number sign, dollar sign, at sign, ampersand and colon.</text>
                          </description>
                        </leaf>
                      </case>
                      <case name="version3">
                        <description>
                          <text>Version3 format.</text>
                        </description>
                        <leaf name="version3-enable">
                          <type name="empty"/>
                          <description>
                            <text>Version3 enable.</text>
                          </description>
                        </leaf>
                      </case>
                    </choice>
                  </container>
                </case>
                <case name="aaa">
                  <description>
                    <text>AAA.</text>
                  </description>
                  <container name="aaa-format">
                    <description>
                      <text>Configure number format from aaa.</text>
                    </description>
                    <choice name="format">
                      <description>
                        <text>Format.</text>
                      </description>
                      <case name="lli">
                        <description>
                          <text>Lli.</text>
                        </description>
                        <leaf name="logical-line-id">
                          <type name="empty"/>
                          <description>
                            <text>Enable Logical line ID.</text>
                          </description>
                        </leaf>
                      </case>
                      <case name="radius-force">
                        <description>
                          <text>Radius force.</text>
                        </description>
                        <leaf name="radius-force">
                          <type name="empty"/>
                          <description>
                            <text>Enable the calling-station-id function delivered by the RADIUS server on the LAC side.</text>
                          </description>
                        </leaf>
                      </case>
                    </choice>
                  </container>
                </case>
              </choice>
              <container name="interface-exclude-subslot">
                <description>
                  <text>Configure interface exclude subslot.</text>
                </description>
                <leaf name="exclude-enable">
                  <type name="boolean"/>
                  <default value="false"/>
                  <description>
                    <text>Enable/disable L2TP AVP configuration information about subslot.</text>
                  </description>
                </leaf>
              </container>
            </container>
          </container>
          <container name="other-function-sets">
            <description>
              <text>Configure other function.</text>
            </description>
            <leaf name="session-limit">
              <type name="uint32">
                <range value="0..65535"/>
              </type>
              <default value="65535"/>
              <description>
                <text>Session limit of the tunnel.</text>
              </description>
            </leaf>
            <leaf name="mtu-enable">
              <type name="boolean"/>
              <default value="true"/>
              <description>
                <text>Enable/disable mtu.</text>
              </description>
            </leaf>
            <leaf name="mss-enable">
              <type name="boolean"/>
              <default value="true"/>
              <description>
                <text>Enable/disable mss.</text>
              </description>
            </leaf>
            <leaf name="allow-address-change-type">
              <type name="enumeration">
                <enum name="setup-only">
                  <value value="1"/>
                  <description>
                    <text>Setup only.</text>
                  </description>
                </enum>
                <enum name="always">
                  <value value="2"/>
                  <description>
                    <text>Always.</text>
                  </description>
                </enum>
              </type>
              <description>
                <text>Allow address change type.</text>
              </description>
            </leaf>
            <leaf name="trust-upstream-priority">
              <type name="boolean"/>
              <default value="false"/>
              <description>
                <text>Enable/disable lac packet priority.</text>
              </description>
            </leaf>
            <leaf name="sccrq-time">
              <type name="uint32">
                <range value="1..1000"/>
              </type>
              <units name="ms"/>
              <description>
                <text>Send sccrq delay time.</text>
              </description>
            </leaf>
            <leaf name="dscp-value">
              <type name="uint32">
                <range value="0..63"/>
              </type>
              <description>
                <text>Lac dscp value.</text>
              </description>
            </leaf>
          </container>
          <container name="cp-source-interfaces">
            <description>
              <text>List of LAC group bind source interface.</text>
            </description>
            <list name="cp-source-interface">
              <must condition="up-id-end &gt;= up-id-begin"/>
              <key value="up-id-begin up-id-end"/>
              <description>
                <text>Configure LAC group bind source interface.</text>
              </description>
              <leaf name="up-id-begin">
                <type name="uint32">
                  <range value="1024..2023 | 2048..3047 | 3072..4071 | 4096..5095"/>
                </type>
                <must condition="../up-id-begin &lt;= (/bras-cu-controller:bras-cu-controller/bras-cu-controller:global-attribute/bras-cu-controller:up-base-id + 999) and ../up-id-begin &gt;= /bras-cu-controller:bras-cu-controller/bras-cu-controller:global-attribute/bras-cu-controller:up-base-id"/>
                <description>
                  <text>Begin up id.</text>
                </description>
              </leaf>
              <leaf name="up-id-end">
                <type name="uint32">
                  <range value="1024..2023 | 2048..3047 | 3072..4071 | 4096..5095"/>
                </type>
                <must condition="../up-id-end &lt;= (/bras-cu-controller:bras-cu-controller/bras-cu-controller:global-attribute/bras-cu-controller:up-base-id + 999) and ../up-id-end &gt;= /bras-cu-controller:bras-cu-controller/bras-cu-controller:global-attribute/bras-cu-controller:up-base-id"/>
                <description>
                  <text>End up id.</text>
                </description>
              </leaf>
              <leaf name="loopback-number">
                <type name="uint32">
                  <range value="0..2147483647"/>
                </type>
                <default value="0"/>
                <description>
                  <text>Loopback number.</text>
                </description>
              </leaf>
            </list>
          </container>
        </container>
        <container name="l2tp-lns">
          <description>
            <text>Configure LNS service.</text>
          </description>
          <container name="allow">
            <must condition="not(../../l2tp-lac/start-l2tp) and (../../name != 'default-lac')"/>
            <must condition="(../../name != 'default-lns' and ./remote-name) or ../../name = 'default-lns'"/>
            <presence value="Allow l2tp virtual-template."/>
            <description>
              <text>Enable/disable specifies the channel peer name and virtual template used by this L2TP group.</text>
            </description>
            <leaf name="virtual-template-number">
              <type name="leafref">
                <path value="/ifm:ifm/ifm:interfaces/ifm:interface/ifm:number"/>
              </type>
              <must condition="/ifm:ifm/ifm:interfaces/ifm:interface[ifm:number=current()]/ifm:type='Virtual-Template'"/>
              <mandatory value="true"/>
              <description>
                <text>Virtual template number.</text>
              </description>
            </leaf>
            <leaf name="remote-name">
              <type name="string">
                <length value="1..253"/>
              </type>
              <description>
                <text>Remote name.</text>
              </description>
            </leaf>
            <leaf name="lac-source-ip-address">
              <type name="inet:ipv4-address-no-zone"/>
              <description>
                <text>Lac source IP address.</text>
              </description>
            </leaf>
          </container>
          <container name="domain">
            <description>
              <text>Configure LNS Service.</text>
            </description>
            <leaf name="set-type">
              <type name="enumeration">
                <enum name="default">
                  <value value="0"/>
                  <description>
                    <text>Default.</text>
                  </description>
                </enum>
                <enum name="force">
                  <value value="1"/>
                  <description>
                    <text>Force.</text>
                  </description>
                </enum>
                <enum name="replace">
                  <value value="2"/>
                  <description>
                    <text>Replace.</text>
                  </description>
                </enum>
              </type>
              <description>
                <text>Force authentication enable.</text>
              </description>
            </leaf>
            <leaf name="name">
              <when condition="../set-type"/>
              <type name="leafref">
                <path value="/aaa:aaa/aaa:domains/aaa:domain/aaa:name"/>
              </type>
              <mandatory value="true"/>
              <description>
                <text>LNS authen domain name.</text>
              </description>
            </leaf>
            <leaf name="roam-name">
              <type name="leafref">
                <path value="/aaa:aaa/aaa:domains/aaa:domain/aaa:name"/>
              </type>
              <description>
                <text>LNS roam domain name.</text>
              </description>
            </leaf>
          </container>
          <container name="calling-station-id">
            <description>
              <text>Configure the L2TP attribute parameters.</text>
            </description>
            <leaf name="agent-remote-id">
              <type name="boolean"/>
              <default value="false"/>
              <description>
                <text>Enable/disable format calling-station-id.</text>
              </description>
            </leaf>
            <leaf name="translate-remote-id">
              <type name="boolean"/>
              <default value="false"/>
              <description>
                <text>Enable/disable attribute translate.</text>
              </description>
            </leaf>
          </container>
          <container name="other-function-sets">
            <description>
              <text>Configure other function.</text>
            </description>
            <leaf name="cdn-send-delay-enable">
              <type name="empty"/>
              <description>
                <text>Cdn send delay enable.</text>
              </description>
            </leaf>
            <leaf name="renegotiation-lcp-enable">
              <type name="empty"/>
              <description>
                <text>Renegotiation lcp enable.</text>
              </description>
            </leaf>
            <leaf name="lns-schedule-enable">
              <type name="boolean"/>
              <default value="false"/>
              <description>
                <text>Enable/disable lns schedule.</text>
              </description>
            </leaf>
            <leaf name="mandatory-chap">
              <type name="enumeration">
                <enum name="mandatory-chap">
                  <value value="1"/>
                  <description>
                    <text>Mandatory chap.</text>
                  </description>
                </enum>
                <enum name="challenge-length">
                  <value value="2"/>
                  <description>
                    <text>Challenge length.</text>
                  </description>
                </enum>
              </type>
              <description>
                <text>Lns chap type.</text>
              </description>
            </leaf>
            <leaf name="mandatory-lcp">
              <type name="enumeration">
                <enum name="mandatory-lcp">
                  <value value="1"/>
                  <description>
                    <text>Mandatory lcp.</text>
                  </description>
                </enum>
                <enum name="on-mismatch">
                  <value value="2"/>
                  <description>
                    <text>On mismatch.</text>
                  </description>
                </enum>
                <enum name="strict">
                  <value value="3"/>
                  <description>
                    <text>Strict.</text>
                  </description>
                </enum>
              </type>
              <description>
                <text>LnsMndtLcpFlg.</text>
              </description>
            </leaf>
            <leaf name="qos-mode">
              <type name="enumeration">
                <enum name="tunnel">
                  <value value="1"/>
                  <description>
