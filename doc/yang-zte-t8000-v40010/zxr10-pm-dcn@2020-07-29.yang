  }
  revision 2020-02-12 {
    description 'Add container "if-neighbor-qx"'
                ;
  }
  revision 2019-12-30 {
    description 'Add container "dcn-route-redistributes"'
                ;
  }
  revision 2019-12-18 {
    description 'Modify rang of "process-id" in "if-neighbor-l3-config"'
                ;
  }
  revision 2019-11-07 {
    description 'Modify type of "target-ip-address" in container "dcn-ping-dete'
              + 'ct"'+"\n"
              + 'Modify type of "target-ip-address" in container "query-dcn-pin'
              + 'g-detect"'
                ;
  }
  revision 2019-09-02 {
    description 'Add "dcn-mngipv6"'+"\n"
              + 'Add "dcn-ospf-notify"'+"\n"
              + 'Add "query-dcn-vlan-ifs"'+"\n"
              + 'Modify "dcn-ping-detect"'+"\n"
              + 'Modify "query-dcn-ping-detects"'
                ;
  }
  revision 2019-08-08 {
    description 'Add  IPV6 function status'+"\n"
              + 'Add  mng OSPFv3 area'+"\n"
              + 'Add  ipv6 static route'+"\n"
              + 'Add  if ipv6 l3'
                ;
  }
  revision 2019-07-02 {
    description 'Add if-neighbor-l2-configs.
Add if-neighbor-l3-configs.'
                ;
  }
  revision 2019-05-17 {
    description 'Add query L2 mac.'
                ;
  }
  revision 2019-05-13 {
    description 'add vlanid opt-type, modefiy Ospf authentication key, add qx m'
              + 'ac.'+"\n"
              + 'add leaf "bangdwidth-mb".Change leaf "key-delay" rang to 0-100'
              + '000.'
                ;
  }
  revision 2019-04-26 {
    description 'Change leaf "port" rang to 1-255.'
                ;
  }
  revision 2019-04-23 {
    description 'add mac addr'
                ;
  }
  revision 2019-03-08 {
    description 'Add leaf dcn-port-work-mode for 61.'
                ;
  }
  revision 2019-02-28 {
    description 'Add container dcn-neighbor-ifs.'
                ;
  }
  revision 2019-01-29 {
    description 'Add ip-route enum edit for 61 and 67.'
                ;
  }
  revision 2019-01-17 {
    description 'shelf range 0-255.slot range 0-255.'
                ;
  }
  revision 2018-10-26 {
    description 'Add topo contains and leafs for 67.'
                ;
  }
  revision 2018-06-06 {
    description 'Add DCN mode eth-p2mp-auto, l3 port mode, query l3 port mode.'
                ;
  }
  revision 2017-11-28 {
    description 'Add leaf neighbor-dcn-mode and contain neighbor-route-aggrs ad'
              + 'd contain neighbor-ospf-stubs.'
                ;
  }
  revision 2017-05-05 {
    description 'Add contain if-neighbor-dcn-mode;'+"\n"
              + 'Add contain if-neighbor-ospf-area;'+"\n"
              + 'Add contain if-neighbor-ospf-area-stub'
                ;
  }
  revision 2015-10-22 {
    description 'add contain dcn-mng-ospf-area and contain query-dcn-mng-ospf-a'
              + 'rea'
                ;
  }
  revision 2015-06-29 ;
  container operation {
    config false ;
    container dcn {
      description 'Configure DCN'
                  ;
      container dcn-network-mode {
        description 'Configure dcn network mode'
                    ;
        leaf network-mode {
          description 'Configure dcn network mode'
                      ;
          type enumeration {
            enum eth-p2p {
              value 1 ;
              description 'Configure dcn mode to eth-p2p'
                          ;
            }
            enum eth-p2p-auto {
              value 3 ;
              description 'Configure dcn mode to auto eth-p2p'
                          ;
            }
            enum pppoe-auto {
              value 4 ;
              description 'Configure dcn mode to auto pppoe'
                          ;
            }
            enum eth-p2mp-auto {
              value 5 ;
              description 'Configure dcn mode to auto eth-p2mp'
                          ;
            }
          }
        }
      }
      container dcn-function-status {
        description 'Enable or disable DCN function'
                    ;
        leaf function-status {
          description 'Enable or disable DCN function'
                      ;
          type enumeration {
            enum enable {
              value 0 ;
              description 'DCN enable'
                          ;
            }
            enum disable {
              value 1 ;
              description 'DCN disable'
                          ;
            }
          }
        }
      }
      container dcn-ipv6-function-status {
        description 'Enable or disable dcn IPv6 function'
                    ;
        leaf ipv6-function-status {
          description 'Enable or disable dcn IPv6 function'
                      ;
          type enumeration {
            enum ipv6-enable {
              value 1 ;
              description 'DCN IPV6 enable'
                          ;
            }
            enum ipv6-disable {
              value 0 ;
              description 'DCN IPV6 disable'
                          ;
            }
          }
        }
        ncd:revise-date 2019-08-08 ;
      }
      container dcn-mngip {
        description 'Configure DCN network Mng IP address'
                    ;
        leaf ip-address {
          description 'IPv4 address'
                      ;
          type ncd:ipv4-address ;
        }
        leaf ip-mask {
          description 'IPv4 address mask'
                      ;
          type ncd:ipv4-mask ;
        }
      }
      container dcn-mngipv6 {
        description 'Configure DCN network Mng IPv6 address'
                    ;
        leaf ipv6-address {
          description 'IPv6 address'
                      ;
          type ncd:ipv6-address ;
        }
        leaf prefix-length {
          description 'IPv6 prefix lenth'
                      ;
          type ncd:ipv6-mask-length ;
        }
        ncd:revise-date 2019-09-02 ;
      }
      container dcn-ospf-area {
        description 'Configure OSPFv2 area route aggregation'
                    ;
        choice area-id {
          description 'OSPFv2 area'
                      ;
          case number-format {
            description 'OSPFv2 area in number format'
                        ;
            leaf number {
              description 'OSPFv2 area in number format'
                          ;
              type uint32 {
                range 0..4294967295 ;
              }
            }
          }
          case ip-format {
            description 'OSPFv2 area in IPv4 address format'
                        ;
            leaf ip {
              description 'OSPFv2 area in IPv4 address format'
                          ;
              type ncd:ipv4-address ;
            }
          }
        }
        leaf routeaggr-ip-address {
          description 'Route aggregation IPv4 address'
                      ;
          type ncd:ipv4-address ;
        }
        leaf routeaggr-ip-mask {
          description 'Route aggregation IPv4 mask'
                      ;
          type ncd:ipv4-mask ;
        }
        leaf range-type {
          description 'Route aggregation range type'
                      ;
          type enumeration {
            enum nssa-external-link {
              value 0 ;
            }
            enum summary-link {
              value 1 ;
            }
          }
        }
        leaf opt-type {
          description 'Operation type'
                      ;
          type enumeration {
            enum config {
              value 0 ;
            }
            enum delete {
              value 1 ;
            }
          }
        }
        leaf instance-number {
          description 'OSPFv2 instance number'
                      ;
          type uint32 {
            range 0..31 ;
          }
          ncd:revise-date 2018-06-06 ;
        }
      }
      container dcn-mng-ospf-area {
        description 'Configure Mng OSPFv2 area'
                    ;
        choice area-id {
          description 'OSPFv2 area'
                      ;
          case number-format {
            description 'OSPFv2 area in number format'
                        ;
            leaf number {
              description 'OSPFv2 area in number format'
                          ;
              type uint32 {
                range 0..4294836225 ;
              }
            }
          }
          case ip-format {
            description 'OSPFv2 area in IPv4 address format'
                        ;
            leaf ip {
              description 'OSPFv2 area in IPv4 address format'
                          ;
              type ncd:ipv4-address ;
            }
          }
        }
        leaf instance-number {
          description 'OSPFv2 instance number'
                      ;
          type uint32 {
            range 0..31 ;
          }
        }
        leaf opt-type {
          description 'Operation type'
                      ;
          type enumeration {
            enum config {
              value 0 ;
            }
            enum delete {
              value 1 ;
            }
          }
        }
        ncd:revise-date 2015-10-22 ;
      }
      container dcn-mng-ospfv3-area {
        description 'Configure Mng OSPFv3 area'
                    ;
        choice area-id {
          description 'OSPFv3 area'
                      ;
          case number-format {
            description 'OSPFv3 area in number format'
                        ;
            leaf number {
              description 'OSPFv3 area in number format'
                          ;
              type uint32 {
                range 0..4294836225 ;
              }
            }
          }
          case ip-format {
            description 'OSPFv3 area in IPv4 address format'
                        ;
            leaf ip {
              description 'OSPFv3 area in IPv4 address format'
                          ;
              type ncd:ipv4-address ;
            }
          }
        }
        leaf opt-type {
          description 'Operation type'
                      ;
          type enumeration {
            enum config {
              value 0 ;
            }
            enum delete {
              value 1 ;
            }
          }
        }
        ncd:revise-date 2019-08-08 ;
      }
      container dcn-ospf-area-stub {
        description 'Configure OSPFv2 stub area'
                    ;
        choice area-id {
          description 'OSPFv2 area'
                      ;
          case number-format {
            description 'OSPFv2 area in number format'
                        ;
            leaf number {
              description 'OSPFv2 area in number format'
                          ;
              type uint32 {
                range 1..4294967295 ;
              }
            }
          }
          case ip-format {
            description 'OSPFv2 area in IPv4 address format'
                        ;
            leaf ip {
              description 'OSPFv2 area in IPv4 address format'
                          ;
              type ncd:ipv4-address ;
            }
          }
        }
        leaf instance-number {
          description 'OSPFv2 instance number'
                      ;
          type uint32 {
            range 0..31 ;
          }
        }
        leaf no-summary {
          description 'Forbid sending 3-LSA to stub area'
                      ;
          type uint8 {
            range 0..1 ;
          }
        }
        leaf default-cost {
          description 'Default cost'
                      ;
          type uint32 {
            range 1..65535 ;
          }
        }
        leaf opt-type {
          description 'Operation type'
                      ;
          type enumeration {
            enum config {
              value 0 ;
            }
            enum delete {
              value 1 ;
            }
          }
        }
        leaf area-type {
          description 'Area type'
                      ;
          type enumeration {
            enum stub {
              value 2 ;
            }
          }
          ncd:revise-date 2018-06-06 ;
        }
        ncd:revise-date 2015-04-23 ;
      }
      container dcn-ospf-redistribute {
        description 'Configure OSPFv2 route redistribute'
                    ;
        leaf protocol {
          description 'Protocol type'
                      ;
          type enumeration {
            enum sl-nat64-ipv4 {
              value 27 ;
            }
            enum user-special {
              value 17 ;
            }
            enum nat {
              value 11 ;
            }
            enum natpt {
              value 13 ;
            }
            enum subscriber-host {
              value 12 ;
            }
            enum subscriber-aggregation {
              value 16 ;
            }
            enum static {
              value 1 ;
            }
            enum connected {
              value 2 ;
            }
            enum rip {
              value 3 ;
            }
            enum ospf-int {
              value 4 ;
            }
            enum ospf-ext {
              value 5 ;
            }
            enum isis-1 {
              value 6 ;
            }
            enum isis-2 {
              value 7 ;
            }
            enum isis-1-2 {
              value 8 ;
            }
            enum bgp-ext {
              value 9 ;
            }
            enum bgp-int {
              value 10 ;
            }
            enum ps-busi-addr {
              value 14 ;
            }
            enum ps-user-addr {
              value 15 ;
            }
          }
        }
        leaf opt-type {
          description 'Operation type'
                      ;
          type enumeration {
            enum config {
              value 0 ;
            }
            enum delete {
              value 1 ;
            }
          }
        }
        leaf ospf-id {
          description 'OSPFv2 process ID'
                      ;
          type uint32 {
            range 1..65535 ;
          }
        }
        leaf dest-ospf-id {
          description 'Destination OSPFv2 process ID'
                      ;
          type uint32 {
            range 0..31 ;
          }
          ncd:revise-date 2018-10-26 ;
        }
        leaf src-ospf-id {
          description 'Source OSPFv2 process ID'
                      ;
          type uint32 {
            range 0..31 ;
          }
          ncd:revise-date 2018-06-06 ;
        }
        leaf isis-id {
          description 'ISIS process ID'
                      ;
          type uint32 {
            range 0..65535 ;
          }
        }
        container as {
          description 'Configure AS'
                      ;
          leaf as-flag {
            description 'AS flag'
                        ;
            type enumeration {
              enum config {
                value 0 ;
              }
              enum delete {
                value 1 ;
              }
            }
          }
          leaf as-value {
            description 'AS number of BGP'
                        ;
            type ncd:asn-dot ;
          }
        }
        leaf peer {
          description 'Invalid value : 0.0.0.0'
                      ;
          type ncd:ipv4-address ;
        }
        leaf with-originate-metric {
          description 'Invalid value : 0'
                      ;
          type uint32 {
            range 0..1 ;
          }
        }
        leaf metric {
          description 'Redistribute LSA metric'
                      ;
          type uint32 {
            range 0..16777214 ;
          }
        }
        leaf metric-type {
          description 'Redistribute LSA metric-type'
                      ;
          type enumeration {
            enum ext-2 {
              value 0 ;
              description 'OSPF external type 2'
                          ;
            }
            enum ext-1 {
              value 1 ;
              description 'OSPF external type 1'
                          ;
            }
          }
        }
        leaf route-map {
          description 'Route matching strategy'
                      ;
          type string ;
        }
        container tag {
          description 'Configure tag'
                      ;
          leaf tag-flag {
            description 'Tag flag'
                        ;
            type enumeration {
              enum config {
                value 1 ;
              }
              enum delete {
                value 0 ;
              }
            }
          }
          leaf tag-value {
            description 'Tag value'
                        ;
            type uint32 {
              range 0..4294967295 ;
            }
          }
        }
        leaf ip-address {
          description 'IPv4 address'
                      ;
          type ncd:ipv4-address ;
          ncd:revise-date 2018-06-06 ;
        }
        leaf ip-mask {
          description 'IPv4 address mask'
                      ;
          type ncd:ipv4-mask ;
          ncd:revise-date 2018-06-06 ;
        }
        leaf host-only {
          description 'Redistribute host route only'
                      ;
          type enumeration {
            enum host-true {
              value 1 ;
              description 'host-only true'
                          ;
            }
            enum host-false {
              value 0 ;
              description 'host-only false'
                          ;
            }
          }
          ncd:revise-date 2019-09-02 ;
        }
      }
      container ip-route {
        description 'Configure DCN IPv4 static route'
                    ;
        leaf ip-address {
          description 'IPv4 address'
                      ;
          type ncd:ipv4-address ;
        }
        leaf ip-mask {
          description 'IPv4 address mask'
                      ;
          type ncd:ipv4-mask ;
        }
        leaf next-hop {
          description 'Nexthop IPv4 address'
                      ;
          type ncd:ipv4-address ;
        }
        leaf bfd-flag {
          description 'Whether enable BFD detection, the default is 0 means dis'
                    + 'able, and 1 means enable. The range is 0-1'
                      ;
          type enumeration {
            enum enable {
              value 1 ;
              description bfd-enable ;
            }
            enum disable {
              value 0 ;
              description bfd-disable ;
            }
          }
          ncd:revise-date 2020-07-20 ;
        }
        leaf opt-type {
          description 'Operation type'
                      ;
          type enumeration {
            enum config {
              value 0 ;
            }
            enum delete {
              value 1 ;
            }
            enum edit {
              value 2 ;
            }
          }
        }
      }
      container dcn-ipv6-static-route {
        description 'Configure DCN IPv6 static route'
                    ;
        leaf ipv6-address {
          description 'IPv6 address'
                      ;
          type ncd:ipv6-address ;
        }
        leaf prefix-length {
          description 'IPv6 prefix length'
                      ;
          type ncd:ipv6-mask-length {
            range 0..128 ;
          }
        }
        leaf next-hop {
          description 'Next-hop IPv6 address'
                      ;
          type ncd:ipv6-address ;
        }
        leaf bfd-flag {
          description 'Whether enable BFD detection, the default is 0 means dis'
                    + 'able, and 1 means enable. The range is 0-1'
                      ;
          type enumeration {
            enum enable {
              value 1 ;
              description bfd-enable ;
            }
            enum disable {
              value 0 ;
              description bfd-disable ;
            }
          }
          ncd:revise-date 2020-07-20 ;
        }
        leaf opt-type {
          description 'Operation type'
                      ;
          type enumeration {
            enum config {
              value 0 ;
            }
            enum delete {
              value 1 ;
            }
          }
        }
        ncd:revise-date 2019-08-08 ;
      }
      container dcn-ospf-process {
        description 'Configure OSPFv2 instance'
                    ;
        leaf process-id {
          description 'OSPFv2 instance number'
                      ;
          type uint32 {
            range 1..31 ;
          }
        }
        leaf router-id {
          description 'OSPFv2 router-id'
                      ;
          type ncd:ipv4-address ;
        }
        leaf opt-type {
          description 'Operation type'
                      ;
          type enumeration {
            enum config {
              value 0 ;
            }
            enum delete {
              value 1 ;
            }
          }
        }
        ncd:revise-date 2018-06-06 ;
      }
      container dcn-ping-detect {
        description 'Configure DCN PING detect'
                    ;
        leaf id {
          description 'Detect group ID'
                      ;
          type uint8 {
            range 1..32 ;
          }
        }
        leaf target-ip-address {
          description 'Target IPv4 address'
                      ;
          type ncd:ipv4-address ;
          ncd:revise-date 2019-11-07 ;
        }
        leaf dest-ip-address {
          description 'Destination IPv4 address'
                      ;
          type ncd:ipv4-address ;
        }
        leaf dest-ip-mask {
          description 'Destination IPv4 address mask'
                      ;
          type ncd:ipv4-mask ;
        }
        leaf nexthop-ip-address {
          description 'Nexthop IPv4 address'
                      ;
          type ncd:ipv4-address ;
        }
        leaf src-ip-address {
          description 'Source IPv4 address'
                      ;
          type ncd:ipv4-address ;
        }
        leaf interval {
          description Looptime(s) ;
          type uint8 {
            range 1..60 ;
          }
        }
        leaf timeout {
          description Timeout(s) ;
          type uint8 {
            range 1..3 ;
          }
        }
        leaf retry {
          description 'Repeat time'
                      ;
          type uint8 {
            range 5..20 ;
          }
        }
        leaf instance-number {
          description 'OSPFv2 instance number'
                      ;
          type uint32 {
            range 0..31 ;
          }
        }
        leaf opt-type {
          description 'Operate type'
                      ;
          type enumeration {
            enum config {
              value 0 ;
            }
            enum delete {
              value 1 ;
            }
          }
        }
        ncd:revise-date 2018-06-06 ;
      }
      container dcn-neighbor-ifs {
        description 'Configure neighbor net-element in batch'
                    ;
        container if-neighbor-l2-configs {
          description 'Configure interface L2 attribute of neighbor net-element'
                      ;
          list if-neighbor-l2-config {
            description 'Configure interface L2/L3 attribute of neighbor net-el'
                      + 'ement'
                        ;
            key if-name ;
            leaf if-name {
              description 'Interface name'
                          ;
              type string ;
            }
            leaf shelf {
              description 'Shelf number'
                          ;
              type uint8 {
                range 0..255 {
                  description 'shelf range.'
                              ;
                }
              }
            }
            leaf slot {
              description 'Slot number'
                          ;
              type uint8 {
                range 0..255 {
                  description 'slot range.'
                              ;
                }
              }
            }
            leaf subslot {
              description 'Subslot number'
                          ;
              type uint8 {
                range 0..255 ;
              }
            }
            leaf port {
              description 'Port number'
                          ;
              type uint32 {
                range 1..255 ;
              }
            }
            leaf dcn {
              description 'Dnable or disable interface DCN function'
                          ;
              type enumeration {
                enum enable {
                  value 0 ;
                  description 'dcn enable'
                              ;
                }
                enum disable {
                  value 1 ;
                  description 'dcn disable'
                              ;
                }
              }
            }
            leaf vlanid {
              description 'Vlan id'
                          ;
              type uint16 {
                range 1..4094 {
                  description 'value range: 1-4094'
                              ;
                }
              }
            }
            leaf bandwidth-mb {
              description Bandwidth(M) ;
              type uint32 {
                range 1..100 {
                  description 'band width range: 1-100'
                              ;
                }
              }
            }
            leaf mac {
              description Mac ;
              type ncd:mac-address {
                length 0..281474976710655 ;
              }
            }
          }
          ncd:revise-date 2019-07-02 ;
        }
        container if-neighbor-l3-configs {
          description 'Configure interface L3 attribute of neighbor net-element'
                      ;
          list if-neighbor-l3-config {
            description 'Configure interface L3 attribute of neighbor net-eleme'
                      + 'nt'
                        ;
            key if-name ;
            leaf if-name {
              description 'Interface name'
                          ;
              type string ;
            }
            leaf shelf {
              description 'Shelf number'
                          ;
              type uint8 {
                range 0..255 {
                  description 'shelf range.'
                              ;
                }
              }
            }
