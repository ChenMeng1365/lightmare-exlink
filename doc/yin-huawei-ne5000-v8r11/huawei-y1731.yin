  <container name="y1731">
    <description>
      <text>Configure Y.1731 attributes. Y.1731 is developed by ITU-T for operations, administration and maintenance (OAM) of Ethernet, which providing fault management and performance management solutions for Ethernet.</text>
    </description>
    <container name="global-attribute">
      <description>
        <text>Configure global Y.1731 attributes.</text>
      </description>
      <leaf name="cfm-version">
        <type name="version"/>
        <default value="standard"/>
        <description>
          <text>Version of Y.1731, which can be standard or draft7.</text>
        </description>
      </leaf>
      <leaf name="mp-address-model">
        <type name="mp-address-model"/>
        <default value="bridge"/>
        <description>
          <text>MAC model of Y.1731, which can be bridge or individual.</text>
        </description>
      </leaf>
      <leaf name="pm-mode-enable">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable Y.1731 PM.</text>
        </description>
      </leaf>
      <leaf name="portid-tlv">
        <type name="portid-tlv-type"/>
        <default value="interface-name"/>
        <description>
          <text>Set the type of port ID TLV in MAC trace packet.</text>
        </description>
      </leaf>
      <leaf name="idle-test-id-count">
        <type name="uint16"/>
        <default value="1"/>
        <description>
          <text>Set the number of idle test ID for query.</text>
        </description>
      </leaf>
    </container>
    <container name="global-enable">
      <when condition="(../global-attribute/cfm-version='standard')"/>
      <presence value="Enable/disable Y.1731."/>
      <description>
        <text>Configure global-enable Y.1731 attributes. Y.1731 is developed by ITU-T for operations, administration and maintenance (OAM) of Ethernet, which providing fault management and performance management solutions for Ethernet.</text>
      </description>
      <container name="mds">
        <description>
          <text>List of all maintenance domains.</text>
        </description>
        <list name="md">
          <key value="name"/>
          <description>
            <text>Configure a maintenance domain.</text>
          </description>
          <leaf name="name">
            <type name="string">
              <length value="1..43"/>
              <pattern value="[^\?]*"/>
            </type>
            <description>
              <text>Configure the name of a maintenance domain (MD).</text>
            </description>
          </leaf>
          <leaf name="level">
            <ext:operation-exclude value="update|delete"/>
            <type name="uint8">
              <range value="0..7"/>
            </type>
            <default value="0"/>
            <description>
              <text>Configure the level of the maintenance domain.</text>
            </description>
          </leaf>
          <leaf name="format">
            <ext:operation-exclude value="update|delete"/>
            <type name="md-format"/>
            <must condition="((../format='string') or ((../format='none') and (not(../format-name))) or ((../format!='none') and (../format-name)))"/>
            <default value="string"/>
            <description>
              <text>Configure the format of the maintenance domain name. By default, the format is a string. If the format is MAC-address, the format name must be in form of H-H-H:U&lt;0-65535&gt;, for example, 0001-0001-0001:1.</text>
            </description>
          </leaf>
          <leaf name="format-name">
            <ext:operation-exclude value="update|delete"/>
            <when condition="../format!='none'"/>
            <type name="string">
              <length value="1..43"/>
              <pattern value="[^\?]*"/>
            </type>
            <ext:dynamic-default>
              <ext:default-value value="../name">
                <when condition="../format='string'"/>
                <description>
                  <text>The default value equals to the value of name when the format is string.</text>
                </description>
              </ext:default-value>
            </ext:dynamic-default>
            <description>
              <text>Configure the name of the maintenance domain which will be formatted. By default, it is the same as the name of the maintenance domain.</text>
            </description>
          </leaf>
          <leaf name="mip-create-type">
            <type name="mip-create-type"/>
            <default value="none"/>
            <description>
              <text>Configure the MIP create type of maintenance domain.</text>
            </description>
          </leaf>
          <leaf name="sender-tlv-type">
            <type name="sender-tlv-type"/>
            <default value="defer"/>
            <description>
              <text>Configure the sender ID TLV type of the default maintenance domain.</text>
            </description>
          </leaf>
          <container name="mas">
            <description>
              <text>List of all maintenance associations.</text>
            </description>
            <list name="ma">
              <key value="name"/>
              <description>
                <text>Configure a maintenance association.</text>
              </description>
              <leaf name="name">
                <type name="string">
                  <length value="1..43"/>
                  <pattern value="[^\?]*"/>
                </type>
                <description>
                  <text>Configure the name of a maintenance association (MA).</text>
                </description>
              </leaf>
              <leaf name="format">
                <ext:operation-exclude value="update|delete"/>
                <type name="ma-format"/>
                <must condition="((../format='icc-based' and ../../../format='none' and ../format-name) or (../format!='icc-based'))"/>
                <default value="string"/>
                <description>
                  <text>Configure the format of the maintenance association's name.</text>
                </description>
              </leaf>
              <leaf name="format-name">
                <ext:operation-exclude value="update|delete"/>
                <type name="string">
                  <length value="1..43"/>
                  <pattern value="[^\?]*"/>
                </type>
                <ext:dynamic-default>
                  <ext:default-value value="../name">
                    <when condition="../format='string'"/>
                    <description>
                      <text>The default value equals to the value of name when the format is string.</text>
                    </description>
                  </ext:default-value>
                </ext:dynamic-default>
                <description>
                  <text>Configure the name of the maintenance association (MA) which will be formatted. By default, as the same with the name of the MA. The length of MA format description is [1,13] when the format is ICC-based, and [1,43] when it is string.</text>
                </description>
              </leaf>
              <choice name="map-service">
                <default value="no-bind"/>
                <description>
                  <text>Configure the type of a maintenance association's service.</text>
                </description>
                <case name="no-bind">
                  <description>
                    <text>MA map no service.</text>
                  </description>
                  <leaf name="no-bind">
                    <type name="empty"/>
                    <description>
                      <text>MA map no service.</text>
                    </description>
                  </leaf>
                </case>
                <case name="bind-vlan">
                  <description>
                    <text>Map a VLAN.</text>
                  </description>
                  <leaf name="vlan-id">
                    <ext:operation-exclude value="update">
                      <when condition="../meps/mep or ../rmeps/rmep"/>
                      <description>
                        <text>The value cannot be updated, when mep or rmep exists.</text>
                      </description>
                    </ext:operation-exclude>
                    <type name="uint16">
                      <range value="1..4094"/>
                    </type>
                    <mandatory value="true"/>
                    <description>
                      <text>Configure the VLAN ID associated with the maintenance association.</text>
                    </description>
                  </leaf>
                  <leaf name="one-way-threshold-value">
                    <ext:operation-exclude value="update"/>
                    <type name="uint32">
                      <range value="1..4294967295"/>
                    </type>
                    <units name="ms"/>
                    <description>
                      <text>Configure the threshold of one-way delay measurement.</text>
                    </description>
                  </leaf>
                  <leaf name="two-way-threshold-value">
                    <ext:operation-exclude value="update"/>
                    <type name="uint32">
                      <range value="1..4294967295"/>
                    </type>
                    <units name="ms"/>
                    <description>
                      <text>Configure the threshold of two-way delay measurement.</text>
                    </description>
                  </leaf>
                </case>
                <case name="bind-l2vc">
                  <description>
                    <text>Map a L2VC.</text>
                  </description>
                  <leaf name="l2vc-id">
                    <ext:operation-exclude value="update">
                      <when condition="../meps/mep or ../rmeps/rmep"/>
                      <description>
                        <text>The 'l2vc-id' cannot be updated, when mep or rmep exists.</text>
                      </description>
                    </ext:operation-exclude>
                    <type name="leafref">
                      <path value="/l2vpn:l2vpn/l2vpn:instances/l2vpn:instance/l2vpn:vpws-ldp/l2vpn:pws/l2vpn:pw/l2vpn:pw-id"/>
                    </type>
                    <must condition="count(../meps/mep)&lt;=1"/>
                    <mandatory value="true"/>
                    <description>
                      <text>Configure the ID of the L2VC associated with the maintenance association.</text>
                    </description>
                    <ext:support-filter value="true"/>
                  </leaf>
                  <leaf name="l2vc-type">
                    <ext:operation-exclude value="update">
                      <when condition="../meps/mep or ../rmeps/rmep"/>
                      <description>
                        <text>The 'l2vc-type' cannot be updated, when mep or rmep exists.</text>
                      </description>
                    </ext:operation-exclude>
                    <type name="l2vc-type"/>
                    <mandatory value="true"/>
                    <description>
                      <text>Configure the type of the L2VC associated with the maintenance association.</text>
                    </description>
                    <ext:support-filter value="true"/>
                  </leaf>
                  <leaf name="l2vc-ip">
                    <type name="leafref">
                      <path value="/l2vpn:l2vpn/l2vpn:instances/l2vpn:instance/l2vpn:vpws-ldp/l2vpn:pws/l2vpn:pw/l2vpn:peer-ip"/>
                    </type>
                    <description>
                      <text>Configure the peer IP of the L2VC associated with the maintenance association.</text>
                    </description>
                  </leaf>
                </case>
                <case name="bind-vsi">
                  <description>
                    <text>Map a VSI.</text>
                  </description>
                  <leaf name="vsi-name">
                    <ext:operation-exclude value="update">
                      <when condition="../meps/mep or ../rmeps/rmep"/>
                      <description>
                        <text>The 'vsi-name' cannot be updated, when mep or rmep exists.</text>
                      </description>
                    </ext:operation-exclude>
                    <type name="leafref">
                      <path value="/l2vpn:l2vpn/l2vpn:instances/l2vpn:instance/l2vpn:name"/>
                    </type>
                    <must condition="/l2vpn:l2vpn/l2vpn:instances/l2vpn:instance[l2vpn:name = current()][l2vpn:type='vpls']"/>
                    <description>
                      <text>Configure the name of the VSI associated with the maintenance association.</text>
                    </description>
                    <ext:support-filter value="true"/>
                  </leaf>
                </case>
              </choice>
              <leaf name="packet-priority">
                <type name="uint8">
                  <range value="0..7"/>
                </type>
                <default value="7"/>
                <description>
                  <text>Configure the priority of the packet which is sent in the maintenance association.</text>
                </description>
              </leaf>
              <leaf name="ccm-interval">
                <type name="ccm-interval"/>
                <default value="1s"/>
                <description>
                  <text>Configure the interval at which CCMs are sent in a maintenance association.</text>
                </description>
              </leaf>
              <leaf name="alarm-occur-time">
                <type name="uint16">
                  <range value="0..30000"/>
                </type>
                <units name="ms"/>
                <must condition="(../alarm-occur-time mod 500)=0"/>
                <default value="2500"/>
                <description>
                  <text>Configure the delay for reporting an alarm.</text>
                </description>
              </leaf>
              <leaf name="alarm-finish-time">
                <type name="uint16">
                  <range value="0..30000"/>
                </type>
                <units name="ms"/>
                <must condition="(../alarm-finish-time mod 500)=0"/>
                <default value="10000"/>
                <description>
                  <text>Configure the delay for clearing an alarm.</text>
                </description>
              </leaf>
              <leaf name="active-time">
                <type name="uint16">
                  <range value="0..600"/>
                </type>
                <units name="s"/>
                <default value="0"/>
                <description>
                  <text>Configure the time to activate an RMEP.</text>
                </description>
              </leaf>
              <leaf name="interface-tlv">
                <type name="boolean"/>
                <default value="false"/>
                <description>
                  <text>Enable/disable interface status TLV in CCM.</text>
                </description>
              </leaf>
              <leaf name="sender-id-tlv">
                <type name="boolean"/>
                <default value="false"/>
                <description>
                  <text>Enable/disable sender ID TLV in CCM.</text>
                </description>
              </leaf>
              <leaf name="port-status-tlv">
                <type name="boolean"/>
                <default value="false"/>
                <description>
                  <text>Enable/disable port status TLV in CCM.</text>
                </description>
              </leaf>
              <leaf name="mip-create-type">
                <type name="ma-mip-create-type"/>
                <default value="defer"/>
                <description>
                  <text>Configures the MIP create type of maintenance association.</text>
                </description>
              </leaf>
              <leaf name="unexpected-mep-alarm">
                <type name="boolean"/>
                <default value="false"/>
                <description>
                  <text>Enable/disable unexpectedMep alarm.</text>
                </description>
              </leaf>
              <container name="ais">
                <presence value="Enable/disable AIS."/>
                <description>
                  <text>Configure the function of all AIS instances.</text>
                </description>
                <leaf name="interval">
                  <type name="ais-interval"/>
                  <default value="1s"/>
                  <description>
                    <text>Configure the interval at which AIS packets are sent.</text>
                  </description>
                </leaf>
                <leaf name="level">
                  <type name="uint8">
                    <range value="0..7"/>
                  </type>
                  <must condition="../level&gt;../../../../level"/>
                  <description>
                    <text>Configure the level of AIS packets.</text>
                  </description>
                </leaf>
                <leaf name="suppress-alarm-enable">
                  <type name="boolean"/>
                  <default value="false"/>
                  <description>
                    <text>Enable/disable suppressing alarms.</text>
                  </description>
                </leaf>
              </container>
              <container name="meps">
                <description>
                  <text>List of all MEPs.</text>
                </description>
                <list name="mep">
                  <must condition="(pe-vlan and ce-vlan) or (not(pe-vlan) and not(ce-vlan))"/>
                  <must condition="(direction and (if-name or peer-ip)) or (not(direction) and not(if-name) and not(peer-ip))"/>
                  <key value="id"/>
                  <description>
                    <text>Configure a MEP point.</text>
                  </description>
                  <leaf name="id">
                    <type name="uint16">
                      <range value="1..8191"/>
                    </type>
                    <description>
                      <text>Configure a MEP ID.</text>
                    </description>
                  </leaf>
                  <choice name="type">
                    <mandatory value="true"/>
                    <description>
                      <text>MEP type.</text>
                    </description>
                    <case name="if-mep">
                      <leaf name="if-name">
                        <ext:operation-exclude value="update"/>
                        <type name="leafref">
                          <path value="/ifm:ifm/ifm:interfaces/ifm:interface/ifm:name"/>
                        </type>
                        <must condition="not(../../../meps/mep[if-name!=current()])"/>
                        <mandatory value="true"/>
                        <description>
                          <text>Configure a MEP interface.</text>
                        </description>
                      </leaf>
                      <choice name="tags">
                        <description>
                          <text>Configure the tags of a MEP.</text>
                        </description>
                        <case name="two-tags">
                          <leaf name="pe-vlan">
                            <ext:operation-exclude value="update|delete"/>
                            <type name="uint16">
                              <range value="1..4094"/>
                            </type>
                            <description>
                              <text>Configure the PE VLAN of a MEP.</text>
                            </description>
                          </leaf>
                          <leaf name="ce-vlan">
                            <ext:operation-exclude value="update|delete"/>
                            <type name="uint16">
                              <range value="1..4094"/>
                            </type>
                            <description>
                              <text>Configure the CE VLAN of a MEP.</text>
                            </description>
                          </leaf>
                          <description>
                            <text>Configure the two tags of a MEP.</text>
                          </description>
                        </case>
                        <case name="one-tag">
                          <description>
                            <text>Configure the dot1q VLAN of a MEP.</text>
                          </description>
                          <leaf name="dot1q-vlan">
                            <ext:operation-exclude value="update|delete"/>
                            <type name="uint16">
                              <range value="1..4094"/>
                            </type>
                            <description>
                              <text>Configure the dot1q VLAN of a MEP.</text>
                            </description>
                          </leaf>
                        </case>
                      </choice>
                      <leaf name="clear-arp">
                        <when condition="../../../vlan-id"/>
                        <type name="boolean"/>
                        <must condition="../direction='outward'"/>
                        <default value="false"/>
                        <description>
                          <text>Enable/disable clear ARP.</text>
                        </description>
                      </leaf>
                      <container name="lck">
                        <presence value="Enable/disable LCK."/>
                        <description>
                          <text>Configure the LCK.</text>
                        </description>
                        <leaf name="level">
                          <type name="uint8">
                            <range value="0..7"/>
                          </type>
                          <must condition="../level &gt; ../../../../../../level"/>
                          <mandatory value="true"/>
                          <description>
                            <text>Configure the LCK message level.</text>
                          </description>
                        </leaf>
                      </container>
                      <container name="eth-bn-receive">
                        <when condition="(../../../vlan-id or ../../../no-bind) and (not (../pe-vlan)) and ../direction='outward'"/>
                        <presence value="Enable the ETN-BN receive function."/>
                        <description>
                          <text>Configure ETH-BN receive.</text>
                        </description>
                      </container>
                      <container name="port-link">
                        <when condition="../direction='inward'"/>
                        <must condition="(loc='true' or tlv='true')"/>
                        <presence value="Enable/disable port link in a MEP."/>
                        <description>
                          <text>Configure port link.</text>
                        </description>
                        <leaf name="port-link-holdoff">
                          <type name="uint8">
                            <range value="0..100"/>
                          </type>
                          <units name="100 ms"/>
                          <default value="0"/>
                          <description>
                            <text>Port linkage hold off time.</text>
                          </description>
                        </leaf>
                        <leaf name="wtr">
                          <type name="uint16">
                            <range value="0..600"/>
                          </type>
                          <units name="s"/>
                          <default value="60"/>
                          <description>
                            <text>Port linkage wait to restore (WTR) time.</text>
                          </description>
                        </leaf>
                        <leaf name="loc">
                          <type name="boolean"/>
                          <default value="true"/>
                          <description>
                            <text>Enable/disable LOC.</text>
                          </description>
                        </leaf>
                        <leaf name="tlv">
                          <type name="boolean"/>
                          <default value="false"/>
                          <description>
                            <text>Enable/disable interface status TLV.</text>
                          </description>
                        </leaf>
                      </container>
                      <description>
                        <text>Configure a MEP on an interface.</text>
                      </description>
                    </case>
                    <case name="pw-mep">
                      <when condition="../../vsi-name"/>
                      <leaf name="peer-ip">
                        <ext:operation-exclude value="update"/>
                        <type name="inet:ipv4-address-no-zone"/>
                        <mandatory value="true"/>
                        <description>
                          <text>Configure the IP of peer end.</text>
                        </description>
                      </leaf>
                      <leaf name="vc-id">
                        <ext:operation-exclude value="update|delete"/>
                        <when condition="../peer-ip"/>
                        <type name="uint32">
                          <range value="1..4294967295"/>
                        </type>
                        <description>
                          <text>Configure the ID of VC.</text>
                        </description>
                      </leaf>
                      <leaf name="config-mac">
                        <ext:operation-exclude value="update|delete"/>
                        <type name="pub-type:mac-address"/>
                        <description>
                          <text>MAC address of a MEP.</text>
                        </description>
                      </leaf>
                      <description>
                        <text>Configure a MEP on a PW.</text>
                      </description>
                    </case>
                  </choice>
                  <leaf name="direction">
                    <ext:operation-exclude value="update|delete"/>
                    <type name="direction"/>
                    <must condition="(../direction='outward' and count(../../mep)=1) or not(../../../meps/mep[direction!=current()])"/>
                    <description>
                      <text>Configure the direction of a MEP.</text>
                    </description>
                  </leaf>
                  <leaf name="cc-send-enable">
                    <type name="boolean"/>
                    <must condition="../cc-send-enable='false' or (../if-name)"/>
                    <default value="false"/>
                    <description>
                      <text>Enable/disable CCMs which can be sent.</text>
                    </description>
                  </leaf>
                  <leaf name="rdi-alarm-disabled">
                    <type name="boolean"/>
                    <must condition="../rdi-alarm-disabled='false' or (../if-name or ../peer-ip)"/>
                    <default value="false"/>
                    <description>
                      <text>Enable/disable suppressing RDI alarm.</text>
                    </description>
                  </leaf>
                  <leaf name="ais-alarm-disabled">
                    <type name="boolean"/>
                    <must condition="../ais-alarm-disabled='false' or (../if-name or ../peer-ip)"/>
                    <default value="false"/>
                    <description>
                      <text>Enable/disable suppressing AIS alarm.</text>
                    </description>
                  </leaf>
                </list>
              </container>
              <container name="rmeps">
                <description>
                  <text>List of all remote maintenance points.</text>
                </description>
                <list name="rmep">
                  <key value="id"/>
                  <description>
                    <text>Configure a remote maintenance point.</text>
                  </description>
                  <leaf name="id">
                    <type name="uint16">
                      <range value="1..8191"/>
                    </type>
                    <description>
                      <text>Configure the ID of a RMEP.</text>
                    </description>
                  </leaf>
                  <leaf name="cc-recv-enabled">
                    <type name="boolean"/>
                    <default value="false"/>
                    <description>
                      <text>Enable/disable CC receive.</text>
                    </description>
                  </leaf>
                </list>
              </container>
            </list>
          </container>
        </list>
      </container>
      <container name="test-instances">
        <description>
          <text>List of all test instances.</text>
        </description>
        <list name="test-instance">
          <key value="test-id"/>
          <description>
            <text>Configure a test instance.</text>
          </description>
          <leaf name="test-id">
            <type name="uint32">
              <range value="1..4294967295"/>
            </type>
            <description>
              <text>Configure the ID of a test instance (65536 to 4294967295 is reserved for automatic allocation).</text>
            </description>
          </leaf>
          <leaf name="description">
            <ext:operation-exclude value="update|delete"/>
            <type name="string">
              <length value="1..63"/>
              <pattern value="[^\?]*"/>
            </type>
            <description>
              <text>Configure the description of a test instance.</text>
            </description>
          </leaf>
          <leaf name="md-name">
            <ext:operation-exclude value="update"/>
            <type name="leafref">
              <path value="/y1731/global-enable/mds/md/name"/>
            </type>
            <mandatory value="true"/>
            <description>
              <text>Configure the name of a maintenance domain (MD).</text>
            </description>
          </leaf>
          <leaf name="ma-name">
            <ext:operation-exclude value="update"/>
            <type name="leafref">
              <path value="/y1731/global-enable/mds/md/mas/ma/name"/>
            </type>
            <must condition="(/y1731:y1731/y1731:global-enable/y1731:mds/y1731:md[y1731:name=current()/../md-name]/y1731:mas/y1731:ma[y1731:name=current()])"/>
            <mandatory value="true"/>
            <description>
              <text>Configure the name of a maintenance association (MA).</text>
            </description>
          </leaf>
          <leaf name="mep-id">
            <ext:operation-exclude value="update"/>
            <type name="leafref">
              <path value="/y1731/global-enable/mds/md/mas/ma/meps/mep/id"/>
            </type>
            <must condition="/y1731:y1731/y1731:global-enable/y1731:mds/y1731:md[y1731:name=current()/../md-name]/y1731:mas/y1731:ma[y1731:name=current()/../ma-name]/y1731:meps/y1731:mep[y1731:id=current()]"/>
            <mandatory value="true"/>
            <description>
              <text>Configure a MEP ID.</text>
            </description>
          </leaf>
          <choice name="remote">
            <description>
              <text>Configure the RMEP ID or MAC.</text>
            </description>
            <case name="rmep-id">
              <leaf name="rmep-id">
                <ext:operation-exclude value="update|delete"/>
                <type name="leafref">
                  <path value="/y1731/global-enable/mds/md/mas/ma/rmeps/rmep/id"/>
                </type>
                <must condition="/y1731:y1731/y1731:global-enable/y1731:mds/y1731:md[y1731:name=current()/../md-name]/y1731:mas/y1731:ma[y1731:name=current()/../ma-name]/y1731:rmeps/y1731:rmep[y1731:id=current()]"/>
                <description>
                  <text>Configure a RMEP ID.</text>
                </description>
              </leaf>
              <description>
                <text>Configure a RMEP ID.</text>
              </description>
            </case>
          </choice>
          <leaf name="priority">
            <ext:operation-exclude value="update|delete"/>
            <type name="uint8">
              <range value="0..7"/>
            </type>
            <must condition="(../priority and not(../peer-ip) and not(../vc-id)) or not(../priority)"/>
            <description>
              <text>Configure the priority of packets.</text>
            </description>
          </leaf>
          <leaf name="peer-ip">
            <ext:operation-exclude value="update|delete"/>
            <type name="inet:ipv4-address-no-zone"/>
            <must condition="(../peer-ip and not(../priority)) or not(../peer-ip)"/>
            <description>
              <text>Configure the IP of peer end.</text>
            </description>
          </leaf>
          <leaf name="vc-id">
            <ext:operation-exclude value="update|delete"/>
            <type name="uint32">
              <range value="1..4294967295"/>
            </type>
            <must condition="((../vc-id and ../peer-ip and not(../priority)) or not(../vc-id))"/>
            <description>
              <text>Configure the ID of VC.</text>
            </description>
          </leaf>
          <leaf name="test-file-enable">
            <ext:operation-exclude value="update|delete"/>
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable test ID file.</text>
            </description>
          </leaf>
          <container name="one-way-delay-send">
            <presence value="Enable/disable one-way DM send."/>
            <description>
              <text>Configure a one-way delay measurement send instance.</text>
            </description>
            <leaf name="interval">
              <ext:operation-exclude value="update"/>
              <type name="interval"/>
              <mandatory value="true"/>
              <description>
                <text>Configure the interval of one-way delay measurement send.</text>
              </description>
            </leaf>
          </container>
          <container name="one-way-delay-recv">
            <presence value="Enable/disable one-way DM receive."/>
            <description>
              <text>Configure a one-way delay measurement receive instance.</text>
            </description>
            <leaf name="is-continue">
              <ext:operation-exclude value="update|delete"/>
              <type name="boolean"/>
              <default value="false"/>
              <description>
                <text>Enable/disable one-way delay continue measurement receive.</text>
              </description>
            </leaf>
          </container>
          <container name="two-way-delay-send">
            <presence value="Enable/disable two-way DM send."/>
            <description>
              <text>Configure a two-way delay measurement send instance.</text>
            </description>
            <leaf name="interval">
              <ext:operation-exclude value="update"/>
              <type name="interval"/>
              <mandatory value="true"/>
              <description>
                <text>Configure the interval of two-way delay measurement send.</text>
              </description>
            </leaf>
            <leaf name="period">
              <ext:operation-exclude value="update|delete"/>
              <type name="uint16">
                <range value="1..65535"/>
              </type>
              <units name="s"/>
              <default value="900"/>
              <description>
                <text>The value of measurement duration.</text>
              </description>
            </leaf>
          </container>
          <container name="two-way-delay-recv">
            <description>
              <text>Configure a two-way delay measurement receive instance.</text>
            </description>
            <leaf name="receive-enabled">
              <type name="boolean"/>
              <default value="false"/>
              <description>
                <text>Enable/disable two-way delay measurement receive instance.</text>
              </description>
            </leaf>
          </container>
          <container name="single-loss-send">
            <must condition="not(../dual-loss/enabled='true')"/>
            <presence value="Enable/disable single loss send."/>
            <description>
              <text>Configure a single-ended loss measurement send instance.</text>
            </description>
            <leaf name="interval">
              <ext:operation-exclude value="update"/>
              <type name="interval"/>
              <mandatory value="true"/>
              <description>
                <text>Configure the interval of single-ended loss measurement send.</text>
              </description>
            </leaf>
            <leaf name="period">
              <ext:operation-exclude value="update|delete"/>
              <type name="uint16">
                <range value="1..65535"/>
              </type>
              <units name="s"/>
              <default value="900"/>
              <description>
                <text>The value of measurement duration.</text>
              </description>
            </leaf>
          </container>
          <container name="single-loss-recv">
            <description>
              <text>Configure a single-ended loss measurement receive instance.</text>
            </description>
            <leaf name="enabled">
              <type name="boolean"/>
              <must condition="(not(../../dual-loss/enabled='true') or ((../../dual-loss/enabled='true') and (../enabled='false')))"/>
              <default value="false"/>
              <description>
                <text>Enable/disable single-ended loss measurement receive instance.</text>
              </description>
            </leaf>
          </container>
          <container name="dual-loss">
            <must condition="/y1731:y1731/y1731:global-enable/y1731:mds/y1731:md[y1731:name=current()/../md-name]/y1731:mas/y1731:ma[y1731:name=current()/../ma-name]/y1731:ccm-interval='100ms' or /y1731:y1731/y1731:global-enable/y1731:mds/y1731:md[y1731:name=current()/../md-name]/y1731:mas/y1731:ma[y1731:name=current()/../ma-name]/y1731:ccm-interval='1s' or /y1731:y1731/y1731:global-enable/y1731:mds/y1731:md[y1731:name=current()/../md-name]/y1731:mas/y1731:ma[y1731:name=current()/../ma-name]/y1731:ccm-interval='10s'"/>
            <description>
              <text>Configure a dual-ended loss measurement instance.</text>
            </description>
            <leaf name="enabled">
              <type name="boolean"/>
              <must condition="(((../enabled='true') and (not(../../single-loss-send/interval) and ../../single-loss-recv/enabled='false')) or (../enabled='false'))"/>
              <default value="false"/>
              <description>
                <text>Enable/disable dual-ended loss measurement instance.</text>
              </description>
            </leaf>
          </container>
          <container name="synthetic-loss-send">
            <presence value="Enable/disable single synthetic LM send enable."/>
            <description>
              <text>Configure a single ended synthetic send instance.</text>
            </description>
            <leaf name="interval">
              <ext:operation-exclude value="update"/>
              <type name="sslm-interval"/>
              <mandatory value="true"/>
              <description>
                <text>Configuration interval of single ended synthetic send.</text>
              </description>
            </leaf>
            <leaf name="send-count">
              <ext:operation-exclude value="update|delete"/>
              <type name="uint16">
                <range value="1..1000"/>
              </type>
              <default value="10"/>
              <description>
                <text>Configuration count of single ended synthetic send.</text>
              </description>
            </leaf>
            <leaf name="timeout">
              <ext:operation-exclude value="update|delete"/>
              <type name="uint8">
                <range value="1..10"/>
              </type>
              <default value="5"/>
              <description>
                <text>Configuration time out value of single ended synthetic send.</text>
              </description>
            </leaf>
            <leaf name="period">
              <ext:operation-exclude value="update|delete"/>
              <type name="uint16">
                <range value="1..65535"/>
              </type>
              <default value="900"/>
              <description>
                <text>Configuration period of single ended synthetic loss measurement.</text>
              </description>
            </leaf>
          </container>
          <container name="synthetic-loss-recv">
            <presence value="Enable/disable single synthetic LM receive."/>
            <description>
              <text>Configure a single ended synthetic receive instance.</text>
            </description>
            <leaf name="timeout">
              <ext:operation-exclude value="update|delete"/>
              <type name="uint16">
                <range value="10..300"/>
              </type>
              <units name="s"/>
              <default value="300"/>
              <description>
                <text>Configuration time out value of single ended synthetic receive.</text>
              </description>
            </leaf>
          </container>
        </list>
      </container>
    </container>
  </container>
</module>
