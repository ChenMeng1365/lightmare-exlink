  <container name="configuration">
    <description>
      <text>Configuration model info.</text>
    </description>
    <container name="aps">
      <description>
        <text>Configure APS parameters.</text>
      </description>
      <container name="tunnel-groups">
        <description>
          <text>Configure tunnel group APS parameters.</text>
        </description>
        <list name="tunnel-group">
          <description>
            <text>Configure tunnel group APS parameters.</text>
          </description>
          <key value="group-id"/>
          <leaf name="group-id">
            <description>
              <text>Group ID of tunnel group APS.</text>
            </description>
            <type name="uint32">
              <range value="1..32768">
                <description>
                  <text>Group ID.</text>
                </description>
              </range>
            </type>
          </leaf>
          <leaf name="active-state">
            <description>
              <text>Active state of tunnel group APS.</text>
            </description>
            <type name="active-state"/>
            <default value="active-or-restore"/>
          </leaf>
          <leaf name="send-packet-mode">
            <description>
              <text>Send packet mode of tunnel group APS.</text>
            </description>
            <type name="send-packet-mode"/>
            <default value="send"/>
          </leaf>
          <leaf name="hold-off-time">
            <description>
              <text>Hold-off timer for tunnel group APS(100ms).</text>
            </description>
            <type name="uint8">
              <range value="0..100">
                <description>
                  <text>hold off time(100ms)</text>
                </description>
              </range>
            </type>
            <units name="100ms"/>
            <default value="0"/>
          </leaf>
          <leaf name="hold-off-precise">
            <description>
              <text>Hold-off precise timer for tunnel group APS(10ms).</text>
            </description>
            <type name="uint8">
              <range value="1..9">
                <description>
                  <text>hold off precise time(10ms)</text>
                </description>
              </range>
            </type>
            <units name="10ms"/>
            <ncd:revise-date date="2018-01-02"/>
          </leaf>
          <leaf name="revertive-mode">
            <description>
              <text>Revertive mode of tunnel group APS.</text>
            </description>
            <type name="revertive-mode"/>
            <default value="revertive"/>
          </leaf>
          <leaf name="wtr-time">
            <description>
              <text>Wait-to-restore timer for tunnel group APS(minute).</text>
            </description>
            <type name="uint8">
              <range value="0..12">
                <description>
                  <text>Wait to restore time(minute).</text>
                </description>
              </range>
            </type>
            <units name="minute"/>
            <default value="5"/>
          </leaf>
          <leaf name="switch-cmd">
            <description>
              <text>Switch command of tunnel group APS.</text>
            </description>
            <type name="switch-cmd-linear"/>
            <default value="null"/>
          </leaf>
          <leaf name="is-active">
            <description>
              <text>Active flag of tunnel group APS.</text>
            </description>
            <type name="is-active"/>
            <default value="active"/>
          </leaf>
