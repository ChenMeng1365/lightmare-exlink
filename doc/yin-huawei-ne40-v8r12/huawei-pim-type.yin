  <typedef name="bsr-scope">
    <type name="enumeration">
      <enum name="not-scoped">
        <value value="0"/>
        <description>
          <text>Domain in which C-BSRs are not configured.</text>
        </description>
      </enum>
      <enum name="global">
        <value value="1"/>
        <description>
          <text>The router is a C-BSR in the global domain. By default, no C-BSR is configured in the global domain.</text>
        </description>
      </enum>
      <enum name="admin-scope">
        <value value="2"/>
        <description>
          <text>A PIM-SM domain is divided into multiple BSR administrative domains to implement RP-Set advertisement. By default, no BSR administrative domain is configured, sot that there is only one BSR in each PIM-SM domain.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>BSR administrative domain.</text>
    </description>
  </typedef>
