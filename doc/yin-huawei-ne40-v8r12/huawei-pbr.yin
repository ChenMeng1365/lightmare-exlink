  <ext:task-name value="qos"/>
  <typedef name="routing-type">
    <type name="enumeration">
      <enum name="false">
        <value value="0"/>
        <description>
          <text>No route.</text>
        </description>
      </enum>
      <enum name="default">
        <value value="1"/>
        <description>
          <text>Filter default route.</text>
        </description>
      </enum>
      <enum name="blackhole">
        <value value="2"/>
        <description>
          <text>Filter blackhole route.</text>
        </description>
      </enum>
      <enum name="default-blackhole">
        <value value="3"/>
        <description>
          <text>Filter default and blackhole route.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Routing type.</text>
    </description>
  </typedef>
  <typedef name="routing-filter-type">
    <type name="enumeration">
      <enum name="default">
        <value value="1"/>
        <description>
          <text>Filter default route.</text>
        </description>
      </enum>
      <enum name="blackhole">
        <value value="2"/>
        <description>
          <text>Filter blackhole route.</text>
        </description>
      </enum>
      <enum name="default-blackhole">
        <value value="3"/>
        <description>
          <text>Filter default and blackhole route.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Routing type.</text>
    </description>
  </typedef>
  <typedef name="sid-type">
    <type name="enumeration">
      <enum name="sid">
        <value value="0"/>
        <description>
          <text>Public network SID.</text>
        </description>
      </enum>
      <enum name="vpnsid">
        <value value="1"/>
        <description>
          <text>Private network SID.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>SID/VPNSID.</text>
    </description>
  </typedef>
  <typedef name="ip-type">
    <type name="enumeration">
      <enum name="ipv4">
        <value value="0"/>
        <description>
          <text>IPv4.</text>
        </description>
      </enum>
      <enum name="ipv6">
        <value value="1"/>
        <description>
          <text>IPv6.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Ip type.</text>
    </description>
  </typedef>
  <augment target-node="/qos:qos/qos:global-config">
    <description>
      <text>Global config.</text>
    </description>
    <container name="pbr">
      <description>
        <text>Configure global redirect.</text>
      </description>
      <container name="global-vpn-groups">
        <description>
          <text>List of VPN-instance groups.</text>
        </description>
        <list name="global-vpn-group">
          <key value="vpn-group-name"/>
          <description>
            <text>Configure VPN-instance group.</text>
          </description>
          <leaf name="vpn-group-name">
            <type name="string">
              <length value="1..31"/>
            </type>
            <description>
              <text>Name of a VPN-instance group.</text>
            </description>
          </leaf>
          <leaf-list name="vrf-name">
            <type name="leafref">
              <path value="/ni:network-instance/ni:instances/ni:instance/ni:name"/>
            </type>
            <must condition="../vrf-name!='_public_'"/>
            <max-elements value="8"/>
            <description>
              <text>Configure VPN-instance name.</text>
            </description>
          </leaf-list>
        </list>
      </container>
    </container>
  </augment>
  <augment target-node="/devm:devm/devm:mpu-boards/devm:mpu-board">
    <description>
      <text>PBR.</text>
    </description>
    <container name="pbr">
      <description>
        <text>Configure To filter default route or black hole route when using redirect.</text>
      </description>
      <container name="routing-filters">
        <description>
          <text>List of to filter default route or black hole route when using redirect.</text>
        </description>
        <list name="routing-filter">
          <key value="type"/>
          <max-elements value="2"/>
          <description>
            <text>Configure action of redirecting to public-network.</text>
          </description>
          <leaf name="type">
            <type name="ip-type"/>
            <description>
              <text>Protocol family.</text>
            </description>
          </leaf>
          <leaf name="routing-type">
            <type name="routing-filter-type"/>
            <mandatory value="true"/>
            <description>
              <text>Routing type.</text>
            </description>
          </leaf>
        </list>
      </container>
    </container>
  </augment>
  <augment target-node="/devm:devm/devm:lpu-boards/devm:lpu-board">
    <description>
      <text>PBR.</text>
    </description>
    <container name="pbr">
      <description>
        <text>Configure to filter default route or black hole route when using redirect.</text>
      </description>
      <container name="routing-filters">
        <description>
          <text>List of to filter default route or black hole route when using redirect.</text>
        </description>
        <list name="routing-filter">
          <key value="type"/>
          <max-elements value="2"/>
          <description>
            <text>Configure action of redirecting to public-network.</text>
          </description>
          <leaf name="type">
            <type name="ip-type"/>
            <description>
              <text>Protocol family.</text>
            </description>
          </leaf>
          <leaf name="routing-type">
            <type name="routing-filter-type"/>
            <mandatory value="true"/>
            <description>
              <text>Routing type.</text>
            </description>
          </leaf>
        </list>
      </container>
    </container>
  </augment>
  <augment target-node="/qos:qos/qos:classifier-template/qos:traffic-behaviors/qos:traffic-behavior">
    <description>
      <text>Traffic behavior.</text>
    </description>
    <container name="pbr">
      <description>
        <text>Configure traffic behavior redirect.</text>
      </description>
      <container name="redirect-vpn-group">
        <must condition="not(../pbr-ipv4/redirect-tunnel or ../pbr-ipv4/redirect-mldp-tunnel&#10;or ../pbr-ipv4/redirect-public-network or ../pbr-ipv4/redirect-srv6-te-policy or ../pbr-ipv4/redirect-to-interface/if-name&#10;or ../pbr-ipv4/ipv4-nexthop or (count(../pbr-ipv4/ipv4-multinhp-interface/multi-nhp-interfaces/multi-nhp-interface)&gt;0)&#10;or (count(../pbr-ipv4/ipv4-multinhp/multi-nhps/multi-nhp)&gt;0) or (count(../pbr-ipv4/ipv4-multinhp-vpn/multi-vpns/multi-vpn)&gt;0)&#10;or (count(../pbr-ipv4/ipv4-multinhp-nqa/multi-nqas/multi-nqa)&gt;0) )"/>
        <must condition="not( ../pbr-ipv6/ipv6-redirect-public-network or ../pbr-ipv6/ipv6-redirect-to-interface/if-name&#10;or ../pbr-ipv6/ipv6-nexthop or (count(../pbr-ipv6/ipv6-multinhp-interface/multi-nhp-interfaces/multi-nhp-interface)&gt;0)&#10;or (count(../pbr-ipv6/ipv6-multinhp/multi-nhps/multi-nhp)&gt;0) or (count(../pbr-ipv6/ipv6-multinhp-vpn/multi-vpns/multi-vpn)&gt;0)&#10;or (count(../pbr-ipv6/ipv6-multinhp-nqa/multi-nqas/multi-nqa)&gt;0) )"/>
        <presence value="create redirect-vpn-group"/>
        <description>
          <text>Enable/disable action of redirecting to the VPN-instance group.</text>
        </description>
        <leaf name="vpn-group-name">
          <type name="leafref">
            <path value="/qos:qos/qos:global-config/pbr:pbr/pbr:global-vpn-groups/pbr:global-vpn-group/pbr:vpn-group-name"/>
          </type>
          <mandatory value="true"/>
          <description>
            <text>Name of a VPN-instance group.</text>
          </description>
        </leaf>
      </container>
      <container name="redirect-lsp">
        <must condition="not(../redirect-vpn-group or ../pbr-ipv4/redirect-tunnel or ../pbr-ipv4/redirect-mldp-tunnel&#10;or ../pbr-ipv4/redirect-public-network or ../pbr-ipv4/redirect-srv6-te-policy or ../pbr-ipv4/redirect-to-interface/if-name&#10;or ../pbr-ipv4/ipv4-nexthop or (count(../pbr-ipv4/ipv4-multinhp-interface/multi-nhp-interfaces/multi-nhp-interface)&gt;0)&#10;or (count(../pbr-ipv4/ipv4-multinhp/multi-nhps/multi-nhp)&gt;0) or (count(../pbr-ipv4/ipv4-multinhp-vpn/multi-vpns/multi-vpn)&gt;0)&#10;or (count(../pbr-ipv4/ipv4-multinhp-nqa/multi-nqas/multi-nqa)&gt;0))"/>
        <presence value="create redirect-lsp"/>
        <description>
          <text>Enable/disable action of redirecting to the public network LSP.</text>
        </description>
        <leaf name="target-address">
          <type name="inet:ipv4-address-no-zone"/>
          <mandatory value="true"/>
          <description>
            <text>Destination address.</text>
          </description>
        </leaf>
        <choice name="property">
          <description>
            <text>LSP other property, such as nexthop-address, if-name, secondary.</text>
          </description>
          <case name="nexthop-address">
            <description>
              <text>Next hop address.</text>
            </description>
            <leaf name="nexthop-address">
              <type name="inet:ipv4-address-no-zone"/>
              <description>
                <text>Next hop address.</text>
              </description>
            </leaf>
          </case>
          <case name="if-name">
            <description>
              <text>Outbound interface name.</text>
            </description>
            <leaf name="if-name">
              <type name="leafref">
                <path value="/ifm:ifm/ifm:interfaces/ifm:interface/ifm:name"/>
              </type>
              <description>
                <text>Outbound interface name.</text>
              </description>
            </leaf>
          </case>
          <case name="secondary">
            <description>
              <text>Secondary LSP.</text>
            </description>
            <leaf name="secondary">
              <type name="empty"/>
              <description>
                <text>Secondary LSP.</text>
              </description>
            </leaf>
          </case>
        </choice>
      </container>
      <container name="pbr-ipv4">
        <description>
          <text>Configure action of redirecting to IPv4.</text>
        </description>
        <choice name="pbr-ipv4">
          <description>
            <text>Action of redirecting to IPv4.</text>
          </description>
          <case name="redirect-tunnel">
            <description>
              <text>Action of redirecting to P2P (RSVP-TE, SR-TE) and P2MP Tunnel.</text>
            </description>
            <container name="redirect-tunnel">
              <must condition="not( ../../pbr-ipv6/ipv6-redirect-public-network or ../../pbr-ipv6/ipv6-redirect-to-interface/if-name&#10;or ../../pbr-ipv6/ipv6-nexthop or (count(../../pbr-ipv6/ipv6-multinhp-interface/multi-nhp-interfaces/multi-nhp-interface)&gt;0)&#10;or (count(../../pbr-ipv6/ipv6-multinhp/multi-nhps/multi-nhp)&gt;0) or (count(../../pbr-ipv6/ipv6-multinhp-vpn/multi-vpns/multi-vpn)&gt;0)&#10;or (count(../../pbr-ipv6/ipv6-multinhp-nqa/multi-nqas/multi-nqa)&gt;0) )"/>
              <presence value="create redirect-tunnel"/>
              <description>
                <text>Enable/disable action of redirecting to P2P (RSVP-TE, SR-TE) and P2MP Tunnel.</text>
              </description>
              <leaf name="tunnel-name">
                <type name="leafref">
                  <path value="/ifm:ifm/ifm:interfaces/ifm:interface/ifm:name"/>
                </type>
                <must condition="/ifm:ifm/ifm:interfaces/ifm:interface[ifm:name=current()]/tnlm:tunnel-protocol/mpls-te:te-tunnel/mpls-te:common-attributes/mpls-te:signal-protocol='rsvp-te' or /ifm:ifm/ifm:interfaces/ifm:interface[ifm:name=current()]/tnlm:tunnel-protocol/mpls-te:te-tunnel/mpls-te:common-attributes/mpls-te:signal-protocol='segment-routing'"/>
                <mandatory value="true"/>
                <description>
                  <text>Name of a tunnel.</text>
                </description>
              </leaf>
              <leaf name="route-forward">
                <type name="boolean"/>
                <default value="false"/>
                <description>
                  <text>Enable/disable route forward, Support P2P (RSVP-TE, SR-TE) Tunnel.</text>
                </description>
              </leaf>
            </container>
          </case>
          <case name="redirect-mldp-tunnel">
            <description>
              <text>Action of redirecting to Mldps.</text>
            </description>
            <container name="redirect-mldp-tunnel">
              <presence value="create redirect-mldp-tunnel"/>
              <description>
                <text>Enable/disable action of redirecting to Mldps.</text>
              </description>
              <leaf name="mldp-tunnel-name">
                <type name="leafref">
                  <path value="/mpls:mpls/mpls-ldp:ldp/mpls-ldp:mldp/mpls-ldp:p2mp/mpls-ldp:tunnels/mpls-ldp:tunnel/mpls-ldp:name"/>
                </type>
                <mandatory value="true"/>
                <description>
                  <text>Name of a MLDP tunnel.</text>
                </description>
              </leaf>
            </container>
          </case>
          <case name="redirect-public-network">
            <description>
              <text>Action of redirecting to IPv4 public-network.</text>
            </description>
            <container name="redirect-public-network">
              <presence value="create redirect-public-network"/>
              <description>
                <text>Enable/disable action of redirecting to public-network.</text>
              </description>
            </container>
          </case>
          <case name="redirect-srv6-te-policy">
            <description>
              <text>Action of redirecting to SRv6-te policy.</text>
            </description>
            <container name="redirect-srv6-te-policy">
              <must condition="not( ../../pbr-ipv6/ipv6-redirect-public-network or ../../pbr-ipv6/ipv6-redirect-to-interface/if-name&#10;or ../../pbr-ipv6/ipv6-nexthop or (count(../../pbr-ipv6/ipv6-multinhp-interface/multi-nhp-interfaces/multi-nhp-interface)&gt;0)&#10;or (count(../../pbr-ipv6/ipv6-multinhp/multi-nhps/multi-nhp)&gt;0) or (count(../../pbr-ipv6/ipv6-multinhp-vpn/multi-vpns/multi-vpn)&gt;0)&#10;or (count(../../pbr-ipv6/ipv6-multinhp-nqa/multi-nqas/multi-nqa)&gt;0) )"/>
              <presence value="create srv6-te-policy"/>
              <description>
                <text>Enable/disable action of redirecting to SRv6-te policy.</text>
              </description>
              <leaf name="endpoint">
                <type name="inet:ipv6-address-no-zone"/>
                <mandatory value="true"/>
                <description>
                  <text>Endpoint.</text>
                </description>
              </leaf>
              <leaf name="color">
                <type name="uint32">
                  <range value="0..4294967295"/>
                </type>
                <mandatory value="true"/>
                <description>
                  <text>Color.</text>
                </description>
              </leaf>
              <leaf name="sid-type">
                <type name="sid-type"/>
                <mandatory value="true"/>
                <description>
                  <text>SID type.</text>
                </description>
              </leaf>
              <leaf name="sid-ip">
                <type name="inet:ipv6-address-no-zone"/>
                <mandatory value="true"/>
                <description>
                  <text>SID IP.</text>
                </description>
              </leaf>
            </container>
          </case>
          <case name="redirect-to-interface">
            <description>
              <text>Action of redirecting to interface.</text>
            </description>
            <container name="redirect-to-interface">
              <description>
                <text>Configure action of redirecting to interface.</text>
              </description>
              <leaf name="if-name">
                <type name="leafref">
                  <path value="/ifm:ifm/ifm:interfaces/ifm:interface/ifm:name"/>
                </type>
                <mandatory value="true"/>
                <description>
                  <text>Outbound interface name.</text>
                </description>
              </leaf>
              <leaf name="route-forward">
                <type name="boolean"/>
                <default value="false"/>
                <description>
                  <text>Enable/disable route forward.</text>
                </description>
              </leaf>
            </container>
          </case>
          <case name="ipv4-nexthop">
            <description>
              <text>Action of redirect to IPv4 nexthop.</text>
            </description>
            <container name="ipv4-nexthop">
              <presence value="create ipv4-nexthop"/>
              <description>
                <text>Enable/disable action of redirect to IPv4 nexthop.</text>
              </description>
              <leaf name="nexthop-address">
                <type name="inet:ipv4-address-no-zone"/>
                <mandatory value="true"/>
                <description>
                  <text>Next hop address.</text>
                </description>
              </leaf>
              <leaf name="common-route-priority">
                <type name="boolean"/>
                <default value="false"/>
                <description>
                  <text>Enable/disable common route priority.</text>
                </description>
              </leaf>
              <choice name="action-type">
                <default value="none"/>
                <description>
                  <text>Action of redirect to IPv4 nexthop type.</text>
                </description>
                <case name="none">
                  <description>
                    <text>Action of redirect to IPv4 nexthop none.</text>
                  </description>
                  <container name="none">
                    <description>
                      <text>Configure action of redirect to IPv4 nexthop none.</text>
                    </description>
                    <leaf name="deny">
                      <type name="boolean"/>
                      <default value="false"/>
                      <description>
                        <text>Enable/disable packets are dropped when routes are invalid.</text>
                      </description>
                    </leaf>
                    <choice name="property">
                      <default value="routing-filter"/>
                      <description>
                        <text>IPv4 property.</text>
                      </description>
                      <case name="vpn-network">
                        <description>
                          <text>Specify VPN network.</text>
                        </description>
                        <leaf name="vpn-network">
                          <type name="empty"/>
                          <description>
                            <text>VPN network.</text>
                          </description>
                        </leaf>
                      </case>
                      <case name="routing-filter">
                        <description>
                          <text>Routing filter.</text>
                        </description>
                        <leaf name="routing-filter">
                          <type name="routing-type"/>
                          <default value="false"/>
                          <description>
                            <text>Routing type.</text>
                          </description>
                        </leaf>
                        <leaf name="public-network">
                          <type name="empty"/>
                          <description>
                            <text>Public network.</text>
                          </description>
                        </leaf>
                      </case>
                    </choice>
                  </container>
                </case>
                <case name="interface">
                  <description>
                    <text>Action of redirect to IPv4 interface.</text>
                  </description>
                  <container name="interface">
                    <presence value="create ipv4-interface"/>
                    <description>
                      <text>Enable/disable action of redirect to IPv4 interface.</text>
                    </description>
                    <leaf name="if-name">
                      <type name="leafref">
                        <path value="/ifm:ifm/ifm:interfaces/ifm:interface/ifm:name"/>
                      </type>
                      <mandatory value="true"/>
                      <description>
                        <text>Outbound interface name.</text>
                      </description>
                    </leaf>
                    <leaf name="route-forward">
                      <type name="boolean"/>
                      <default value="false"/>
                      <description>
                        <text>Enable/disable route forward.</text>
                      </description>
                    </leaf>
                  </container>
                </case>
                <case name="vpn-instance">
                  <description>
                    <text>Private VRF LSP.</text>
                  </description>
                  <container name="vpn-instance">
                    <presence value="create ipv4-vpn"/>
                    <description>
                      <text>Enable/disable action of redirect to VRF LSP.</text>
                    </description>
                    <leaf name="vrf-name">
                      <type name="leafref">
                        <path value="/ni:network-instance/ni:instances/ni:instance/ni:name"/>
                      </type>
                      <must condition="../vrf-name!='_public_'"/>
                      <mandatory value="true"/>
                      <description>
                        <text>VPN name.</text>
                      </description>
                    </leaf>
                    <leaf name="nqa-admin-name">
                      <type name="leafref">
                        <path value="/nqa:nqa/nqa:instances/nqa:instance/nqa:admin-name"/>
                      </type>
                      <must condition="../nqa-instance"/>
                      <description>
                        <text>NQA admin name.</text>
                      </description>
                    </leaf>
                    <leaf name="nqa-instance">
                      <type name="leafref">
                        <path value="/nqa:nqa/nqa:instances/nqa:instance[nqa:admin-name=current()/../nqa-admin-name]/nqa:test-name"/>
                      </type>
                      <must condition="../nqa-admin-name"/>
                      <description>
                        <text>NQA instance.</text>
                      </description>
                    </leaf>
                    <leaf name="routing-filter">
                      <type name="routing-type"/>
