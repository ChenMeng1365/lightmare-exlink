<?xml version="1.0" encoding="UTF-8"?>
<module name="huawei-vxlan-path-detect"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:vxlan-path-detect="urn:huawei:yang:huawei-vxlan-path-detect"
        xmlns:inet="urn:ietf:params:xml:ns:yang:ietf-inet-types"
        xmlns:pub-type="urn:huawei:yang:huawei-pub-type"
        xmlns:ext="urn:huawei:yang:huawei-extension"
        xmlns:ifm="urn:huawei:yang:huawei-ifm"
        xmlns:ni="urn:huawei:yang:huawei-network-instance">
  <yang-version value="1"/>
  <namespace uri="urn:huawei:yang:huawei-vxlan-path-detect"/>
  <prefix value="vxlan-path-detect"/>
  <import module="ietf-inet-types">
    <prefix value="inet"/>
  </import>
  <import module="huawei-pub-type">
    <prefix value="pub-type"/>
  </import>
  <import module="huawei-extension">
    <prefix value="ext"/>
  </import>
  <import module="huawei-ifm">
    <prefix value="ifm"/>
  </import>
  <import module="huawei-network-instance">
    <prefix value="ni"/>
  </import>
  <organization>
    <text>Huawei Technologies Co., Ltd.</text>
  </organization>
  <contact>
    <text>Huawei Industrial Base
Bantian, Longgang
Shenzhen 518129
People's Republic of China
Website: http://www.huawei.com
Email: support@huawei.com</text>
  </contact>
  <description>
    <text>Vxlan oam management, which includes dscp path detection, IP ping, MAC ping, IPv6 ping.</text>
  </description>
  <revision date="2020-06-20">
    <description>
      <text>Add RPC.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <revision date="2020-01-21">
    <description>
      <text>Initial revision.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <ext:task-name value="forwarding"/>
  <typedef name="protocol-type">
    <type name="enumeration">
      <enum name="icmp">
        <value value="1"/>
        <description>
          <text>Protocol type is ICMP.</text>
        </description>
      </enum>
      <enum name="tcp">
        <value value="6"/>
        <description>
          <text>Protocol type is TCP.</text>
        </description>
      </enum>
      <enum name="udp">
        <value value="17"/>
        <description>
          <text>Protocol type is UDP.</text>
        </description>
      </enum>
      <enum name="sctp">
        <value value="132"/>
        <description>
          <text>Protocol type is SCTP.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Protocol type.</text>
    </description>
  </typedef>
  <container name="vxlan-path-detect">
    <description>
      <text>Vxlan path detect.</text>
    </description>
    <container name="ipv4-dscp-path-detect">
      <presence value="Enable IPv4 DSCP detect."/>
      <description>
        <text>Configure IPv4 DSCP path detect.</text>
      </description>
      <leaf name="dscp-value">
        <type name="inet:dscp"/>
        <mandatory value="true"/>
        <description>
          <text>DSCP value.</text>
        </description>
      </leaf>
    </container>
    <container name="ipv6-dscp-path-detect">
      <presence value="Enable IPv6 DSCP detect."/>
      <description>
        <text>Configure IPv6 DSCP path detect.</text>
      </description>
      <leaf name="dscp-value">
        <type name="inet:dscp"/>
        <mandatory value="true"/>
        <description>
          <text>DSCP value.</text>
        </description>
      </leaf>
    </container>
    <container name="ip-ping-detects">
      <description>
        <text>List of IP ping detect.</text>
      </description>
      <list name="ip-ping-detect">
        <key value="src-ip dst-ip"/>
        <description>
          <text>Configure IP ping detect.</text>
        </description>
        <leaf name="src-ip">
          <type name="inet:ipv4-address-no-zone"/>
          <description>
            <text>Source IPv4 address.</text>
          </description>
        </leaf>
        <leaf name="dst-ip">
          <type name="inet:ipv4-address-no-zone"/>
          <description>
            <text>Destination IPv4 address.</text>
          </description>
        </leaf>
      </list>
    </container>
    <container name="mac-ping-detects">
      <description>
        <text>List of MAC ping detect.</text>
      </description>
      <list name="mac-ping-detect">
        <key value="src-ip dst-ip"/>
        <description>
          <text>Configure MAC ping detect.</text>
        </description>
        <leaf name="src-ip">
          <type name="inet:ipv4-address-no-zone"/>
          <description>
            <text>Source IPv4 address.</text>
          </description>
        </leaf>
        <leaf name="dst-ip">
          <type name="inet:ipv4-address-no-zone"/>
          <description>
            <text>Destination IPv4 address.</text>
          </description>
        </leaf>
        <leaf name="src-mac">
          <type name="pub-type:mac-address"/>
          <description>
            <text>Source MAC address.</text>
          </description>
        </leaf>
        <leaf name="dst-mac">
          <type name="pub-type:mac-address"/>
          <description>
            <text>Destination MAC address.</text>
          </description>
        </leaf>
      </list>
    </container>
    <container name="ipv6-ping-detects">
      <description>
        <text>List of IPv6 ping detect.</text>
      </description>
      <list name="ipv6-ping-detect">
        <key value="src-ipv6 dst-ipv6"/>
        <description>
          <text>Configure IPv6 ping detect.</text>
        </description>
        <leaf name="src-ipv6">
          <type name="inet:ipv6-address-no-zone"/>
          <description>
            <text>Source IPv6 address.</text>
          </description>
        </leaf>
        <leaf name="dst-ipv6">
          <type name="inet:ipv6-address-no-zone"/>
          <description>
            <text>Destination IPv6 address.</text>
          </description>
        </leaf>
      </list>
    </container>
  </container>
  <rpc name="path-detection-send-pkt">
    <description>
      <text>Sending one packet when path detection.</text>
    </description>
    <input>
      <leaf name="source-mac">
        <type name="pub-type:mac-address"/>
        <mandatory value="true"/>
        <description>
          <text>Source MAC.</text>
        </description>
      </leaf>
      <leaf name="destination-mac">
        <type name="pub-type:mac-address"/>
        <mandatory value="true"/>
        <description>
          <text>Destination MAC.</text>
        </description>
      </leaf>
      <leaf name="pe-vlan">
        <type name="uint16">
          <range value="1..4094"/>
        </type>
        <description>
          <text>Pe-VLAN.</text>
        </description>
      </leaf>
      <leaf name="priority">
        <when condition="../pe-vlan"/>
        <type name="uint8">
          <range value="0..7"/>
        </type>
        <description>
          <text>IEEE 802.1P encapsulation priority.</text>
        </description>
      </leaf>
      <leaf name="ce-vlan">
        <when condition="../pe-vlan"/>
        <type name="uint16">
          <range value="1..4094"/>
        </type>
        <description>
          <text>Ce-VLAN.</text>
        </description>
      </leaf>
      <leaf name="source-ip">
        <type name="inet:ip-address-no-zone"/>
        <mandatory value="true"/>
        <description>
          <text>Source IP.</text>
        </description>
      </leaf>
      <leaf name="destination-ip">
        <type name="inet:ip-address-no-zone"/>
        <mandatory value="true"/>
        <description>
          <text>Destination IP.</text>
        </description>
      </leaf>
      <leaf name="protocol">
        <type name="protocol-type"/>
        <mandatory value="true"/>
        <description>
          <text>Protocol type.</text>
        </description>
      </leaf>
      <leaf name="source-port">
        <when condition="../protocol='tcp' or ../protocol='udp' or ../protocol='sctp'"/>
        <type name="uint16">
          <range value="1..65535"/>
        </type>
        <mandatory value="true"/>
        <description>
          <text>Source port.</text>
        </description>
      </leaf>
      <leaf name="destination-port">
        <when condition="../protocol='tcp' or ../protocol='udp' or ../protocol='sctp'"/>
        <type name="uint16">
          <range value="1..65535"/>
        </type>
        <mandatory value="true"/>
        <description>
          <text>Destination port.</text>
        </description>
      </leaf>
      <leaf name="gtp-teid">
        <when condition="../protocol='udp' and (../source-port=2152 or ../destination-port=2152)"/>
        <type name="uint32"/>
        <description>
          <text>GTP TEID.</text>
        </description>
      </leaf>
      <leaf name="dscp">
        <type name="inet:dscp"/>
        <mandatory value="true"/>
        <description>
          <text>DSCP value.</text>
        </description>
      </leaf>
      <leaf name="vpn-instance-name">
        <type name="leafref">
          <path value="/ni:network-instance/ni:instances/ni:instance/ni:name"/>
        </type>
        <description>
          <text>VPN instance name.</text>
        </description>
      </leaf>
      <leaf name="interface-name">
        <type name="leafref">
          <path value="/ifm:ifm/ifm:interfaces/ifm:interface/ifm:name"/>
        </type>
        <mandatory value="true"/>
        <description>
          <text>Interface name.</text>
        </description>
      </leaf>
      <leaf name="test-id">
        <type name="uint32"/>
        <mandatory value="true"/>
        <description>
          <text>Test ID.</text>
        </description>
      </leaf>
    </input>
  </rpc>
</module>
