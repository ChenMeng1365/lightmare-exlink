                  <ext:default-value value="1024">
                    <when condition="../group = 'whitelist'"/>
                    <description>
                      <text>The default value is 1024 when type is whitelist.</text>
                    </description>
                  </ext:default-value>
                  <ext:default-value value="100">
                    <when condition="../group = 'system-message'"/>
                    <description>
                      <text>The default value is 100 when type is system-message.</text>
                    </description>
                  </ext:default-value>
                  <ext:default-value value="0">
                    <description>
                      <text>The default value is 0 when type is others.</text>
                    </description>
                  </ext:default-value>
                </ext:dynamic-default>
                <description>
