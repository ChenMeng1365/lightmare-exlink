                                                }
                                            }
                                        },
                                        "enable": {
                                            "description": "To configure the TELNET server enable.",
                                            "properties": {
                                                "enable": {
                                                    "description": "Enable TELNET server.",
                                                    "type": "string"
                                                },
                                                "listen-port": {
                                                    "description": "The listenning port of the TELNET server. It can be configured as 23 or any integer between 49152 and 65535. It cannot be changed while TELNET server has been enabled.",
                                                    "type": "string"
                                                }
                                            }
                                        }
                                    }
                                },
                                "dscp": {
                                    "description": "The DSCP value is used in packets of the TELNET server. It can be configured any integer between 0 and 63.",
                                    "type": "string"
                                },
                                "idle-timeout": {
                                    "description": "Telnet maximum idle time in minutes(0-1000).Distinguish project,support performance parameter.",
                                    "type": "string"
                                },
                                "absolute-timeout": {
                                    "description": "Telnet maximum online time in minutes(0-10000).Distinguish project,support performance parameter.",
                                    "type": "string"
                                },
                                "max-link": {
                                    "description": "Maximum number of telnet links.",
                                    "type": "string"
                                },
                                "ipv4-acl": {
                                    "description": "The IPv4 ACL name which is binded to the TELNET server.",
                                    "type": "string"
                                },
                                "ipv6-acl": {
                                    "description": "The IPv6 ACL name which is binded to the TELNET server.",
                                    "type": "string"
                                },
                                "max-connection": {
                                    "description": "Maximum number of telnet connections.",
                                    "type": "string"
                                }
                            }
                        },
                        "console": {
                            "description": "Configure the console parameters.",
                            "properties": {
                                "idle-timeout": {
                                    "description": "Console maximum idle time in minutes(0-1000).\n\nDistinguish project,support performance parameter.",
                                    "type": "string"
                                },
                                "absolute-timeout": {
                                    "description": "Console maximum absolute time in minutes(0-10000).\n\nDistinguish project,support performance parameter.",
