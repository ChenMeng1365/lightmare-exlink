            <leaf name="name">
              <type name="string">
                <length value="0..63"/>
              </type>
              <config value="false"/>
              <description>
                <text>The feature information name of a license file.</text>
              </description>
            </leaf>
            <leaf name="product">
              <type name="string">
                <length value="0..63"/>
              </type>
              <config value="false"/>
              <description>
                <text>The product information of a license file.</text>
              </description>
            </leaf>
            <leaf name="esn">
              <type name="string">
                <length value="0..2055"/>
              </type>
              <config value="false"/>
              <description>
                <text>The ESN of a license file.</text>
              </description>
            </leaf>
            <leaf name="auth-type">
              <type name="auth-type"/>
              <config value="false"/>
              <description>
                <text>The authorizing type of a license file.</text>
              </description>
            </leaf>
            <leaf name="running-deadline">
              <type name="pub-type:date"/>
              <config value="false"/>
              <description>
                <text>The running deadline of a license file.</text>
              </description>
            </leaf>
            <leaf name="trial-days">
              <type name="uint32"/>
              <config value="false"/>
              <description>
                <text>List of the trial days of a license file.</text>
              </description>
            </leaf>
            <leaf name="software-deadline">
              <type name="pub-type:date"/>
              <config value="false"/>
              <description>
                <text>The software deadline of a license file.</text>
              </description>
            </leaf>
            <leaf name="hardware-deadline">
              <type name="pub-type:date"/>
              <config value="false"/>
              <description>
                <text>The hardware deadline of a license file.</text>
              </description>
            </leaf>
            <leaf name="upgrate-deadline">
              <type name="pub-type:date"/>
              <config value="false"/>
              <description>
                <text>The upgrate deadline of a license file.</text>
              </description>
            </leaf>
            <container name="feature-items">
              <config value="false"/>
              <description>
                <text>List of all feature items of a license file.</text>
              </description>
              <list name="feature-item">
                <key value="name"/>
                <config value="false"/>
                <description>
                  <text>Operational state of feature item of a license file.</text>
                </description>
                <leaf name="name">
                  <type name="string">
                    <length value="0..63"/>
                  </type>
                  <config value="false"/>
                  <description>
                    <text>Feature item name of a license file.</text>
                  </description>
                </leaf>
                <leaf name="value">
                  <type name="uint32"/>
                  <config value="false"/>
                  <description>
                    <text>Feature item value of a license file.</text>
                  </description>
                </leaf>
              </list>
            </container>
          </list>
        </container>
      </list>
    </container>
    <container name="resource-usages">
      <config value="false"/>
      <description>
        <text>List of all license resource usages.</text>
      </description>
      <list name="resource-usage">
        <key value="name"/>
        <config value="false"/>
        <description>
          <text>Operational state of license resource usage.</text>
        </description>
        <leaf name="name">
          <type name="string">
            <length value="0..31"/>
          </type>
          <config value="false"/>
          <description>
            <text>The name of resource item.</text>
          </description>
        </leaf>
        <leaf name="usage">
          <type name="uint32"/>
          <config value="false"/>
          <description>
            <text>The usage of resource item.</text>
          </description>
        </leaf>
        <leaf name="active-value">
          <type name="uint32"/>
          <config value="false"/>
          <description>
            <text>The active value of resource item.</text>
          </description>
        </leaf>
      </list>
    </container>
    <container name="resource-thresholds">
      <config value="false"/>
      <description>
        <text>List of license resource usage thresholds.</text>
      </description>
      <list name="resource-threshold">
        <key value="name"/>
        <config value="false"/>
        <description>
          <text>Operational state of license resource usage threshold.</text>
        </description>
        <leaf name="name">
          <type name="string">
            <length value="0..31"/>
          </type>
          <config value="false"/>
          <description>
            <text>Name of a license resource control item.</text>
          </description>
        </leaf>
        <leaf name="threshold">
          <type name="uint32">
            <range value="50..95"/>
          </type>
          <config value="false"/>
          <description>
            <text>Usage threshold of a license control resource item.</text>
          </description>
        </leaf>
        <leaf name="configurable-switch">
          <type name="uint32">
            <range value="0..1"/>
          </type>
          <default value="0"/>
          <config value="false"/>
          <description>
            <text>Configurable flag of a license resource item.</text>
          </description>
        </leaf>
        <leaf name="description">
          <type name="string">
            <length value="0..255"/>
          </type>
          <config value="false"/>
          <description>
            <text>Description of a license resource control item.</text>
          </description>
        </leaf>
      </list>
    </container>
    <container name="cloud-servers">
      <description>
        <text>List of cloud license servers.</text>
      </description>
      <list name="cloud-server">
        <key value="server-ip server-port"/>
        <max-elements value="1"/>
        <description>
          <text>Configure cloud license server.</text>
        </description>
        <leaf name="server-ip">
          <type name="inet:ipv4-address-no-zone"/>
          <description>
            <text>The IP address of cloud license server.</text>
          </description>
        </leaf>
        <leaf name="server-port">
          <type name="uint32"/>
          <description>
            <text>The port of cloud license server.</text>
          </description>
        </leaf>
        <leaf name="register-state">
          <type name="register-state"/>
          <config value="false"/>
          <description>
            <text>The register state with cloud license server.</text>
          </description>
        </leaf>
        <leaf name="policy-name">
          <type name="string">
            <length value="1..255"/>
          </type>
          <config value="false"/>
          <description>
            <text>The policy name.</text>
          </description>
        </leaf>
      </list>
    </container>
  </container>
  <rpc name="license-active">
    <description>
      <text>To active a license file.</text>
    </description>
    <input>
      <leaf name="filename">
        <type name="string">
          <length value="5..127"/>
        </type>
        <description>
          <text>The name of a license file.</text>
        </description>
      </leaf>
    </input>
  </rpc>
  <rpc name="license-revoke">
    <description>
      <text>To revoke a license file.</text>
    </description>
  </rpc>
  <rpc name="license-verify">
    <description>
      <text>To verify a license file.</text>
    </description>
    <ext:node-ref value="/lcs"/>
    <input>
      <leaf name="filename">
        <type name="string">
          <length value="5..127"/>
        </type>
        <description>
          <text>The name of a license file.</text>
        </description>
      </leaf>
    </input>
  </rpc>
  <rpc name="license-revoke-chassis">
    <description>
      <text>To revoke a license file by device ID.</text>
    </description>
    <input>
      <leaf name="chassis-id">
        <type name="string">
          <length value="1..31"/>
        </type>
        <description>
          <text>The device ID of revoke license action.</text>
        </description>
      </leaf>
    </input>
  </rpc>
  <rpc name="license-delete">
    <description>
      <text>To delete a license file in $_license.</text>
    </description>
    <input>
      <leaf name="filename">
        <type name="string">
          <length value="1..127"/>
        </type>
        <description>
          <text>The name of a license file.</text>
        </description>
      </leaf>
    </input>
  </rpc>
  <rpc name="license-trial">
    <description>
      <text>Enable or disable the trial of a specific control item.</text>
    </description>
    <input>
      <leaf name="trial-switch">
        <type name="boolean"/>
        <mandatory value="true"/>
        <description>
          <text>Whether to enable or disable the trial function of a control item.</text>
        </description>
      </leaf>
    </input>
  </rpc>
  <rpc name="license-export">
    <description>
      <text>Export the license to a file.</text>
    </description>
    <input>
      <leaf name="filename">
        <type name="string">
          <length value="5..127"/>
        </type>
        <description>
          <text>The file name of exporting license.</text>
        </description>
      </leaf>
    </input>
  </rpc>
  <rpc name="startup-paf">
    <description>
      <text>Configure an existing PAF file as the startup PAF file.</text>
    </description>
    <input>
      <leaf name="filename">
        <type name="string">
          <length value="5..48"/>
        </type>
        <mandatory value="true"/>
        <description>
          <text>Name of a PAF file.</text>
        </description>
      </leaf>
    </input>
  </rpc>
  <rpc name="license-emergency">
    <description>
      <text>To activate license emergency mode.</text>
    </description>
  </rpc>
  <rpc name="license-assign">
    <description>
      <text>Assign cloud license.</text>
    </description>
    <input>
      <leaf name="item-name">
        <type name="string">
          <length value="1..31"/>
        </type>
        <description>
          <text>License item.</text>
        </description>
      </leaf>
      <leaf name="item-num">
        <type name="uint32"/>
        <description>
          <text>License number.</text>
        </description>
      </leaf>
    </input>
  </rpc>
  <rpc name="license-switch-mode">
    <description>
      <text>Switch license mode.</text>
    </description>
    <input>
      <leaf name="license-mode">
        <type name="mode-type"/>
        <description>
          <text>License mode.</text>
        </description>
      </leaf>
    </input>
  </rpc>
</module>
