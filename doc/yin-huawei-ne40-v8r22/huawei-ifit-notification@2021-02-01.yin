<?xml version="1.0" encoding="UTF-8"?>
<module name="huawei-ifit-notification"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:ifit-notification="urn:huawei:yang:huawei-ifit-notification"
        xmlns:ext="urn:huawei:yang:huawei-extension">
  <yang-version value="1"/>
  <namespace uri="urn:huawei:yang:huawei-ifit-notification"/>
  <prefix value="ifit-notification"/>
  <import module="huawei-extension">
    <prefix value="ext"/>
  </import>
  <organization>
    <text>Huawei Technologies Co., Ltd.</text>
  </organization>
  <contact>
    <text>Huawei Industrial Base
Bantian, Longgang
Shenzhen 518129
People's Republic of China
Website: http://www.huawei.com
Email: support@huawei.com</text>
  </contact>
  <description>
    <text>Notification of ifit.</text>
  </description>
  <revision date="2021-02-01">
    <description>
      <text>Initial revision.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <ext:task-name value="ifit"/>
  <notification name="dynflow-ingress-exceed">
    <description>
      <text>The number of iFIT dynamic ingress flow reaches the upper limit.</text>
    </description>
    <leaf name="upper-limit">
      <type name="uint32"/>
      <description>
        <text>Total dynamic ingress flow number upper limit of iFIT.</text>
      </description>
    </leaf>
  </notification>
  <notification name="dynflow-ingress-resume">
    <description>
      <text>The number of iFIT dynamic ingress flow is below the upper limit.</text>
    </description>
    <leaf name="upper-limit">
      <type name="uint32"/>
      <description>
        <text>Total dynamic ingress flow number upper limit of iFIT.</text>
      </description>
    </leaf>
  </notification>
  <notification name="dynflow-midegress-exceed">
    <description>
      <text>The number of iFIT dynamic midegress flow reaches the upper limit.</text>
    </description>
    <leaf name="upper-limit">
      <type name="uint32"/>
      <description>
        <text>Total dynamic midegress flow number upper limit of iFIT.</text>
      </description>
    </leaf>
  </notification>
  <notification name="dynflow-midegress-resume">
    <description>
      <text>The number of iFIT dynamic midegress flow is below the upper limit.</text>
    </description>
    <leaf name="upper-limit">
      <type name="uint32"/>
      <description>
        <text>Total dynamic midegress flow number upper limit of iFIT.</text>
      </description>
    </leaf>
  </notification>
  <notification name="ingress-exceed">
    <description>
      <text>The number of iFIT ingress flow reaches the upper limit.</text>
    </description>
    <leaf name="upper-limit">
      <type name="uint32"/>
      <description>
        <text>Total ingress flow number upper limit of iFIT.</text>
      </description>
    </leaf>
  </notification>
  <notification name="ingress-resume">
    <description>
      <text>The number of iFIT ingress flow is below the upper limit.</text>
    </description>
    <leaf name="upper-limit">
      <type name="uint32"/>
      <description>
        <text>Total ingress flow number upper limit of iFIT.</text>
      </description>
    </leaf>
  </notification>
</module>
