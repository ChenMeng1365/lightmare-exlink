    </description>
    <input>
      <leaf name="ip-pool-name">
        <type name="string">
          <length value="1..128"/>
        </type>
        <description>
          <text>IP pool name.</text>
        </description>
      </leaf>
    </input>
  </rpc>
  <rpc name="cut-access-user-by-user-id">
    <description>
      <text>Cut the connection with a user with a specified ID or a specified user ID range.</text>
    </description>
    <input>
      <leaf name="start-user-id">
        <type name="uint32"/>
        <mandatory value="true"/>
        <description>
          <text>Specifies the start index number of the access user.</text>
        </description>
      </leaf>
      <leaf name="end-user-id">
        <type name="uint32"/>
        <must condition="../end-user-id &gt;= ../start-user-id"/>
        <description>
          <text>Specifies the end index number of the access user.</text>
        </description>
      </leaf>
    </input>
  </rpc>
  <rpc name="filter-access-tables">
    <description>
      <text>List of the user access statistics tables.</text>
    </description>
    <input>
      <leaf name="up-id">
        <type name="int32">
          <range value="1024..2023 | 2048..3047 | 3072..4071 | 4096..5095"/>
        </type>
        <description>
          <text>UP ID.</text>
        </description>
      </leaf>
      <leaf name="slot">
        <type name="string">
          <length value="1..15"/>
        </type>
        <description>
          <text>Slot Number.</text>
        </description>
      </leaf>
      <leaf name="interface">
        <type name="string">
          <length value="1..63"/>
        </type>
        <description>
          <text>User access interface.</text>
        </description>
