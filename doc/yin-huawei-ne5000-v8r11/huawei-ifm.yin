  <container name="ifm">
    <description>
      <text>Common interface management. It includes the public configuration of interfaces.</text>
    </description>
    <container name="global">
      <description>
        <text>Configure globally configured attributes.</text>
      </description>
      <leaf name="statistic-interval">
        <type name="uint32">
          <range value="10..600"/>
        </type>
        <units name="s"/>
        <must condition="(../statistic-interval) mod 10 = 0"/>
        <default value="300"/>
        <description>
          <text>Set the interval at which global flow statistics are collected.</text>
        </description>
      </leaf>
      <leaf name="ipv4-ignore-primary-sub">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable the function of ignoring primary/secondary of IPv4 address type. All secondary addresses must be deleted before this function is enabled.</text>
        </description>
      </leaf>
      <leaf name="sub-interface-link-trap-enable">
        <type name="boolean"/>
        <default value="true"/>
        <description>
          <text>Enable/disable the trap function on all Sub-interface.</text>
        </description>
      </leaf>
      <container name="ipv4-conflict-enable">
        <must condition="../ipv4-ignore-primary-sub='false'"/>
        <presence value="Enable/disable IPv4 conflict function."/>
        <description>
          <text>Enable/disable ip conflict function.</text>
        </description>
        <leaf name="preempt-enable">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable IPv4 address conflict preemption.</text>
          </description>
        </leaf>
      </container>
      <container name="ipv6-conflict-enable">
        <presence value="Enable/disable IPv6 conflict function."/>
        <description>
          <text>Enable/disable IPv6 conflict function.</text>
        </description>
        <leaf name="preempt-enable">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable IPv6 address conflict preemption.</text>
          </description>
        </leaf>
      </container>
    </container>
    <container name="damp">
      <presence value="Enable/disable suppression on interfaces' physical status flappings."/>
      <description>
        <text>Enable/disable suppression on interfaces' physical status flappings.</text>
      </description>
      <leaf name="tx-off">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable interfaces from sending signals if the interfaces' physical status flappings are suppressed.</text>
        </description>
      </leaf>
      <choice name="level">
        <default value="auto"/>
        <description>
          <text>Interface physical status damping level.</text>
        </description>
        <case name="auto">
          <description>
            <text>Auto damping level.</text>
          </description>
          <container name="auto">
            <description>
              <text>Configure an auto suppression level.</text>
            </description>
            <leaf name="level">
              <type name="damp-level-type"/>
              <default value="light"/>
              <description>
                <text>Auto damping level.</text>
              </description>
            </leaf>
          </container>
        </case>
        <case name="manual">
          <description>
            <text>Manual damping level.</text>
          </description>
          <container name="manual">
            <description>
              <text>Configure a manual suppression level.</text>
            </description>
            <leaf name="suppress">
              <type name="uint32">
                <range value="1..20000"/>
              </type>
              <must condition="(../suppress&gt;../reuse)"/>
              <mandatory value="true"/>
              <description>
                <text>1000 times of suppress threshold.</text>
              </description>
            </leaf>
            <leaf name="reuse">
              <type name="uint32">
                <range value="1..20000"/>
              </type>
              <must condition="(../reuse&lt;../suppress)"/>
              <mandatory value="true"/>
              <description>
                <text>1000 times of reuse threshold.</text>
              </description>
            </leaf>
            <leaf name="max-suppress-time">
              <type name="uint16">
                <range value="1..255"/>
              </type>
              <units name="s"/>
              <mandatory value="true"/>
              <description>
                <text>Max suppress time.</text>
              </description>
            </leaf>
            <leaf name="half-life-period">
              <type name="uint16">
                <range value="1..60"/>
              </type>
              <units name="s"/>
              <mandatory value="true"/>
              <description>
                <text>Half life time.</text>
              </description>
            </leaf>
          </container>
        </case>
      </choice>
    </container>
    <container name="auto-recovery-times">
      <description>
        <text>List of automatic recovery time configuration.</text>
      </description>
      <list name="auto-recovery-time">
        <key value="error-down-type"/>
        <description>
          <text>Configure automatic recovery time.</text>
        </description>
        <leaf name="error-down-type">
          <type name="error-down-type"/>
          <description>
            <text>Cause of the error-down event.</text>
          </description>
        </leaf>
        <leaf name="time-value">
          <type name="uint32">
            <range value="30..86400"/>
          </type>
          <units name="s"/>
          <mandatory value="true"/>
          <description>
            <text>Delay for the status transition from down to up.</text>
          </description>
        </leaf>
      </list>
    </container>
    <container name="interfaces">
      <description>
        <text>List of configuring information on an interface.</text>
      </description>
      <list name="interface">
        <key value="name"/>
        <description>
          <text>Configure information on an interface. Physical, NULL, Virtual-if, and Virtual-Template0 interfaces cannot be created or deleted.</text>
        </description>
        <leaf name="name">
          <type name="pub-type:if-name"/>
          <description>
            <text>The textual name of the interface. It should be the name of the interface as assigned by the local device. It should be suitable for use in commands which entered at the device's 'console'. This might be a text name, such as 'NULL0', depending on the interface naming syntax of the device.</text>
          </description>
        </leaf>
        <leaf name="class">
          <ext:operation-exclude value="update|delete"/>
          <type name="class-type"/>
          <ext:dynamic-default/>
          <description>
            <text>Identify a main interface or a sub-interface.</text>
          </description>
          <ext:support-filter value="true"/>
        </leaf>
        <leaf name="type">
          <ext:operation-exclude value="update|delete"/>
          <type name="port-type"/>
          <ext:dynamic-default/>
          <description>
            <text>Type of an interface. Interfaces include physical and logical interfaces.</text>
          </description>
          <ext:support-filter value="true"/>
        </leaf>
        <leaf name="parent-name">
          <ext:operation-exclude value="update|delete"/>
          <when condition="../class='sub-interface'"/>
          <type name="leafref">
            <path value="/ifm:ifm/ifm:interfaces/ifm:interface/ifm:name"/>
          </type>
          <ext:dynamic-default/>
          <description>
            <text>Name of the main interface. For example, Ethernet0/1/0.</text>
          </description>
        </leaf>
        <leaf name="number">
          <ext:operation-exclude value="update|delete"/>
          <type name="string">
            <length value="1..63"/>
            <pattern value="(\d+/\d+/\d+/\d+)|(\d+/\d+/\d+)|(\d+/\d+)|(\d+)|(\d+/\d+/\d+[:]\d+)|(\d+/\d+/\d+[:]\d+[:]\d+[:]\d+)|(\d+/\d+/\d+/\d+[:]\d+)|(\d+/\d+/\d+/\d+[:]\d+[:]\d+[:]\d+)|(\d+/\d+/\d+/\d+/\d+[:]\d+)|(\d+/\d+[:]\d+)"/>
          </type>
          <ext:dynamic-default/>
          <description>
            <text>Number of an interface. For example,1, 0/1/0, or 2:1.</text>
          </description>
        </leaf>
        <leaf name="description">
          <type name="string">
            <length value="1..242"/>
          </type>
          <description>
            <text>Description of an interface.</text>
          </description>
        </leaf>
        <leaf name="admin-status">
          <type name="port-status"/>
          <ext:dynamic-default/>
          <description>
            <text>Administrative status of an interface. Capabilities supported by this node vary according to interface types.</text>
          </description>
        </leaf>
        <leaf name="link-protocol">
          <type name="link-protocol"/>
          <ext:dynamic-default/>
          <description>
            <text>Link protocol. Capabilities supported by this node vary according to interface types.</text>
          </description>
        </leaf>
        <leaf name="router-type">
          <ext:operation-exclude value="update|delete"/>
          <type name="router-type"/>
          <ext:dynamic-default/>
          <description>
            <text>Route attribute of an interface. Capabilities supported by this node vary according to interface types.</text>
          </description>
        </leaf>
        <leaf name="clear-ip-df">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable a device to forcibly fragment IP packets on an outbound interface. Capabilities supported by this node vary according to interface types.</text>
          </description>
        </leaf>
        <leaf name="link-up-down-trap-enable">
          <type name="boolean"/>
          <default value="true"/>
          <description>
            <text>Enable/disable the trap function on an interface. Capabilities supported by this node vary according to interface types.</text>
          </description>
        </leaf>
        <leaf name="statistic-enable">
          <type name="boolean"/>
          <ext:dynamic-default/>
          <description>
            <text>Enable/disable the statistics function on an interface. The default value of this node varies according to different interface types. Capabilities supported by this node vary according to interface types.</text>
          </description>
        </leaf>
        <leaf name="statistic-mode">
          <type name="statistic-mode"/>
          <must condition="../statistic-mode = 'interface-based' or (../l2-mode-enable = 'true' and ../statistic-enable = 'true')"/>
          <default value="interface-based"/>
          <description>
            <text>Mode of statistics collection. Capabilities supported by this node vary according to interface types.</text>
          </description>
        </leaf>
        <choice name="bandwidth-type">
          <description>
            <text>Configure the bandwidth of an interface.</text>
          </description>
          <case name="bandwidth-mbps">
            <description>
              <text>Configure the bandwidth of an interface.</text>
            </description>
            <leaf name="bandwidth">
              <type name="uint32">
                <range value="1..1000000"/>
              </type>
              <units name="Mbit/s"/>
              <description>
                <text>Configure the bandwidth of an interface. Capabilities supported by this node vary according to interface types.</text>
              </description>
            </leaf>
          </case>
          <case name="bandwidth-kbps">
            <description>
              <text>Configure the bandwidth of an interface.</text>
            </description>
            <leaf name="bandwidth-kbps">
              <type name="uint32">
                <range value="1..1000000"/>
              </type>
              <units name="kbit/s"/>
              <description>
                <text>Configure the bandwidth of an interface. Capabilities supported by this node vary according to interface types.</text>
              </description>
            </leaf>
          </case>
        </choice>
        <leaf name="mtu">
          <type name="uint32">
            <range value="0..64000"/>
          </type>
          <units name="Byte"/>
          <ext:dynamic-default/>
          <description>
            <text>Maximum transmission unit of an interface.
Special explanation:
Range: The server may restrict the allowed values for this leaf, depending on the interface types.
Capabilities supported by this node vary according to interface types.</text>
          </description>
        </leaf>
        <leaf name="spread-mtu-flag">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable the function of spreading the MTU value of main interface to subinterface. Capabilities supported by this node vary according to interface types.</text>
          </description>
        </leaf>
        <leaf name="statistic-interval">
          <type name="uint32">
            <range value="10..600"/>
          </type>
          <units name="s"/>
          <must condition="(../statistic-interval) mod 10 = 0"/>
          <description>
            <text>Interval at which flow statistics are collected on an interface. Capabilities supported by this node vary according to interface types.</text>
          </description>
        </leaf>
        <leaf name="vrf-name">
          <type name="leafref">
            <path value="/ni:network-instance/ni:instances/ni:instance/ni:name"/>
          </type>
          <default value="_public_"/>
          <description>
            <text>Name of a VPN instance. It uniquely identifies a VPN instance. The VRF name of the DCN interface cannot be created, modified, or deleted.</text>
          </description>
        </leaf>
        <leaf name="l2-mode-enable">
          <ext:operation-exclude value="update|delete"/>
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable the function of creating an L2 Sub-interface. Capabilities supported by this node vary according to interface types.</text>
          </description>
        </leaf>
        <leaf name="down-delay-time">
          <when condition="../type='Eth-Trunk' and ../class='main-interface'"/>
          <type name="uint32">
            <range value="0..1000"/>
          </type>
          <units name="ms"/>
          <must condition="(../down-delay-time) mod 100 = 0"/>
          <default value="0"/>
          <description>
            <text>Specify down-delay-time value in milliseconds.</text>
          </description>
        </leaf>
        <leaf name="mac-address">
          <type name="pub-type:mac-address">
            <length value="0..255"/>
          </type>
          <description>
            <text>Config MAC Address. Constraints on MAC address configuration varies according to interfaces. Capabilities supported by this node vary according to interface types.</text>
          </description>
        </leaf>
        <leaf name="index">
          <type name="uint32"/>
          <config value="false"/>
          <description>
            <text>Index of an interface.</text>
          </description>
        </leaf>
        <leaf name="position">
          <type name="string">
            <length value="1..256"/>
          </type>
          <config value="false"/>
          <description>
            <text>Position of an interface. For example, 0/1/0. Data can be queried only on physical interfaces.</text>
          </description>
        </leaf>
        <leaf name="aggregation-name">
          <type name="pub-type:if-name"/>
          <config value="false"/>
          <description>
            <text>Name of an aggregation interface.</text>
          </description>
        </leaf>
        <leaf name="is-l2-switch">
          <type name="boolean"/>
          <default value="false"/>
          <config value="false"/>
          <description>
            <text>To identify a layer2 switch port.</text>
          </description>
        </leaf>
        <leaf name="vs-name">
          <type name="string">
            <length value="1..50"/>
          </type>
          <config value="false"/>
          <description>
            <text>Name of a VS to which an interface belongs.</text>
          </description>
        </leaf>
        <leaf name="l2-switch-port-index">
          <type name="uint32"/>
          <config value="false"/>
          <description>
            <text>L2 switch port index of an interface.</text>
          </description>
        </leaf>
        <leaf name="service-type">
          <type name="service-type"/>
          <config value="false"/>
          <description>
            <text>Type of a member interface.</text>
          </description>
        </leaf>
        <leaf name="encapsulation-type">
          <type name="encapsulation-type"/>
          <config value="false"/>
          <description>
            <text>Effective encapsulation type.</text>
          </description>
        </leaf>
        <leaf name="network-layer-status">
          <type name="network-layer-state"/>
          <default value="ipv4-ipv6-up"/>
          <description>
            <text>Network-layer status of an interface.</text>
          </description>
        </leaf>
        <leaf name="protocol-up-delay-time">
          <type name="uint32">
            <range value="0..7200"/>
          </type>
          <units name="s"/>
          <ext:dynamic-default/>
          <description>
            <text>Specify the protocol up-delay-time value in seconds.</text>
          </description>
        </leaf>
        <container name="control-flap">
          <presence value="Enable/disable flapping control on an interface."/>
          <description>
            <text>Enable/disable flapping control on an interface.</text>
          </description>
          <leaf name="suppress">
            <type name="uint32">
              <range value="1..20000"/>
            </type>
            <must condition="(../suppress&gt;../reuse) and ((../suppress&lt;../ceiling))"/>
            <default value="2000"/>
            <description>
              <text>Suppression threshold on an interface. The value must be greater than the reusing threshold and smaller than the suppression upper limit.</text>
            </description>
          </leaf>
          <leaf name="reuse">
            <type name="uint32">
              <range value="1..20000"/>
            </type>
            <must condition="(../reuse&lt;../suppress)"/>
            <default value="750"/>
            <description>
              <text>Reusing threshold of an interface. The value must be smaller than the suppression threshold.</text>
            </description>
          </leaf>
          <leaf name="ceiling">
            <type name="uint32">
              <range value="1001..20000"/>
            </type>
            <must condition="(../ceiling&gt;../suppress)"/>
            <default value="6000"/>
            <description>
