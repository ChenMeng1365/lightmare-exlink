  <container name="operation">
    <config value="false"/>
    <description>
      <text>The operation top node of system-manage</text>
    </description>
    <container name="system-manage">
      <description>
        <text>The node of system-manage.</text>
      </description>
      <container name="save-configuration">
        <description>
          <text>The node to save configuration.</text>
        </description>
        <presence value="save-configuration"/>
        <container name="target">
          <description>
            <text>The target of configuration to be saved.</text>
          </description>
          <choice name="type-choice">
            <description>
              <text>The choice node of configuration saving type,such as txt or zdb.</text>
            </description>
            <mandatory value="true"/>
            <case name="txt-type">
              <description>
                <text>The txt type of configuration saving.</text>
              </description>
              <leaf name="txt">
                <description>
                  <text>Configuration saved in txt type.</text>
                </description>
                <type name="empty"/>
              </leaf>
            </case>
            <case name="zdb-type">
              <description>
                <text>Configuration saved in zdb type.</text>
              </description>
              <leaf name="zdb">
                <description>
                  <text>Configuration saved in zdb type.</text>
                </description>
                <type name="empty"/>
              </leaf>
            </case>
          </choice>
        </container>
      </container>
      <container name="startrun-download">
        <description>
          <text>The operation of startrun download.</text>
        </description>
        <choice name="copy-type">
          <description>
            <text>The copy type of startrun download,such as local or remote.</text>
          </description>
          <case name="local">
            <description>
              <text>The local copy type of startrun download.</text>
            </description>
            <container name="local">
              <description>
                <text>The operation of local copy.</text>
              </description>
              <leaf name="path">
                <description>
                  <text>Specify the local path of startrun download.</text>
                </description>
                <type name="string">
                  <length value="1..159"/>
                </type>
                <mandatory value="true"/>
              </leaf>
            </container>
          </case>
          <case name="remote">
            <description>
              <text>The remote copy type of startrun download.</text>
            </description>
            <container name="remote">
              <description>
                <text>The operation of remote copy.</text>
              </description>
              <choice name="transport-type">
                <description>
                  <text>Transport type</text>
                </description>
                <mandatory value="true"/>
                <case name="ftp">
                  <description>
                    <text>The FTP copy type of startrun download.</text>
                  </description>
                  <leaf name="ftp">
                    <description>
                      <text>The FTP copy type of startrun download.</text>
                    </description>
                    <type name="empty"/>
                  </leaf>
                </case>
                <case name="sftp">
                  <description>
                    <text>The SFTP copy type of startrun download.</text>
                  </description>
                  <leaf name="sftp">
                    <description>
                      <text>The SFTP copy type of startrun download.</text>
                    </description>
                    <type name="empty"/>
                  </leaf>
                </case>
              </choice>
              <choice name="copy-mode">
                <description>
                  <text>Specify the mode of file transmition.</text>
                </description>
                <case name="dcn">
                  <description>
                    <text>To transmit file by DCN.</text>
                  </description>
                  <leaf name="dcn">
                    <description>
                      <text>DCN mode.</text>
                    </description>
                    <type name="empty"/>
                  </leaf>
                </case>
                <case name="vrf">
                  <description>
                    <text>To transmit file by VRF.</text>
                  </description>
                  <leaf name="vrf-name">
                    <description>
                      <text>Specify VRF name.</text>
                    </description>
                    <type name="string">
                      <length value="1..32"/>
                    </type>
                    <mandatory value="true"/>
                  </leaf>
                </case>
              </choice>
              <choice name="ip-address">
                <description>
                  <text>Specify the IP address.</text>
                </description>
                <mandatory value="true"/>
                <case name="ipv4">
                  <description>
                    <text>IPv4 address.</text>
                  </description>
                  <leaf name="ipv4">
                    <description>
                      <text>IPv4 address.</text>
                    </description>
                    <type name="ncd:ipv4-address-none-zero"/>
                  </leaf>
                </case>
                <case name="ipv6">
                  <description>
                    <text>IPv6 address.</text>
                  </description>
                  <leaf name="ipv6">
                    <description>
                      <text>IPv6 address.</text>
                    </description>
                    <type name="ncd:ipv6-address"/>
                  </leaf>
                </case>
              </choice>
              <leaf name="port">
                <description>
                  <text>Specify listen port.</text>
                </description>
                <type name="uint16">
                  <range value="1..65535"/>
                </type>
              </leaf>
              <leaf name="username">
                <description>
                  <text>Specify username.</text>
                </description>
                <type name="string">
                  <length value="1..65"/>
                </type>
                <mandatory value="true"/>
              </leaf>
              <leaf name="password">
                <description>
                  <text>Specify password.</text>
                </description>
                <type name="string">
                  <length value="1..128"/>
                </type>
                <mandatory value="true"/>
              </leaf>
              <leaf name="path">
                <description>
                  <text>Specify the path of startrun download.</text>
                </description>
                <type name="string">
                  <length value="1..159"/>
                </type>
                <mandatory value="true"/>
              </leaf>
            </container>
          </case>
        </choice>
        <leaf name="unzip">
          <description>
            <text>Download file and uncompress it.</text>
          </description>
          <type name="empty"/>
          <ncd:revise-date date="2019-02-27"/>
        </leaf>
        <ncd:revise-date date="2018-09-04"/>
      </container>
    </container>
  </container>
</module>
