<?xml version="1.0" encoding="UTF-8"?>
<module name="huawei-ztp"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:ztp="urn:huawei:yang:huawei-ztp"
        xmlns:ext="urn:huawei:yang:huawei-extension">
  <namespace uri="urn:huawei:yang:huawei-ztp"/>
  <prefix value="ztp"/>
  <import module="huawei-extension">
    <prefix value="ext"/>
  </import>
  <organization>
    <text>Huawei Technologies Co., Ltd.</text>
  </organization>
  <contact>
    <text>Huawei Industrial Base
Bantian, Longgang
Shenzhen 518129
People's Republic of China
Website: http://www.huawei.com
Email: support@huawei.com</text>
  </contact>
  <description>
    <text>ZTP (Zero Touch Provisioning) refers to the function of an automatically loading of version file (including system software, configuration files and patch files) used for powering on a new factory device or an empty configuration device.</text>
  </description>
  <revision date="2019-09-29">
    <description>
      <text>Init revision.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <ext:task-name value="ztp"/>
  <container name="ztp">
    <description>
      <text>ZTP (Zero Touch Provisioning) refers to the function of an automatically loading of version file (including system software, configuration files and patch files) used for powering on a new factory device or an empty configuration device.</text>
    </description>
    <container name="status">
      <config value="false"/>
      <description>
        <text>ZTP function status, the ZTP function for used to query the enabled state and completion state.</text>
      </description>
      <leaf name="enable">
        <type name="boolean"/>
        <description>
          <text>ZTP function enable or disable status.</text>
        </description>
      </leaf>
      <leaf name="last">
        <type name="boolean"/>
        <description>
          <text>ZTP function completion status, used to record the completion of the last ZTP function.</text>
        </description>
      </leaf>
      <leaf name="pre-configuration-name">
        <type name="string">
          <length value="1..65"/>
        </type>
        <description>
          <text>ZTP function previous configuration file name.</text>
        </description>
      </leaf>
    </container>
  </container>
  <rpc name="set-enable-status">
    <description>
      <text>Set ZTP enable status.</text>
    </description>
    <input>
      <leaf name="enable">
        <type name="boolean"/>
        <description>
          <text>Enable status.</text>
        </description>
      </leaf>
    </input>
  </rpc>
  <rpc name="set-pre-configuration">
    <description>
      <text>Set ZTP previous configuration file name.</text>
    </description>
    <input>
      <leaf name="file-name">
        <type name="string">
          <length value="1..65"/>
        </type>
        <description>
          <text>Previous configuration file name.</text>
        </description>
      </leaf>
    </input>
  </rpc>
  <rpc name="reset-pre-configuration">
    <description>
      <text>Reset ZTP previous configuration file name.</text>
    </description>
    <input>
      <leaf name="confirm">
        <type name="boolean"/>
        <description>
          <text>Confirm reset.</text>
        </description>
      </leaf>
    </input>
  </rpc>
</module>
