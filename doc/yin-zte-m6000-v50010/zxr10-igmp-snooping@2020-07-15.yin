  <typedef name="switch-type">
    <type name="enumeration">
      <enum name="disable">
        <value value="0"/>
        <description>
          <text>Disable this switch.</text>
        </description>
      </enum>
      <enum name="enable">
        <value value="1"/>
        <description>
          <text>Enable this switch.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Type of switch status.</text>
    </description>
  </typedef>
  <typedef name="packet-dispose-type">
    <type name="enumeration">
      <enum name="accept">
        <value value="0"/>
        <description>
          <text>Accept packet mode.</text>
        </description>
      </enum>
      <enum name="ignore">
        <value value="1"/>
        <description>
          <text>Ignore packet mode.</text>
        </description>
      </enum>
      <enum name="discard">
        <value value="2"/>
        <description>
          <text>Discard packet mode.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Type of packet dispose.</text>
    </description>
  </typedef>
  <typedef name="work-mode-type">
    <type name="enumeration">
      <enum name="proxy">
        <value value="0"/>
        <description>
          <text>Proxy mode.</text>
        </description>
      </enum>
      <enum name="route">
        <value value="1"/>
        <description>
          <text>Route mode</text>
        </description>
      </enum>
      <enum name="transparent">
        <value value="2"/>
        <description>
          <text>Transparent mode.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Type of work mode.</text>
    </description>
  </typedef>
  <typedef name="version-type">
    <type name="enumeration">
      <enum name="v1">
        <value value="1"/>
        <description>
          <text>IGMP version1.</text>
        </description>
      </enum>
      <enum name="v2">
        <value value="2"/>
        <description>
          <text>IGMP version2.</text>
        </description>
      </enum>
      <enum name="v3">
        <value value="3"/>
        <description>
          <text>IGMP version3.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>The version of IGMP.</text>
    </description>
  </typedef>
  <typedef name="mode-type">
    <type name="enumeration">
      <enum name="dynamic">
        <value value="0"/>
        <description>
          <text>Dynamic mode.</text>
        </description>
      </enum>
      <enum name="static">
        <value value="1"/>
        <description>
          <text>Static mode.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Type of mode. include dynamic mode or static mode.</text>
    </description>
  </typedef>
  <typedef name="mrouter-version-type">
    <type name="enumeration">
      <enum name="v1">
        <value value="5"/>
        <description>
          <text>Mrouter port version 1.</text>
        </description>
      </enum>
      <enum name="v2">
        <value value="7"/>
        <description>
          <text>Mrouter port version 2.</text>
        </description>
      </enum>
      <enum name="v3">
        <value value="9"/>
        <description>
          <text>Mrouter port version 3.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>The version of IGMP mrouter port.</text>
    </description>
  </typedef>
  <typedef name="redundancy-pw-transmitting-type">
    <type name="enumeration">
      <enum name="double">
        <value value="2"/>
        <description>
          <text>If set to double,It means that IGMP packets will be sent through both working PW and protection PW.</text>
        </description>
      </enum>
      <enum name="single">
        <value value="1"/>
        <description>
          <text>If set to single,It means that IGMP packets will be sent through only working PW.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Redundancy PW transmitting mode:single or double.</text>
    </description>
  </typedef>
  <container name="configuration">
    <description>
      <text>Configuration model.</text>
    </description>
    <container name="igmp-snooping">
      <description>
        <text>Configure IGMP snooping parameters.</text>
      </description>
      <container name="global">
        <description>
          <text>IGMP snooping global configure.</text>
        </description>
        <leaf name="global-enable">
          <description>
            <text>Enable/disable IGMP snooping in this device.Its default value depends on the product.</text>
          </description>
          <type name="switch-type"/>
        </leaf>
        <leaf name="global-query-enable">
          <description>
            <text>Enable/disable IGMP queries in this device.</text>
          </description>
          <type name="switch-type"/>
          <default value="disable"/>
        </leaf>
        <leaf name="query-interval">
          <description>
            <text>Configure query interval.The query interval is the interval between general queries sent by the querier.</text>
          </description>
          <type name="uint16">
            <range value="30..65535"/>
          </type>
          <units name="second"/>
          <default value="125"/>
        </leaf>
        <leaf name="query-rsp-interval">
          <description>
            <text>Configure query response interval. The response interval inserted into the periodic general queries.</text>
          </description>
          <type name="uint8">
            <range value="1..255"/>
          </type>
          <units name="tick (1 tick = 100ms)"/>
          <default value="100"/>
        </leaf>
        <leaf name="v1-packet-dispose">
          <description>
            <text>The disposal mode of IGMPv1 Report message.</text>
          </description>
          <type name="packet-dispose-type"/>
          <default value="accept"/>
        </leaf>
        <leaf name="v2-packet-dispose">
          <description>
            <text>The disposal mode of IGMPV2 Report/Leave message.</text>
          </description>
          <type name="packet-dispose-type"/>
          <default value="accept"/>
        </leaf>
        <leaf name="v3-packet-dispose">
          <description>
            <text>The disposal mode of IGMPV3 Report message.</text>
          </description>
          <type name="packet-dispose-type"/>
          <default value="discard"/>
        </leaf>
        <leaf name="host-aging-closedown">
          <description>
            <text>Configure host aging closedown.</text>
          </description>
          <type name="switch-type"/>
          <default value="disable"/>
          <ncd:revise-date date="2018-02-12"/>
        </leaf>
        <container name="transmit-to-block-port">
          <description>
            <text>Transmit packets through block ports.</text>
          </description>
          <leaf name="trans-query-to-block-port">
            <description>
              <text>Configure query packet through blocked port.</text>
            </description>
            <type name="switch-type"/>
            <default value="disable"/>
          </leaf>
          <leaf name="trans-report-leave-to-block-port">
            <description>
              <text>Configure report and leave packet through blocked port.</text>
            </description>
            <type name="switch-type"/>
            <default value="disable"/>
          </leaf>
          <ncd:revise-date date="2018-02-12"/>
        </container>
        <container name="transmit-from-block-port">
          <description>
            <text>Transmit packets from block ports.</text>
          </description>
          <leaf name="trans-query-from-block-port">
            <description>
              <text>Allow to transmit query packet from blocked port.</text>
            </description>
            <type name="switch-type"/>
            <default value="disable"/>
          </leaf>
          <ncd:revise-date date="2018-02-12"/>
        </container>
        <leaf name="querier-send-to-mrouter">
          <description>
            <text>Configure querier sending query to mrouter.</text>
          </description>
          <type name="enumeration">
            <enum name="enable">
              <value value="1"/>
              <description>
                <text>Configure sending query to mrouter enable.</text>
              </description>
            </enum>
            <enum name="disable">
              <value value="2"/>
              <description>
                <text>Configure sending query to mrouter disable.</text>
              </description>
            </enum>
          </type>
          <default value="disable"/>
          <ncd:revise-date date="2018-08-07"/>
        </leaf>
        <container name="logging">
          <description>
            <text>Logging the igmp packet.</text>
          </description>
          <leaf name="packet-report">
            <description>
              <text>Logging the IGMP report.</text>
            </description>
            <type name="switch-type"/>
            <default value="disable"/>
          </leaf>
          <leaf name="overrun-report">
            <description>
              <text>Logging the IGMP report for group or member number overrun.</text>
            </description>
            <type name="switch-type"/>
            <default value="disable"/>
          </leaf>
          <ncd:revise-date date="2019-11-04"/>
        </container>
      </container>
      <container name="vpls">
        <description>
          <text>IGMP snooping VPLS configure.Can't config IGMP snooping in VPLS if the VPLS instance not exist.If IGMP snooping configuration exist in VPLS,the VPLS instance can't be deleted.</text>
        </description>
        <list name="vpls">
          <description>
            <text>The config of instance VPLS.</text>
          </description>
          <key value="name"/>
          <leaf name="name">
            <description>
              <text>Submode instance Name of VPLS.</text>
            </description>
            <type name="string">
              <length value="1..32"/>
            </type>
          </leaf>
          <leaf name="snooping-switch">
            <description>
              <text>Enable/disable IGMP snooping in this instance.</text>
            </description>
            <type name="switch-type"/>
            <default value="disable"/>
          </leaf>
          <leaf name="member-fast-leave">
            <description>
              <text>Enable/disable group membership fast leave in this instance.</text>
            </description>
            <type name="switch-type"/>
            <default value="disable"/>
          </leaf>
          <leaf name="acl-name">
            <description>
              <text>Configure an ACL rule in this instance.</text>
            </description>
            <type name="string">
              <length value="0..31"/>
            </type>
          </leaf>
          <leaf name="member-time-out">
            <description>
              <text>Member aging time in this instance.</text>
            </description>
            <type name="uint16">
              <range value="30..65535"/>
            </type>
            <units name="second"/>
            <default value="260"/>
          </leaf>
          <leaf name="mrouter-time-out">
            <description>
              <text>Multicast router port aging time in this instance.</text>
            </description>
            <type name="uint16">
              <range value="30..65535"/>
            </type>
            <units name="second"/>
            <default value="260"/>
          </leaf>
          <leaf name="work-mode">
            <description>
              <text>Work mode in this instance: proxy, transparent or router.</text>
            </description>
            <type name="work-mode-type"/>
            <default value="proxy"/>
          </leaf>
          <leaf name="proxy-ip">
            <description>
              <text>This IP address is used as source IP when sending IGMP Report and Leave in proxy mode.</text>
            </description>
            <type name="ncd:ipv4-address"/>
          </leaf>
          <leaf name="query-ip">
            <description>
              <text>This IP address is used as source IP when sending IGMP Query.</text>
            </description>
            <type name="ncd:ipv4-address"/>
          </leaf>
          <leaf name="max-group-num">
            <description>
              <text>Max number of multicast group can access in this instance.Its default value and maximum value depends on the product.</text>
            </description>
            <type name="uint32">
