<?xml version="1.0" encoding="UTF-8"?>
<module name="huawei-bras-said"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:bras-said="urn:huawei:yang:huawei-bras-said"
        xmlns:ext="urn:huawei:yang:huawei-extension">
  <namespace uri="urn:huawei:yang:huawei-bras-said"/>
  <prefix value="bras-said"/>
  <import module="huawei-extension">
    <prefix value="ext"/>
  </import>
  <organization>
    <text>Huawei Technologies Co., Ltd.</text>
  </organization>
  <contact>
    <text>Huawei Industrial Base
Bantian, Longgang
Shenzhen 518129
People's Republic of China
Website: http://www.huawei.com
Email: support@huawei.com</text>
  </contact>
  <description>
    <text>Bras said.</text>
  </description>
  <revision date="2020-03-18">
    <description>
      <text>Modify description.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <revision date="2019-04-10">
    <description>
      <text>Initial revision.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <ext:task-name value="aaa-access"/>
  <container name="bras-said">
    <description>
      <text>Bras said.</text>
    </description>
    <container name="said-exclude-fail-codes">
      <description>
        <text>Configure the excluding of login failures with a specified cause code from being detected or diagnosed by SAID nodes. By default, a login failure is not excluded from the detection or diagnosis of SAID nodes.</text>
      </description>
      <leaf-list name="visible-code">
        <type name="uint16">
          <range value="1..741"/>
        </type>
        <description>
          <text>Configure the visible on-line failure cause type.</text>
        </description>
      </leaf-list>
      <leaf-list name="invisible-code">
        <type name="uint16">
          <range value="0..62"/>
        </type>
        <description>
          <text>Configure the sxternal invisible on-line failure cause type.</text>
        </description>
      </leaf-list>
    </container>
    <container name="said-parameter">
      <description>
        <text>Configure the user Access.</text>
      </description>
      <leaf name="increase-number-of-online-fail">
        <type name="uint32">
          <range value="100..256000"/>
        </type>
        <default value="10000"/>
        <description>
          <text>Sets the sepical increase number of online-fail.</text>
        </description>
      </leaf>
      <leaf name="recover-interval">
        <type name="uint32">
          <range value="1..180"/>
        </type>
        <units name="d"/>
        <description>
          <text>Sets the special days of interval between recover operations.</text>
        </description>
      </leaf>
      <leaf name="online-fail-number">
        <type name="uint32">
          <range value="1..256000"/>
        </type>
        <default value="10000"/>
        <description>
          <text>The sepical increase number of online-fail.</text>
        </description>
      </leaf>
      <leaf name="online-user-reduce-number">
        <type name="uint32">
          <range value="1..256000"/>
        </type>
        <default value="1000"/>
        <description>
          <text>The sepical reduce of online-user number.</text>
        </description>
      </leaf>
      <leaf name="online-success-rate">
        <type name="uint32">
          <range value="1..100"/>
        </type>
        <units name="%"/>
        <default value="30"/>
        <description>
          <text>The sepical rate of online success, unit is percent.</text>
        </description>
      </leaf>
      <leaf name="detect-by-user-number-reduce-ratio">
        <type name="uint32">
          <range value="0..100"/>
        </type>
        <units name="%"/>
        <default value="50"/>
        <description>
          <text>Accords to the number of users decreased the percentage of detection when the specified value. Only when the number of users is detected to reach the specified drop rate, it is considered that the SAID condition is satisfied and the diagnosis and recovery are continued.</text>
        </description>
      </leaf>
      <leaf name="detect-by-flow-speed-reduce-ratio">
        <type name="uint32">
          <range value="0..100"/>
        </type>
        <units name="%"/>
        <default value="50"/>
        <description>
          <text>Accords to the percentage drop in the flow rate when the specified value, Only when the traffic rate reduction is detected reaches the specified drop rate, it is considered that the SAID condition is satisfied and the diagnosis and recovery are continued.</text>
        </description>
      </leaf>
      <leaf name="said-enable">
        <type name="boolean"/>
        <default value="true"/>
        <description>
          <text>Enable/disable SAID function.</text>
        </description>
      </leaf>
    </container>
  </container>
</module>
