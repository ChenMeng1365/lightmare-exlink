                ;
  }
  revision 2014-03-24 {
    description '1. Move setup-priority and holding priority to priority contai'
              + "ner.
\n"
              + '2. Add default value to ds-te/model/value,access-tunnel/tunnel'
              + "/activation.
\n"
              + '3. Add reset value to static-tunnel/tunnel/lsps/positive(rever'
              + "se)-attribute next-hop share-tunnel pre-hop.
\n"
              + "4. Add convergence-ratio container.
\n"
              + "5. Add tunnel-bind.
\n"
              + '6. Add pathoption list,add egress tunnel,modify dynamic_tunnel'
              + ' list,modify tnnl-ar-hop.'
                ;
  }
  revision 2014-03-07 {
    description "1. Add performCode to ingress-tunnel-id and bfd.
\n"
              + "2. Add interface filter number to static tunnel name.
\n"
              + "3. Add frr-promotion unit.
\n"
              + "4. Add maximum instance number if-bandwidthes.
\n"
              + "5. Add maximum instance number of affinity.
\n"
              + "6. Modify protect-type Mandatory.
\n"
              + "7. Add dynamic case to hotstandby path type.
\n"
              + '8. Add default value to policy class.'
                ;
  }
  revision 2014-03-05 {
    description '1.add the range of bandwidth and delete performance of bandwid'
              + "th in dynamic-tunnel and static-tunnel.
\n"
              + '2.add reset value and delete default value of cbs, eir and ebs'
              + " in dynamic-tunnel.
\n"
              + "3.add the range of cir and pir in tnnl-rate-limit.
\n"
              + '4.add the reset value of inlabel and outlabel in positive-attr'
              + "ibute and reverse-attribute.
\n"
              + "5.modify the range of ar-relative-metric.
\n"
              + "6.modify the description of tunnel list.
\n"
              + "7.modify the name of remote tunnel.\n"
              + '8.Add dscp container.'
                ;
  }
  revision 2014-02-18 {
    description "1.remove filter of retran-config.
\n"
              + "2.add the invalid value of interval and frequency.
\n"
              + "3.add the range of explicit-path id.
\n"
              + '4.modify ar-enable wrong value.'
                ;
  }
  revision 2014-02-15 {
    description '1.add sd container' ;
  }
  revision 2014-01-23 {
    description 'Add the activation of dynamic tunnel. Modify the sequence of r'
              + 'ole-type and triple. Modify the  scope of the inlabel and outl'
              + 'abel. Delete the default value and default performance of out-'
              + 'bandwidth leaf.'
                ;
  }
  revision 2014-01-15 {
    description "Add te-trunk container.
\n"
              + "Move soft-preempt container from mpls-te to dynamic-tunnel.
\n"
              + "Remove resv-style and alias-name container.
\n"
              + 'Add filter to router-id and tunnel-destination.'
                ;
  }
  revision 2014-01-08 {
    description "Add container affinity and frr-promotion.
\n"
              + "Add the filter of invalid address and delete default value.
\n"
              + 'Add the UnactiveTime,delay_switch_over,delay_reactive,delay_li'
              + 'nkup,crankback.'
                ;
  }
  revision 2013-12-31 {
    description "modify te-metric range and add sample-enable reset value.
\n"
              + 'modify description of refresh-interval/restart-time/recovery-t'
              + "ime of graceful-restart function.
\n"
              + "modify description of srlg of interface.
\n"
              + "Add container for auto-bandwidth and ds-te.
\n"
              + 'Add filter for policy-class of static-tunnel.'
                ;
  }
  revision 2013-12-26 {
    description 'Add performance and interface filter. Add filter in policy-cla'
              + 'ss of access-tunnel.'
                ;
  }
  revision 2013-12-11 {
    description 'add resv-style,bw-protect,alias-name,soft-preemption,te-ecmp-g'
              + "roups,limit-hop-num,hop-prior,reopt-hsb-lsp to model.
\n"
              + 'modify retran-config/enable configuration value.'
                ;
  }
  revision 2013-12-05 {
    description 'Modify srlg container to add one-to-one, facility, auto-backup'
              + ' and hotstandby type.'
                ;
  }
  revision 2013-11-25 {
    description "add regular expression to admin-weight tunnel name.
\n"
              + 'add reset default value 0 to frr facility and one2one.'
                ;
  }
  revision 2013-11-10 {
    description 'Change the type of model from unit32 to enumeration, the type '
              + 'of percent from unit32 to unit8, the range of tunnel bfd enabl'
              + 'e and policy list of dynamic-tunnel and access-tunnel.'
                ;
  }
  revision 2013-09-24 {
    description 'Add the static tunnel model, see called "static-tunnels" of co'
              + 'ntainer.'
                ;
  }
  revision 2013-09-18 {
    description 'Add filter, change some choice to single container and modify '
              + 'some testing mistakes.'
                ;
  }
  revision 2013-08-01 {
    description 'Initial version.' ;
  }
  container configuration {
    description 'The branch of configuration model.' ;
    container mpls-te {
      description 'The configuration of MPLS TE.' ;
      leaf te-enable {
        description 'The global switch of TE.' ;
        type enumeration {
          enum disabled {
            value 0 ;
          }
          enum enabled {
            value 1 ;
          }
        }
        default disabled ;
      }
      container global-config {
        description 'TE global common configuration. Include refresh configurat'
                  + 'ion, retran and srefresh congiguration, explicit none cong'
                  + 'iguration and resv confirm congiguration.'
                    ;
        presence 'global configuration' ;
        container refresh-config {
          description 'The signalling packet containing path and resv refresh c'
                    + 'onfiguration.'
                      ;
          leaf interval {
            description 'Configuration of time interval for sending refresh mes'
                      + "sage, range: from 5000 to 65535000,
\n"
                      + 'units: ms, default value is 30000 ms.'
                        ;
            type uint32 {
              range 5000..65535000 ;
            }
            units ms ;
            default 30000 ;
          }
          leaf misses {
            description 'Configuration of miss number for refresh message, rang'
                      + 'e: from 2 to 10, default value is 4.'
                        ;
            type uint32 {
              range 2..10 ;
            }
            default 4 ;
          }
        }
        container retran-config {
          description 'The retransmit configuration.' ;
          leaf enable {
            description 'The switch of retransmit. Ref-reduction-enable is depe'
                      + 'nd on retran-config enable.'
                        ;
            type empty ;
          }
          leaf interval {
            description 'The interval of retransmit signalling message, range: '
                      + 'from 500 to 3000, unit: ms, default value: 1000 ms.'
                        ;
            type uint32 {
              range 500..3000 ;
            }
            units ms ;
            default 1000 ;
          }
          leaf limit {
            description 'The limit of signalling message retransmit, range: fro'
                      + 'm 2 to 10, default value: 3.'
                        ;
            type uint32 {
              range 2..10 ;
            }
            default 3 ;
          }
          leaf ref-reduction-enable {
            description 'Enable summary refresh function, default value is disa'
                      + "bled.
\n"
                      + 'Ref-reduction-enable is depend on retran-config enable'
                      + '.'
                        ;
            type enumeration {
              enum disabled {
                value 0 ;
              }
              enum enabled {
                value 1 ;
              }
            }
            default disabled ;
          }
        }
        container explicit-none {
          description 'The egress label management. The default label is implic'
                    + 'it-null.'
                      ;
          leaf explicit-null {
            description 'When this attribute is configured, it indicates that t'
                      + 'he label of egress node is explicit-null.'
                        ;
            type boolean ;
            default false ;
          }
          leaf none-null {
            description 'When this attribute is configured, it indicates that t'
                      + 'he label of egress node is none-null.'
                        ;
            type boolean ;
            default false ;
          }
        }
        container sd-config {
          description 'Configuration of SD.' ;
          leaf enable {
            description 'The switch of sd. default value is disabled.' ;
            type enumeration {
              enum disabled {
                value 0 ;
              }
              enum enabled {
                value 1 ;
              }
            }
            default disabled ;
          }
          leaf interval {
            description 'The interval of sd alarm, range: from 10 to 30, unit: '
                      + 's, default value: 10 s.'
                        ;
            type int16 {
              range 10..30 ;
            }
            units s ;
            default 10 ;
          }
        }
        container resv-confirm {
          description 'The resv confirm.' ;
          leaf enable {
            description 'Enable resv confirm function, default value is disable'
                      + "d.
\n"
                      + 'It is configured on egress.'
                        ;
            type enumeration {
              enum disabled {
                value 0 ;
              }
              enum enabled {
                value 1 ;
              }
            }
            default disabled ;
          }
        }
        container router-id {
          leaf route-id {
            description 'The TE route ID configuration. Must config router id b'
                      + 'efore any other configuration under dynamic-tunnl node'
                      + ' and ensure that router id is reachable in route.'
                        ;
            type ncd:ipv4-address {
              pattern '([1-9]|[1-9][0-9]|1[0-9][0-9]|2[0-4][0-9]|25[0-5])(.([0-'
                    + '9]|[1-9][0-9]|1[0-9][0-9]|2[0-4][0-9]|25[0-5])){3}'
                      ;
            }
          }
        }
        container tnnl-unact-time {
          description 'Tunnel reconstruction interval regularly.' ;
          leaf interval {
            description 'Tunnel reconstruction interval regularly.' ;
            type uint32 {
              range 30..300 ;
            }
            units seconds ;
          }
        }
        container delay {
          description 'Static TE delay function of management object.' ;
          leaf delay_switch_over {
            description 'Time delay back cut.' ;
            type uint32 {
              range 0..30 ;
            }
            units seconds ;
            default 0 ;
          }
          leaf delay_reactive {
            description 'Delay reactive time.' ;
            type uint32 {
              range 0..30 ;
            }
            units seconds ;
            default 10 ;
          }
          leaf delay_linkup {
            description 'The linkup of time delay response.' ;
            type uint32 {
              range 0..30 ;
            }
            units seconds ;
            default 0 ;
          }
        }
      }
      container explicit-path {
        description 'Configuration of explicit path.' ;
        presence 'Configuration of explicit-path' ;
        list name-based {
          description 'The explicit path name list' ;
          key name ;
          max-elements 8192 ;
          min-elements 0 ;
          leaf name {
            description 'The explicit path name.' ;
            type string {
              length 1..64 ;
            }
          }
          container path-paras {
            description 'Every explicit-path contains 32 hops.' ;
            list path-para {
              description 'Explicit path address information.' ;
              key index ;
              max-elements 131072 ;
              leaf index {
                description 'The explicit path index.' ;
                type uint8 {
                  range 1..64 ;
                }
              }
              leaf hop-ip {
                description 'Hop IP.' ;
                type ncd:ipv4-address ;
                mandatory true ;
              }
              leaf hop-type {
                description 'The hop type.' ;
                type enumeration {
                  enum include {
                    value 1 ;
                  }
                  enum exclude {
                    value 2 ;
                  }
                }
                mandatory true ;
              }
              leaf hop-attr {
                description 'The hop attribute. If hop type is include route, c'
                          + 'an configure strict route and loose route. If hop '
                          + 'type is exclude route, can configure exclude inter'
                          + 'face and exclude node.'
                            ;
                type enumeration {
                  enum strict-route {
                    value 1 ;
                  }
                  enum loose-route {
                    value 2 ;
                  }
                  enum exclude-intf {
                    value 3 ;
                  }
                  enum exclude-node {
                    value 4 ;
                  }
                }
                mandatory true ;
              }
            }
          }
        }
        list id-based {
          description 'The explicit path ID list.' ;
          key id ;
          max-elements 8192 ;
          min-elements 0 ;
          leaf id {
            description 'The explicit path ID.' ;
            type uint16 {
              range 1..65535 ;
            }
          }
          container path-paras {
            description 'Every explicit-path contains 32 hops.' ;
            list path-para {
              description 'Explicit path address information.' ;
              key index ;
              max-elements 131072 ;
              leaf index {
                description 'Explicit path index.' ;
                type uint8 {
                  range 1..64 ;
                }
              }
              leaf hop-ip {
                description 'Hop IP.' ;
                type ncd:ipv4-address ;
                mandatory true ;
              }
              leaf hop-type {
                description 'The hop type. Include include route and exclude ro'
                          + 'ute.'
                            ;
                type enumeration {
                  enum include {
                    value 1 ;
                  }
                  enum exclude {
                    value 2 ;
                  }
                }
                mandatory true ;
              }
              leaf hop-attr {
                description 'The hop attribute. If hop type is include route, c'
                          + 'an configure strict route and loose route. If hop '
                          + 'type is exclude route, can configure exclude inter'
                          + 'face and exclude node.'
                            ;
                type enumeration {
                  enum strict-route {
                    value 1 ;
                  }
                  enum loose-route {
                    value 2 ;
                  }
                  enum exclude-intf {
                    value 3 ;
                  }
                  enum exclude-node {
                    value 4 ;
                  }
                }
                mandatory true ;
              }
            }
          }
        }
      }
      container te-interfaces {
        description 'Enable interface to join in TE.All of the interface config'
                  + 'uration depend on TE interface.'
                    ;
        presence 'configuration of te-interface' ;
        list interface {
          description 'TE interface list.' ;
          key name ;
          max-elements 255 ;
          leaf name {
            description 'Interface name.' ;
            type ncd:ifname ;
          }
          leaf dscp {
            description 'DSCP value of TE interface, range: 0-63.' ;
            type uint8 {
              range 0..63 ;
            }
          }
          container convergence-ratio {
            description 'Interface convergence ratio.' ;
            leaf convergence-ratio-value {
              description 'Interface convergence ratio value.' ;
              type uint8 {
                range 1..100 ;
              }
              default 100 ;
            }
          }
        }
      }
      container dynamic-tunnel {
        description 'The dynamic tunnel.' ;
        presence 'configurationg of dynamic-tunnel' ;
        container reoptimize {
          description 'The global configuration of reoptimization.' ;
          presence 'Enable TECP reoptimize frequency.' ;
          leaf frequency {
            description 'The tunnels reoptimize timer frequency in global mode.'
                      + ' Configure tnnl-reoptimize enable node to specify whic'
                      + 'h tunnel uses reoptimize function.'
                        ;
            type int32 {
              range 30..604800 ;
            }
            units s ;
          }
          leaf event {
            description 'Specify which event can triger reoptimize.' ;
            type uint8 {
              range 0..1 {
                description 'linkup(1), none(0)' ;
              }
            }
            default 0 ;
          }
          leaf loose-node-enable {
            description 'Configuration of loose node reoptimize.' ;
            type enumeration {
              enum disabled {
                value 0 ;
              }
              enum enabled {
                value 1 ;
              }
            }
            default disabled ;
          }
        }
        container crankback {
          description 'Set the crankback retry count limit and information life'
                    + ' cycle.'
                      ;
          leaf limit_count {
            description 'Set the crankback retry count limit.' ;
            type uint8 {
              range 1..10 ;
            }
            default 5 ;
          }
          leaf ttl_value {
            description 'Set the crankback information life cycle.' ;
            type uint16 {
              range 30..300 ;
            }
            default 60 ;
          }
        }
        container tunnels {
          description 'Dynamic Tunnel Configuration.' ;
          list tunnel {
            description 'The dynamic tunnel configuration list.' ;
            key name ;
            max-elements 131070 ;
            leaf name {
              description 'The tunnel name.' ;
              type string {
                length 10..31 ;
                pattern 'te_tunnel[0-9]{1,22}' ;
              }
            }
            leaf hsb-lsp-frr {
              description 'To indicate that hotstandby lsp whether enable FRR o'
                        + 'r not.'
                          ;
              type enumeration {
                enum disabled {
                  value 0 ;
                }
                enum enabled {
                  value 1 ;
                }
              }
              default disabled ;
            }
            leaf destination {
              description 'The tunnel destination.' ;
              type ncd:ipv4-address {
                pattern '([1-9]|[1-9][0-9]|1[0-9][0-9]|2[0-4][0-9]|25[0-5])(.(['
                      + '0-9]|[1-9][0-9]|1[0-9][0-9]|2[0-4][0-9]|25[0-5])){3}'
                        ;
              }
            }
            leaf reoptimize {
              description 'Specify MPLS tunnel whether to open the timing re-op'
                        + 'timization.'
                          ;
              type enumeration {
                enum disabled {
                  value 0 ;
                }
                enum enabled {
                  value 1 ;
                }
              }
              default disabled ;
            }
            leaf limit-hop-num {
              description 'The maximum value of hops.' ;
              type uint8 {
                range 0..32 ;
              }
              default 0 ;
            }
            leaf hop-prior {
              description 'Enable the minimum hop priority function of a tunnel'
                        + '.'
                          ;
              type enumeration {
                enum enabled {
                  value 1 ;
                }
                enum disabled {
                  value 2 ;
                }
              }
              default disabled ;
            }
            leaf reopt-hsb-lsp {
              description 'Enable hsb-lsp auto-reoptimize.' ;
              type enumeration {
                enum disabled {
                  value 0 ;
                }
                enum enabled {
                  value 1 ;
                }
              }
              default disabled ;
            }
            leaf activation {
              description 'The activation of dynamic tunnel enable.' ;
              type enumeration {
                enum disabled {
                  value 0 ;
                }
                enum enabled {
                  value 1 ;
                }
              }
              default enabled ;
            }
            leaf crankback {
              description 'Tunnel crankback features enabled.' ;
              type enumeration {
                enum disabled {
                  value 0 ;
                }
                enum end-to-end {
                  value 1 ;
                }
              }
              default disabled ;
            }
            leaf resv-style {
              description "Configure resv-style in tunnel mode.
\n"
                        + 'You must noticed that ff style is confilct to frr co'
                        + 'nfiguration.If frr has been configured first in tunn'
                        + 'el mode,ff style can not be configured.If ff style h'
                        + 'as been configured,frr can not be configured.'
                          ;
              type enumeration {
                enum se {
                  value 1 ;
                  description 'se(Shared Explicit)' ;
                }
                enum ff {
                  value 2 ;
                  description 'ff(Fixed Filter)' ;
                }
              }
              default se ;
            }
            leaf alias-name {
              description 'Configuration of tunnel alias name.' ;
              type string {
                length 1..63 ;
              }
            }
            leaf track-name {
              description 'Configuration of tunnel track name,use for BFD detec'
                        + 'tion.'
                          ;
              type string {
                length 1..31 ;
              }
            }
            leaf soft-preemption-enable {
              description 'Enable soft-preemption in tunnel mode,and this tunne'
                        + 'l will be teared after the specifc time named timeou'
                        + 't configured under timeout container.'
                          ;
              type enumeration {
                enum disabled {
                  value 0 ;
                }
                enum enabled {
                  value 1 ;
                }
              }
              default disabled ;
            }
            list path-option-list {
              description 'Tunnel path option configuration.' ;
              key number ;
              max-elements 786420 ;
              leaf number {
                description 'Path option number which the specified tunnel is u'
                          + 'sed.'
                            ;
                type uint8 {
                  range 1..16 ;
                }
              }
              choice path-class {
                description 'Path Selection Type.' ;
                mandatory true ;
                case dynamic {
                  description 'Dynamic Path Selection.' ;
                  leaf dynamic {
                    description 'The path type is dynamic.' ;
                    type empty ;
                    mandatory true ;
                  }
                }
                case explicit-name {
                  description 'Based on the name of the explicit path selection'
                            + '.'
                              ;
                  leaf explicit-path-name {
                    description 'The related explicit path name.' ;
                    type string {
                      length 1..64 ;
                    }
                    mandatory true ;
                  }
                }
                case expilict-id {
                  description 'Id-based explicit path selection.' ;
                  leaf explicit-path-id {
                    description 'The related explicit path ID.' ;
                    type uint16 {
                      range 1..65535 ;
                    }
                    mandatory true ;
                  }
                }
              }
              leaf lockdown {
                description 'Lockdown switch is turned on.' ;
                type boolean ;
                default false ;
              }
            }
            container binary-tunnel {
              description 'Bidirectional Tunnel Setting.' ;
              leaf co-route {
                description 'The binary tunnel enable or disable.' ;
                type enumeration {
                  enum disabled {
                    value 0 ;
                  }
                  enum enabled {
                    value 1 ;
                  }
                }
                default disabled ;
              }
            }
            container policy-class {
              description 'The tunnel policy class.' ;
              leaf class {
                description 'The tunnel policy class configuration. From 0 to 7'
                          + ' represent normal configuration. Ingress and Egres'
                          + 's of co-tunnel uses 255 as its default value.'
                            ;
                type uint8 {
                  range '0..7 | 255' {
                    description '0-7 represent normal configuration. Ingress an'
                              + 'd Egress of co-tunnel uses 255 as its default '
                              + 'value.'
                                ;
                  }
                }
                default 255 ;
              }
            }
            container bandwidth {
              description 'The tunnel bandwidth.' ;
              presence 'Tunnel bandwidth allocation.' ;
              leaf bandwidth {
                description 'The tunnel bandwidth configuration.' ;
                type uint32 {
                  range 0..4294967295 ;
                }
                units kbps ;
                mandatory true ;
              }
              leaf cbs {
                description 'The tunnel CBS.' ;
                type uint32 {
                  range 1..4294967295 ;
                }
                units byte ;
              }
              leaf eir {
                description 'The tunnel EIR.' ;
                type uint32 {
                  range 1..4294967295 ;
                }
                units kbps ;
              }
              leaf ebs {
                description 'The tunnel EBS.' ;
                type uint32 {
                  range 1..4294967295 ;
                }
                units byte ;
              }
            }
            container ct-bandwidths {
              description 'Tunnel classtype and its request bandwidth. It is ex'
                        + 'clusive with common bandwidth and tunnel-rate-limit.'
                          ;
              list ct-bandwidth {
                description 'Tunnel classtype and its request bandwidth.' ;
                key class-type ;
                leaf class-type {
                  description 'The configuration of class type.' ;
                  type uint8 {
                    range 0..7 ;
                  }
                }
                leaf bandwidth {
                  description 'The bandwidth of class type.' ;
                  type uint32 {
                    range 1..4294967295 ;
                  }
                  mandatory true ;
                }
              }
            }
            container rros {
              description 'The RRO function of dynamic tunnel.' ;
              leaf rro-enable {
