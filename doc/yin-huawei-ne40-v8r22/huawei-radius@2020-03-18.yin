        <config value="false"/>
        <description>
          <text>Statistics of a RADIUS server parameter.</text>
        </description>
        <leaf name="group-name">
          <type name="string">
            <length value="1..32"/>
          </type>
          <config value="false"/>
          <description>
            <text>Name of a RADIUS template.</text>
          </description>
        </leaf>
        <leaf name="server-type">
          <type name="bras-radius-server-type"/>
          <config value="false"/>
          <description>
            <text>Type of Radius Server.</text>
          </description>
        </leaf>
        <leaf name="ip-address">
