        <value value="1"/>
        <description>
          <text>User authentication type password.</text>
        </description>
      </enum>
      <enum name="rsa">
        <value value="2"/>
        <description>
          <text>User authentication type RSA.</text>
        </description>
      </enum>
      <enum name="all">
        <value value="3"/>
        <description>
          <text>User authentication type all.</text>
        </description>
      </enum>
      <enum name="password-rsa">
        <value value="4"/>
        <description>
          <text>User authentication type password RSA.</text>
        </description>
      </enum>
      <enum name="dsa">
        <value value="5"/>
        <description>
          <text>User authentication type DSA.</text>
        </description>
      </enum>
      <enum name="password-dsa">
        <value value="6"/>
        <description>
          <text>User authentication type password DSA.</text>
        </description>
      </enum>
      <enum name="ecc">
        <value value="7"/>
        <description>
          <text>User authentication type ECC.</text>
        </description>
      </enum>
      <enum name="password-ecc">
        <value value="8"/>
        <description>
          <text>User authentication type password ECC.</text>
        </description>
      </enum>
      <enum name="sm2">
        <value value="9"/>
        <description>
          <text>User authentication type SM2.</text>
        </description>
      </enum>
      <enum name="password-sm2">
        <value value="10"/>
        <description>
          <text>User authentication type password SM2.</text>
        </description>
      </enum>
      <enum name="x509v3-rsa">
        <value value="11"/>
        <description>
          <text>User authentication type is x509v3-rsa.</text>
        </description>
      </enum>
      <enum name="password-x509v3-rsa">
        <value value="12"/>
        <description>
          <text>User authentication type is password-x509v3-rsa.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>SSH server user authentication type list.</text>
    </description>
  </typedef>
  <typedef name="sshs-user-pubkey-type">
    <type name="enumeration">
      <enum name="not-set">
        <value value="0"/>
        <description>
          <text>Server public key is not set.</text>
        </description>
      </enum>
      <enum name="RSA">
        <value value="1"/>
        <description>
          <text>RSA server public key.</text>
        </description>
      </enum>
      <enum name="DSA">
        <value value="2"/>
        <description>
          <text>DSA server public key.</text>
        </description>
      </enum>
      <enum name="ECC">
        <value value="3"/>
        <description>
          <text>ECC server public key.</text>
        </description>
      </enum>
      <enum name="SM2">
        <value value="5"/>
        <description>
          <text>SM2 server public key.</text>
        </description>
      </enum>
      <enum name="PKI">
        <value value="6"/>
        <description>
          <text>PKI server public key.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>SSH server public key encryption type list for user.</text>
    </description>
  </typedef>
  <typedef name="sshs-pubkey-type">
    <type name="enumeration">
      <enum name="not-set">
        <value value="0"/>
        <description>
          <text>Server public key is not set.</text>
        </description>
      </enum>
      <enum name="RSA">
        <value value="1"/>
        <description>
          <text>RSA server public key.</text>
        </description>
      </enum>
      <enum name="DSA">
        <value value="2"/>
        <description>
          <text>DSA server public key.</text>
        </description>
      </enum>
      <enum name="ECC">
        <value value="3"/>
        <description>
          <text>ECC server public key.</text>
        </description>
      </enum>
      <enum name="SM2">
        <value value="5"/>
        <description>
          <text>SM2 server public key.</text>
        </description>
      </enum>
      <enum name="RSA-SHA2-256">
        <value value="7"/>
        <description>
          <text>RSA SHA2-256 server public key.</text>
        </description>
      </enum>
      <enum name="RSA-SHA2-512">
        <value value="8"/>
        <description>
          <text>RSA SHA2-512 server public key.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>SSH server public key encryption type list.</text>
    </description>
  </typedef>
  <typedef name="sshs-bool-type">
    <type name="enumeration">
      <enum name="false">
        <value value="0"/>
        <description>
          <text>Disabled state.</text>
        </description>
      </enum>
      <enum name="true">
        <value value="1"/>
        <description>
          <text>Enabled state.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>SSH server enable flag list.</text>
    </description>
  </typedef>
  <typedef name="defauthor">
    <type name="enumeration">
      <enum name="aaa">
        <value value="0"/>
        <description>
          <text>AAA authorization.</text>
        </description>
      </enum>
      <enum name="root">
        <value value="1"/>
        <description>
          <text>ROOT authorization.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>SSH server default authorization list.</text>
    </description>
  </typedef>
  <typedef name="version">
    <type name="enumeration">
      <enum name="sshver2">
        <value value="0"/>
        <description>
          <text>SSH protocol version 2.0.</text>
        </description>
      </enum>
      <enum name="sshver1-99">
        <value value="1"/>
        <description>
          <text>SSH protocol version 1.99.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>SSH server version type list.</text>
    </description>
  </typedef>
  <typedef name="sshs-bool">
    <type name="enumeration">
      <enum name="enable">
        <value value="1"/>
        <description>
          <text>SSH server/service enable.</text>
        </description>
      </enum>
      <enum name="disable">
        <value value="2"/>
        <description>
          <text>SSH server/service disable.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>SSH server bool switch.</text>
    </description>
  </typedef>
