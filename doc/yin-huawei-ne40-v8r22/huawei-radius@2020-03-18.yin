          </description>
        </leaf>
        <leaf name="limit-value">
          <type name="uint32">
            <range value="1..255"/>
          </type>
          <units name="packet"/>
          <mandatory value="true"/>
          <description>
            <text>Configure the pending number of the packets send by radius.</text>
          </description>
        </leaf>
      </list>
    </container>
    <container name="server-speed-limits">
      <description>
