     |     +--ro pe-vlan?                       string
     |     +--ro ce-vlan?                       string
     |     +--ro free-time?                     string
     |     +--ro chasten-type?                  string
     +--ro ppp-option105-chasten-statistics
        +--ro ppp-option105-chasten-statistic* [slot-id]
           +--ro slot-id                        string
           +--ro circuit-id?                    string
           +--ro remote-id?                     string
           +--ro mac-address?                   string
           +--ro global-online-fail?            string
           +--ro global-quick-offline?          string
           +--ro vlan-online-fail?              string
           +--ro vlan-quick-offline?            string
           +--ro global-online-fail-number?     string
           +--ro global-quick-offline-number?   string
           +--ro vlan-online-fail-number?       string
           +--ro vlan-quick-offline-number?     string
           +--ro index?                         string
           +--ro interface-name?                string
           +--ro pe-vlan?                       string
           +--ro ce-vlan?                       string
           +--ro free-time?                     string
           +--ro chasten-type?                  string

  rpcs:
    +---x reset-pppoe-user-login-failure-number
    |  +---w input
    |     +---w board-position    string
    +---x reset-ppp-slot-statistic
    |  +---w input
    |     +---w board-position    string
    +---x reset-pppoe-slot-statistic
    |  +---w input
    |     +---w board-position    string
    +---x reset-ppp-interface-statistic
    |  +---w input
    |     +---w interface-name    string
    +---x reset-pppoe-interface-statistic
    |  +---w input
    |     +---w interface-name    string
    +---x reset-chasten-ppp-user-by-mac
    |  +---w input
    |     +---w board-position    string
    |     +---w mac               pub-type:mac-address
    +---x reset-chasten-ppp-user-by-option105
    |  +---w input
    |     +---w board-position      string
    |     +---w (id-type)
    |        +--:(circuit-id)
    |        |  +---w circuit-id    string
    |        +--:(remote-id)
    |           +---w remote-id     string
    +---x reset-urpf-discard-statistic
    |  +---w input
    |     +---w reset-flag?   empty
    +---x reset-specified-urpf-statistic
       +---w input
          +---w ip-type           urpf-ip-type
          +---w board-position    string
