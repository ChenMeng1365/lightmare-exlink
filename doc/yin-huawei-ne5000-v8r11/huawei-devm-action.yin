<?xml version="1.0" encoding="UTF-8"?>
<submodule name="huawei-devm-action"
           xmlns="urn:ietf:params:xml:ns:yang:yin:1"
           xmlns:devm="urn:huawei:yang:huawei-devm"
           xmlns:pub-type="urn:huawei:yang:huawei-pub-type"
           xmlns:inet="urn:ietf:params:xml:ns:yang:ietf-inet-types"
           xmlns:ext="urn:huawei:yang:huawei-extension">
  <belongs-to module="huawei-devm">
    <prefix value="devm"/>
  </belongs-to>
  <import module="huawei-pub-type">
    <prefix value="pub-type"/>
  </import>
  <import module="ietf-inet-types">
    <prefix value="inet"/>
  </import>
  <import module="huawei-extension">
    <prefix value="ext"/>
  </import>
  <include module="huawei-devm-type"/>
  <organization>
    <text>Huawei Technologies Co., Ltd.</text>
  </organization>
  <contact>
    <text>Huawei Industrial Base
Bantian, Longgang
Shenzhen 518129
People's Republic of China
Website: http://www.huawei.com
Email: support@huawei.com</text>
  </contact>
  <description>
    <text>The submodule of devm : devm_action.</text>
  </description>
  <revision date="2020-04-10">
    <description>
      <text>Modify description warnings.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <revision date="2019-09-09">
    <description>
      <text>Change description.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <revision date="2019-04-03">
    <description>
      <text>Initial revision.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <rpc name="reset-board">
    <description>
      <text>Reset board.</text>
    </description>
    <input>
      <leaf name="board-position">
        <type name="string">
          <length value="1..32"/>
        </type>
        <mandatory value="true"/>
        <description>
          <text>Board position.</text>
        </description>
      </leaf>
    </input>
  </rpc>
  <rpc name="reset-mpu-board">
    <description>
      <text>Reset MPU.</text>
    </description>
    <ext:node-ref value="/devm/mpu-boards/mpu-board"/>
    <input>
      <leaf name="board-position">
        <type name="string">
          <length value="1..32"/>
        </type>
        <mandatory value="true"/>
        <description>
          <text>Board position.</text>
        </description>
      </leaf>
    </input>
  </rpc>
  <rpc name="reset-lpu-board">
    <description>
      <text>Reset LPU.</text>
    </description>
    <ext:node-ref value="/devm/lpu-boards/lpu-board"/>
    <input>
      <leaf name="board-position">
        <type name="string">
          <length value="1..32"/>
        </type>
        <mandatory value="true"/>
        <description>
          <text>Board position.</text>
        </description>
      </leaf>
    </input>
  </rpc>
  <rpc name="reset-sfu-board">
    <description>
      <text>Reset SFU.</text>
    </description>
    <ext:node-ref value="/devm/sfu-boards/sfu-board"/>
    <input>
      <leaf name="board-position">
        <type name="string">
          <length value="1..32"/>
        </type>
        <mandatory value="true"/>
        <description>
          <text>Board position.</text>
        </description>
      </leaf>
    </input>
  </rpc>
  <rpc name="reset-card">
    <description>
      <text>Reset Card.</text>
    </description>
    <ext:node-ref value="/devm/cards/card"/>
    <input>
      <leaf name="card-position">
        <type name="string">
          <length value="1..32"/>
        </type>
        <mandatory value="true"/>
        <description>
          <text>Card position.</text>
        </description>
      </leaf>
    </input>
  </rpc>
  <rpc name="reboot">
    <description>
      <text>Reboot.</text>
    </description>
    <input>
      <leaf name="save-flag">
        <type name="boolean"/>
        <default value="true"/>
        <description>
          <text>Flag indicating whether to save the configuration. True: save; False: not save.</text>
        </description>
      </leaf>
    </input>
  </rpc>
  <rpc name="transfer-pnpp">
    <description>
      <text>Transfer pnpp files.</text>
    </description>
    <input>
      <leaf name="server-ip-address">
        <type name="inet:ip-address-no-zone"/>
        <mandatory value="true"/>
        <description>
          <text>FTP server IP address.</text>
        </description>
      </leaf>
      <leaf name="server-port">
        <type name="uint32">
          <range value="21..65535"/>
        </type>
        <default value="21"/>
        <description>
          <text>Server port.</text>
        </description>
      </leaf>
      <leaf name="vpn-instance-name">
        <type name="string">
          <length value="1..128"/>
        </type>
        <description>
          <text>VPN instance name.</text>
        </description>
      </leaf>
      <leaf name="source-ipv4-address">
        <type name="inet:ipv4-address-no-zone"/>
        <description>
          <text>Source IPv4 address.</text>
        </description>
      </leaf>
      <leaf name="source-interface-name">
        <type name="pub-type:if-name"/>
        <description>
          <text>Source interface name.</text>
        </description>
      </leaf>
      <leaf name="server-type">
        <type name="server-type"/>
        <default value="ftp"/>
        <description>
          <text>Server type.</text>
        </description>
      </leaf>
      <leaf name="command-type">
        <type name="trans-type"/>
        <default value="put"/>
        <description>
          <text>Transmission type.</text>
        </description>
      </leaf>
      <leaf name="user-name">
        <type name="string">
          <length value="1..85"/>
        </type>
        <mandatory value="true"/>
        <description>
          <text>User name.</text>
        </description>
      </leaf>
      <leaf name="password">
        <type name="pub-type:password"/>
        <mandatory value="true"/>
        <description>
          <text>Password.</text>
        </description>
      </leaf>
      <leaf name="local-file-name">
        <type name="string">
          <length value="1..128"/>
        </type>
        <default value="pnpp.7z"/>
        <description>
          <text>Local file name.</text>
        </description>
      </leaf>
      <leaf name="remote-file-name">
        <type name="string">
          <length value="1..128"/>
        </type>
        <description>
          <text>File name on the FTP server.</text>
        </description>
      </leaf>
      <leaf name="remote-extension">
        <type name="string">
          <length value="1..5"/>
        </type>
        <default value=".hw!"/>
        <description>
          <text>Temporary file name extension used for reliability.</text>
        </description>
      </leaf>
      <container name="secure-transfer-options">
        <description>
          <text>Transfer encryption options.</text>
        </description>
        <leaf name="prefer-kex">
          <type name="prefer-kex-type"/>
          <description>
            <text>Encryption algorithm.</text>
          </description>
        </leaf>
        <leaf name="prefer-stoc-cipher">
          <type name="cipher-type"/>
          <description>
            <text>Encryption algorithm for messages from a client to server.</text>
          </description>
        </leaf>
        <leaf name="prefer-ctos-cipher">
          <type name="cipher-type"/>
          <description>
            <text>Encryption algorithm for messages from a server to client.</text>
          </description>
        </leaf>
        <leaf name="prefer-stoc-hmac">
          <type name="stoc-hmac-type"/>
          <description>
            <text>Prefer Stoc Hmac.</text>
          </description>
        </leaf>
        <leaf name="prefer-ctos-hmac">
          <type name="stoc-hmac-type"/>
          <description>
            <text>Prefer Ctos Hmac.</text>
          </description>
        </leaf>
        <leaf name="keep-alive-interval">
          <type name="uint32">
            <range value="0..60"/>
          </type>
          <default value="0"/>
          <description>
            <text>Keepalive interval.</text>
          </description>
        </leaf>
        <leaf name="keep-alive-count">
          <type name="uint32">
            <range value="0..100"/>
          </type>
          <default value="0"/>
          <description>
            <text>Number of Keepalive packets.</text>
          </description>
        </leaf>
      </container>
    </input>
  </rpc>
  <rpc name="reset-offline-config">
    <description>
      <text>Reset offline configuration.</text>
    </description>
    <input>
      <leaf name="board-position">
        <type name="string">
          <length value="1..32"/>
        </type>
        <mandatory value="true"/>
        <description>
          <text>Board position.</text>
        </description>
      </leaf>
    </input>
  </rpc>
  <rpc name="clear-inactive-config">
    <description>
      <text>Clear Inactive Configuration.</text>
    </description>
    <input>
      <leaf name="entity-class">
        <type name="offline-class-type"/>
        <mandatory value="true"/>
        <description>
          <text>Class.</text>
        </description>
      </leaf>
      <leaf name="position">
        <type name="string">
          <length value="1..32"/>
        </type>
        <mandatory value="true"/>
        <description>
          <text>Position.</text>
        </description>
      </leaf>
    </input>
  </rpc>
  <rpc name="cold-reset-lpu">
    <description>
      <text>Cold reset LPU.</text>
    </description>
    <ext:node-ref value="/devm/lpu-boards/lpu-board"/>
    <input>
      <leaf name="board-position">
        <type name="string">
          <length value="1..32"/>
        </type>
        <mandatory value="true"/>
        <description>
          <text>Board position.</text>
        </description>
      </leaf>
    </input>
  </rpc>
  <rpc name="warm-reset-lpu">
    <description>
      <text>Warm reset LPU.</text>
    </description>
    <ext:node-ref value="/devm/lpu-boards/lpu-board"/>
    <input>
      <leaf name="board-position">
        <type name="string">
          <length value="1..32"/>
        </type>
        <mandatory value="true"/>
        <description>
          <text>Board position.</text>
        </description>
      </leaf>
    </input>
  </rpc>
  <rpc name="cold-reset-mpu">
    <description>
      <text>Cold reset MPU.</text>
    </description>
    <ext:node-ref value="/devm/mpu-boards/mpu-board"/>
    <input>
      <leaf name="board-position">
        <type name="string">
          <length value="1..32"/>
        </type>
        <mandatory value="true"/>
        <description>
          <text>Board position.</text>
        </description>
      </leaf>
    </input>
  </rpc>
  <rpc name="warm-reset-mpu">
    <description>
      <text>Warm reset MPU.</text>
    </description>
    <ext:node-ref value="/devm/mpu-boards/mpu-board"/>
    <input>
      <leaf name="board-position">
        <type name="string">
          <length value="1..32"/>
        </type>
        <mandatory value="true"/>
        <description>
          <text>Board position.</text>
        </description>
      </leaf>
    </input>
  </rpc>
  <rpc name="schedule-reboot-at-time">
    <description>
      <text>Schedule reboot at time.</text>
    </description>
    <input>
      <leaf name="schedule-date">
        <type name="pub-type:date"/>
        <mandatory value="true"/>
        <description>
          <text>Date.</text>
        </description>
      </leaf>
      <leaf name="schedule-datetime">
        <type name="pub-type:time-hhmm"/>
        <mandatory value="true"/>
        <description>
          <text>Time.</text>
        </description>
      </leaf>
      <leaf name="save-flag">
        <type name="boolean"/>
        <default value="true"/>
        <description>
          <text>Flag indicating whether save config.</text>
        </description>
      </leaf>
    </input>
  </rpc>
  <rpc name="schedule-reboot-delay-time">
    <description>
      <text>Schedule reboot delay time.</text>
    </description>
    <input>
      <leaf name="delay-time">
        <type name="string">
          <length value="1..6"/>
        </type>
        <mandatory value="true"/>
        <description>
          <text>Delay time, total minutes or hours:minutes (mm or hh:mm), the max value is 720 hours.</text>
        </description>
      </leaf>
      <leaf name="save-flag">
        <type name="boolean"/>
        <default value="true"/>
        <description>
          <text>Flag indicating whether save config.</text>
        </description>
      </leaf>
    </input>
  </rpc>
  <rpc name="undo-schedule-reboot">
    <description>
      <text>Undo Schedule reboot.</text>
    </description>
  </rpc>
</submodule>
