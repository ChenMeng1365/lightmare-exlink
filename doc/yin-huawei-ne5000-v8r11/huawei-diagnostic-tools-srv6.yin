  <rpc name="srv6-te-start-ping">
    <description>
      <text>Start srv6 te ping.</text>
    </description>
    <ext:node-ref value="/diagnostic-tools:diagnostic-tools/diagnostic-tools:srv6/diagnostic-tools:ping-results"/>
    <input>
      <leaf name="test-name">
        <type name="string">
          <length value="1..32"/>
        </type>
        <mandatory value="true"/>
        <description>
          <text>Test name.</text>
        </description>
      </leaf>
      <choice name="type">
        <description>
          <text>The type of srv6-te policy ping.</text>
        </description>
        <case name="policy-name">
          <description>
            <text>Policy name.</text>
          </description>
          <leaf name="policy-name">
            <type name="string">
              <length value="1..32"/>
            </type>
            <mandatory value="true"/>
            <description>
              <text>Policy name.</text>
            </description>
          </leaf>
        </case>
        <case name="endpoint-ip">
          <description>
            <text>Endpoint IP.</text>
          </description>
          <leaf name="end-point-ip">
            <type name="inet:ipv6-address-no-zone"/>
            <mandatory value="true"/>
            <description>
              <text>End point IP address.</text>
            </description>
          </leaf>
          <leaf name="color">
            <type name="uint32">
              <range value="0..4294967295"/>
            </type>
            <mandatory value="true"/>
            <description>
              <text>Color.</text>
            </description>
          </leaf>
        </case>
        <case name="binding-sid">
          <description>
            <text>Binding SID.</text>
          </description>
          <leaf name="binding-sid">
            <type name="inet:ipv6-address-no-zone"/>
            <mandatory value="true"/>
            <description>
              <text>Binding segmentlist ID.</text>
            </description>
          </leaf>
        </case>
      </choice>
      <leaf name="source-address">
        <type name="inet:ipv6-address-no-zone"/>
        <description>
          <text>Source Address.</text>
        </description>
      </leaf>
      <leaf name="interval">
        <type name="uint32">
          <range value="1..10000"/>
        </type>
        <units name="ms"/>
        <default value="2000"/>
        <description>
          <text>Interval for sending test packets.</text>
        </description>
      </leaf>
      <leaf name="packet-size">
        <type name="uint32">
          <range value="65..9600"/>
        </type>
        <default value="100"/>
        <description>
          <text>Number of bytes in a sent test packet.</text>
        </description>
      </leaf>
      <leaf name="count">
        <type name="uint32">
          <range value="1..4294967295"/>
        </type>
        <default value="5"/>
        <description>
          <text>Number of sent test packets.</text>
        </description>
      </leaf>
      <leaf name="timeout">
        <type name="uint32">
          <range value="0..65535"/>
        </type>
        <units name="ms"/>
        <default value="2000"/>
        <description>
          <text>Time to wait for a response packet after a test packet is sent. If no response packet is received within the timeout period, the system considers that the test packet times out.</text>
        </description>
      </leaf>
      <leaf name="traffic-class">
        <type name="uint32">
          <range value="0..255"/>
        </type>
        <description>
          <text>Traffic class value of IPv6 packets. The traffic class value is used to distinguish the service types of IPv6 packets.</text>
        </description>
      </leaf>
      <leaf name="hop-limit">
        <type name="uint32">
          <range value="1..255"/>
        </type>
        <default value="1"/>
        <description>
          <text>Hop limit of send packets.</text>
        </description>
      </leaf>
      <leaf name="end-op">
        <type name="inet:ipv6-address-no-zone"/>
        <description>
          <text>End-OP segment ID.</text>
        </description>
      </leaf>
    </input>
  </rpc>
  <rpc name="srv6-te-stop-ping">
    <description>
      <text>Stop srv6 te ping.</text>
    </description>
    <ext:node-ref value="/diagnostic-tools:diagnostic-tools/diagnostic-tools:srv6/diagnostic-tools:ping-results"/>
    <input>
      <leaf name="test-name">
        <type name="leafref">
          <path value="/diagnostic-tools:diagnostic-tools/diagnostic-tools:srv6/diagnostic-tools:ping-results/diagnostic-tools:ping-result/diagnostic-tools:test-name"/>
        </type>
        <mandatory value="true"/>
        <description>
          <text>Test name.</text>
        </description>
      </leaf>
    </input>
  </rpc>
