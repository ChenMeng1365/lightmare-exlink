  <container name="operation">
    <config value="false"/>
    <description>
      <text>Command of MLD snooping clear.</text>
    </description>
    <container name="mld-snooping">
      <description>
        <text>Clear MLD snooping groups.</text>
      </description>
      <container name="clear">
        <description>
          <text>Clear MLD snooping groups.</text>
        </description>
        <container name="group">
          <description>
            <text>Clear the specific group of the specific instance.</text>
          </description>
          <presence value="group"/>
          <leaf name="group-ip">
            <description>
              <text>Multicast group IP address.</text>
            </description>
            <type name="ncd:ipv6-address"/>
            <mandatory value="true"/>
          </leaf>
          <leaf name="source-ip">
            <description>
              <text>Source IP address.</text>
            </description>
            <type name="ncd:ipv6-address"/>
          </leaf>
          <choice name="instance">
            <description>
              <text>Instance.</text>
            </description>
            <mandatory value="true"/>
            <case name="vlan">
              <description>
                <text>Clear all groups of the specific VLAN instance.</text>
              </description>
              <leaf name="vlan-id">
                <description>
                  <text>VLAN ID.</text>
                </description>
                <type name="uint16">
                  <range value="1..4094"/>
                </type>
              </leaf>
            </case>
            <case name="vpls">
              <description>
                <text>Clear all groups of the specific VPLS instance.</text>
              </description>
              <leaf name="vpls-name">
                <description>
                  <text>VPLS name.</text>
                </description>
                <type name="string">
                  <length value="1..32"/>
                </type>
              </leaf>
            </case>
            <case name="ip-global">
              <description>
                <text>Clear all groups of the specific IP-global instance.</text>
              </description>
              <leaf name="ip-global">
                <description>
                  <text>IP-global.</text>
                </description>
                <type name="empty"/>
              </leaf>
            </case>
            <case name="ring-tree">
              <description>
                <text>Clear all groups of the specific Ring Tree instance.</text>
              </description>
              <leaf name="ring-tree-name">
                <description>
                  <text>Ring Tree name.</text>
                </description>
                <type name="string">
                  <length value="1..32"/>
                </type>
              </leaf>
              <ncd:revise-date date="2017-11-02"/>
            </case>
            <case name="vrf">
              <description>
                <text>Clear all groups of the specific VRF instance.</text>
              </description>
              <leaf name="vrf-name">
                <description>
                  <text>VRF name.</text>
                </description>
                <type name="string">
                  <length value="1..32"/>
                </type>
              </leaf>
              <ncd:revise-date date="2017-12-08"/>
            </case>
          </choice>
        </container>
        <container name="groups">
          <description>
            <text>Clear all groups of the specific instance.</text>
          </description>
          <choice name="type">
            <description>
              <text>Clear all groups of the specific instance.</text>
            </description>
            <case name="vlan">
              <description>
                <text>Clear all groups of the specific VLAN instance.</text>
              </description>
              <leaf name="vlan-id">
                <description>
                  <text>VLAN ID.</text>
                </description>
                <type name="uint16">
                  <range value="1..4094"/>
                </type>
              </leaf>
            </case>
            <case name="vpls">
              <description>
                <text>Clear all groups of the specific VPLS instance.</text>
              </description>
              <leaf name="vpls-name">
                <description>
                  <text>VPLS name.</text>
                </description>
                <type name="string">
                  <length value="1..32"/>
                </type>
              </leaf>
            </case>
            <case name="ip-global">
              <description>
                <text>Clear all groups of the specific IP-global instance.</text>
              </description>
              <leaf name="ip-global">
                <description>
                  <text>IP-global.</text>
                </description>
                <type name="empty"/>
              </leaf>
            </case>
            <case name="ring-tree">
              <description>
                <text>Clear all groups of the specific Ring Tree instance.</text>
              </description>
              <leaf name="ring-tree-name">
                <description>
                  <text>Ring Tree name.</text>
                </description>
                <type name="string">
                  <length value="1..32"/>
                </type>
              </leaf>
              <ncd:revise-date date="2017-11-02"/>
            </case>
            <case name="vrf">
              <description>
                <text>Clear all groups of the specific VRF instance.</text>
              </description>
              <leaf name="vrf-name">
                <description>
                  <text>VRF name.</text>
                </description>
                <type name="string">
                  <length value="1..32"/>
                </type>
              </leaf>
              <ncd:revise-date date="2017-12-08"/>
            </case>
            <case name="all">
              <description>
                <text>Clear all groups.</text>
              </description>
              <leaf name="all-groups">
                <description>
                  <text>All groups.</text>
                </description>
                <type name="empty"/>
              </leaf>
            </case>
          </choice>
        </container>
      </container>
    </container>
  </container>
