<?xml version="1.0" encoding="UTF-8"?>
<module name="zxr10-snetconf"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:snetc="http://www.zte.com.cn/zxr10/netconf/schema/rosng/snetconf">
  <namespace uri="http://www.zte.com.cn/zxr10/netconf/schema/rosng/snetconf"/>
  <prefix value="snetc"/>
  <organization>
    <text>ZTE Corporation</text>
  </organization>
  <contact>
    <text>ZTE Corporation

         Nanjing Institute of ZTE Corporation



         No.68 Zijinghua Rd. Yuhuatai District, Nanjing, China



         Tel: +86-25-52870000</text>
  </contact>
  <description>
    <text>ZXR10 ROSNG:the netconf model for snetconf</text>
  </description>
  <revision date="2015-01-12">
    <description>
      <text>modify the listenning port of snetconf</text>
    </description>
  </revision>
  <revision date="2013-11-25">
    <description>
      <text>add description to some nodes; modify range for port node;</text>
    </description>
  </revision>
  <revision date="2013-01-31">
    <description>
      <text>initial version</text>
    </description>
  </revision>
  <container name="configuration">
    <description>
      <text>configuration model</text>
    </description>
    <container name="snetconf">
      <description>
        <text>Configure SNETCONF parameters</text>
      </description>
      <container name="server">
        <description>
          <text>Configure snetconf server parameters</text>
        </description>
        <choice name="status">
          <description>
            <text>The status of snetconf server,the default state is disable</text>
          </description>
          <default value="disable"/>
          <case name="disable">
            <leaf name="disable">
              <description>
                <text>disable snetconf server</text>
              </description>
              <type name="empty"/>
            </leaf>
          </case>
          <case name="enable">
            <leaf name="enable">
              <description>
                <text>enable snetconf server</text>
              </description>
              <type name="empty"/>
            </leaf>
          </case>
        </choice>
        <leaf name="port">
          <description>
            <text>The listenning port of snetconf. the default value is 830</text>
          </description>
          <type name="uint16">
            <range value="22 | 830 | 1025..20000"/>
          </type>
          <default value="830"/>
        </leaf>
      </container>
    </container>
  </container>
</module>
