  <ext:task-name value="ospf"/>
  <grouping name="policy-type">
    <description>
      <text>Type of a filtering rule. The available options are ACL Name or ACL Number or IP Prefix List or Route policy.</text>
    </description>
    <choice name="policy-type">
      <mandatory value="true"/>
      <description>
        <text>Type of a filtering rule.</text>
      </description>
      <case name="basic-acl">
        <description>
          <text>Apply basic ACL Name or ACL Number.</text>
        </description>
        <leaf name="name-or-num">
          <type name="leafref">
            <path value="/acl:acl/acl:group6s/acl:group6/acl:identity"/>
          </type>
          <must condition="/acl:acl/acl:group6s/acl:group6[acl:identity=current()]/acl:type='basic'"/>
          <description>
            <text>Apply basic ACL Name or ACL Number.</text>
          </description>
        </leaf>
      </case>
      <case name="ipv6-prefix">
        <description>
          <text>Set the name of an IPv6 prefix list.</text>
        </description>
        <leaf name="ipv6-prefix">
          <type name="leafref">
            <path value="/rtp:routing-policy/rtp:ipv6-prefix-filters/rtp:ipv6-prefix-filter/rtp:name"/>
          </type>
          <description>
            <text>Set the name of an IPv6 prefix list.</text>
          </description>
        </leaf>
      </case>
      <case name="route-policy">
        <description>
          <text>Set a route policy name.</text>
        </description>
        <leaf name="route-policy">
          <type name="leafref">
            <path value="/rtp:routing-policy/rtp:policy-definitions/rtp:policy-definition/rtp:name"/>
          </type>
          <description>
            <text>Set a route policy name.</text>
          </description>
        </leaf>
      </case>
      <case name="route-filter">
        <description>
          <text>Set a route filter name.</text>
        </description>
        <leaf name="route-filter">
          <type name="leafref">
            <path value="/xpl:xpl/xpl:route-filters/xpl:route-filter/xpl:name"/>
          </type>
          <description>
            <text>Set a route filter name.</text>
          </description>
        </leaf>
      </case>
    </choice>
  </grouping>
  <container name="ospfv3">
    <description>
      <text>Configure OSPFv3 config.</text>
    </description>
    <container name="check-rt-id">
      <description>
        <text>Configure automatic modification of conflicted router IDs. If enable, check and resolve OSPF router ID conflict.</text>
      </description>
      <leaf name="conflict-flag">
        <type name="boolean"/>
        <default value="true"/>
        <description>
          <text>Enable/disable automatic modification of conflicted router IDs. If enable, check and resolve OSPF router ID conflict.</text>
        </description>
      </leaf>
    </container>
    <container name="advertise-max-cost">
      <description>
        <text>Configure OSPF LSA advertise max cost globally.</text>
      </description>
      <leaf name="enable">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable OSPF LSAs to advertise max cost globally.</text>
        </description>
      </leaf>
    </container>
    <container name="session-car">
      <must condition="pir-interval&gt;=cir-interval"/>
      <must condition="pbs-interval&gt;=cbs-interval"/>
      <description>
        <text>Configure OSPFv3 Session CAR.</text>
      </description>
      <leaf name="enable">
        <type name="boolean"/>
        <default value="true"/>
        <description>
          <text>Enable/disable OSPFv3 Session CAR.</text>
        </description>
      </leaf>
      <leaf name="cir-interval">
        <type name="uint32">
          <range value="0..1000000"/>
        </type>
        <units name="kbit/s"/>
        <default value="8"/>
        <description>
          <text>Specify the Session CAR CIR interval.</text>
        </description>
      </leaf>
      <leaf name="cbs-interval">
        <type name="uint32">
          <range value="0..1000000"/>
        </type>
        <units name="Byte"/>
        <default value="100000"/>
        <description>
          <text>Specify the Session CAR CBS interval.</text>
        </description>
      </leaf>
      <leaf name="pir-interval">
        <type name="uint32">
          <range value="0..1000000"/>
        </type>
        <units name="kbit/s"/>
        <default value="16000"/>
        <description>
          <text>Specify the Session CAR PIR interval.</text>
        </description>
      </leaf>
      <leaf name="pbs-interval">
        <type name="uint32">
          <range value="0..10000000"/>
        </type>
        <units name="Byte"/>
        <default value="9000000"/>
        <description>
          <text>Specify the Session CAR PBS interval.</text>
        </description>
      </leaf>
    </container>
    <container name="micro-isolation-car">
      <must condition="pir&gt;=cir"/>
      <must condition="pbs&gt;=cbs"/>
      <description>
        <text>Configure OSPFv3 Micro Isolation CAR.</text>
      </description>
      <leaf name="enable">
        <type name="boolean"/>
        <default value="true"/>
        <description>
          <text>Enable/disable OSPFv3 Micro Isolation CAR.</text>
        </description>
      </leaf>
      <leaf name="cir">
        <type name="uint32">
          <range value="0..1000000"/>
        </type>
        <units name="kbit/s"/>
        <default value="8"/>
        <description>
          <text>Specify the Micro Isolation CAR CIR interval.</text>
        </description>
      </leaf>
      <leaf name="cbs">
        <type name="uint32">
          <range value="0..1000000"/>
        </type>
        <units name="Byte"/>
        <default value="100000"/>
        <description>
          <text>Specify the Micro Isolation CAR CBS interval.</text>
        </description>
      </leaf>
      <leaf name="pir">
        <type name="uint32">
          <range value="0..1000000"/>
        </type>
        <units name="kbit/s"/>
        <default value="16000"/>
        <description>
          <text>Specify the Micro Isolation CAR PIR interval.</text>
        </description>
      </leaf>
      <leaf name="pbs">
        <type name="uint32">
          <range value="0..10000000"/>
        </type>
        <units name="Byte"/>
        <default value="9000000"/>
        <description>
          <text>Specify the Micro Isolation CAR PBS interval.</text>
        </description>
      </leaf>
    </container>
    <container name="mib-binding">
      <description>
        <text>Configure MIB binding an OSPF process.</text>
      </description>
      <leaf name="process-id">
        <type name="leafref">
          <path value="/ni:network-instance/ni:instances/ni:instance/ospfv3:ospfv3/ospfv3:sites/ospfv3:site/ospfv3:process-id"/>
        </type>
        <description>
          <text>Process ID.</text>
        </description>
      </leaf>
    </container>
    <container name="flush-source-trace">
      <description>
        <text>Configure OSPF flush source trace globally.</text>
      </description>
      <leaf name="enable">
        <type name="boolean"/>
        <default value="true"/>
        <description>
          <text>Enable/disable OSPF flush source trace globally.</text>
        </description>
      </leaf>
      <leaf name="port">
        <type name="uint16">
          <range value="1025..65535"/>
        </type>
        <default value="50122"/>
        <description>
          <text>Set OSPF flush source trace port allocated by huawei-pp4.The related CLI is 'display ip port info | inc UDP'.</text>
        </description>
      </leaf>
    </container>
  </container>
  <rpc name="reset-redistribution">
    <description>
      <text>To re-import routes.</text>
    </description>
    <input>
      <leaf name="process-id">
        <type name="uint32">
          <range value="1..4294967295"/>
        </type>
        <description>
          <text>Process ID.</text>
        </description>
      </leaf>
    </input>
  </rpc>
  <rpc name="reset-counter">
    <description>
      <text>To clear the counter.</text>
    </description>
    <input>
      <leaf name="process-id">
        <type name="uint32">
          <range value="1..4294967295"/>
        </type>
        <description>
          <text>Process ID.</text>
        </description>
      </leaf>
      <leaf name="router-id">
        <type name="inet:ipv4-address-no-zone"/>
        <description>
          <text>Neighbor router ID.</text>
        </description>
      </leaf>
      <leaf name="if-name">
        <type name="leafref">
          <path value="/ifm:ifm/ifm:interfaces/ifm:interface/ifm:name"/>
        </type>
        <description>
          <text>Interface name.</text>
        </description>
      </leaf>
    </input>
  </rpc>
  <rpc name="reset-process">
    <description>
      <text>To restart an OSPF process.</text>
    </description>
    <input>
      <leaf name="process-id">
        <type name="uint32">
          <range value="1..4294967295"/>
        </type>
        <description>
          <text>Process ID.</text>
        </description>
      </leaf>
    </input>
  </rpc>
  <rpc name="reset-peer">
    <description>
      <text>To re-establish a neighbor.</text>
    </description>
    <input>
      <leaf name="process-id">
        <type name="uint32">
          <range value="1..4294967295"/>
        </type>
        <description>
          <text>Process ID.</text>
        </description>
      </leaf>
      <leaf name="router-id">
        <type name="inet:ipv4-address-no-zone"/>
        <description>
          <text>Neighbor router ID.</text>
        </description>
      </leaf>
      <leaf name="if-name">
        <type name="leafref">
          <path value="/ifm:ifm/ifm:interfaces/ifm:interface/ifm:name"/>
        </type>
        <description>
          <text>Interface name.</text>
        </description>
      </leaf>
    </input>
  </rpc>
  <rpc name="reset-frr">
    <description>
      <text>Restart FRR.</text>
    </description>
    <input>
      <leaf name="process-id">
        <type name="uint32">
          <range value="1..4294967295"/>
        </type>
        <description>
          <text>Process ID.</text>
        </description>
      </leaf>
    </input>
  </rpc>
  <rpc name="reset-suppress-peer">
    <description>
      <text>Restart OSPF peer flapping suppress.</text>
    </description>
    <input>
      <leaf name="process-id">
        <type name="uint32">
          <range value="1..4294967295"/>
        </type>
        <description>
          <text>Process ID.</text>
        </description>
      </leaf>
      <leaf name="if-name">
        <type name="leafref">
          <path value="/ifm:ifm/ifm:interfaces/ifm:interface/ifm:name"/>
        </type>
        <description>
          <text>Interface name.</text>
        </description>
      </leaf>
      <leaf name="notify-peer-flag">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable notification neighbor quit peer flapping suppress flag.</text>
        </description>
      </leaf>
    </input>
  </rpc>
  <augment target-node="/ni:network-instance/ni:instances/ni:instance">
    <description>
      <text>OSPFv3 augment to L3VPN.</text>
    </description>
    <container name="ospfv3">
      <description>
        <text>Configure OSPFv3.</text>
      </description>
      <container name="gtsm">
        <when condition="../../l3vpn:afs/l3vpn:af[l3vpn:type='ipv6-unicast']"/>
        <presence value="Enable gtsm."/>
        <description>
          <text>Configure the OSPFv3 GTSM.</text>
        </description>
        <leaf name="hops">
          <type name="uint8">
            <range value="1..255"/>
          </type>
          <mandatory value="true"/>
          <description>
            <text>Specifies the number of TTL hops to be detected.</text>
          </description>
        </leaf>
      </container>
      <container name="sites">
        <when condition="../../l3vpn:afs/l3vpn:af[l3vpn:type='ipv6-unicast']"/>
        <description>
          <text>List of OSPFv3 instance.</text>
        </description>
        <list name="site">
          <key value="process-id"/>
          <description>
            <text>Configure OSPFv3 instance.</text>
          </description>
          <leaf name="process-id">
            <type name="uint32">
              <range value="1..4294967295"/>
            </type>
            <description>
              <text>Process ID.</text>
            </description>
          </leaf>
          <leaf name="router-id">
            <type name="inet:ipv4-address-no-zone"/>
            <description>
              <text>Router ID. It is used to set the OSPFv3 private ID.</text>
            </description>
          </leaf>
          <leaf name="fa-mode">
            <type name="forwarding-address-type"/>
            <default value="disable"/>
            <description>
              <text>Forwarding address mode.</text>
            </description>
          </leaf>
          <leaf name="description">
            <type name="string">
              <length value="1..80"/>
              <pattern value="([^?]*)"/>
            </type>
            <description>
              <text>Description of an OSPFv3 process.</text>
            </description>
          </leaf>
          <leaf name="rfc1583-compatible">
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable the route selection rule that is compatible with RFC 1583.</text>
            </description>
          </leaf>
          <leaf name="silent-all-interface">
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable to suppress all interfaces in an OSPF process from sending or receiving OSPF packets.</text>
            </description>
          </leaf>
          <leaf name="retransmission-limit">
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable maximum retransmission times enable.</text>
            </description>
          </leaf>
          <leaf name="retransmission-limit-value">
            <when condition="../retransmission-limit='true'"/>
            <type name="uint32">
              <range value="2..255"/>
            </type>
            <default value="30"/>
            <description>
              <text>Maximum retransmission times.</text>
            </description>
          </leaf>
          <leaf name="lsa-refresh">
            <type name="boolean"/>
            <default value="true"/>
            <description>
              <text>Enable/disable refresh the age of LSA with greater lsa age.</text>
            </description>
          </leaf>
          <leaf name="peer-flapping-suppress">
            <type name="boolean"/>
            <default value="true"/>
            <description>
              <text>Enable/disable peer flapping suppress feature.</text>
            </description>
          </leaf>
          <leaf name="qppb-enable">
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable QPPB on this process.</text>
            </description>
          </leaf>
          <leaf name="sham-hello">
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable peers keep alive by receiving any valid OSPF packet.</text>
            </description>
          </leaf>
          <leaf name="max-link-cost">
            <type name="uint16">
              <range value="1..65534"/>
            </type>
            <description>
              <text>Set OSPFv3 maximum link cost value.</text>
            </description>
          </leaf>
          <leaf name="maxage-lsa">
            <type name="uint32">
              <range value="0..65535"/>
            </type>
            <units name="s"/>
            <default value="20"/>
            <description>
              <text>Calculation of route delay when the OSPF LSA frequently flaps.</text>
            </description>
          </leaf>
          <container name="private-net">
            <when condition="../../../../ni:name!='_public_'"/>
            <description>
              <text>Configure private network configuration of OSPFv3.</text>
            </description>
            <choice name="route-tag">
              <description>
                <text>To set route tag value.</text>
              </description>
              <case name="value">
                <description>
                  <text>Tag of an imported VPN route.</text>
                </description>
                <leaf name="value">
                  <type name="uint32">
                    <range value="0..4294967295"/>
                  </type>
                  <description>
                    <text>Tag of an imported VPN route. By default, the first two bytes of the tag value are fixed to be 0xD000, and the last two bytes are the local AS number of the local BGP. For example, if the AS number of BGP is 100, the default tag value in decimal notation is 3489661028.If not configed, the default is 0.</text>
                  </description>
                </leaf>
              </case>
            </choice>
            <leaf name="vpn-capability">
              <type name="boolean"/>
              <default value="false"/>
              <description>
                <text>Enable/disable route loop detection and directly calculate routes. When VPN multi instance is supported on the Multi VPN Instance CE (MCE) router, loop detection must be cancelled. It is applicable to only OSPF VPN instances.</text>
              </description>
            </leaf>
            <container name="domain-id">
              <presence value="ID of an OSPF domain."/>
              <description>
                <text>Configure ID of an OSPF domain.</text>
              </description>
              <leaf name="domain-id">
                <type name="inet:ipv4-address-no-zone"/>
                <mandatory value="true"/>
                <description>
                  <text>ID of an OSPF domain.</text>
                </description>
              </leaf>
              <leaf name="type">
                <type name="domain-id-type"/>
                <default value="0x0005"/>
                <description>
                  <text>Type of an OSPF domain ID.</text>
                </description>
              </leaf>
              <leaf name="value">
                <type name="pub-type:hex-binary">
                  <length value="1..6"/>
                  <pattern value="((0x|0X)?[0-9a-fA-F]{0,4})"/>
                </type>
                <default value="0x0"/>
                <description>
                  <text>Value of an OSPF domain ID type.</text>
                </description>
              </leaf>
            </container>
            <container name="domain-id-secondarys">
              <when condition="../domain-id"/>
              <description>
                <text>List of the ID of a secondary domain.</text>
              </description>
              <list name="domain-id-secondary">
                <key value="domain-id type value"/>
                <description>
                  <text>Configure the ID of a secondary domain.</text>
                </description>
                <leaf name="domain-id">
                  <type name="inet:ipv4-address-no-zone"/>
                  <description>
                    <text>ID of an OSPF domain.</text>
                  </description>
                </leaf>
                <leaf name="type">
                  <type name="domain-id-type"/>
                  <description>
                    <text>Type of an OSPF domain ID.</text>
                  </description>
                </leaf>
                <leaf name="value">
                  <type name="pub-type:hex-binary">
                    <length value="1..6"/>
                    <pattern value="((0x|0X)?[0-9a-fA-F]{0,4})"/>
                  </type>
                  <description>
                    <text>Value of an OSPF domain ID type.</text>
                  </description>
                </leaf>
              </list>
            </container>
          </container>
          <container name="bandwidth">
            <description>
              <text>Configure bandwidth.</text>
            </description>
            <leaf name="reference-value">
              <type name="uint32">
                <range value="1..2147483648"/>
              </type>
              <default value="100"/>
              <description>
                <text>Reference to the calculation of link costs.</text>
              </description>
            </leaf>
          </container>
          <container name="timer">
            <description>
              <text>Configure OSPFv3 timer.</text>
            </description>
            <container name="lsa-arrival">
              <must condition="((start-interval&lt;=max-interval) and (hold-interval&lt;=max-interval)) or (not(max-interval) and not(start-interval)and not(hold-interval))"/>
              <description>
                <text>Configure interval between acceptance for two new instances of the same LSA.</text>
              </description>
              <choice name="timer-type">
                <default value="intelligent-timer"/>
                <description>
                  <text>Interval between acceptance for two new instances of the same LSA.</text>
                </description>
                <case name="timer">
                  <description>
                    <text>Specify an interval.</text>
                  </description>
                  <leaf name="interval">
                    <type name="uint32">
                      <range value="0..10000"/>
                    </type>
                    <units name="ms"/>
                    <description>
                      <text>Interval for receiving LSAs.</text>
                    </description>
                  </leaf>
                </case>
                <case name="intelligent-timer">
                  <description>
                    <text>Specify an intelligent timer.</text>
                  </description>
                  <leaf name="max-interval">
                    <type name="uint32">
                      <range value="1..120000"/>
                    </type>
                    <units name="ms"/>
                    <default value="1000"/>
                    <description>
                      <text>Maximum interval for receiving OSPFv3 LSAs.</text>
                    </description>
                  </leaf>
                  <leaf name="start-interval">
                    <type name="uint32">
                      <range value="0..60000"/>
                    </type>
                    <units name="ms"/>
                    <default value="500"/>
                    <description>
                      <text>Initial interval for receiving OSPFv3 LSAs.</text>
                    </description>
                  </leaf>
                  <leaf name="hold-interval">
                    <type name="uint32">
                      <range value="1..60000"/>
                    </type>
                    <units name="ms"/>
                    <default value="500"/>
                    <description>
                      <text>Hold interval for receiving OSPFv3 LSAs.</text>
                    </description>
                  </leaf>
                </case>
              </choice>
            </container>
            <container name="lsa-arr-supp-flap">
              <description>
                <text>Configure the delay timer for LSA acceptance.</text>
              </description>
              <leaf name="interval">
                <type name="uint32">
                  <range value="0..600"/>
                </type>
                <units name="s"/>
                <default value="10"/>
                <description>
                  <text>Specifies an interval for suppressing LSA advertising in case of route flapping. The value 0 indicates that LSA advertising is not suppressed.</text>
                </description>
              </leaf>
              <leaf name="threshold-count">
                <type name="uint32">
                  <range value="3..100"/>
                </type>
                <default value="30"/>
                <description>
                  <text>Value of the threshold of LSAs to be suppressed.</text>
                </description>
              </leaf>
            </container>
            <container name="lsa-originate">
              <must condition="((start-interval&lt;=max-interval) and (hold-interval&lt;=max-interval)) or (not(max-interval) and not(start-interval)and not(hold-interval))"/>
              <description>
                <text>Configure interval for receiving LSAs either through an intelligent timer or timer.</text>
              </description>
              <leaf name="other-type-interval">
                <type name="uint32">
                  <range value="0..10"/>
                </type>
                <units name="s"/>
                <default value="5"/>
                <description>
                  <text>Specify an updating/originating interval for Type 3, Type 4 and Type 10 LSAs. Interval for updating the LSAs except OSPF router LSAs and network LSAs.</text>
                </description>
              </leaf>
              <choice name="timer-type">
                <default value="interval"/>
                <description>
                  <text>Specify an updating/originating interval for Type 3, Type 4 and Type 10 LSAs.</text>
                </description>
                <case name="interval">
                  <description>
                    <text>Specify an intelligent timer for router LSA, network LSA, external LSA and NSSA LSA.</text>
                  </description>
                  <leaf name="max-interval">
                    <type name="uint32">
                      <range value="1..120000"/>
                    </type>
                    <default value="5000"/>
                    <description>
                      <text>Maximum interval for updating OSPFv3 LSAs.</text>
                    </description>
                  </leaf>
                  <leaf name="start-interval">
                    <type name="uint32">
                      <range value="0..60000"/>
                    </type>
                    <default value="500"/>
                    <description>
                      <text>Initial interval for updating OSPFv3 LSAs.</text>
                    </description>
                  </leaf>
                  <leaf name="hold-interval">
                    <type name="uint32">
                      <range value="1..60000"/>
                    </type>
                    <default value="1000"/>
                    <description>
                      <text>Hold interval for updating OSPFv3 LSAs.</text>
                    </description>
                  </leaf>
                </case>
              </choice>
            </container>
            <container name="lsa-org-supp-flap">
              <description>
                <text>Configure the delay timer for LSA generation.</text>
              </description>
              <leaf name="interval">
                <type name="uint32">
                  <range value="0..600"/>
                </type>
                <units name="s"/>
                <default value="10"/>
                <description>
                  <text>Specifies an interval for suppressing LSA advertising in case of route flapping. The value 0 indicates that LSA advertising is not suppressed.</text>
                </description>
              </leaf>
              <leaf name="threshold-count">
                <type name="uint32">
                  <range value="3..100"/>
                </type>
                <default value="30"/>
                <description>
                  <text>Value of the threshold of LSAs to be suppressed.</text>
                </description>
              </leaf>
            </container>
            <container name="spf-schedule">
              <must condition="((start-interval&lt;=max-interval) and (hold-interval&lt;=max-interval)) or (not(max-interval) and not(start-interval)and not(hold-interval))"/>
              <description>
                <text>Configure the interval for SPF calculation.</text>
              </description>
              <choice name="schedule-type">
                <default value="intelligent-timer"/>
                <description>
                  <text>Set the interval for SPF calculation.</text>
                </description>
                <case name="timer">
                  <description>
                    <text>Specify an updating/originating interval for Type 3, Type 4 and Type 10 LSAs.</text>
                  </description>
                  <leaf name="type">
                    <type name="spf-timer-type"/>
                    <mandatory value="true"/>
                    <description>
                      <text>SPF timer type.</text>
                    </description>
                  </leaf>
                  <leaf name="delay-interval">
                    <type name="uint32">
                      <range value="0..65535"/>
                    </type>
                    <units name="s"/>
                    <default value="5"/>
                    <description>
                      <text>SPF delay between receiving a change to SPF calculation.</text>
                    </description>
                  </leaf>
                  <leaf name="hold-interval-timer">
                    <type name="uint32">
                      <range value="0..65535"/>
                    </type>
                    <units name="s"/>
                    <default value="10"/>
                    <description>
                      <text>SPF hold time between consecutive SPF calculations.</text>
                    </description>
                  </leaf>
                </case>
                <case name="intelligent-timer">
                  <description>
                    <text>Specify an intelligent timer.</text>
                  </description>
                  <leaf name="max-interval">
                    <type name="uint32">
                      <range value="1..120000"/>
                    </type>
                    <units name="ms"/>
                    <must condition="../start-interval&lt;=../max-interval and ../hold-interval&lt;=../max-interval"/>
                    <default value="5000"/>
                    <description>
                      <text>Maximum interval for OSPF SPF calculation set through an intelligent timer. By adjusting the interval for SPF calculation, you can prevent the occupation of too many bandwidths and device resources caused by frequent network changes.</text>
                    </description>
                  </leaf>
                  <leaf name="start-interval">
                    <type name="uint32">
                      <range value="1..60000"/>
                    </type>
                    <units name="ms"/>
                    <must condition="../start-interval&lt;=../max-interval"/>
                    <default value="50"/>
                    <description>
                      <text>Initial interval for OSPF SPF calculation set through an intelligent timer. By adjusting the interval for SPF calculation, you can prevent the occupation of too many bandwidth and router resources caused by frequent network changes.</text>
                    </description>
                  </leaf>
                  <leaf name="hold-interval">
                    <type name="uint32">
                      <range value="1..60000"/>
                    </type>
                    <units name="ms"/>
                    <must condition="../hold-interval&lt;=../max-interval"/>
                    <default value="200"/>
                    <description>
                      <text>Hold interval for OSPF SPF calculation set through an intelligent timer. By adjusting the interval for calculating SPF, you can prevent the occupation of too many bandwidth and router resources caused by frequent network changes.</text>
                    </description>
                  </leaf>
                </case>
                <case name="millisecond">
                  <description>
                    <text>Specify an interval.</text>
                  </description>
                  <leaf name="millisecond">
                    <type name="uint32">
                      <range value="1..10000"/>
                    </type>
                    <units name="ms"/>
                    <description>
                      <text>Interval for OSPF SPF calculation. By adjusting the interval for SPF calculation, you can prevent the occupation of too many bandwidths and device resources caused by frequent network changes.</text>
                    </description>
                  </leaf>
                </case>
              </choice>
            </container>
          </container>
          <container name="bfd">
            <description>
              <text>Configure BFD in a OSPFv3 process.</text>
            </description>
            <leaf name="enable">
              <type name="boolean"/>
              <default value="false"/>
              <description>
                <text>Enable/disable BFD in an OSPFv3 process.</text>
              </description>
            </leaf>
            <leaf name="min-rx">
              <type name="uint32">
                <range value="3..20000"/>
              </type>
              <units name="ms"/>
              <description>
                <text>Minimum interval for receiving BFD packets from the peer end. The value range and default value are controlled by the PAF.</text>
              </description>
            </leaf>
            <leaf name="min-tx">
              <type name="uint32">
                <range value="3..20000"/>
              </type>
              <units name="ms"/>
              <description>
                <text>Minimum interval for sending BFD packets to the peer end. The value range and default value are controlled by the PAF.</text>
              </description>
            </leaf>
            <leaf name="detect-multiplier">
              <type name="uint32">
                <range value="3..50"/>
              </type>
              <default value="3"/>
              <description>
                <text>Local detection multiplier.</text>
              </description>
            </leaf>
            <leaf name="incr-cost">
              <type name="uint32">
                <range value="1..65535"/>
              </type>
              <description>
                <text>BFD sync cost.</text>
              </description>
            </leaf>
            <leaf name="frr-binding">
              <type name="boolean"/>
              <default value="false"/>
              <description>
                <text>Enable/disable bind FRR with BFD.</text>
              </description>
            </leaf>
          </container>
          <container name="stub-router">
            <description>
              <text>Configure stub router is used to configure the router as stub router.</text>
            </description>
            <leaf name="flag">
              <type name="stub-router-flag"/>
              <default value="no-config"/>
              <description>
                <text>Stub router. The router configured with the stub router command notifies other OSPF routers of not using the stub router to forward data by increasing the metric greater than 65535 of the links in the LSA that is generated by the router.</text>
              </description>
            </leaf>
            <leaf name="interval">
              <when condition="../flag='stub-on-boot'"/>
              <type name="uint32">
                <range value="5..65535"/>
              </type>
              <default value="500"/>
              <description>
                <text>Interval for remaining as the stub router during the active/standby switchover of a router.</text>
              </description>
            </leaf>
            <leaf name="include-stub">
              <when condition="../flag!='no-config'"/>
              <type name="boolean"/>
              <default value="false"/>
              <description>
                <text>Enable/disable maximum cost for router lsa stub link.</text>
              </description>
            </leaf>
            <leaf name="external-lsa">
              <when condition="../flag!='no-config'"/>
              <type name="boolean"/>
              <default value="false"/>
              <description>
                <text>Enable/disable maximum cost for external lsa.</text>
              </description>
            </leaf>
            <leaf name="external-cost">
              <when condition="../external-lsa='true'"/>
              <type name="uint32">
                <range value="1..16777215"/>
              </type>
              <default value="16711680"/>
              <description>
                <text>Advertise external LSAs with configured/default overriding metric.</text>
              </description>
            </leaf>
            <leaf name="summary-lsa">
              <when condition="../flag!='no-config'"/>
              <type name="boolean"/>
              <default value="false"/>
              <description>
                <text>Enable/disable maximum cost for summary lsa.</text>
              </description>
            </leaf>
            <leaf name="summary-cost">
              <when condition="../summary-lsa='true'"/>
              <type name="uint32">
                <range value="1..16777215"/>
              </type>
              <default value="16711680"/>
              <description>
                <text>Advertise summary LSAs with configured/default overriding metric.</text>
              </description>
            </leaf>
          </container>
          <container name="bgp-ls">
            <description>
              <text>Configure BGP LS.</text>
            </description>
            <leaf name="enable">
              <type name="boolean"/>
              <must condition="../enable='false' or ../../../../../ni:name='_public_'"/>
              <default value="false"/>
              <description>
                <text>Enable/disable BGP LS.</text>
              </description>
            </leaf>
            <leaf name="identifier">
              <when condition="../enable='true'"/>
              <type name="uint32">
                <range value="2..65535"/>
              </type>
              <description>
                <text>TOPO identifier.</text>
              </description>
            </leaf>
          </container>
          <container name="import-limit">
            <presence value="Enable import-limit"/>
            <description>
              <text>Configure limits the maximum number of routes to be imported.</text>
            </description>
            <leaf name="max-number">
              <type name="uint32">
                <range value="1..4294967295"/>
              </type>
              <mandatory value="true"/>
              <description>
                <text>Maximum number of external routes that can be imported.</text>
              </description>
            </leaf>
            <leaf name="threshold-upper">
              <type name="uint32">
                <range value="1..100"/>
              </type>
              <must condition="../threshold-upper&gt;=../threshold-lower"/>
              <default value="80"/>
              <description>
                <text>Upper threshold of the maximum number of routes that can be imported.</text>
              </description>
            </leaf>
            <leaf name="threshold-lower">
              <type name="uint32">
                <range value="1..100"/>
              </type>
              <must condition="../threshold-upper&gt;=../threshold-lower"/>
              <default value="70"/>
              <description>
                <text>Lower threshold of the maximum number of routes that can be imported.</text>
              </description>
            </leaf>
          </container>
          <container name="spf-control">
            <description>
              <text>Configure SPF calculation control.</text>
            </description>
            <leaf name="max-load-balancing">
              <type name="uint32">
                <range value="1..128"/>
              </type>
              <ext:dynamic-default/>
              <description>
                <text>Maximum number of the equal cost routes of the destination address in the routing table. The range and default are based on PAF.</text>
              </description>
            </leaf>
          </container>
          <container name="default-para">
            <description>
              <text>Configure multiple topologies in a process.</text>
            </description>
            <leaf name="cost">
              <type name="uint32">
                <range value="0..16777214"/>
              </type>
              <default value="1"/>
              <description>
                <text>Default metric of the external routes imported by OSPFv3.</text>
              </description>
            </leaf>
            <leaf name="tag">
              <type name="uint32">
                <range value="0..4294967295"/>
              </type>
              <default value="1"/>
              <description>
                <text>Tag of an external route.</text>
              </description>
            </leaf>
            <leaf name="type">
              <type name="external-route-type"/>
              <default value="type2"/>
              <description>
                <text>Type of an external route.</text>
              </description>
            </leaf>
          </container>
          <container name="asbr-summary-prefixs">
            <description>
              <text>List of aggregate addresses for OSPF (only for ASBR).</text>
            </description>
            <list name="asbr-summary-prefix">
              <key value="prefix network-mask"/>
              <description>
                <text>Configure aggregate addresses for OSPF (only for ASBR).</text>
              </description>
              <leaf name="prefix">
                <type name="inet:ipv6-address-no-zone"/>
                <description>
                  <text>IP address.</text>
                </description>
              </leaf>
              <leaf name="network-mask">
                <type name="uint32">
                  <range value="1..128"/>
                </type>
                <description>
                  <text>IP address mask.</text>
                </description>
              </leaf>
              <leaf name="cost">
                <type name="uint32">
                  <range value="1..16777214"/>
                </type>
                <description>
                  <text>Specifies the cost of the aggregated route. By default, the maximal cost in that of all the routes which are aggregated serves as the cost of the aggregated route.</text>
                </description>
              </leaf>
              <leaf name="tag">
                <type name="uint32">
                  <range value="0..4294967295"/>
                </type>
                <description>
                  <text>Specifies that the tag is mainly used to control advertisement of routes through the routing policy.</text>
                </description>
              </leaf>
              <leaf name="distribute-delay">
                <type name="uint32">
                  <range value="1..65535"/>
                </type>
                <units name="s"/>
                <description>
                  <text>Distribute delay interval.</text>
                </description>
              </leaf>
              <leaf name="advertise">
                <type name="boolean"/>
                <default value="true"/>
                <description>
                  <text>Enable/disable the aggregated route is not advertised.</text>
                </description>
              </leaf>
            </list>
          </container>
          <container name="default-route">
            <presence value="Enable default-route-advertise."/>
            <description>
              <text>Configure default routes to the OSPFv3 routing area.</text>
            </description>
            <leaf name="cost">
              <type name="uint32">
                <range value="0..16777214"/>
              </type>
              <description>
                <text>Metric of an ASE LSA.</text>
              </description>
            </leaf>
            <leaf name="tag">
              <type name="uint32">
                <range value="0..4294967295"/>
              </type>
              <description>
                <text>Route tag for default imported routes.</text>
              </description>
            </leaf>
            <leaf name="type">
              <type name="external-route-type"/>
              <description>
                <text>Metric type of an ASE LSA for the default routes.</text>
              </description>
            </leaf>
            <leaf name="distribute-delay">
              <type name="uint32">
                <range value="1..65535"/>
              </type>
              <description>
                <text>Distribute delay timer used for the local router to calculate the default routes advertised by other routers.</text>
              </description>
            </leaf>
            <choice name="mode">
              <description>
                <text>Mode for default route.</text>
              </description>
              <case name="permit-calculate-other">
                <description>
                  <text>Set permit calculate other.</text>
                </description>
                <leaf name="permit-calculate-other">
                  <type name="empty"/>
                  <description>
                    <text>To generate and advertise an ASE LSA that describes the default route only when there are valid default routes of other protocols in the routing table of the host. The router configured with permit calculate other still calculates the default routes of other routers.</text>
                  </description>
                </leaf>
              </case>
              <case name="always">
                <description>
                  <text>Set always.</text>
                </description>
                <leaf name="always">
                  <type name="empty"/>
                  <description>
                    <text>The router configured with the always parameter does not calculate the default routes of other routers.</text>
                  </description>
                </leaf>
              </case>
            </choice>
            <choice name="policy-type">
              <description>
                <text>Mode of the policy used for default route.</text>
              </description>
              <case name="route-policy-name">
                <description>
                  <text>Set a route policy name.</text>
                </description>
                <leaf name="route-policy-name">
                  <type name="leafref">
                    <path value="/rtp:routing-policy/rtp:policy-definitions/rtp:policy-definition/rtp:name"/>
                  </type>
                  <description>
                    <text>Name of a routing policy. Through the routing policy, the device matches the default routing entry generated by the device that does not run OSPF in the routing table, and advertises the default route according to the parameters set through the routing policy.</text>
                  </description>
                </leaf>
              </case>
              <case name="route-filter-name">
                <description>
                  <text>Set a route filter name.</text>
                </description>
                <leaf name="route-filter-name">
                  <type name="leafref">
                    <path value="/xpl:xpl/xpl:route-filters/xpl:route-filter/xpl:name"/>
                  </type>
                  <description>
                    <text>Name of a routing Filter.</text>
                  </description>
                </leaf>
              </case>
            </choice>
          </container>
          <container name="route-preferences">
            <description>
              <text>List of preference of an OSPF route.</text>
            </description>
            <list name="route-preference">
              <key value="route-type"/>
              <max-elements value="2"/>
              <ext:generated-by value="system">
                <ext:filter value="route-type='default' or route-type='ase'"/>
              </ext:generated-by>
              <description>
                <text>Configure preference of an OSPF route.</text>
              </description>
              <leaf name="route-type">
                <type name="ospf-route-type"/>
                <description>
                  <text>Router type.</text>
                </description>
              </leaf>
              <leaf name="preference">
                <type name="uint32">
                  <range value="1..255"/>
                </type>
                <ext:dynamic-default>
                  <ext:default-value value="10">
                    <when condition="../route-type = 'default'"/>
                    <description>
                      <text>The default value is 10 when route type is default.</text>
                    </description>
                  </ext:default-value>
                  <ext:default-value value="150">
                    <when condition="../route-type = 'ase'"/>
                    <description>
                      <text>The default value is 150 when route type is ase.</text>
                    </description>
                  </ext:default-value>
                </ext:dynamic-default>
                <description>
                  <text>Preference of an OSPF route. The default of intra and inter area is 10.And The default of external is 150.</text>
                </description>
              </leaf>
              <choice name="policy-type">
                <description>
                  <text>Type of a filtering rule.</text>
                </description>
                <case name="route-policy-name">
                  <description>
                    <text>Name of a routing policy.</text>
                  </description>
                  <leaf name="route-policy-name">
                    <type name="leafref">
                      <path value="/rtp:routing-policy/rtp:policy-definitions/rtp:policy-definition/rtp:name"/>
                    </type>
                    <description>
                      <text>Name of a routing policy.</text>
                    </description>
                  </leaf>
                </case>
                <case name="route-filter-name">
                  <description>
                    <text>Name of a routing Filter.</text>
                  </description>
                  <leaf name="route-filter-name">
                    <type name="leafref">
                      <path value="/xpl:xpl/xpl:route-filters/xpl:route-filter/xpl:name"/>
                    </type>
                    <description>
                      <text>Name of a routing Filter.</text>
                    </description>
                  </leaf>
                </case>
              </choice>
            </list>
          </container>
          <container name="nexthop-weights">
            <description>
              <text>List of preference of an equal cost route.</text>
            </description>
            <list name="nexthop-weight">
              <key value="ip-address if-name"/>
              <description>
                <text>Configure preference of an equal cost route.</text>
              </description>
              <leaf name="ip-address">
                <type name="inet:ipv4-address-no-zone"/>
                <description>
                  <text>The Router ID of the next hop.</text>
                </description>
              </leaf>
              <leaf name="if-name">
                <type name="leafref">
                  <path value="/ifm:ifm/ifm:interfaces/ifm:interface/ifm:name"/>
                </type>
                <must condition="/ifm:ifm/ifm:interfaces/ifm:interface[ifm:name=current()]/ip:ipv6"/>
                <description>
                  <text>Next Hop Interface Name. Only Ethernet, NULL and Pos are used for nexthop.</text>
                </description>
              </leaf>
              <leaf name="weight">
                <type name="uint32">
                  <range value="1..254"/>
                </type>
                <mandatory value="true"/>
                <description>
                  <text>Weight of the next hop. After OSPFv3 calculates the equal cost routes, the next hop is chosen from these equal-cost routes based on the value of weight. The smaller the value is, the higher the preference is. By default, the weight is 255. This indicates that load balancing is carried out among equal cost routes and packets are forwarded at the same time without the preference being set.</text>
                </description>
              </leaf>
            </list>
          </container>
          <container name="prefix-prioritys">
            <description>
              <text>List of priority of OSPF route.</text>
            </description>
            <list name="prefix-priority">
              <key value="priority"/>
              <description>
                <text>Configure priority of OSPF route.</text>
              </description>
              <leaf name="priority">
                <type name="prefix-priority"/>
                <description>
                  <text>Convergence priority of OSPF routes.</text>
                </description>
              </leaf>
              <leaf name="ip-prefix">
                <type name="leafref">
                  <path value="/rtp:routing-policy/rtp:ipv6-prefix-filters/rtp:ipv6-prefix-filter/rtp:name"/>
                </type>
                <mandatory value="true"/>
                <description>
                  <text>IPv6 Prefix List Name in String.</text>
                </description>
              </leaf>
            </list>
          </container>
          <container name="authentication">
            <description>
              <text>Configure authentication.</text>
            </description>
            <leaf name="mode">
              <type name="authen-mode"/>
              <default value="none"/>
              <description>
                <text>Set authentication type.</text>
              </description>
            </leaf>
            <leaf name="key-id">
              <when condition="../mode='hmac-sha256' or ../mode='hmac-sm3'"/>
              <type name="uint32">
                <range value="1..65535"/>
              </type>
              <mandatory value="true"/>
              <description>
                <text>Authentication identifier.</text>
              </description>
            </leaf>
            <leaf name="text">
              <when condition="../mode='hmac-sha256' or ../mode='hmac-sm3'"/>
              <type name="pub-type:password-extend"/>
              <mandatory value="true"/>
              <description>
                <text>Encrypted password.</text>
              </description>
            </leaf>
            <leaf name="key-chain-name">
              <when condition="../mode='keychain'"/>
              <type name="string">
                <length value="1..47"/>
                <pattern value="[^A-Z]+"/>
              </type>
              <ext:case-sensitivity value="lower-only"/>
              <mandatory value="true"/>
              <description>
                <text>Keychain name.</text>
              </description>
            </leaf>
          </container>
          <container name="helper-role">
            <presence value="Enable OSPFv3 GR Helper."/>
            <description>
              <text>Configure OSPFv3 GR Helper.</text>
            </description>
            <leaf name="planned-only">
              <type name="empty"/>
              <description>
                <text>To support only planned GR on routers.</text>
              </description>
            </leaf>
            <leaf name="ignore-external-lsa">
              <type name="empty"/>
              <description>
                <text>Ignore external lsa check (Type5 and Type7 LSAs).</text>
              </description>
            </leaf>
            <leaf name="period">
              <type name="uint32">
                <range value="1..1800"/>
              </type>
              <default value="1800"/>
              <description>
                <text>Maximum grace period.</text>
              </description>
            </leaf>
            <choice name="filter-type">
              <description>
                <text>Basic ACl name or ACL number or IP prefix List.</text>
              </description>
              <case name="acl-name-or-num">
                <description>
                  <text>Specify the basic ACL number or ACL name for filtering peer.</text>
                </description>
                <leaf name="acl-name-or-num">
                  <type name="leafref">
                    <path value="/acl:acl/acl:group6s/acl:group6/acl:identity"/>
                  </type>
                  <must condition="/acl:acl/acl:group6s/acl:group6[acl:identity=current()]/acl:type='basic'"/>
                  <description>
                    <text>Specify the basic ACL number or ACL name for filtering peer.</text>
                  </description>
                </leaf>
              </case>
              <case name="ip-prefix-name">
                <description>
                  <text>Specify the IP prefix name for filtering peers.</text>
                </description>
                <leaf name="ip-prefix-name">
                  <type name="leafref">
                    <path value="/rtp:routing-policy/rtp:ipv6-prefix-filters/rtp:ipv6-prefix-filter/rtp:name"/>
                  </type>
                  <description>
                    <text>Specify the IP prefix name for filtering peers.</text>
                  </description>
                </leaf>
              </case>
            </choice>
          </container>
          <container name="filter-import">
            <presence value="filter policy import"/>
            <description>
              <text>Configure the filter policy import command, you can set the rules for filtering intra area, inter area, and AS external routes of an OSPF process.</text>
            </description>
            <choice name="policy-type">
              <mandatory value="true"/>
              <description>
                <text>Type of a filtering rule.</text>
              </description>
              <case name="basic-acl6">
                <description>
                  <text>Apply basic ACL6 name or ACL6 number.</text>
                </description>
                <leaf name="name-or-num">
                  <type name="leafref">
                    <path value="/acl:acl/acl:group6s/acl:group6/acl:identity"/>
                  </type>
                  <must condition="/acl:acl/acl:group6s/acl:group6[acl:identity=current()]/acl:type='basic'"/>
                  <description>
                    <text>Apply basic ACL6 name or ACL6 number.</text>
                  </description>
                </leaf>
              </case>
              <case name="ip-prefix6">
                <description>
                  <text>Set the name of an IP prefix list.</text>
                </description>
                <leaf name="ip-prefix6">
                  <type name="leafref">
                    <path value="/rtp:routing-policy/rtp:ipv6-prefix-filters/rtp:ipv6-prefix-filter/rtp:name"/>
                  </type>
                  <description>
                    <text>Set the name of an IP prefix list.</text>
                  </description>
                </leaf>
              </case>
              <case name="route-policy">
                <description>
                  <text>Set a route policy name.</text>
                </description>
                <leaf name="route-policy">
                  <type name="leafref">
                    <path value="/rtp:routing-policy/rtp:policy-definitions/rtp:policy-definition/rtp:name"/>
                  </type>
                  <description>
                    <text>Set a route policy name.</text>
                  </description>
                </leaf>
              </case>
              <case name="route-filter">
                <description>
                  <text>Set a route filter name.</text>
                </description>
                <leaf name="route-filter">
                  <type name="leafref">
                    <path value="/xpl:xpl/xpl:route-filters/xpl:route-filter/xpl:name"/>
                  </type>
                  <description>
                    <text>Set a route filter name.</text>
                  </description>
                </leaf>
              </case>
            </choice>
          </container>
          <container name="filter-exports">
            <description>
              <text>List of the filter policy export command. You can set the rules for filtering the routes imported through the import route (OSPF) command when the routes are advertised in type5 LSAs to the OSPF AS.</text>
            </description>
            <list name="filter-export">
              <key value="protocol protocol-id"/>
              <description>
                <text>Configure the filter policy export command. You can set the rules for filtering the routes imported through the import route (OSPF) command when the routes are advertised in type5 LSAs to the OSPF AS.</text>
              </description>
              <leaf name="protocol">
                <type name="filter-rt-protocol"/>
                <description>
                  <text>Protocol that advertises routes.</text>
                </description>
              </leaf>
              <leaf name="protocol-id">
                <type name="uint32">
                  <range value="1..4294967295"/>
                </type>
                <must condition="../protocol-id=1 or ../protocol='ospfv3' or ../protocol='isis' or ../protocol='ripng'"/>
                <description>
                  <text>Process ID when the advertised protocol is RIPng, ISIS, or OSPFv3.</text>
                </description>
              </leaf>
              <choice name="policy-type">
                <mandatory value="true"/>
                <description>
                  <text>Type of a filtering rule.</text>
                </description>
                <case name="basic-acl6">
                  <description>
                    <text>Apply basic ACL6 name or ACL6 number.</text>
                  </description>
                  <leaf name="name-or-num">
                    <type name="leafref">
                      <path value="/acl:acl/acl:group6s/acl:group6/acl:identity"/>
                    </type>
                    <must condition="/acl:acl/acl:group6s/acl:group6[acl:identity=current()]/acl:type='basic'"/>
                    <description>
                      <text>Apply basic ACL6 name or ACL6 number.</text>
                    </description>
                  </leaf>
                </case>
                <case name="ip-prefix6">
                  <description>
                    <text>Set the name of an IP prefix list.</text>
                  </description>
                  <leaf name="ip-prefix6">
                    <type name="leafref">
                      <path value="/rtp:routing-policy/rtp:ipv6-prefix-filters/rtp:ipv6-prefix-filter/rtp:name"/>
                    </type>
                    <description>
                      <text>Set the name of an IP prefix list.</text>
                    </description>
                  </leaf>
                </case>
              </choice>
            </list>
          </container>
          <container name="import-routes">
            <description>
              <text>List of import the routes learned from other routing protocols.</text>
            </description>
            <list name="import-route">
              <key value="protocol process-id"/>
              <description>
                <text>Configure import the routes learned from other routing protocols.</text>
              </description>
              <leaf name="protocol">
                <type name="import-rt-protocol"/>
                <description>
                  <text>Routing protocol whose routes are imported.</text>
                </description>
              </leaf>
              <leaf name="process-id">
                <type name="uint32">
                  <range value="0..4294967295"/>
                </type>
                <must condition="(../process-id=0 and (../protocol='direct' or ../protocol='static' or ../protocol='bgp' or ../protocol='unr' or ../protocol='opr')) or (../process-id&gt;0 and (../protocol='ospfv3' or ../protocol='isis' or ../protocol='ripng'))"/>
                <description>
                  <text>Process ID of the routing protocol whose routes are imported. It needs to be set only when the protocol is RIPng, OSPFv3, or ISIS.</text>
                </description>
              </leaf>
              <leaf name="tag">
                <type name="uint32">
                  <range value="0..4294967295"/>
                </type>
                <description>
                  <text>Route tag of an external LSA.</text>
                </description>
              </leaf>
              <leaf name="type">
                <type name="external-route-type"/>
                <description>
                  <text>Metric type of an external LSA.</text>
                </description>
              </leaf>
              <leaf name="permit-ibgp">
                <type name="boolean"/>
                <must condition="../permit-ibgp='false' or (../permit-ibgp='true' and ../protocol='bgp')"/>
                <default value="false"/>
                <description>
                  <text>Enable/disable to import IBGP routes.</text>
                </description>
              </leaf>
              <choice name="cost">
                <description>
                  <text>Route cost.</text>
                </description>
                <case name="inherit-cost">
                  <description>
                    <text>Import the routes with the original cost.</text>
                  </description>
                  <leaf name="inherit-cost">
                    <type name="empty"/>
                    <description>
                      <text>Import the routes with the original cost.</text>
                    </description>
                  </leaf>
                </case>
                <case name="cost-value">
                  <description>
                    <text>Route cost.</text>
                  </description>
                  <leaf name="cost-value">
                    <type name="uint32">
                      <range value="0..16777214"/>
                    </type>
                    <description>
                      <text>Route cost.</text>
                    </description>
                  </leaf>
                </case>
              </choice>
              <choice name="policy-type">
                <description>
                  <text>Type of a filtering rule.</text>
                </description>
                <case name="route-policy-name">
                  <description>
                    <text>Name of a routing policy.</text>
                  </description>
                  <leaf name="route-policy-name">
                    <type name="leafref">
                      <path value="/rtp:routing-policy/rtp:policy-definitions/rtp:policy-definition/rtp:name"/>
                    </type>
                    <description>
                      <text>Name of a routing policy.</text>
                    </description>
                  </leaf>
                </case>
                <case name="route-filter-name">
                  <description>
                    <text>Name of a routing filter.</text>
                  </description>
                  <leaf name="route-filter-name">
                    <type name="leafref">
                      <path value="/xpl:xpl/xpl:route-filters/xpl:route-filter/xpl:name"/>
                    </type>
                    <description>
                      <text>Name of a routing Filter.</text>
                    </description>
                  </leaf>
                </case>
              </choice>
            </list>
          </container>
          <container name="frr">
            <description>
              <text>Configure OSPFv3 FRR.</text>
            </description>
            <leaf name="enable">
              <type name="boolean"/>
              <default value="false"/>
              <description>
                <text>Enable/disable FRR for this particular process.</text>
              </description>
            </leaf>
            <leaf name="lfa-enable">
              <type name="boolean"/>
              <must condition="../lfa-enable='false' or ../enable='true'"/>
              <default value="false"/>
              <description>
                <text>Enable/disable loop free alternate.</text>
              </description>
            </leaf>
            <container name="tiebreaker">
              <when condition="../lfa-enable='true'"/>
              <description>
                <text>Configure tiebreaker for multiple backups.</text>
              </description>
              <leaf name="node-protect">
                <type name="uint8">
                  <range value="1..255"/>
                </type>
                <default value="40"/>
                <description>
                  <text>Set the value of node protect preference.</text>
                </description>
              </leaf>
              <leaf name="lowest-cost">
                <type name="uint8">
                  <range value="1..255"/>
                </type>
                <default value="20"/>
                <description>
                  <text>Set the value of lowcost preference.</text>
                </description>
              </leaf>
            </container>
            <container name="route-policy">
              <when condition="../enable='true'"/>
              <description>
                <text>Configure FRR route policy.</text>
              </description>
              <choice name="policy-type">
                <description>
                  <text>Type of a filtering rule.</text>
                </description>
                <case name="route-policy-name">
                  <description>
                    <text>Name of a routing policy.</text>
                  </description>
                  <leaf name="route-policy-name">
                    <type name="leafref">
                      <path value="/rtp:routing-policy/rtp:policy-definitions/rtp:policy-definition/rtp:name"/>
                    </type>
                    <description>
                      <text>Name of a routing policy.</text>
                    </description>
                  </leaf>
                </case>
                <case name="route-filter-name">
                  <description>
                    <text>Name of a routing filter.</text>
                  </description>
                  <leaf name="route-filter-name">
                    <type name="leafref">
                      <path value="/xpl:xpl/xpl:route-filters/xpl:route-filter/xpl:name"/>
                    </type>
                    <description>
                      <text>Name of a routing filter.</text>
                    </description>
                  </leaf>
                </case>
              </choice>
            </container>
          </container>
          <container name="silent-interfaces">
            <when condition="../silent-all-interface='false'"/>
            <description>
              <text>List of interface. To prevent the OSPF routing information from being received by routers in a network segment and forbid the local router to receive the routing update information that is advertised by other routers in the network segment, you can use the silent interface command to suppress the interface to receive or send the OSPF packets.</text>
            </description>
            <list name="silent-interface">
              <key value="if-name"/>
              <description>
                <text>Configure interface. To prevent the OSPF routing information from being received by routers in a network segment and forbid the local router to receive the routing update information that is advertised by other routers in the network segment, you can use the silent interface command to suppress the interface to receive or send the OSPF packets.</text>
              </description>
              <leaf name="if-name">
                <type name="leafref">
                  <path value="/ifm:ifm/ifm:interfaces/ifm:interface/ifm:name"/>
                </type>
                <must condition="/ifm:ifm/ifm:interfaces/ifm:interface[ifm:name=current()]/ip:ipv6"/>
                <description>
                  <text>Interface name.</text>
                </description>
              </leaf>
            </list>
          </container>
          <container name="undo-silent-interfaces">
            <when condition="../silent-all-interface='true'"/>
            <description>
              <text>List of interface. Enables sending and receiving packet on this interface when true. This information is useful when sending and receiving packet is suppressed on all interface on process level and user want it to be enabled on certain interface.</text>
            </description>
            <list name="undo-silent-interface">
              <key value="if-name"/>
              <description>
                <text>Configure interface. Enables sending and receiving packet on this interface when true. This information is useful when sending and receiving packet is suppressed on all interface on process level and user want it to be enabled on certain interface.</text>
              </description>
              <leaf name="if-name">
                <type name="leafref">
                  <path value="/ifm:ifm/ifm:interfaces/ifm:interface/ifm:name"/>
                </type>
                <must condition="/ifm:ifm/ifm:interfaces/ifm:interface[ifm:name=current()]/ip:ipv6"/>
                <description>
                  <text>Interface name.</text>
                </description>
              </leaf>
            </list>
          </container>
          <container name="host-name">
            <description>
              <text>Configure OSPFv3 host name.</text>
            </description>
            <leaf name="enable">
              <type name="boolean"/>
              <default value="false"/>
              <description>
                <text>Enable/disable host name.</text>
              </description>
            </leaf>
            <leaf name="name">
              <when condition="(../enable='true')"/>
              <type name="string">
                <length value="1..255"/>
              </type>
              <description>
                <text>Host name.</text>
              </description>
            </leaf>
          </container>
          <container name="cost-bindings">
            <description>
              <text>List of cost-binding. Indicate cost of routing from indicated router source ID binding interface.</text>
            </description>
            <list name="cost-binding">
              <key value="router-id if-name"/>
              <description>
                <text>Configure cost-binding. Indicate cost of routing from indicated router source ID binding interface.</text>
              </description>
              <leaf name="router-id">
                <type name="inet:ipv4-address-no-zone"/>
                <description>
                  <text>Source router ID.</text>
                </description>
              </leaf>
              <leaf name="if-name">
                <type name="leafref">
                  <path value="/ifm:ifm/ifm:interfaces/ifm:interface/ifm:name"/>
                </type>
                <description>
                  <text>Interface name.</text>
                </description>
              </leaf>
            </list>
          </container>
          <container name="dn-bit">
            <when condition="../../../../ni:name!='_public_'"/>
            <description>
              <text>Configure VPN optionA feature will help PEs in inter AS exchange routes through OSPF. VPN optionA provide two ways to suppress DN bit so that so that inter AS can exchange routes through OSPF. First way dose not set DN bit while import routes from BGP, second way dose not check DN bit while learning the routes from neighbor router. In both the scenario PE will be able to learn route and propagate the route in the new VPN cloud.</text>
            </description>
            <leaf name="set-ase">
              <type name="boolean"/>
              <default value="false"/>
              <description>
                <text>Enable/disable setting of DN bit in all ASE LSA, applicable in private network.</text>
              </description>
            </leaf>
            <leaf name="set-nssa">
              <type name="boolean"/>
              <default value="false"/>
              <description>
                <text>Enable/disable setting of DN bit in all NSSA LSA, applicable in private network.</text>
              </description>
            </leaf>
            <leaf name="set-summary">
              <type name="boolean"/>
              <default value="false"/>
              <description>
                <text>Enable/disable setting of DN bit in all summary LSA, applicable in private network.</text>
              </description>
            </leaf>
            <leaf name="check-ase">
              <type name="boolean"/>
              <default value="false"/>
              <description>
                <text>Enable/disable checking of DN bit in all ASE LSA, applicable in private network.</text>
              </description>
            </leaf>
            <leaf name="check-nssa">
              <type name="boolean"/>
              <default value="false"/>
              <description>
                <text>Enable/disable checking of DN bit in all NSSA LSA, applicable in private network.</text>
              </description>
            </leaf>
            <leaf name="check-summary">
              <type name="boolean"/>
              <default value="false"/>
              <description>
                <text>Enable/disable checking of DN bit in all summary LSA, applicable in private network.</text>
              </description>
            </leaf>
            <container name="summary-router-ids">
              <when condition="../check-summary='false'"/>
              <description>
                <text>List of router ID for which checking for DN bit summary LSA is disabled, applicable in private network.</text>
              </description>
              <list name="summary-router-id">
                <key value="router-id"/>
                <description>
                  <text>Configure router ID for which checking for DN bit summary LSA is disabled, applicable in private network.</text>
                </description>
                <leaf name="router-id">
                  <type name="inet:ipv4-address-no-zone"/>
                  <description>
                    <text>Router ID for which checking for DN bit summary LSA is disabled, applicable in private network.</text>
                  </description>
                </leaf>
              </list>
            </container>
          </container>
          <container name="areas">
            <description>
              <text>List of OSPFv3 area.</text>
            </description>
            <list name="area">
              <key value="area-id"/>
              <description>
                <text>Configure OSPFv3 area.</text>
              </description>
              <leaf name="area-id">
                <type name="inet:ipv4-address-no-zone"/>
                <description>
                  <text>Area ID.</text>
                </description>
              </leaf>
              <leaf name="area-type">
                <type name="area-type"/>
                <default value="normal"/>
                <description>
                  <text>Area type.</text>
                </description>
              </leaf>
              <leaf name="description">
                <type name="string">
                  <length value="1..80"/>
                  <pattern value="([^?]*)"/>
                </type>
                <description>
                  <text>Description of an OSPFv3 area.</text>
                </description>
              </leaf>
              <container name="stub">
                <when condition="../area-type='stub'"/>
                <description>
                  <text>Configure stub area.</text>
                </description>
                <leaf name="backbone-peer-ignore">
                  <type name="boolean"/>
                  <default value="false"/>
                  <description>
                    <text>Enable/disable ignore backbone peer for Stub area.</text>
                  </description>
                </leaf>
                <leaf name="no-summary">
                  <type name="boolean"/>
                  <default value="false"/>
                  <description>
                    <text>Enable/disable forbid an ABR to Send Summary LSAs to the Stub Area.</text>
                  </description>
                </leaf>
                <leaf name="default-cost">
                  <type name="int32">
                    <range value="0..16777214"/>
                  </type>
                  <default value="1"/>
                  <description>
                    <text>Cost of the default Type3 routes that are sent by an OSPF process to a stub area or an NSSA area. The value is an integer ranging from 1 to 16777214. This parameter takes effect only when it is configured on the ABR connected to a stub area or an NSSA area. The default is 1.</text>
                  </description>
                </leaf>
              </container>
              <container name="nssa">
                <when condition="../area-type='nssa'"/>
                <description>
                  <text>Configure nssa area.</text>
                </description>
                <leaf name="no-summary">
                  <type name="boolean"/>
                  <default value="false"/>
                  <description>
                    <text>Enable/disable forbid an ABR to Send Summary LSAs to the NSSA Area.</text>
                  </description>
                </leaf>
                <leaf name="translator-always">
                  <type name="boolean"/>
                  <default value="false"/>
                  <description>
                    <text>Enable/disable the router to act as translator always for NSSA Area.</text>
                  </description>
                </leaf>
                <leaf name="default-route-advertise">
                  <type name="boolean"/>
                  <default value="false"/>
                  <description>
                    <text>Enable/disable originate Type 7 default into NSSA area.</text>
                  </description>
                </leaf>
                <leaf name="backbone-peer-ignore">
                  <when condition="../default-route-advertise='true'"/>
                  <type name="boolean"/>
                  <default value="false"/>
                  <description>
                    <text>Enable/disable ignore backbone peer for Nssa area.</text>
                  </description>
                </leaf>
                <leaf name="no-import-route">
                  <type name="boolean"/>
                  <default value="false"/>
                  <description>
                    <text>Enable/disable redistribute type 7 LSA into this area.</text>
                  </description>
                </leaf>
                <leaf name="set-n-bit">
                  <type name="boolean"/>
                  <default value="false"/>
                  <description>
                    <text>Enable/disable setting of 'N' bit in the DD packet.</text>
                  </description>
                </leaf>
                <leaf name="suppress-fa">
                  <type name="boolean"/>
                  <default value="false"/>
                  <description>
                    <text>Enable/disable the forwarding address as 0 for all translated type 5 LSAs.</text>
                  </description>
                </leaf>
                <leaf name="translator-interval">
                  <type name="uint32">
                    <range value="1..120"/>
                  </type>
                  <default value="40"/>
                  <description>
                    <text>Translator interval.</text>
                  </description>
                </leaf>
                <leaf name="default-cost">
                  <type name="int32">
                    <range value="0..16777214"/>
                  </type>
                  <default value="1"/>
                  <description>
                    <text>Cost of the default Type3 routes that are sent by an OSPF process to a stub area or an NSSA area. This parameter takes effect only when it is configured on the ABR connected to a stub area or an NSSA area.</text>
                  </description>
                </leaf>
              </container>
              <container name="authentication">
                <description>
                  <text>Configure area authentication.</text>
                </description>
                <leaf name="mode">
                  <type name="authen-mode"/>
                  <default value="none"/>
                  <description>
                    <text>Set authentication type.</text>
                  </description>
                </leaf>
                <leaf name="key-id">
                  <when condition="../mode='hmac-sha256' or ../mode='hmac-sm3'"/>
                  <type name="uint32">
                    <range value="1..65535"/>
                  </type>
                  <mandatory value="true"/>
                  <description>
                    <text>Authentication identifier.</text>
                  </description>
                </leaf>
                <leaf name="auth-text">
                  <when condition="../mode='hmac-sha256' or ../mode='hmac-sm3'"/>
                  <type name="pub-type:password-extend"/>
                  <mandatory value="true"/>
                  <description>
                    <text>Encrypted Password.</text>
                  </description>
                </leaf>
                <leaf name="key-chain-name">
                  <when condition="../mode='keychain'"/>
                  <type name="string">
                    <length value="1..47"/>
                    <pattern value="[^A-Z]+"/>
                  </type>
                  <ext:case-sensitivity value="lower-only"/>
                  <mandatory value="true"/>
                  <description>
                    <text>Keychain name.</text>
                  </description>
                </leaf>
              </container>
              <container name="abr-summary-prefixs">
                <description>
                  <text>List of abr-summary.</text>
                </description>
                <list name="abr-summary-prefix">
                  <key value="prefix network-mask"/>
                  <description>
                    <text>Configure abr-summary.</text>
                  </description>
                  <leaf name="prefix">
                    <type name="inet:ipv6-address-no-zone"/>
                    <description>
                      <text>Specifies the IP address in dotted decimal notation.</text>
                    </description>
                  </leaf>
                  <leaf name="network-mask">
                    <type name="uint32">
                      <range value="1..128"/>
                    </type>
                    <description>
                      <text>Specifies the mask of the IP address. The mask address is in dotted decimal notation.</text>
                    </description>
                  </leaf>
                  <choice name="cost">
                    <description>
                      <text>Set the cost. By default, the maximal cost in that of all the routes which are aggregated serves as the cost of the aggregated route.</text>
                    </description>
                    <case name="cost-value">
                      <description>
                        <text>Specifies the cost of the aggregated route.</text>
                      </description>
                      <leaf name="cost-value">
                        <type name="uint32">
                          <range value="0..16777214"/>
                        </type>
                        <description>
                          <text>Specifies the cost of the aggregated route.</text>
                        </description>
                      </leaf>
                    </case>
                  </choice>
                  <leaf name="hold-max-cost">
                    <type name="uint32">
                      <range value="10..1800"/>
                    </type>
                    <description>
                      <text>Hold max cost duration.</text>
                    </description>
                  </leaf>
                  <leaf name="advertise">
                    <type name="boolean"/>
                    <default value="true"/>
                    <description>
                      <text>Enable/disable the aggregated route is advertised.</text>
                    </description>
                  </leaf>
                </list>
              </container>
              <container name="filters">
                <description>
                  <text>List of an ABR to filter outgoing or incoming summary LSAs of the local area.</text>
                </description>
                <list name="filter">
                  <key value="mode"/>
                  <description>
                    <text>Configure an ABR to filter outgoing or incoming summary LSAs of the local area.</text>
                  </description>
                  <leaf name="mode">
                    <type name="area-filter-mode"/>
                    <description>
                      <text>Using the filter export command, you can configure an ABR to filter outgoing summary LSAs of the local area. Using the filter import command, you can configure an ABR to filter incoming summary LSAs of the local area.</text>
                    </description>
                  </leaf>
                  <uses name="policy-type"/>
                </list>
              </container>
              <container name="interfaces">
                <description>
                  <text>List of the interface enabled with OSPFv3.</text>
                </description>
                <list name="interface">
                  <key value="name instance-id"/>
                  <description>
                    <text>Configure the interface enabled with OSPFv3.</text>
                  </description>
                  <leaf name="name">
                    <type name="leafref">
                      <path value="/ifm:ifm/ifm:interfaces/ifm:interface/ifm:name"/>
                    </type>
                    <must condition="../../../../../../../../ni:name=/ifm:ifm/ifm:interfaces/ifm:interface[ifm:name=current()]/ifm:vrf-name"/>
                    <must condition="/ifm:ifm/ifm:interfaces/ifm:interface[ifm:name=current()]/ip:ipv6"/>
                    <description>
                      <text>Interface name. OSPFv3 does not support the configuration of a NULL/LMP/MTUNNEL/RPR/STACKPORT/SIP/ATMBUNDLE/IMAGROUP/Remote-Ap/VBridge interface types.</text>
                    </description>
                  </leaf>
                  <leaf name="instance-id">
                    <type name="uint32">
                      <range value="0..255"/>
                    </type>
                    <description>
                      <text>Interface instance ID.</text>
                    </description>
                  </leaf>
                  <leaf name="network-type">
                    <type name="network-type"/>
                    <ext:dynamic-default/>
                    <description>
                      <text>Network type of an OSPFv3 interface. By default, the network type of an interface is determined by the physical interface.
In the same network segment, if only two routers run OSPFv3, you can change the network type of an interface to P2P.</text>
                    </description>
                  </leaf>
                  <leaf name="transmit-delay">
                    <type name="int32">
                      <range value="1..800"/>
                    </type>
                    <default value="1"/>
                    <description>
                      <text>Delay for transmitting LSAs on an interface.</text>
                    </description>
                  </leaf>
                  <leaf name="mtu-enable">
                    <type name="boolean"/>
                    <default value="false"/>
                    <description>
                      <text>Enable/disable MTU filled in a DD packet when the packet is sent. The default MTU in the DD packet is 0. Using the command, you can manually configure the interface to fill in the MTU field (actual MTU of the interface) when the interface sends DD packets.
After a virtual connection is established through the virtual template or tunnel, different vendors may use different MTUs as default settings. To ensure consistency, the MTU is set to 0 by default when the interface sends DD packets. Thus, the ospf mtu-enable command usually does not need to be configured.
OSPF does not support the preceding configuration on a null interface.</text>
                    </description>
                  </leaf>
                  <leaf name="dr-priority">
                    <type name="int32">
                      <range value="0..255"/>
                    </type>
                    <default value="1"/>
                    <description>
                      <text>Priority of the interface that participates in the DR and BDR election. The greater the value is, the higher the priority is. The router whose priority is 0 cannot be elected as a DR or a BDR.</text>
                    </description>
                  </leaf>
                  <leaf name="peer-hold-max-timer">
                    <type name="uint32">
                      <range value="100..1000000"/>
                    </type>
                    <units name="ms"/>
                    <description>
                      <text>Config peer hold max cost interval.</text>
                    </description>
                  </leaf>
                  <leaf name="cost">
                    <type name="uint32">
                      <range value="1..65535"/>
                    </type>
                    <description>
                      <text>To set the cost of an interface.</text>
                    </description>
                  </leaf>
                  <leaf name="link-cost">
                    <type name="uint32">
                      <range value="1..65535"/>
                    </type>
                    <description>
                      <text>Link cost to be incremented when link quality is low.</text>
                    </description>
                  </leaf>
                  <container name="frr">
                    <description>
                      <text>Configure FRR.</text>
                    </description>
                    <leaf name="frr-disable">
                      <type name="boolean"/>
                      <default value="false"/>
                      <description>
                        <text>Enable/disable not use this interface for FRR calculation.</text>
                      </description>
                    </leaf>
                  </container>
                  <container name="timer">
                    <description>
                      <text>Configure timer.</text>
                    </description>
                    <leaf name="hello-interval">
                      <type name="int32">
                        <range value="1..65535"/>
                      </type>
                      <units name="s"/>
                      <ext:dynamic-default>
                        <ext:default-value value="10">
                          <when condition="../../network-type = 'p2p' or ../../network-type = 'broadcast'"/>
                          <description>
                            <text>The default value is 10 when network-type is p2p or broadcast.</text>
                          </description>
                        </ext:default-value>
                        <ext:default-value value="30">
                          <when condition="../../network-type = 'p2mp' or ../../network-type = 'nbma'"/>
                          <description>
                            <text>The default value is 30 when network-type is p2mp or nbma.</text>
                          </description>
                        </ext:default-value>
                      </ext:dynamic-default>
                      <description>
                        <text>Interval for sending Hello packets on an interface.</text>
                      </description>
                    </leaf>
                    <leaf name="hello-conser">
                      <type name="boolean"/>
                      <default value="false"/>
                      <description>
                        <text>Enable/disable the compensation mechanism of controling the deadtimer to enable.</text>
                      </description>
                    </leaf>
                    <leaf name="dead-interval">
                      <type name="int32">
                        <range value="1..65535"/>
                      </type>
                      <units name="s"/>
                      <must condition="(../hello-interval&lt;../dead-interval) or (../hello-interval=65535 and ../dead-interval=65535)"/>
                      <ext:dynamic-default>
                        <ext:default-value value="4*(../hello-interval)">
                          <description>
                            <text>The default value of dead interval is 4 times that of hello interval.</text>
                          </description>
                        </ext:default-value>
                      </ext:dynamic-default>
                      <description>
                        <text>Dead interval of an OSPF neighbor.</text>
                      </description>
                    </leaf>
                    <leaf name="wait-interval">
                      <type name="uint32">
                        <range value="1..65535"/>
                      </type>
                      <ext:dynamic-default>
                        <ext:default-value value="../dead-interval">
                          <description>
                            <text>The default value of wait interval is equal to dead interval.</text>
                          </description>
                        </ext:default-value>
                      </ext:dynamic-default>
                      <description>
                        <text>Wait time.</text>
                      </description>
                    </leaf>
                    <leaf name="poll-interval">
                      <type name="int32">
                        <range value="1..65535"/>
                      </type>
                      <default value="120"/>
                      <description>
                        <text>Poll interval for sending Hello packets. On the NBMA network, after the neighbor is invalid, the router sends Hello packets periodically according to the interval set through the ospf timer poll command. The poll interval should be at least four times of the interval for sending Hello packets.</text>
                      </description>
                    </leaf>
                    <leaf name="retransmit-interval">
                      <type name="int32">
                        <range value="1..3600"/>
                      </type>
                      <default value="5"/>
                      <description>
                        <text>Interval for retransmitting LSAs on an interface.</text>
                      </description>
                    </leaf>
                  </container>
                  <container name="bfd">
                    <description>
                      <text>Configure BFD.</text>
                    </description>
                    <leaf name="enable">
                      <type name="boolean"/>
                      <must condition="../enable='false' or ../block='false'"/>
                      <default value="false"/>
                      <description>
                        <text>Enable/disable BFD.</text>
                      </description>
                    </leaf>
                    <leaf name="block">
                      <type name="boolean"/>
                      <must condition="../block='false' or ../enable='false'"/>
                      <default value="false"/>
                      <description>
                        <text>Enable/disable to prevent the dynamic establishment of a BFD session on an interface.</text>
                      </description>
                    </leaf>
                    <leaf name="detect-multiplier">
                      <type name="uint32">
                        <range value="3..50"/>
                      </type>
                      <default value="3"/>
                      <description>
                        <text>Local detection multiplier.</text>
                      </description>
                    </leaf>
                    <leaf name="min-tx">
                      <type name="uint32">
                        <range value="3..20000"/>
                      </type>
                      <units name="ms"/>
                      <description>
                        <text>Minimum interval for sending BFD packets to the peer end. The value range and default value are controlled by the PAF.</text>
                      </description>
                    </leaf>
                    <leaf name="min-rx">
                      <type name="uint32">
                        <range value="3..20000"/>
                      </type>
                      <units name="ms"/>
                      <description>
                        <text>Minimum interval for receiving BFD packets from the peer end. The value range and default value are controlled by the PAF.</text>
                      </description>
                    </leaf>
                    <leaf name="frr-binding">
                      <type name="boolean"/>
                      <default value="false"/>
                      <description>
                        <text>Enable/disable bind FRR with BFD.</text>
                      </description>
                    </leaf>
                    <choice name="incr-cost">
                      <description>
                        <text>Interface BFD sync cost.</text>
                      </description>
                      <case name="incr-cost-block">
                        <description>
                          <text>Interface BFD sync cost block.</text>
                        </description>
                        <leaf name="incr-cost-block">
                          <type name="empty"/>
                          <description>
                            <text>Interface BFD sync cost block.</text>
                          </description>
                        </leaf>
                      </case>
                      <case name="incr-cost-value">
                        <description>
                          <text>Interface BFD sync cost.</text>
                        </description>
                        <leaf name="incr-cost-value">
                          <type name="uint32">
                            <range value="1..65535"/>
                          </type>
                          <description>
                            <text>Interface BFD sync cost.</text>
                          </description>
                        </leaf>
                      </case>
                    </choice>
                    <leaf name="per-link-flag">
                      <type name="boolean"/>
                      <must condition="../one-arm-echo-flag=../per-link-flag and (../per-link-flag='false' or ../enable='true')"/>
                      <default value="false"/>
                      <description>
                        <text>Enable/disable BFD session per link, support only VLANif interface.</text>
                      </description>
                    </leaf>
                    <leaf name="one-arm-echo-flag">
                      <type name="boolean"/>
                      <must condition="../one-arm-echo-flag=../per-link-flag"/>
                      <default value="false"/>
                      <description>
                        <text>Enable/disable echo detection mode, support only VLANif interface.</text>
                      </description>
                    </leaf>
                  </container>
                  <container name="authentication">
                    <description>
                      <text>Configure interface authentication.</text>
                    </description>
                    <leaf name="mode">
                      <type name="authen-mode"/>
                      <default value="none"/>
                      <description>
                        <text>Set authentication type.</text>
                      </description>
                    </leaf>
                    <leaf name="key-id">
                      <when condition="../mode='hmac-sha256' or ../mode='hmac-sm3'"/>
                      <type name="uint32">
                        <range value="1..65535"/>
                      </type>
                      <mandatory value="true"/>
                      <description>
                        <text>Authentication identifier.</text>
                      </description>
                    </leaf>
                    <leaf name="auth-text">
                      <when condition="../mode='hmac-sha256' or ../mode='hmac-sm3'"/>
                      <type name="pub-type:password-extend"/>
                      <mandatory value="true"/>
                      <description>
                        <text>Encrypted Password.</text>
                      </description>
                    </leaf>
                    <leaf name="key-chain-name">
                      <when condition="../mode='keychain'"/>
                      <type name="string">
                        <length value="1..47"/>
                        <pattern value="[^A-Z]+"/>
                      </type>
                      <ext:case-sensitivity value="lower-only"/>
                      <mandatory value="true"/>
                      <description>
                        <text>Keychain name.</text>
                      </description>
                    </leaf>
                  </container>
                  <container name="suppress-flapping">
                    <description>
                      <text>Configure flapping suppression.</text>
                    </description>
                    <container name="peer">
                      <must condition="resume-interval&gt;detect-interval"/>
                      <description>
                        <text>Configure flapping suppression peer. Not support Loopback.</text>
                      </description>
                      <leaf name="enable">
                        <type name="boolean"/>
                        <default value="true"/>
                        <description>
                          <text>Enable/disable interface peer flapping suppress.</text>
                        </description>
                      </leaf>
                      <leaf name="detect-interval">
                        <type name="uint32">
                          <range value="1..300"/>
                        </type>
                        <default value="60"/>
                        <description>
                          <text>Detecting interval.</text>
                        </description>
                      </leaf>
                      <leaf name="threshold">
                        <type name="uint32">
                          <range value="1..1000"/>
                        </type>
                        <default value="10"/>
                        <description>
                          <text>Threshold.</text>
                        </description>
                      </leaf>
                      <leaf name="resume-interval">
                        <type name="uint32">
                          <range value="2..1000"/>
                        </type>
                        <default value="120"/>
                        <description>
                          <text>Resume interval.</text>
                        </description>
                      </leaf>
                      <leaf name="hold-down-interval">
                        <type name="uint32">
                          <range value="1..86400"/>
                        </type>
                        <description>
                          <text>Hold down interval.</text>
                        </description>
                      </leaf>
                      <leaf name="hold-max-cost">
                        <type name="boolean"/>
                        <default value="true"/>
                        <description>
                          <text>Enable/disable hold max cost.</text>
                        </description>
                      </leaf>
                    </container>
                  </container>
                  <container name="peers">
                    <description>
                      <text>List of the IP address and DR priority of the neighbor router on the NBMA network.</text>
                    </description>
                    <list name="peer">
                      <key value="nbr-router-id"/>
                      <unique tag="nbr-link-local-addr"/>
                      <description>
                        <text>Configure the Link Local address and DR eligibility of the neighbor router on the NBMA network.</text>
                      </description>
                      <leaf name="nbr-router-id">
                        <type name="inet:ipv4-address-no-zone"/>
                        <description>
                          <text>Neighbor router ID.</text>
                        </description>
                      </leaf>
                      <leaf name="nbr-link-local-addr">
                        <type name="inet:ipv6-address-no-zone"/>
                        <mandatory value="true"/>
                        <description>
                          <text>Link Local address of the neighbor router.
Link Local Address should be in IPv6 format.
Valid Link Local Address starts with FE80:</text>
                        </description>
                      </leaf>
                      <leaf name="cost">
                        <type name="uint32">
                          <range value="1..65535"/>
                        </type>
                        <description>
                          <text>Cost to reach P2MP non broadcast neighbor.</text>
                        </description>
                      </leaf>
                      <leaf name="dr-eligible">
                        <type name="boolean"/>
                        <default value="false"/>
                        <description>
                          <text>Enable/disable DR Eligibility of the NBMA neighbor.</text>
                        </description>
                      </leaf>
                      <leaf name="poll">
                        <type name="int32">
                          <range value="1..65535"/>
                        </type>
                        <description>
                          <text>Poll interval for NBMA neighbor.</text>
                        </description>
                      </leaf>
                    </list>
                  </container>
                  <container name="flush-source-trace">
                    <description>
                      <text>Configure OSPF flush source trace function.</text>
                    </description>
                    <leaf name="block">
                      <type name="boolean"/>
                      <default value="false"/>
                      <description>
                        <text>Enable/disable OSPF flush source trace.</text>
                      </description>
                    </leaf>
                  </container>
                  <container name="ipv6-prefixs">
                    <config value="false"/>
                    <description>
                      <text>List of the IPv6 prefixes.</text>
                    </description>
                    <list name="ipv6-prefix">
                      <key value="ipv6-address ipv6-mask-len"/>
                      <description>
                        <text>Statistics of the IPv6 prefixes.</text>
                      </description>
                      <leaf name="ipv6-address">
                        <type name="inet:ipv6-address-no-zone"/>
                        <description>
                          <text>IPv6 address.</text>
                        </description>
                      </leaf>
                      <leaf name="ipv6-mask-len">
                        <type name="uint32"/>
                        <description>
                          <text>Mask length.</text>
                        </description>
                      </leaf>
                    </list>
                  </container>
                  <container name="attribute">
                    <config value="false"/>
                    <description>
                      <text>Statistics of interface attributes.</text>
                    </description>
                    <leaf name="prefix-len">
                      <type name="uint32"/>
                      <description>
                        <text>Prefix length.</text>
                      </description>
                    </leaf>
                    <leaf name="link-protocol">
                      <type name="interface-state"/>
                      <description>
                        <text>OSPFv3 link protocol state.</text>
                      </description>
                    </leaf>
                    <leaf name="mtu">
                      <type name="uint32"/>
                      <description>
                        <text>MTU of an interface.</text>
                      </description>
                    </leaf>
                    <leaf name="link-local-addr">
                      <type name="inet:ipv6-address-no-zone"/>
                      <description>
                        <text>Link local address.</text>
                      </description>
                    </leaf>
                    <leaf name="event">
                      <type name="uint32"/>
                      <description>
                        <text>Interface event.</text>
                      </description>
                    </leaf>
                    <leaf name="lsa-count">
                      <type name="uint32"/>
                      <description>
                        <text>Interface Lsa Count.</text>
                      </description>
                    </leaf>
                    <leaf name="lsa-checksum">
                      <type name="uint32"/>
                      <description>
                        <text>Interface Lsa Checksum.</text>
                      </description>
                    </leaf>
                    <leaf name="effective-cost">
                      <type name="uint32"/>
                      <description>
                        <text>Valid cost value.</text>
                      </description>
                    </leaf>
                    <leaf name="cost-type">
                      <type name="string">
                        <length value="1..40"/>
                      </type>
                      <description>
                        <text>Cost type.</text>
                      </description>
                    </leaf>
                    <leaf name="state">
                      <type name="interface-state"/>
                      <description>
                        <text>OSPFv3 interface state.</text>
                      </description>
                    </leaf>
                    <leaf name="dr">
                      <type name="inet:ipv4-address-no-zone"/>
                      <description>
                        <text>Designated router.</text>
                      </description>
                    </leaf>
                    <leaf name="dr-link-local-addr">
                      <type name="inet:ipv6-address-no-zone"/>
                      <description>
                        <text>DR link local address.</text>
                      </description>
                    </leaf>
                    <leaf name="bdr">
                      <type name="inet:ipv4-address-no-zone"/>
                      <description>
                        <text>Backup designated router.</text>
                      </description>
                    </leaf>
                    <leaf name="bdr-link-local-addr">
                      <type name="inet:ipv6-address-no-zone"/>
                      <description>
                        <text>BDR link local address.</text>
                      </description>
                    </leaf>
                    <leaf name="hello-due-timer">
                      <type name="string">
                        <length value="1..8"/>
                      </type>
                      <description>
                        <text>Hello due timer.</text>
                      </description>
                    </leaf>
                    <leaf name="count">
                      <type name="uint32"/>
                      <description>
                        <text>Number of neighbors.</text>
                      </description>
                    </leaf>
                    <leaf name="adjacent-count">
                      <type name="uint32"/>
                      <description>
                        <text>Number of adjacents.</text>
                      </description>
                    </leaf>
                    <leaf name="bandwidth-high">
                      <type name="uint32"/>
                      <description>
                        <text>Higher order 32-bit of the interface bandwidth value.</text>
                      </description>
                    </leaf>
                    <leaf name="bandwidth-low">
                      <type name="uint32"/>
                      <description>
                        <text>Lower order 32-bit of the interface bandwidth value.</text>
                      </description>
                    </leaf>
                    <container name="bfd">
                      <description>
                        <text>Statistics of bfd attributes.</text>
                      </description>
                      <leaf name="enable">
                        <type name="boolean"/>
                        <default value="false"/>
                        <description>
                          <text>Enable/disable BFD in an OSPFv3 process.</text>
                        </description>
                      </leaf>
                      <leaf name="frr-binding">
                        <type name="boolean"/>
                        <default value="false"/>
                        <description>
                          <text>Enable/disable bind FRR with BFD.</text>
                        </description>
                      </leaf>
                      <leaf name="min-tx">
                        <type name="uint32">
                          <range value="3..20000"/>
                        </type>
                        <units name="ms"/>
                        <description>
                          <text>Minimum interval for sending BFD packets to the peer end. The value range and default value are controlled by the PAF.</text>
                        </description>
                      </leaf>
                      <leaf name="min-rx">
                        <type name="uint32">
                          <range value="3..20000"/>
                        </type>
                        <units name="ms"/>
                        <description>
                          <text>Minimum interval for receiving BFD packets from the peer end. The value range and default value are controlled by the PAF.</text>
                        </description>
                      </leaf>
                      <leaf name="detect-multiplier">
                        <type name="uint32">
                          <range value="3..50"/>
                        </type>
                        <default value="3"/>
                        <description>
                          <text>Local detection multiplier.</text>
                        </description>
                      </leaf>
                    </container>
                    <leaf name="auth-flag">
                      <type name="auth-type"/>
                      <description>
                        <text>Authentication which can be configured using the ospf authentication-mode command.</text>
                      </description>
                    </leaf>
                    <leaf name="auth-seq-high">
                      <type name="uint32"/>
                      <description>
                        <text>Higher order 32-bit of the sequence number of the sent packet.</text>
                      </description>
                    </leaf>
                    <leaf name="auth-seq-low">
                      <type name="uint32"/>
                      <description>
                        <text>Lower order 32-bit of the sequence number of the sent packet.</text>
                      </description>
                    </leaf>
                    <leaf name="sa-name-apply">
                      <type name="string">
                        <length value="1..15"/>
                      </type>
                      <description>
                        <text>The name of iP security association applied.</text>
                      </description>
                    </leaf>
                    <leaf name="sa-name">
                      <type name="string">
                        <length value="1..15"/>
                      </type>
                      <description>
                        <text>The name of iP security association configured.</text>
                      </description>
                    </leaf>
                    <leaf name="peer-flapping-suppress">
                      <type name="peer-flapping-suppress-type"/>
