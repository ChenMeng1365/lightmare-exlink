<?xml version="1.0" encoding="UTF-8"?>
<module name="huawei-debug"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:debug="urn:huawei:yang:huawei-debug"
        xmlns:ext="urn:huawei:yang:huawei-extension">
  <namespace uri="urn:huawei:yang:huawei-debug"/>
  <prefix value="debug"/>
  <import module="huawei-extension">
    <prefix value="ext"/>
  </import>
  <organization>
    <text>Huawei Technologies Co., Ltd.</text>
  </organization>
  <contact>
    <text>Huawei Industrial Base
Bantian, Longgang
Shenzhen 518129
People's Republic of China
Website: http://www.huawei.com
Email: support@huawei.com</text>
  </contact>
  <description>
    <text>Global configuration.</text>
  </description>
  <revision date="2021-09-24">
    <description>
      <text>Add container disk for disk detection configurations.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <revision date="2020-05-29">
    <description>
      <text>Init revision.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <ext:task-name value="diagnose"/>
  <container name="debug">
    <description>
      <text>Debug information.</text>
    </description>
    <container name="cpu-infos">
      <description>
        <text>List of CPU information of boards.</text>
      </description>
      <list name="cpu-info">
        <must condition="unoverload-threshold &lt;= (overload-threshold - 3)"/>
        <key value="position"/>
        <ext:generated-by value="system"/>
        <ext:operation-exclude value="create"/>
        <description>
          <text>Configure CPU information of a board.</text>
        </description>
        <leaf name="position">
          <type name="string">
            <length value="1..32"/>
          </type>
          <description>
            <text>Board position.</text>
          </description>
        </leaf>
        <leaf name="overload-threshold">
          <type name="uint32">
            <range value="4..100"/>
          </type>
          <units name="%"/>
          <default value="90"/>
          <description>
            <text>Overload threshold.</text>
          </description>
        </leaf>
        <leaf name="unoverload-threshold">
          <type name="uint32">
            <range value="1..97"/>
          </type>
          <units name="%"/>
          <default value="75"/>
          <description>
            <text>Overload alarm clear threshold.</text>
          </description>
        </leaf>
        <leaf name="interval">
          <type name="uint32">
            <range value="1..60"/>
          </type>
          <units name="min"/>
          <default value="8"/>
          <description>
            <text>Overload alarm cycle.</text>
          </description>
        </leaf>
        <leaf name="index">
          <type name="uint32"/>
          <config value="false"/>
          <description>
            <text>Board index.</text>
          </description>
        </leaf>
        <leaf name="system-cpu-usage">
          <type name="uint32">
            <range value="0..100"/>
          </type>
          <units name="%"/>
          <config value="false"/>
          <description>
            <text>CPU usage.</text>
          </description>
        </leaf>
        <leaf name="monitor-number">
          <type name="uint32">
            <range value="6..360"/>
          </type>
          <config value="false"/>
          <description>
            <text>Monitor number in one alarm cycle.</text>
          </description>
        </leaf>
        <leaf name="monitor-cycle">
          <type name="uint32"/>
          <units name="s"/>
          <config value="false"/>
          <description>
            <text>Monitor cycle.</text>
          </description>
        </leaf>
        <leaf name="overload-state-change-time">
          <type name="string">
            <length value="1..32"/>
          </type>
          <config value="false"/>
          <description>
            <text>Last overload state changetime.</text>
          </description>
        </leaf>
        <leaf name="current-overload-state">
          <type name="string">
            <length value="1..31"/>
          </type>
          <config value="false"/>
          <description>
            <text>Current overload state.</text>
          </description>
        </leaf>
      </list>
    </container>
    <container name="memory-infos">
      <description>
        <text>List of memory information of boards.</text>
      </description>
      <list name="memory-info">
        <must condition="unoverload-threshold &lt;= (overload-threshold - 3)"/>
        <must condition="overload-threshold &lt;= memreli-exception-threshold"/>
        <must condition="memreli-notice-threshold &lt;= (memreli-overload-threshold - 5)"/>
        <must condition="memreli-overload-threshold &lt;= (memreli-exception-threshold - 5)"/>
        <key value="position"/>
        <ext:generated-by value="system"/>
        <ext:operation-exclude value="create"/>
        <description>
          <text>Configure memory information about a board.</text>
        </description>
        <leaf name="position">
          <type name="string">
            <length value="1..32"/>
          </type>
          <description>
            <text>Board position.</text>
          </description>
        </leaf>
        <leaf name="overload-threshold">
          <ext:dynamic-default/>
          <type name="uint32">
            <range value="75..100"/>
          </type>
          <units name="%"/>
          <description>
            <text>Overload threshold.</text>
          </description>
        </leaf>
        <leaf name="unoverload-threshold">
          <ext:dynamic-default/>
          <type name="uint32">
            <range value="1..97"/>
          </type>
          <units name="%"/>
          <description>
            <text>Overload alarm clear threshold.</text>
          </description>
        </leaf>
        <leaf name="memreli-notice-threshold">
          <ext:dynamic-default/>
          <type name="uint32">
            <range value="5..90"/>
          </type>
          <units name="%"/>
          <description>
            <text>Board memory reliability notice threshold.</text>
          </description>
        </leaf>
        <leaf name="memreli-overload-threshold">
          <ext:dynamic-default/>
          <type name="uint32">
            <range value="10..95"/>
          </type>
          <units name="%"/>
          <description>
            <text>Board memory reliability overload threshold.</text>
          </description>
        </leaf>
        <leaf name="memreli-exception-threshold">
          <ext:dynamic-default/>
          <type name="uint32">
            <range value="75..100"/>
          </type>
          <units name="%"/>
          <description>
            <text>Board memory reliability exception threshold.</text>
          </description>
        </leaf>
        <leaf name="index">
          <type name="uint32"/>
          <config value="false"/>
          <description>
            <text>Board index.</text>
          </description>
        </leaf>
        <leaf name="os-memory-total">
          <type name="uint32"/>
          <units name="kB"/>
          <config value="false"/>
          <description>
            <text>Total OS memory.</text>
          </description>
        </leaf>
        <leaf name="os-memory-use">
          <type name="uint32"/>
          <units name="kB"/>
          <config value="false"/>
          <description>
            <text>Used OS memory.</text>
          </description>
        </leaf>
        <leaf name="os-memory-free">
          <type name="uint32"/>
          <units name="kB"/>
          <config value="false"/>
          <description>
            <text>Available OS memory.</text>
          </description>
        </leaf>
        <leaf name="os-memory-usage">
          <type name="uint32">
            <range value="0..100"/>
          </type>
          <units name="%"/>
          <config value="false"/>
          <description>
            <text>OS memory usage.</text>
          </description>
        </leaf>
        <leaf name="do-memory-total">
          <type name="uint32"/>
          <units name="kB"/>
          <config value="false"/>
          <description>
            <text>Total VRP memory.</text>
          </description>
        </leaf>
        <leaf name="do-memory-use">
          <type name="uint32"/>
          <units name="kB"/>
          <config value="false"/>
          <description>
            <text>Used VRP memory.</text>
          </description>
        </leaf>
        <leaf name="do-memory-free">
          <type name="uint32"/>
          <units name="kB"/>
          <config value="false"/>
          <description>
            <text>Available VRP memory.</text>
          </description>
        </leaf>
        <leaf name="do-memory-usage">
          <type name="uint32">
            <range value="0..100"/>
          </type>
          <units name="%"/>
          <config value="false"/>
          <description>
            <text>VRP memory usage.</text>
          </description>
        </leaf>
        <leaf name="simple-memory-total">
          <type name="uint32"/>
          <units name="kB"/>
          <config value="false"/>
          <description>
            <text>Total simple memory.</text>
          </description>
        </leaf>
        <leaf name="simple-memory-use">
          <type name="uint32"/>
          <units name="kB"/>
          <config value="false"/>
          <description>
            <text>Used simple memory.</text>
          </description>
        </leaf>
        <leaf name="simple-memory-free">
          <type name="uint32"/>
          <units name="kB"/>
          <config value="false"/>
          <description>
            <text>Available simple memory.</text>
          </description>
        </leaf>
        <leaf name="simple-memory-usage">
          <type name="uint32">
            <range value="0..100"/>
          </type>
          <units name="%"/>
          <config value="false"/>
          <description>
            <text>Simple memory usage.</text>
          </description>
        </leaf>
        <leaf name="overload-state-change-time">
          <type name="string">
            <length value="1..31"/>
          </type>
          <config value="false"/>
          <description>
            <text>Last overload state changetime.</text>
          </description>
        </leaf>
        <leaf name="current-overload-state">
          <type name="string">
            <length value="1..32"/>
          </type>
          <config value="false"/>
          <description>
            <text>Current overload state.</text>
          </description>
        </leaf>
      </list>
    </container>
    <container name="resouce-reliability">
      <description>
        <text>Configure resource reliability info.</text>
      </description>
      <container name="memory-reliability">
        <description>
          <text>Configure memory reliability info.</text>
        </description>
        <leaf name="enable">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable memory reliability.</text>
          </description>
        </leaf>
      </container>
      <container name="memory-reliability-switchover-threshold">
        <description>
          <text>Configure memory threshold for board switchover.</text>
        </description>
        <leaf name="threshold">
          <ext:dynamic-default/>
          <type name="uint32">
            <range value="50..100"/>
          </type>
          <units name="%"/>
          <description>
            <text>Memory threshold for board switchover.</text>
          </description>
        </leaf>
      </container>
      <container name="flow-control-message-reliability">
        <description>
          <text>Configure flow-control-message reliability info.</text>
        </description>
        <leaf name="enable">
          <type name="boolean"/>
          <default value="true"/>
          <description>
            <text>Enable/disable flow-control-message reliability.</text>
          </description>
        </leaf>
      </container>
    </container>
    <container name="disk">
      <description>
        <text>Configure disk detection configurations.</text>
      </description>
      <container name="global">
        <description>
          <text>Configure disk detection global configurations.</text>
        </description>
        <leaf name="fault-detect-tolerance-time">
          <type name="uint8">
            <range value="15..120"/>
          </type>
          <units name="s"/>
          <default value="90"/>
          <description>
            <text>Fault detection tolerance time.</text>
          </description>
        </leaf>
      </container>
    </container>
    <container name="service-cpu-infos">
      <config value="false"/>
      <description>
        <text>List of service cpu-usage information of boards.</text>
      </description>
      <list name="service-cpu-info">
        <key value="position service-name"/>
        <config value="false"/>
        <description>
          <text>Service cpu-usage statistics for a board.</text>
        </description>
        <leaf name="position">
          <type name="string">
            <length value="1..32"/>
          </type>
          <config value="false"/>
          <description>
            <text>Board position.</text>
          </description>
        </leaf>
        <leaf name="service-name">
          <type name="string">
            <length value="0..50"/>
          </type>
          <config value="false"/>
          <description>
            <text>Service name.</text>
          </description>
        </leaf>
        <leaf name="service-cpu-usage">
          <type name="uint32">
            <range value="0..100"/>
          </type>
          <units name="%"/>
          <config value="false"/>
          <description>
            <text>CPU usage of service.</text>
          </description>
        </leaf>
      </list>
    </container>
    <container name="board-resouce-states">
      <config value="false"/>
      <description>
        <text>List of board monitoring information.</text>
      </description>
      <list name="board-resouce-state">
        <key value="position"/>
        <config value="false"/>
        <description>
          <text>Board monitoring statistics.</text>
        </description>
        <leaf name="position">
          <type name="string">
            <length value="1..32"/>
          </type>
          <config value="false"/>
          <description>
            <text>Board position.</text>
          </description>
        </leaf>
        <leaf name="entity-index">
          <type name="uint32"/>
          <config value="false"/>
          <description>
            <text>Board index.</text>
          </description>
        </leaf>
        <leaf name="board-name">
          <type name="string">
            <length value="0..128"/>
          </type>
          <config value="false"/>
          <description>
            <text>Board name.</text>
          </description>
        </leaf>
        <leaf name="cpu-usage">
          <type name="uint32"/>
          <units name="%"/>
          <config value="false"/>
          <description>
            <text>CPU usage.</text>
          </description>
        </leaf>
        <leaf name="memory-total-size">
          <type name="uint32"/>
          <units name="kB"/>
          <config value="false"/>
          <description>
            <text>Total memory size.</text>
          </description>
        </leaf>
        <leaf name="memory-used-size">
          <type name="uint32"/>
          <units name="kB"/>
          <config value="false"/>
          <description>
            <text>Used memory size.</text>
          </description>
        </leaf>
        <leaf name="memory-usage">
          <type name="uint32"/>
          <units name="%"/>
          <config value="false"/>
          <description>
            <text>Memory usage.</text>
          </description>
        </leaf>
      </list>
    </container>
  </container>
</module>
