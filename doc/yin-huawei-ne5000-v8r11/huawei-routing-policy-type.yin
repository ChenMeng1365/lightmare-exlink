  <typedef name="match-mode-type">
    <type name="enumeration">
      <enum name="permit">
        <value value="0"/>
        <description>
          <text>Matching mode of filters.</text>
        </description>
      </enum>
      <enum name="deny">
        <value value="1"/>
        <description>
          <text>Matching mode of filters.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Match mode.</text>
    </description>
  </typedef>
  <typedef name="community-filter-type">
    <type name="enumeration">
      <enum name="basic">
        <value value="1"/>
        <description>
          <text>The basic type of filter.</text>
        </description>
      </enum>
      <enum name="advanced">
        <value value="2"/>
        <description>
          <text>The advanced type of filter.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Filter type.</text>
    </description>
  </typedef>
  <typedef name="protocol-type">
    <type name="enumeration">
      <enum name="direct">
        <value value="1"/>
        <description>
          <text>Direct.</text>
        </description>
      </enum>
      <enum name="ospf">
        <value value="2"/>
        <description>
          <text>Open Shortest Path First.</text>
        </description>
      </enum>
      <enum name="isis">
        <value value="3"/>
        <description>
          <text>Intermediate System-to-Intermediate System.</text>
        </description>
      </enum>
      <enum name="static">
        <value value="4"/>
        <description>
          <text>Static.</text>
        </description>
      </enum>
      <enum name="rip">
        <value value="5"/>
        <description>
          <text>Routing Information Protocol.</text>
        </description>
      </enum>
      <enum name="bgp">
        <value value="6"/>
        <description>
          <text>Border Gateway Protocol.</text>
        </description>
      </enum>
      <enum name="ospfv3">
        <value value="7"/>
        <description>
          <text>OSPFv3.</text>
        </description>
      </enum>
      <enum name="ripng">
        <value value="8"/>
