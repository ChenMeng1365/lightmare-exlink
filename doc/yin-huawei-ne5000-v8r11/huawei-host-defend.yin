  <container name="host-defend">
    <description>
      <text>Configure host packet security attack defense configuration.</text>
    </description>
    <container name="global-management-plane-defend">
      <description>
        <text>Configure policy for global management and service plane protection.</text>
      </description>
      <leaf name="enable">
