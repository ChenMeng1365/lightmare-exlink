module: huawei-ztp
  +--rw ztp
     +--ro status
        +--ro enable?                   boolean
        +--ro last?                     boolean
        +--ro pre-configuration-name?   string

  rpcs:
    +---x set-enable-status
    |  +---w input
    |     +---w enable?   boolean
    +---x set-pre-configuration
    |  +---w input
    |     +---w file-name?   string
    +---x reset-pre-configuration
       +---w input
          +---w confirm?   boolean
