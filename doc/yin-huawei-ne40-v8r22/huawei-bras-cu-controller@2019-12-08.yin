      <description>
        <text>UP state.</text>
      </description>
    </leaf>
    <leaf name="interface">
      <type name="pub-type:if-name"/>
      <description>
        <text>UP interface name.</text>
      </description>
    </leaf>
    <leaf name="track-odd-mac-state">
      <type name="up-state-type"/>
      <description>
        <text>Track odd MAC state.</text>
      </description>
    </leaf>
    <leaf name="track-even-mac-state">
      <type name="up-state-type"/>
      <description>
        <text>Track even MAC state.</text>
      </description>
    </leaf>
    <leaf name="track-odd-interface">
      <type name="pub-type:if-name"/>
      <description>
        <text>Odd interface name.</text>
      </description>
    </leaf>
    <leaf name="track-even-interface">
      <type name="pub-type:if-name"/>
      <description>
        <text>Even interface name.</text>
      </description>
    </leaf>
  </grouping>
  <grouping name="interface-status">
    <description>
