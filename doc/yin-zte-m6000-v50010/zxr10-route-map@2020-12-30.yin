                <description>
                  <text>The ID of URPF.</text>
                </description>
                <type name="uint16">
                  <range value="1..65534">
                    <description>
                      <text>The range of URPF ID is from 1 to 65534</text>
                    </description>
                  </range>
                </type>
                <mandatory value="true"/>
              </leaf>
              <ncd:revise-date date="2019-02-27"/>
            </container>
            <container name="aigp-metric">
              <description>
                <text>Set AIGP metric.</text>
              </description>
              <presence value="aigp-metric"/>
              <choice name="aigp-metric">
                <description>
                  <text>Set AIGP metric, metric-value and igp-metric can't co-exist in one sequence.</text>
                </description>
                <mandatory value="true"/>
                <case name="metric-value">
                  <description>
                    <text>Set AIGP metric value.</text>
                  </description>
                  <leaf name="value">
                    <description>
                      <text>The AGIP metric value.</text>
                    </description>
                    <type name="uint64">
                      <range value="0..4294967295">
                        <description>
                          <text>the range of metric-value is from 0 to 4294967295.</text>
                        </description>
                      </range>
                    </type>
                    <mandatory value="true"/>
                  </leaf>
                </case>
                <case name="igp-metric">
                  <description>
                    <text>Set IGP metric as AIGP metric.</text>
                  </description>
                  <leaf name="igp-metric">
                    <description>
                      <text>Set IGP metric value as AIGP metric.</text>
                    </description>
                    <type name="empty"/>
                    <mandatory value="true"/>
                  </leaf>
                </case>
              </choice>
              <ncd:revise-date date="2019-02-27"/>
            </container>
            <container name="metric">
              <description>
                <text>Set metric value for destination routing protocol.</text>
              </description>
              <choice name="metric">
                <description>
                  <text>Set metric value for destination routing protocol.</text>
                </description>
                <case name="metric">
                  <description>
                    <text>Set metric value for destination routing protocol.</text>
                  </description>
                  <leaf name="add-del">
                    <description>
                      <text>Add metric + Del metric  -.</text>
                    </description>
                    <type name="string">
                      <pattern value="^(\+|\-)$"/>
                    </type>
                  </leaf>
                  <leaf name="value">
                    <description>
                      <text>Metric value &lt;0-4294967295&gt;.</text>
                    </description>
                    <type name="uint32">
                      <range value="0..4294967295">
                        <description>
                          <text>The range of metric-value is from 0 to 4294967295.</text>
                        </description>
                      </range>
                    </type>
                    <mandatory value="true"/>
                  </leaf>
                </case>
                <case name="inherit-metric">
                  <description>
                    <text>Set inherit metric flag for destination routing protocol.</text>
                  </description>
                  <leaf name="inherit">
                    <description>
                      <text>Set inherit metric flag for destination routing protocol.</text>
                    </description>
                    <type name="empty"/>
                    <mandatory value="true"/>
                  </leaf>
                  <ncd:revise-date date="2019-07-22"/>
                </case>
                <case name="inherit-igp-metric">
                  <description>
                    <text>Set inherit igp metric flag for destination routing protocol.</text>
                  </description>
                  <leaf name="inherit-igp-metric">
                    <description>
                      <text>Set inherit igp metric flag for destination routing protocol.</text>
                    </description>
                    <type name="empty"/>
                    <mandatory value="true"/>
                  </leaf>
                  <ncd:revise-date date="2019-07-22"/>
                </case>
              </choice>
              <ncd:revise-date date="2019-04-17"/>
            </container>
            <container name="metric-type">
              <description>
                <text>Set type of metric for destination routing protocol.</text>
              </description>
              <presence value="metric-type is not Mandatory"/>
              <leaf name="value">
                <description>
                  <text>Set type of metric for destination routing protocol;
External  IS-IS external metric;
Internal  IS-IS internal metric;
Type-1    OSPF external type 1 metric;
Type-2    OSPF external type 2 metric.</text>
                </description>
                <type name="enumeration">
                  <enum name="internal">
                    <value value="1"/>
                    <description>
                      <text>Internal  IS-IS internal metric</text>
                    </description>
                  </enum>
                  <enum name="external">
                    <value value="2"/>
                    <description>
                      <text>External  IS-IS external metric</text>
                    </description>
                  </enum>
                  <enum name="type-1">
                    <value value="3"/>
                    <description>
                      <text>Type-1    OSPF external type 1 metric</text>
                    </description>
                  </enum>
                  <enum name="type-2">
                    <value value="4"/>
                    <description>
                      <text>Type-2    OSPF external type 2 metric</text>
                    </description>
                  </enum>
                </type>
                <mandatory value="true"/>
              </leaf>
              <ncd:revise-date date="2019-04-17"/>
            </container>
            <container name="tag">
              <description>
                <text>Set tag value for destination routing protocol.</text>
              </description>
              <presence value="tag is not Mandatory"/>
