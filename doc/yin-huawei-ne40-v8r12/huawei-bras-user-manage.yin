    <container name="access-services">
      <description>
        <text>List of the access service templates.</text>
      </description>
      <list name="access-service">
        <key value="name"/>
        <description>
          <text>Configure an access service template.</text>
        </description>
        <leaf name="name">
          <type name="string">
            <length value="1..32"/>
            <pattern value="[!-~]{1,32}"/>
            <pattern value="([^A-Z&quot;%'*/:&lt;&gt;\?@|]{1,32})"/>
          </type>
          <must condition="../name != '-' and ../name != '--'"/>
          <description>
            <text>Access service name.</text>
          </description>
          <ext:case-sensitivity value="lower-only"/>
        </leaf>
        <container name="qos-profile">
          <description>
            <text>Configure the QoS profile bound to an access service template.</text>
          </description>
          <leaf name="qos-profile-name">
            <type name="leafref">
              <path value="/qos:qos/qos:queue-scheduler-template/qos:profiles/qos:profile/qos:name"/>
            </type>
            <must condition="/qos:qos/qos:queue-scheduler-template/qos:profiles/qos:profile[qos:name=current()]/qos:mode='normal'"/>
            <description>
              <text>Specifies the name of a bound QoS profile.</text>
            </description>
          </leaf>
        </container>
        <container name="qos-profile-time-ranges">
          <description>
            <text>List of the QoS profile containing a time segment bound to an access service template.</text>
          </description>
          <list name="qos-profile-time-range">
            <key value="time-range-name"/>
            <max-elements value="16"/>
            <description>
              <text>Configure the QoS profile containing a time segment bound to an access service template.</text>
            </description>
            <leaf name="time-range-name">
              <type name="leafref">
                <path value="/timerange:time-range/timerange:time-range-instances/timerange:time-range-instance/timerange:name"/>
              </type>
              <description>
                <text>Specifies the name of a time range.</text>
              </description>
            </leaf>
            <leaf name="qos-profile-name">
              <type name="leafref">
                <path value="/qos:qos/qos:queue-scheduler-template/qos:profiles/qos:profile/qos:name"/>
              </type>
              <must condition="/qos:qos/qos:queue-scheduler-template/qos:profiles/qos:profile[qos:name=current()]/qos:mode='normal'"/>
              <mandatory value="true"/>
              <description>
                <text>Specifies the name of a bound QoS profile.</text>
              </description>
            </leaf>
          </list>
        </container>
      </list>
    </container>
