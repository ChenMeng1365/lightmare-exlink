<?xml version="1.0" encoding="UTF-8"?>
<module name="zxr10-udld"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:udld="http://www.zte.com.cn/zxr10/netconf/schema/rosng/udld"
        xmlns:ncd="http://www.zte.com.cn/zxr10/netconf/schema/rosng/netconf-defs">
  <namespace uri="http://www.zte.com.cn/zxr10/netconf/schema/rosng/udld"/>
  <prefix value="udld"/>
  <import module="zxr10-netconf-defs">
    <prefix value="ncd"/>
  </import>
  <organization>
    <text>ZTE Corporation</text>
  </organization>
  <contact>
    <text>ZTE Corporation  
   Nanjing Institute of ZTE Corporation 
   No.68 Zijinghua Rd. Yuhuatai District, Nanjing, China 
   Tel: +86-25-52870000</text>
  </contact>
  <description>
    <text>The netconf module to describe Unidirection Link Detection(UDLD)</text>
  </description>
  <revision date="2018-05-23">
    <description>
      <text>The initial version</text>
    </description>
  </revision>
  <container name="configuration">
    <description>
      <text>Describe the configuration of UDLD</text>
    </description>
    <container name="udld">
      <description>
        <text>Configuration data of UDLD</text>
      </description>
      <container name="udld-attrs">
        <description>
          <text>UDLD attribute configuration</text>
        </description>
        <list name="udld-attr">
          <description>
            <text>Configure UDLD parameters</text>
          </description>
          <key value="ifname"/>
          <leaf name="ifname">
            <description>
              <text>This value identifies the UDLD interface name.This attribute is created by Interface Management</text>
            </description>
            <type name="ncd:ifname"/>
          </leaf>
          <leaf name="protocol-flag">
            <description>
              <text>UDLD protocol correlation configuration</text>
            </description>
            <type name="enumeration">
              <enum name="disable">
                <value value="0"/>
                <description>
                  <text>Disable UDLD</text>
                </description>
              </enum>
              <enum name="enable">
                <value value="1"/>
                <description>
                  <text>Enable UDLD</text>
                </description>
              </enum>
            </type>
            <default value="disable"/>
          </leaf>
          <leaf name="work-mode">
            <description>
              <text>Configure UDLD mode</text>
            </description>
            <type name="enumeration">
              <enum name="normal">
                <value value="0"/>
                <description>
                  <text>Normal mode</text>
                </description>
              </enum>
              <enum name="aggressive">
                <value value="1"/>
                <description>
                  <text>Aggressive mode</text>
                </description>
              </enum>
            </type>
            <default value="normal"/>
          </leaf>
          <leaf name="message-timer">
            <description>
              <text>Configure UDLD message timer</text>
            </description>
            <type name="uint8">
              <range value="7..90"/>
            </type>
            <units name="seconds"/>
            <default value="15"/>
          </leaf>
          <leaf name="recovery-flag">
            <description>
              <text>Configure UDLD auto-recovery</text>
            </description>
            <type name="enumeration">
              <enum name="disable">
                <value value="0"/>
                <description>
                  <text>Disable auto-recovery</text>
                </description>
              </enum>
              <enum name="enable">
                <value value="1"/>
                <description>
                  <text>Enable auto-recovery</text>
                </description>
              </enum>
            </type>
            <default value="disable"/>
          </leaf>
          <leaf name="dos-protect">
            <description>
              <text>Configure UDLD dos protect</text>
            </description>
            <type name="enumeration">
              <enum name="disable">
                <value value="0"/>
                <description>
                  <text>Disable UDLD DoS protect</text>
                </description>
              </enum>
              <enum name="enable">
                <value value="1"/>
                <description>
                  <text>Enable UDLD DoS protect</text>
                </description>
              </enum>
            </type>
            <default value="disable"/>
          </leaf>
          <leaf name="recovery-timer">
            <description>
              <text>Configure UDLD recovery timer</text>
            </description>
            <type name="uint32">
              <range value="10..600"/>
            </type>
            <units name="seconds"/>
            <default value="30"/>
          </leaf>
        </list>
      </container>
    </container>
  </container>
  <container name="operation">
    <description>
      <text>Describe the operation of UDLD</text>
    </description>
    <config value="false"/>
    <container name="udld">
      <description>
        <text>Operation data of UDLD</text>
      </description>
      <container name="udld-reset">
        <description>
          <text>UDLD reset correlation operation</text>
        </description>
        <list name="reset">
          <description>
            <text>Reset interface down by UDLD</text>
          </description>
          <key value="ifname"/>
          <leaf name="ifname">
            <description>
              <text>This value identifies the UDLD interface name.This attribute is created by Interface Management</text>
            </description>
            <type name="ncd:ifname"/>
          </leaf>
          <leaf name="udld-reset">
            <description>
              <text>Reset interface down by UDLD</text>
            </description>
            <type name="uint8">
              <range value="0..1"/>
            </type>
          </leaf>
        </list>
      </container>
    </container>
  </container>
  <container name="state">
    <description>
      <text>Describe the state of UDLD</text>
    </description>
    <config value="false"/>
    <container name="udld">
      <description>
        <text>State data of UDLD</text>
      </description>
      <container name="udld-info">
        <description>
          <text>Show UDLD information</text>
        </description>
        <list name="udld-state">
          <description>
            <text>UDLD correlation state</text>
          </description>
          <key value="ifname"/>
          <leaf name="ifname">
            <description>
              <text>This value identifies the UDLD interface name.This attribute is created by Interface Management</text>
            </description>
            <type name="ncd:ifname"/>
          </leaf>
          <leaf name="protocol-flag">
            <description>
              <text>UDLD protocol state</text>
            </description>
            <type name="enumeration">
              <enum name="disable">
                <value value="0"/>
                <description>
                  <text>Udld disable</text>
                </description>
              </enum>
              <enum name="enable">
                <value value="1"/>
                <description>
                  <text>Udld enable</text>
                </description>
              </enum>
            </type>
          </leaf>
          <leaf name="work-mode">
            <description>
              <text>UDLD protocol mode</text>
            </description>
            <type name="enumeration">
              <enum name="normal">
                <value value="0"/>
                <description>
                  <text>Normal mode</text>
                </description>
              </enum>
              <enum name="aggressive">
                <value value="1"/>
                <description>
                  <text>Aggressive mode</text>
                </description>
              </enum>
            </type>
          </leaf>
          <leaf name="current-state">
            <description>
              <text>UDLD machine status</text>
            </description>
            <type name="enumeration">
              <enum name="disable">
                <value value="0"/>
                <description>
                  <text>Disable state</text>
                </description>
              </enum>
              <enum name="probe">
                <value value="1"/>
                <description>
                  <text>Probe state</text>
                </description>
              </enum>
              <enum name="always-hello">
                <value value="2"/>
                <description>
                  <text>Always hello state</text>
                </description>
              </enum>
              <enum name="neighbour-build">
                <value value="3"/>
                <description>
                  <text>Neighbour build state</text>
                </description>
              </enum>
              <enum name="bidirection">
                <value value="4"/>
                <description>
                  <text>Bidirection state</text>
                </description>
              </enum>
              <enum name="unidirection">
                <value value="5"/>
                <description>
                  <text>Unidirection state</text>
                </description>
              </enum>
              <enum name="other">
                <value value="6"/>
                <description>
                  <text>Other state</text>
                </description>
              </enum>
            </type>
          </leaf>
          <leaf name="recovery-flag">
            <description>
              <text>UDLD auto-recovery flag</text>
            </description>
            <type name="enumeration">
              <enum name="disable">
                <value value="0"/>
                <description>
                  <text>Auto-recovery disable</text>
                </description>
              </enum>
              <enum name="enable">
                <value value="1"/>
                <description>
                  <text>Auto-recovery enable</text>
                </description>
              </enum>
            </type>
          </leaf>
          <leaf name="recovery-timer">
            <description>
              <text>UDLD recovery timer</text>
            </description>
            <type name="uint32">
              <range value="10..600"/>
            </type>
          </leaf>
          <leaf name="message-timer">
            <description>
              <text>UDLD message timer</text>
            </description>
            <type name="uint8">
              <range value="7..90"/>
            </type>
          </leaf>
          <leaf name="neighbour-num">
            <description>
              <text>Neighbour num</text>
            </description>
            <type name="uint8">
              <range value="0..16"/>
            </type>
          </leaf>
          <container name="neighbour">
            <description>
              <text>UDLD neighbour information</text>
            </description>
            <list name="neighbour-info">
              <description>
                <text>UDLD neighbour information</text>
              </description>
              <key value="port-id"/>
              <leaf name="port-id">
                <description>
                  <text>Neighbour port ID</text>
                </description>
                <type name="ncd:ifname"/>
              </leaf>
              <leaf name="device-id">
                <description>
                  <text>Neighbour device ID</text>
                </description>
                <type name="string">
                  <length value="1..32"/>
                </type>
              </leaf>
              <leaf name="device-name">
                <description>
                  <text>Neighbour device ID</text>
                </description>
                <type name="string">
                  <length value="1..64"/>
                </type>
              </leaf>
              <leaf name="expiration-timer">
                <description>
                  <text>Neighbour expiration timer</text>
                </description>
                <type name="uint32">
                  <range value="0..270"/>
                </type>
              </leaf>
              <leaf name="message-timer">
                <description>
                  <text>Neighbour message timer</text>
                </description>
                <type name="uint8">
                  <range value="0..90"/>
                </type>
              </leaf>
              <leaf name="timeout-interval">
                <description>
                  <text>Neighbour timeout interval</text>
                </description>
                <type name="uint8">
                  <range value="0..255"/>
                </type>
              </leaf>
            </list>
          </container>
        </list>
      </container>
    </container>
  </container>
</module>
