  <feature name="log-directory">
    <description>
      <text>The configured path of the policy</text>
    </description>
  </feature>
  <feature name="portal-ftp">
    <description>
      <text>Portal ftp feature.</text>
    </description>
  </feature>
  <feature name="log-nat">
    <description>
      <text>Log nat feature.</text>
    </description>
  </feature>
  <feature name="log-nat-sftp">
    <description>
      <text>Log nat sftp feature.</text>
    </description>
  </feature>
  <feature name="alarm-threshold">
    <description>
      <text>The alarm threshold of logging buffer.</text>
    </description>
  </feature>
  <feature name="logging-sftp">
    <description>
      <text>Logging sftp feature.</text>
    </description>
  </feature>
  <typedef name="alarm-level">
    <type name="enumeration">
      <enum name="emergencies">
        <value value="1"/>
        <description>
          <text>Level of emergence.</text>
        </description>
      </enum>
      <enum name="alerts">
        <value value="2"/>
        <description>
          <text>Level of alert.</text>
        </description>
      </enum>
      <enum name="critical">
        <value value="3"/>
        <description>
          <text>Level of  critical.</text>
        </description>
      </enum>
      <enum name="errors">
        <value value="4"/>
        <description>
          <text>Level of error.</text>
        </description>
      </enum>
      <enum name="warnings">
        <value value="5"/>
        <description>
          <text>Level of warning.</text>
        </description>
      </enum>
      <enum name="notifications">
        <value value="6"/>
        <description>
          <text>Level of notification.</text>
        </description>
      </enum>
      <enum name="informational">
        <value value="7"/>
        <description>
          <text>Level of information.</text>
        </description>
      </enum>
      <enum name="debugging">
        <value value="8"/>
        <description>
          <text>Level of debug.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Alarm level.</text>
    </description>
  </typedef>
  <typedef name="overflow-mode">
    <type name="enumeration">
      <enum name="clear">
        <value value="2"/>
        <description>
          <text>Clear logging buffer when logging buffer is full.</text>
        </description>
      </enum>
      <enum name="part-removed">
        <value value="3"/>
        <description>
          <text>Clear 1/3 logging buffer when logging buffer is full.</text>
        </description>
      </enum>
      <enum name="cycle">
        <value value="4"/>
        <description>
          <text>Cycle when logging buffer is full.</text>
        </description>
      </enum>
      <enum name="drop">
        <value value="5"/>
        <description>
          <text>No logging to buffer and file when logging buffer is full.</text>
        </description>
      </enum>
    </type>
    <default value="part-removed"/>
    <description>
      <text>The clear mode of logging buffer.</text>
    </description>
  </typedef>
  <typedef name="severity-relation">
    <type name="enumeration">
      <enum name="higher-or-equal">
        <value value="1"/>
      </enum>
      <enum name="equal">
        <value value="2"/>
      </enum>
      <enum name="lower-or-equal">
        <value value="3"/>
      </enum>
    </type>
    <description>
      <text>Severity relation.</text>
    </description>
  </typedef>
  <typedef name="flush-destination">
    <type name="enumeration">
      <enum name="local">
        <value value="0"/>
        <description>
          <text>Flush to local file.</text>
        </description>
      </enum>
      <enum name="ftp">
        <value value="1"/>
        <description>
          <text>Flush to FTP/SFTP server.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Destination</text>
    </description>
  </typedef>
  <typedef name="switch-type">
    <type name="enumeration">
      <enum name="off">
        <value value="0"/>
        <description>
          <text>Switch off.</text>
        </description>
      </enum>
      <enum name="on">
        <value value="1"/>
        <description>
          <text>Switch on.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Swtich.</text>
    </description>
  </typedef>
  <typedef name="protocol-type">
    <type name="enumeration">
      <enum name="ftp">
        <value value="0"/>
        <description>
          <text>FTP transmission protocol.</text>
        </description>
      </enum>
      <enum name="sftp">
        <value value="1"/>
        <description>
          <text>SFTP transmission protocol.</text>
        </description>
      </enum>
    </type>
    <default value="ftp"/>
    <description>
      <text>FTP or SFTP transmission protocol.</text>
    </description>
  </typedef>
  <container name="configuration">
    <description>
      <text>Configuration info.</text>
    </description>
    <container name="log">
      <description>
        <text>Log module.</text>
      </description>
      <container name="global-config">
        <description>
          <text>Set logging global configuration.</text>
        </description>
        <leaf name="timestamps">
          <description>
            <text>Set logging timestamps.</text>
          </description>
          <type name="enumeration">
            <enum name="localtime">
              <value value="1"/>
              <description>
                <text>Use local time zone for timestamps.</text>
              </description>
            </enum>
            <enum name="uptime">
              <value value="2"/>
              <description>
                <text>Timestamp with system uptime.</text>
              </description>
            </enum>
            <enum name="precisetime">
              <value value="3"/>
              <description>
                <text>Timestamp with high precision time.</text>
              </description>
            </enum>
          </type>
          <default value="localtime"/>
        </leaf>
        <leaf name="switch">
          <description>
            <text>Logging switch to all supported destinations.</text>
          </description>
          <type name="switch-type"/>
          <default value="on"/>
        </leaf>
        <leaf name="zip">
          <description>
            <text>Set logging zip.</text>
          </description>
          <type name="enumeration">
            <enum name="off">
              <value value="0"/>
              <description>
                <text>Logging zip off.</text>
              </description>
            </enum>
            <enum name="on">
              <value value="1"/>
              <description>
                <text>Logging zip on.</text>
              </description>
            </enum>
          </type>
          <default value="off"/>
          <ncd:revise-date date="2019-04-09"/>
        </leaf>
        <container name="header-info">
          <description>
            <text>Logging header info.</text>
          </description>
          <leaf name="header-flag">
            <description>
              <text>Set logging header flag.</text>
            </description>
            <type name="enumeration">
              <enum name="disable">
                <value value="0"/>
                <description>
                  <text>Logging header disable.</text>
                </description>
              </enum>
              <enum name="enable">
                <value value="1"/>
                <description>
                  <text>Logging header enable.</text>
                </description>
              </enum>
            </type>
            <default value="disable"/>
          </leaf>
          <ncd:revise-date date="2020-02-29"/>
        </container>
      </container>
      <container name="alarm">
        <description>
          <text>Set alarm logging parameters.</text>
        </description>
        <choice name="realtime-upload">
          <description>
            <text>Set realtime upload parameters.</text>
          </description>
          <case name="ftp">
            <description>
              <text>Set FTP logging parameters.</text>
            </description>
            <container name="ftp">
              <description>
                <text>Set FTP logging parameters.</text>
              </description>
              <presence value="Set FTP logging parameters"/>
              <leaf name="vrf-name">
                <description>
                  <text>VPN routing/forwarding instance (VRF) name.</text>
                </description>
                <type name="string">
                  <length value="1..32"/>
                </type>
              </leaf>
              <leaf name="file-name">
                <description>
                  <text>File name.</text>
                </description>
                <type name="string">
                  <length value="1..31"/>
                </type>
                <default value="zxralarm.log"/>
              </leaf>
              <leaf name="level">
                <description>
                  <text>The alarm level.</text>
                </description>
                <type name="alarm-level"/>
                <mandatory value="true"/>
              </leaf>
              <leaf name="user-name">
                <description>
                  <text>The FTP user name.</text>
                </description>
                <type name="string">
                  <length value="1..65"/>
                </type>
                <mandatory value="true"/>
              </leaf>
              <choice name="pwd-type">
                <description>
                  <text>FTP password.</text>
                </description>
                <mandatory value="true"/>
                <case name="clear-pwd">
                  <description>
                    <text>Clear-text choice.</text>
                  </description>
                  <leaf name="password">
                    <description>
                      <text>The FTP password, when configuration can only enter the clear password, the length range is 1 to 128. The query result returns the encrypted password with a fixed length 64,84,108,148,172,192,212 or 236.</text>
                    </description>
                    <type name="string">
                      <length value="1..128"/>
                    </type>
                    <mandatory value="true"/>
                  </leaf>
                </case>
                <case name="enc-pwd">
                  <description>
                    <text>Encrypted choice.</text>
                  </description>
                  <leaf name="enc-password">
                    <description>
                      <text>Encrypted password,fixed length is 64,84,108,148,172,192,212 or 236.</text>
                    </description>
                    <type name="string">
                      <length value="64..236">
                        <description>
                          <text>Length must be 64,84,108,148,172,192,212 or 236..</text>
                        </description>
                      </length>
                    </type>
                    <mandatory value="true"/>
                  </leaf>
                  <ncd:revise-date date="2019-01-31"/>
                </case>
              </choice>
              <choice name="server-addr">
                <description>
                  <text>The FTP server address.</text>
                </description>
                <mandatory value="true"/>
                <case name="ipv4">
                  <description>
                    <text>IPv4 address.</text>
                  </description>
                  <container name="ipv4-addr">
                    <description>
                      <text>IPv4 address.</text>
                    </description>
                    <leaf name="ipv4-address">
                      <description>
                        <text>IPv4 address.</text>
                      </description>
                      <type name="ncd:ipv4-address"/>
                      <mandatory value="true"/>
                    </leaf>
                  </container>
                </case>
                <case name="ipv6">
                  <description>
                    <text>IPv6 address.</text>
                  </description>
                  <container name="ipv6-addr">
                    <description>
                      <text>IPv6 address.</text>
                    </description>
                    <choice name="ipv6-type">
                      <description>
                        <text>IPv6 address type.</text>
                      </description>
                      <mandatory value="true"/>
                      <case name="linklocal">
                        <description>
                          <text>Link-local type.</text>
                        </description>
                        <container name="ipv6-linklocal">
                          <description>
                            <text>IPv6 link-local type.</text>
                          </description>
                          <leaf name="ipv6-linklocal">
                            <description>
                              <text>IPv6 address.</text>
                            </description>
                            <type name="ncd:ipv6-address">
                              <pattern value="^[Ff][Ee][8-9a-bA-B][0-9a-fA-F]"/>
                            </type>
                            <mandatory value="true"/>
                          </leaf>
                          <leaf name="interface-name">
                            <description>
                              <text>Interface name.</text>
                            </description>
                            <type name="ncd:ifname">
