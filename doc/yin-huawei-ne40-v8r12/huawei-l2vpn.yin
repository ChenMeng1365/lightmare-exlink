    <description>
      <text>VPWS switch instance.</text>
    </description>
    <leaf name="switch-type">
      <ext:operation-exclude value="update"/>
      <type name="l2vpn-switch-type"/>
      <mandatory value="true"/>
      <description>
        <text>Switch type of a VPWS switch instance.</text>
      </description>
    </leaf>
    <leaf name="encapsulation-type">
      <type name="encapsulation-type"/>
      <must condition="../encapsulation-type='fr' or&#10;../encapsulation-type='atm-aal5-sdu' or&#10;../encapsulation-type='atm-trans-cell' or&#10;../encapsulation-type='vlan' or&#10;../encapsulation-type='ethernet' or&#10;../encapsulation-type='hdlc' or&#10;../encapsulation-type='ppp' or&#10;../encapsulation-type='atm-nto1-vcc' or&#10;../encapsulation-type='atm-nto1-vpc' or&#10;../encapsulation-type='ip-layer2' or&#10;../encapsulation-type='atm-1to1-vcc' or&#10;../encapsulation-type='atm-1to1-vpc' or&#10;../encapsulation-type='atm-aal5-pdu' or&#10;../encapsulation-type='cep' or&#10;../encapsulation-type='satop-e1' or&#10;../encapsulation-type='satop-t1' or&#10;../encapsulation-type='satop-e3' or&#10;../encapsulation-type='cesopsn-basic' or&#10;../encapsulation-type='ip-interworking'"/>
      <mandatory value="true"/>
      <description>
        <text>Encapsulation type of a VPWS switch instance.</text>
      </description>
    </leaf>
    <leaf name="control-word-trans">
      <type name="boolean"/>
      <default value="false"/>
      <description>
        <text>Enable/disable the control word of a VPWS switching instance to be transparently transmitted.</text>
      </description>
    </leaf>
    <leaf name="control-word">
      <when condition="not(../switch-type='ldp-ldp' or ../switch-type='ldp-ldp-bak')"/>
      <type name="l2vpn-control-word"/>
      <must condition=".!='prefer'"/>
      <default value="default"/>
      <description>
        <text>Enable the control word function. The control word function is usually used for TDM, ATM, and FR PWs.</text>
      </description>
    </leaf>
    <leaf name="mtu">
      <when condition="../switch-type='ldp-svc' and (../encapsulation-type='vlan' or ../encapsulation-type='ethernet')"/>
      <type name="uint16">
        <range value="46..9600"/>
      </type>
      <default value="1500"/>
      <description>
        <text>MTU of a VPWS switch instance.</text>
      </description>
    </leaf>
    <leaf name="bit-error-detection">
      <when condition="../switch-type='ldp-ldp'"/>
      <type name="boolean"/>
      <default value="false"/>
      <description>
        <text>Enable/disable bit error detection on SPEs for an LDP MS-PW.</text>
      </description>
    </leaf>
    <leaf name="create-time">
      <type name="string">
        <length value="1..80"/>
      </type>
      <config value="false"/>
      <description>
        <text>How long a VPWS switch instance has been created.</text>
      </description>
    </leaf>
    <leaf name="up-time">
      <type name="string">
        <length value="1..80"/>
      </type>
      <config value="false"/>
      <description>
        <text>How long a VPWS switch instance has been in the up state. If a VPWS instance is in the down state, its VPWS instance up duration is 0.</text>
      </description>
    </leaf>
    <leaf name="last-change-time">
      <type name="string">
        <length value="1..80"/>
      </type>
      <config value="false"/>
      <description>
        <text>How long the status of a VPWS switch instance stays unchanged since the last change time.</text>
      </description>
    </leaf>
    <container name="pws">
      <description>
        <text>List of VPWS PWs.</text>
      </description>
      <list name="pw">
        <ext:operation-exclude value="update|delete"/>
        <must condition="pw-id and peer-ip"/>
        <must condition="(((../../switch-type='ldp-svc' and role='right-primary') or ../../switch-type='svc-svc') and transmit-label and receive-label) or not((../../switch-type='ldp-svc' and role='right-primary') or ../../switch-type='svc-svc')"/>
        <key value="role"/>
        <unique tag="peer-ip"/>
        <min-elements value="2"/>
        <max-elements value="3"/>
        <description>
          <text>Configure a PW in a VPWS switching instance.</text>
        </description>
        <leaf name="role">
          <type name="ms-pw-role-type"/>
          <description>
            <text>Role of a PW.</text>
          </description>
        </leaf>
        <leaf name="pw-id">
          <ext:operation-exclude value="update|delete"/>
          <type name="uint32">
            <range value="1..4294967295"/>
          </type>
          <description>
            <text>PW ID.</text>
          </description>
        </leaf>
        <leaf name="peer-ip">
          <ext:operation-exclude value="update|delete"/>
          <type name="l2vpn-peer-address-type"/>
          <description>
            <text>IP address of a peer.</text>
          </description>
        </leaf>
        <leaf name="transmit-label">
          <ext:operation-exclude value="update|delete"/>
          <when condition="(../../../switch-type='ldp-svc' and ../role='right-primary') or ../../../switch-type='svc-svc'"/>
          <type name="uint32">
            <range value="0..1048575"/>
          </type>
          <description>
            <text>Static label for sending packets.</text>
          </description>
        </leaf>
        <leaf name="receive-label">
          <ext:operation-exclude value="update|delete"/>
          <when condition="(../../../switch-type='ldp-svc' and ../role='right-primary') or ../../../switch-type='svc-svc'"/>
          <type name="uint32">
            <range value="16..1048575"/>
          </type>
          <description>
            <text>Static label for receiving packets.</text>
          </description>
        </leaf>
        <leaf name="tunnel-policy">
          <type name="string">
            <length value="1..39"/>
          </type>
          <description>
            <text>Name of a tunnel policy. If the tunnel policy name is not specified, the system uses the default tunnel policy. The default tunnel policy preferentially selects LSPs for packet transmission and the number of load-balancing tunnels is 1. If the tunnel policy name is specified but the corresponding tunnel policy does not exist, the system still uses the default tunnel policy.</text>
          </description>
        </leaf>
        <container name="flow-label">
          <must condition="ability or oam-packet-pop='true'"/>
          <presence value="enbale flow label for pw"/>
          <description>
            <text>Enable/disable the flow label of a PW.</text>
          </description>
          <leaf name="ability">
            <ext:operation-exclude value="delete"/>
            <when condition="../../../../switch-type='ldp-svc' and ../oam-packet-pop ='false'"/>
            <type name="flow-label-ability"/>
            <must condition=".!='default' and .!='disable'"/>
            <description>
              <text>Flow label capability. The values of the ability node at both ends of a PW must be consistent.</text>
            </description>
          </leaf>
          <leaf name="oam-packet-pop">
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable the function of popping out flow labels in the OAM packets.</text>
            </description>
          </leaf>
        </container>
        <container name="hard-pipe">
          <when condition="../../../switch-type='svc-svc' or (../../../switch-type='ldp-svc' and ../role='right-primary')"/>
          <presence value="Enable hard pipe for pw."/>
          <description>
            <text>Enable/disable hard pipe.</text>
          </description>
          <leaf name="bandwidth">
            <when condition="../../../../encapsulation-type='ethernet' or ../../../../encapsulation-type='vlan'"/>
            <type name="uint32">
              <range value="16..4294967294"/>
            </type>
            <units name="kbit/s"/>
            <mandatory value="true"/>
            <description>
              <text>Specify bandwidth value.</text>
            </description>
          </leaf>
          <leaf name="expand-ratio">
            <type name="uint16">
              <range value="0..10000"/>
            </type>
            <default value="10"/>
            <description>
              <text>Specify the value of bandwidth expand ratio (%). As to CES service, expand ratio ranges from 0 to 10000. The other service, expand ratio ranges from 0 to 100.</text>
            </description>
          </leaf>
        </container>
        <container name="tdm-parameter">
          <when condition="../../../switch-type='ldp-svc' and&#10;(../../../encapsulation-type='cep' or&#10;../../../encapsulation-type='cesopsn-basic' or&#10;../../../encapsulation-type='satop-e1' or&#10;../../../encapsulation-type='satop-t1' or&#10;../../../encapsulation-type='satop-e3') and&#10;(../role='left-primary')"/>
          <description>
            <text>Configure TDM parameters.</text>
          </description>
          <leaf name="tdm-encapsulate-number">
            <when condition="../../../../encapsulation-type='cesopsn-basic' or&#10;../../../../encapsulation-type='satop-e1' or&#10;../../../../encapsulation-type='satop-t1' or&#10;../../../../encapsulation-type='satop-e3'"/>
            <type name="uint32">
              <range value="1..5000"/>
            </type>
            <ext:dynamic-default/>
            <description>
              <text>Number of TDM frames encapsulated in a CESoPSN or SAToP packet in the TDMoPSN application.</text>
            </description>
          </leaf>
          <leaf name="rtp-header">
            <type name="l2vpn-rtp-header-type"/>
            <default value="false"/>
            <description>
              <text>Add the RTP header to transparently transmitted TDM frames.</text>
            </description>
          </leaf>
          <leaf name="time-slot">
            <when condition="../../../../encapsulation-type='cesopsn-basic'"/>
            <type name="uint32">
              <range value="1..32"/>
            </type>
            <description>
              <text>Number of time slots on an interface.</text>
            </description>
          </leaf>
        </container>
        <container name="pw-info">
          <config value="false"/>
          <description>
            <text>Operational data of VPWS PW information.</text>
          </description>
          <leaf name="session-state">
            <type name="l2vpn-state"/>
            <description>
              <text>Status of an LDP session between the endpoint devices of a VC.</text>
            </description>
          </leaf>
          <leaf name="pw-state">
            <type name="l2vpn-pw-state"/>
            <description>
              <text>Status of a PW.</text>
            </description>
          </leaf>
          <leaf name="pw-id">
            <type name="uint32"/>
            <description>
              <text>PW ID.</text>
            </description>
          </leaf>
          <leaf name="encapsulation-type">
            <type name="encapsulation-type"/>
            <description>
              <text>Encapsulation type of a PW.</text>
            </description>
          </leaf>
          <leaf name="destination">
            <type name="inet:ip-address-no-zone"/>
            <description>
              <text>Destination address, usually the LSR ID of a peer.</text>
            </description>
          </leaf>
          <leaf name="local-vc-label">
            <type name="uint32"/>
            <description>
              <text>Local VC label.</text>
            </description>
          </leaf>
          <leaf name="remote-vc-label">
            <type name="uint32"/>
            <description>
              <text>VC label on the remote end.</text>
            </description>
          </leaf>
          <leaf name="local-ac-oam">
            <type name="l2vpn-state"/>
            <description>
              <text>OAM status of a local AC.</text>
            </description>
          </leaf>
          <leaf name="local-psn-oam">
            <type name="l2vpn-state"/>
            <description>
              <text>PSN OAM status on the local end.</text>
            </description>
          </leaf>
          <leaf name="local-forward-state">
            <type name="l2vpn-forward-state"/>
            <description>
              <text>Forwarding status of the local end.</text>
            </description>
          </leaf>
          <leaf name="remote-ac-oam">
            <type name="l2vpn-state"/>
            <description>
              <text>OAM status on the remote end.</text>
            </description>
          </leaf>
          <leaf name="remote-psn-oam">
            <type name="l2vpn-state"/>
            <description>
              <text>PSN OAM status on the remote end.</text>
            </description>
          </leaf>
          <leaf name="remote-forward-state">
            <type name="l2vpn-forward-state"/>
            <description>
              <text>Forwarding status of the remote end.</text>
            </description>
          </leaf>
          <leaf name="local-vc-mtu">
            <type name="uint32"/>
            <description>
              <text>MTU for a local VC.</text>
            </description>
          </leaf>
          <leaf name="remote-vc-mtu">
            <type name="uint32"/>
            <description>
              <text>VC MTU on the remote end.</text>
            </description>
          </leaf>
          <leaf name="local-vccv">
            <type name="string">
              <length value="0..40"/>
            </type>
            <description>
              <text>VCCV capability supported by the local end. By default, the LSP ping and BFD for the label alert mode can be used if the control word function is disabled. If the control word function is enabled, the LSP ping and BFD for both the label alert mode and control word mode can be used.</text>
            </description>
          </leaf>
          <leaf name="remote-vccv">
            <type name="string">
              <length value="0..40"/>
            </type>
            <description>
              <text>VCCV capability supported by the remote end. By default, the LSP ping and BFD for the label alert mode can be used if the control word function is disabled. If the control word function is enabled, the LSP ping and BFD for both the label alert mode and control word mode can be used.</text>
            </description>
          </leaf>
          <leaf name="local-control-word">
            <type name="l2vpn-control-word"/>
            <description>
              <text>Enable the control word function on the local end.</text>
            </description>
          </leaf>
          <leaf name="remote-control-word">
            <type name="l2vpn-control-word"/>
            <description>
              <text>Enable the control word function on the remote end.</text>
            </description>
          </leaf>
          <leaf name="tunnel-policy">
            <type name="string">
              <length value="1..39"/>
            </type>
            <description>
              <text>Name of a tunnel policy.</text>
            </description>
          </leaf>
          <leaf name="tunnel-count">
            <type name="uint16"/>
            <default value="0"/>
            <description>
              <text>Number of effective tunnels.</text>
            </description>
          </leaf>
          <leaf name="spe-count">
            <type name="uint32">
              <range value="0..4294967295"/>
            </type>
            <default value="0"/>
            <description>
              <text>Number of SPEs in the path to the peer.</text>
            </description>
          </leaf>
          <leaf name="create-time">
            <type name="string">
              <length value="1..80"/>
            </type>
            <description>
              <text>How long a PW has been created.</text>
            </description>
          </leaf>
          <leaf name="up-time">
            <type name="string">
              <length value="1..80"/>
            </type>
            <description>
              <text>How long a PW has been in the up state. If a PW is in the down state, its PW up duration is 0.</text>
            </description>
          </leaf>
          <leaf name="last-change-time">
            <type name="string">
              <length value="1..80"/>
            </type>
            <description>
              <text>How long the status of a PW has stayed unchanged since the last change time.</text>
            </description>
          </leaf>
          <leaf name="last-up-time">
            <type name="string">
              <length value="1..80"/>
            </type>
            <description>
              <text>Date and time when a PW was up for the last time, for example, 2012/07/20 02:30:46.</text>
            </description>
          </leaf>
          <leaf name="total-up-time">
            <type name="string">
              <length value="1..80"/>
            </type>
            <description>
              <text>Total up time of a PW.</text>
            </description>
          </leaf>
          <leaf name="c-key">
            <type name="uint32"/>
            <description>
              <text>Customer key. Each CKey corresponds to a PW.</text>
            </description>
          </leaf>
          <leaf name="n-key">
            <type name="uint32"/>
            <description>
              <text>Network key. Each NKey corresponds to a combination of the peer and tunnel policy.</text>
            </description>
          </leaf>
          <leaf name="remote-vccv-code">
            <type name="pub-type:hex-binary"/>
            <description>
              <text>Remote VCCV code.</text>
            </description>
          </leaf>
          <leaf name="hard-pipe-bandwidth">
            <type name="uint32">
              <range value="16..4294967294"/>
            </type>
            <units name="kbit/s"/>
            <description>
              <text>Specify bandwidth value.</text>
            </description>
          </leaf>
          <leaf name="hard-pipe-expand-ratio">
            <type name="string">
              <length value="0..7"/>
            </type>
            <description>
              <text>Specify the value of bandwidth expand ratio (%).</text>
            </description>
          </leaf>
          <leaf name="pw-token-exceed">
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Indicates whether the number of PW tokens exceeds the maximum limit.</text>
            </description>
          </leaf>
          <container name="spes">
            <config value="false"/>
            <description>
              <text>Operational data of SPEs on the path to the peer.</text>
            </description>
            <uses name="l2vpn:spe-infos-type"/>
          </container>
          <container name="tunnels">
            <config value="false"/>
            <description>
              <text>List of VPWS PW tunnels.</text>
            </description>
            <list name="tunnel">
              <key value="tunnel-key"/>
              <max-elements value="32"/>
              <description>
                <text>Operational data of tunnels.</text>
              </description>
              <leaf name="tunnel-key">
                <type name="string">
                  <length value="0..21"/>
                </type>
                <description>
                  <text>Index of a public network tunnel.</text>
                </description>
              </leaf>
              <leaf name="tunnel-type">
                <type name="l2vpn-tunnel-type"/>
                <description>
                  <text>Type of the tunnel used by the PW.</text>
                </description>
              </leaf>
              <leaf name="tunnel-name">
                <type name="string">
                  <length value="0..64"/>
                </type>
                <description>
                  <text>Name of the tunnel used by the PW.</text>
                </description>
              </leaf>
              <leaf name="public-next-hop">
                <type name="inet:ipv4-address-no-zone"/>
                <description>
                  <text>Next hop of a PW on the public network.</text>
                </description>
              </leaf>
            </list>
          </container>
          <container name="down-causes">
            <description>
              <text>Operational data of the down causes.</text>
            </description>
            <uses name="l2vpn:l2vpn-pw-down-causes-type"/>
          </container>
          <container name="status-changes">
            <description>
              <text>Operational data of the reason and time.</text>
            </description>
            <uses name="l2vpn:l2vpn-pw-status-changes-type"/>
          </container>
        </container>
      </list>
    </container>
    <container name="reliability">
      <when condition="../switch-type='ldp-ldp-bak'"/>
      <description>
        <text>Configure PW reliability.</text>
      </description>
      <leaf name="redundancy-mode">
        <type name="pw-redundancy-mode"/>
        <must condition="../redundancy-mode='master'"/>
        <default value="master"/>
        <description>
          <text>PW redundancy mode.</text>
        </description>
      </leaf>
      <leaf name="switchover">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable the PW switchover.</text>
        </description>
      </leaf>
      <container name="reroute">
        <description>
          <text>Configure a switchback policy list.</text>
        </description>
        <leaf name="reroute-policy">
          <type name="l2vpn-reroute-mode"/>
          <default value="delay"/>
          <description>
            <text>Switchback policy.</text>
          </description>
        </leaf>
        <leaf name="delay-time">
          <when condition="../reroute-policy='delay'"/>
          <type name="uint32">
            <range value="10..10800"/>
          </type>
          <units name="s"/>
          <default value="30"/>
          <description>
            <text>Delay for switching traffic back to the primary PW.</text>
          </description>
        </leaf>
        <leaf name="last-switchback-reason">
          <type name="string">
            <length value="0..100"/>
          </type>
          <config value="false"/>
          <description>
            <text>Last switchback reason.</text>
          </description>
        </leaf>
        <leaf name="last-switchback-time">
          <type name="string">
            <length value="1..60"/>
          </type>
          <config value="false"/>
          <description>
            <text>Last switchback time.</text>
          </description>
        </leaf>
        <leaf name="delay-residual">
          <type name="uint32"/>
          <units name="s"/>
          <config value="false"/>
          <description>
            <text>Remaining delay time.</text>
          </description>
        </leaf>
      </container>
    </container>
  </grouping>
  <grouping name="vpws-bgp-instance-grp">
    <description>
      <text>VPWS BGP VPN instance connection.</text>
    </description>
    <leaf name="vpn-instance">
      <type name="leafref">
        <path value="/l2vpn/common/vpws-bgp-vpns/vpws-bgp-vpn/name"/>
      </type>
      <mandatory value="true"/>
      <description>
        <text>VPWS BGP instance.</text>
      </description>
    </leaf>
    <leaf name="local-site">
      <type name="leafref">
        <path value="/l2vpn/common/vpws-bgp-vpns/vpws-bgp-vpn/sites/site/name"/>
      </type>
      <mandatory value="true"/>
      <description>
        <text>Local site name.</text>
      </description>
    </leaf>
    <container name="acs">
      <description>
        <text>List of attachment circuits in a VPWS BGP VPN instance.</text>
      </description>
      <list name="ac">
        <key value="interface-name"/>
        <min-elements value="1"/>
        <max-elements value="1"/>
        <description>
          <text>Configure a connection attachment circuit.</text>
        </description>
        <leaf name="interface-name">
          <type name="leafref">
            <path value="/ifm:ifm/ifm:interfaces/ifm:interface/ifm:name"/>
          </type>
          <description>
            <text>VPWS BGP VPN instance connection attachment circuit interface name. The encapsulation type of this interface must be the same as that of the VPN instance. Otherwise, the PW cannot go Up.</text>
          </description>
        </leaf>
        <leaf name="tagged">
          <when condition="/l2vpn:l2vpn/l2vpn:common/l2vpn:vpws-bgp-vpns/l2vpn:vpws-bgp-vpn[l2vpn:name=current()/../../../l2vpn:vpn-instance]/l2vpn:encapsulation-type !='ip-interworking' and /l2vpn:l2vpn/l2vpn:common/l2vpn:vpws-bgp-vpns/l2vpn:vpws-bgp-vpn[l2vpn:name=current()/../../../l2vpn:vpn-instance]/l2vpn:encapsulation-type !='ip-layer2'"/>
          <ext:operation-exclude value="update|delete"/>
          <type name="boolean"/>
          <description>
            <text>Enable/disable the attachment circuit tag of the VPWS BGP instance.</text>
          </description>
          <ext:dynamic-default/>
        </leaf>
        <leaf name="trigger-interface-down">
          <type name="boolean"/>
          <must condition="(/ifm:ifm/ifm:interfaces/ifm:interface[ifm:name=current()/../interface-name]/ifm:class='main-interface' and /ifm:ifm/ifm:interfaces/ifm:interface[ifm:name=current()/../interface-name]/ifm:link-protocol='ethernet' and . = 'true') or (. = 'false')"/>
          <default value="false"/>
          <description>
            <text>Enable/disable remote interface shutdown-based association between the PW status and interface status.</text>
          </description>
        </leaf>
        <leaf name="state">
          <type name="l2vpn-state"/>
          <config value="false"/>
          <description>
            <text>Status of an AC interface.</text>
          </description>
        </leaf>
        <leaf name="token-exceed">
          <type name="boolean"/>
          <default value="false"/>
          <config value="false"/>
          <description>
            <text>Indicates whether the number of AC tokens exceeds the maximum limit.</text>
          </description>
        </leaf>
      </list>
    </container>
    <container name="pws">
      <description>
        <text>List of PWs.</text>
      </description>
      <list name="pw">
        <key value="remote-site-id"/>
        <unique tag="role"/>
        <min-elements value="1"/>
        <description>
          <text>Configure a PW.</text>
        </description>
        <leaf name="remote-site-id">
          <type name="uint16">
            <range value="0..65534"/>
          </type>
          <must condition="../l2vpn:remote-site-id &lt; (/l2vpn:l2vpn/l2vpn:common/l2vpn:vpws-bgp-vpns/l2vpn:vpws-bgp-vpn[l2vpn:name=current()/../../../l2vpn:vpn-instance]/l2vpn:sites/l2vpn:site[l2vpn:name = current()/../../../l2vpn:local-site]/range + /l2vpn:l2vpn/l2vpn:common/l2vpn:vpws-bgp-vpns/l2vpn:vpws-bgp-vpn[l2vpn:name = current()/../../../l2vpn:vpn-instance]/l2vpn:sites/l2vpn:site[l2vpn:name = current()/../../../l2vpn:local-site]/default-offset) and ../l2vpn:remote-site-id &gt;= /l2vpn:l2vpn/l2vpn:common/l2vpn:vpws-bgp-vpns/l2vpn:vpws-bgp-vpn[l2vpn:name = current()/../../../l2vpn:vpn-instance]/l2vpn:sites/l2vpn:site[l2vpn:name = current()/../../../l2vpn:local-site]/default-offset"/>
          <description>
            <text>VPWS BGP VPN instance connection remote customer edge identifier.</text>
          </description>
        </leaf>
        <leaf name="role">
          <ext:operation-exclude value="update|delete"/>
          <type name="pw-role-type"/>
          <must condition="(.='primary') or (.='backup' and ../../pw[role='primary'])"/>
          <default value="primary"/>
          <description>
            <text>VPWS BGP VPN instance connection pseudo wire role. Configure a primary PW first. The PW role canot be changed.</text>
          </description>
        </leaf>
        <leaf name="tunnel-policy">
          <type name="string">
            <length value="1..39"/>
          </type>
          <description>
            <text>VPWS BGP VPN instance connection tunnel policy name.</text>
          </description>
        </leaf>
        <leaf name="traffic-statistics-collection">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable connection traffic statistics collection of a VPWS BGP VPN instance.</text>
          </description>
        </leaf>
        <leaf name="manual-set-fault">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable the manual setting of faults in a VPWS BGP VPN instance connection.</text>
          </description>
        </leaf>
        <container name="bfd">
          <when condition="/bfd:bfd"/>
          <must condition="/l2vpn:l2vpn/l2vpn:common/l2vpn:vpws-bgp-vpns/l2vpn:vpws-bgp-vpn[l2vpn:name=current()/../../../l2vpn:vpn-instance]/l2vpn:control-word='enable'"/>
          <presence value="enable pw bfd."/>
          <description>
            <text>Enable/disable BFD for PW in a VPWS BGP VPN instance.</text>
          </description>
          <uses name="l2vpn-dyn-bfd-para-type"/>
          <leaf name="track-interface">
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable BFD track interface state.</text>
            </description>
          </leaf>
        </container>
        <container name="pw-info">
          <config value="false"/>
          <description>
            <text>Operational data of VPWS BGP PW information.</text>
          </description>
          <leaf name="type">
            <type name="vpws-bgp-pw-type"/>
            <description>
              <text>Indicates the type of the pw.</text>
            </description>
          </leaf>
          <leaf name="local-vc-state">
            <type name="l2vpn-pw-state"/>
            <description>
              <text>Indicates the status of the local PW.</text>
            </description>
          </leaf>
          <leaf name="remote-vc-state">
            <type name="l2vpn-pw-state"/>
            <description>
              <text>Indicates the status of the remote PW.</text>
            </description>
          </leaf>
          <leaf name="peer-ip">
            <type name="inet:ipv4-address-no-zone"/>
            <description>
              <text>Indicates the LSR ID of the VC peer device.</text>
            </description>
          </leaf>
          <leaf name="local-vc-label">
            <type name="uint32"/>
            <description>
              <text>Indicates the local VC label.</text>
            </description>
          </leaf>
          <leaf name="remote-vc-label">
            <type name="uint32"/>
            <description>
              <text>Indicates the remote VC label.</text>
            </description>
          </leaf>
          <leaf name="ac-oam-state">
            <type name="vpws-bgp-ac-oam-state"/>
            <description>
              <text>Indicates the OAM status of the AC.</text>
            </description>
          </leaf>
          <leaf name="bfd-state">
            <type name="l2vpn-vpws-bfd-state"/>
            <description>
              <text>Indicates the status of the BFD for PW.</text>
            </description>
          </leaf>
          <leaf name="control-flag-bit">
            <type name="control-flag-bits-type"/>
            <description>
              <text>Indicates the control flag.</text>
            </description>
          </leaf>
          <leaf name="pw-token-exceed">
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Indicates whether the number of PW tokens exceeds the maximum limit.</text>
            </description>
          </leaf>
          <container name="down-causes">
            <config value="false"/>
            <description>
              <text>Operational data of the down causes.</text>
            </description>
            <uses name="l2vpn:l2vpn-pw-down-causes-type"/>
          </container>
        </container>
        <container name="tunnels">
          <config value="false"/>
          <description>
            <text>List of VPWS BGP PW tunnel info.</text>
          </description>
          <list name="tunnel">
            <key value="tunnel-key"/>
            <max-elements value="32"/>
            <description>
              <text>Operational data of tunnels.</text>
            </description>
            <leaf name="tunnel-key">
              <type name="string">
                <length value="0..21"/>
              </type>
              <description>
                <text>Index of a public network tunnel.</text>
              </description>
            </leaf>
            <leaf name="tunnel-type">
              <type name="l2vpn-tunnel-type"/>
              <description>
                <text>Type of the tunnel used by the PW.</text>
              </description>
            </leaf>
            <leaf name="tunnel-name">
              <type name="string">
                <length value="0..64"/>
              </type>
              <description>
                <text>Name of the tunnel used by the PW.</text>
              </description>
            </leaf>
            <leaf name="public-next-hop">
              <type name="inet:ipv4-address-no-zone"/>
              <description>
                <text>Next hop of a PW on the public network.</text>
              </description>
            </leaf>
          </list>
        </container>
        <container name="traffic-statistics">
          <config value="false"/>
          <description>
            <text>Operational data of VPWS BGP PW traffic statistics.</text>
          </description>
          <leaf name="interface-name">
            <type name="leafref">
              <path value="/ifm:ifm/ifm:interfaces/ifm:interface/ifm:name"/>
            </type>
            <description>
              <text>The name of interface.</text>
            </description>
          </leaf>
          <leaf name="last-clear-time">
            <type name="string">
              <length value="1..60"/>
            </type>
            <description>
              <text>Time when statistics are cleared for the last time.</text>
            </description>
          </leaf>
          <leaf name="input-bytes-rate">
            <type name="uint64"/>
            <description>
              <text>Rate of traffic received on the PW.</text>
            </description>
          </leaf>
          <leaf name="input-packets-rate">
            <type name="uint64"/>
            <description>
              <text>Rate of frames received on the PW.</text>
            </description>
          </leaf>
          <leaf name="output-bytes-rate">
            <type name="uint64"/>
            <description>
              <text>Rate of traffic transmitted out of the PW.</text>
            </description>
          </leaf>
          <leaf name="output-packets-rate">
            <type name="uint64"/>
            <description>
              <text>Rate of frames transmitted out of the PW.</text>
            </description>
          </leaf>
          <leaf name="input-bytes">
            <type name="uint64"/>
            <description>
              <text>Number of bytes received on the PW.</text>
            </description>
          </leaf>
          <leaf name="input-packets">
            <type name="uint64"/>
            <description>
              <text>Number of frames received on the PW.</text>
            </description>
          </leaf>
          <leaf name="output-bytes">
            <type name="uint64"/>
            <description>
              <text>Number of bytes transmitted out of the PW.</text>
            </description>
          </leaf>
          <leaf name="output-packets">
            <type name="uint64"/>
            <description>
              <text>Number of frames transmitted out of the PW.</text>
            </description>
          </leaf>
        </container>
      </list>
    </container>
    <container name="reliability">
      <when condition="count(../pws/pw)=2"/>
      <description>
        <text>Configure PW reliability.</text>
      </description>
      <leaf name="redundancy-mode">
        <type name="pw-redundancy-mode"/>
        <must condition="../redundancy-mode='frr'"/>
        <default value="frr"/>
        <description>
          <text>PW redundancy mode.</text>
        </description>
      </leaf>
      <leaf name="dual-receive">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable the receipt of both the primary and secondary PWs to receive packets.</text>
        </description>
      </leaf>
      <container name="reroute">
        <description>
          <text>Configure a switchback policy list.</text>
        </description>
        <leaf name="reroute-policy">
          <type name="l2vpn-reroute-mode"/>
          <default value="delay"/>
          <description>
            <text>Switchback policy.</text>
          </description>
        </leaf>
        <leaf name="delay-time">
          <when condition="../reroute-policy='delay'"/>
          <type name="uint32">
            <range value="10..10800"/>
          </type>
          <units name="s"/>
          <default value="30"/>
          <description>
            <text>Delay for switching traffic back to the primary PW.</text>
          </description>
        </leaf>
        <leaf name="resume-time">
          <when condition="not(../reroute-policy='never')"/>
          <type name="uint32">
            <range value="0..600"/>
          </type>
          <units name="s"/>
          <default value="10"/>
          <description>
            <text>Time after which the peer PE on the secondary PW is notified that the local PE has recovered. This parameter can be configured only in FRR mode.</text>
          </description>
        </leaf>
        <leaf name="last-switchback-reason">
          <type name="string">
            <length value="0..100"/>
          </type>
          <config value="false"/>
          <description>
            <text>Last switchback reason.</text>
          </description>
        </leaf>
        <leaf name="last-switchback-time">
          <type name="string">
            <length value="1..60"/>
          </type>
          <config value="false"/>
          <description>
            <text>Last switchback time.</text>
          </description>
        </leaf>
        <leaf name="delay-residual">
          <type name="uint32"/>
          <units name="s"/>
          <config value="false"/>
          <description>
            <text>Remaining delay time.</text>
          </description>
        </leaf>
        <leaf name="resume-residual">
          <type name="uint32"/>
          <units name="s"/>
          <config value="false"/>
          <description>
            <text>Remaining resume time.</text>
          </description>
        </leaf>
      </container>
    </container>
    <container name="oam-mapping">
      <presence value="enable AC OAM mapping"/>
      <description>
        <text>Enable/disable OAM mapping.</text>
      </description>
      <leaf name="type">
        <type name="ac-oam-mapping-type"/>
        <must condition="(../type='1ag' and ../md-name and ../ma-name) or ../type!='1ag'"/>
        <mandatory value="true"/>
        <description>
          <text>VPWS attachment circuit OAM mapping type.</text>
        </description>
      </leaf>
      <leaf name="md-name">
        <ext:operation-exclude value="update|delete"/>
        <when condition="../type='1ag'"/>
        <type name="string">
          <length value="1..43"/>
        </type>
        <description>
          <text>VPWS OAM mapping parameter md name.</text>
        </description>
      </leaf>
      <leaf name="ma-name">
        <ext:operation-exclude value="update|delete"/>
        <when condition="../type='1ag'"/>
        <type name="string">
          <length value="1..43"/>
        </type>
        <description>
          <text>VPWS OAM mapping parameter ma name.</text>
        </description>
      </leaf>
    </container>
  </grouping>
  <grouping name="vpls-instance-grp">
    <description>
      <text>VPLS instance.</text>
    </description>
    <leaf name="member-discovery-mode">
      <type name="l2vpn-vpls-vsi-mem-disc-type"/>
      <default value="default"/>
      <description>
        <text>VPLS member discovery mode for a specified VSI.</text>
      </description>
    </leaf>
    <leaf name="work-mode">
      <ext:operation-exclude value="update"/>
      <type name="vsi-work-mode"/>
      <default value="normal"/>
      <description>
        <text>Workmode of a VSI.</text>
      </description>
    </leaf>
    <leaf name="encapsulation-type">
      <type name="l2vpn-vpls-encap-type"/>
      <must condition=".='vlan' or not(../etree)"/>
      <default value="vlan"/>
      <description>
        <text>Encapsulation type of a VSI.</text>
      </description>
    </leaf>
    <leaf name="mtu">
      <type name="uint16">
        <range value="46..65535"/>
      </type>
      <default value="1500"/>
      <description>
        <text>VPLS MTU.</text>
      </description>
    </leaf>
    <leaf name="tunnel-policy">
      <type name="string">
        <length value="1..39"/>
      </type>
      <description>
        <text>Name of a tunnel policy for a VSI. If the tunnel policy name is not specified, the system uses the default tunnel policy. The default tunnel policy preferentially selects LSPs for packet transmission and the number of load-balancing tunnels is 1. If the tunnel policy name is specified but the corresponding tunnel policy does not exist, the system still uses the default tunnel policy.</text>
      </description>
    </leaf>
    <leaf name="shutdown">
      <type name="boolean"/>
      <default value="false"/>
      <description>
        <text>Enable/disable the function of shutting down a specified VSI.</text>
      </description>
    </leaf>
    <leaf name="isolate-spoken">
      <when condition="../work-mode='normal'"/>
      <type name="boolean"/>
      <must condition=".='false' or not(../etree)"/>
      <default value="false"/>
      <description>
        <text>Enable/disable forwarding isolation between AC interfaces, between UPE PWs, and between ACs and UPE PWs in a VSI.</text>
      </description>
    </leaf>
    <leaf name="isolate-spoken-include-multicast">
      <when condition="../isolate-spoken='true'"/>
      <type name="boolean"/>
      <must condition=".='false' or not(../etree)"/>
      <default value="false"/>
      <description>
        <text>Enable/disable isolating forwarding between AC interfaces, between UPE PWs, and between ACs and UPE PWs in a VSI.</text>
      </description>
    </leaf>
    <leaf name="ignore-ac-state">
      <type name="boolean"/>
      <default value="false"/>
      <description>
        <text>Enable/disable the function for the VSI status not to be affected by changes in the AC status.</text>
      </description>
    </leaf>
    <leaf name="response-ac-state">
      <when condition="../work-mode='bd-mode'"/>
      <type name="boolean"/>
      <default value="false"/>
      <description>
        <text>Enable/disable the function for the BD VSI status to be affected by changes in the BD AC status.</text>
      </description>
    </leaf>
    <leaf name="admin-vsi">
      <type name="boolean"/>
      <must condition="(.='false') or (.='true' and ../isolate-spoken='false' and not(../etree))"/>
      <default value="false"/>
      <description>
        <text>Enable/disable the function for the current service VSI to be configured as the mVSI.</text>
      </description>
    </leaf>
    <leaf name="track-admin-vsi">
      <ext:operation-exclude value="update|delete"/>
      <when condition="../work-mode='normal' or ../work-mode='b-vsi'"/>
      <type name="string">
        <length value="1..31"/>
      </type>
      <must condition="(../admin-vsi='false' and /l2vpn:l2vpn/l2vpn:instances/l2vpn:instance[l2vpn:name=current()][l2vpn:type='vpls']/l2vpn:vpls/l2vpn:admin-vsi='true')"/>
      <description>
        <text>Name of the mVSI to which a service VSI is bound.</text>
      </description>
    </leaf>
    <leaf name="unknown-unicast-action">
      <when condition="not(../work-mode='bd-mode')"/>
      <type name="l2vpn-vpls-unknow-frame-act"/>
      <default value="broadcast"/>
      <description>
        <text>Action taken to process unknown unicast frames.</text>
      </description>
    </leaf>
    <leaf name="unknown-multicast-action">
      <when condition="not(../work-mode='bd-mode')"/>
      <type name="l2vpn-vpls-unknow-frame-act"/>
      <must condition="((.='broadcast' or .='drop'))"/>
      <default value="broadcast"/>
      <description>
        <text>Action taken to process unknown multicast frames.</text>
      </description>
    </leaf>
    <leaf name="isolate-hub-ac-pw">
      <when condition="../work-mode='normal'"/>
      <type name="boolean"/>
      <must condition=".='false' or not(../etree)"/>
      <default value="false"/>
      <description>
        <text>Enable/disable forwarding isolation between hub ACs and PWs in a VSI.</text>
      </description>
    </leaf>
    <leaf name="p2p-vsi">
      <when condition="../work-mode='normal'"/>
      <type name="boolean"/>
      <must condition=".='false' or (.='true' and ../admin-vsi='false' and ../mac-learn='false' and ../mac-learn-style='unqualify' and not(../etree))"/>
      <default value="false"/>
      <description>
        <text>Enable/disable the P2P VSI.</text>
      </description>
    </leaf>
    <leaf name="traffic-statistics-collection">
      <when condition="../work-mode='normal' or ../work-mode='bd-mode'"/>
      <type name="boolean"/>
      <default value="false"/>
      <description>
        <text>Enable/disable VSI QoS traffic statistics collection.</text>
      </description>
    </leaf>
    <leaf name="mac-remove-mode">
      <type name="mac-remove-type"/>
      <must condition="((not(.='all-from-me')) or ((.='all-from-me') and (not(../work-mode='bd-mode')) and (not(../work-mode='b-vsi')) and (not(../work-mode='i-vsi')) and (not(../work-mode='i-vsi-p2p')) and (not(../work-mode='i-vsi-b-evpn-interworking'))))"/>
      <default value="invalid"/>
      <description>
        <text>Local MAC remove mode.</text>
      </description>
    </leaf>
    <leaf name="mac-learn">
      <when condition="not(../work-mode='bd-mode')"/>
      <type name="boolean"/>
      <must condition=".='false' or (.='true' and ../p2p-vsi='false')"/>
      <default value="true"/>
      <description>
        <text>Enable/disable the function for a VSI to learn MAC addresses.</text>
      </description>
    </leaf>
    <leaf name="mac-learn-style">
      <when condition="../work-mode='normal'"/>
      <type name="l2vpn-mac-learn-style"/>
      <must condition=".='unqualify' or not(../etree)"/>
      <default value="unqualify"/>
      <description>
        <text>MAC address learning style of PWs.</text>
      </description>
    </leaf>
    <leaf name="mac-age-time">
      <when condition="not(../work-mode='bd-mode')"/>
      <type name="uint32">
        <range value="0 | 60..1000000"/>
      </type>
      <units name="s"/>
      <description>
        <text>MAC address aging timer that sets the aging time of MAC addresses.</text>
      </description>
    </leaf>
    <leaf name="local-mac-age-time">
      <when condition="../work-mode='normal'"/>
      <type name="uint32">
        <range value="0 | 60..1000000"/>
      </type>
      <units name="s"/>
      <description>
        <text>Local MAC address aging timer that sets the aging time of MAC addresses.</text>
      </description>
    </leaf>
    <leaf name="remote-mac-age-time">
      <when condition="../work-mode='normal'"/>
      <type name="uint32">
        <range value="0 | 60..1000000"/>
      </type>
      <units name="s"/>
      <description>
        <text>Remote MAC address aging timer that sets the aging time of MAC addresses.</text>
      </description>
    </leaf>
    <leaf name="mac-withdraw">
      <type name="boolean"/>
      <default value="false"/>
      <description>
        <text>Enable/disable the function for a VSI to delete the local MAC address and notifies all remote peers of the deletion when an AC or a UPE fault occurs and the VSI remains up.</text>
      </description>
    </leaf>
    <leaf name="interface-change-mac-withdraw">
      <when condition="not(../work-mode='i-vsi-b-evpn-interworking')"/>
      <type name="boolean"/>
      <default value="false"/>
      <description>
        <text>Enable/disable the function for PEs to send LDP MAC withdraw messages to all peers when the status of the AC interface bound to the VSI changes.</text>
      </description>
    </leaf>
    <leaf name="interface-change-mac-withdraw-type">
      <type name="interface-change-withdraw-type"/>
      <must condition=".='all' or ../interface-change-mac-withdraw='true'"/>
      <default value="all"/>
      <description>
        <text>Specify the status change type for a VSI AC interface.</text>
      </description>
    </leaf>
    <leaf name="bd-pwtag-mac-withdraw">
      <when condition="../work-mode='bd-mode'"/>
      <type name="boolean"/>
      <default value="false"/>
      <description>
        <text>Enable/disable the function for LDP MAC withdraw messages to carry PW tag corresponding to the VSI bound to the bridge domain. This node can be configured only when the VSI works in BD mode.</text>
      </description>
    </leaf>
    <leaf name="bd-down-mac-withdraw">
      <when condition="../work-mode='bd-mode'"/>
      <type name="boolean"/>
      <default value="true"/>
      <description>
        <text>Enable/disable the function for PEs to send LDP MAC withdraw messages to all peers when BD goes down. This node can be configured only when the VSI works in BD mode.</text>
      </description>
    </leaf>
    <leaf name="propagate-mac-withdraw">
      <when condition="not(../work-mode='i-vsi-b-evpn-interworking')"/>
      <type name="boolean"/>
      <default value="false"/>
      <description>
        <text>Enable/disable the function for a PE to propagate LDP MAC withdraw messages to other peers after receiving the message from a remote peer.</text>
      </description>
    </leaf>
    <leaf name="mac-flush-rfc-compatible">
      <when condition="../work-mode='normal' or ../work-mode='bd-mode'"/>
      <type name="boolean"/>
      <default value="false"/>
      <description>
        <text>Enable/disable the transmission of standard MAC-flush message when the PW status changes.</text>
      </description>
    </leaf>
    <leaf name="vrrp-mac-withdraw-retry">
      <type name="uint16">
        <range value="0..60"/>
      </type>
      <default value="0"/>
      <description>
        <text>VRRP backup Mac withdraw retry.</text>
      </description>
    </leaf>
    <leaf name="token-exceed">
      <type name="boolean"/>
      <default value="false"/>
      <config value="false"/>
      <description>
        <text>Indicates whether the number of VSI tokens exceeds the maximum limit.</text>
      </description>
    </leaf>
    <leaf name="ac-count">
      <type name="uint32"/>
      <config value="false"/>
      <description>
        <text>Total number of ACs in a VSI.</text>
      </description>
    </leaf>
    <leaf name="create-time">
      <type name="string">
        <length value="1..60"/>
      </type>
      <config value="false"/>
      <description>
        <text>Time when a VSI was created.</text>
      </description>
    </leaf>
    <leaf name="ignore-ac-state-effect">
      <type name="boolean"/>
      <default value="false"/>
      <config value="false"/>
      <description>
        <text>Configure the VSI status not to be affected by changes in the AC status. By default, the VSI status changes according to the AC status.</text>
      </description>
    </leaf>
    <container name="etree">
      <when condition="../work-mode='normal'"/>
      <must condition="(root-vlan and leaf-vlan) or ((not(root-vlan)) and (not(leaf-vlan)))"/>
      <presence value="Enable ethernet tree."/>
      <description>
        <text>Enable/disable vpls ethernet tree.</text>
      </description>
      <leaf name="static">
        <type name="boolean"/>
        <must condition="../static='true'"/>
        <default value="true"/>
        <description>
          <text>Enable/disable static mode.</text>
        </description>
      </leaf>
      <leaf name="root-vlan">
        <type name="uint16">
          <range value="1..4094"/>
        </type>
        <description>
          <text>Specify root VLAN id.</text>
        </description>
      </leaf>
      <leaf name="leaf-vlan">
        <type name="uint16">
          <range value="1..4094"/>
        </type>
        <description>
          <text>Specify leaf VLAN id.</text>
        </description>
      </leaf>
    </container>
    <container name="ldp-signaling">
      <presence value="Label Distribution Protocol (LDP) signaling."/>
      <description>
        <text>Enable/disable Label Distribution Protocol (LDP) signaling.</text>
      </description>
      <leaf name="vsi-id">
        <ext:operation-exclude value="update"/>
        <type name="uint64">
          <range value="1..4294967295"/>
        </type>
        <must condition="count(/l2vpn:l2vpn/l2vpn:instances/l2vpn:instance/l2vpn:vpls/l2vpn:ldp-signaling/l2vpn:vsi-id[.=current()])&lt;=1"/>
        <must condition="count(/l2vpn:l2vpn/l2vpn:instances/l2vpn:instance/l2vpn:vpws-ldp/l2vpn:pws/l2vpn:pw/l2vpn:pw-id[.=current()])=0"/>
        <must condition="count(/l2vpn:l2vpn/l2vpn:instances/l2vpn:instance/l2vpn:vpws-switch/l2vpn:pws/l2vpn:pw/l2vpn:pw-id[.=current()])=0"/>
        <description>
          <text>VPLS LDP VSI ID. Can not be modified after configuration.</text>
        </description>
      </leaf>
      <leaf name="traffic-statistics-collection">
        <when condition="../vsi-id"/>
        <type name="boolean"/>
        <must condition=". = 'false' or not(../../traffic-statistics-collection='true')"/>
        <default value="false"/>
        <description>
          <text>Enable/disable traffic statistics collection.</text>
        </description>
      </leaf>
      <leaf name="control-word">
        <type name="l2vpn-control-word"/>
        <must condition=".='disable' or .='enable'"/>
        <default value="disable"/>
        <description>
          <text>Enable the control word feature. The control word feature is usually used for TDM, ATM, and FR PWs.</text>
        </description>
      </leaf>
      <leaf name="hard-pipe">
        <type name="boolean"/>
        <must condition=".='false' or (../../work-mode='normal' and not(../../bgp-signaling) and not(../../bgpad-signaling) and ../../p2p-vsi='false')"/>
        <default value="false"/>
        <description>
          <text>Enable/disable hard pipe.</text>
        </description>
      </leaf>
      <leaf name="mac-withdraw">
        <type name="boolean"/>
        <must condition=".='false' or (../../mac-withdraw='false' and ../../propagate-mac-withdraw='false' and ../../interface-change-mac-withdraw='false')"/>
        <default value="false"/>
        <description>
          <text>Enable/disable the function for a VSI to delete the local MAC address and notify all remote peers of the deletion when an AC or a UPE fault occurs and the VSI remains up.</text>
        </description>
      </leaf>
      <leaf name="interface-change-mac-withdraw">
        <type name="boolean"/>
        <must condition=".='false' or  (.='true' and ../../mac-withdraw='false' and ../../propagate-mac-withdraw='false' and ../../interface-change-mac-withdraw='false' and ../../work-mode!='i-vsi-b-evpn-interworking')"/>
        <default value="false"/>
        <description>
          <text>Enable/disable the function for PEs to send LDP MAC withdraw messages to all peers when the status of the AC interface bound to the VSI changes.</text>
        </description>
      </leaf>
      <leaf name="interface-change-mac-withdraw-type">
        <type name="interface-change-withdraw-type"/>
        <must condition=".='all' or ../interface-change-mac-withdraw='true'"/>
        <default value="all"/>
        <description>
          <text>Specify the status change type for a VSI AC interface.</text>
        </description>
      </leaf>
      <leaf name="upe-upe-mac-withdraw">
        <type name="boolean"/>
        <must condition=".='false' or (../../mac-withdraw='false' and ../../propagate-mac-withdraw='false' and ../../interface-change-mac-withdraw='false' and ../../work-mode!='i-vsi')"/>
        <default value="false"/>
        <description>
          <text>Enable/disable the function for an NPE to forward the LDP MAC withdraw messages received from a UPE to other UPEs.</text>
        </description>
      </leaf>
      <leaf name="upe-npe-mac-withdraw">
        <type name="boolean"/>
        <must condition=".='false' or (../../mac-withdraw='false' and ../../propagate-mac-withdraw='false' and ../../interface-change-mac-withdraw='false' and ../../work-mode!='i-vsi')"/>
        <default value="false"/>
        <description>
          <text>Enable/disable the function for an NPE to forward the LDP MAC withdraw messages received from a UPE to other NPEs.</text>
        </description>
      </leaf>
      <leaf name="npe-upe-mac-withdraw">
        <type name="boolean"/>
        <must condition=".='false' or (../../mac-withdraw='false' and ../../propagate-mac-withdraw='false' and ../../interface-change-mac-withdraw='false' and ../../work-mode!='i-vsi')"/>
        <default value="false"/>
        <description>
          <text>Enable/disable the function for an NPE to forward the LDP MAC withdraw messages received from another NPE to UPEs.</text>
        </description>
      </leaf>
      <container name="flow-label">
        <presence value="enable flow-label for ldp signaling pws."/>
        <description>
          <text>Enable/disable flow label capability of a PW.</text>
        </description>
        <leaf name="ability">
          <type name="flow-label-ability"/>
          <must condition=".!='default' and .!='disable'"/>
          <mandatory value="true"/>
          <description>
            <text>Enable/disable flow label capability.</text>
          </description>
        </leaf>
        <leaf name="static">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable the static flow label function.</text>
          </description>
        </leaf>
      </container>
      <container name="pws">
        <when condition="../vsi-id"/>
        <description>
          <text>List of VPLS PWs.</text>
        </description>
        <list name="pw">
          <key value="peer-ip negotiation-vc-id encapsulation-type"/>
          <description>
            <text>Configure VPLS PWs.</text>
          </description>
          <leaf name="peer-ip">
            <type name="inet:ipv4-address-no-zone"/>
            <description>
              <text>Peer IP address. The loopback IP address of a peer is usually used as the peer IP address.</text>
            </description>
          </leaf>
          <leaf name="negotiation-vc-id">
            <type name="uint32">
              <range value="1..4294967295"/>
            </type>
            <description>
              <text>PW ID used to uniquely identify a VC. The PW ID is usually used when both ends need to communicate but the VSI IDs on both ends are different.</text>
            </description>
          </leaf>
          <leaf name="encapsulation-type">
            <type name="l2vpn-vpls-ldp-pw-encap-type"/>
            <description>
              <text>Encapsulation type of a VPLS PW. 'unsupport' indicates that the encapsulation type of a VSI is inherited.</text>
            </description>
          </leaf>
          <leaf name="role">
            <ext:operation-exclude value="update|delete"/>
            <type name="vpls-pw-role-type"/>
            <must condition="../role='primary' or (../role='backup' and  ../../pw[role='primary'])"/>
            <default value="primary"/>
            <description>
              <text>Role of a PW, either primary or secondary. Configure a primary PW first.</text>
            </description>
          </leaf>
          <leaf name="name">
            <ext:operation-exclude value="update"/>
            <type name="string">
              <length value="1..15"/>
            </type>
            <description>
              <text>PW name.</text>
            </description>
          </leaf>
          <leaf name="signal-type">
            <ext:operation-exclude value="update|delete"/>
            <type name="pw-signal-type"/>
            <must condition="(../signal-type='ldp') or ../role!='backup'"/>
            <must condition="(../signal-type='static' and ../tunnel-policy) or ../../../hard-pipe='false'"/>
            <must condition="(../signal-type='static' and ../transmit-label and ../receive-label) or ../signal-type='ldp'"/>
            <must condition="(.='static' and ../ignore-standby='false') or .!='static'"/>
            <default value="ldp"/>
            <description>
              <text>PW type, either dynamic or static.</text>
            </description>
          </leaf>
          <leaf name="transmit-label">
            <ext:operation-exclude value="update|delete"/>
            <when condition="../signal-type='static'"/>
            <type name="uint32">
              <range value="0..1048575"/>
            </type>
            <description>
              <text>Label for sending packets.</text>
            </description>
          </leaf>
          <leaf name="receive-label">
            <ext:operation-exclude value="update|delete"/>
            <when condition="../signal-type='static'"/>
            <type name="uint32">
              <range value="16..1048575"/>
            </type>
            <description>
              <text>Label for receiving packets.</text>
            </description>
          </leaf>
          <leaf name="control-word">
            <when condition="../name"/>
            <type name="l2vpn-control-word"/>
            <default value="default"/>
            <description>
              <text>Enable the control word function.</text>
            </description>
          </leaf>
          <leaf name="vccv">
            <when condition="../name"/>
            <type name="boolean"/>
            <default value="true"/>
            <description>
              <text>Enable/disable virtual circuit connectivity verification capability. VCCV supports two modes: control word and label alert.</text>
            </description>
          </leaf>
          <leaf name="ignore-standby">
            <ext:operation-exclude value="update|delete"/>
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable the function for the created PW to ignore the standby state sent from the peer device.</text>
            </description>
          </leaf>
          <leaf name="upe">
            <ext:operation-exclude value="update|delete"/>
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable the peer device as a UPE.</text>
            </description>
          </leaf>
          <leaf name="tunnel-policy">
            <type name="string">
              <length value="1..39"/>
            </type>
            <description>
              <text>Name of the tunnel policy for a PW.</text>
            </description>
          </leaf>
          <leaf name="traffic-statistics-collection">
            <type name="l2vpn-vpls-traffic-statistics-enable-type"/>
            <must condition=". = 'default' or not(../../../../traffic-statistics-collection = 'true')"/>
            <default value="default"/>
            <description>
              <text>Enable the function to collect traffic statistics.</text>
            </description>
          </leaf>
          <leaf name="track-hub-pw">
            <when condition="../name"/>
            <type name="boolean"/>
            <must condition="../track-hub-pw='false' or (../track-hub-pw='true' and ../upe='true' and ../../../../work-mode!='i-vsi-b-evpn-interworking')"/>
            <default value="false"/>
            <description>
              <text>Enable/disable the function for the spoke PW to track the hub PW. If a PW is not a spoke PW, it cannot track the hub PW.</text>
            </description>
          </leaf>
          <leaf name="ignore-stp-loop-check">
            <when condition="../name"/>
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable the function of ignoring STP loops.</text>
            </description>
          </leaf>
          <leaf name="hard-pipe-bandwidth">
            <ext:operation-exclude value="delete"/>
            <when condition="../../../hard-pipe='true'"/>
            <type name="uint32">
              <range value="16..4294967294"/>
            </type>
            <units name="kbit/s"/>
            <description>
              <text>Specify bandwidth value.</text>
            </description>
          </leaf>
          <leaf name="hard-pipe-burst-time">
            <ext:operation-exclude value="delete"/>
            <when condition="../hard-pipe-bandwidth"/>
            <type name="uint32">
              <range value="100..5000"/>
            </type>
            <units name="us"/>
            <default value="100"/>
            <description>
              <text>Burst time.</text>
            </description>
          </leaf>
          <leaf name="status-code-change-flush-mac-disable">
            <when condition="../name"/>
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable the function of flushing MAC when status code change.</text>
            </description>
          </leaf>
          <leaf name="ac-mode">
            <ext:operation-exclude value="update|delete"/>
            <when condition="../../../../work-mode='bd-mode'"/>
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable the AC mode.</text>
            </description>
          </leaf>
          <leaf name="etree-mode">
            <when condition="../../../../etree and ../name"/>
            <type name="etree-work-mode"/>
            <default value="default"/>
            <description>
              <text>Specify e-tree work mode.</text>
            </description>
          </leaf>
          <leaf name="etree-leaf">
            <when condition="../etree-mode = 'compatible'"/>
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable e-tree leaf.</text>
            </description>
          </leaf>
          <container name="flow-label">
            <when condition="../name"/>
            <presence value="enable flow label"/>
            <description>
              <text>Enable/disable the flow label capability of a PW.</text>
            </description>
            <leaf name="ability">
              <type name="flow-label-ability"/>
              <must condition=".!='default'"/>
              <mandatory value="true"/>
              <description>
                <text>Flow label capability.</text>
              </description>
            </leaf>
            <leaf name="static">
              <type name="boolean"/>
              <must condition=".='false' or ../ability!='disable'"/>
              <default value="false"/>
              <description>
                <text>Enable/disable the static flow label function.</text>
              </description>
            </leaf>
          </container>
          <container name="bfd">
            <when condition="../name and /bfd:bfd"/>
            <presence value="enable vpls ldp pw bfd."/>
            <description>
              <text>Enable/disable BFD.</text>
            </description>
            <leaf name="min-rx-interval">
              <type name="uint16">
                <range value="0..65535"/>
              </type>
              <units name="ms"/>
              <default value="10"/>
              <description>
                <text>Minimum receive interval.</text>
              </description>
            </leaf>
            <leaf name="min-tx-interval">
              <type name="uint16">
                <range value="0..65535"/>
              </type>
              <units name="ms"/>
              <default value="10"/>
              <description>
                <text>Minimum transmit interval.</text>
              </description>
            </leaf>
            <leaf name="detect-multiplier">
              <type name="uint16">
                <range value="3..50"/>
              </type>
              <default value="3"/>
              <description>
                <text>Detection multiplier.</text>
              </description>
            </leaf>
            <leaf name="option-tlv">
              <type name="boolean"/>
              <default value="false"/>
              <description>
                <text>Enable/disable BFD detection with options.</text>
              </description>
            </leaf>
          </container>
          <container name="pw-info">
            <config value="false"/>
            <description>
              <text>Operational data of VPLS LDP PW information.</text>
            </description>
            <leaf name="peer-ip">
              <type name="inet:ipv4-address-no-zone"/>
              <description>
                <text>Peer IP address.</text>
              </description>
            </leaf>
            <leaf name="negotiation-vc-id">
              <type name="uint32"/>
              <description>
                <text>PW ID used to uniquely identify a VC. The PW ID is usually used when both ends need to communicate but the VSI IDs on both ends are different.</text>
              </description>
            </leaf>
            <leaf name="pw-type">
              <type name="l2vpn-vpls-pw-type"/>
              <description>
                <text>PW type, which can be label, QinQ, or MEHVPLS.</text>
              </description>
            </leaf>
            <leaf name="session-state">
              <type name="l2vpn-state"/>
              <description>
                <text>Status of the session between the local end and its peer. The value is either up or down. This item is displayed only when the VPLS type is Martini.</text>
              </description>
            </leaf>
            <leaf name="pw-state">
              <type name="l2vpn-pw-state"/>
              <description>
                <text>Current PW status.</text>
              </description>
            </leaf>
            <leaf name="local-vc-label">
              <type name="uint32"/>
              <description>
                <text>VC label allocated by the local device.</text>
              </description>
            </leaf>
            <leaf name="remote-vc-label">
              <type name="uint32"/>
              <description>
                <text>VC label allocated by the peer.</text>
              </description>
            </leaf>
            <leaf name="remote-control-word">
              <type name="l2vpn-control-word"/>
              <description>
                <text>Enable the control word function on the remote end.</text>
              </description>
            </leaf>
            <leaf name="tunnel-policy">
              <type name="string">
                <length value="1..39"/>
              </type>
              <description>
                <text>Name of the tunnel policy for a PW.</text>
              </description>
            </leaf>
            <leaf name="last-up-time">
              <type name="string">
                <length value="1..80"/>
              </type>
              <description>
                <text>Date and time when a PW was up for the last time, for example, 2012/07/20 02:30:46.</text>
              </description>
            </leaf>
            <leaf name="total-up-time">
              <type name="string">
                <length value="1..80"/>
              </type>
              <description>
                <text>Total up time of the PW.</text>
              </description>
            </leaf>
            <leaf name="c-key">
              <type name="uint32"/>
              <description>
                <text>Customer key. Each CKey corresponds to a PW.</text>
              </description>
            </leaf>
            <leaf name="n-key">
              <type name="uint32"/>
              <description>
                <text>Network key. Each NKey corresponds to a combination of the peer and tunnel policy.</text>
              </description>
            </leaf>
            <leaf name="spe-count">
              <type name="uint32">
                <range value="0..4294967295"/>
              </type>
              <description>
                <text>Number of SPEs in the path to the peer.</text>
              </description>
            </leaf>
            <leaf name="stp-enable">
              <type name="boolean"/>
              <description>
                <text>Enable STP for PW.</text>
              </description>
            </leaf>
            <leaf name="mac-flapping-state">
              <type name="pw-block-status"/>
              <description>
                <text>MAC flapping information.</text>
              </description>
            </leaf>
            <leaf name="pw-index">
              <type name="uint32"/>
              <description>
                <text>PW index.</text>
              </description>
            </leaf>
            <leaf name="remote-group-id">
              <type name="uint32"/>
              <description>
                <text>ID of a remote PW group.</text>
              </description>
            </leaf>
            <leaf name="remote-mtu">
              <type name="uint32"/>
              <description>
                <text>MTU of the remote VSI.</text>
              </description>
            </leaf>
            <leaf name="remote-state-code">
              <type name="l2vpn-vpls-ldp-pw-state-code"/>
              <description>
                <text>Status code information on the remote end.</text>
              </description>
            </leaf>
            <leaf name="received-flow-label">
              <type name="flow-label-ability"/>
              <description>
                <text>Received flow label.</text>
              </description>
            </leaf>
            <leaf name="negotiated-flow-label">
              <type name="flow-label-ability"/>
              <description>
                <text>Negotiated flow label.</text>
              </description>
            </leaf>
            <leaf name="remote-vccv-code">
              <type name="pub-type:hex-binary"/>
              <description>
                <text>Remote VCCV code.</text>
              </description>
            </leaf>
            <leaf name="bfd-enable">
              <type name="boolean"/>
              <description>
                <text>Enable BFD.</text>
              </description>
            </leaf>
            <leaf name="bfd-state">
              <type name="l2vpn-vpls-pw-bfd-state"/>
              <description>
                <text>BFD status.</text>
              </description>
            </leaf>
            <leaf name="bfd-session-index">
              <when condition="../dynamic-bfd-session='false'"/>
              <type name="uint32"/>
              <description>
                <text>BFD index.</text>
              </description>
            </leaf>
            <leaf name="dynamic-bfd-session">
              <type name="boolean"/>
              <description>
                <text>Enable dynamic BFD.</text>
              </description>
            </leaf>
            <leaf name="bfd-for-pw">
              <when condition="../dynamic-bfd-session='true'"/>
              <type name="boolean"/>
              <description>
                <text>Dynamic BFD session status.</text>
              </description>
            </leaf>
            <leaf name="block-status">
              <type name="pw-block-status"/>
              <description>
                <text>Specifies whether a PW is blocked. If the PW is blocked, it cannot receive or transmit services.</text>
              </description>
            </leaf>
            <leaf name="pw-token-exceed">
              <type name="boolean"/>
              <default value="false"/>
              <description>
                <text>Indicates whether the number of PW tokens exceeds the maximum limit.</text>
              </description>
            </leaf>
            <container name="tunnels">
              <config value="false"/>
              <description>
                <text>List of VPLS LDP PW tunnels.</text>
              </description>
              <list name="tunnel">
                <key value="tunnel-key"/>
                <max-elements value="32"/>
                <description>
                  <text>Operational data of VPLS LDP PW tunnels.</text>
                </description>
                <leaf name="tunnel-key">
                  <type name="string">
                    <length value="0..21"/>
                  </type>
                  <description>
                    <text>Tunnel ID in hexadecimal notation that is assigned by the system.</text>
                  </description>
                </leaf>
                <leaf name="tunnel-type">
                  <type name="l2vpn-tunnel-type"/>
                  <description>
                    <text>Type of a tunnel.</text>
                  </description>
                </leaf>
                <leaf name="out-interface">
                  <type name="string">
                    <length value="0..257"/>
                  </type>
                  <description>
                    <text>Outbound interface of a PW.</text>
                  </description>
                </leaf>
                <leaf name="tunnel-name">
                  <type name="string">
                    <length value="0..64"/>
                  </type>
                  <description>
                    <text>Name of the tunnel used by the PW.</text>
                  </description>
                </leaf>
                <leaf name="public-next-hop">
                  <type name="inet:ipv4-address-no-zone"/>
                  <description>
                    <text>Public next hop of a PW.</text>
                  </description>
                </leaf>
              </list>
            </container>
            <container name="spes">
              <description>
                <text>Operational data of SPEs on the path to the peer.</text>
              </description>
              <uses name="l2vpn:spe-infos-type"/>
            </container>
            <container name="label-stacks">
              <description>
                <text>Operational data of PW label stacks.</text>
              </description>
              <uses name="l2vpn:l2vpn-label-stacks-type"/>
            </container>
            <container name="down-causes">
              <description>
                <text>Operational data of the down causes.</text>
              </description>
              <uses name="l2vpn:l2vpn-pw-down-causes-type"/>
            </container>
            <container name="status-changes">
              <description>
                <text>Operational data of the reason and time.</text>
              </description>
              <uses name="l2vpn:l2vpn-pw-status-changes-type"/>
            </container>
          </container>
          <uses name="l2vpn:suppression-statisticss-grp"/>
          <uses name="l2vpn:pw-traffic-statisticss-grp"/>
          <container name="hard-pipe-traffic-statistics">
            <config value="false"/>
            <description>
              <text>Operational data of traffic statistics about VPLS LDP PW HardPipe.</text>
            </description>
            <leaf name="last-clear-time">
              <type name="string">
                <length value="1..60"/>
              </type>
              <description>
                <text>Time when statistics are cleared for the last time.</text>
              </description>
            </leaf>
            <leaf name="pass-packets-rate">
              <type name="uint64"/>
              <description>
                <text>The last 300 seconds pass packets rate.</text>
              </description>
            </leaf>
            <leaf name="pass-bytes-rate">
              <type name="uint64"/>
              <description>
                <text>The last 300 seconds pass bytes rate.</text>
              </description>
            </leaf>
            <leaf name="discard-packets-rate">
              <type name="uint64"/>
              <description>
                <text>The last 300 seconds discard packets rate.</text>
              </description>
            </leaf>
            <leaf name="discard-bytes-rate">
              <type name="uint64"/>
              <description>
                <text>The last 300 seconds discard bytes rate.</text>
              </description>
            </leaf>
            <leaf name="pass-packets">
              <type name="uint64"/>
              <description>
                <text>Number of pass packets in the last 300 seconds.</text>
              </description>
            </leaf>
            <leaf name="pass-bytes">
              <type name="uint64"/>
              <description>
                <text>Number of pass bytes in the last 300 seconds.</text>
              </description>
            </leaf>
            <leaf name="drop-packets">
              <type name="uint64"/>
              <description>
                <text>Number of discard packets in the last 300 seconds.</text>
              </description>
            </leaf>
            <leaf name="drop-bytes">
              <type name="uint64"/>
              <description>
                <text>Number of discard bytes in the last 300 seconds.</text>
              </description>
            </leaf>
          </container>
        </list>
      </container>
      <container name="redundancy-protect-groups">
        <when condition="../vsi-id"/>
        <description>
          <text>List of VPLS PW protection groups.</text>
        </description>
        <list name="redundancy-protect-group">
          <key value="name"/>
          <max-elements value="32"/>
          <description>
            <text>Configure VPLS PW protection groups.</text>
          </description>
          <leaf name="name">
            <type name="l2vpn-protect-group-name"/>
            <description>
              <text>Name of a PW protection group. The name is a string of case-sensitive characters.</text>
            </description>
          </leaf>
          <leaf name="protect-mode">
            <type name="vpls-redundancy-mode"/>
            <default value="none"/>
            <description>
              <text>Switching mode of the PW protection group.</text>
            </description>
          </leaf>
          <leaf name="dual-receive">
            <when condition="../protect-mode!='none'"/>
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable the receipt of both the primary and secondary PWs.</text>
            </description>
          </leaf>
          <leaf name="holdoff">
            <when condition="../protect-mode='master'"/>
            <type name="uint16">
              <range value="1..180"/>
            </type>
            <units name="s"/>
            <description>
              <text>Hold time for traffic switching.</text>
            </description>
          </leaf>
          <container name="reroute-policy">
            <when condition="../protect-mode='master'"/>
            <description>
              <text>Configure a switchback policy.</text>
            </description>
            <uses name="l2vpn:l2vpn-reroute-policy-type"/>
          </container>
          <container name="pw-members">
            <when condition="not(../protect-mode='none')"/>
            <description>
              <text>List of PW members in a protection group.</text>
            </description>
            <list name="pw-member">
              <must condition="../../../../pws/pw[peer-ip=current()/peer-ip][negotiation-vc-id=current()/negotiation-vc-id][encapsulation-type=current()/encapsulation-type]"/>
              <key value="peer-ip negotiation-vc-id encapsulation-type"/>
              <unique tag="preference"/>
              <max-elements value="2"/>
              <description>
                <text>Configure PW members in a protection group.</text>
              </description>
              <leaf name="peer-ip">
                <type name="inet:ipv4-address-no-zone"/>
                <description>
                  <text>Peer IP address. The loopback IP address of a peer is usually used as the peer IP address.</text>
                </description>
              </leaf>
              <leaf name="negotiation-vc-id">
                <type name="uint32">
                  <range value="1..4294967295"/>
                </type>
                <description>
                  <text>PW ID used to uniquely identify a VC. The PW ID is usually used when both ends need to communicate but the VSI IDs on both ends are different.</text>
                </description>
              </leaf>
              <leaf name="encapsulation-type">
                <type name="l2vpn-vpls-ldp-pw-encap-type"/>
                <description>
                  <text>Encapsulate type.</text>
                </description>
              </leaf>
              <leaf name="preference">
                <type name="uint32">
                  <range value="1..32"/>
                </type>
                <mandatory value="true"/>
                <description>
                  <text>Priority of a PW in the PW protection group.</text>
                </description>
              </leaf>
              <leaf name="role">
                <type name="l2vpn-backup-mode"/>
                <config value="false"/>
                <description>
                  <text>Primary/secondary status of a PW.</text>
                </description>
              </leaf>
              <leaf name="active-state">
                <type name="boolean"/>
                <default value="false"/>
                <config value="false"/>
                <description>
                  <text>Whether an interface is active.</text>
                </description>
              </leaf>
            </list>
          </container>
          <container name="historys">
            <config value="false"/>
            <description>
              <text>Operational data of historical switching information of PW protection groups.</text>
            </description>
            <uses name="l2vpn:l2vpn-protect-group-historys-type"/>
          </container>
        </list>
      </container>
      <container name="traffic-statistics">
        <config value="false"/>
        <description>
          <text>Operational data of traffic statistics.</text>
        </description>
        <leaf name="last-cleared">
          <type name="string">
            <length value="1..60"/>
          </type>
          <description>
            <text>Time when traffic statistics were cleared the last time.</text>
          </description>
        </leaf>
        <leaf name="in-packets-rate">
          <type name="uint64"/>
          <description>
            <text>Rate of packets received.</text>
          </description>
        </leaf>
        <leaf name="out-packets-rate">
          <type name="uint64"/>
          <description>
            <text>Rate of packets sent.</text>
          </description>
        </leaf>
        <leaf name="in-packets">
          <type name="uint64"/>
          <description>
            <text>Number of packets received.</text>
          </description>
        </leaf>
        <leaf name="out-packets">
          <type name="uint64"/>
          <description>
            <text>Number of packets sent.</text>
          </description>
        </leaf>
        <leaf name="in-unicast-packets">
          <type name="uint64"/>
          <description>
            <text>Number of unicast packets received.</text>
          </description>
        </leaf>
        <leaf name="in-multicast-packets">
          <type name="uint64"/>
          <description>
            <text>Number of multicast packets received.</text>
          </description>
        </leaf>
        <leaf name="in-drop-packets">
          <type name="uint64"/>
          <description>
            <text>Number of packets received and then discarded.</text>
          </description>
        </leaf>
        <leaf name="out-unicast-packets">
          <type name="uint64"/>
          <description>
            <text>Number of unicast packets sent.</text>
          </description>
        </leaf>
        <leaf name="out-multicast-packets">
          <type name="uint64"/>
          <description>
            <text>Number of multicast packets sent.</text>
          </description>
        </leaf>
      </container>
    </container>
    <container name="bgp-signaling">
      <when condition="../work-mode='normal' or ../work-mode='bd-mode'"/>
      <must condition="../admin-vsi!='true' and ../track-admin-vsi!='true' and ../p2p-vsi!='true' and (../mac-remove-mode='invalid' or (../mac-remove-mode!='invalid' and ../ldp-signaling))"/>
      <must condition="not(../etree)"/>
      <presence value="Container for bgp protocol vpls instance."/>
      <description>
        <text>Enable/disable BGP VSIs.</text>
      </description>
      <leaf name="route-distinguisher">
        <type name="string">
          <length value="3..21"/>
        </type>
        <description>
          <text>Route distinguisher.</text>
        </description>
      </leaf>
      <leaf name="encapsulation-rfc4761-compatible">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable the function of changing an encapsulation type.</text>
        </description>
      </leaf>
      <leaf name="encapsulation-vpls-family-compatible">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable the function of changing an encapsulation type to private mode.</text>
        </description>
      </leaf>
      <leaf name="mtu-negotiate-disable">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable the function of ignoring MTU match between local VSI and remote VSI. The default implementation is match.</text>
        </description>
      </leaf>
      <leaf name="control-word">
        <type name="l2vpn-control-word"/>
        <must condition="((../control-word='disable' or ../control-word='enable' ) )"/>
        <default value="disable"/>
        <description>
          <text>Enable the control word feature. The control word feature is usually used for TDM, ATM, and FR PWs.</text>
        </description>
      </leaf>
      <leaf name="auto-route-distinguisher">
        <type name="string">
          <length value="3..21"/>
        </type>
        <config value="false"/>
        <description>
          <text>Automatic route distinguisher (RD). If no RD is configured for an L2VPN instance, you can specify an IPv4 address using the rd-ip parameter, based on which an RD is automatically generated in the format of 32-bit prefix IP address:index, for example, 192.168.122.15:1. An index is an integer automatically allocated by the system and ranges from 1 to 65535.Each L2VPN instance must use a unique RD.</text>
        </description>
      </leaf>
      <container name="sites">
        <description>
          <text>List of BGP sites.</text>
        </description>
        <list name="site">
          <must condition="not(site-id=0 and default-offset=1)"/>
          <key value="site-id"/>
          <max-elements value="1"/>
          <description>
            <text>Configure BGP sites.</text>
          </description>
          <leaf name="site-id">
            <type name="uint32">
              <range value="0..65534"/>
            </type>
            <description>
              <text>BGP site ID.</text>
            </description>
          </leaf>
          <leaf name="range">
            <type name="uint32">
              <range value="1..65535"/>
            </type>
            <default value="10"/>
            <description>
              <text>BGP site range. Cannot decrease site range.</text>
            </description>
          </leaf>
          <leaf name="default-offset">
            <ext:operation-exclude value="update|delete"/>
            <type name="uint16">
              <range value="0..1"/>
            </type>
            <default value="0"/>
            <description>
              <text>The default offset of BGP site. The value of this node cannot be modified or deleted after being set.</text>
            </description>
          </leaf>
        </list>
      </container>
      <container name="vpn-targets">
        <description>
          <text>List of VPN targets.</text>
        </description>
        <list name="vpn-target">
          <key value="rt-value rt-type"/>
          <max-elements value="32"/>
          <description>
            <text>Configure VPN targets.</text>
          </description>
          <leaf name="rt-value">
            <type name="string">
              <length value="3..21"/>
            </type>
            <description>
              <text>Route target. The format of an RT can be:
- 16-bit AS number:32-bit user-defined number, for example, 1:3. An AS number ranges from 0 to 65535, and a user-defined number ranges from 0 to 4294967295. The AS number and user-defined number cannot be both 0s. This means that the RT cannot be 0:0.
- Integral 4-byte AS number:2-byte user-defined number, for example, 65537:3. An AS number ranges from 65536 to 4294967295. A user-defined number ranges from 0 to 65535. The AS number and user-defined number cannot be both 0s. This means that the RT cannot be 0:0.
- 32-bit IP address:16-bit user-defined number, for example: 192.168.122.15:1. The IP address ranges from 0.0.0.0 to 255.255.255.255, and the user-defined number ranges from 0 to 65535.
- 32-bit AS number:16-bit user-defined number, for example, 10.11:3. An AS number ranges from 0.0 to 65535.65535, and a user-defined number ranges from 0 to 65535. The AS number and user-defined number cannot be both 0s. This means that the RT cannot be 0.0:0.</text>
            </description>
          </leaf>
          <leaf name="rt-type">
            <type name="vpn-rt-type"/>
            <description>
              <text>RT type, which can be:
- export-extcommunity: specifies the value of the extended community attribute of the route from an outbound interface to the destination VPN.
- import-extcommunity: receives routes that carry the specified extended community attribute value.</text>
            </description>
          </leaf>
        </list>
      </container>
      <container name="flow-label">
        <presence value="enable flow-label for bgp signaling pws."/>
        <description>
          <text>Enable/disable flow label capability of a PW.</text>
        </description>
        <leaf name="ability">
          <type name="flow-label-ability"/>
          <must condition=".!='default' and .!='disable'"/>
          <mandatory value="true"/>
          <description>
            <text>Flow label capability.</text>
          </description>
        </leaf>
        <leaf name="static">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable the static flow label function.</text>
          </description>
        </leaf>
      </container>
      <container name="pws">
        <description>
          <text>List of VPLS BGP PWs.</text>
        </description>
        <list name="pw">
          <must condition="name or traffic-statistics-collection='true'"/>
          <key value="peer-ip remote-site-id"/>
          <unique tag="name"/>
          <description>
            <text>Configure VPLS BGP PWs.</text>
          </description>
          <leaf name="peer-ip">
            <type name="inet:ipv4-address-no-zone"/>
            <description>
              <text>Peer IP address.</text>
            </description>
          </leaf>
          <leaf name="remote-site-id">
            <type name="uint32">
              <range value="0..65534"/>
            </type>
            <description>
              <text>Remote BGP site ID.</text>
            </description>
          </leaf>
          <leaf name="name">
            <type name="string">
              <length value="1..15"/>
            </type>
            <description>
              <text>PW name.</text>
            </description>
          </leaf>
          <leaf name="traffic-statistics-collection">
            <type name="boolean"/>
            <must condition=". = 'false' or not(../../../../traffic-statistics-collection = 'true' or ../../../../ldp-signaling/traffic-statistics-collection = 'true' or ../../../../ldp-signaling/pws/pw[traffic-statistics-collection != 'default'])"/>
            <default value="false"/>
            <description>
              <text>Enable/disable the function to collect traffic statistics.</text>
            </description>
          </leaf>
          <uses name="l2vpn:suppression-statisticss-grp"/>
        </list>
      </container>
      <container name="local-label-blocks">
        <config value="false"/>
        <description>
          <text>Operational data of local label blocks.</text>
        </description>
        <uses name="bgp-local-label-block-grp"/>
      </container>
      <container name="remote-label-blocks">
        <config value="false"/>
        <description>
          <text>Operational data of remote label blocks.</text>
        </description>
        <uses name="bgp-remote-label-block-grp"/>
      </container>
      <container name="pw-infos">
        <config value="false"/>
        <description>
          <text>List of BGP PWs.</text>
        </description>
        <list name="pw-info">
          <key value="site-id offset"/>
          <description>
            <text>Operational data of the BGP PW list.</text>
          </description>
          <leaf name="site-id">
            <type name="uint32">
              <range value="0..65534"/>
            </type>
            <description>
              <text>BGP site ID.</text>
            </description>
          </leaf>
          <leaf name="offset">
            <type name="uint16"/>
            <description>
              <text>BGP site offset.</text>
            </description>
          </leaf>
          <leaf name="peer-ip">
            <type name="inet:ipv4-address-no-zone"/>
            <description>
              <text>BGP peer IP address.</text>
            </description>
          </leaf>
          <leaf name="pw-type">
            <type name="l2vpn-vpls-pw-type"/>
            <description>
              <text>PW type, which can be label, QinQ, or MEHVPLS.</text>
            </description>
          </leaf>
          <leaf name="pw-state">
            <type name="l2vpn-pw-state"/>
            <description>
              <text>PW status.</text>
            </description>
          </leaf>
          <leaf name="local-vc-label">
            <type name="uint32"/>
            <description>
              <text>VC label allocated by the local device.</text>
            </description>
          </leaf>
          <leaf name="remote-vc-label">
            <type name="uint32"/>
            <description>
              <text>VC label allocated by the peer.</text>
            </description>
