{
    "swagger": "2.0",
    "info": {
        "description": "huawei-dhcp-server API generated from huawei-dhcp-server.yang",
        "version": "1.0.0",
        "title": "huawei-dhcp-server API"
    },
    "host": "localhost:8080",
    "basePath": "/restconf",
    "schemes": [
        "http"
    ],
    "paths": {},
    "definitions": {
        "dhcp-server-type": {
            "description": "Configure DHCP server attributes.",
            "properties": {
                "common": {
                    "description": "Configure DHCP server common attributes.",
                    "properties": {
                        "ping-packet-nub": {
                            "type": "string",
                            "description": "Number of ping packets."
                        },
                        "ping-packet-timeout": {
                            "type": "string",
                            "description": "Timeout interval of ping reply."
                        },
                        "trust-option82": {
                            "type": "boolean",
                            "description": "Enable/disable the system to trust option82."
                        },
                        "bootp-enable": {
                            "type": "boolean",
                            "description": "Enable/disable BOOTP."
                        },
                        "bootp-auto-enable": {
                            "type": "boolean",
                            "description": "Enable/disable BOOTP automatic IP address assignment."
                        },
                        "database-enable": {
                            "type": "boolean",
                            "description": "Enable/disable database."
                        },
                        "database-recover": {
                            "type": "boolean",
                            "description": "Enable/disable database recover."
                        },
                        "write-delay-interval": {
                            "type": "string",
                            "description": "Write delay interval, in the range of 300 to 86400, in seconds."
                        },
                        "option60-enable": {
                            "type": "boolean",
                            "description": "Enable/disable to allocate IP address by option60."
                        },
                        "dest-ip-enable": {
                            "type": "boolean",
                            "description": "Enable/disable to use destionation IP address of the request packet as server identifier option."
                        },
                        "lease-items-path": {
                            "type": "string",
                            "description": "Lease items path."
                        },
                        "conflict-items-path": {
                            "type": "string",
                            "description": "Conflict items path."
                        },
                        "link-selection-enable": {
                            "type": "boolean",
                            "description": "Enable/disable DHCP server to assign IP addresses based on the link-selection value in option82."
                        },
                        "server-id-override-enable": {
                            "type": "boolean",
                            "description": "Enable/disable DHCP server to encapsulate option54 into packet based on the server-id-override value in option82."
                        },
                        "request-packet-all-interface-enable": {
                            "type": "boolean",
                            "description": "Enable/disable the function of DHCP server on all interfaces."
                        }
                    }
                },
                "global-ip-pools": {
                    "description": "List of global ip pools.",
                    "properties": {
                        "global-ip-pool": {
                            "description": "Configure a global ip pool.",
                            "type": "array",
                            "items": {
                                "properties": {
                                    "ip-pool-name": {
                                        "type": "string",
                                        "description": "Pool name."
                                    },
                                    "vpn-instance": {
                                        "type": "string",
                                        "x-path": "/ni:network-instance/ni:instances/ni:instance/ni:name",
                                        "description": "VPN instance."
                                    },
                                    "locked-flag": {
                                        "type": "boolean",
                                        "description": "Enable/disable to lock."
                                    },
                                    "option-router-disable": {
                                        "type": "boolean",
                                        "description": "Enable/disable reply router option."
                                    },
                                    "warning-exhaust-enable": {
                                        "type": "boolean",
                                        "description": "Enable/disable the warning-exhaust of the pool."
                                    },
                                    "warning-threshold": {
                                        "type": "string",
                                        "description": "The warning threshold of the pool."
                                    },
                                    "gateway": {
                                        "description": "Configure the gateway information of the pool.",
                                        "properties": {
                                            "ip-address": {
                                                "type": "string",
                                                "description": "The gateway address of the pool."
                                            },
                                            "mask": {
                                                "type": "string",
                                                "description": "The gateway mask of the pool."
                                            }
                                        }
                                    },
                                    "conflict-recycle": {
                                        "description": "Configure the conflict address auto recycle.",
                                        "properties": {
                                            "auto-recycle-enable": {
                                                "type": "boolean",
                                                "description": "Enable/disable auto recycle of the conflict address."
                                            },
                                            "conflict-recycle-interval": {
                                                "type": "string",
                                                "description": "The interval of the conflict address auto recycle."
                                            }
                                        }
                                    },
                                    "lease-time": {
                                        "description": "Configure lease time.",
                                        "properties": {
                                            "day": {
                                                "type": "string",
                                                "description": "The lease time day."
                                            },
                                            "hour": {
                                                "type": "string",
                                                "description": "The lease time hour."
                                            },
                                            "minute": {
                                                "type": "string",
                                                "description": "The lease time minute."
                                            },
                                            "unlimited": {
                                                "type": "boolean",
                                                "description": "Enable/disable to unlimit lease time."
                                            }
                                        }
                                    },
                                    "rebind-time": {
                                        "description": "Configure the rebind time of the pool.",
                                        "properties": {
                                            "day": {
                                                "type": "string",
                                                "description": "The rebinding time day."
                                            },
                                            "hour": {
                                                "type": "string",
                                                "description": "The rebinding time hour."
                                            },
                                            "minute": {
                                                "type": "string",
                                                "description": "The rebinding time minute."
                                            }
                                        }
                                    },
                                    "renew-time": {
                                        "description": "Configure the renew time of the pool.",
                                        "properties": {
                                            "day": {
                                                "type": "string",
                                                "description": "The renew time day."
                                            },
                                            "hour": {
                                                "type": "string",
                                                "description": "The renew time hour."
                                            },
                                            "minute": {
                                                "type": "string",
                                                "description": "The renew time minute."
                                            }
                                        }
                                    },
                                    "option125": {
                                        "description": "Configure an option125 of the pool.",
                                        "properties": {
                                            "enterprise-code": {
                                                "type": "string",
                                                "description": "The enterprise information option code."
                                            },
                                            "option-string": {
                                                "type": "string",
                                                "description": "The enterprise information option value."
                                            }
                                        }
                                    },
                                    "sip-server": {
                                        "description": "Configure the SIP server of the pool.",
                                        "properties": {
                                            "sip-server-mode": {
                                                "type": "string",
                                                "enum": [
                                                    "ip-mode",
                                                    "list-mode"
                                                ],
                                                "description": "The mode of SIP server."
                                            },
                                            "sip-server-format": {
                                                "description": "The format of SIP server.",
                                                "properties": {
                                                    "ip-format": {
                                                        "description": "The IP of SIP server.",
                                                        "properties": {
                                                            "sip-server-ip1": {
                                                                "type": "string",
                                                                "description": "The first IP of SIP server."
                                                            },
                                                            "sip-server-ip2": {
                                                                "type": "string",
                                                                "description": "The second IP of SIP server."
                                                            }
                                                        }
                                                    },
                                                    "string-format": {
                                                        "description": "The name of SIP server.",
                                                        "properties": {
                                                            "sip-server-name1": {
                                                                "type": "string",
                                                                "description": "The first name of SIP server."
                                                            },
                                                            "sip-server-name2": {
                                                                "type": "string",
                                                                "description": "The second name of SIP server."
                                                            }
                                                        }
                                                    }
                                                }
                                            }
                                        }
                                    },
                                    "sections": {
                                        "description": "List of the sections of the pool.",
                                        "properties": {
                                            "section": {
                                                "description": "Configure the section of the pool.",
                                                "type": "array",
                                                "items": {
                                                    "properties": {
                                                        "section-index": {
                                                            "type": "string",
                                                            "description": "The section index."
                                                        },
                                                        "start-ip": {
                                                            "type": "string",
                                                            "description": "The start IP address of the section."
                                                        },
                                                        "end-ip": {
                                                            "type": "string",
                                                            "description": "The end IP address of the section."
                                                        }
                                                    }
                                                },
                                                "x-key": "section-index"
                                            }
                                        }
                                    },
                                    "dns-lists": {
                                        "description": "List of DNS IPs.",
                                        "properties": {
                                            "dns-list": {
                                                "description": "Configure a DNS IP instance.",
                                                "type": "array",
                                                "items": {
                                                    "properties": {
                                                        "dns-ip": {
                                                            "type": "string",
                                                            "description": "DNS IP."
                                                        }
                                                    }
                                                },
                                                "x-key": "dns-ip"
                                            }
                                        }
                                    },
                                    "user-options": {
                                        "description": "List of configuring the user define options of the pool.",
                                        "properties": {
                                            "user-option": {
                                                "description": "Configure a user define option of the pool.",
                                                "type": "array",
                                                "items": {
                                                    "properties": {
                                                        "option-code": {
                                                            "type": "string",
                                                            "description": "The user define option code."
                                                        },
                                                        "option-format": {
                                                            "description": "The format of server option.",
                                                            "properties": {
                                                                "ascii-format": {
                                                                    "description": "ASCII string format.",
                                                                    "properties": {
                                                                        "ascii-string": {
                                                                            "type": "string",
                                                                            "description": "The ASCII string of the option."
                                                                        }
                                                                    }
                                                                }
                                                            }
                                                        }
                                                    }
                                                },
                                                "x-key": "option-code"
                                            }
                                        }
                                    },
                                    "option-force-replys": {
                                        "description": "List of the force reply options of the pool.",
                                        "properties": {
                                            "option-force-reply": {
                                                "description": "Configure the force reply option of the pool.",
                                                "type": "array",
                                                "items": {
                                                    "properties": {
                                                        "option-code": {
                                                            "type": "string",
                                                            "description": "The force reply option code."
                                                        }
                                                    }
                                                },
                                                "x-key": "option-code"
                                            }
                                        }
                                    }
                                }
                            },
                            "x-key": "ip-pool-name"
                        }
                    }
                }
            }
        }
    }
}