<?xml version="1.0" encoding="UTF-8"?>
<module name="zxr10-ipv6"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:ipv6="http://www.zte.com.cn/zxr10/netconf/schema/rosng/ipv6"
        xmlns:ncd="http://www.zte.com.cn/zxr10/netconf/schema/rosng/netconf-defs">
  <namespace uri="http://www.zte.com.cn/zxr10/netconf/schema/rosng/ipv6"/>
  <prefix value="ipv6"/>
  <yang-version value="1"/>
  <import module="zxr10-netconf-defs">
    <prefix value="ncd"/>
  </import>
  <organization>
    <text>ZTE Corporation</text>
  </organization>
  <contact>
    <text>ZTE Corporation  
   Nanjing Institute of ZTE Corporation 
   No.68 Zijinghua Rd. Yuhuatai District, Nanjing, China 
   Tel: +86-25-52870000</text>
  </contact>
  <description>
    <text>NETCONF of IPv6 module</text>
  </description>
  <revision date="2021-04-08">
    <description>
      <text>Add node:/configuration/ipv6/nni-gateways/nni-gateway/if-name.
Add node:/configuration/ipv6/nni-gateways/nni-gateway/ipv6-addr.</text>
    </description>
  </revision>
  <revision date="2021-02-02">
    <description>
      <text>Add node:/configuration/ipv6/icmp-config/icmp-options/time-exceed.</text>
    </description>
  </revision>
  <revision date="2020-09-02">
    <description>
      <text>Add node:/configuration/ipv6/ipv6-option/strict-pmtu.</text>
    </description>
  </revision>
  <revision date="2020-07-23">
    <description>
      <text>Add node:/configuration/ipv6/ipv6-option/load-sharing-mode.</text>
    </description>
  </revision>
  <revision date="2014-06-04">
    <description>
      <text>IPv6 inial model.</text>
    </description>
  </revision>
  <feature name="load-sharing-feature">
    <description>
      <text>load-sharing-mode node use this feature.</text>
    </description>
  </feature>
  <feature name="nni-gateway-feature">
    <description>
      <text>nni-gateway node use this feature.</text>
    </description>
  </feature>
  <container name="configuration">
    <description>
      <text>Config node.</text>
    </description>
    <container name="ipv6">
      <description>
        <text>IPV6 module configuration entry.</text>
      </description>
      <container name="ipv6-option">
        <description>
          <text>Global IPV6 configuration nodes.</text>
        </description>
        <leaf name="load-sharing">
          <description>
            <text>Turn on the load sharing and set mode.Its default value which is turning on per-stream mode or disable and generally disable, depends on product specification..</text>
          </description>
          <type name="enumeration">
            <enum name="per-stream">
              <value value="1"/>
              <description>
                <text>Turn on the load sharing,mode is per-stream.</text>
              </description>
            </enum>
            <enum name="disable">
              <value value="0"/>
              <description>
                <text>Turn off the load sharing.</text>
              </description>
            </enum>
          </type>
        </leaf>
        <leaf name="strict-pmtu">
          <description>
            <text>Strict-pmtu function.</text>
          </description>
          <type name="enumeration">
            <enum name="disable">
              <value value="0"/>
              <description>
                <text>Disable strict PMTU function.</text>
              </description>
            </enum>
            <enum name="enable">
              <value value="1"/>
              <description>
                <text>Enable strict PMTU function.</text>
              </description>
            </enum>
          </type>
          <default value="disable"/>
          <ncd:revise-date date="2020-09-02"/>
        </leaf>
      </container>
      <container name="icmp-config">
        <description>
          <text>Enter ICMP configure mode.</text>
        </description>
        <container name="icmp-options">
          <description>
            <text>Set ICMP options.</text>
          </description>
          <leaf name="time-exceed">
            <description>
              <text>Set ICMPv6 time exceeded messages function.</text>
            </description>
            <type name="enumeration">
              <enum name="disable">
                <value value="0"/>
                <description>
                  <text>Disable sending ICMPv6 Time Exceeded messages.</text>
                </description>
              </enum>
              <enum name="enable">
                <value value="1"/>
                <description>
                  <text>Enable sending ICMPv6 Time Exceeded messages.</text>
                </description>
              </enum>
            </type>
            <default value="enable"/>
          </leaf>
        </container>
        <ncd:revise-date date="2021-02-02"/>
      </container>
      <container name="nni-gateways">
        <description>
          <text>Set gateway for NNI Ping&amp;Trace.</text>
        </description>
        <list name="nni-gateway">
          <description>
            <text>Set gateway for NNI Ping&amp;Trace for specify interface.</text>
          </description>
          <key value="if-name"/>
          <leaf name="if-name">
            <description>
              <text>Interface name.</text>
            </description>
            <type name="ncd:ifname"/>
          </leaf>
          <leaf name="ipv6-addr">
            <description>
              <text>Ipv6 address of the gateway.</text>
            </description>
            <type name="ncd:ipv6-address"/>
          </leaf>
        </list>
        <ncd:revise-date date="2021-04-08"/>
      </container>
    </container>
    <ncd:revise-date date="2020-07-23"/>
  </container>
  <container name="state">
    <config value="false"/>
    <description>
      <text>Dynamic information.</text>
    </description>
    <container name="ipv6">
      <description>
        <text>IPv6 information.</text>
      </description>
      <container name="traffic">
        <description>
          <text>IPv6 statistics data.</text>
        </description>
        <container name="ip">
          <description>
            <text>IP statistics data.</text>
          </description>
          <leaf name="total-rcv">
            <description>
              <text>The number of receiving packets.</text>
            </description>
            <type name="uint32"/>
          </leaf>
          <leaf name="local-destination">
            <description>
              <text>The number of packets whose destination is local IP.</text>
            </description>
            <type name="uint32"/>
          </leaf>
          <leaf name="format-error">
            <description>
              <text>The number of receiving packets whose format is wrong.</text>
            </description>
            <type name="uint32"/>
          </leaf>
          <leaf name="truncated-error">
            <description>
              <text>The number of receiving packets which is truncated.</text>
            </description>
            <type name="uint32"/>
          </leaf>
          <leaf name="holimit-exceed">
            <description>
              <text>The number of packets whose hoplimit will be exceeded during tramitting.</text>
            </description>
            <type name="uint32"/>
          </leaf>
          <leaf name="unknown-proto">
            <description>
              <text>The number of receving packets whose proto is unknown.</text>
            </description>
            <type name="uint32"/>
          </leaf>
          <leaf name="reassembled">
            <description>
              <text>The number of receiving packet which will be reassembled.</text>
            </description>
            <type name="uint32"/>
          </leaf>
          <leaf name="reassemble-timeout">
            <description>
              <text>The number of pacekts which is timeout during reassembling.</text>
            </description>
            <type name="uint32"/>
          </leaf>
          <leaf name="reassembled-fail">
            <description>
              <text>The number of reassembled failure.</text>
            </description>
            <type name="uint32"/>
          </leaf>
          <leaf name="fragments">
            <description>
              <text>The number of packets which is fragmented.</text>
            </description>
            <type name="uint32"/>
          </leaf>
          <leaf name="fragment-fail">
            <description>
              <text>The number of fragment failure.</text>
            </description>
            <type name="uint32"/>
          </leaf>
          <leaf name="mcast-in">
            <description>
              <text>The number of receiving packets whose destination is multicast IP.</text>
            </description>
            <type name="uint32"/>
          </leaf>
          <leaf name="mcast-out">
            <description>
              <text>The number of sending packets whose destination is multicast IP.</text>
            </description>
            <type name="uint32"/>
          </leaf>
          <leaf name="local-generated">
            <description>
              <text>The number of packets which is generated locally excluding forwarding.</text>
            </description>
            <type name="uint32"/>
          </leaf>
          <leaf name="forward">
            <description>
              <text>The number of forwarding packets.</text>
            </description>
            <type name="uint32"/>
          </leaf>
          <leaf name="dropped">
            <description>
              <text>The number of lost packets due to nobufs.</text>
            </description>
            <type name="uint32"/>
          </leaf>
          <leaf name="no-route">
            <description>
              <text>The number of dropped packet due to no route.</text>
            </description>
            <type name="uint32"/>
          </leaf>
        </container>
        <container name="icmp">
          <description>
            <text>ICMP statistics data.</text>
          </description>
          <container name="receive">
            <description>
              <text>ICMP reveiving paragrams.</text>
            </description>
            <leaf name="total-rcv">
              <description>
                <text>The number of receving packets.</text>
              </description>
              <type name="uint32"/>
            </leaf>
            <leaf name="format-error">
              <description>
                <text>The number of receiving packets whose format is wrong.</text>
              </description>
              <type name="uint32"/>
            </leaf>
            <leaf name="checksum-error">
              <description>
                <text>The number of receiving packets whose checksum is wrong.</text>
              </description>
              <type name="uint32"/>
            </leaf>
            <leaf name="bad-code">
              <description>
                <text>The number of receiving packets whose "code" is wrong.</text>
              </description>
              <type name="uint32"/>
            </leaf>
            <leaf name="bad-len">
              <description>
                <text>The number of receiving packets whose lenth is wrong.</text>
              </description>
              <type name="uint32"/>
            </leaf>
            <leaf name="dest-unreach">
              <description>
