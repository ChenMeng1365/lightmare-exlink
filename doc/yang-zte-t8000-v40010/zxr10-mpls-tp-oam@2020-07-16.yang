module zxr10-mpls-tp-oam {
  namespace 'http://www.zte.com.cn/zxr10/netconf/schema/rosng/mpls-tp-oam'
            ;
  prefix mpls-tp-oam ;
  yang-version 1 ;
  import zxr10-netconf-defs {
    prefix ncd ;
  }
  organization 'ZTE Corporation'
               ;
  contact 'ZTE Corporation  '+"\n"
        + '   Nanjing Institute of ZTE Corporation '+"\n"
        + '   No.68 Zijinghua Rd. Yuhuatai District, Nanjing, China '+"\n"
        + '   Tel: +86-25-52870000'
          ;
  description 'The netconf module to describe MPLS-TP OAM'
              ;
  revision 2020-07-16 {
    description '1 Obsolete  table of state/mpls-tp-oam/lm-dm-results.'+"\n"
              + '2 Add table of state/mpls-tp-oam/lm-and-dm-results.'
                ;
  }
  revision 2019-11-08 {
    description '1 Add leaf node of configuration/mpls-tp-oam/megs/meg/mep-or-m'
              + 'ip/mep/mep/statistics-interval in megs table.'+"\n"
              + '2 Add table of state/mpls-tp-oam/lm-dm-results.'+"\n"
              + '3 Modify leaf node of lb-event/lb-result/format-error-count.'
                ;
  }
  revision 2019-07-23 {
    description '1 Add leaf node of lb-event/lb-result/format-error-count.'+"\n"
              + '2 Add leaf node of state/mpls-tp-oam/lb-results/lb-result/form'
              + 'at-error-count.'+"\n"
              + '3 Add leaf node of operation/mpls-tp-oam/lbs/lb/status/enable/'
              + 'pattern.'
                ;
  }
  revision 2018-06-27 {
    description '1 modify the description and range of ondemand-lm-event/lm-res'
              + 'ult/meg-index.'+"\n"
              + '2 modify the description and range of ondemand-dm-event/dm-res'
              + 'ult/meg-index.'+"\n"
              + '3 modify the description and range of lb-event/lb-result/meg-i'
              + 'ndex.'+"\n"
              + '4 modify the description and range of lt-event/lt-result/meg-i'
              + 'ndex.'+"\n"
              + '5 modify the description and range of test-event/test-result/m'
              + 'eg-index.'+"\n"
              + '6 modify the description and range of configuration/mpls-tp-oa'
              + 'm/megs/meg/meg-index.'+"\n"
              + '7 modify the description and range of state/mpls-tp-oam/lb-res'
              + 'ults/lb-result/meg-index.'+"\n"
              + '8 modify the description and range of state/mpls-tp-oam/lt-res'
              + 'ults/lt-result/meg-index.'+"\n"
              + '9 modify the description and range of state/mpls-tp-oam/test-r'
              + 'esults/test-result/meg-index.'+"\n"
              + '10 modify the description and range of state/mpls-tp-oam/lm-re'
              + 'sults/lm-result/meg-index.'+"\n"
              + '11 modify the description and range of state/mpls-tp-oam/dm-re'
              + 'sults/dm-result/meg-index.'+"\n"
              + '12 modify the description and range of state/mpls-tp-oam/megid'
              + 's-query/megid-query/meg-index.'+"\n"
              + '13 modify the description and range of operation/mpls-tp-oam/l'
              + 'bs/lb/meg-index.'+"\n"
              + '14 modify the description and range of operation/mpls-tp-oam/l'
              + 'ts/lt/meg-index.'+"\n"
              + '15 modify the description and range of operation/mpls-tp-oam/t'
              + 'ests/test/meg-index.'+"\n"
              + '16 modify the description and range of operation/mpls-tp-oam/l'
              + 'ms/lm/meg-index.'+"\n"
              + '17 modify the description and range of operation/mpls-tp-oam/d'
              + 'ms/dm/meg-index.'
                ;
  }
  revision 2018-05-04 {
    description '1 Add type define node of phb-all.'+"\n"
              + '2 Add feature node of function-ccm-stop.'+"\n"
              + '3 Add feature node of function-rdi.'+"\n"
              + '4 Add feature node of function-oam-statistics.'+"\n"
              + '5 Add feature node of delay.'+"\n"
              + '6 Modify feature node of function-lmmlmr, and support the 6700'
              + ' product. Obsolete the node of configuration/mpls-tp-oam/megs/'
              + 'mep/pro-lmmlmr-phb, and add the node of configuration/mpls-tp-'
              + 'oam/megs/mep/pro-lmmlmr-phb-ex.'+"\n"
              + '7 Add leaf node of configuration/mpls-tp-oam/global/delay.'+"\n"
              + '8 Add leaf node of configuration/mpls-tp-oam/megs/mep/oammappi'
              + 'ng-use-ccmstop.'+"\n"
              + '9 Add leaf node of configuration/mpls-tp-oam/megs/mep/oammappi'
              + 'ng-use-rdi.'+"\n"
              + '10 Add leaf node of configuration/mpls-tp-oam/megs/mep/oam-pac'
              + 'ket-statistics.'+"\n"
              + '11 Add leaf node of operation/mpls-tp-oam/tests/test/mep-id,an'
              + 'd operation/mpls-tp-oam/tests/test/service-type.'+"\n"
              + '12 Add leaf node of operation/mpls-tp-oam/lbs/lb/mep-or-mip/if'
              + '-number.'
                ;
  }
  revision 2017-08-17 {
    description '1 Support the ASR,ZSRV2 and ZSRV2-GM product.'
                ;
  }
  revision 2017-08-07 {
    description '1 Add feature node of instance-mtunnel.'+"\n"
              + '2 Add feature node of function-lmmlmr.'+"\n"
              + '3 Add feature node of function-test.'+"\n"
              + '4 Add leaf node of configuration/mpls-tp-oam/global/node-id in'
              + ' global table.'+"\n"
              + '5 Add case node of configuration/mpls-tp-oam/megs/sta-mtunnel,'
              + ' and it contains leaf nodes of mtunnel-id and lsp-id.'+"\n"
              + '6 Add leaf node of configuration/mpls-tp-oam/megs/mep/ccm-sd-s'
              + 'tatus.'+"\n"
              + '7 Add leaf node of configuration/mpls-tp-oam/megs/mep/icc-leng'
              + 'th.'+"\n"
              + '8 Add leaf nodes of configuration/mpls-tp-oam/megs/mep/pro-lmm'
              + 'lmr-enable, pro-lmmlmr-phb, pro-lmmlmr-period and pro-lmmlmr-t'
              + 'ype.'+"\n"
              + '9 Add case node of mip-node, and it contains leaf nodes of ope'
              + 'ration/mpls-tp-oam/lbs/lb/mep-or-mip/node-id and node-ttl.'+"\n"
              + '10 Add case node of mip-none, and it contains leaf nodes of op'
              + 'eration/mpls-tp-oam/lbs/lb/mep-or-mip/none-flag and none-ttl.'+"\n"
              + '11 Add leaf node of operation/mpls-tp-oam/tests/test/out-seg.'+"\n"
              + '12 Modify the revision node of 2016-09-18.'
                ;
  }
  revision 2016-09-18 {
    description '1 delete the default value of configuration/mpls-tp-oam/global'
              + '/rdi-to-sf in global table.'+"\n"
              + '2 Add default perf-code of configuration/mpls-tp-oam/global/rd'
              + 'i-to-sf in global table.'
                ;
  }
  revision 2015-09-01 {
    description '1 add the description and range of operation/mpls-tp-oam/lbs/l'
              + 'b/status/report-interval and operation/mpls-tp-oam/tests/test/'
              + 'status/report-interval.'+"\n"
              + '2 create new table of lb results upsending table, which includ'
              + 'ing indformation about megindex,serial number,local mepid,send'
              + 'ing packets,receiving packets,lossing packets and so on.'+"\n"
              + '3 create new table of lt results upsending table, which includ'
              + 'ing indformation about megindex,serial number,local mepid,link'
              + 'ing result,successful hops of LT,failed hop and so on.'+"\n"
              + '4 create new table of test results upsending table, which incl'
              + 'uding indformation about megindex,local mepid,receiving TST pa'
              + 'ckets,losing TST packets,bandwidth and so on.'+"\n"
              + '5 modify the description of state/mpls-tp-oam/test-results/tes'
              + 't-result/discard-count and state/mpls-tp-oam/lb-results/lb-res'
              + 'ult/discard-count.'
                ;
  }
  revision 2015-04-28 {
    description '1 Del leaf node of cconfiguration/mpls-tp-oam/megs/meg/mep-or-'
              + 'mip/mep/mep/ais-status in megs table.'+"\n"
              + '2 Del leaf node of cconfiguration/mpls-tp-oam/megs/meg/mep-or-'
              + 'mip/mep/mep/ais-phb in megs table.'+"\n"
              + '3 del leaf node of cconfiguration/mpls-tp-oam/megs/meg/mep-or-'
              + 'mip/mip/mip/ais-status in megs table.'+"\n"
              + '4 Del leaf node of cconfiguration/mpls-tp-oam/megs/meg/mep-or-'
              + 'mip/mip/mip/ais-phb in megs table.'+"\n"
              + '5 Add feature node of fei-period.'+"\n"
              + '6 Add leaf node of cconfiguration/mpls-tp-oam/global/fei-perio'
              + 'd in global table.'
                ;
  }
  revision 2015-02-06 {
    description '1 add leaf node of cconfiguration/mpls-tp-oam/global/ais-perio'
              + 'd in global table.'+"\n"
              + '2 add table of state/mpls-tp-oam/megids-query.'+"\n"
              + '3 change mandatory from false to true of configuration/mpls-tp'
              + '-oam/megs/meg/mep-or-mip/mep/local-mep-id and configuration/mp'
              + 'ls-tp-oam/megs/meg/mep-or-mip/mip/mip-id.'+"\n"
              + '4 modify the description and range of operation/mpls-tp-oam/lm'
              + 's/lm/status/report-interval and operation/mpls-tp-oam/dms/dm/s'
              + 'tatus/report-interval.'
                ;
  }
  revision 2014-11-24 {
    description '1 add report-interval node in lm-operation table.'+"\n"
              + '2 add report-interval node in dm-operation table.'+"\n"
              + '3 create new table of on-demand lm results upsending table, wh'
              + 'ich including indformation about megindex,local mepid,local/pe'
              + 'er sending packets,local/peer receiving packets,local/peer los'
              + 'sing packets,the ratio(*1000000) of local/peer lossing packets'
              + ' and so on.'+"\n"
              + '4 create new table of on-demand dm results upsending table, wh'
              + 'ich including indformation about megindex,local mepid,local/pe'
              + 'er sending packets,local/peer receiving packets,local/peer los'
              + 'sing packets,the ratio(*1000000) of local/peer lossing packets'
              + ' and so on.'
                ;
  }
  revision 2014-02-20 {
    description '1 Correcting the global node name of channel-type.'+"\n"
              + '2 Adding the limit for operation of mandatory.'+"\n"
              + '3 The type of pw-name is changed from string to ncd:ifname.'+"\n"
              + '4 The upper limit of tunnel id depend on the device.'
                ;
  }
  revision 2013-12-01 {
    description '1 Add the global node of statistic-time.'+"\n"
              + '2 Add default values for the global nodes of ais-speed and csf'
              + '-speed.'+"\n"
              + '3 The type of mip-interface is changed from string to ncd:ifna'
              + 'me.'+"\n"
              + '4 0 indicates unknown changed to 3 indicates unknown in the no'
              + 'de of type in dm-result table.'+"\n"
              + '5 Changing the testing structure of operation type:delete orig'
              + 'inal node of status;change the node of choice to the node of s'
              + 'tatus;change the node of case to the node of enable or disable'
              + '.'
                ;
  }
  revision 2013-11-25 {
    description 'Modify the description of the hop-sequence.'
                ;
  }
  revision 2013-09-12 {
    description 'Initial version.'
                ;
  }
  feature ais-speed {
    description 'AIS fast or slow packets config.'
                ;
  }
  feature csf-speed {
    description 'CSF fast or slow packets config.'
                ;
  }
  feature mip-up-or-down {
    description 'The type of MIP, including down MIP and up MIP(default: down).'
                ;
  }
  feature mip-interface {
    description 'MIP interface. This parameter is not necessary. It must be phy'
              + 'sical interface if the interface is configured.'
                ;
  }
  feature fei-period {
    description 'FEI packet period config.'
                ;
  }
  feature instance-mtunnel {
    description 'Support the instance of static P2MP tunnel.'
                ;
  }
  feature function-lmmlmr {
    description 'Support the function of proactive LMMLMR.'
                ;
  }
  feature function-test {
    description 'Support the out-seg ID configure of TEST.'
                ;
  }
  feature function-ccm-stop {
    description 'Support the function of CCM-STOP.'
                ;
  }
  feature function-rdi {
    description 'Support the function of oammapping RDI.'
                ;
  }
  feature function-oam-statistics {
    description 'Support the function of packet statistics.'
                ;
  }
  feature delay {
    description 'Support the delay time configure.'
                ;
  }
  feature statistics-interval {
    description 'Support the statistics interval.'
                ;
  }
  typedef mep-type {
    type enumeration {
      enum source {
        value 1 ;
        description 'Source MEP.'
                    ;
      }
      enum destination {
        value 2 ;
        description 'Destination MEP.'
                    ;
      }
      enum bidirection {
        value 3 ;
        description 'Bidirection MEP.'
                    ;
      }
    }
    description 'The type of MEP, including source MEP, destination MEP and bid'
              + 'irection MEP.'
                ;
  }
  typedef phb {
    type enumeration {
      enum be {
        value 1 ;
        description 'Best effort.'
                    ;
      }
      enum af11 {
        value 2 ;
        description 'Assured forwarding 11.'
                    ;
      }
      enum af12 {
        value 3 ;
        description 'Assured forwarding 12.'
                    ;
      }
      enum af21 {
        value 4 ;
        description 'Assured forwarding 21.'
                    ;
      }
      enum af22 {
        value 5 ;
        description 'Assured forwarding 22.'
                    ;
      }
      enum af31 {
        value 6 ;
        description 'Assured forwarding 31.'
                    ;
      }
      enum af32 {
        value 7 ;
        description 'Assured forwarding 32.'
                    ;
      }
      enum af41 {
        value 8 ;
        description 'Assured forwarding 41.'
                    ;
      }
      enum af42 {
        value 9 ;
        description 'Assured forwarding 42.'
                    ;
      }
      enum cs6 {
        value 10 ;
        description 'Class selector 6.'
                    ;
      }
      enum cs7 {
        value 11 ;
        description 'Class selector 7.'
                    ;
      }
      enum ef {
        value 12 ;
        description 'Expedited forwarding.'
                    ;
      }
    }
    description 'The PHB value carried in packets.'
                ;
  }
  typedef phb-all {
    type enumeration {
      enum be {
        value 1 ;
        description 'Best effort.'
                    ;
      }
      enum af11 {
        value 2 ;
        description 'Assured forwarding 11.'
                    ;
      }
      enum af12 {
        value 3 ;
        description 'Assured forwarding 12.'
                    ;
      }
      enum af21 {
        value 4 ;
        description 'Assured forwarding 21.'
                    ;
      }
      enum af22 {
        value 5 ;
        description 'Assured forwarding 22.'
                    ;
      }
      enum af31 {
        value 6 ;
        description 'Assured forwarding 31.'
                    ;
      }
      enum af32 {
        value 7 ;
        description 'Assured forwarding 32.'
                    ;
      }
      enum af41 {
        value 8 ;
        description 'Assured forwarding 41.'
                    ;
      }
      enum af42 {
        value 9 ;
        description 'Assured forwarding 42.'
                    ;
      }
      enum cs6 {
        value 10 ;
        description 'Class selector 6.'
                    ;
      }
      enum cs7 {
        value 11 ;
        description 'Class selector 7.'
                    ;
      }
      enum ef {
        value 12 ;
        description 'Expedited forwarding.'
                    ;
      }
      enum all {
        value 13 ;
        description 'All EXP forwarding.'
                    ;
      }
    }
    description 'The PHB value carried in packets(include all).'
                ;
  }
  container configuration {
    description 'Configuration model info.'
                ;
    container mpls-tp-oam {
      description 'MPLS-TP OAM instences configuration'
                  ;
      container global {
        description 'Configure MPLS-TP OAM of global info.'
                    ;
        leaf status {
          description 'The switch for turning on/off MPLS-TP OAM function(defau'
                    + 'lt: enable).'
                      ;
          type enumeration {
            enum disable {
              value 0 ;
              description 'Turn off MPLS-TP OAM function.'
                          ;
            }
            enum enable {
              value 1 ;
              description 'Turn on MPLS-TP OAM function.'
                          ;
            }
          }
          default enable ;
        }
        leaf mel {
          description 'The value of MEL field in OAM packets, ranges from 0 to '
                    + '7(default: 7).'
                      ;
          type uint8 {
            range 0..7 {
              description 'The value of MEL.'
                          ;
            }
          }
          default 7 ;
        }
        leaf channel-type {
          description 'The identify of Y.1731 packet in G-ACH Label, ranges fro'
                    + 'm 1 to 65535(default: 32762).'
                      ;
          type uint16 {
            range 1..65535 {
              description 'The value of channel type.'
                          ;
            }
          }
          default 32762 ;
        }
        leaf tc {
          description 'The value of TC field in the label of GAL, ranges from 0'
                    + ' to 7(default: 7).'
                      ;
          type uint8 {
            range 0..7 {
              description 'The value of GAL TC.'
                          ;
            }
          }
          default 7 ;
        }
        leaf rdi-to-sf {
          description 'This switch used to transform the RDI alarm to SF alarm.'
                      ;
          type enumeration {
            enum disable {
              value 0 ;
              description 'Turn off RDI to SF function.'
                          ;
            }
            enum enable {
              value 1 ;
              description 'Turn on RDI to SF function.'
                          ;
            }
          }
        }
        leaf statistic-time {
          description 'The value of reporting interval of statistic functionali'
                    + 'ty, ranges from 5s to 60s(default: 5s).'
                      ;
          type uint8 {
            range 5..60 {
              description 'The value of reporting interval of statistic functio'
                        + 'nality.'
                          ;
            }
          }
          default 5 ;
        }
        leaf ais-speed {
          if-feature ais-speed ;
          description 'AIS fast or slow packets config.'
                      ;
          type enumeration {
            enum fast {
              value 1 ;
              description 'Fast packets.'
                          ;
            }
            enum slow {
              value 2 ;
              description 'Slow packets.'
                          ;
            }
          }
        }
        leaf csf-speed {
          if-feature csf-speed ;
          description 'CSF fast or slow packets config.'
                      ;
          type enumeration {
            enum fast {
              value 1 ;
              description 'Fast packets'
                          ;
            }
            enum slow {
              value 2 ;
              description 'Slow packets'
                          ;
            }
          }
        }
        leaf ais-period {
          description 'The period carried in the AIS frame,including 1s,and 1mi'
                    + 'n (defaut: 1s).'
                      ;
          type enumeration {
            enum 1s {
              value 0 ;
              description '1 second, it means 1 packet per second.'
                          ;
            }
            enum 1min {
              value 1 ;
              description '1 minute, it means 1 packet per minute.'
                          ;
            }
          }
          default 1s ;
        }
        leaf fei-period {
          if-feature fei-period ;
          description 'The sending period of FEI packets, ranges from 1 to 5(un'
                    + 'its:s, default: 1).'
                      ;
          type uint8 {
            range 1..5 {
              description 'FEI packet send period'
                          ;
            }
          }
          units s ;
          default 1 ;
        }
        leaf node-id {
          description 'The node ID is used to identify the MIP.'
                      ;
          type ncd:ipv4-address ;
          ncd:revise-date 2017-08-07 ;
        }
        leaf delay {
          if-feature delay ;
          description 'The delay time, ranges from 0 to 60(units:min, default: '
                    + '0).'
                      ;
          type uint8 {
            range 0..60 {
              description 'The value of delay time.'
                          ;
            }
          }
          default 0 ;
          ncd:revise-date 2018-05-04 ;
        }
      }
      container megs {
        description 'Configure MEG parameters.'
                    ;
        list meg {
          description 'Configure MEG parameters.'
                      ;
          key meg-index ;
          leaf meg-index {
            description 'The value of MEG index,the upper limit of this paramet'
                      + 'er depends on the device.'
                        ;
            type uint32 {
              range 1..4294967295 {
                description 'The value of MEG index.'
                            ;
              }
            }
          }
          choice instance-type {
            description 'Configure Instance type.'
                        ;
            mandatory true ;
            case section {
              description 'Configure MPLS-TP OAM of section.'
                          ;
              container section {
                description 'Configure MPLS-TP OAM of section.'
                            ;
                leaf section-id {
                  description 'Section ID used to identify the section entity.'
                              ;
                  type uint32 {
                    range 1..4000 {
                      description 'The value of section ID.'
                                  ;
                    }
                  }
                  mandatory true ;
                }
              }
            }
            case sta-tunnel {
              description 'Configure MPLS-TP OAM of static tunnel.'
                          ;
              container sta-tunnel {
                description 'Configure MPLS-TP OAM of static tunnel.'
                            ;
                leaf tunnel-id {
                  description 'The identity of static tunnel,the upper limit of'
                            + ' this parameter depend on the device.'
                              ;
                  type uint32 {
                    range 1..4294967295 {
                      description 'The value of static tunnel number,and the up'
                                + 'per limit of this parameter depend on the de'
                                + 'vice.'
                                  ;
                    }
                  }
                  mandatory true ;
                }
                leaf lsp-id {
                  description 'The identity of the static LSP.'
                              ;
                  type uint16 {
                    range 1..65535 {
                      description 'The value of LSP ID.'
                                  ;
                    }
                  }
                  mandatory true ;
                }
              }
            }
            case dyn-tunnel {
              description 'Configure MPLS-TP OAM of  dynamic tunnel.'
                          ;
              container dyn-tunnel {
                description 'Configure MPLS-TP OAM of  dynamic tunnel.'
                            ;
                leaf lsp-role {
                  description 'The LSP role of dynamic tunnel, including master'
                            + ' LSP and slave LSP.'
                              ;
                  type enumeration {
                    enum master {
                      value 1 ;
                      description 'Master LSP.'
                                  ;
                    }
                    enum slave {
                      value 2 ;
                      description 'Slave LSP.'
                                  ;
                    }
                  }
                  mandatory true ;
                }
                leaf tunnel-num {
                  description 'The identity of dynamic tunnel,the upper limit o'
                            + 'f this parameter depend on the device.'
                              ;
                  type uint32 {
                    range 1..4294967295 {
                      description 'The value of dynamic tunnel number,and the u'
                                + 'pper limit of this parameter depend on the d'
                                + 'evice.'
                                  ;
                    }
                  }
                  mandatory true ;
                }
              }
            }
            case pw {
              description 'Configure MPLS-TP OAM of PW.'
                          ;
              container pw-name {
                leaf pw-name {
                  description 'The name of PW.'
                              ;
                  type ncd:ifname {
                    length 1..31 {
                      description 'The length of pw name.'
                                  ;
                    }
                  }
                  mandatory true ;
                }
              }
            }
            case sta-mtunnel {
              if-feature instance-mtunnel ;
              description 'Configure MPLS-TP OAM of static P2MP tunnel.'
                          ;
              container sta-mtunnel {
                if-feature instance-mtunnel ;
                description 'Configure MPLS-TP OAM of static P2MP tunnel.'
                            ;
                leaf mtunnel-id {
                  if-feature instance-mtunnel ;
                  description 'The identity of static P2MP tunnel.'
                              ;
                  type uint32 {
                    range 1..4294967294 {
                      description 'The value of static tunnel number,and the up'
                                + 'per limit of this parameter depend on the de'
                                + 'vice.'
                                  ;
                    }
                  }
                  mandatory true ;
                }
                leaf lsp-id {
                  if-feature instance-mtunnel ;
                  description 'The identity of the static P2MP LSP.'
                              ;
                  type uint16 {
                    range 1 {
                      description 'The value of LSP ID.'
                                  ;
                    }
                  }
                  mandatory true ;
                }
              }
              ncd:revise-date 2017-08-07 ;
            }
          }
          leaf meg-id {
            description 'MEG identity name, ranges from 0 to 13 characters. If '
                      + 'MEG ID is not configrued, the length of string is 0.'
                        ;
            type string {
