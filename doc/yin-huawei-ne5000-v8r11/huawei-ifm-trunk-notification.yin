<?xml version="1.0" encoding="UTF-8"?>
<module name="huawei-ifm-trunk-notification"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:ifm-trunk-notification="urn:huawei:yang:huawei-ifm-trunk-notification"
        xmlns:pub-type="urn:huawei:yang:huawei-pub-type"
        xmlns:ext="urn:huawei:yang:huawei-extension">
  <yang-version value="1"/>
  <namespace uri="urn:huawei:yang:huawei-ifm-trunk-notification"/>
  <prefix value="ifm-trunk-notification"/>
  <import module="huawei-pub-type">
    <prefix value="pub-type"/>
  </import>
  <import module="huawei-extension">
    <prefix value="ext"/>
  </import>
  <organization>
    <text>Huawei Technologies Co., Ltd.</text>
  </organization>
  <contact>
    <text>Huawei Industrial Base
Bantian, Longgang
Shenzhen 518129
People's Republic of China
Website: http://www.huawei.com
Email: support@huawei.com</text>
  </contact>
  <description>
    <text>Notification of ifm-trunk.</text>
  </description>
  <revision date="2020-05-30">
    <description>
      <text>Initial revision.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <ext:task-name value="trunk"/>
  <typedef name="port-status">
    <type name="enumeration">
      <enum name="up">
        <value value="1"/>
        <description>
          <text>The interface is in Up state.</text>
        </description>
      </enum>
      <enum name="down">
        <value value="2"/>
        <description>
          <text>The interface is in Down state.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Interface suppression status.</text>
    </description>
  </typedef>
  <notification name="all-member-down">
    <description>
      <text>The last member interface link status changes to down.</text>
    </description>
    <leaf name="trunk-index">
      <type name="uint32"/>
      <mandatory value="true"/>
      <description>
        <text>Trunk interface index.</text>
      </description>
    </leaf>
    <leaf name="trunk-name">
      <type name="pub-type:if-name"/>
      <mandatory value="true"/>
      <description>
        <text>Name of the Trunk interface.</text>
      </description>
    </leaf>
  </notification>
  <notification name="all-member-down-resume">
    <description>
      <text>The first member interface link status changes to up.</text>
    </description>
    <leaf name="trunk-index">
      <type name="uint32"/>
      <mandatory value="true"/>
      <description>
        <text>Trunk interface index.</text>
      </description>
    </leaf>
    <leaf name="trunk-name">
      <type name="pub-type:if-name"/>
      <mandatory value="true"/>
      <description>
        <text>Name of the Trunk interface.</text>
      </description>
    </leaf>
  </notification>
  <notification name="all-member-up">
    <description>
      <text>All member links of the trunk went Up.</text>
    </description>
    <leaf name="trunk-name">
      <type name="pub-type:if-name"/>
      <mandatory value="true"/>
      <description>
        <text>Name of the trunk interface.</text>
      </description>
    </leaf>
  </notification>
  <notification name="member-link-down">
    <description>
      <text>A trunk member interface went Down.</text>
    </description>
    <leaf name="port-index">
      <type name="uint32"/>
      <mandatory value="true"/>
      <description>
        <text>Index of a trunk member interface.</text>
      </description>
    </leaf>
    <leaf name="member-admin-status">
      <type name="uint32"/>
      <mandatory value="true"/>
      <description>
        <text>Administration status of a trunk member interface.</text>
      </description>
    </leaf>
    <leaf name="member-oper-status">
      <type name="uint32"/>
      <mandatory value="true"/>
      <description>
        <text>Operating status of a trunk member interface.</text>
      </description>
    </leaf>
    <leaf name="port-name">
      <type name="pub-type:if-name"/>
      <mandatory value="true"/>
      <description>
        <text>Name of a trunk member interface.</text>
      </description>
    </leaf>
    <leaf name="member-phy-status">
      <type name="uint32"/>
      <mandatory value="true"/>
      <description>
        <text>Physical status of a trunk member interface.</text>
      </description>
    </leaf>
    <leaf name="trunk-index">
      <type name="uint32"/>
      <mandatory value="true"/>
      <description>
        <text>Index of the trunk interface to which the member interface belongs.</text>
      </description>
    </leaf>
    <leaf name="trunk-name">
      <type name="pub-type:if-name"/>
      <mandatory value="true"/>
      <description>
        <text>Name of the trunk interface to which the member interface belongs.</text>
      </description>
    </leaf>
  </notification>
  <notification name="member-link-up">
    <description>
      <text>A trunk member interface went Up.</text>
    </description>
    <leaf name="port-index">
      <type name="uint32"/>
      <mandatory value="true"/>
      <description>
        <text>Index of a trunk member interface.</text>
      </description>
    </leaf>
    <leaf name="member-admin-status">
      <type name="uint32"/>
      <mandatory value="true"/>
      <description>
        <text>Administration status of a trunk member interface.</text>
      </description>
    </leaf>
    <leaf name="member-oper-status">
      <type name="uint32"/>
      <mandatory value="true"/>
      <description>
        <text>Operating status of a trunk member interface.</text>
      </description>
    </leaf>
    <leaf name="port-name">
      <type name="pub-type:if-name"/>
      <mandatory value="true"/>
      <description>
        <text>Name of a trunk member interface.</text>
      </description>
    </leaf>
    <leaf name="member-phy-status">
      <type name="uint32"/>
      <mandatory value="true"/>
      <description>
        <text>Physical status of a trunk member interface.</text>
      </description>
    </leaf>
    <leaf name="trunk-index">
      <type name="uint32"/>
      <description>
        <text>Index of a trunk interface.</text>
      </description>
    </leaf>
    <leaf name="trunk-name">
      <type name="pub-type:if-name"/>
      <mandatory value="true"/>
      <description>
        <text>Name of the trunk interface to which the member interface belongs.</text>
      </description>
    </leaf>
  </notification>
  <notification name="member-number-change">
    <description>
      <text>The number of active trunk member interfaces changed.</text>
    </description>
    <leaf name="trunk-name">
      <type name="pub-type:if-name"/>
      <mandatory value="true"/>
      <description>
        <text>Name of the trunk interface.</text>
      </description>
    </leaf>
    <leaf name="port-name">
      <type name="pub-type:if-name"/>
      <mandatory value="true"/>
      <description>
        <text>Name of the trunk member interface.</text>
      </description>
    </leaf>
    <leaf name="trunk-mem-number">
      <type name="uint32"/>
      <mandatory value="true"/>
      <description>
        <text>Number of active trunk member interfaces.</text>
      </description>
    </leaf>
    <leaf name="reason">
      <type name="string">
        <length value="0..127"/>
      </type>
      <mandatory value="true"/>
      <description>
        <text>Reason.</text>
      </description>
    </leaf>
  </notification>
  <notification name="trunk-status-change">
    <description>
      <text>The trunk interface status changes.</text>
    </description>
    <leaf name="trunk-name">
      <type name="pub-type:if-name"/>
      <mandatory value="true"/>
      <description>
        <text>Name of the trunk interface.</text>
      </description>
    </leaf>
    <leaf name="trunk-index">
      <type name="uint32"/>
      <mandatory value="true"/>
      <description>
        <text>Trunk index.</text>
      </description>
    </leaf>
    <leaf name="trunk-admin-status">
      <type name="port-status"/>
      <mandatory value="true"/>
      <description>
        <text>Administration status of trunk interface.</text>
      </description>
    </leaf>
    <leaf name="trunk-status">
      <type name="port-status"/>
      <mandatory value="true"/>
      <description>
        <text>Status of trunk interface.</text>
      </description>
    </leaf>
  </notification>
</module>
