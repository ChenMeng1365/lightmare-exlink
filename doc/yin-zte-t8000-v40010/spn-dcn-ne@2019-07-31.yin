<?xml version="1.0" encoding="UTF-8"?>
<module name="spn-dcn-ne"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:spn-dcn-ne="urn:cmcc:yang:spn-dcn-ne"
        xmlns:yang="urn:ietf:params:xml:ns:yang:ietf-yang-types"
        xmlns:inet="urn:ietf:params:xml:ns:yang:ietf-inet-types">
  <namespace uri="urn:cmcc:yang:spn-dcn-ne"/>
  <prefix value="spn-dcn-ne"/>
  <import module="ietf-yang-types">
    <prefix value="yang"/>
  </import>
  <import module="ietf-inet-types">
    <prefix value="inet"/>
  </import>
  <organization>
    <text>China Mobile Communications Corporation (CMCC)</text>
  </organization>
  <contact>
    <text>wangminxue@chinamobile.com</text>
  </contact>
  <description>
    <text>spn dcn network element management, which gets new network elements and sends to the OMC.</text>
  </description>
  <revision date="2019-07-31">
    <description>
      <text>Initial modeling.</text>
    </description>
  </revision>
  <grouping name="dcn-ne-parameters">
    <description>
      <text>Parameters for DCN-NE.</text>
    </description>
    <leaf name="chassis-id">
      <type name="yang:mac-address"/>
      <description>
        <text>The string value used to identify the chassis component
associated with the local system.</text>
      </description>
    </leaf>
    <leaf name="system-name">
      <type name="string">
        <length value="0..255"/>
      </type>
      <description>
        <text>The string value used to identify the system name of the
local system.  If the local agent supports IETF RFC 3418,
lldpLocSysName object should have the same value of sysName
object.</text>
      </description>
    </leaf>
    <leaf name="system-description">
      <type name="string">
        <length value="0..255"/>
      </type>
      <description>
        <text>The string value used to identify the system description
of the local system.  If the local agent supports IETF RFC 3418,
lldpLocSysDesc object should have the same value of sysDesc
object.</text>
      </description>
    </leaf>
    <leaf name="management-address-ipv6">
      <type name="inet:ipv6-address"/>
      <description>
        <text>Management address for IPv6.
The string value used to identify the management address
component associated with the remote system.  The purpose
of this address is to contact the management entity.</text>
      </description>
    </leaf>
    <leaf name="management-address-ipv4">
      <type name="inet:ipv4-address"/>
      <description>
        <text>Management address for IPv4.
The string value used to identify the management address
component associated with the remote system.  The purpose
of this address is to contact the management entity.</text>
      </description>
    </leaf>
  </grouping>
  <container name="dcn-nes">
    <config value="false"/>
    <list name="dcn-ne">
      <key value="chassis-id"/>
      <uses name="dcn-ne-parameters"/>
    </list>
  </container>
  <container name="dcn-ne-ipv6">
    <leaf name="ipv6-address">
      <type name="inet:ipv6-address"/>
    </leaf>
    <leaf name="prefix-length">
      <type name="uint8">
        <range value="0..128"/>
      </type>
    </leaf>
  </container>
  <notification name="dcn-events-notification">
    <uses name="dcn-ne-parameters"/>
  </notification>
</module>
