  <container name="state">
    <config value="false"/>
    <description>
      <text>The state of CGN.</text>
    </description>
    <container name="cgn">
      <description>
        <text>The CGN state.</text>
      </description>
      <container name="global-counters">
        <description>
          <text>The global counters of CGN.</text>
        </description>
        <leaf name="trans-packet-num">
          <description>
            <text>The number of packets successfully translated.</text>
          </description>
          <type name="uint32"/>
          <default value="0"/>
        </leaf>
        <leaf name="port-deplete-num">
          <description>
            <text>The number of packets discarded due to port resource depletion.</text>
          </description>
          <type name="uint32"/>
          <default value="0"/>
        </leaf>
        <leaf name="quota-exceed-num">
          <description>
            <text>The number of packets discarded because of the quota exceeded.</text>
          </description>
          <type name="uint32"/>
          <default value="0"/>
        </leaf>
        <leaf name="trans-num">
          <description>
            <text>Translations number.</text>
          </description>
          <type name="uint32"/>
          <default value="0"/>
        </leaf>
        <leaf name="ipv4-address-num">
          <description>
            <text>The number of private IPv4 addresses.</text>
          </description>
          <type name="uint32"/>
          <default value="0"/>
        </leaf>
        <leaf name="private-ipv4-num">
          <description>
            <text>The number of private IPv4 addresses.</text>
          </description>
          <type name="uint32"/>
          <default value="0"/>
        </leaf>
      </container>
      <container name="translations-summary">
        <description>
          <text>CGN translations summary.</text>
        </description>
        <container name="total-sum">
          <description>
            <text>CGN translations total number.</text>
          </description>
          <leaf name="static-nat-trans-num">
            <description>
              <text>Static NAT translations number.</text>
            </description>
            <type name="uint32"/>
          </leaf>
          <leaf name="static-pat-trans-num">
            <description>
              <text>Static PAT translations number.</text>
            </description>
            <type name="uint32"/>
          </leaf>
          <leaf name="dynamic-nat-trans-num">
            <description>
              <text>Dynamic NAT translations number.</text>
            </description>
            <type name="uint32"/>
          </leaf>
          <leaf name="dynamic-pat-trans-num">
            <description>
              <text>Dynamic PAT translations number.</text>
            </description>
            <type name="uint32"/>
          </leaf>
          <leaf name="alg-trans-num">
            <description>
              <text>ALG translations number.</text>
            </description>
            <type name="uint32"/>
          </leaf>
          <leaf name="pcp-trans-num">
            <description>
              <text>PCP translations number.</text>
            </description>
            <type name="uint32"/>
          </leaf>
          <leaf name="local-ip-num">
            <description>
              <text>Local IP number.</text>
            </description>
            <type name="uint32"/>
          </leaf>
          <leaf name="total-translations-num">
            <description>
              <text>Total translations number.</text>
            </description>
            <type name="uint32"/>
          </leaf>
        </container>
        <list name="single-cpu-sum">
          <description>
            <text>CGN translations number of one CPU.</text>
          </description>
          <key value="cpu-name"/>
          <leaf name="cpu-name">
            <description>
              <text>CPU Name.</text>
            </description>
            <type name="ncd:cpu"/>
          </leaf>
          <leaf name="static-nat-trans-num">
            <description>
              <text>Static NAT translations number.</text>
            </description>
            <type name="uint32"/>
          </leaf>
          <leaf name="static-pat-trans-num">
