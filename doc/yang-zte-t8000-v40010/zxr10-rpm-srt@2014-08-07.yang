        container route-status {
          description 'The routing status query.' ;
          list view-list {
            description 'The list of routing status query.' ;
            key 'objtype vrfname addr mask nhtype nexthop ifname' ;
            leaf objtype {
              description 'The route type. The "MAIN" means the public route, t'
                        + 'he "VRF" means private route, the "MNG" means mng ro'
                        + 'ute, and the "ALL" means all routes (which is not us'
                        + 'ed now).'
                          ;
              type enumeration {
                enum MAIN {
                  value 1 ;
                  description 'The public static routes.' ;
                }
                enum VRF {
                  value 2 ;
                  description 'The private static routes.' ;
                }
                enum MNG {
                  value 3 ;
                  description 'The mng static routes.' ;
                }
                enum ALL {
                  value 4 ;
                  description 'The all static routes.' ;
                }
              }
            }
            leaf vrfname {
              description 'The name of the private network VPN. The "" means pu'
                        + 'blic route.'
                          ;
              type ncd:string-all {
                length 0..32 ;
              }
            }
            leaf addr {
              description 'The destination address' ;
              type ncd:ipv4-address ;
            }
            leaf mask {
              description 'The mask' ;
              type ncd:ipv4-address ;
            }
            leaf nhtype {
              description 'The nexthop type. The "IP" means configuration only '
                        + 'with nexthop, the "IF" means configuration only with'
                        + ' interface, and the "IFIP" means configuration both '
                        + 'with interface and nexthop.'
                          ;
              type enumeration {
                enum IP {
                  value 1 ;
                  description 'Only has nexthop.' ;
                }
                enum IF {
                  value 2 ;
                  description 'Only has interface.' ;
                }
                enum IFIP {
                  value 3 ;
                  description 'Both have nexthop and interface.' ;
                }
              }
            }
            leaf nexthop {
              description 'The route nexthop.' ;
              type ncd:ipv4-address ;
            }
            leaf ifname {
              description 'The interface name of route.' ;
              type ncd:string-all ;
            }
            leaf route-status {
              description 'The route status. The "VALID" means that route is va'
                        + 'lid, the "INVALID" means that route is invalid, and '
                        + 'the "NOTEXIST" means route is not exist (which is no'
                        + 't used now).'
                          ;
              type enumeration {
                enum VALID {
                  value 0 ;
                  description 'Route is valid.' ;
                }
                enum INVALID {
                  value 1 ;
                  description 'Route is invalid.' ;
                }
                enum NOTEXIST {
                  value 2 ;
                  description 'Route is not exist.' ;
                }
              }
              default VALID ;
            }
            leaf route-tag {
              description 'The route tag, and the default value is 0. The range'
                        + ' is 0-4294967295'
                          ;
              type uint32 {
                range 0..4294967295 ;
              }
              default 0 ;
            }
            leaf is-used-backup {
              description 'The route is backup route or not. The "YES" means ye'
                        + 's, and the "NO" means no.'
                          ;
              type enumeration {
                enum NO {
                  value 0 ;
                  description 'The route is not backup route.' ;
                }
                enum YES {
                  value 1 ;
                  description 'The route is backup route.' ;
                }
              }
              default NO ;
            }
