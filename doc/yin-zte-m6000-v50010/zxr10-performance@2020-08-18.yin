  <feature name="flow-statistics"/>
  <container name="configuration">
    <description>
      <text>Configuration model info.</text>
    </description>
    <container name="performance">
      <description>
        <text>The configuration info about performance.</text>
      </description>
      <container name="statistics">
        <description>
          <text>The configuration info about statistics.</text>
        </description>
        <container name="perf-masks">
          <description>
            <text>The rules for performance mask.</text>
          </description>
          <list name="perf-mask">
            <description>
              <text>The rules for performance mask.</text>
            </description>
            <key value="checkpoint-type tpid-matcher perfcode cycle tpid-mask tpid-class"/>
            <leaf name="checkpoint-type">
              <description>
                <text>The type of tpid. 0xFFFF for any type.</text>
              </description>
              <type name="uint16">
                <range value="1..65535"/>
              </type>
            </leaf>
            <leaf name="tpid-matcher">
              <description>
                <text>The matcher about tpid, the format is the same to tpid. It always to be used with tpid-mask, they can be indicated a group of tpids.</text>
              </description>
              <type name="binary">
                <length value="62..128"/>
              </type>
            </leaf>
            <leaf name="perfcode">
              <description>
                <text>Performance code, one tpid may have serval performance codes. 0xFFFF for any code.</text>
              </description>
              <type name="uint16">
                <range value="1..65535"/>
              </type>
            </leaf>
            <leaf name="cycle">
              <description>
                <text>The performance cycle. 15(5) minutes, 24 hours or all.</text>
              </description>
              <type name="enumeration">
                <enum name="fifteen-minutes">
                  <value value="1"/>
                  <description>
                    <text>Minute cycle. The actual cycle value depends on the minute-cycle configuration.</text>
                  </description>
                </enum>
                <enum name="twenty-four-hours">
                  <value value="2"/>
                </enum>
                <enum name="all">
                  <value value="3"/>
                </enum>
              </type>
            </leaf>
            <leaf name="tpid-mask">
              <description>
                <text>The mask about tpid-matcher, it indicates which position of tpid-matcher is available.</text>
              </description>
              <type name="binary">
                <length value="8..16"/>
              </type>
            </leaf>
            <leaf name="tpid-length">
              <description>
                <text>The length of tpid.</text>
              </description>
              <type name="uint8">
                <range value="62..128"/>
              </type>
            </leaf>
            <leaf name="tpid-class">
              <description>
                <text>The class of TPID.</text>
              </description>
              <type name="enumeration">
                <enum name="board">
                  <value value="1"/>
                </enum>
                <enum name="CPU">
                  <value value="2"/>
                </enum>
                <enum name="system">
                  <value value="3"/>
                </enum>
                <enum name="all">
                  <value value="255"/>
                </enum>
              </type>
            </leaf>
          </list>
        </container>
        <container name="perf-suppresses">
          <description>
            <text>The function about performance suppress.</text>
          </description>
          <list name="perf-suppress">
            <description>
              <text>The function about performance suppress.</text>
            </description>
            <key value="checkpoint-type tpid cycle"/>
            <leaf name="checkpoint-type">
              <description>
                <text>The type of tpid. Only for any tpid type. The value is always 0xFFFF.</text>
              </description>
              <type name="uint16">
                <range value="1..65535"/>
              </type>
            </leaf>
            <leaf name="tpid">
              <description>
                <text>Check point id, only every positon's value is 0xFF can be accepted.</text>
              </description>
              <type name="binary">
                <length value="62..128"/>
              </type>
            </leaf>
            <leaf name="cycle">
              <description>
                <text>Statistics cycle.</text>
              </description>
              <type name="enumeration">
                <enum name="cur15minutes">
                  <value value="1"/>
                  <description>
                    <text>Current minute cycle. The actual cycle value depends on the minute-cycle configuration.</text>
                  </description>
                </enum>
                <enum name="his15minutes">
                  <value value="2"/>
                  <description>
                    <text>History minute cycle. The actual cycle value depends on the minute-cycle configuration.</text>
                  </description>
                </enum>
                <enum name="cur24hours">
                  <value value="3"/>
                </enum>
                <enum name="his24hours">
                  <value value="4"/>
                </enum>
              </type>
            </leaf>
            <leaf name="tpid-length">
              <description>
                <text>The length of TPID.</text>
              </description>
              <type name="uint8">
                <range value="62..128"/>
