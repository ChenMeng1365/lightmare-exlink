  <container name="configuration">
    <description>
      <text>The configuration of PTP.</text>
    </description>
    <container name="ptp">
      <description>
        <text>The configuration of PTP.</text>
      </description>
      <container name="clock-source">
        <description>
          <text>The GPS configuration.</text>
        </description>
        <container name="utc-time">
          <description>
            <text>The PTP global configuration of GPS time.</text>
          </description>
          <leaf name="utc-offset">
            <description>
              <text>The UTC offset,in second.</text>
            </description>
            <type name="uint8">
              <range value="0..255">
                <description>
                  <text>The UTC offset.</text>
                </description>
              </range>
            </type>
            <units name="second"/>
            <default value="35"/>
          </leaf>
          <leaf name="leap-attr">
            <description>
              <text>The leap attribution,leap59/leap61.</text>
            </description>
            <type name="enumeration">
              <enum name="leap59">
                <value value="1"/>
                <description>
                  <text>The leap attribution is leap59.</text>
                </description>
              </enum>
              <enum name="leap61">
                <value value="2"/>
                <description>
                  <text>The leap attribution is leap61.</text>
                </description>
              </enum>
            </type>
          </leaf>
          <leaf name="leap-year">
            <description>
              <text>The leap year,the range is 1980~2100.</text>
            </description>
            <type name="uint16">
              <range value="1980..2100">
                <description>
                  <text>The leap year.</text>
                </description>
              </range>
            </type>
          </leaf>
          <leaf name="leap-month">
            <description>
              <text>The leap month,june/december.</text>
            </description>
            <type name="enumeration">
              <enum name="june">
                <value value="1"/>
                <description>
                  <text>The leap month is june.</text>
                </description>
              </enum>
              <enum name="december">
                <value value="2"/>
                <description>
                  <text>The leap month is december.</text>
                </description>
              </enum>
            </type>
          </leaf>
          <leaf name="utc-trace">
            <description>
              <text>The UTC trace mode,manual/auto.</text>
            </description>
            <type name="enumeration">
              <enum name="manual">
                <value value="1"/>
                <description>
                  <text>The UTC trace mode is manual.</text>
                </description>
              </enum>
              <enum name="auto">
                <value value="2"/>
                <description>
                  <text>The UTC trace mode is auto.</text>
                </description>
              </enum>
            </type>
            <default value="manual"/>
          </leaf>
          <leaf name="utc-auto-holdover">
            <description>
              <text>The holdover time after UTC tod losing,the range is 0~48,in hours.This supports in auto trace mode only.</text>
            </description>
            <type name="uint8">
              <range value="0..48">
                <description>
                  <text>The holdover time after UTC tod losing.</text>
                </description>
              </range>
            </type>
            <units name="hour"/>
          </leaf>
        </container>
        <container name="time-source-lists">
          <description>
            <text>The time source list configuration.</text>
          </description>
          <list name="time-source">
            <description>
              <text>The configuration of time source.</text>
            </description>
            <key value="ifname"/>
            <leaf name="ifname">
              <description>
                <text>The interface name of time source.</text>
              </description>
              <type name="ncd:ifname"/>
            </leaf>
            <leaf name="priority1">
              <description>
                <text>The priority1 of time source,the range is 0~255.</text>
              </description>
              <type name="uint8">
                <range value="0..255">
                  <description>
                    <text>The priority1 of the time source.</text>
                  </description>
                </range>
              </type>
              <default value="128"/>
            </leaf>
            <leaf name="priority2">
              <description>
                <text>The priority2 of time source,the range is 0~255.</text>
              </description>
              <type name="uint8">
                <range value="0..255">
                  <description>
                    <text>The priority2 of the time source.</text>
                  </description>
                </range>
              </type>
              <default value="128"/>
            </leaf>
            <leaf name="local-priority">
              <description>
                <text>The local priority of time source.</text>
              </description>
              <type name="uint8">
                <range value="1..255">
                  <description>
                    <text>The local priority of time source.</text>
                  </description>
                </range>
              </type>
              <default value="128"/>
            </leaf>
            <leaf name="grandmaster-id">
              <description>
                <text>The grandmasterIdentity of time source.</text>
              </description>
              <type name="string">
                <length value="0..16">
                  <description>
                    <text>The grandmasterIdentity of time source(0~9,A~F).</text>
                  </description>
                </length>
              </type>
              <default value=""/>
            </leaf>
            <leaf name="stepsremoved">
              <description>
                <text>The steps removed from grandmaster of time source.</text>
              </description>
              <type name="uint8">
                <range value="0..254">
                  <description>
                    <text>The steps removed from grandmaster of time source.</text>
                  </description>
                </range>
              </type>
              <default value="0"/>
            </leaf>
            <leaf name="offsetscaledlogvariance">
              <description>
                <text>The offset scaled log variance of time source.</text>
              </description>
              <type name="uint16">
                <range value="0..65535">
                  <description>
                    <text>The offset scaled log variance of time source.</text>
                  </description>
                </range>
