  <feature name="vxlan"/>
  <feature name="srv6"/>
  <feature name="uni-nni-trace">
    <description>
      <text>UNI and NNI trace</text>
    </description>
  </feature>
  <feature name="uni-nni-trace-ipv6">
    <description>
      <text>UNI and NNI trace IPv6</text>
    </description>
  </feature>
  <container name="operation">
    <config value="false"/>
    <description>
      <text>TRACE operation model info.</text>
    </description>
    <container name="trace">
      <description>
        <text>TRACE operation parameters.</text>
      </description>
      <presence value="trace"/>
      <choice name="ip-type">
        <description>
          <text>The type of TRACE operation, IPV4 or IPV6.</text>
        </description>
        <mandatory value="true"/>
        <case name="ipv4">
          <description>
            <text>IPv4 TRACE operation parameters.</text>
          </description>
          <container name="trace4s">
            <description>
              <text>IPv4 TRACE operation parameters.</text>
            </description>
            <list name="trace4">
              <description>
                <text>IPv4 TRACE operation parameters.</text>
              </description>
              <key value="serial-num"/>
              <leaf name="serial-num">
                <description>
                  <text>IPv4 TRACE serial number.</text>
                </description>
                <type name="uint64"/>
              </leaf>
              <choice name="status">
                <description>
                  <text>The switch for turning on/off IPv4 TRACE.</text>
                </description>
                <mandatory value="true"/>
                <case name="enable">
                  <description>
                    <text>Start IPv4 TRACE function.</text>
                  </description>
                  <leaf name="trace-enable">
                    <description>
                      <text>Start IPv4 TRACE function.</text>
                    </description>
                    <type name="empty"/>
                    <mandatory value="true"/>
                  </leaf>
                  <container name="trace-data">
                    <description>
                      <text>Parameters for IPv4 TRACE.</text>
                    </description>
                    <choice name="trace4-type">
                      <description>
                        <text>The type of IPv4 TRACE.</text>
                      </description>
                      <mandatory value="true"/>
                      <case name="normal">
                        <description>
                          <text>Normal IP TRACE operation.</text>
                        </description>
                        <container name="normals">
                          <description>
                            <text>Normal IP TRACE operation.</text>
                          </description>
                          <choice name="dest-type">
                            <description>
                              <text>The type of normal TRACE.</text>
                            </description>
                            <mandatory value="true"/>
                            <case name="ip">
                              <description>
                                <text>The destination of TRACE is IP address.</text>
                              </description>
                              <leaf name="dst-address">
                                <description>
                                  <text>The destination IPv4 address.</text>
                                </description>
                                <type name="ncd:ipv4-address"/>
                                <mandatory value="true"/>
                              </leaf>
                            </case>
                            <case name="domain">
                              <description>
                                <text>The destination of TRACE is domain name.</text>
                              </description>
                              <leaf name="domain-name">
                                <description>
                                  <text>The domain name.</text>
                                </description>
                                <type name="string">
                                  <length value="1..128">
                                    <description>
                                      <text>The domain name.</text>
                                    </description>
                                  </length>
                                </type>
                                <mandatory value="true"/>
                              </leaf>
                            </case>
                          </choice>
                        </container>
                        <container name="normal-opt">
                          <description>
                            <text>Option parameters for normal TRACE.</text>
                          </description>
                          <choice name="vrf">
                            <description>
                              <text>The VRF type.</text>
                            </description>
                            <case name="normal-vrf">
                              <description>
                                <text>The normal VRF.</text>
                              </description>
                              <leaf name="vrf-name">
                                <description>
                                  <text>The VRF name.</text>
                                </description>
                                <type name="string">
                                  <length value="1..32">
                                    <description>
                                      <text>The VRF name.</text>
                                    </description>
                                  </length>
                                </type>
                              </leaf>
                            </case>
                            <case name="dcn-vrf">
                              <description>
                                <text>The DCN VRF.</text>
                              </description>
                              <leaf name="dcn">
                                <description>
                                  <text>Whether DCN VRF or not.</text>
                                </description>
                                <type name="empty"/>
                              </leaf>
                            </case>
                          </choice>
                          <leaf name="out-if">
                            <description>
                              <text>The interface of sending packets.</text>
                            </description>
                            <type name="ncd:ifname">
                              <length value="1..31">
                                <description>
                                  <text>The interface of sending packets.</text>
                                </description>
                              </length>
                            </type>
                          </leaf>
                          <leaf name="source">
                            <description>
                              <text>The source IP address of the packet.</text>
                            </description>
                            <type name="ncd:ipv4-address"/>
                          </leaf>
                          <choice name="next-hop-option">
                            <description>
                              <text>The next hop address.</text>
                            </description>
                            <case name="ipv4-next-hop">
                              <description>
                                <text>The next hop address.</text>
                              </description>
                              <leaf name="ipv4-next-hop">
                                <description>
                                  <text>The Ipv4 next hop address.</text>
                                </description>
                                <type name="ncd:ipv4-address-none-zero"/>
                              </leaf>
                            </case>
                            <case name="ipv6-next-hop">
                              <description>
                                <text>The next hop address.</text>
                              </description>
                              <leaf name="ipv6-next-hop">
                                <description>
                                  <text>The Ipv6 next hop address.</text>
                                </description>
                                <type name="ncd:ipv6-address-none-zero"/>
                              </leaf>
                            </case>
                            <ncd:revise-date date="2021-03-03"/>
                          </choice>
                          <leaf name="size">
                            <description>
                              <text>The packet size.</text>
                            </description>
                            <type name="int16">
                              <range value="36..8192">
                                <description>
                                  <text>The packet size, include IP header.</text>
                                </description>
                              </range>
                            </type>
                            <default value="100"/>
                          </leaf>
                          <leaf name="ttl-mode">
                            <description>
                              <text>The TTL mode.</text>
                            </description>
                            <type name="enumeration">
                              <enum name="uniform">
                                <value value="0"/>
                                <description>
                                  <text>Uniform TTL mode.</text>
                                </description>
                              </enum>
                              <enum name="pipe">
                                <value value="1"/>
                                <description>
                                  <text>Pipe TTL mode.</text>
                                </description>
                              </enum>
                            </type>
                            <default value="uniform"/>
                          </leaf>
                        </container>
                      </case>
                      <case name="uni">
                        <description>
                          <text>UNI-TRACE.</text>
                        </description>
                        <container name="unis">
                          <description>
                            <text>UNI-TRACE.</text>
                          </description>
                          <leaf name="dst-address">
                            <description>
                              <text>The destination IPv4 address.</text>
                            </description>
                            <type name="ncd:ipv4-address"/>
                            <mandatory value="true"/>
                          </leaf>
                          <leaf name="vrf-name">
                            <description>
                              <text>The VRF name.</text>
                            </description>
                            <type name="string">
                              <length value="1..32">
                                <description>
                                  <text>The VRF name.</text>
                                </description>
                              </length>
                            </type>
                          </leaf>
                        </container>
                      </case>
                      <case name="nni">
                        <description>
                          <text>NNI-TRACE.</text>
                        </description>
                        <container name="nnis">
                          <description>
                            <text>NNI-TRACE.</text>
                          </description>
                          <leaf name="dst-address">
                            <description>
                              <text>The destination IPv4 address.</text>
                            </description>
                            <type name="ncd:ipv4-address"/>
                            <mandatory value="true"/>
                          </leaf>
                          <leaf name="next-hop">
                            <description>
                              <text>The next hop address for NNI-TRACE.</text>
                            </description>
                            <type name="ncd:ipv4-address"/>
                            <mandatory value="true"/>
                          </leaf>
                          <leaf name="vrf-name">
                            <description>
                              <text>The VRF name.</text>
                            </description>
                            <type name="string">
                              <length value="1..32">
                                <description>
                                  <text>The VRF name.</text>
                                </description>
                              </length>
                            </type>
                          </leaf>
                        </container>
                      </case>
                      <case name="vxlan">
                        <description>
                          <text>VXLAN-TRACE.</text>
                        </description>
                        <container name="vxlans">
                          <description>
                            <text>VXLAN-TRACE.</text>
                          </description>
                          <leaf name="dst-address">
                            <description>
                              <text>The destination IPv4 address.</text>
                            </description>
                            <type name="ncd:ipv4-address"/>
                            <mandatory value="true"/>
                          </leaf>
                          <leaf name="vrf-name">
                            <description>
                              <text>The VRF name.</text>
                            </description>
                            <type name="string">
                              <length value="1..32">
                                <description>
                                  <text>The VRF name.</text>
                                </description>
                              </length>
                            </type>
                          </leaf>
                        </container>
                        <container name="vxlan-option">
                          <description>
                            <text>Optional parameters of VXLAN TRACE.</text>
                          </description>
                          <leaf name="source">
                            <description>
                              <text>The source IP address of the packet.</text>
                            </description>
                            <type name="ncd:ipv4-address"/>
                          </leaf>
                          <leaf name="size">
                            <description>
                              <text>The packet size.</text>
                            </description>
                            <type name="int16">
                              <range value="36..8192">
                                <description>
                                  <text>The packet size, include IP header.</text>
                                </description>
                              </range>
                            </type>
                            <default value="100"/>
                          </leaf>
                          <leaf name="ttl-mode">
                            <description>
                              <text>The TTL mode.</text>
                            </description>
                            <type name="enumeration">
                              <enum name="uniform">
                                <value value="0"/>
                                <description>
                                  <text>Uniform TTL mode.</text>
                                </description>
                              </enum>
                              <enum name="pipe">
                                <value value="1"/>
                                <description>
                                  <text>Pipe TTL mode.</text>
                                </description>
                              </enum>
                            </type>
                            <default value="uniform"/>
                          </leaf>
                          <choice name="underlay">
                            <description>
                              <text>underlay VXLAN information.</text>
                            </description>
                            <case name="dynamic">
                              <description>
                                <text>Dynamic VXLAN Tunnel.</text>
                              </description>
                              <leaf name="local-ip">
                                <description>
                                  <text>The local IPv4 address.</text>
                                </description>
                                <type name="ncd:ipv4-address"/>
                                <mandatory value="true"/>
                              </leaf>
                              <leaf name="remote-ip">
                                <description>
                                  <text>The remote IPv4 address.</text>
                                </description>
                                <type name="ncd:ipv4-address"/>
                                <mandatory value="true"/>
                              </leaf>
                            </case>
                            <case name="static">
                              <description>
                                <text>Static VXLAN Tunnel.</text>
                              </description>
                              <leaf name="vxlan-interface">
                                <description>
                                  <text>The VXLAN interface of sending packets.</text>
                                </description>
                                <type name="ncd:ifname">
                                  <length value="1..31">
                                    <description>
                                      <text>The VXLAN interface of sending packets.</text>
                                    </description>
                                  </length>
                                </type>
                              </leaf>
                            </case>
                            <case name="ipv6-dynamic">
                              <description>
                                <text>Dynamic VXLAN IPv6 address.</text>
                              </description>
                              <container name="ipv6-dynamics">
                                <description>
                                  <text>Dynamic VXLAN IPv6 address.</text>
                                </description>
                                <leaf name="ipv6-local">
                                  <description>
                                    <text>The local IPv6 address.</text>
                                  </description>
                                  <type name="ncd:ipv6-address"/>
                                  <mandatory value="true"/>
                                </leaf>
                                <leaf name="ipv6-remote">
                                  <description>
                                    <text>The remote IPv6 address.</text>
                                  </description>
                                  <type name="ncd:ipv6-address"/>
                                  <mandatory value="true"/>
                                </leaf>
                              </container>
                              <ncd:revise-date date="2020-04-07"/>
                            </case>
                          </choice>
                        </container>
                        <ncd:revise-date date="2020-02-22"/>
                      </case>
                    </choice>
                  </container>
                  <container name="common-opt">
                    <description>
                      <text>Option parameters for IPv4 TRACE.</text>
                    </description>
                    <leaf name="max-ttl">
                      <description>
                        <text>The maximum of time to live.</text>
                      </description>
                      <type name="uint8">
                        <range value="1..255">
                          <description>
                            <text>The maximum of time to live.</text>
                          </description>
                        </range>
                      </type>
                      <default value="30"/>
                    </leaf>
                    <leaf name="time-out">
                      <description>
                        <text>Timeout.</text>
                      </description>
                      <type name="uint8">
                        <range value="1..20">
                          <description>
                            <text>Timeout.</text>
                          </description>
                        </range>
                      </type>
                      <default value="3"/>
                    </leaf>
                    <leaf name="pattern">
                      <description>
                        <text>The pad bytes.</text>
                      </description>
                      <type name="string">
                        <length value="4">
                          <description>
                            <text>The pattern string.</text>
                          </description>
                        </length>
                      </type>
                      <ncd:revise-date date="2019-07-10"/>
                    </leaf>
                    <choice name="pkt-priority">
                      <description>
                        <text>The priority of sending packets.</text>
                      </description>
                      <case name="dscp">
                        <description>
                          <text>Differentiated Services Code Point.</text>
                        </description>
                        <leaf name="dscp">
                          <description>
                            <text>Differentiated Services Code Point.</text>
                          </description>
                          <type name="uint8">
                            <range value="0..63">
                              <description>
                                <text>Differentiated Services Code Point.</text>
                              </description>
                            </range>
                          </type>
                        </leaf>
                      </case>
                      <case name="tos">
                        <description>
                          <text>Type of service.</text>
                        </description>
                        <leaf name="tos">
                          <description>
                            <text>Type of services.</text>
                          </description>
                          <type name="uint8">
                            <range value="0..255">
                              <description>
                                <text>Type of services.</text>
                              </description>
                            </range>
                          </type>
