  |     |     +--rw status?                enumeration
  |     |     +--rw period?                uint8
  |     |     +--rw detect-policy
  |     |     |  +--rw (policy)?
  |     |     |     +--:(block)
  |     |     |     |  +--rw block-policy         empty
  |     |     |     |  +--rw diagnosis-time?      uint32
  |     |     |     +--:(normal)
