            description 'Specify syslog-server host of IPv6 without linklocal.'
                        ;
            key ipv6-address ;
            leaf ipv6-address {
              description 'IPv6 address.'
                          ;
              type ncd:ipv6-address-none-zero ;
            }
            container ssl {
              description 'SSL name and PKI profile name.'
                          ;
              presence 'ssl-contex mandatory'
                       ;
              leaf ssl-context {
                description 'SSL name.'
                            ;
                type string {
                  length 1..31 ;
                }
                mandatory true ;
              }
              leaf pki-profile {
                description 'PKI profile name.'
                            ;
                type string {
                  length 1..63 ;
                }
              }
            }
            container src-ip {
              description 'Source IPv6 address.'
                          ;
              leaf src-ipv6 {
                description 'Source IPv6 address.'
                            ;
                type ncd:ipv6-address-none-zero ;
              }
            }
            leaf fport {
              description 'The remote port.'
                          ;
              type uint16 {
                range 1..65535 ;
              }
              default 514 ;
            }
            leaf alarmlog {
              description 'Send alarm log to syslog server.'
                          ;
              type syslog-log-switch ;
              default off ;
            }
            leaf cmdlog {
              description 'Send command log to syslog server.'
                          ;
              type syslog-log-switch ;
              default off ;
            }
            leaf debugmsg {
              description 'Send debug message to syslog server.'
                          ;
              type syslog-log-switch ;
              default off ;
            }
            leaf servicelog {
              description 'Send service log to syslog server.'
                          ;
              type syslog-log-switch ;
              default off ;
            }
            leaf braslog {
              description 'Send bras log to syslog server.'
                          ;
              type syslog-log-switch ;
              default off ;
            }
            leaf netconflog {
              description 'Send NETCONF log to syslog server.'
                          ;
              type syslog-log-switch ;
              default off ;
            }
            leaf snmplog {
              description 'Send SNMP log to syslog server.'
                          ;
              type syslog-log-switch ;
              default off ;
            }
            leaf weblog {
              description 'Send WEB log to syslog server.'
                          ;
              type syslog-log-switch ;
              default off ;
            }
            leaf traplog {
              description 'Send TRAP log to syslog server.'
                          ;
              type syslog-log-switch ;
              default off ;
              ncd:revise-date 2021-01-18 ;
            }
          }
        }
        ncd:revise-date 2020-07-28 ;
      }
      leaf facility {
        description 'Facility parameter for syslog messages.'
                    ;
        type syslog-facility ;
        default local0 ;
      }
      leaf level {
        description 'Set syslog logging level.'
                    ;
        type alarm-level ;
        default notifications ;
      }
    }
    container syslog {
      description 'Specify SYSLOG.'
                  ;
      container format {
        description 'Specify the format of SYSLOG.'
                    ;
        leaf format {
          if-feature syslog-format ;
          description 'Syslog format.'
                      ;
          type syslog-format ;
          default rfc3164 ;
        }
      }
      ncd:revise-date 2020-05-21 ;
    }
  }
}
