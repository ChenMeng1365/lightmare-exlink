  <ext:task-name value="forwarding"/>
  <augment target-node="/evpn:evpn">
    <description>
      <text>EVPN configuration.</text>
    </description>
    <container name="system">
      <description>
        <text>Configure the system configuration of evpn.</text>
      </description>
      <container name="port-select">
        <description>
          <text>Configure the port selection abort EVPN accessing VLL.</text>
        </description>
        <leaf name="flag">
          <type name="boolean"/>
          <default value="true"/>
          <description>
            <text>Enable/disable the coupling flag in the EVPN-accessing-VLL direction.</text>
          </description>
        </leaf>
      </container>
      <container name="self-board">
        <description>
          <text>Configure evpn reserve-interface enhancement.</text>
        </description>
        <leaf name="flag">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable the flag to use the internal GRE reserved interfaces to implement public-network-and-private-network decoupling in the BUM forwarding process.</text>
          </description>
        </leaf>
      </container>
      <container name="evpn-udp-port">
        <description>
          <text>Configure the UDP port.</text>
        </description>
        <leaf name="port-id">
          <type name="uint32">
            <range value="1025..65535"/>
          </type>
          <description>
            <text>Specifies a port number for a UDP interface.</text>
          </description>
        </leaf>
      </container>
    </container>
  </augment>
  <augment target-node="/evpn:evpn/evpn:instances/evpn:instance/evpn:normal-evpn">
    <description>
      <text>Normal evpn pipe configuration.</text>
    </description>
    <container name="pipe">
      <description>
        <text>Configure the diffserv mode of evpn.</text>
      </description>
      <leaf name="split-mode">
        <type name="boolean"/>
        <must condition="((../split-mode='false' and not(../ingress-pipe-mode) and not(../egress-pipe-mode) and ../pipe-mode) or (../split-mode='true' and not(../pipe-mode) and ../ingress-pipe-mode and ../egress-pipe-mode and not(../ingress-pipe-mode='uniform' and ../egress-pipe-mode='uniform')))"/>
        <default value="false"/>
        <description>
          <text>Enable/disable split mode.</text>
        </description>
      </leaf>
      <leaf name="pipe-mode">
        <when condition="../split-mode='false'"/>
        <type name="evpn-pipe-mode"/>
        <must condition="(../split-mode='false') and ((../pipe-mode='pipe' and (../service-class) and (../color)) or (../pipe-mode='shortpipe' and (../service-class) and (../color) and (../domain-name)) or (../pipe-mode='uniform'))"/>
        <default value="uniform"/>
        <description>
          <text>Specify the pipe mode.</text>
        </description>
      </leaf>
      <leaf name="ingress-pipe-mode">
        <when condition="../split-mode='true'"/>
        <type name="evpn-pipe-mode"/>
        <must condition="((../split-mode='true') and (((../service-class) and (../color) and (../ingress-pipe-mode='shortpipe' or ../ingress-pipe-mode='pipe')) or (../ingress-pipe-mode='uniform' and ../egress-pipe-mode!='uniform')))"/>
        <default value="uniform"/>
        <description>
          <text>Specify the ingress pipe mode.</text>
        </description>
      </leaf>
      <leaf name="egress-pipe-mode">
        <when condition="../split-mode='true'"/>
        <type name="evpn-pipe-mode"/>
        <must condition="((../split-mode='true') and (((../domain-name) and (../trust-type) and (../egress-pipe-mode='shortpipe')) or (../egress-pipe-mode='pipe') or (../egress-pipe-mode='uniform' and ../ingress-pipe-mode!='uniform')))"/>
        <default value="uniform"/>
        <description>
          <text>Specify the egress pipe mode.</text>
        </description>
      </leaf>
      <leaf name="service-class">
        <when condition="(../pipe-mode='pipe' or ../pipe-mode='shortpipe' or ../ingress-pipe-mode='pipe' or ../ingress-pipe-mode='shortpipe')"/>
        <type name="evpn-service-class"/>
        <must condition="(../pipe-mode='pipe' or ../pipe-mode='shortpipe' or ../ingress-pipe-mode='pipe' or ../ingress-pipe-mode='shortpipe')"/>
        <default value="be"/>
        <description>
          <text>Specify the CoS.</text>
        </description>
      </leaf>
      <leaf name="color">
        <when condition="(../pipe-mode='pipe' or ../pipe-mode='shortpipe' or ../ingress-pipe-mode='pipe' or ../ingress-pipe-mode='shortpipe')"/>
        <type name="evpn-color"/>
        <must condition="(../pipe-mode='pipe' or ../pipe-mode='shortpipe' or ../ingress-pipe-mode='pipe' or ../ingress-pipe-mode='shortpipe')"/>
        <default value="green"/>
        <description>
          <text>Specify the color of packets.</text>
        </description>
      </leaf>
      <leaf name="domain-name">
        <when condition="(../pipe-mode='shortpipe' or ../egress-pipe-mode='shortpipe')"/>
        <type name="leafref">
          <path value="/qos:qos/qos:classifier-template/qos:diffserv-domains/qos:diffserv-domain/qos:name"/>
        </type>
        <must condition="(../pipe-mode='shortpipe' or ../egress-pipe-mode='shortpipe')"/>
        <default value="default"/>
        <description>
          <text>DS domain name.</text>
        </description>
      </leaf>
      <leaf name="trust-type">
        <when condition="../egress-pipe-mode='shortpipe'"/>
        <type name="evpn-trust-type"/>
        <must condition="../egress-pipe-mode='shortpipe'"/>
        <default value="outpri"/>
        <description>
          <text>Specify the trust type.</text>
        </description>
      </leaf>
    </container>
    <container name="reserve-if-switch">
      <description>
        <text>Configure EVPN broadcast fast switching.</text>
      </description>
      <leaf name="enable">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable the flag of EVPN broadcast fast switching.</text>
        </description>
      </leaf>
    </container>
  </augment>
