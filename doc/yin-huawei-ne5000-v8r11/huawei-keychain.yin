<?xml version="1.0" encoding="UTF-8"?>
<module name="huawei-keychain"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:keychain="urn:huawei:yang:huawei-keychain"
        xmlns:pub-type="urn:huawei:yang:huawei-pub-type"
        xmlns:ext="urn:huawei:yang:huawei-extension">
  <yang-version value="1"/>
  <namespace uri="urn:huawei:yang:huawei-keychain"/>
  <prefix value="keychain"/>
  <import module="huawei-pub-type">
    <prefix value="pub-type"/>
  </import>
  <import module="huawei-extension">
    <prefix value="ext"/>
  </import>
  <organization>
    <text>Huawei Technologies Co., Ltd.</text>
  </organization>
  <contact>
    <text>Huawei Industrial Base
Bantian, Longgang
Shenzhen 518129
People's Republic of China
Website: http://www.huawei.com
Email: support@huawei.com</text>
  </contact>
  <description>
    <text>Keychain.</text>
  </description>
  <revision date="2020-04-19">
    <description>
      <text>Init revision.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <ext:task-name value="key-chain"/>
  <typedef name="keychain-mode">
    <type name="enumeration">
      <enum name="absolute">
        <value value="5"/>
        <description>
          <text>Absolute time mode.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Timing mode.</text>
    </description>
  </typedef>
  <typedef name="keychain-time-zone">
    <type name="enumeration">
      <enum name="UTC">
        <value value="1"/>
        <description>
          <text>UTC time zone.</text>
        </description>
      </enum>
      <enum name="LMT">
        <value value="0"/>
        <description>
          <text>LMT time zone.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Time Zone.</text>
    </description>
  </typedef>
  <typedef name="keychain-algorithm-type">
    <type name="enumeration">
      <enum name="md5">
        <value value="1"/>
        <description>
          <text>MD5 algorithm.</text>
        </description>
      </enum>
      <enum name="sha-1">
        <value value="2"/>
        <description>
          <text>SHA-1 algorithm.</text>
        </description>
      </enum>
      <enum name="hmac-md5">
        <value value="3"/>
        <description>
          <text>HMAC-MD5 algorithm.</text>
        </description>
      </enum>
      <enum name="hmac-sha1-12">
        <value value="4"/>
        <description>
          <text>HMAC-SHA1-12 algorithm.</text>
        </description>
      </enum>
      <enum name="hmac-sha1-20">
        <value value="5"/>
        <description>
          <text>HMAC-SHA1-20 algorithm.</text>
        </description>
      </enum>
      <enum name="hmac-sha-256">
        <value value="6"/>
        <description>
          <text>HMAC-SHA-256 algorithm.</text>
        </description>
      </enum>
      <enum name="sha-256">
        <value value="7"/>
        <description>
          <text>SHA-256 algorithm.</text>
        </description>
      </enum>
      <enum name="sm3">
        <value value="8"/>
        <description>
          <text>SM3 algorithm.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Algorithm configuration for that Key-ID.</text>
    </description>
  </typedef>
  <typedef name="keychain-key-id-status">
    <type name="enumeration">
      <enum name="active">
        <value value="0"/>
        <description>
          <text>Active.</text>
        </description>
      </enum>
      <enum name="inactive">
        <value value="1"/>
        <description>
          <text>Inactive.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Status of key ID.</text>
    </description>
  </typedef>
  <typedef name="keychain-dig-len-16or32">
    <type name="enumeration">
      <enum name="16">
        <value value="16"/>
        <description>
          <text>16 Byte..</text>
        </description>
      </enum>
      <enum name="32">
        <value value="32"/>
        <description>
          <text>32 Byte..</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Digest length 16 or 32 byte.</text>
    </description>
  </typedef>
  <typedef name="keychain-dig-len-16or20">
    <type name="enumeration">
      <enum name="16">
        <value value="16"/>
        <description>
          <text>16 Byte..</text>
        </description>
      </enum>
      <enum name="20">
        <value value="20"/>
        <description>
          <text>20 Byte..</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Digest length 16 or 20 byte.</text>
    </description>
  </typedef>
  <container name="keychain">
    <description>
      <text>Keychain.</text>
    </description>
    <container name="keychains">
      <description>
        <text>List of keychain instances.</text>
      </description>
      <list name="keychain">
        <key value="name"/>
        <max-elements value="5000"/>
        <description>
          <text>Configure keychain instance.</text>
        </description>
        <leaf name="name">
          <type name="string">
            <length value="1..47"/>
            <pattern value="[^A-Z]+"/>
          </type>
          <ext:case-sensitivity value="lower-only"/>
          <description>
            <text>The name of keychain instance.</text>
          </description>
        </leaf>
        <leaf name="kc-mode">
          <type name="keychain-mode"/>
          <mandatory value="true"/>
          <description>
            <text>The time mode of keychain instance.</text>
          </description>
          <ext:support-filter value="true"/>
        </leaf>
        <leaf name="time-zone">
          <type name="keychain-time-zone"/>
          <default value="LMT"/>
          <description>
            <text>The time zone of keychain instance.</text>
          </description>
          <ext:support-filter value="true"/>
        </leaf>
        <leaf name="digest-len-sha256">
          <type name="keychain-dig-len-16or32"/>
          <default value="32"/>
          <description>
            <text>The digest length of SHA-256 algorithm.</text>
          </description>
          <ext:support-filter value="true"/>
        </leaf>
        <leaf name="digest-len-hmac-sha256">
          <type name="keychain-dig-len-16or32"/>
          <default value="32"/>
          <description>
            <text>The digest length of HMAC-SHA-256 algorithm.</text>
          </description>
          <ext:support-filter value="true"/>
        </leaf>
        <leaf name="digest-len-hmac-sha120">
          <type name="keychain-dig-len-16or20"/>
          <default value="20"/>
          <description>
            <text>The digest length of HMAC-SHA1-20 algorithm.</text>
          </description>
          <ext:support-filter value="true"/>
        </leaf>
        <leaf name="recv-tolerance">
          <type name="uint32">
            <range value="0..14400"/>
          </type>
          <units name="min"/>
          <description>
            <text>The receive tolerance value in minutes for a keychain, 0 means that time is infinite.</text>
          </description>
          <ext:support-filter value="true"/>
        </leaf>
        <container name="kc-tcp">
          <description>
            <text>Configure the parameter for TCP enhanced authentication option. The corresponding keychain must have been created.</text>
          </description>
          <leaf name="tcp-kind">
            <type name="uint32">
              <range value="28..255"/>
            </type>
            <default value="254"/>
            <description>
              <text>The value of TCP kind.</text>
            </description>
            <ext:support-filter value="true"/>
          </leaf>
          <leaf name="tcp-md5-alg-id">
            <type name="uint32">
              <range value="1..63"/>
            </type>
            <default value="3"/>
            <description>
              <text>TCP algorithm Id of MD5.</text>
            </description>
            <ext:support-filter value="true"/>
          </leaf>
          <leaf name="tcp-sha1-alg-id">
            <type name="uint32">
              <range value="1..63"/>
            </type>
            <default value="4"/>
            <description>
              <text>TCP algorithm Id of SHA-1.</text>
            </description>
            <ext:support-filter value="true"/>
          </leaf>
          <leaf name="tcp-hmac-md5-alg-id">
            <type name="uint32">
              <range value="1..63"/>
            </type>
            <default value="5"/>
            <description>
              <text>TCP algorithm Id of HMAC-MD5.</text>
            </description>
            <ext:support-filter value="true"/>
          </leaf>
          <leaf name="tcp-hmac-sha112-id">
            <type name="uint32">
              <range value="1..63"/>
            </type>
            <default value="2"/>
            <description>
              <text>TCP algorithm Id of HMAC-SHA1-12.</text>
            </description>
            <ext:support-filter value="true"/>
          </leaf>
          <leaf name="tcp-hmac-sha120-id">
            <type name="uint32">
              <range value="1..63"/>
            </type>
            <default value="6"/>
            <description>
              <text>TCP algorithm Id of HMAC-SHA1-20.</text>
            </description>
            <ext:support-filter value="true"/>
          </leaf>
          <leaf name="tcp-hmac-sha256-id">
            <type name="uint32">
              <range value="1..63"/>
            </type>
            <default value="7"/>
            <description>
              <text>TCP algorithm Id of HMAC-SHA-256.</text>
            </description>
            <ext:support-filter value="true"/>
          </leaf>
          <leaf name="tcp-sha256-alg-id">
            <type name="uint32">
              <range value="1..63"/>
            </type>
            <default value="8"/>
            <description>
              <text>TCP algorithm Id of SHA-256.</text>
            </description>
            <ext:support-filter value="true"/>
          </leaf>
          <leaf name="tcp-sm3-alg-id">
            <type name="uint32">
              <range value="1..63"/>
            </type>
            <default value="9"/>
            <description>
              <text>TCP algorithm Id of SM3.</text>
            </description>
            <ext:support-filter value="true"/>
          </leaf>
        </container>
        <container name="abs-key-ids">
          <when condition="../kc-mode='absolute'"/>
          <description>
            <text>List of absolute time mode key-id.</text>
          </description>
          <list name="abs-key-id">
            <key value="key-id"/>
            <max-elements value="64"/>
            <description>
              <text>Configure absolute time mode key-id instance. The corresponding keychain must have been created.</text>
            </description>
            <leaf name="key-id">
              <type name="uint32">
                <range value="0..63"/>
              </type>
              <description>
                <text>ID of key-id.</text>
              </description>
            </leaf>
            <leaf name="algorithm">
              <type name="keychain-algorithm-type"/>
              <description>
                <text>The algorithm that should be associated to the key-id.</text>
              </description>
              <ext:support-filter value="true"/>
            </leaf>
            <leaf name="cipher-string">
              <type name="pub-type:password-extend">
                <length value="1..432"/>
              </type>
              <description>
                <text>Authentication password of key-id.</text>
              </description>
            </leaf>
            <leaf name="default-send-key-id">
              <type name="boolean"/>
              <default value="false"/>
              <description>
                <text>Enable/disable the default send key-id of keychain instance. A keychain instance can have only one key-id as the default send key-id.</text>
              </description>
              <ext:support-filter value="true"/>
            </leaf>
            <leaf name="abs-send-start-date">
              <type name="pub-type:date"/>
              <must condition="../abs-send-start-time and (../send-duration or (../abs-send-end-date and ../abs-send-end-time))"/>
              <description>
                <text>Date for the key-id start to be send activity, the combination of abs-send-start-date and abs-send-start-time must be earlier than that of abs-send-end-date and abs-send-end-time.</text>
              </description>
              <ext:support-filter value="true"/>
            </leaf>
            <leaf name="abs-send-start-time">
              <type name="pub-type:time-hhmm"/>
              <must condition="../abs-send-start-date and (../send-duration or (../abs-send-end-date and ../abs-send-end-time))"/>
              <description>
                <text>Time for the key-id start to be send activity, the combination of abs-send-start-date and abs-send-start-time must be earlier than that of abs-send-end-date and abs-send-end-time.</text>
              </description>
              <ext:support-filter value="true"/>
            </leaf>
            <leaf name="abs-recv-start-date">
              <type name="pub-type:date"/>
              <must condition="../abs-recv-start-time and (../recv-duration or (../abs-recv-end-date and ../abs-recv-end-time))"/>
              <description>
                <text>Date for the key-id start to be receive activity, the combination of abs-recv-start-date and abs-recv-start-time must be earlier than that of abs-recv-end-date and abs-recv-end-time.</text>
              </description>
              <ext:support-filter value="true"/>
            </leaf>
            <leaf name="abs-recv-start-time">
              <type name="pub-type:time-hhmm"/>
              <must condition="../abs-recv-start-date and (../recv-duration or (../abs-recv-end-date and ../abs-recv-end-time))"/>
              <description>
                <text>Time for the key-id start to be receive activity, the combination of abs-recv-start-date and abs-recv-start-time must be earlier than that of abs-recv-end-date and abs-recv-end-time.</text>
              </description>
              <ext:support-filter value="true"/>
            </leaf>
            <choice name="send-end-mode">
              <description>
                <text>The end mode keychain instance.</text>
              </description>
              <case name="end-by-duration">
                <description>
                  <text>End the keychain instance by duration.</text>
                </description>
                <leaf name="send-duration">
                  <type name="uint32">
                    <range value="0..26280000"/>
                  </type>
                  <units name="min"/>
                  <must condition="../abs-send-start-date and ../abs-send-start-time"/>
                  <description>
                    <text>Duration of the key-id keep send active, 0 means that time is infinite.</text>
                  </description>
                  <ext:support-filter value="true"/>
                </leaf>
              </case>
              <case name="end-by-specified-time">
                <description>
                  <text>End the keychain instance by specified-time.</text>
                </description>
                <leaf name="abs-send-end-date">
                  <type name="pub-type:date"/>
                  <must condition="../abs-send-start-date and ../abs-send-start-time and ../abs-send-end-time"/>
                  <description>
                    <text>Date for the key-id stop to be send activity, the combination of abs-send-start-date and abs-send-start-time must be earlier than that of abs-send-end-date and abs-send-end-time.</text>
                  </description>
                  <ext:support-filter value="true"/>
                </leaf>
                <leaf name="abs-send-end-time">
                  <type name="pub-type:time-hhmm"/>
                  <must condition="../abs-send-start-date and ../abs-send-start-time and ../abs-send-end-date"/>
                  <description>
                    <text>Time for the key-id stop to be send activity, the combination of abs-send-start-date and abs-send-start-time must be earlier than that of abs-send-end-date and abs-send-end-time.</text>
                  </description>
                  <ext:support-filter value="true"/>
                </leaf>
              </case>
            </choice>
            <choice name="receive-end-mode">
              <description>
                <text>The end mode keychain instance.</text>
              </description>
              <case name="end-by-duration">
                <description>
                  <text>End the keychain instance by duration.</text>
                </description>
                <leaf name="recv-duration">
                  <type name="uint32">
                    <range value="0..26280000"/>
                  </type>
                  <units name="min"/>
                  <must condition="../abs-recv-start-date and ../abs-recv-start-time"/>
                  <description>
                    <text>Duration of the key-id keep receive active, 0 means that time is infinite.</text>
                  </description>
                  <ext:support-filter value="true"/>
                </leaf>
              </case>
              <case name="end-by-specified-time">
                <description>
                  <text>End the keychain instance by specified-time.</text>
                </description>
                <leaf name="abs-recv-end-date">
                  <type name="pub-type:date"/>
                  <must condition="../abs-recv-start-date and ../abs-recv-start-time and ../abs-recv-end-time"/>
                  <description>
                    <text>Date for the key-id stop to be receive activity, the combination of abs-recv-start-date and abs-recv-start-time must be earlier than that of abs-recv-end-date and abs-recv-end-time.</text>
                  </description>
                  <ext:support-filter value="true"/>
                </leaf>
                <leaf name="abs-recv-end-time">
                  <type name="pub-type:time-hhmm"/>
                  <must condition="../abs-recv-start-date and ../abs-recv-start-time and ../abs-recv-end-date"/>
                  <description>
                    <text>Time for the key-id stop to be receive activity, the combination of abs-recv-start-date and abs-recv-start-time must be earlier than that of abs-recv-end-date and abs-recv-end-time.</text>
                  </description>
                  <ext:support-filter value="true"/>
                </leaf>
              </case>
            </choice>
            <leaf name="send-status">
              <type name="keychain-key-id-status"/>
              <config value="false"/>
              <description>
                <text>Send status of key-id.</text>
              </description>
            </leaf>
            <leaf name="recv-status">
              <type name="keychain-key-id-status"/>
              <config value="false"/>
              <description>
                <text>Receive status of key-id.</text>
              </description>
            </leaf>
          </list>
        </container>
      </list>
    </container>
  </container>
</module>
