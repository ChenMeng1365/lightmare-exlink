  <container name="configuration">
    <description>
      <text>TACPLUS configuration.</text>
    </description>
    <container name="tacplus">
      <description>
        <text>Tacplus info.</text>
      </description>
      <container name="global">
        <description>
          <text>Tacplus global configuration.</text>
        </description>
        <leaf name="enable-state">
          <description>
            <text>Enable or disable TACPLUS.</text>
          </description>
          <type name="enumeration">
            <enum name="disable">
              <value value="0"/>
              <description>
                <text>Disable TACPLUS.</text>
              </description>
            </enum>
            <enum name="enable">
              <value value="128"/>
              <description>
                <text>Enable TACPLUS.</text>
              </description>
            </enum>
          </type>
          <default value="disable"/>
          <ncd:revise-date date="2015-03-30"/>
        </leaf>
        <leaf name="pkt-max-len">
          <description>
            <text>Max packet size (default: 1024 bytes).</text>
          </description>
          <type name="uint32">
            <range value="1024..4096">
              <description>
                <text>Max packet size (default: 1024 bytes).</text>
              </description>
            </range>
          </type>
          <units name="byte"/>
          <default value="1024"/>
          <ncd:revise-date date="2015-03-30"/>
        </leaf>
        <leaf name="timeout">
          <description>
            <text>Time to wait for TACPLUS server to reply(second).</text>
          </description>
          <type name="uint32">
            <range value="1..1000">
              <description>
                <text>Timeout value (default: 5 seconds).</text>
              </description>
              <reference>
                <text>Timeout value (default: 5 seconds)</text>
              </reference>
            </range>
          </type>
          <units name="second"/>
          <default value="5"/>
        </leaf>
        <leaf name="min-key-len">
          <description>
            <text>Min TACPLUS key length.(default: 0).</text>
          </description>
          <type name="uint32">
            <range value="0..63">
              <description>
                <text>Min TACPLUS key length.(default: 0).</text>
              </description>
              <reference>
                <text>Min TACPLUS key length.(default: 0).</text>
              </reference>
            </range>
          </type>
          <units name="byte"/>
          <default value="0"/>
          <ncd:revise-date date="2018-10-24"/>
        </leaf>
        <leaf name="key">
          <description>
            <text>Set TACPLUS  encryption key.</text>
          </description>
          <type name="string">
            <length value="1..63 | 64..128">
              <description>
                <text>Key string (1-63 characters)
Encrypted key(64 - 128characters).</text>
              </description>
            </length>
          </type>
        </leaf>
        <leaf name="deadtime">
          <description>
            <text>Deadtime(default:5 minutes).</text>
          </description>
          <type name="uint32">
            <range value="0..65535">
              <description>
                <text>Deadtime(default:5 minutes).</text>
              </description>
              <reference>
                <text>Deadtime(default:5 minutes)</text>
              </reference>
            </range>
          </type>
          <units name="minute"/>
          <default value="5"/>
        </leaf>
        <container name="tacacs-client">
          <description>
            <text>Configure TACPLUS  V4 client.</text>
          </description>
          <presence value="tacacs-client"/>
          <choice name="ip-if">
            <description>
              <text>IPv4 address or source-interface configuration of TACPLUS  client.</text>
            </description>
            <mandatory value="true"/>
            <case name="ip">
              <description>
                <text>IP address of TACPLUS  client.</text>
              </description>
              <leaf name="ip-address">
                <description>
                  <text>IP address of TACPLUS  client.</text>
                </description>
                <type name="ncd:ipv4-address"/>
                <mandatory value="true"/>
              </leaf>
            </case>
            <case name="if">
              <description>
                <text>TACACS client source-interface name.</text>
              </description>
              <leaf name="source-interface">
                <description>
                  <text>TACACS client source-interface name.</text>
                </description>
                <type name="ncd:ifname">
                  <length value="1..31">
                    <description>
                      <text>interface name length.</text>
                    </description>
                  </length>
                </type>
                <mandatory value="true"/>
              </leaf>
            </case>
            <ncd:revise-date date="2015-03-30"/>
          </choice>
          <leaf name="port">
            <description>
              <text>Port number of TACPLUS  client for IPV4.</text>
            </description>
            <type name="uint16">
              <range value="1025..65535">
                <description>
                  <text>Port number value.</text>
                </description>
                <reference>
                  <text>Port number value</text>
                </reference>
              </range>
            </type>
          </leaf>
        </container>
        <container name="tacacs-client6">
          <description>
            <text>IPv6 TACPLUS  client.</text>
          </description>
          <presence value="tacacs-client6"/>
          <choice name="ip-if">
            <description>
              <text>IPv6 address or source-interface configuration of TACPLUS  client.</text>
            </description>
            <mandatory value="true"/>
            <case name="ip">
              <description>
                <text>IPv6 address of TACPLUS  client.</text>
              </description>
              <leaf name="ipv6-address">
                <description>
                  <text>IPv6 address of TACPLUS  client.</text>
                </description>
                <type name="ncd:ipv6-address"/>
                <mandatory value="true"/>
              </leaf>
            </case>
            <case name="if">
              <description>
                <text>TACACS client6 source-interface name.</text>
              </description>
              <leaf name="source-interface">
                <description>
                  <text>TACACS client6 source-interface name.</text>
                </description>
                <type name="ncd:ifname">
                  <length value="1..31">
                    <description>
                      <text>source-interface name length.</text>
                    </description>
                  </length>
                </type>
                <mandatory value="true"/>
              </leaf>
            </case>
            <ncd:revise-date date="2015-03-30"/>
          </choice>
          <leaf name="port">
            <description>
              <text>Port number of TACPLUS  client for IPV6.</text>
            </description>
            <type name="uint16">
              <range value="1025..65535">
                <description>
                  <text>Port number value.</text>
                </description>
              </range>
            </type>
          </leaf>
        </container>
        <container name="enable-packet">
          <description>
            <text>Configure TACPLUS  enable packet authentication type.</text>
          </description>
          <leaf name="authen-type">
            <description>
              <text>Configure TACPLUS  enable packet authentication type.</text>
            </description>
            <type name="authen-type"/>
            <default value="unknown"/>
          </leaf>
        </container>
        <container name="author-packet">
          <description>
            <text>Configure TACPLUS  authorization packet attribute.</text>
          </description>
          <leaf name="authen-type">
            <description>
              <text>Configure TACPLUS authorization packet authentication type.</text>
            </description>
            <type name="authen-type"/>
            <default value="unknown"/>
          </leaf>
          <leaf name="authen-method">
            <description>
              <text>Configure TACPLUS authorization packet authentication method.</text>
            </description>
            <type name="authen-method"/>
            <default value="unknown"/>
          </leaf>
          <leaf name="authen-service">
            <description>
              <text>Configure TACPLUS authorization packet authentication service.</text>
            </description>
            <type name="authen-service"/>
            <default value="unknown"/>
          </leaf>
        </container>
      </container>
      <container name="hostv4">
        <description>
          <text>Specify a TACPLUS server.</text>
        </description>
        <container name="hosts">
          <description>
            <text>Specify a TACPLUS server.</text>
          </description>
          <list name="host">
            <description>
              <text>Specify a TACPLUS server.</text>
            </description>
            <max-elements value="256"/>
            <key value="ip-address port"/>
            <leaf name="ip-address">
              <description>
                <text>A.B.C.D  IP address of TACPLUS server.</text>
              </description>
              <type name="ncd:ipv4-address"/>
            </leaf>
            <leaf name="port">
              <description>
                <text>Port number of TACPLUS server.</text>
              </description>
              <type name="uint16">
                <range value="49 | 1025..65535">
                  <description>
                    <text>Port number value.</text>
                  </description>
                </range>
              </type>
            </leaf>
            <leaf name="timeout">
              <description>
                <text>Time to wait for TACPLUS server to reply(second).</text>
              </description>
              <type name="uint32">
                <range value="1..1000">
                  <description>
                    <text>Timeout value (second).</text>
                  </description>
                </range>
              </type>
              <units name="second"/>
            </leaf>
            <leaf name="key">
              <description>
                <text>Set TACPLUS encryption key.</text>
              </description>
              <type name="string">
                <length value="1..63 | 64..128">
                  <description>
                    <text>WORD       Key string (1-63 characters)
Encrypted  Set TACACS+ encrypted key(64-128 characters).</text>
                  </description>
                </length>
              </type>
            </leaf>
          </list>
        </container>
        <container name="host-mngs">
          <description>
