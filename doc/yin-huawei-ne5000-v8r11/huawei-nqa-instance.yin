        <text>IP address configurations.</text>
      </description>
      <case name="ipv4-address">
        <description>
          <text>IPv4 address.</text>
        </description>
        <leaf name="ipv4-dest-address">
          <type name="inet:ipv4-address-no-zone"/>
          <description>
            <text>Destination IPv4 address.</text>
          </description>
        </leaf>
        <leaf name="ipv4-source-address">
          <type name="inet:ipv4-address-no-zone"/>
          <description>
            <text>Source IPv4 address.</text>
          </description>
        </leaf>
      </case>
      <case name="ipv6-address">
        <description>
          <text>IPv6 address.</text>
        </description>
        <leaf name="ipv6-dest-address">
          <type name="inet:ipv6-address-no-zone"/>
          <description>
            <text>Destination IPv6 address.</text>
          </description>
        </leaf>
        <leaf name="ipv6-source-address">
          <type name="inet:ipv6-address-no-zone"/>
          <description>
            <text>Source IPv6 address.</text>
          </description>
        </leaf>
      </case>
    </choice>
    <leaf name="ttl">
      <type name="uint8">
        <range value="1..255"/>
      </type>
      <default value="30"/>
      <description>
        <text>Time to live.</text>
      </description>
    </leaf>
    <leaf name="probe-count">
      <type name="uint32">
        <range value="1..15"/>
      </type>
      <default value="3"/>
      <description>
        <text>Number of probes.</text>
      </description>
    </leaf>
    <leaf name="hardware-enable">
      <type name="boolean"/>
      <must condition="not(../jitter-mode='icmp-echo') or (../jitter-mode='icmp-echo' and (../hardware-enable='false'))"/>
      <default value="false"/>
      <description>
        <text>Enable/disable packets are sent by hardware.</text>
      </description>
    </leaf>
    <leaf name="jitter-packet-num">
      <type name="uint32">
        <range value="1..3000"/>
      </type>
      <default value="20"/>
      <description>
        <text>Number of packets.</text>
      </description>
    </leaf>
    <leaf name="data-size">
      <type name="uint16">
        <range value="0..8100"/>
      </type>
      <units name="Byte"/>
      <must condition="../jitter-mode='icmp-echo' or (not(../jitter-mode='icmp-echo') and not(../data-size))"/>
      <default value="0"/>
      <description>
        <text>Packet size.</text>
      </description>
    </leaf>
    <leaf name="data-fill">
      <type name="string">
        <length value="1..230"/>
      </type>
      <must condition="../jitter-mode='icmp-echo' or (not(../jitter-mode='icmp-echo') and not(../data-fill))"/>
      <description>
        <text>Packing string.</text>
      </description>
    </leaf>
    <leaf name="tos">
      <type name="uint16">
        <range value="0..255"/>
      </type>
      <default value="0"/>
      <description>
        <text>TOS.</text>
      </description>
    </leaf>
    <leaf name="ni-name">
      <type name="leafref">
        <path value="/ni:network-instance/ni:instances/ni:instance/ni:name"/>
      </type>
      <default value="_public_"/>
      <description>
        <text>Name of a VPN instance.</text>
      </description>
    </leaf>
    <leaf name="jitter-mode">
      <when condition="not(../ipv6-dest-address) and not(../ipv6-source-address)"/>
      <type name="jitter-mode-type"/>
      <default value="icmp-timestamp"/>
      <description>
        <text>Packet format.</text>
      </description>
    </leaf>
    <leaf name="interval">
      <type name="uint32">
        <range value="10..60000"/>
      </type>
      <units name="ms"/>
      <must condition="../hardware-enable = 'false' and ../interval &gt;= 20 or ../hardware-enable = 'true'"/>
      <default value="20"/>
      <description>
        <text>Interval for sending packets, expressed in milliseconds.</text>
      </description>
    </leaf>
    <leaf name="timestamp">
      <when condition="not(../ipv6-dest-address) and not(../ipv6-source-address)"/>
      <type name="time-unit-type"/>
      <must condition="../hardware-enable = 'true' or ../timestamp != 'microsecond'"/>
      <default value="millisecond"/>
      <description>
        <text>The unit of timestamp.</text>
      </description>
    </leaf>
    <leaf name="out-if-name">
      <type name="leafref">
        <path value="/ifm:ifm/ifm:interfaces/ifm:interface/ifm:name"/>
      </type>
      <description>
