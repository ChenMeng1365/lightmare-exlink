  <ext:task-name value="ietf-snmp"/>
  <typedef name="key-hex-string">
    <type name="string">
      <length value="2..max"/>
      <pattern value="([0-9a-fA-F]{2}(:[0-9a-fA-F]{2})*)?"/>
    </type>
    <description>
      <text>A hexadecimal string with octets represented as hex digits separated by colons.</text>
    </description>
  </typedef>
  <augment target-node="/snmp:snmp/snmp:usm/snmp:local/snmp:user/snmp:auth/snmp:protocol">
    <description>
      <text>Authentication algorithm.</text>
    </description>
    <case name="sha2-224">
      <description>
        <text>SHA224 authentication algorithm.</text>
      </description>
      <container name="sha2-224">
        <description>
          <text>Configure SNMPv3 user authen protocol.</text>
        </description>
        <leaf name="key">
          <type name="key-hex-string"/>
          <mandatory value="true"/>
          <description>
            <text>Localized key specified as a list of colon-specified hexadecimal octets.</text>
          </description>
        </leaf>
      </container>
    </case>
