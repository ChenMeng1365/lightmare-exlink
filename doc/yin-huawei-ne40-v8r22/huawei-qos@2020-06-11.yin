                        </type>
                        <units name="%"/>
                        <description>
                          <text>Percentage of shaping.</text>
                        </description>
                      </leaf>
                    </case>
                    <case name="car">
                      <description>
                        <text>Specify shaping parameters choice case as car value.</text>
                      </description>
                      <leaf name="car-value">
                        <when condition="../service-class='ef' or ../service-class='cs6' or ../service-class='cs7'"/>
                        <type name="uint32">
                          <range value="8..4294967294"/>
                        </type>
                        <units name="kbit/s"/>
                        <description>
                          <text>Rate of car.</text>
                        </description>
                      </leaf>
                    </case>
                    <case name="car-percent">
                      <description>
                        <text>Specify shaping parameters choice case as car percentage value.</text>
                      </description>
                      <leaf name="car-percentage">
                        <when condition="../service-class='ef' or ../service-class='cs6' or ../service-class='cs7'"/>
