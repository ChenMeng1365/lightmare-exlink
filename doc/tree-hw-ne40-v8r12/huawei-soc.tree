        +--rw l3-loop-detect
        |  +--rw enable?   boolean
        +--ro l2-loop-detect
           +--ro loop-statuss
           |  +--ro loop-status* [slot interface vlan-id]
           |     +--ro slot               string
           |     +--ro interface          pub-type:if-name
           |     +--ro vlan-id            uint16
           |     +--ro loop-level?        l2-loop-level
           |     +--ro interface-state?   interface-state
           +--ro packets
              +--ro packet* [slot interface packet-number]
                 +--ro slot                 string
                 +--ro interface            pub-type:if-name
                 +--ro packet-number        uint16
                 +--ro logical-interface?   pub-type:if-name
                 +--ro outer-vlan?          uint16
                 +--ro inner-vlan?          uint16
                 +--ro protocol-type?       uint16
                 +--ro packet-type?         mac-address-type
