<?xml version="1.0" encoding="UTF-8"?>
<module name="huawei-macsec-notification"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:macsec-notification="urn:huawei:yang:huawei-macsec-notification"
        xmlns:ext="urn:huawei:yang:huawei-extension">
  <yang-version value="1"/>
  <namespace uri="urn:huawei:yang:huawei-macsec-notification"/>
  <prefix value="macsec-notification"/>
  <import module="huawei-extension">
    <prefix value="ext"/>
  </import>
  <organization>
    <text>Huawei Technologies Co., Ltd.</text>
  </organization>
  <contact>
    <text>Huawei Industrial Base
Bantian, Longgang
Shenzhen 518129
People's Republic of China
Website: http://www.huawei.com
Email: support@huawei.com</text>
  </contact>
  <description>
    <text>Notification of macsec-notification.</text>
  </description>
  <revision date="2020-04-01">
    <description>
      <text>Add new nodes.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <revision date="2020-02-20">
    <description>
      <text>Initial revision.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <ext:task-name value="macsec"/>
  <typedef name="role-type">
    <type name="enumeration">
      <enum name="client">
        <value value="1"/>
        <description>
          <text>Client.</text>
        </description>
      </enum>
      <enum name="server">
        <value value="2"/>
        <description>
          <text>Server.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Role type.</text>
    </description>
  </typedef>
  <notification name="macsec-ckn-switch-fail">
    <description>
      <text>If the CKN switchover fails or the switchover is not complete three minutes later, an alarm is generated.</text>
    </description>
    <leaf name="if-index">
      <type name="uint32"/>
      <description>
        <text>Interface index.</text>
      </description>
    </leaf>
    <leaf name="role">
      <type name="role-type"/>
      <description>
        <text>Role.</text>
      </description>
    </leaf>
    <leaf name="local-port">
      <type name="string">
        <length value="1..64"/>
      </type>
      <description>
        <text>Local port.</text>
      </description>
    </leaf>
    <leaf name="old-ckn">
      <type name="string">
        <length value="1..65"/>
      </type>
      <description>
        <text>Old ckn.</text>
      </description>
    </leaf>
    <leaf name="new-ckn">
      <type name="string">
        <length value="1..65"/>
      </type>
      <description>
        <text>New ckn.</text>
      </description>
    </leaf>
    <leaf name="switch-reason">
      <type name="string">
        <length value="1..128"/>
      </type>
      <description>
        <text>Switch reason.</text>
      </description>
    </leaf>
    <leaf name="fail-reason">
      <type name="string">
        <length value="1..128"/>
      </type>
      <description>
        <text>Fail reason.</text>
      </description>
    </leaf>
  </notification>
  <notification name="macsec-receive-response-timeout">
    <description>
      <text>The MKA protocol status of the local interface becomes Down because the response message cannot be received during the timeout period.</text>
    </description>
    <leaf name="if-index">
      <type name="uint32"/>
      <description>
        <text>Interface index.</text>
      </description>
    </leaf>
    <leaf name="role">
      <type name="role-type"/>
      <description>
        <text>Role.</text>
      </description>
    </leaf>
    <leaf name="local-port">
      <type name="string">
        <length value="1..64"/>
      </type>
      <description>
        <text>Local port.</text>
      </description>
    </leaf>
  </notification>
  <notification name="macsec-receive-response-timeout-resume">
    <description>
      <text>The MKA protocol status is restored or the MACSEC configuration is deleted.</text>
    </description>
    <leaf name="if-index">
      <type name="uint32"/>
      <description>
        <text>Interface index.</text>
      </description>
    </leaf>
    <leaf name="role">
      <type name="role-type"/>
      <description>
        <text>Role.</text>
      </description>
    </leaf>
    <leaf name="local-port">
      <type name="string">
        <length value="1..64"/>
      </type>
      <description>
        <text>Local port.</text>
      </description>
    </leaf>
  </notification>
</module>
