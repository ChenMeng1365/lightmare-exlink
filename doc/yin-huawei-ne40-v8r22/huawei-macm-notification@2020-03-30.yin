<?xml version="1.0" encoding="UTF-8"?>
<module name="huawei-macm-notification"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:macm-notification="urn:huawei:yang:huawei-macm-notification"
        xmlns:ext="urn:huawei:yang:huawei-extension">
  <yang-version value="1"/>
  <namespace uri="urn:huawei:yang:huawei-macm-notification"/>
  <prefix value="macm-notification"/>
  <import module="huawei-extension">
    <prefix value="ext"/>
  </import>
  <organization>
    <text>Huawei Technologies Co., Ltd.</text>
  </organization>
  <contact>
    <text>Huawei Industrial Base
Bantian, Longgang
Shenzhen 518129
People's Republic of China
Website: http://www.huawei.com
Email: support@huawei.com</text>
  </contact>
  <description>
    <text>Notification of macm-notification.</text>
  </description>
  <revision date="2020-03-30">
    <description>
      <text>The modifications in the schema file are synchronized to the YANG file.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <revision date="2019-07-08">
    <description>
      <text>Initial revision.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <notification name="lr-mac-exceed">
    <description>
      <text>The number of MAC routes on the device exceeded the maximum value.</text>
    </description>
    <leaf name="vstm-cur-mac-num">
      <type name="uint32"/>
      <description>
        <text>Current MAC route number of the device.</text>
      </description>
    </leaf>
    <leaf name="vstm-max-mac-num">
      <type name="uint32"/>
      <description>
        <text>Maximum number of MAC addresses.</text>
      </description>
    </leaf>
  </notification>
  <notification name="lr-mac-exceed-clear">
    <description>
      <text>The number of MAC routes on the device fell below the maximum value.</text>
    </description>
    <leaf name="vstm-cur-mac-num">
      <type name="uint32"/>
      <description>
        <text>Current MAC route number of the device.</text>
      </description>
    </leaf>
    <leaf name="vstm-max-mac-num">
      <type name="uint32"/>
      <description>
        <text>Maximum number of MAC addresses.</text>
      </description>
    </leaf>
  </notification>
  <ext:task-name value="vstm"/>
</module>
