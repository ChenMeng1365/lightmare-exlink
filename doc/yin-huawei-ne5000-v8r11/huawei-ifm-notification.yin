<?xml version="1.0" encoding="UTF-8"?>
<module name="huawei-ifm-notification"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:ifm-notification="urn:huawei:yang:huawei-ifm-notification"
        xmlns:pub-type="urn:huawei:yang:huawei-pub-type"
        xmlns:ext="urn:huawei:yang:huawei-extension">
  <yang-version value="1"/>
  <namespace uri="urn:huawei:yang:huawei-ifm-notification"/>
  <prefix value="ifm-notification"/>
  <import module="huawei-pub-type">
    <prefix value="pub-type"/>
  </import>
  <import module="huawei-extension">
    <prefix value="ext"/>
  </import>
  <organization>
    <text>Huawei Technologies Co., Ltd.</text>
  </organization>
  <contact>
    <text>Huawei Industrial Base
Bantian, Longgang
Shenzhen 518129
People's Republic of China
Website: http://www.huawei.com
Email: support@huawei.com</text>
  </contact>
  <description>
    <text>Notification of ifm.</text>
  </description>
  <revision date="2020-05-30">
    <description>
      <text>Initial revision.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <ext:task-name value="interface-mgr"/>
  <typedef name="port-status">
    <type name="enumeration">
      <enum name="up">
        <value value="1"/>
        <description>
          <text>The interface is in Up state.</text>
        </description>
      </enum>
      <enum name="down">
        <value value="2"/>
        <description>
          <text>The interface is in Down state.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Interface suppression status.</text>
    </description>
  </typedef>
  <typedef name="suppress-status">
    <type name="enumeration">
      <enum name="unsuppress">
        <value value="0"/>
        <description>
          <text>The interface is not suppressed.</text>
        </description>
      </enum>
      <enum name="suppress">
        <value value="1"/>
        <description>
          <text>The interface is suppressed.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Interface suppression status.</text>
    </description>
  </typedef>
  <notification name="error-down">
    <description>
      <text>Error-down occurred.</text>
    </description>
    <leaf name="index">
      <type name="uint32"/>
      <mandatory value="true"/>
      <description>
        <text>Index of an interface.</text>
      </description>
    </leaf>
    <leaf name="name">
      <type name="pub-type:if-name"/>
      <mandatory value="true"/>
      <description>
        <text>Interface name.</text>
      </description>
    </leaf>
    <leaf name="cause">
      <type name="string">
        <length value="1..50"/>
      </type>
      <mandatory value="true"/>
      <description>
        <text>Cause of the errordown event.</text>
      </description>
    </leaf>
    <leaf name="recovery-type">
      <type name="string">
        <length value="1..20"/>
      </type>
      <mandatory value="true"/>
      <description>
        <text>Specifies the alarm clearance type. The value is auto recovery or Null. The alarm clearance type triggered by restarting an interface is auto recovery.</text>
      </description>
    </leaf>
  </notification>
  <notification name="error-down-recovery">
    <description>
      <text>Error-down recovered.</text>
    </description>
    <leaf name="index">
      <type name="uint32"/>
      <mandatory value="true"/>
      <description>
        <text>Index of an interface.</text>
      </description>
    </leaf>
    <leaf name="name">
      <type name="pub-type:if-name"/>
      <mandatory value="true"/>
      <description>
        <text>Interface name.</text>
      </description>
    </leaf>
    <leaf name="cause">
      <type name="string">
        <length value="1..50"/>
      </type>
      <mandatory value="true"/>
      <description>
        <text>Cause of the errordown event.</text>
      </description>
    </leaf>
    <leaf name="recovery-type">
      <type name="string">
        <length value="1..20"/>
      </type>
      <mandatory value="true"/>
      <description>
        <text>Specifies the alarm clearance type. The value is auto recovery or Null. The alarm clearance type triggered by restarting an interface is auto recovery.</text>
      </description>
    </leaf>
  </notification>
  <notification name="if-damp-suppress">
    <description>
      <text>The state of interface changed from unsuppress to suppress.</text>
    </description>
    <leaf name="interface-name">
      <type name="pub-type:if-name"/>
      <description>
        <text>Interface name.</text>
      </description>
    </leaf>
    <leaf name="suppress-state">
      <type name="suppress-status"/>
      <description>
        <text>Interface suppression status: - 0: The interface is not suppressed. - 1: The interface is suppressed.</text>
      </description>
    </leaf>
  </notification>
  <notification name="if-damp-resume">
    <description>
      <text>The state of interface changed from suppress to unsuppress.</text>
    </description>
    <leaf name="interface-name">
      <type name="pub-type:if-name"/>
      <description>
        <text>Interface name.</text>
      </description>
    </leaf>
    <leaf name="suppress-state">
      <type name="suppress-status"/>
      <description>
        <text>Interface suppression status: - 0: The interface is not suppressed. - 1: The interface is suppressed.</text>
      </description>
    </leaf>
  </notification>
  <notification name="physical-admin-if-down">
    <description>
      <text>The physical manage interface physical status changes to down.</text>
    </description>
    <leaf name="index">
      <type name="uint32"/>
      <mandatory value="true"/>
      <description>
        <text>Interface index.</text>
      </description>
    </leaf>
    <leaf name="name">
      <type name="pub-type:if-name"/>
      <mandatory value="true"/>
      <description>
        <text>Name of the physical management interface.</text>
      </description>
    </leaf>
    <leaf name="oper-status">
      <type name="port-status"/>
      <mandatory value="true"/>
      <description>
        <text>Status of the physical management interface.</text>
      </description>
    </leaf>
  </notification>
  <notification name="physical-admin-if-up">
    <description>
      <text>The physical manage interface physical status changes to up.</text>
    </description>
    <leaf name="index">
      <type name="uint32"/>
      <mandatory value="true"/>
      <description>
        <text>Interface index.</text>
      </description>
    </leaf>
    <leaf name="name">
      <type name="pub-type:if-name"/>
      <mandatory value="true"/>
      <description>
        <text>Name of the physical management interface.</text>
      </description>
    </leaf>
    <leaf name="oper-status">
      <type name="port-status"/>
      <mandatory value="true"/>
      <description>
        <text>Status of the physical management interface.</text>
      </description>
    </leaf>
  </notification>
  <notification name="ipv6-if-state-change">
    <description>
      <text>The IPv6 status changes.</text>
    </description>
    <leaf name="index">
      <type name="uint32"/>
      <mandatory value="true"/>
      <description>
        <text>Interface index.</text>
      </description>
    </leaf>
    <leaf name="name">
      <type name="pub-type:if-name"/>
      <mandatory value="true"/>
      <description>
        <text>Indicates the interface name.</text>
      </description>
    </leaf>
    <leaf name="oper-status">
      <type name="port-status"/>
      <mandatory value="true"/>
      <description>
        <text>Indicates the current status of an interface.</text>
      </description>
    </leaf>
    <leaf name="admin-status">
      <type name="port-status"/>
      <mandatory value="true"/>
      <description>
        <text>Indicates the administrative status of an interface.</text>
      </description>
    </leaf>
    <leaf name="main-if-name">
      <type name="pub-type:if-name"/>
      <mandatory value="true"/>
      <description>
        <text>Main interface name.</text>
      </description>
    </leaf>
    <leaf name="reason-code">
      <type name="uint32"/>
      <mandatory value="true"/>
      <description>
        <text>Cause for trap generation.</text>
      </description>
    </leaf>
  </notification>
  <notification name="link-down">
    <description>
      <text>The interface status changes.</text>
    </description>
    <leaf name="index">
      <type name="uint32"/>
      <mandatory value="true"/>
      <description>
        <text>Interface index.</text>
      </description>
    </leaf>
    <leaf name="admin-status">
      <type name="port-status"/>
      <mandatory value="true"/>
      <description>
        <text>Configured interface status.</text>
      </description>
    </leaf>
    <leaf name="oper-status">
      <type name="port-status"/>
      <mandatory value="true"/>
      <description>
        <text>Current interface status.</text>
      </description>
    </leaf>
    <leaf name="name">
      <type name="pub-type:if-name"/>
      <mandatory value="true"/>
      <description>
        <text>Interface name.</text>
      </description>
    </leaf>
    <leaf name="reason-code">
      <type name="uint32"/>
      <mandatory value="true"/>
      <description>
        <text>Cause for trap generation.</text>
      </description>
    </leaf>
    <leaf name="main-if-name">
      <type name="pub-type:if-name"/>
      <mandatory value="true"/>
      <description>
        <text>Name of the main interface.</text>
      </description>
    </leaf>
  </notification>
  <notification name="link-up">
    <description>
      <text>The interface status changes.</text>
    </description>
    <leaf name="index">
      <type name="uint32"/>
      <mandatory value="true"/>
      <description>
        <text>Interface index.</text>
      </description>
    </leaf>
    <leaf name="admin-status">
      <type name="port-status"/>
      <mandatory value="true"/>
      <description>
        <text>Configured interface status.</text>
      </description>
    </leaf>
    <leaf name="oper-status">
      <type name="port-status"/>
      <mandatory value="true"/>
      <description>
        <text>Current interface status.</text>
      </description>
    </leaf>
    <leaf name="name">
      <type name="pub-type:if-name"/>
      <mandatory value="true"/>
      <description>
        <text>Interface name.</text>
      </description>
    </leaf>
    <leaf name="reason-code">
      <type name="uint32"/>
      <mandatory value="true"/>
      <description>
        <text>Cause for trap generation.</text>
      </description>
    </leaf>
    <leaf name="main-if-name">
      <type name="pub-type:if-name"/>
      <mandatory value="true"/>
      <description>
        <text>Name of the main interface.</text>
      </description>
    </leaf>
  </notification>
</module>
