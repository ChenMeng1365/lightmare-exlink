          <text>Configure Vm total car.</text>
        </description>
        <leaf name="cir">
          <type name="uint32">
            <range value="0..1000000"/>
          </type>
          <units name="kbit/s"/>
          <default value="58491"/>
          <description>
            <text>Committed information rate.</text>
          </description>
        </leaf>
        <leaf name="cbs">
          <type name="uint32">
            <range value="0..90000000"/>
          </type>
          <units name="Byte"/>
          <default value="9000000"/>
          <description>
            <text>Committed burst size.</text>
          </description>
        </leaf>
      </container>
    </container>
  </augment>
</module>
