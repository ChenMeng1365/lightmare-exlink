          </description>
        </leaf>
        <leaf name="drops">
          <type name="uint32">
            <range value="0..65535"/>
          </type>
          <description>
            <text>Number of packets lost.</text>
          </description>
        </leaf>
        <container name="hops">
          <description>
            <text>List of result records of a single hop.</text>
          </description>
          <list name="hop">
            <key value="index"/>
            <description>
              <text>Statistics of result records of a single hop.</text>
            </description>
            <leaf name="index">
              <type name="uint32">
                <range value="1..1000000"/>
              </type>
              <description>
                <text>Hop index.</text>
              </description>
            </leaf>
            <leaf name="sent-probes">
              <type name="uint32">
                <range value="0..65535"/>
              </type>
              <description>
                <text>Total number of sent packets.</text>
              </description>
            </leaf>
            <leaf name="response-probes">
              <type name="uint32">
                <range value="0..65535"/>
              </type>
              <description>
                <text>Total number of received packets.</text>
              </description>
            </leaf>
            <leaf name="rtt-min">
              <type name="uint32">
                <range value="0..65535"/>
              </type>
              <description>
                <text>Minimum RTT of all test packets, expressed in milliseconds.</text>
              </description>
            </leaf>
            <leaf name="rtt-average">
              <type name="uint32">
                <range value="0..65535"/>
              </type>
              <description>
                <text>Average RTT of all test packets, expressed in milliseconds.</text>
              </description>
            </leaf>
            <leaf name="rtt-max">
              <type name="uint32">
                <range value="0..65535"/>
              </type>
              <description>
                <text>Maximum RTT of all test packets, expressed in milliseconds.</text>
              </description>
            </leaf>
            <leaf name="rtt-sum">
              <type name="uint32">
                <range value="0..65535"/>
              </type>
              <description>
                <text>Sum of all test packets'RTT, expressed in milliseconds.</text>
              </description>
            </leaf>
            <leaf name="rtt-sum-square">
              <type name="uint64"/>
              <description>
                <text>Sum of RTT square.</text>
              </description>
            </leaf>
            <leaf name="rtd-thresholds">
              <type name="uint32">
                <range value="0..65535"/>
              </type>
              <description>
                <text>Counts of RTD over Threshold.</text>
              </description>
            </leaf>
            <leaf name="last-probe-time">
              <type name="yang:date-and-time"/>
              <description>
                <text> Last good probe time.</text>
              </description>
            </leaf>
            <leaf name="dest-address">
              <type name="inet:ip-address-no-zone"/>
              <description>
                <text>Destination IP address.</text>
              </description>
            </leaf>
            <leaf name="packet-loss-ratio">
              <type name="uint8">
                <range value="0..100"/>
              </type>
              <units name="%"/>
              <description>
                <text>Packet loss ratio.</text>
              </description>
            </leaf>
          </list>
        </container>
      </list>
    </container>
  </grouping>
</submodule>
