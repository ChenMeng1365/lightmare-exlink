  <rpc name="software-operate">
    <input>
      <leaf name="software-operation">
        <description>
          <text>Operation type.</text>
        </description>
        <config value="false"/>
        <type name="enumeration">
          <enum name="SWOP_DOWNLOAD">
            <value value="1"/>
          </enum>
          <enum name="SWOP_ACTIVATE">
            <value value="2"/>
          </enum>
          <enum name="SWOP_ROLLBACK">
            <value value="3"/>
          </enum>
          <enum name="SWOP_COMMIT">
            <value value="4"/>
          </enum>
          <enum name="SWOP_DELETE">
            <value value="5"/>
          </enum>
          <enum name="SWOP_CLEANUP">
            <value value="6"/>
          </enum>
          <enum name="SWOP_DOWNLOAD_STOP">
            <value value="7"/>
          </enum>
        </type>
      </leaf>
      <leaf name="file-type">
        <description>
          <text>File type.</text>
        </description>
        <config value="false"/>
        <type name="enumeration">
          <enum name="software">
            <value value="1"/>
          </enum>
          <enum name="startrun">
            <value value="2"/>
          </enum>
        </type>
      </leaf>
      <leaf name="operate-target">
        <description>
          <text>Operation target.</text>
        </description>
        <config value="false"/>
        <type name="string">
          <length value="1..180"/>
        </type>
      </leaf>
      <choice name="server-info">
        <config value="false"/>
        <case name="tftp">
          <container name="tftp-server-info">
            <leaf name="port">
              <description>
                <text>Tftp server port.</text>
              </description>
              <type name="uint32"/>
            </leaf>
            <leaf name="ip-address">
              <description>
                <text>Tftp server ip address.</text>
              </description>
              <type name="string"/>
            </leaf>
          </container>
        </case>
        <case name="ftp">
          <container name="ftp-server-info">
            <leaf name="port">
              <description>
                <text>Ftp server port.</text>
              </description>
              <type name="uint32"/>
            </leaf>
            <leaf name="user-name">
              <description>
                <text>Ftp server username.</text>
              </description>
              <type name="string">
                <length value="1..64"/>
              </type>
            </leaf>
            <leaf name="user-password">
              <description>
                <text>Ftp server password.</text>
              </description>
              <type name="string">
                <length value="1..64"/>
              </type>
            </leaf>
            <leaf name="ip-address">
              <description>
                <text>Ftp server ip address.</text>
              </description>
              <type name="string"/>
            </leaf>
          </container>
        </case>
        <case name="sftp">
          <container name="sftp-server-info">
            <leaf name="port">
              <description>
                <text>Sftp server port.</text>
              </description>
              <type name="uint32"/>
            </leaf>
            <leaf name="user-name">
              <description>
                <text>Sftp server username.</text>
              </description>
              <type name="string">
                <length value="1..64"/>
              </type>
            </leaf>
            <leaf name="user-password">
              <description>
                <text>Sftp server password.</text>
              </description>
              <type name="string">
                <length value="1..64"/>
              </type>
            </leaf>
            <leaf name="ip-address">
              <description>
                <text>Sftp server ip address.</text>
              </description>
              <type name="string"/>
            </leaf>
          </container>
        </case>
        <case name="storage">
          <container name="null"/>
        </case>
      </choice>
      <leaf name="upgrade-type">
        <description>
          <text>Upgrade type.</text>
        </description>
        <config value="false"/>
        <type name="enumeration">
          <enum name="issu">
            <value value="1"/>
          </enum>
          <enum name="parallel-reload">
            <value value="2"/>
          </enum>
          <enum name="reserve-standby">
            <value value="3"/>
          </enum>
        </type>
      </leaf>
      <leaf name="reboot-type">
        <description>
          <text>Reboot type.</text>
        </description>
        <config value="false"/>
        <type name="enumeration">
          <enum name="soft">
            <value value="1"/>
          </enum>
          <enum name="hard">
            <value value="2"/>
          </enum>
        </type>
      </leaf>
      <leaf name="vrf-name">
        <description>
          <text>Vrf name.</text>
        </description>
        <type name="string"/>
      </leaf>
    </input>
    <output>
      <leaf name="result">
        <description>
          <text>Operation result.</text>
        </description>
        <config value="false"/>
        <type name="string">
          <length value="1..32"/>
        </type>
      </leaf>
    </output>
  </rpc>
