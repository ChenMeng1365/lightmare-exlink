<?xml version="1.0" encoding="UTF-8"?>
<module name="huawei-igmp-mld-notification"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:igmp-mld-n="urn:huawei:yang:huawei-igmp-mld-notification"
        xmlns:pub-type="urn:huawei:yang:huawei-pub-type"
        xmlns:inet="urn:ietf:params:xml:ns:yang:ietf-inet-types"
        xmlns:ext="urn:huawei:yang:huawei-extension">
  <yang-version value="1"/>
  <namespace uri="urn:huawei:yang:huawei-igmp-mld-notification"/>
  <prefix value="igmp-mld-n"/>
  <import module="huawei-pub-type">
    <prefix value="pub-type"/>
  </import>
  <import module="ietf-inet-types">
    <prefix value="inet"/>
  </import>
  <import module="huawei-extension">
    <prefix value="ext"/>
  </import>
  <organization>
    <text>Huawei Technologies Co., Ltd.</text>
  </organization>
  <contact>
    <text>Huawei Industrial Base
Bantian, Longgang
Shenzhen 518129
People's Republic of China
Website: http://www.huawei.com
Email: support@huawei.com</text>
  </contact>
  <description>
    <text>The module defines notifications for IGMP and MLD.</text>
  </description>
  <revision date="2019-07-08">
    <description>
      <text>Initial revision.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <typedef name="ip-addr-type">
    <type name="enumeration">
      <enum name="unknown">
        <value value="0"/>
        <description>
          <text>Unknown type.</text>
        </description>
      </enum>
      <enum name="ipv4unicast">
        <value value="1"/>
        <description>
          <text>IPv4 unicast.</text>
        </description>
      </enum>
      <enum name="ipv6unicast">
        <value value="2"/>
        <description>
          <text>IPv6 unicast.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Address family.</text>
    </description>
  </typedef>
  <typedef name="instance-name">
    <type name="string">
      <length value="1..31"/>
    </type>
    <description>
      <text>Instance name.</text>
    </description>
  </typedef>
  <notification name="hw-igmp-mld-interface-limit">
    <description>
      <text>Membership report message is discarded because the interface-specific IGMP or MLD group membership limitation is exceeded.</text>
    </description>
    <leaf name="address-family">
      <type name="igmp-mld-n:ip-addr-type"/>
      <description>
        <text>Address family.</text>
      </description>
    </leaf>
    <leaf name="src-addr">
      <type name="inet:ip-address-no-zone"/>
      <description>
        <text>Source address.</text>
      </description>
    </leaf>
    <leaf name="grp-addr">
      <type name="inet:ip-address-no-zone"/>
      <description>
        <text>Group address.</text>
      </description>
    </leaf>
    <leaf name="if-index">
      <type name="uint32"/>
      <description>
        <text>Interface index.</text>
      </description>
    </leaf>
    <leaf name="current-count">
      <type name="uint32"/>
      <description>
        <text>Limit on the number of IGMP entries on the device.</text>
      </description>
    </leaf>
    <leaf name="if-name">
      <type name="pub-type:if-name"/>
      <description>
        <text>Name of the interface.</text>
      </description>
    </leaf>
    <leaf name="instance-name">
      <type name="igmp-mld-n:instance-name"/>
      <description>
        <text>Name of the instance where the number of IGMP entries on the interface exceeded the limit.</text>
      </description>
    </leaf>
  </notification>
  <notification name="hw-igmp-mld-interface-limit-clear">
    <description>
      <text>The new Report message can be processed because the number of IGMP or MLD group memberships on the interface falls below the upper limit.</text>
    </description>
    <leaf name="address-family">
      <type name="igmp-mld-n:ip-addr-type"/>
      <description>
        <text>Address family.</text>
      </description>
    </leaf>
    <leaf name="src-addr">
      <type name="inet:ip-address-no-zone"/>
      <description>
        <text>Source address.</text>
      </description>
    </leaf>
    <leaf name="grp-addr">
      <type name="inet:ip-address-no-zone"/>
      <description>
        <text>Group address.</text>
      </description>
    </leaf>
    <leaf name="if-index">
      <type name="uint32"/>
      <description>
        <text>Interface index.</text>
      </description>
    </leaf>
    <leaf name="current-count">
      <type name="uint32"/>
      <description>
        <text>Limit on the number of IGMP entries on the device.</text>
      </description>
    </leaf>
    <leaf name="if-name">
      <type name="pub-type:if-name"/>
      <description>
        <text>Name of the interface.</text>
      </description>
    </leaf>
    <leaf name="instance-name">
      <type name="igmp-mld-n:instance-name"/>
      <description>
        <text>Name of the instance where the number of IGMP entries on the interface exceeded the limit.</text>
      </description>
    </leaf>
  </notification>
  <notification name="hw-igmp-mld-interface-threshold-exceed">
    <description>
      <text>The number of IGMP or MLD entries on the interface reached the upper threshold.</text>
    </description>
    <leaf name="address-family">
      <type name="igmp-mld-n:ip-addr-type"/>
      <description>
        <text>Address family.</text>
      </description>
    </leaf>
    <leaf name="if-index">
      <type name="uint32"/>
      <description>
        <text>Interface index.</text>
      </description>
    </leaf>
    <leaf name="current-count">
      <type name="uint32"/>
      <description>
        <text>Number of created IGMP/MLD entries on the interface.</text>
      </description>
    </leaf>
    <leaf name="threshold-count">
      <type name="uint32"/>
      <description>
        <text>Threshold at which the alarm is generated.</text>
      </description>
    </leaf>
    <leaf name="if-name">
      <type name="pub-type:if-name"/>
      <description>
        <text>Name of the interface.</text>
      </description>
    </leaf>
    <leaf name="instance-name">
      <type name="igmp-mld-n:instance-name"/>
      <description>
        <text>Instance of which the number of created (S, G) entries on the device reaches the specified limit.</text>
      </description>
    </leaf>
  </notification>
  <notification name="hw-igmp-mld-interface-threshold-exceed-clear">
    <description>
      <text>The number of IGMP or MLD entries on the interface fell below the lower threshold.</text>
    </description>
    <leaf name="address-family">
      <type name="igmp-mld-n:ip-addr-type"/>
      <description>
        <text>Address family.</text>
      </description>
    </leaf>
    <leaf name="if-index">
      <type name="uint32"/>
      <description>
        <text>Interface index.</text>
      </description>
    </leaf>
    <leaf name="current-count">
      <type name="uint32"/>
      <description>
        <text>Number of created IGMP/MLD entries on the interface.</text>
      </description>
    </leaf>
    <leaf name="threshold-count">
      <type name="uint32"/>
      <description>
        <text>Threshold at which the alarm is generated.</text>
      </description>
    </leaf>
    <leaf name="if-name">
      <type name="pub-type:if-name"/>
      <description>
        <text>Name of the interface.</text>
      </description>
    </leaf>
    <leaf name="instance-name">
      <type name="igmp-mld-n:instance-name"/>
      <description>
        <text>Instance of which the number of created (S, G) entries on the device reaches the specified limit.</text>
      </description>
    </leaf>
  </notification>
  <ext:task-name value="igmp"/>
</module>
