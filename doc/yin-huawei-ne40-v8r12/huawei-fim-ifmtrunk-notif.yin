<?xml version="1.0" encoding="UTF-8"?>
<module name="huawei-fim-ifmtrunk-notif"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:fim-ifmtrunk-notif="urn:huawei:yang:huawei-fim-ifmtrunk-notif"
        xmlns:ext="urn:huawei:yang:huawei-extension">
  <yang-version value="1"/>
  <namespace uri="urn:huawei:yang:huawei-fim-ifmtrunk-notif"/>
  <prefix value="fim-ifmtrunk-notif"/>
  <import module="huawei-extension">
    <prefix value="ext"/>
  </import>
  <organization>
    <text>Huawei Technologies Co., Ltd.</text>
  </organization>
  <contact>
    <text>Huawei Industrial Base
Bantian, Longgang
Shenzhen 518129
People's Republic of China
Website: http://www.huawei.com
Email: support@huawei.com</text>
  </contact>
  <description>
    <text>Notification of Trunk.</text>
  </description>
  <revision date="2021-03-01">
    <description>
      <text>Initial revision.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <ext:task-name value="forwarding"/>
  <notification name="fim-trunk-bandwidth-imbalance">
    <description>
      <text>The bandwidth of the member interfaces of trunk differs greatly. Backpressure may occur on member interfaces with low bandwidth, affecting traffic forwarding.</text>
    </description>
    <leaf name="trunk-name">
      <type name="string">
        <length value="1..63"/>
      </type>
      <description>
        <text>Name of a Trunk Interface.</text>
      </description>
    </leaf>
    <leaf name="interface-name">
      <type name="string">
        <length value="1..63"/>
      </type>
      <description>
        <text>Name of a Trunk member interface.</text>
      </description>
    </leaf>
  </notification>
  <notification name="fim-trunk-bandwidth-imbalance-resume">
    <description>
      <text>The bandwidth ratio between trunk member interfaces is within the normal range.</text>
    </description>
    <leaf name="trunk-name">
      <type name="string">
        <length value="1..63"/>
      </type>
      <description>
        <text>Name of a Trunk Interface.</text>
      </description>
    </leaf>
    <leaf name="interface-name">
      <type name="string">
        <length value="1..63"/>
      </type>
      <description>
        <text>Name of a Trunk member interface.</text>
      </description>
    </leaf>
  </notification>
</module>
