<?xml version="1.0" encoding="UTF-8"?>
<module name="zxr10-adm-mgr-dev-fn"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:adm-mgr-dev-fn="http://www.zte.com.cn/zxr10/netconf/schema/rosng/adm-mgr-dev-fn"
        xmlns:adm-mgr="http://www.zte.com.cn/zxr10/netconf/schema/rosng/adm-mgr">
  <namespace uri="http://www.zte.com.cn/zxr10/netconf/schema/rosng/adm-mgr-dev-fn"/>
  <prefix value="adm-mgr-dev-fn"/>
  <import module="zxr10-adm-mgr">
    <prefix value="adm-mgr"/>
  </import>
  <organization>
    <text>ZTE Corporation</text>
  </organization>
  <contact>
    <text>ZTE Corporation  
   Nanjing Institute of ZTE Corporation 
   No.68 Zijinghua Rd. Yuhuatai District, Nanjing, China 
   Tel: +86-25-52870000</text>
  </contact>
  <revision date="2021-02-18">
    <description>
      <text>15K, vBRAS-C and T8000 project support the range of leaf node 'password-duration' from 0 to 360</text>
    </description>
  </revision>
  <revision date="2019-04-22">
    <description>
      <text>MW(NR8000) project need support the range of leaf node 'password-duration' from 0 to 360</text>
    </description>
  </revision>
  <revision date="2019-01-03">
    <description>
      <text>OTN(7000/8700/9700/M721) project need support the range of leaf node 'password-duration' from 0 to 360</text>
    </description>
  </revision>
  <revision date="2018-03-26">
    <description>
      <text>C600 project need support the range of leaf node 'password-duration' from 0 to 360</text>
    </description>
  </revision>
  <deviation target-node="/adm-mgr:configuration/adm-mgr:adm-mgr/adm-mgr:usernames/adm-mgr:username/adm-mgr:password-duration">
    <description>
      <text>Configuration password validity period</text>
    </description>
    <deviate value="replace">
      <type name="uint16">
        <range value="0..360"/>
      </type>
    </deviate>
  </deviation>
</module>
