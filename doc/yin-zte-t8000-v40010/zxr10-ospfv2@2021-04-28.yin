  <feature name="ti-lfa">
    <description>
      <text>The feature is for TI-LFA.</text>
    </description>
  </feature>
  <feature name="segment-routing">
    <description>
      <text>SR support labels</text>
    </description>
  </feature>
  <feature name="distance-policy">
    <description>
      <text>The feature is for distance-policy.</text>
    </description>
  </feature>
  <container name="configuration">
    <description>
      <text>Configure OSPFV2 parameters.</text>
    </description>
    <container name="ospfv2">
      <description>
        <text>Configure OSPFV2 parameters.</text>
      </description>
      <container name="processes">
        <description>
          <text>Ospf Instance</text>
        </description>
        <list name="process">
          <description>
            <text>Ospf Instance</text>
          </description>
          <key value="pro-id"/>
          <leaf name="pro-id">
            <description>
              <text>Ospf process ID</text>
            </description>
            <type name="uint32">
              <range value="1..65535">
                <description>
                  <text>Ospf process ID from 1 to 65535</text>
                </description>
              </range>
            </type>
          </leaf>
          <leaf name="vrf-name">
            <description>
              <text>Vrf name</text>
            </description>
            <type name="string">
              <length value="1..32">
                <description>
                  <text>Characters from 1 to 32</text>
                </description>
              </length>
            </type>
          </leaf>
          <leaf name="lsa-pacing">
            <description>
              <text>Ospf LSA group pacing timers,Secs between group of LSA being refreshed</text>
            </description>
            <type name="uint32">
              <range value="1..1800">
                <description>
                  <text>Secs between group of LSA being refreshed</text>
                </description>
              </range>
            </type>
            <units name="second"/>
            <default value="1"/>
          </leaf>
          <leaf name="timer-frr">
            <description>
              <text>Ospf ipfrr spf timers, hold time in secs between consecutive spf</text>
            </description>
            <type name="uint32">
              <range value="1..65535">
                <description>
                  <text>Ospf ipfrr spf timers, hold time in secs between consecutive spf</text>
                </description>
              </range>
            </type>
            <units name="second"/>
            <default value="1"/>
          </leaf>
          <leaf name="maximum-paths">
            <description>
              <text>Maximum number of parallel routes OSPF can support</text>
            </description>
            <type name="uint32">
              <range value="1..32">
                <description>
                  <text>Maximum number of parallel routes OSPF can support</text>
                </description>
              </range>
            </type>
            <default value="1"/>
          </leaf>
          <leaf name="default-metric">
            <description>
              <text>Ospf default metric</text>
            </description>
            <type name="uint32">
              <range value="1..16777214">
                <description>
                  <text>Ospf default metric form 1 to 16777214</text>
                </description>
              </range>
            </type>
            <default value="20"/>
          </leaf>
          <leaf name="auto-cost">
            <description>
              <text>Calculate OSPF interface cost according to bandwidth, the reference bandwidth in terms of Mbps</text>
            </description>
            <type name="uint32">
              <range value="1..4000000">
                <description>
                  <text>The reference bandwidth in terms of Mbps</text>
                </description>
              </range>
            </type>
            <units name="Mbps"/>
