        </description>
      </leaf>
      <leaf name="policy-name">
        <type name="leafref">
          <path value="/qos:qos/qos:classifier-template/qos:traffic-policys/qos:traffic-policy/qos:name"/>
        </type>
        <mandatory value="true"/>
        <description>
          <text>Policy name.</text>
        </description>
      </leaf>
    </input>
  </rpc>
  <rpc name="reset-policy-apply-statistics-vxlan">
    <description>
      <text>To clear traffic policy statistics.</text>
    </description>
    <ext:node-ref value="/qos-bd:qos-bd/qos-bd:qos-query/qos-bd:vxlan-mode-policy-statisticss"/>
    <input>
      <leaf name="domain-id">
        <type name="leafref">
          <path value="/bd:bd/bd:instances/bd:instance/bd:id"/>
        </type>
        <mandatory value="true"/>
        <description>
          <text>Bridge domain ID.</text>
        </description>
      </leaf>
      <leaf name="direction">
        <type name="qos:qos-direction-type"/>
        <mandatory value="true"/>
        <description>
          <text>Direction.</text>
        </description>
      </leaf>
    </input>
  </rpc>
  <augment target-node="/bd:bd/bd:instances/bd:instance">
    <description>
      <text>Adds qos configuration to the board.</text>
    </description>
    <container name="qos">
      <description>
        <text>Configure QoS application.</text>
      </description>
      <container name="domain-service-classs">
        <description>
          <text>Configure forced traffic classification.</text>
        </description>
        <container name="domain-service-class">
          <presence value="create domain-service-classs"/>
          <description>
            <text>Enable/disable forced traffic classification.</text>
          </description>
          <leaf name="service-class">
            <type name="qos:qos-service-class"/>
            <mandatory value="true"/>
            <description>
              <text>Class of service.</text>
            </description>
          </leaf>
        </container>
      </container>
      <container name="domain-policy-applys">
        <description>
          <text>List of traffic policy applications.</text>
        </description>
        <list name="domain-policy-apply">
          <key value="direction policy-name layer"/>
          <unique tag="direction"/>
          <max-elements value="2"/>
          <description>
            <text>Configure application of a traffic policy.</text>
          </description>
          <leaf name="direction">
            <type name="qos:qos-direction-type"/>
            <description>
              <text>Direction.</text>
            </description>
          </leaf>
          <leaf name="policy-name">
            <type name="leafref">
              <path value="/qos:qos/qos:classifier-template/qos:traffic-policys/qos:traffic-policy/qos:name"/>
            </type>
            <description>
              <text>Policy name.</text>
            </description>
          </leaf>
          <leaf name="layer">
            <type name="qos-policy-apply-layer"/>
            <description>
              <text>Classification based on layer 2 or layer 3 information.</text>
            </description>
          </leaf>
        </list>
      </container>
      <container name="trusts">
        <description>
          <text>List of trust diffserv domain or 8021p.</text>
        </description>
        <list name="trust">
          <key value="trust-type direction"/>
          <description>
            <text>Configure trust diffserv domain or 8021p list.</text>
          </description>
          <leaf name="trust-type">
            <type name="qos:trust-type"/>
            <description>
              <text>To select trusting DS domain, 8021p or all.</text>
            </description>
          </leaf>
          <leaf name="direction">
            <type name="qos:trust-direction-type"/>
            <description>
              <text>Direction.</text>
            </description>
          </leaf>
          <leaf name="diffserv-domain-name">
            <when condition="../trust-type='ip-dscp'"/>
            <type name="leafref">
              <path value="/qos:qos/qos:classifier-template/qos:diffserv-domains/qos:diffserv-domain/qos:name"/>
            </type>
            <mandatory value="true"/>
            <description>
              <text>Name of a ds domain.</text>
            </description>
          </leaf>
        </list>
      </container>
      <container name="phb-vxlans">
        <description>
          <text>Configure the PHB operation.</text>
        </description>
        <container name="phb-vxlan">
          <presence value="create phb-vxlan "/>
          <description>
            <text>Enable/disable PHB operation.</text>
          </description>
          <leaf name="operate-type">
            <type name="vxlan-operate-type"/>
            <mandatory value="true"/>
            <description>
              <text>To select enable or disable phb.</text>
            </description>
          </leaf>
          <leaf name="diffserv-domain-name">
            <type name="leafref">
              <path value="/qos:qos/qos:classifier-template/qos:diffserv-domains/qos:diffserv-domain/qos:name"/>
            </type>
            <description>
              <text>Name of a DS domain.</text>
            </description>
          </leaf>
        </container>
      </container>
      <container name="phbs">
        <description>
          <text>List of the PHB operation.</text>
        </description>
        <list name="phb">
          <must condition="(./map-type='none' and not (../phb[map-type!='none'])) or (./map-type!='none' and not (../phb[map-type='none']))"/>
          <must condition="(./operate-type='enable' and not (../phb[operate-type!='enable'])) or (./operate-type!='enable' and not (../phb[operate-type='enable']))"/>
          <must condition="(./operate-type!='enable') or (./operate-type='enable' and ./map-type='none')"/>
          <key value="operate-type map-type"/>
          <description>
            <text>Configure PHB operation.</text>
          </description>
          <leaf name="operate-type">
            <type name="operate-type"/>
            <description>
              <text>To select enable or disable phb.</text>
            </description>
          </leaf>
          <leaf name="map-type">
            <type name="qos:map-type"/>
            <description>
              <text>To select disable type.</text>
            </description>
          </leaf>
          <leaf name="diffserv-domain-name">
            <when condition="not(../operate-type='disable')"/>
            <type name="leafref">
              <path value="/qos:qos/qos:classifier-template/qos:diffserv-domains/qos:diffserv-domain/qos:name"/>
            </type>
            <description>
              <text>Name of a DS domain.</text>
            </description>
          </leaf>
        </list>
      </container>
      <container name="car-vxlan-modes">
        <description>
          <text>List of the car operation.</text>
        </description>
        <list name="car-vxlan-mode">
          <must condition="((./exclude-mode='no-exclude' and not (../car-vxlan-mode[exclude-mode='exclude'])) or (./exclude-mode!='no-exclude' and not(../car-vxlan-mode[exclude-mode='no-exclude'])))"/>
          <key value="slot"/>
          <description>
            <text>Configure CAR operation.</text>
          </description>
          <leaf name="slot">
            <type name="string">
              <length value="1..32"/>
            </type>
            <description>
              <text>Car disable slot.</text>
            </description>
          </leaf>
          <leaf name="exclude-mode">
            <type name="qos:exclude-type"/>
            <mandatory value="true"/>
            <description>
              <text>Exclude Mode.</text>
            </description>
            <ext:operation-exclude value="update">
              <description>
                <text>After the configuration is created, the node cannot be modified.</text>
              </description>
            </ext:operation-exclude>
          </leaf>
        </list>
      </container>
      <container name="vxlan-mode-policy-applys">
        <description>
          <text>List of traffic policy application.</text>
        </description>
        <list name="vxlan-mode-policy-apply">
          <key value="direction"/>
          <description>
            <text>Configure the application of a traffic policy.</text>
          </description>
          <leaf name="direction">
            <type name="qos:qos-direction-type"/>
            <description>
              <text>Direction.</text>
            </description>
          </leaf>
          <leaf name="policy-name">
            <type name="leafref">
              <path value="/qos:qos/qos:classifier-template/qos:traffic-policys/qos:traffic-policy/qos:name"/>
            </type>
            <mandatory value="true"/>
            <description>
              <text>Policy name.</text>
            </description>
            <ext:operation-exclude value="update">
              <description>
                <text>After the configuration is created, the node cannot be modified.</text>
              </description>
            </ext:operation-exclude>
          </leaf>
          <leaf name="layer">
            <when condition="../direction='inbound'"/>
            <type name="qos:policy-apply-vxlan-layer-type"/>
            <description>
              <text>Classification based on Layer 2 or Layer 3 information.</text>
            </description>
            <ext:operation-exclude value="create|delete">
              <description>
                <text>The node cannot be created or deleted.</text>
              </description>
            </ext:operation-exclude>
          </leaf>
        </list>
      </container>
      <container name="vxlan-mode-trusts">
        <description>
          <text>List of trust diffserv domain or 8021p.</text>
        </description>
        <list name="vxlan-mode-trust">
          <key value="trust-type"/>
          <description>
            <text>Configure trust diffserv domain or 8021p.</text>
          </description>
          <leaf name="trust-type">
            <type name="qos:vxlan-trust-type"/>
            <description>
              <text>To select trusting diffserv domain, 8021p.</text>
            </description>
          </leaf>
          <leaf name="ds-name">
            <when condition="../trust-type='upstream'"/>
            <type name="leafref">
              <path value="/qos:qos/qos:classifier-template/qos:diffserv-domains/qos:diffserv-domain/qos:name"/>
            </type>
            <mandatory value="true"/>
            <description>
              <text>Name of a DS domain.</text>
            </description>
          </leaf>
        </list>
      </container>
    </container>
  </augment>
</module>
