                 |     +--:(ipv6-bfr-prefix)
                 |        +--rw ipv6-bfr-prefix?   ncd:ipv6-address-none-zero
                 +--rw bsl
                 |  +--rw (bsl)?
                 |     +--:(bsl-256)
                 |     |  +--rw bsl-256-info
                 |     |     +--rw encapsulation-type    encapsulation-type
                 |     |     +--rw bift-coding?          enumeration
                 |     |     +--rw max-si?               uint16
                 |     +--:(bsl-512)
