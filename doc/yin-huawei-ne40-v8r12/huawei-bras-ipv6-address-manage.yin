                <path value="/bras-ipv6-address-manage:bras-ipv6-address-manage/bras-ipv6-address-manage:ipv6-prefixs/bras-ipv6-address-manage:ipv6-prefix/bras-ipv6-address-manage:name"/>
              </type>
              <must condition="/bras-ipv6-address-manage:bras-ipv6-address-manage/bras-ipv6-address-manage:ipv6-prefixs/bras-ipv6-address-manage:ipv6-prefix[name=current()]/bras-ipv6-address-manage:type ='local'"/>
              <description>
                <text>Bind an IPv6 prefix pool to an IPv6 address pool.</text>
              </description>
            </leaf>
          </container>
          <uses name="remote-not-support"/>
          <uses name="rapid-commit-config"/>
          <uses name="ipv6-pool-comm"/>
          <uses name="ipv6-constant-index"/>
        </container>
        <container name="usage">
          <config value="false"/>
          <description>
            <text>Display the usage statistics for IPv6 address pool.</text>
          </description>
          <leaf name="vpn-instance">
            <type name="leafref">
              <path value="/ni:network-instance/ni:instances/ni:instance/ni:name"/>
            </type>
            <description>
              <text>Specify the VPN instance name.</text>
            </description>
            <ext:support-filter value="true"/>
          </leaf>
          <uses name="pool-usage"/>
        </container>
        <container name="statistic">
          <config value="false"/>
          <description>
            <text>Display the IPv6 address or prefix statistics.</text>
          </description>
          <leaf name="pool-index">
            <type name="int32">
              <range value="0..16383"/>
            </type>
            <description>
              <text>The index of IPv6 pool.</text>
            </description>
          </leaf>
          <uses name="pool-statistics"/>
        </container>
      </list>
    </container>
    <container name="ipv6-unr-route-tag">
      <description>
        <text>Configure route tags for IPv6 users.</text>
      </description>
      <leaf name="delegated-ipv6-prefix-tag">
        <type name="uint32"/>
        <default value="0"/>
        <description>
          <text>Set a route tag for PD IPv6 addresses delivered by a RADIUS server.</text>
        </description>
      </leaf>
      <leaf name="delegated-pool-tag">
        <type name="uint32"/>
        <default value="0"/>
        <description>
          <text>Set a route tag for delegation IPv6 address pools.</text>
        </description>
      </leaf>
      <leaf name="framed-ipv6-address-tag">
        <type name="uint32"/>
        <default value="0"/>
        <description>
          <text>Set a route tag for IA_NA IPv6 addresses delivered by a RADIUS server.</text>
        </description>
      </leaf>
      <leaf name="framed-ipv6-prefix-tag">
        <type name="uint32"/>
        <default value="0"/>
        <description>
          <text>Set a route tag for ND IPv6 addresses delivered by a RADIUS server.</text>
        </description>
      </leaf>
      <leaf name="framed-ipv6-route-tag">
        <type name="uint32"/>
        <default value="0"/>
        <description>
          <text>Set a route tag for IPv6 route addresses delivered by a RADIUS server.</text>
        </description>
      </leaf>
      <leaf name="local-pool-tag">
        <type name="uint32"/>
        <default value="0"/>
        <description>
          <text>Set a route tag for local IPv6 address pools.</text>
        </description>
      </leaf>
      <leaf name="relay-pool-tag">
        <type name="uint32"/>
        <default value="0"/>
        <description>
          <text>Set a route tag for relay IPv6 address pools.</text>
        </description>
      </leaf>
      <leaf name="remote-pool-tag">
        <type name="uint32"/>
        <default value="0"/>
        <description>
          <text>Set a route tag for remote IPv6 address pools.</text>
        </description>
      </leaf>
    </container>
    <container name="address-assign-eui-64">
      <description>
        <text>Configure an interface ID to be in the standard EUI-64 format when DHCPv6 is used to allocate addresses.</text>
      </description>
      <leaf name="enable">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable an interface ID to be in the standard EUI-64 format when DHCPv6 is used to allocate addresses.</text>
        </description>
      </leaf>
    </container>
    <container name="statistic-include-shared-user">
      <description>
        <text>Configure the IPv6 address pool statistics to include statistics about users sharing the prefix pool.</text>
      </description>
      <leaf name="enable">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable the IPv6 address pool statistics to include statistics about users sharing the prefix pool.</text>
        </description>
      </leaf>
    </container>
    <container name="pool-select-algorithm">
      <must condition="not (./remote-loading-share = 'true' and ./remote-round-robin = 'true')"/>
      <description>
        <text>Configure a device to assign addresses from IPv6 remote address pools based on weights of the address pools.</text>
      </description>
      <leaf name="remote-loading-share">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable a device to assign addresses from IPv6 remote address pools based on weights of the address pools.</text>
        </description>
      </leaf>
      <leaf name="remote-round-robin">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable the round robin algorithm for remote IPv6 address pool.</text>
        </description>
      </leaf>
      <leaf name="local-round-robin">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable the round robin algorithm for local IPv6 address pool.</text>
        </description>
      </leaf>
    </container>
    <container name="auto-block">
      <description>
        <text>Configure automatic isolation of IPv6 prefix pools.</text>
      </description>
      <leaf name="enable">
        <type name="boolean"/>
        <default value="true"/>
        <description>
          <text>Enable/disable automatic isolation of IPv6 prefix pools.</text>
        </description>
      </leaf>
      <leaf name="slave-switch">
        <type name="boolean"/>
        <default value="true"/>
        <description>
          <text>Enable/disable the switchover function for IPv6 prefix pool isolation.</text>
        </description>
      </leaf>
    </container>
    <container name="statistics-by-domains">
      <config value="false"/>
      <description>
        <text>List of domain-based statistics.</text>
      </description>
      <list name="statistics-by-domain">
        <key value="domain-name"/>
        <description>
          <text>Domain-based statistics.</text>
        </description>
        <leaf name="domain-name">
          <type name="leafref">
            <path value="/aaa:aaa/aaa:domains/aaa:domain/aaa:name"/>
          </type>
          <description>
            <text>Specify the name of a domain.</text>
          </description>
        </leaf>
        <container name="pool-usage">
          <description>
            <text>Display the statistics for IPv6 address pool usage.</text>
          </description>
          <uses name="pool-usage"/>
        </container>
        <container name="cu-pool-usages">
          <description>
            <text>List of statistics for IPv6 addresses pool usage.</text>
          </description>
          <list name="cu-pool-usage">
            <key value="up-id"/>
            <description>
              <text>Display the statistics for IPv6 address pool usage.</text>
            </description>
            <leaf name="up-id">
              <type name="uint16">
                <range value="1024..2023 | 2048..3047 | 3072..4071 | 4096..5095"/>
              </type>
              <must condition="(../up-id&lt;=(/bras-cu-controller:bras-cu-controller/bras-cu-controller:global-attribute/bras-cu-controller:up-base-id + 999) and ../up-id&gt;=/bras-cu-controller:bras-cu-controller/bras-cu-controller:global-attribute/bras-cu-controller:up-base-id)"/>
              <description>
                <text>The ID of a vBRAS-UP.</text>
              </description>
            </leaf>
            <uses name="pool-usage"/>
          </list>
        </container>
        <container name="usage-threshold">
          <description>
            <text>Display the statistics for domains whose IPv6 address pool or prefix pool usage exceeds a specified threshold.</text>
          </description>
          <leaf name="threshold-type">
            <type name="threshold-type"/>
            <description>
              <text>Include upper-threshold and lower-threshold.</text>
            </description>
            <ext:support-filter value="true"/>
          </leaf>
          <uses name="pool-usage"/>
        </container>
      </list>
    </container>
    <container name="pool-statistics">
      <config value="false"/>
      <description>
        <text>Statistics of IPv6 pool usage.</text>
      </description>
      <leaf name="statistical-unit">
        <type name="pool-usage-type"/>
        <default value="domain"/>
        <description>
          <text>Statistical unit.</text>
        </description>
      </leaf>
      <uses name="pool-usage"/>
    </container>
    <container name="statistics-by-vpn-instance">
      <config value="false"/>
      <description>
        <text>Display the statistics for IPv6 address pools bound by the vpn-instance.</text>
      </description>
      <container name="statistics">
        <description>
          <text>List of statistics for IPv6 address pools bound by the vpn-instance.</text>
        </description>
        <list name="statistic">
          <key value="vpn-instance"/>
          <description>
            <text>Display the statistics for IPv6 address pools bound by the vpn-instance.</text>
          </description>
          <leaf name="vpn-instance">
            <type name="leafref">
              <path value="/ni:network-instance/ni:instances/ni:instance/ni:name"/>
            </type>
            <description>
              <text>Specify the VPN instance name.</text>
            </description>
          </leaf>
          <container name="usage">
            <description>
              <text>Display the usage statistics for IPv6 address pools bound by vpn-instance.</text>
            </description>
            <uses name="pool-usage"/>
          </container>
          <container name="usage-warning-threshold">
            <config value="false"/>
            <description>
              <text>Display the statistics for address pool usage based on a specified VPN instance.</text>
            </description>
            <leaf name="total-number">
              <type name="int32"/>
              <description>
                <text>Total number of IP addresses.</text>
              </description>
            </leaf>
            <leaf name="used-number">
              <type name="int32"/>
              <description>
                <text>Number of used IP addresses.</text>
              </description>
            </leaf>
            <leaf name="idle-number">
              <type name="int32"/>
              <description>
                <text>Number of idle IP addresses.</text>
              </description>
            </leaf>
            <leaf name="conflict-number">
              <type name="int32"/>
              <description>
                <text>Number of conflicting IP addresses.</text>
              </description>
            </leaf>
            <leaf name="exclude-number">
              <type name="int32"/>
              <description>
                <text>Number of excluded IP addresses.</text>
              </description>
            </leaf>
            <leaf name="used-ratio">
              <type name="int32"/>
              <description>
                <text>IP address usage.</text>
              </description>
            </leaf>
          </container>
        </list>
      </container>
      <container name="total-usage">
        <description>
          <text>Display the usage statistics for IPv6 address pools which are for all vpn-instance.</text>
        </description>
        <uses name="pool-usage"/>
      </container>
    </container>
    <container name="global-statistic">
      <config value="false"/>
      <description>
        <text>Statistics of IPv6 pool.</text>
      </description>
      <leaf name="total-pool-number">
        <type name="int32"/>
        <description>
          <text>Number of configured IPv6 address pools.</text>
        </description>
      </leaf>
      <uses name="pool-statistics"/>
    </container>
  </container>
  <augment target-node="/ni:network-instance/ni:instances/ni:instance/l3vpn:afs/l3vpn:af/bras-ipv4-address-manage:bas">
    <description>
      <text>The vpn instance who is enabled the IPv6 address family.</text>
    </description>
    <container name="ipv6-address-manage">
      <when condition="../../l3vpn:type='ipv6-unicast' and ../../../../ni:name!='_public_'"/>
      <description>
        <text>Configure the vpn instance who is enabled the IPv6 address family.</text>
      </description>
      <container name="warning-threshold">
        <description>
          <text>Configure the warning threshold of IPv6 pools who bound with the vpn instance.</text>
        </description>
        <leaf name="value">
          <type name="uint8">
            <range value="0..100"/>
          </type>
          <default value="80"/>
          <description>
            <text>The percentage of IPv6 pools warning threshold.</text>
          </description>
        </leaf>
      </container>
    </container>
  </augment>
</module>
