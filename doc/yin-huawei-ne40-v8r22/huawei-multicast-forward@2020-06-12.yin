  <typedef name="resource-pool-type">
    <type name="enumeration">
      <enum name="0">
        <value value="0"/>
        <description>
          <text>Resource pool 0. The outgoing interface of each multicast group preferentially applies for resource pool 0, and only one can be applied for.</text>
        </description>
      </enum>
      <enum name="1">
        <value value="1"/>
        <description>
          <text>Resource pool 1. If a multicast group has applied for an outgoing interface resource in resource pool 0, it applies for other outgoing interface resources in resource pool 1.</text>
        </description>
      </enum>
      <enum name="NULL">
        <value value="255"/>
        <description>
          <text>Null type.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Resource pool type.</text>
    </description>
