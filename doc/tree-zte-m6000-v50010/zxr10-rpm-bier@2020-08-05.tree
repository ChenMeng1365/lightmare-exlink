module: zxr10-rpm-bier
  +--rw configuration
     +--rw bier
        +--rw bier-global
           o--rw encapsulation-type?   encapsulation-type
           o--rw bitstringlength?      bsl
           o--rw bfr-id?               uint16
           o--rw ipv4-bfr-prefix?      ncd:ipv4-address
           o--rw sub-domains
           |  +--rw sub-domain* [sub-domain-id]
           |     +--rw sub-domain-id      uint16
           |     +--rw igp-type?          igp-type
           |     +--rw mt-id?             uint32
           |     +--rw bfr-id?            uint16
           |     +--rw bitstringlength?   bsl
           |     +--rw address-family
