  <typedef name="activeStatus">
    <type name="enumeration">
      <enum name="active">
        <value value="1"/>
        <description>
          <text>Active status</text>
        </description>
      </enum>
      <enum name="inactive">
        <value value="0"/>
        <description>
          <text>Inactive status</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Active status</text>
    </description>
  </typedef>
  <typedef name="algorithmType">
    <type name="enumeration">
      <enum name="hmac-md5">
        <value value="1"/>
        <description>
          <text>HMAC-MD5 algorithm</text>
        </description>
      </enum>
      <enum name="hmac-sha256">
        <value value="2"/>
        <description>
          <text>HMAC-SHA256 algorithm</text>
        </description>
      </enum>
      <enum name="hmac-sha1-12">
        <value value="3"/>
        <description>
          <text>HMAC-SHA1-12 algorithm</text>
        </description>
      </enum>
      <enum name="hmac-sha1-20">
        <value value="4"/>
        <description>
          <text>HMAC-SHA1-20 algorithm</text>
        </description>
      </enum>
      <enum name="md5">
        <value value="5"/>
        <description>
          <text>MD5 algorithm</text>
        </description>
      </enum>
      <enum name="sha1">
        <value value="6"/>
        <description>
          <text>SHA1 algorithm</text>
        </description>
      </enum>
      <enum name="sha256">
        <value value="7"/>
        <description>
          <text>SHA256 algorithm</text>
        </description>
      </enum>
      <enum name="cmac-aes128-12">
        <value value="10"/>
        <description>
          <text>CMAC-AES128 algorithm</text>
        </description>
      </enum>
