  <notification name="flexe-calendar-mismatch">
    <description>
      <text>Alarm of mismatch between the actually received and expected client calendars on the FlexE Group.</text>
    </description>
    <leaf name="group-index">
      <type name="uint32"/>
      <description>
        <text>Indicates the FlexE group index.</text>
      </description>
    </leaf>
    <leaf name="group-name">
      <type name="string">
        <length value="1..32"/>
      </type>
      <description>
        <text>Indicates the FlexE group name.</text>
      </description>
    </leaf>
  </notification>
  <notification name="flexe-calendar-mismatch-resume">
    <description>
