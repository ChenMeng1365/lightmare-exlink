              <type name="enumeration">
                <enum name="in">
                  <value value="1"/>
                  <description>
                    <text>Inbound packets.</text>
                  </description>
                </enum>
                <enum name="out">
                  <value value="2"/>
                  <description>
                    <text>Outbound packets.</text>
                  </description>
                </enum>
              </type>
            </leaf>
            <leaf name="interface">
              <description>
                <text>Interface name.</text>
              </description>
              <type name="ncd:ifname">
                <length value="1..31"/>
              </type>
            </leaf>
            <leaf name="acl-name">
              <description>
                <text>Access list of networks to apply offset (1-31 characters).</text>
              </description>
              <type name="string">
                <length value="1..31"/>
              </type>
              <mandatory value="true"/>
            </leaf>
            <leaf name="metric">
              <description>
                <text>Offset list metric.</text>
              </description>
              <type name="uint8">
                <range value="0..16"/>
              </type>
              <mandatory value="true"/>
            </leaf>
          </list>
        </container>
      </container>
      <container name="auth-keychains">
        <description>
          <text>Configure authentication keychain in RIP module.</text>
        </description>
        <list name="auth-keychain">
          <description>
            <text>Configure authentication keychain in RIP module.</text>
          </description>
          <key value="if-name key-id"/>
          <leaf name="if-name">
            <description>
              <text>Interface name.</text>
            </description>
            <type name="ncd:ifname"/>
          </leaf>
          <leaf name="key-id">
            <description>
              <text>Key identifier.</text>
            </description>
            <type name="uint8">
              <range value="1..255"/>
            </type>
          </leaf>
          <choice name="auth-key">
            <description>
              <text>Specify the authentication key.</text>
            </description>
            <case name="auth-key-clear">
              <description>
                <text>Set the authentication key in clear text.</text>
              </description>
              <container name="auth-key-clear">
                <description>
                  <text>Set the authentication key in clear text.</text>
                </description>
                <leaf name="key-clear">
                  <description>
                    <text>Set the authentication key in clear text.</text>
                  </description>
                  <type name="string">
                    <length value="1..16"/>
                  </type>
                  <mandatory value="true"/>
                </leaf>
              </container>
            </case>
            <case name="auth-key-encrypted">
              <description>
                <text>Set the authentication key in encrypted text.</text>
              </description>
              <container name="auth-key-encrypted">
                <description>
                  <text>Set the authentication key in encrypted text.</text>
                </description>
                <leaf name="key-encrypted">
                  <description>
                    <text>Set the authentication key in encrypted text.</text>
                  </description>
                  <type name="string">
                    <length value="1..84"/>
                  </type>
                  <mandatory value="true"/>
                </leaf>
              </container>
            </case>
          </choice>
        </list>
      </container>
    </container>
  </container>
  <container name="state">
    <config value="false"/>
    <description>
      <text>The querying information processing of RIP module.</text>
    </description>
    <container name="rip">
