  <feature name="mac-flooding">
    <description>
      <text>This node is used to distinguish whether the MAC flooding function can be enabled.</text>
    </description>
  </feature>
  <feature name="vxlan-instance-mode">
    <description>
      <text>This node is used to distinguish whether the VXLAN instance mode can be enabled.</text>
    </description>
  </feature>
  <feature name="vtep-instance-mode">
    <description>
      <text>This node is used to distinguish whether the vtep instance mode can be enabled.</text>
    </description>
  </feature>
  <feature name="static-tunnel">
    <description>
      <text>This node is used to distinguish whether static tunnel function can be enabled.</text>
    </description>
  </feature>
  <feature name="redundancy-group-bind">
    <description>
      <text>This node is used to distinguish whether redundancy group bind table function can be enabled.</text>
    </description>
  </feature>
  <feature name="vtep-bind">
    <description>
      <text>This node is used to distinguish whether the vtep bind can be enabled.</text>
    </description>
  </feature>
  <feature name="move-dampening">
    <description>
      <text>This node is used to distinguish whether the move dampening policy can be enabled.</text>
    </description>
  </feature>
  <container name="configuration">
    <description>
      <text>Configuration model info.</text>
    </description>
    <container name="vxlan">
      <description>
        <text>Configure VXLAN parameters.</text>
      </description>
      <container name="global-config">
        <description>
          <text>Global enable configuration of VXLAN.</text>
        </description>
        <presence value="global-config"/>
        <leaf name="global-enable">
          <description>
            <text>VXLAN global enble.</text>
          </description>
          <type name="empty"/>
          <mandatory value="true"/>
        </leaf>
      </container>
      <container name="mac-floodings">
        <description>
          <text>Configuration of MAC flooding.</text>
        </description>
        <list name="mac-flooding">
          <description>
            <text>Configuration of MAC flooding.</text>
          </description>
          <key value="mac-address vlan-id"/>
          <leaf name="mac-address">
            <description>
              <text>Specific MAC address of MAC flooding.</text>
            </description>
            <type name="ncd:mac-address"/>
          </leaf>
          <leaf name="vlan-id">
            <description>
              <text>VLAN ID of MAC flooding.</text>
            </description>
            <type name="uint16">
              <range value="1..4094">
                <description>
                  <text>The value of VLAN,it must be between 1 to 4094.</text>
                </description>
              </range>
            </type>
          </leaf>
        </list>
        <ncd:revise-date date="2016-01-08"/>
      </container>
      <container name="vxlan-instances">
        <description>
          <text>Configuration of VXLAN instances.</text>
        </description>
        <list name="vxlan-instance">
          <description>
            <text>Configuration of VXLAN instance.</text>
          </description>
          <key value="vxlan-id"/>
          <leaf name="vxlan-id">
            <description>
              <text>VXLAN ID of VXLAN instance.</text>
            </description>
            <type name="uint32">
              <range value="1..16777215">
                <description>
                  <text>The value of VXLAN,it must be between 1 to 16777215.</text>
                </description>
              </range>
            </type>
          </leaf>
          <leaf name="description">
            <description>
              <text>The description information of VXLAN instance.</text>
            </description>
            <type name="string">
              <length value="0..64">
                <description>
                  <text>VXLAN instance description information.</text>
                </description>
              </length>
            </type>
            <default value=""/>
            <ncd:revise-date date="2016-01-08"/>
          </leaf>
          <leaf name="unknow-unicast-drop">
            <description>
              <text>Unknow unicast drop configuration of VXLAN instance.</text>
            </description>
            <type name="enumeration">
              <enum name="enable">
                <value value="1"/>
                <description>
                  <text>Unknown unicast drop enable.</text>
                </description>
              </enum>
              <enum name="disable">
                <value value="2"/>
                <description>
                  <text>Unknown unicast drop disable.</text>
                </description>
              </enum>
            </type>
            <default value="enable"/>
            <ncd:revise-date date="2016-01-08"/>
          </leaf>
          <leaf name="filter-vrrp">
            <description>
              <text>VRRP packets filter configuration of VXLAN instance.</text>
            </description>
            <type name="enumeration">
              <enum name="enable">
                <value value="1"/>
                <description>
                  <text>VRRP packets filter.</text>
                </description>
              </enum>
              <enum name="disable">
                <value value="2"/>
                <description>
                  <text>VRRP packets not filter.</text>
                </description>
              </enum>
            </type>
            <default value="enable"/>
            <ncd:revise-date date="2016-01-08"/>
          </leaf>
          <container name="vxlan-multicast-ip">
            <status value="obsolete">
              <ncd:revise-date date="2016-01-08"/>
            </status>
            <description>
              <text>VXLAN multicast IP address.</text>
            </description>
            <presence value="vxlan-multicast-ip"/>
            <leaf name="multicast-ip">
              <status value="obsolete">
                <ncd:revise-date date="2016-01-08"/>
              </status>
              <description>
                <text>VXLAN multicast IP address.</text>
              </description>
              <type name="ncd:ipv4-address"/>
              <mandatory value="true"/>
            </leaf>
          </container>
          <container name="vxlan-multicast-ipv6">
            <status value="obsolete">
              <ncd:revise-date date="2016-01-08"/>
            </status>
            <description>
              <text>VXLAN multicast IPv6 address.</text>
            </description>
            <presence value="vxlan-multicast-ipv6"/>
            <leaf name="multicast-ipv6">
              <status value="obsolete">
                <ncd:revise-date date="2016-01-08"/>
              </status>
              <description>
                <text>VXLAN multicast IPv6 address.</text>
              </description>
              <type name="ncd:ipv6-address"/>
              <mandatory value="true"/>
            </leaf>
            <ncd:revise-date date="2015-11-25"/>
          </container>
          <container name="vxlan-map-tables">
            <description>
              <text>VXLAN map tables.</text>
            </description>
            <presence value="vxlan-map-tables"/>
            <choice name="vxlan-map-types">
              <description>
                <text>VXLAN map type choice.</text>
              </description>
              <case name="vxlan-map-vlan">
                <description>
                  <text>VXLAN map VLAN.</text>
                </description>
                <leaf name="map-type-vlan">
                  <description>
                    <text>VXLAN map VLAN.</text>
                  </description>
                  <type name="enumeration">
                    <enum name="map-type-vlan1to1">
                      <value value="1"/>
                      <description>
                        <text>Map type VLAN1:1.</text>
                      </description>
                    </enum>
                    <enum name="map-type-vlan1ton">
                      <value value="2"/>
                      <description>
                        <text>Map type VLAN1:n.</text>
                      </description>
                    </enum>
                  </type>
                  <default value="map-type-vlan1to1"/>
                </leaf>
                <list name="map-vlan">
                  <description>
                    <text>VXLAN map VLAN.</text>
                  </description>
                  <key value="vlan-id"/>
                  <leaf name="vlan-id">
                    <description>
                      <text>VLAN ID.</text>
                    </description>
                    <type name="uint16">
                      <range value="1..4094">
                        <description>
                          <text>The value of VLAN,it must be between 1 to 4094.</text>
                        </description>
                      </range>
                    </type>
                  </leaf>
                </list>
              </case>
              <case name="vxlan-map-mac">
                <description>
                  <text>VXLAN map MAC.</text>
                </description>
                <leaf name="map-type-mac">
                  <description>
                    <text>VXLAN map MAC.</text>
                  </description>
                  <type name="empty"/>
                </leaf>
                <container name="map-mac">
                  <description>
                    <text>VXLAN map MAC.</text>
                  </description>
                  <presence value="map-mac"/>
                  <leaf name="mac">
                    <description>
                      <text>MAC address.</text>
                    </description>
                    <type name="ncd:mac-address"/>
                    <mandatory value="true"/>
                  </leaf>
                </container>
              </case>
              <case name="vxlan-map-l2interface">
                <description>
                  <text>VXLAN map L2 interface.</text>
                </description>
                <leaf name="map-type-l2interface">
                  <description>
                    <text>VXLAN map L2 interface.</text>
                  </description>
                  <type name="empty"/>
                </leaf>
                <list name="map-l2interface">
                  <description>
                    <text>VXLAN map L2 interface.</text>
                  </description>
                  <key value="interface-name"/>
                  <leaf name="interface-name">
                    <description>
                      <text>L2 interface name.</text>
                    </description>
                    <type name="ncd:ifname"/>
                  </leaf>
                  <leaf name="ingress-adjust-tag-as-payload">
                    <status value="obsolete">
                      <ncd:revise-date date="2017-03-30"/>
                    </status>
                    <description>
                      <text>The ingress tag as payload flag of the ethernet parameter.</text>
                    </description>
                    <type name="enumeration">
                      <enum name="default">
                        <value value="0"/>
                        <description>
                          <text>Sub-interface means pTag. The others means uTag.</text>
                        </description>
                      </enum>
                      <enum name="all">
                        <value value="1"/>
                        <description>
                          <text>The ingress tag as payload flag is from outer most.</text>
                        </description>
                      </enum>
                      <enum name="from-sublayer">
                        <value value="2"/>
                        <description>
                          <text>The ingress tag as payload flag is from sub layer.</text>
                        </description>
                      </enum>
                    </type>
                    <default value="default"/>
                  </leaf>
                  <choice name="ingress-adjust">
                    <description>
                      <text>Configure ingress-adjustment operation</text>
                    </description>
                    <case name="tag-as-payload">
                      <description>
                        <text>The ingress tag as payload flag of the ethernet parameter.</text>
                      </description>
                      <leaf name="tag-as-payload">
                        <description>
                          <text>The ingress tag as payload flag of the ethernet parameter.</text>
                        </description>
                        <type name="enumeration">
                          <enum name="default">
                            <value value="0"/>
                            <description>
                              <text>Sub-interface means pTag. The others means uTag.</text>
                            </description>
                          </enum>
                          <enum name="all">
                            <value value="1"/>
                            <description>
                              <text>The ingress tag as payload flag is from outer most.</text>
                            </description>
                          </enum>
                          <enum name="from-sublayer">
                            <value value="2"/>
                            <description>
                              <text>The ingress tag as payload flag is from sub layer.</text>
                            </description>
                          </enum>
                        </type>
                        <default value="default"/>
                      </leaf>
                    </case>
                    <ncd:revise-date date="2017-03-30"/>
                  </choice>
                  <ncd:revise-date date="2016-08-18"/>
                </list>
              </case>
              <case name="vxlan-map-l3interface">
                <description>
                  <text>VXLAN map L3 interface.</text>
                </description>
                <leaf name="map-type-l3interface">
                  <description>
                    <text>VXLAN map L3 interface.</text>
                  </description>
                  <type name="empty"/>
                </leaf>
                <list name="map-l3interface">
                  <description>
                    <text>VXLAN map L3 interface.</text>
                  </description>
                  <key value="interface-name"/>
                  <leaf name="interface-name">
                    <description>
                      <text>L3 interface name.</text>
                    </description>
                    <type name="ncd:ifname">
                      <length value="1..31">
                        <description>
                          <text>The length of interface name must be between 1 to 31.</text>
                        </description>
                      </length>
                    </type>
                  </leaf>
                </list>
              </case>
            </choice>
          </container>
          <container name="vxlan-rd">
            <description>
              <text>Information of route distinguisher.</text>
            </description>
            <presence value="vxlan-rd"/>
            <leaf name="rd">
              <description>
                <text>The route distinguisher parameter.</text>
              </description>
              <type name="union">
                <type name="ncd:asn-nn"/>
                <type name="ncd:ipadd-nn"/>
              </type>
              <mandatory value="true"/>
            </leaf>
            <ncd:revise-date date="2016-01-08"/>
          </container>
          <container name="vxlan-route-targets">
            <description>
              <text>The configuration of route target.</text>
            </description>
            <presence value="vxlan-route-targets"/>
            <list name="vxlan-route-target">
              <description>
