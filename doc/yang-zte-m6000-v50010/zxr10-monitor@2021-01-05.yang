  container configuration {
    description 'MONITOR module configuration model'
                ;
    container monitor {
      description 'MONITOR module configuration root node'
                  ;
      container sessions {
        description 'MONITOR sessions'
                    ;
        list session {
          description 'MONITOR session'
                      ;
          key session-id ;
          leaf session-id {
            description 'MONITOR session ID'
                        ;
            type uint32 {
              range 1..64 ;
            }
          }
          leaf description {
            description 'Description for use.'
                        ;
            type ncd:string-with-blank {
              length 1..79 ;
            }
            ncd:revise-date 2020-06-15 ;
          }
          container acl-rules {
            description 'MONITOR destinations for acl rules'
                        ;
            list acl-rule {
              description 'MONITOR destination for one acl rule'
                          ;
              key 'acl-type acl-name'
                  ;
              leaf acl-type {
                description 'The ACL type'
                            ;
                type enumeration {
                  enum ipv4-acl {
                    value 1 ;
                    description 'The type of ACL is IPv4'
                                ;
                  }
                  enum ipv6-acl {
                    value 2 ;
                    description 'The type of ACL is IPv6'
                                ;
                  }
                  enum link-acl {
                    value 3 ;
                    description 'The type of ACL is LINK'
                                ;
                  }
                  enum ipv4-mixed-acl {
                    value 4 ;
                    description 'The type of ACL is IPv4 mixed'
                                ;
                  }
                }
              }
              leaf acl-name {
                description 'The ACL name'
                            ;
                type string {
                  length 1..31 ;
                }
              }
              choice destination-type {
                description 'The type of MONITOR destination'
                            ;
                mandatory true ;
                case interface {
                  description 'The type of MONITOR destination is interface'
                              ;
                  leaf interface-name {
                    description 'The interface name'
                                ;
                    type ncd:ifname ;
                  }
                }
                case vpws {
                  description 'The type of MONITOR destination is vpws.'
                              ;
                  leaf vpws-name {
                    description 'The vpws-for-monitor name'
                                ;
                    type string {
                      length 1..32 ;
                    }
                  }
                  ncd:revise-date 2017-04-14 ;
                }
              }
            }
          }
          container default {
            description 'The default MONITOR destination'
                        ;
            presence 'Default destination interface of mirror'
                     ;
            choice destination-type {
              description 'The type of MONITOR destination'
                          ;
              mandatory true ;
              case interface {
                description 'The type of MONITOR destination is interface'
                            ;
                leaf interface-name {
                  description 'The interface name of MONITOR destination'
                              ;
                  type ncd:ifname ;
                }
              }
            }
          }
          container default-vpwss {
            description 'Configuration port mirror of remote mirror by vpws.'
                        ;
            list default-vpws {
              description 'Configuration list of port mirror of remote mirror b'
                        + 'y vpws.'
                          ;
              key vpws-name ;
              leaf vpws-name {
                description 'The vpws name'
                            ;
                type string {
                  length 1..32 ;
                }
              }
            }
            ncd:revise-date 2017-04-14 ;
          }
          container sources {
            description 'The MONITOR sources'
                        ;
            list interface-source {
              description 'The MONITOR source whose type is interface'
                          ;
              key 'interface-name direction'
                  ;
              leaf interface-name {
                description 'The interface name of MONITOR source'
                            ;
                type ncd:ifname ;
              }
              leaf direction {
                description 'The direction for MONITOR source'
                            ;
                type enumeration {
                  enum rx {
                    value 2 ;
                    description 'The MONITOR direction is receive'
                                ;
                  }
                  enum tx {
                    value 3 ;
                    description 'The MONITOR direction is transmit'
                                ;
                  }
                }
              }
              leaf flow-type {
                description flow-type ;
                type enumeration {
                  enum bfd-default {
                    value 62 ;
                    description 'Packets from unknown BFD sessions'
                                ;
                  }
                }
                ncd:revise-date 2017-04-14 ;
              }
            }
            list vpws-source {
              description 'The MONITOR source whose type is vpws'
                          ;
              key 'vpws-name direction'
                  ;
              leaf vpws-name {
                description 'The vpws name of MONITOR source'
                            ;
                type string {
                  length 1..32 ;
                }
              }
              leaf direction {
                description 'The direction for MONITOR source'
                            ;
                type enumeration {
                  enum rx {
                    value 2 ;
                    description 'The MONITOR direction is receive'
                                ;
                  }
                }
              }
              ncd:revise-date 2017-04-14 ;
            }
            container switchvlan-sources {
              // if-feature switchvlan-source-feature ;
              description 'The switchvlan source.'
                          ;
              list switchvlan-source {
                description 'The switchvlan source.'
                            ;
                key 'switchvlan-id direction'
                    ;
                leaf switchvlan-id {
                  description 'The vlanid of switchvlan source.'
                              ;
                  type uint16 {
                    range 1..4094 ;
                  }
                }
                leaf direction {
                  description 'The direction for monitor source.'
                              ;
                  type enumeration {
                    enum rx {
                      value 2 ;
                      description 'The monitor source direction is receive'
                                  ;
                    }
                    enum tx {
                      value 3 ;
                      description 'The monitor source direction is transmit'
                                  ;
                    }
                    enum both {
                      value 1 ;
                      description 'Set direction to both rx and tx'
                                  ;
                    }
                  }
                }
              }
              ncd:revise-date 2021-01-05 ;
            }
            container cpu-sources {
              // if-feature cpu-source-feature ;
              description 'The CPU source.'
                          ;
              list cpu-source {
                description 'The CPU source.'
                            ;
                key 'interface-name direction flow-type'
                    ;
                leaf interface-name {
                  description 'The interface name of MONITOR source'
                              ;
                  type ncd:ifname ;
                }
                leaf direction {
                  description 'The direction for MONITOR source'
                              ;
                  type enumeration {
                    enum rx {
                      value 5 ;
                    }
                    enum tx {
                      value 6 ;
                    }
                  }
                }
                leaf flow-type {
                  description 'Flow type.'
                              ;
                  type flow-type ;
                }
              }
              ncd:revise-date 2021-01-05 ;
            }
          }
          container default-observers {
            description 'Specify the source address and destination address of '
                      + 'port mirror.'
                        ;
            list default-observer {
              description 'Specify the source address and destination address o'
                        + 'f port mirror.'
                          ;
              key 'source-ipv4-addr server-ipv4-addr'
                  ;
              leaf source-ipv4-addr {
                description 'Specify source IPv4 address.'
                            ;
                type ncd:ipv4-address ;
              }
              leaf server-ipv4-addr {
                description 'Specify server IPv4 address.'
                            ;
                type ncd:ipv4-address ;
              }
            }
            ncd:revise-date 2017-04-14 ;
          }
          container rule-observers {
            description 'Specify the source address and destination address of '
                      + 'acl mirror.'
                        ;
            list rule-observer {
              description 'Specify the source address and destination address o'
                        + 'f acl mirror.'
                          ;
              key 'acl-type acl-name source-ipv4-addr server-ipv4-addr'
                  ;
              leaf acl-type {
                description 'ACL type.'
                            ;
                type enumeration {
                  enum ipv4-acl {
                    value 1 ;
                    description 'The type of ACL is IPv4'
                                ;
                  }
                  enum ipv6-acl {
                    value 2 ;
                    description 'The type of ACL is IPv6'
                                ;
                  }
                  enum link-acl {
                    value 3 ;
                    description 'The type of ACL is LINK'
                                ;
                  }
                }
              }
              leaf acl-name {
                description 'acl name'
                            ;
                type string {
                  length 1..31 {
                    description 'The acl name'
                                ;
                  }
                }
              }
              leaf source-ipv4-addr {
                description 'Specify source IPv4 address.'
                            ;
                type ncd:ipv4-address ;
              }
              leaf server-ipv4-addr {
                description 'Specify server IPv4 address.'
                            ;
                type ncd:ipv4-address ;
              }
            }
            ncd:revise-date 2017-04-14 ;
          }
          container default-gres {
            description 'config destination of gre interface'
                        ;
            list default-gre {
              description 'destination of gre interface'
                          ;
              key gre-interface ;
              leaf gre-interface {
                description 'Gre_tunnel interface'
                            ;
                type ncd:ifname ;
              }
              container encap-remote-monitor {
                // if-feature encap-remote-monitor-feature ;
                description 'Configure the ERSPAN.'
                            ;
                leaf tpid {
                  description 'Configure the MAC header TPID of ERmonitor'
                              ;
                  type enumeration {
                    enum 0x8100 {
                      value 1 ;
                    }
                  }
                  default 0x8100 ;
                }
                leaf precedence {
                  description 'Configure the IP precedence value of ERmonitor.'
                              ;
                  type uint8 {
                    range 0..7 ;
                  }
                  default 0 ;
                }
                leaf flags {
                  description 'Configure the VLAN flags of ERmonitor traffic'
                              ;
                  type enumeration {
                    enum disable {
                      value 0 ;
                    }
                    enum enable {
                      value 1 ;
                    }
                  }
                  default disable ;
                }
                leaf ttl {
                  description 'Configure the IP time-to-live (TTL) value.'
                              ;
                  type uint8 {
                    range 1..255 ;
                  }
                  default 128 ;
                }
                leaf dscp {
                  description 'Configure the IP DSCP value.'
                              ;
                  type uint8 {
                    range 0..63 {
                      description 'dscp range'
                                  ;
                    }
                  }
                  default 0 ;
                }
                ncd:revise-date 2021-01-05 ;
              }
            }
            ncd:revise-date 2017-05-16 ;
          }
          container multi-acl-destinations {
            // if-feature multi-acl-feature ;
            description 'Specify multiply ACL destination.'
                        ;
            list multi-interface {
              description 'Specify interface destination.'
                          ;
              key 'acl-type acl-name interface-name'
                  ;
              leaf acl-type {
                description 'Specify ACL type.'
                            ;
                type enumeration {
                  enum ipv4-acl {
                    value 1 ;
                  }
                  enum ipv6-acl {
                    value 2 ;
                  }
                  enum link-acl {
                    value 3 ;
                  }
                  enum ipv4-mixed-acl {
                    value 4 ;
                  }
                }
              }
              leaf acl-name {
                description 'Specify ACL name.'
                            ;
                type string {
                  length 1..31 ;
                }
              }
              leaf interface-name {
                description 'Specify interface name.'
                            ;
                type ncd:ifname ;
              }
            }
            list multi-vpws {
              description 'Specify VPWS destination.'
                          ;
              key 'acl-type acl-name vpws-name'
                  ;
              leaf acl-type {
                description 'Specify ACL type.'
                            ;
                type enumeration {
                  enum ipv4-acl {
                    value 1 ;
                  }
                  enum ipv6-acl {
                    value 2 ;
                  }
                  enum link-acl {
                    value 3 ;
                  }
                  enum ipv4-mixed-acl {
                    value 4 ;
                  }
                }
              }
              leaf acl-name {
                description 'Specify ACL name.'
                            ;
                type string {
                  length 1..31 ;
                }
              }
              leaf vpws-name {
                description 'Specify VPWS name.'
                            ;
                type string {
                  length 1..32 ;
                }
              }
            }
            ncd:revise-date 2020-04-13 ;
          }
          container default-interfaces {
            // if-feature default-interface-feature ;
            description 'Specify interface destination.'
                        ;
            list default-interface {
              description 'Specify interface destination.'
                          ;
              key interface-name ;
              leaf interface-name {
                description 'Specify interface name.'
                            ;
                type ncd:ifname ;
              }
              container remote-monitor {
                // if-feature remote-monitor-feature ;
                description 'Configure remote monitor.'
                            ;
                presence 'Configuration rspan'
                         ;
                leaf vlan-id {
                  description 'Configure the RSPAN.'
                              ;
                  type uint16 {
                    range 1..4094 {
                      description 'RSPAN VLAN ID'
                                  ;
                    }
                  }
                  mandatory true ;
                }
                container tpid-priority {
                  description 'Configure the tpid and priority.'
                              ;
                  leaf tpid {
                    description 'Configure the MAC header TPID.'
                                ;
                    type enumeration {
                      enum 0x8100 {
                        value 1 ;
                      }
                      enum 0x88A8 {
                        value 2 ;
                      }
                      enum 0x9100 {
                        value 3 ;
                      }
                      enum 0x9200 {
                        value 4 ;
                      }
                    }
                    default 0x8100 ;
                  }
                  leaf priority {
                    description 'Configure the priority.'
                                ;
                    type uint8 {
                      range 0..7 ;
                    }
                    default 0 ;
                  }
                }
                ncd:revise-date 2021-01-05 ;
              }
            }
            ncd:revise-date 2020-04-13 ;
          }
          container default-vxlans {
            // if-feature default-vxlan-feature ;
            description 'The destination of VXLAN.'
                        ;
            container ipv4s {
              description 'Specify IPv4 address.'
                          ;
              list ipv4 {
                description 'Specify IPv4 address.'
                            ;
                key 'source-ipv4-addr server-ipv4-addr'
                    ;
                leaf source-ipv4-addr {
                  description 'Source IPv4 address.'
                              ;
                  type ncd:ipv4-address ;
                }
                leaf server-ipv4-addr {
                  description 'Server IPv4 address.'
                              ;
                  type ncd:ipv4-address ;
                }
                leaf vpls {
                  description 'VPLS instance.'
                              ;
                  type string {
                    length 1..32 ;
                  }
                  mandatory true ;
                }
              }
            }
            container ipv6s {
              description 'Specify IPv6 address.'
                          ;
              list ipv6 {
                description 'Specify IPv6 address.'
                            ;
                key 'source-ipv6-addr server-ipv6-addr'
                    ;
                leaf source-ipv6-addr {
                  description 'Source IPv6 address.'
                              ;
                  type ncd:ipv6-address ;
                }
                leaf server-ipv6-addr {
                  description 'Server IPv6 address.'
                              ;
                  type ncd:ipv6-address ;
                }
                leaf vpls {
                  description 'VPLS instance.'
                              ;
                  type string {
                    length 1..32 ;
                  }
                  mandatory true ;
                }
              }
            }
            ncd:revise-date 2021-01-05 ;
          }
        }
      }
      container rate-limits {
        description 'MONITOR rate limit configuration'
                    ;
        list rate-limit {
          description 'MONITOR rate limit configuration list'
                      ;
          key board-name ;
          leaf board-name {
            description 'The board name supported rate limit by monitor'
                        ;
            type ncd:board {
              length 1..19 ;
            }
          }
          leaf cir {
            description 'Configure Committed Information Rate value,Unit is Mbp'
                      + 's.'
                        ;
            type uint32 {
              range 1..30000 {
                description 'CIR value(Mbps)'
                            ;
              }
            }
            mandatory true ;
          }
          leaf cbs {
            description 'Configure Committed Burst Size value,Unit is MB.'
                        ;
            type uint32 {
              range 1..16 {
                description 'CBS value(MB)'
                            ;
              }
            }
            mandatory true ;
          }
          container pir-pbs {
            description 'Configure Peak Information Rate value and Peak Burst S'
                      + 'ize value.'
                        ;
            presence pir-pbs ;
            leaf pir {
              description 'Configure Peak Information Rate value,Unit is Mbps.'
                          ;
              type uint32 {
                range 1..30000 {
                  description 'PIR value(Mbps)'
                              ;
                }
              }
              mandatory true ;
            }
            leaf pbs {
              description 'Configure Peak Burst Size value,Unit is MB.'
                          ;
              type uint32 {
                range 1..16 {
                  description 'PBS value(MB)'
                              ;
                }
              }
              mandatory true ;
            }
          }
        }
        ncd:revise-date 2017-04-14 ;
      }
    }
  }
}
