    <container name="tcp-aos">
      <description>
        <text>List of TCP AO configuration.</text>
      </description>
      <list name="tcp-ao">
        <key value="name"/>
        <description>
          <text>Configure TCP AO instance.</text>
        </description>
        <leaf name="name">
          <type name="string">
            <length value="1..47"/>
          </type>
          <description>
            <text>The name of a TCP AO instance.</text>
          </description>
        </leaf>
        <leaf name="accept-mismatch">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable accepting TCP segments even though AO mismatched.</text>
          </description>
        </leaf>
        <container name="keychains">
          <description>
            <text>List of bound keychain.</text>
          </description>
          <list name="keychain">
            <key value="kc-name"/>
            <max-elements value="1"/>
            <description>
              <text>Configure a keychain instance.</text>
            </description>
            <leaf name="kc-name">
              <type name="string">
                <length value="1..47"/>
                <pattern value="[^A-Z]+"/>
              </type>
              <description>
                <text>Specify a keychain name.</text>
              </description>
            </leaf>
            <container name="key-ids">
              <description>
                <text>List of key on a TCP AO instance.</text>
              </description>
              <list name="key-id">
                <key value="kc-keyid"/>
                <unique tag="send-id"/>
                <unique tag="receive-id"/>
                <description>
                  <text>Configure key instance.</text>
                </description>
                <leaf name="kc-keyid">
                  <type name="uint32">
                    <range value="0..63"/>
                  </type>
                  <description>
                    <text>ID of key.</text>
                  </description>
                </leaf>
                <leaf name="send-id">
                  <type name="uint8"/>
                  <description>
                    <text>Send ID.</text>
                  </description>
                </leaf>
                <leaf name="receive-id">
                  <type name="uint8"/>
                  <description>
                    <text>Receive ID.</text>
                  </description>
                </leaf>
                <leaf name="include-tcp-options">
                  <type name="boolean"/>
                  <default value="true"/>
                  <description>
                    <text>Enable/disable including TCP options header with AO.</text>
                  </description>
                </leaf>
              </list>
            </container>
          </list>
        </container>
      </list>
    </container>
    <container name="tcp-statisticss">
      <config value="false"/>
      <description>
        <text>List of TCP packets statistics.</text>
      </description>
      <list name="tcp-statistics">
        <key value="family"/>
        <description>
          <text>Statistics of TCP packets.</text>
        </description>
        <leaf name="family">
          <type name="socket-proto-type"/>
          <description>
            <text>Protocol type.</text>
          </description>
        </leaf>
        <leaf name="rcv-packet">
          <type name="uint32"/>
          <units name="packet"/>
          <description>
            <text>Received packets statistics.</text>
          </description>
        </leaf>
        <leaf name="rcv-count">
          <type name="uint32"/>
          <units name="packet"/>
          <description>
            <text>Indicates the number of received fragments.</text>
          </description>
        </leaf>
        <leaf name="pkt-in-seq">
          <type name="uint32"/>
          <units name="packet"/>
          <description>
            <text>Packets in sequence.</text>
          </description>
        </leaf>
        <leaf name="rcv-win-probe-pkt">
          <type name="uint32"/>
          <units name="packet"/>
          <description>
            <text>Received window probe packets.</text>
          </description>
