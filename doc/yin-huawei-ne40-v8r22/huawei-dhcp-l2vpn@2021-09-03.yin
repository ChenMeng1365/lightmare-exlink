<?xml version="1.0" encoding="UTF-8"?>
<module name="huawei-dhcp-l2vpn"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:dhcp-l2vpn="urn:huawei:yang:huawei-dhcp-l2vpn"
        xmlns:dhcp="urn:huawei:yang:huawei-dhcp"
        xmlns:l2vpn="urn:huawei:yang:huawei-l2vpn"
        xmlns:ext="urn:huawei:yang:huawei-extension">
  <yang-version value="1"/>
  <namespace uri="urn:huawei:yang:huawei-dhcp-l2vpn"/>
  <prefix value="dhcp-l2vpn"/>
  <import module="huawei-dhcp">
    <prefix value="dhcp"/>
  </import>
  <import module="huawei-l2vpn">
    <prefix value="l2vpn"/>
  </import>
  <import module="huawei-extension">
    <prefix value="ext"/>
  </import>
  <organization>
    <text>Huawei Technologies Co., Ltd.</text>
  </organization>
  <contact>
    <text>Huawei Industrial Base
Bantian, Longgang
Shenzhen 518129
People's Republic of China
Website: http://www.huawei.com
Email: support@huawei.com</text>
  </contact>
  <description>
    <text>This module contains a collection of YANG definitions for managing DHCP l2vpn.</text>
  </description>
  <revision date="2021-09-03">
    <description>
      <text>Initial revision.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <ext:task-name value="dhcp"/>
  <augment target-node="/l2vpn:l2vpn/l2vpn:instances/l2vpn:instance/l2vpn:vpls">
    <description>
      <text>Extend L2VPN attribute.</text>
    </description>
    <container name="dhcp-snooping-vsi">
      <description>
        <text>Configure DHCP snooping VSI attributes.</text>
      </description>
      <leaf name="nni-server-enable">
        <when condition="/dhcp:dhcp/dhcp:snooping/dhcp:global/dhcp:enable='true'"/>
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable the network node interface to server.</text>
        </description>
      </leaf>
    </container>
  </augment>
</module>
