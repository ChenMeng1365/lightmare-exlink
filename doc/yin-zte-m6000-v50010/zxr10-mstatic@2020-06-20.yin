      </container>
      <container name="mstatic-v6">
        <description>
          <text>IPv6 MSTATIC configuration.</text>
        </description>
        <presence value="mstatic-v6"/>
        <container name="public">
          <description>
            <text>Public mode of IPv6 MSTATIC module configure node.</text>
          </description>
          <container name="global">
            <description>
              <text>IPv6 MSTATIC global start configuration.</text>
            </description>
            <leaf name="start">
              <description>
                <text>IPv6 MSTATIC start flag.</text>
              </description>
              <type name="empty"/>
            </leaf>
          </container>
          <container name="limit">
            <description>
              <text>Configure IPv6 MSTATIC route limit.</text>
            </description>
            <presence value="limit"/>
            <leaf name="xg-limit">
              <description>
                <text>XG limit.</text>
              </description>
              <type name="int32">
