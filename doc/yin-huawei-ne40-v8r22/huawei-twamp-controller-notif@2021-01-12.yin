  <grouping name="session-info">
    <description>
      <text>Session information of TWAMP alarm.</text>
    </description>
    <leaf name="sender-address">
      <type name="inet:ip-address-no-zone"/>
      <description>
        <text>Sender IP.</text>
      </description>
    </leaf>
    <leaf name="reflector-address">
      <type name="inet:ip-address-no-zone"/>
      <description>
        <text>Reflector IP.</text>
      </description>
    </leaf>
    <leaf name="sender-port">
      <type name="uint32"/>
      <description>
        <text>Sender Port.</text>
      </description>
    </leaf>
    <leaf name="reflector-port">
      <type name="uint32"/>
      <description>
        <text>Reflector port.</text>
      </description>
    </leaf>
    <leaf name="vpn-name">
      <type name="leafref">
        <path value="/ni:network-instance/ni:instances/ni:instance/ni:name"/>
      </type>
      <description>
        <text>VPN instance name.</text>
      </description>
    </leaf>
  </grouping>
  <notification name="sender-session-num-exceed">
    <description>
      <text>The number of TWAMP sender session is out of device capability.</text>
    </description>
  </notification>
  <notification name="sender-session-num-recovery">
    <description>
      <text>The number of TWAMP sender session is under device capability.</text>
    </description>
  </notification>
  <notification name="twoway-delay-exceed">
    <description>
      <text>The two-way delay of the TWAMP test instance exceeded the upper threshold in three consecutive test intervals.</text>
    </description>
    <leaf name="session-id">
      <type name="uint32"/>
      <description>
        <text>TWAMP Light test session ID.</text>
      </description>
    </leaf>
    <leaf name="value">
      <type name="uint32"/>
      <description>
        <text>Delay value.</text>
      </description>
    </leaf>
    <leaf name="upper-limit-threshold">
      <type name="uint32"/>
      <description>
        <text>The Delay trigger-alarm threshold value.</text>
      </description>
    </leaf>
    <leaf name="lower-limit-threshold">
      <type name="uint32"/>
      <description>
        <text>The clear-alarm Delay threshold value.</text>
      </description>
    </leaf>
    <uses name="session-info"/>
  </notification>
