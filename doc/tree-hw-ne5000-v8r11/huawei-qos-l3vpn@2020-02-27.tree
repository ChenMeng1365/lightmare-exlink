module: huawei-qos-l3vpn
  +--ro qos-l3vpn
     +--ro policy-statisticss
     |  +--ro policy-statistics* [vrf-name direction slot-id]
     |     +--ro vrf-name                  -> /ni:network-instance/instances/instance/name
     |     +--ro direction                 qos:in-direction-type
     |     +--ro slot-id                   qos:slot-id
     |     +--ro policy-name?              -> /qos:qos/classifier-template/traffic-policys/traffic-policy/name
     |     +--ro apply-time?               yang:date-and-time
     |     +--ro enable-time?              yang:date-and-time
     |     +--ro clear-time?               yang:date-and-time
     |     +--ro clear-flag?               boolean
     |     +--ro match-packets?            uint64
     |     +--ro match-bytes?              uint64
     |     +--ro match-packet-rate?        uint64
     |     +--ro match-byte-rate?          uint64
     |     +--ro miss-packets?             uint64
     |     +--ro miss-bytes?               uint64
     |     +--ro match-pass-packets?       uint64
     |     +--ro match-pass-bytes?         uint64
