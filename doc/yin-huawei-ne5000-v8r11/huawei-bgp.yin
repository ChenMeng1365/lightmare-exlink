                  <units name="s"/>
                  <default value="0"/>
                  <description>
                    <text>Route selection delay.</text>
                  </description>
                </leaf>
                <uses name="reflect-type"/>
                <uses name="reflector-cluster-type"/>
                <leaf name="reflect-between-client">
                  <type name="boolean"/>
