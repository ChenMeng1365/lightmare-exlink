  <augment target-node="/ifm:ifm/ifm:interfaces/ifm:interface">
    <description>
      <text>Configure Y1731 attributes on interfaces.</text>
    </description>
    <container name="y1731">
      <when condition="/y1731:y1731/y1731:global-enable"/>
      <description>
        <text>Configure Y1731 attributes on an interface.</text>
      </description>
      <container name="manual-mip">
        <presence value="Enable/disable a MIP on an interface."/>
        <description>
          <text>Configure a MIP on an interface.</text>
        </description>
        <leaf name="level">
          <type name="uint8">
            <range value="0..7"/>
          </type>
          <mandatory value="true"/>
          <description>
            <text>Configure the level of a MIP.</text>
          </description>
          <ext:support-filter value="true"/>
        </leaf>
        <leaf name="mac">
          <type name="pub-type:mac-address"/>
          <config value="false"/>
          <description>
            <text>MAC address of an interface.</text>
          </description>
        </leaf>
      </container>
      <container name="auto-mips">
        <config value="false"/>
        <description>
          <text>Operational state of instances of MIP points.</text>
        </description>
        <leaf name="level">
          <type name="uint8">
            <range value="0..7"/>
          </type>
          <description>
            <text>The level of a MIP.</text>
          </description>
          <ext:support-filter value="true"/>
        </leaf>
        <leaf name="mac">
          <type name="pub-type:mac-address"/>
          <description>
            <text>MAC address of an interface.</text>
          </description>
        </leaf>
      </container>
      <container name="measure-granularity">
        <when condition="/y1731:y1731/y1731:global-enable/y1731:mds/y1731:md/y1731:mas/y1731:ma/y1731:meps/y1731:mep[y1731:if-name=current()/../../ifm:name]"/>
        <description>
          <text>Configure the measure granularity of an interface.</text>
        </description>
        <leaf name="measure-gran">
          <type name="measure-gran"/>
          <default value="port-vlan"/>
          <description>
            <text>Configure measure granularity of an interface.</text>
          </description>
        </leaf>
      </container>
      <container name="trigger-if">
        <presence value="Enable/disable a trigger type on an interface."/>
        <description>
          <text>Configure the y1731 trigger if.</text>
        </description>
        <leaf name="md-name">
          <ext:operation-exclude value="update"/>
          <type name="leafref">
            <path value="/y1731:y1731/y1731:global-enable/y1731:mds/y1731:md/y1731:name"/>
          </type>
          <mandatory value="true"/>
          <description>
            <text>Configure the name of a maintenance domain (MD).</text>
          </description>
        </leaf>
        <leaf name="ma-name">
          <ext:operation-exclude value="update"/>
          <type name="leafref">
            <path value="/y1731:y1731/y1731:global-enable/y1731:mds/y1731:md[y1731:name=current()/../y1731:md-name]/y1731:mas/y1731:ma/y1731:name"/>
          </type>
          <mandatory value="true"/>
          <description>
            <text>Configure the name of a maintenance association (MA).</text>
          </description>
        </leaf>
        <leaf name="rmep-id">
          <ext:operation-exclude value="update"/>
          <type name="leafref">
            <path value="/y1731:y1731/y1731:global-enable/y1731:mds/y1731:md[y1731:name=current()/../y1731:md-name]/y1731:mas/y1731:ma[y1731:name=current()/../y1731:ma-name]/y1731:rmeps/y1731:rmep/y1731:id"/>
          </type>
          <mandatory value="true"/>
          <description>
            <text>Configure a RMEP ID.</text>
          </description>
        </leaf>
        <leaf name="trigger-type">
          <ext:operation-exclude value="update"/>
          <type name="trigger-type"/>
          <mandatory value="true"/>
          <description>
            <text>Configure a trigger type.</text>
          </description>
        </leaf>
      </container>
    </container>
  </augment>
</module>
