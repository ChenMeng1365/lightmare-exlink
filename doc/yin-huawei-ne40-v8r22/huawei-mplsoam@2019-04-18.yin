  <grouping name="session-status">
    <leaf name="bdi-defect-status">
      <type name="defect-state"/>
      <config value="false"/>
      <description>
        <text>BDI defect status.</text>
      </description>
    </leaf>
    <leaf name="bdi-defect-type">
      <type name="string">
        <length value="1..15"/>
      </type>
      <config value="false"/>
      <description>
        <text>BDI defect type.</text>
      </description>
    </leaf>
    <leaf name="available-status">
      <type name="available-state"/>
      <config value="false"/>
      <description>
        <text>Available status.</text>
      </description>
    </leaf>
    <leaf name="defect-type">
      <type name="string">
        <length value="1..15"/>
      </type>
      <config value="false"/>
      <description>
        <text>Defect type.</text>
      </description>
    </leaf>
    <leaf name="local-detect-status">
      <type name="defect-status-type"/>
      <default value="init"/>
      <config value="false"/>
      <description>
        <text>Local detection status.</text>
      </description>
    </leaf>
    <leaf name="local-invalid-time">
      <type name="uint32">
        <range value="0..4294967294"/>
      </type>
      <config value="false"/>
      <description>
        <text>Local invalid time.</text>
      </description>
    </leaf>
    <leaf name="local-defect-location">
      <type name="inet:ipv4-address-no-zone"/>
      <config value="false"/>
      <description>
        <text>Local defect status.</text>
      </description>
    </leaf>
    <leaf name="local-defect-type">
      <type name="defect-type"/>
      <config value="false"/>
      <description>
        <text>Local defect type.</text>
      </description>
    </leaf>
    <leaf name="remote-detect-status">
      <type name="defect-status-type"/>
      <default value="init"/>
      <config value="false"/>
      <description>
        <text>Remote detection status.</text>
      </description>
    </leaf>
    <leaf name="remote-invalid-time">
      <type name="uint32">
        <range value="0..4294967294"/>
      </type>
      <config value="false"/>
      <description>
        <text>Remote invalid time.</text>
      </description>
    </leaf>
    <leaf name="remote-defect-location">
      <type name="inet:ipv4-address-no-zone"/>
      <config value="false"/>
      <description>
        <text>Remote defect status.</text>
      </description>
    </leaf>
    <leaf name="remote-defect-type">
      <type name="defect-type"/>
      <config value="false"/>
      <description>
        <text>Remote defect type.</text>
      </description>
    </leaf>
    <description>
      <text>Session status.</text>
    </description>
  </grouping>
  <grouping name="l2vpn-pw">
    <leaf name="remote-lsr-id">
      <type name="inet:ipv4-address-no-zone"/>
      <ext:dynamic-default/>
      <description>
        <text>Configure a remote LSR ID.</text>
      </description>
    </leaf>
    <leaf name="remote-vc-id">
      <type name="uint32">
        <range value="0..4294967295"/>
      </type>
      <ext:dynamic-default/>
      <description>
        <text>Configure a remote VC ID of a static PW to which MPLS OAM is bound.</text>
      </description>
    </leaf>
    <leaf name="remote-vc-type">
      <type name="l2vc-type"/>
      <ext:dynamic-default/>
      <description>
        <text>Configure a remote VC type of a static PW to which MPLS OAM is bound.</text>
      </description>
    </leaf>
    <leaf name="detect-pkt-type">
      <type name="detect-pkt-type"/>
      <default value="cv"/>
      <description>
        <text>Configure a detection packet type.</text>
      </description>
    </leaf>
    <leaf name="detect-mode">
      <type name="detect-mode"/>
      <default value="auto"/>
      <description>
        <text>Configure a detection mode.</text>
      </description>
    </leaf>
    <leaf name="auto-mode-over-time">
      <when condition="../detect-mode='auto'"/>
      <type name="uint8">
        <range value="0..255"/>
      </type>
      <default value="10"/>
      <description>
        <text>Configure the over time.</text>
      </description>
    </leaf>
    <leaf name="detect-frequence">
      <type name="detect-interval"/>
      <must condition="(../detect-pkt-type='cv' and ../detect-frequence='1000ms') or (../detect-pkt-type='ffd' and ../detect-frequence!='1000ms')"/>
      <mandatory value="true"/>
      <description>
        <text>Configure the detection packet type.</text>
      </description>
    </leaf>
    <leaf name="bdi-frequence">
      <type name="bdi-frequency"/>
      <default value="detect-freq"/>
      <description>
        <text>BDI packet frequency, detect-freq indicate the same as frequency of detecting packet.</text>
      </description>
    </leaf>
    <leaf name="compatibility-mode">
      <type name="compatibility-mode"/>
      <default value="route-mode"/>
      <description>
        <text>Configure compatibility mode.</text>
      </description>
    </leaf>
    <leaf name="oam-active-status">
      <type name="active-type"/>
      <default value="deactive"/>
      <description>
        <text>Whether MPLS OAM detection is enabled.</text>
      </description>
    </leaf>
    <leaf name="send-enable">
      <type name="boolean"/>
      <default value="false"/>
      <description>
        <text>Enable/disable the MPLS OAM packets can be sent.</text>
      </description>
    </leaf>
    <leaf name="receive-enable">
      <type name="boolean"/>
      <default value="false"/>
      <description>
        <text>Enable/disable MPLS OAM packets can be received.</text>
      </description>
    </leaf>
    <leaf name="oam-loss-sd">
      <type name="uint32">
        <range value="0..100"/>
      </type>
      <default value="0"/>
      <description>
        <text>Configure the SD threshold of OAM loss measurement.</text>
      </description>
    </leaf>
    <leaf name="oam-loss-sf">
      <type name="uint32">
        <range value="0..100"/>
      </type>
      <must condition="../oam-loss-sd&lt;=../oam-loss-sf"/>
      <default value="0"/>
      <description>
        <text>Configure the SF threshold of OAM loss Measurement.</text>
      </description>
    </leaf>
    <leaf name="slm-send-enable">
      <type name="boolean"/>
      <default value="false"/>
      <description>
        <text>Enable/disable a device sends packets for proactive single-ended loss measurement.</text>
      </description>
    </leaf>
    <leaf name="slm-exp">
      <when condition="../slm-send-enable='true'"/>
      <type name="uint8">
        <range value="0..7"/>
      </type>
      <default value="7"/>
      <description>
        <text>Configure the exp for proactive single-ended loss measurement.</text>
      </description>
    </leaf>
    <leaf name="slm-threshold1">
      <type name="uint32">
        <range value="1..1000000"/>
      </type>
      <default value="1"/>
      <description>
        <text>Configure threshold1 of proactive single-ended loss measurement.</text>
      </description>
    </leaf>
    <leaf name="slm-threshold2">
      <type name="uint32">
        <range value="1..1000000"/>
      </type>
      <default value="10"/>
      <description>
        <text>Configure threshold2 of proactive single-ended loss measurement.</text>
      </description>
    </leaf>
    <leaf name="two-dm-send-enable">
      <type name="boolean"/>
      <default value="false"/>
      <description>
        <text>Enable/disable a device sends packets for proactive two-way delay measurement.</text>
      </description>
    </leaf>
    <leaf name="two-dm-exp">
      <when condition="../two-dm-send-enable='true'"/>
      <type name="uint8">
        <range value="0..7"/>
      </type>
      <default value="7"/>
      <description>
        <text>Configure the exp value of proactive two-way delay measurement.</text>
      </description>
    </leaf>
    <leaf name="two-dm-packet-size">
      <when condition="../two-dm-send-enable='true'"/>
      <type name="uint32">
        <range value="64..1500"/>
      </type>
      <default value="64"/>
