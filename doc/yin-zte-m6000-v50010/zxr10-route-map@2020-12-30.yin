                  <description>
                    <text>Set the DT2M function code value.</text>
                  </description>
                  <type name="ncd:ipv6-address"/>
                  <mandatory value="true"/>
                </leaf>
              </container>
              <container name="dt2u">
                <description>
                  <text>DT2U function code.</text>
                </description>
                <presence value="dt2u"/>
                <leaf name="value">
                  <description>
                    <text>Set the DT2U function code value.</text>
                  </description>
                  <type name="ncd:ipv6-address"/>
