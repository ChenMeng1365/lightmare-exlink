                  </description>
                  <type name="ncd:ifname">
                    <length value="1..31"/>
                  </type>
                  <ncd:revise-date date="2015-08-28"/>
                </leaf>
                <leaf name="slave-track-name">
                  <description>
                    <text>Slave upstream interface track name.</text>
                  </description>
                  <type name="string">
                    <length value="1..31"/>
                  </type>
                  <ncd:revise-date date="2015-08-28"/>
                </leaf>
                <leaf name="permanent">
                  <description>
                    <text>Permanent route.</text>
                  </description>
                  <type name="empty"/>
                  <ncd:revise-date date="2015-08-28"/>
                </leaf>
                <leaf name="oif-index">
                  <description>
                    <text>Downstream interface index.</text>
                  </description>
                  <type name="uint16">
                    <range value="1..100">
                      <description>
