  <augment target-node="/radius:radius/radius:radius-server-groups/radius:radius-server-group">
    <description>
      <text>Adds l2tp configuration to radius group model.</text>
    </description>
    <container name="l2tp">
      <description>
        <text>Configure l2tp to radius group model.</text>
      </description>
      <container name="lts-tunnel-format-vendor">
        <description>
          <text>Configure a vendor-specific encapsulation format for LTS user tunnel attributes.</text>
        </description>
        <leaf name="redback-encapsulation">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable the Redback encapsulation format for LTS user tunnel attributes.</text>
          </description>
        </leaf>
      </container>
      <container name="lns-nas-port-id">
        <description>
          <text>Configure the format of the nas-port-id attribute sent by the L2TP LNS to the RADIUS server.</text>
        </description>
        <container name="include-attributes">
          <description>
            <text>List of the user-defined format of the attributes.</text>
          </description>
          <list name="include-attribute">
            <key value="attribute-type"/>
            <max-elements value="7"/>
            <ordered-by value="user"/>
            <description>
              <text>Configure the user-defined format of the attribute.</text>
            </description>
            <leaf name="attribute-type">
              <type name="enumeration">
                <enum name="local-tunnel-ip">
                  <value value="1"/>
                  <description>
                    <text>Local tunnel IP.</text>
                  </description>
                </enum>
                <enum name="peer-tunnel-ip">
                  <value value="2"/>
                  <description>
                    <text>Peer tunnel IP.</text>
                  </description>
                </enum>
                <enum name="local-tunnel-id">
                  <value value="3"/>
                  <description>
                    <text>Local tunnel ID.</text>
                  </description>
                </enum>
                <enum name="peer-tunnel-id">
                  <value value="4"/>
                  <description>
                    <text>Peer tunnel ID.</text>
                  </description>
                </enum>
                <enum name="local-session-id">
                  <value value="5"/>
                  <description>
                    <text>Local session ID.</text>
                  </description>
                </enum>
                <enum name="peer-session-id">
                  <value value="6"/>
                  <description>
                    <text>Peer session ID.</text>
                  </description>
                </enum>
                <enum name="call-serial-number">
                  <value value="7"/>
                  <description>
                    <text>Call serial number.</text>
                  </description>
                </enum>
              </type>
              <description>
                <text>Configures the user-defined format of the nas-port-id attribute type.</text>
              </description>
            </leaf>
            <leaf name="delimiter">
              <type name="string">
                <length value="1"/>
              </type>
              <description>
                <text>Configures the user-defined format of the attribute delimiter one.</text>
              </description>
            </leaf>
          </list>
        </container>
        <choice name="string-or-delimiter">
          <description>
            <text>Configures the format of the nas-port-id.</text>
          </description>
          <case name="lns-string">
            <description>
              <text>Specifies a user-defined character string.</text>
            </description>
            <leaf name="lns-string">
              <type name="string">
                <length value="1..31"/>
              </type>
              <must condition="count(../include-attributes/include-attribute) &gt;= 1"/>
              <description>
                <text>Specifies a user-defined character string.</text>
              </description>
            </leaf>
          </case>
          <case name="ip-delimiter">
            <description>
              <text>Specifies a lns ip delimiter.</text>
            </description>
            <leaf name="lns-ip-delimiter">
              <type name="string">
                <length value="1"/>
              </type>
              <must condition="count(../include-attributes/include-attribute) &gt;= 1"/>
              <description>
                <text>Specifies a lns ip delimiter.</text>
              </description>
            </leaf>
          </case>
        </choice>
      </container>
    </container>
  </augment>
