<?xml version="1.0" encoding="UTF-8"?>
<module name="huawei-ifm-fr"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:ifm-fr="urn:huawei:yang:huawei-ifm-fr"
        xmlns:ifm="urn:huawei:yang:huawei-ifm"
        xmlns:ext="urn:huawei:yang:huawei-extension">
  <yang-version value="1"/>
  <namespace uri="urn:huawei:yang:huawei-ifm-fr"/>
  <prefix value="ifm-fr"/>
  <import module="huawei-ifm">
    <prefix value="ifm"/>
  </import>
  <import module="huawei-extension">
    <prefix value="ext"/>
  </import>
  <organization>
    <text>Huawei Technologies Co., Ltd.</text>
  </organization>
  <contact>
    <text>Huawei Industrial Base
Bantian, Longgang
Shenzhen 518129
People's Republic of China
Website: http://www.huawei.com
Email: support@huawei.com</text>
  </contact>
  <description>
    <text>Frame relay protocol.</text>
  </description>
  <revision date="2019-12-25">
    <description>
      <text>Initial revision.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <ext:task-name value="fr"/>
  <typedef name="encapsulation-format">
    <type name="enumeration">
      <enum name="ietf">
        <value value="19"/>
        <description>
          <text>IETF.</text>
        </description>
      </enum>
      <enum name="nonstandard">
        <value value="20"/>
        <description>
          <text>Nonstandard.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>FR interface encapsulation format.</text>
    </description>
  </typedef>
  <typedef name="lmi-type">
    <type name="enumeration">
      <enum name="ansi">
        <value value="0"/>
        <description>
          <text>LMI ANSI.</text>
        </description>
      </enum>
      <enum name="q933a">
        <value value="1"/>
        <description>
          <text>LMI Q933A.</text>
        </description>
      </enum>
      <enum name="nonstandard">
        <value value="2"/>
        <description>
          <text>LMI nonstandard.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>FR interface LMI type.</text>
    </description>
  </typedef>
  <typedef name="fr-type">
    <type name="enumeration">
      <enum name="dte">
        <value value="1"/>
        <description>
          <text>FR DTE.</text>
        </description>
      </enum>
      <enum name="dce">
        <value value="2"/>
        <description>
          <text>FR DCE.</text>
        </description>
      </enum>
      <enum name="nni">
        <value value="3"/>
        <description>
          <text>FR NNI.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>FR interface type.</text>
    </description>
  </typedef>
  <augment target-node="/ifm:ifm/ifm:interfaces/ifm:interface">
    <description>
      <text>Configure extend attributes on the interface.</text>
    </description>
    <container name="fr-encapsulation">
      <when condition="../ifm:class='main-interface' and ../ifm:type='Pos' and ../ifm:link-protocol='fr'"/>
      <description>
        <text>Configure frame relay interface encapsulation format.</text>
      </description>
      <leaf name="encapsulation">
        <type name="encapsulation-format"/>
        <default value="ietf"/>
        <description>
          <text>FR interface encapsulation format.</text>
        </description>
      </leaf>
    </container>
  </augment>
</module>
