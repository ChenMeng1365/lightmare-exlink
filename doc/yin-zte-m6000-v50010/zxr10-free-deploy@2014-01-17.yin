<?xml version="1.0" encoding="UTF-8"?>
<module name="zxr10-free-deploy"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:free-deploy="http://www.zte.com.cn/zxr10/netconf/schema/rosng/free-deploy"
        xmlns:ncd="http://www.zte.com.cn/zxr10/netconf/schema/rosng/netconf-defs">
  <namespace uri="http://www.zte.com.cn/zxr10/netconf/schema/rosng/free-deploy"/>
  <prefix value="free-deploy"/>
  <import module="zxr10-netconf-defs">
    <prefix value="ncd"/>
  </import>
  <organization>
    <text>ZTE Corporation</text>
  </organization>
  <contact>
    <text>ZTE Corporation

   Nanjing Institute of ZTE Corporation

   No.68 Zijinghua Rd. Yuhuatai District, Nanjing, China

   Tel: +86-25-52870000</text>
  </contact>
  <description>
    <text>The netconf model to describe free deploy.</text>
  </description>
  <revision date="2014-01-17">
    <description>
      <text>Increase  the interface filter number</text>
    </description>
  </revision>
  <revision date="2013-08-26">
    <description>
      <text>Initial version.</text>
    </description>
  </revision>
  <container name="configuration">
    <description>
      <text>Configuration model info.</text>
    </description>
    <container name="free-deploy">
      <description>
        <text>Free-deploy module</text>
      </description>
      <container name="interface-free-deploys">
        <description>
          <text>Interface free-deploys</text>
        </description>
        <list name="interface-free-deploy">
          <description>
            <text>Configure of interface free-deploy, for two layer switching properties ports (61 projects), support for Ethernet physical interface and the SG interface enabled; three layer routing properties ports (61 and 65 projects), support for ordinary Ethernet physical interface to enable</text>
          </description>
          <key value="interface-name"/>
          <leaf name="interface-name">
            <description>
              <text>Interface name</text>
            </description>
            <type name="ncd:ifname">
              <length value="1..31"/>
            </type>
          </leaf>
          <leaf name="free-deploy-status">
            <description>
              <text>Interface free-deploy enable or disable</text>
            </description>
            <type name="enumeration">
              <enum name="disable">
                <value value="0"/>
                <description>
                  <text>interface free-deploy switch turn off</text>
                </description>
              </enum>
              <enum name="enable">
                <value value="1"/>
                <description>
                  <text>interface free-deploy switch turn on</text>
                </description>
              </enum>
            </type>
            <mandatory value="true"/>
          </leaf>
        </list>
      </container>
    </container>
  </container>
</module>
