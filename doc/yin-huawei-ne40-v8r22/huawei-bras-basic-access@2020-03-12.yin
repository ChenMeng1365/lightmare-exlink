    </container>
    <container name="frame-ip-conflict-recycle">
      <description>
        <text>Configure the frame ip conflict recycle.</text>
      </description>
      <leaf name="enable">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable the framed-ip conflict auto-recycle.</text>
        </description>
      </leaf>
    </container>
    <container name="access-delay-all">
      <must condition="minimum &lt;= maximum"/>
      <presence value="Enables the access response delay function globally."/>
      <description>
        <text>Configure the access response delay function globally, and set the maximum and minimum access response delays on the specified board.</text>
      </description>
      <leaf name="step">
        <type name="uint32">
          <range value="1..262144"/>
        </type>
        <mandatory value="true"/>
        <description>
          <text>Indicates the step of the number of access users. Each time the number of access users on the router increases by the number specified by step value, the response delay is prolonged. The response delay for the access users within the step range is the same.</text>
        </description>
      </leaf>
      <leaf name="minimum">
        <type name="uint16">
          <range value="0..2550"/>
        </type>
        <mandatory value="true"/>
        <description>
          <text>Indicates the minimum response delay for access users on the minimum-time specifies the minimum response delay.</text>
        </description>
      </leaf>
      <leaf name="maximum">
        <type name="uint16">
          <range value="1..2550"/>
        </type>
        <mandatory value="true"/>
        <description>
          <text>Indicates the maximum response delay for access users on the maximum-time specifies the maximum response delay.</text>
        </description>
      </leaf>
    </container>
    <container name="access-delay-slots">
      <description>
        <text>List of the access response delay function globally, and set the maximum and minimum access response delays on the specified board.</text>
      </description>
      <list name="access-delay-slot">
        <must condition="minimum &lt;= maximum"/>
        <key value="slot"/>
        <description>
          <text>Configure the access response delay function globally, and set the maximum and minimum access response delays on the specified board.</text>
        </description>
        <leaf name="slot">
          <type name="leafref">
            <path value="/devm:devm/devm:lpu-boards/devm:lpu-board/devm:position"/>
          </type>
          <description>
            <text>Indicates the access response delay function for access users on the specified board.</text>
          </description>
        </leaf>
        <leaf name="step">
          <type name="uint32">
            <range value="1..262144"/>
          </type>
          <mandatory value="true"/>
          <description>
            <text>Indicates the step of the number of access users. Each time the number of access users on the router increases by the number specified by step value, the response delay is prolonged. The response delay for the access users within the step range is the same.</text>
          </description>
        </leaf>
        <leaf name="minimum">
          <type name="uint16">
            <range value="0..2550"/>
          </type>
          <mandatory value="true"/>
          <description>
            <text>Indicates the minimum response delay for access users on the minimum-time specifies the minimum response delay.</text>
          </description>
        </leaf>
        <leaf name="maximum">
          <type name="uint16">
            <range value="1..2550"/>
          </type>
          <mandatory value="true"/>
          <description>
            <text>Indicates the maximum response delay for access users on the maximum-time specifies the maximum response delay.</text>
          </description>
        </leaf>
      </list>
    </container>
    <container name="pppuser-offline-standardize">
      <description>
        <text>Configure a PPP user to preferentially get online when the PPP user and a DHCP user with the same MAC address attempt to get online.</text>
      </description>
      <leaf name="enable">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable a PPP user to preferentially get online.</text>
        </description>
      </leaf>
    </container>
    <container name="nas-port-id-format">
      <description>
        <text>Configure nas port id format.</text>
      </description>
      <choice name="format">
        <default value="version20"/>
        <description>
          <text>Configure format.</text>
        </description>
        <case name="standard">
          <description>
            <text>Configure standard format.</text>
          </description>
          <leaf name="standard">
            <type name="enumeration">
              <enum name="standard">
                <value value="0"/>
                <description>
                  <text>Indicates the standard format conforming to the VBAS protocol.</text>
                </description>
              </enum>
              <enum name="standard-ignore">
                <value value="1"/>
                <description>
                  <text>Indicates that the RID field in the Nas-Port-Id attribute carried by Option 82 is removed when indicates the standard format conforming to the VBAS protocol.</text>
                </description>
              </enum>
              <enum name="trust-outer-vlan">
                <value value="2"/>
                <description>
                  <text>Indicates the standard format conforming to the VBAS protocol when trusts the outer VLAN tag value.</text>
                </description>
              </enum>
              <enum name="trust-outer-vlan-ignore">
                <value value="3"/>
                <description>
                  <text>Indicates that the RID field in the Nas-Port-Id attribute carried by Option 82 is removed when trusts the outer VLAN tag value.</text>
                </description>
              </enum>
              <enum name="trust-inner-vlan">
                <value value="4"/>
                <description>
                  <text>Indicates the standard format conforming to the VBAS protocol when trusts the inner VLAN tag value.</text>
                </description>
              </enum>
              <enum name="trust-inner-vlan-ignore">
                <value value="5"/>
                <description>
                  <text>Indicates that the RID field in the Nas-Port-Id attribute carried by Option 82 is removed when trusts the inner VLAN tag value.</text>
                </description>
              </enum>
            </type>
            <description>
              <text>Configure the type of standard format.</text>
            </description>
          </leaf>
        </case>
        <case name="turkey">
          <description>
            <text>Configure turkey format.</text>
          </description>
          <leaf name="turkey">
            <type name="empty"/>
            <description>
              <text>Indicates the format of Turkey version.</text>
            </description>
          </leaf>
        </case>
        <case name="version10">
          <description>
            <text>Configure version10 format.</text>
          </description>
          <leaf name="version10">
            <type name="empty"/>
            <description>
              <text>Indicates the format of version 1.0.</text>
            </description>
          </leaf>
        </case>
        <case name="version20">
          <description>
            <text>Configure version20 format.</text>
          </description>
          <leaf name="version20">
            <type name="empty"/>
            <description>
              <text>Indicates the format of version 2.0, by default, the format of version 2.0 is adopted.</text>
            </description>
          </leaf>
        </case>
      </choice>
    </container>
    <container name="packet-strict-check">
      <description>
        <text>Configure the router to strictly check user packets of a specific type.</text>
      </description>
      <leaf name="nd">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable the strict ND packets check function.</text>
        </description>
      </leaf>
      <leaf name="dhcpv6">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable the strict DHCPv6 packets check function.</text>
        </description>
      </leaf>
      <leaf name="dhcp">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable the strict DHCP packets check function.</text>
        </description>
      </leaf>
      <leaf name="ppp">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable the strict PPP packets check function.</text>
        </description>
      </leaf>
      <leaf name="l2tp">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable the strict L2TP packets check function.</text>
        </description>
      </leaf>
      <leaf name="dot1x">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable the strict dot1x packets check function.</text>
        </description>
      </leaf>
    </container>
    <container name="protocol-statistics">
      <description>
        <text>Configure statistics about user protocol packets.</text>
      </description>
      <leaf name="enable">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable flag.</text>
        </description>
      </leaf>
    </container>
    <container name="ui-mode">
      <description>
        <text>Configure the command output in the user-defined format.</text>
      </description>
      <leaf name="type">
        <type name="enumeration">
          <enum name="type1">
            <value value="1"/>
            <description>
              <text>User-defined Type 1 mode.</text>
            </description>
          </enum>
          <enum name="type2">
            <value value="2"/>
            <description>
              <text>User-defined Type 2 mode.</text>
            </description>
          </enum>
        </type>
        <description>
          <text>Enable the user-defined format.</text>
        </description>
      </leaf>
    </container>
    <container name="user-flow-statistics">
      <description>
        <text>Configure user flow statistics.</text>
      </description>
      <leaf name="enable">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable user flow statistics.</text>
        </description>
      </leaf>
    </container>
    <container name="load-balance-enhance-algorithm">
      <description>
        <text>Configure the enhanced load balancing algorithm.</text>
      </description>
      <leaf name="enable">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable the enhanced load balancing algorithm.</text>
        </description>
      </leaf>
      <leaf name="update-frequency">
        <type name="uint32">
          <range value="1..1000"/>
        </type>
        <default value="1"/>
        <description>
          <text>Specifies the frequency for triggering load balancing. It is the change of the number of users on an interface.</text>
        </description>
      </leaf>
    </container>
    <container name="load-balance-groups">
      <description>
        <text>List of load balancing groups.</text>
      </description>
      <list name="load-balance-group">
        <key value="name"/>
        <max-elements value="1024"/>
        <description>
          <text>Configure a load balanceing group.</text>
        </description>
        <leaf name="name">
          <type name="string">
            <length value="1..31"/>
            <pattern value="[!-~]{1,31}"/>
            <pattern value="[^A-Z']{1,31}"/>
          </type>
          <description>
            <text>Specifies a name for a load balancing group.</text>
          </description>
          <ext:case-sensitivity value="lower-only"/>
        </leaf>
        <leaf name="delay-time">
          <type name="uint32">
            <range value="1..255"/>
          </type>
          <units name="10 ms"/>
          <description>
            <text>Specifies a delay time for a load balancing group.</text>
          </description>
        </leaf>
        <leaf name="member-count">
          <type name="uint32">
            <range value="0..16"/>
          </type>
          <config value="false"/>
          <description>
            <text>Number of member interfaces in a load balancing group.</text>
          </description>
        </leaf>
        <leaf name="active-count">
          <type name="uint32">
            <range value="0..16"/>
          </type>
          <config value="false"/>
          <description>
            <text>Number of active member interfaces in a load balancing group.</text>
          </description>
        </leaf>
        <container name="member-infos">
          <config value="false"/>
          <description>
            <text>List of interface information of load balancing group.</text>
          </description>
          <list name="member-info">
            <key value="interface-name"/>
            <description>
              <text>Statistics of interface name of load balancing group member.</text>
            </description>
            <leaf name="interface-name">
              <type name="string">
                <length value="1..63"/>
              </type>
              <description>
                <text>Name of a member interface.</text>
              </description>
            </leaf>
            <leaf name="number">
              <type name="uint32"/>
              <description>
                <text>Number of a member interface.</text>
              </description>
            </leaf>
            <leaf name="delay-time">
              <type name="uint32"/>
              <units name="10 ms"/>
              <description>
                <text>Delay time after which an interface responds to user login requests.</text>
              </description>
            </leaf>
            <leaf name="up-down-times">
              <type name="uint32"/>
              <description>
                <text>Number of times that a member interface alternates between up and down.</text>
              </description>
            </leaf>
            <leaf name="is-chasten">
              <type name="boolean"/>
              <description>
                <text>Penalty flag. A punished interface is not used for load balancing. The default penalty time is 5 minutes. After 5 minutes, the interface is available for load balancing again.</text>
              </description>
            </leaf>
            <leaf name="first-up-down-time">
              <type name="string">
                <length value="1..31"/>
              </type>
              <description>
                <text>Time when an interface goes up or down for the first time if the penalty flag is not set, or penalty time if the penalty flag is set.</text>
              </description>
            </leaf>
          </list>
        </container>
      </list>
    </container>
  </container>
  <augment target-node="/ifm:ifm/ifm:interfaces/ifm:interface">
    <description>
      <text>Bas interface configurations.</text>
    </description>
    <container name="bas">
      <when condition="../ifm:type='Ethernet' or&#10;../ifm:type='GigabitEthernet' or&#10;../ifm:type='Eth-Trunk' or&#10;../ifm:type='100GE' or&#10;../ifm:type='10GE' or&#10;../ifm:type='Virtual-Ethernet' or&#10;../ifm:type='25GE' or&#10;../ifm:type='XGigabitEthernet' or&#10;../ifm:type='FlexE' or&#10;../ifm:type='PW-VE' or&#10;../ifm:type='50GE' or&#10;../ifm:type='50|100GE' or&#10;../ifm:type='400GE'"/>
      <must condition="not(../ethernet:ethernet/ethernet:main-interface/ethernet:l2-attribute)"/>
      <must condition="not((../ifm:name)=(/ifm:ifm/ifm:interfaces/ifm:interface/ifm-trunk:trunk/ifm-trunk:members/ifm-trunk:member/ifm-trunk:name))"/>
      <must condition="not(../ifm:l2-mode-enable='true')"/>
      <presence value="Create bas."/>
      <description>
        <text>Configure bas interface, BRAS access can be configured on a maximum of 500 trunk interfaces.</text>
      </description>
      <choice name="access-type">
        <description>
          <text>Configure the access type of a bas interface.</text>
        </description>
        <case name="layer2-subscriber">
          <description>
            <text>Configure layer2 subscriber access type.</text>
          </description>
          <container name="layer2-subscriber">
            <must condition="not(./dhcp-lease-proxy and ./wlan-switch/enable='true')"/>
            <presence value="layer2-subscriber."/>
            <description>
              <text>Configure layer2 subscriber access type.</text>
            </description>
            <container name="subscriber-base">
              <description>
                <text>Configure layer2 subscriber base.</text>
              </description>
              <uses name="subscriber-base"/>
              <uses name="bas-interface-name"/>
              <uses name="accounting-radius-server"/>
            </container>
            <uses name="nas-port-type"/>
            <container name="authentication-method">
              <must condition="bind-authentication or (dot1x-authentication or ppp-authentication or (fast-authentication or web-authentication))"/>
              <description>
                <text>Configure layer2 subscriber authentication method.</text>
              </description>
              <choice name="authentication-method">
                <default value="unbind"/>
                <description>
                  <text>Layer2 subscriber authentication method choice.</text>
                </description>
                <case name="bind">
                  <description>
                    <text>Layer2 subscriber bind authentication method.</text>
                  </description>
                  <leaf name="bind-authentication">
                    <type name="empty"/>
                    <description>
                      <text>Indicates binding authentication.</text>
                    </description>
                  </leaf>
                </case>
                <case name="unbind">
                  <description>
                    <text>Layer2 subscriber unbind authentication method.</text>
                  </description>
                  <leaf name="dot1x-authentication">
                    <type name="empty"/>
                    <description>
                      <text>Indicates 802.1x authentication.</text>
                    </description>
                  </leaf>
                  <leaf name="ppp-authentication">
                    <type name="empty"/>
                    <description>
                      <text>Indicates PPP authentication (default authentication method).</text>
                    </description>
                  </leaf>
                  <choice name="fast-web">
                    <description>
                      <text>Layer2 subscriber fast or web authentication method.</text>
                    </description>
                    <case name="fast">
                      <description>
                        <text>Layer2 subscriber fast authentication method.</text>
                      </description>
                      <leaf name="fast-authentication">
                        <type name="empty"/>
                        <description>
                          <text>Indicates fast authentication.</text>
                        </description>
                      </leaf>
                    </case>
                    <case name="web">
                      <description>
                        <text>Layer2 subscriber web authentication method.</text>
                      </description>
                      <leaf name="web-authentication">
                        <type name="empty"/>
                        <description>
                          <text>Indicates web authentication.</text>
                        </description>
                      </leaf>
                    </case>
                  </choice>
                </case>
              </choice>
            </container>
            <uses name="access-line-id-insert"/>
            <uses name="access-delay"/>
            <uses name="client-vendor-class"/>
            <uses name="client-option18"/>
            <uses name="client-option37"/>
            <uses name="access-limit"/>
            <uses name="access-limit-for-vlan">
              <when condition="../../ifm:class='sub-interface'"/>
            </uses>
            <uses name="access-limit-for-qinq">
              <when condition="../../ifm:class='sub-interface'"/>
            </uses>
            <uses name="access-line-id-identify-type"/>
            <uses name="access-line-id-transmit-mode"/>
            <uses name="access-line-id-parse-value"/>
            <uses name="igmp-report-loose-check"/>
            <uses name="user-policy-interface-down"/>
            <uses name="arp-proxy"/>
            <uses name="user-detect"/>
            <uses name="link-account-resolve"/>
            <uses name="wlan-switch"/>
            <uses name="bas-nas-logic"/>
            <uses name="dhcp-proxy"/>
            <container name="dhcp-lease-proxy">
              <when condition="../dhcp-proxy/enable='true'"/>
              <description>
                <text>Configure the proxy lease.</text>
              </description>
              <uses name="dhcp-lease-proxy-config"/>
            </container>
            <uses name="trigger"/>
            <uses name="user-flow-vpn-statistics"/>
          </container>
        </case>
        <case name="layer2-leaseline">
          <description>
            <text>Configure layer2 leaseline access type.</text>
          </description>
          <container name="layer2-leaseline">
            <presence value="layer2-leaseline."/>
            <description>
              <text>Configure layer2 leaseline access type.</text>
            </description>
            <leaf name="user-name">
              <type name="string">
                <length value="1..253"/>
              </type>
              <mandatory value="true"/>
              <description>
                <text>Lease line user name.</text>
              </description>
            </leaf>
            <uses name="leaseline-access"/>
            <uses name="nas-port-type"/>
            <uses name="trust-access-line-id-insert"/>
            <uses name="access-limit"/>
            <uses name="access-limit-for-vlan">
              <when condition="../../ifm:class='sub-interface'"/>
            </uses>
            <uses name="access-limit-for-qinq">
              <when condition="../../ifm:class='sub-interface'"/>
            </uses>
            <uses name="access-line-id-identify-type"/>
            <uses name="access-line-id-transmit-mode"/>
            <uses name="access-line-id-parse-value"/>
            <uses name="igmp-report-loose-check"/>
            <uses name="arp-proxy"/>
            <uses name="user-detect"/>
            <uses name="dhcp-proxy"/>
            <container name="dhcp-lease-proxy">
              <when condition="../dhcp-proxy/enable='true'"/>
              <description>
                <text>Configure the proxy lease.</text>
              </description>
              <uses name="dhcp-lease-proxy-config"/>
            </container>
            <uses name="trigger"/>
            <uses name="user-flow-vpn-statistics"/>
          </container>
        </case>
        <case name="layer3-subscriber">
          <description>
            <text>Configure layer3 subscriber access type.</text>
          </description>
          <container name="layer3-subscriber">
            <presence value="layer3-subscriber."/>
            <description>
              <text>Configure layer3 subscriber access type.</text>
            </description>
            <container name="subscriber-base">
              <description>
                <text>Configure layer3 subscriber base.</text>
              </description>
              <uses name="subscriber-base"/>
            </container>
            <uses name="nas-port-type"/>
            <uses name="access-line-id-insert"/>
            <uses name="access-line-id-identify-type"/>
            <uses name="access-line-id-transmit-mode"/>
            <uses name="access-line-id-parse-value"/>
          </container>
        </case>
        <case name="layer3-leaseline">
          <description>
            <text>Configure layer3 leaseline access type.</text>
          </description>
          <container name="layer3-leaseline">
            <must condition="./name or ./template"/>
            <presence value="layer3-leaseline."/>
            <description>
              <text>Configure layer3 leaseline access type.</text>
            </description>
            <choice name="user-name">
              <description>
                <text>User name choice.</text>
              </description>
              <case name="by-name">
                <description>
                  <text>User name.</text>
                </description>
                <leaf name="name">
                  <type name="string">
                    <length value="1..253"/>
                  </type>
                  <description>
                    <text>Lease line user name.</text>
                  </description>
                </leaf>
              </case>
              <case name="by-template">
                <description>
                  <text>User template.</text>
                </description>
                <leaf name="template">
                  <type name="empty"/>
                  <description>
                    <text>User name template.</text>
                  </description>
                </leaf>
              </case>
            </choice>
            <uses name="leaseline-access"/>
            <container name="trust-access-line-id-insert">
              <description>
                <text>Configure the access-line-id information, including the following commands:client-option82.</text>
              </description>
              <leaf name="insert-type">
                <type name="bras-basic-access-access-line-id-type"/>
                <must condition="../insert-type='option82'"/>
                <description>
                  <text>Inserts information about the BAS interface in the telecom format.</text>
                </description>
              </leaf>
              <leaf name="insert-version">
                <when condition="../insert-type"/>
                <type name="bras-basic-access-trust-access-line-id-insert-version"/>
                <description>
                  <text>Specifies encapsulating the access-line-id information into a DHCP packet in version2 format.</text>
                </description>
              </leaf>
            </container>
          </container>
        </case>
        <case name="l2vpn-leaseline">
          <description>
            <text>Configure layer2 vpn leaseline access type.</text>
          </description>
          <container name="l2vpn-leaseline">
            <presence value="l2vpn-leaseline."/>
            <description>
              <text>Configure layer2 vpn leaseline access type.</text>
            </description>
            <leaf name="user-name">
              <type name="string">
                <length value="1..253"/>
              </type>
              <mandatory value="true"/>
              <description>
                <text>Lease line user name.</text>
              </description>
            </leaf>
            <uses name="leaseline-access"/>
            <uses name="nas-port-type"/>
            <uses name="access-line-id-identify-type"/>
            <uses name="access-line-id-transmit-mode"/>
            <uses name="access-line-id-parse-value"/>
          </container>
        </case>
      </choice>
      <container name="eap-authentication-domain-individual">
        <description>
          <text>Configure the EAP-authentication-based RADIUS proxy authentication domain name does not depend on the DHCP user name.</text>
        </description>
        <leaf name="enable">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable the EAP-authentication-based RADIUS proxy authentication domain name does not depend on the DHCP user name.</text>
          </description>
        </leaf>
      </container>
      <container name="bas-interface">
        <config value="false"/>
        <description>
          <text>Bas interface statistics.</text>
        </description>
        <container name="information">
          <config value="false"/>
          <description>
            <text>Statistics of a specified BAS interface.</text>
          </description>
          <leaf name="is-static-user-exist">
            <type name="boolean"/>
            <default value="false"/>
            <config value="false"/>
            <description>
              <text>Whether the static user is exist.</text>
            </description>
          </leaf>
          <leaf name="is-selected">
            <type name="boolean"/>
            <default value="false"/>
            <config value="false"/>
            <description>
              <text>Is selected interface.</text>
            </description>
          </leaf>
          <leaf name="increased-user-num">
            <type name="int32">
              <range value="-1000..1000"/>
            </type>
            <config value="false"/>
            <description>
              <text>Increase user number since last update prior interface.</text>
            </description>
          </leaf>
        </container>
        <container name="access-users">
          <config value="false"/>
          <description>
            <text>List of the number of access users on a specified BAS interface.</text>
          </description>
          <list name="access-user">
            <key value="pe-vlan ce-vlan"/>
            <config value="false"/>
            <description>
              <text>Statistics of the number of access users on a specified BAS interface.</text>
            </description>
            <leaf name="pe-vlan">
              <type name="uint16">
                <range value="0..4094"/>
              </type>
              <config value="false"/>
              <description>
                <text>Outer VLAN ID of an interface.</text>
              </description>
            </leaf>
            <leaf name="ce-vlan">
              <type name="uint16">
                <range value="0..4094"/>
              </type>
              <config value="false"/>
              <description>
                <text>Inner VLAN ID of an interface.</text>
              </description>
            </leaf>
            <leaf name="user-number">
              <type name="uint32"/>
              <config value="false"/>
              <description>
                <text>Number of access users on a BAS interface.</text>
              </description>
            </leaf>
            <leaf name="ipoe-user-number">
              <type name="uint32">
                <range value="0..4294967295"/>
              </type>
              <config value="false"/>
              <description>
                <text>Number of IPoE users on an interface.</text>
              </description>
            </leaf>
            <leaf name="pppoe-user-number">
              <type name="uint32">
                <range value="0..4294967295"/>
              </type>
              <config value="false"/>
              <description>
                <text>Number of PPPoE users on an interface.</text>
              </description>
            </leaf>
          </list>
        </container>
        <container name="statistics">
          <config value="false"/>
          <description>
            <text>Statistics of a specified BAS interface.</text>
          </description>
          <leaf name="last-statistics-time">
            <type name="string">
              <length value="1..32"/>
            </type>
            <config value="false"/>
            <description>
              <text>Time of a specified interface statistics collected and displayed.</text>
            </description>
          </leaf>
          <leaf name="input-rate">
            <type name="uint32"/>
            <units name="Mbit/s"/>
            <config value="false"/>
            <description>
              <text>Interface uplink traffic rate, Unit Mbit/s.</text>
            </description>
          </leaf>
          <leaf name="output-rate">
            <type name="uint32"/>
            <units name="Mbit/s"/>
            <config value="false"/>
            <description>
              <text>Interface downlink traffic rate, Unit Mbit/s.</text>
            </description>
          </leaf>
        </container>
      </container>
    </container>
    <container name="bras">
      <description>
        <text>Configure the bras interface.</text>
      </description>
      <container name="load-balance-mode">
        <when condition="../../ifm:type='Eth-Trunk'"/>
        <description>
          <text>Configure Eth-Trunk interfaces in load-balancing mode, and configures load balancing based on traffic volume, the actual traffic volume, and the number of access users.</text>
        </description>
        <leaf name="mode">
          <type name="enumeration">
            <enum name="user-mode">
              <value value="0"/>
              <description>
                <text>Indicates load balancing based on the number of access users.</text>
              </description>
            </enum>
            <enum name="flow-mode">
              <value value="1"/>
              <description>
                <text>Indicates that load balancing is performed based on the allocated bandwidth.</text>
              </description>
            </enum>
            <enum name="real-flow-mode">
              <value value="2"/>
              <description>
                <text>Indicates load balancing based on the actual traffic volume.</text>
              </description>
            </enum>
          </type>
          <default value="user-mode"/>
          <description>
            <text>Configures Eth-Trunk interfaces in load-balancing mode.</text>
          </description>
        </leaf>
      </container>
    </container>
  </augment>
  <augment target-node="/aaa:aaa/aaa:domains/aaa:domain">
    <description>
      <text>Bas domain configuration.</text>
    </description>
    <container name="bas">
      <description>
        <text>Configure the bas domain.</text>
      </description>
      <container name="ipv4-basic-access">
        <description>
          <text>Configure the IPv4 basic access domain.</text>
        </description>
        <container name="max-user-reauth-time">
          <description>
            <text>Configure the maximum reauthentication time of a domain.</text>
          </description>
          <leaf name="time">
            <type name="uint16"/>
            <units name="s"/>
            <default value="300"/>
            <description>
              <text>Sets the maximum reauthentication time of a domain.</text>
            </description>
          </leaf>
        </container>
        <container name="ip-pools">
          <description>
            <text>List of ip pools bound to the domain.</text>
          </description>
          <list name="ip-pool">
            <key value="name"/>
            <max-elements value="1024"/>
            <description>
              <text>Configure the list of IP address pools bound to the domain.</text>
            </description>
            <leaf name="name">
              <type name="leafref">
                <path value="/bras-ipv4-address-manage:bras-ipv4-address-manage/bras-ipv4-address-manage:ipv4-pools/bras-ipv4-address-manage:ipv4-pool/bras-ipv4-address-manage:name"/>
              </type>
              <must condition="(/bras-ipv4-address-manage:bras-ipv4-address-manage/bras-ipv4-address-manage:ipv4-pools/bras-ipv4-address-manage:ipv4-pool[bras-ipv4-address-manage:name=current()]/bras-ipv4-address-manage:type = 'dynamic' or&#10;(/bras-ipv4-address-manage:bras-ipv4-address-manage/bras-ipv4-address-manage:ipv4-pools/bras-ipv4-address-manage:ipv4-pool[bras-ipv4-address-manage:name=current()]/bras-ipv4-address-manage:local/bras-ipv4-address-manage:gateway/bras-ipv4-address-manage:gate-ip or&#10;/bras-ipv4-address-manage:bras-ipv4-address-manage/bras-ipv4-address-manage:ipv4-pools/bras-ipv4-address-manage:ipv4-pool[bras-ipv4-address-manage:name=current()]/bras-ipv4-address-manage:remote/bras-ipv4-address-manage:gateway/bras-ipv4-address-manage:gate-ip))"/>
              <description>
                <text>Specifies the name of an IP address pool.</text>
              </description>
            </leaf>
          </list>
        </container>
        <container name="accounting-copy-radius">
          <description>
            <text>Configure RADIUS server group to send accounting packets of a domain.</text>
          </description>
          <leaf name="name">
            <type name="leafref">
              <path value="/radius:radius/radius:radius-server-groups/radius:radius-server-group/radius:name"/>
            </type>
            <description>
              <text>Enables the system to send accounting packets of a domain to a configured RADIUS server group.</text>
            </description>
          </leaf>
          <leaf name="realtime-accounting-copy">
            <type name="boolean"/>
            <default value="true"/>
            <description>
              <text>Enable/disable users' real-time accounting packets not copied to the RADIUS server.</text>
            </description>
          </leaf>
        </container>
        <container name="dns-server">
          <description>
            <text>Configure the IP address of the DNS server.</text>
          </description>
          <leaf name="primary-ip">
            <type name="inet:ipv4-address-no-zone"/>
            <description>
              <text>Specifies the IP address of the primary DNS server.</text>
            </description>
          </leaf>
          <leaf name="secondary-ip">
            <type name="inet:ipv4-address-no-zone"/>
            <description>
              <text>Specifies the IP address of the secondary DNS server.</text>
            </description>
          </leaf>
        </container>
        <container name="nbns-server">
          <description>
            <text>Configure the IP address of the nbns server.</text>
          </description>
          <leaf name="primary-ip">
            <type name="inet:ipv4-address-no-zone"/>
            <description>
              <text>Specifies the IP address of the primary DNS server.</text>
            </description>
          </leaf>
          <leaf name="secondary-ip">
            <type name="inet:ipv4-address-no-zone"/>
            <description>
              <text>Specifies the IP address of the secondary DNS server.</text>
            </description>
          </leaf>
        </container>
        <container name="user-group">
          <description>
            <text>Configure the name of the user group bound to the domain.</text>
          </description>
          <leaf name="name">
            <type name="leafref">
              <path value="/bras-basic-access:bras-basic-access/bras-basic-access:user-groups/bras-basic-access:user-group/bras-basic-access:name"/>
            </type>
            <description>
              <text>Specifies the name of the user group bound to the domain.</text>
            </description>
          </leaf>
        </container>
        <container name="user-group-list">
          <description>
            <text>Configure the name of the user group list bound to the domain.</text>
          </description>
          <leaf name="name">
            <type name="leafref">
              <path value="/bras-basic-access:bras-basic-access/bras-basic-access:user-group-lists/bras-basic-access:user-group-list/bras-basic-access:name"/>
            </type>
            <description>
              <text>Specifies the name of the user group list bound to the domain.</text>
            </description>
          </leaf>
        </container>
        <container name="policy-route">
          <description>
            <text>Configure the next-hop of the policy-based route for all users in a domain.</text>
          </description>
          <leaf name="ipv4-address">
            <type name="inet:ipv4-address-no-zone"/>
            <description>
              <text>Specifies the next-hop IP address of the policy-based route for all users in a domain.</text>
            </description>
          </leaf>
          <leaf name="ipv6-address">
            <type name="inet:ipv6-address-no-zone"/>
            <description>
              <text>Specifies the next-hop IPv6 address of the policy-based route for all users in a domain.</text>
            </description>
          </leaf>
        </container>
        <container name="user-max-session">
          <description>
            <text>Configure the maximum number of users.</text>
          </description>
          <leaf name="number">
            <type name="uint32">
              <range value="0..3145728"/>
            </type>
            <description>
              <text>Specifies the maximum number of users that can use the same user account.</text>
            </description>
          </leaf>
          <leaf name="local-user-name-case-insensitive">
            <when condition="../number"/>
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable the local user account is case-insensitive.</text>
            </description>
          </leaf>
        </container>
        <container name="ip-unr-route-tag">
          <description>
            <text>Configure a route tag for host routes of IPv4 users and network segment routes generated based on the Framed-Route attribute delivered by the RADIUS server.</text>
          </description>
          <leaf name="value">
            <type name="uint32"/>
            <description>
              <text>Specify a route tag.</text>
            </description>
          </leaf>
        </container>
        <container name="user-detect">
          <must condition="(retransmit-time and interval) or (not(retransmit-time) and not(interval))"/>
          <description>
            <text>Configure a user group in the domain.</text>
          </description>
          <leaf name="retransmit-time">
            <type name="uint32">
              <range value="2..120"/>
            </type>
            <description>
              <text>Indicates the number of times for detecting users by sending ARP packets.</text>
            </description>
          </leaf>
          <leaf name="interval">
            <type name="uint32">
              <range value="0..1800"/>
            </type>
            <units name="s"/>
            <description>
              <text>Indicates the interval for detecting users by sending ARP packets.</text>
            </description>
          </leaf>
        </container>
        <container name="user-basic-service-ip-type">
          <description>
            <text>Configure the user basic service type.</text>
          </description>
          <leaf name="ipv4">
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable the user basic service type is IPv4.</text>
            </description>
          </leaf>
          <leaf name="ipv6">
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable the user basic service type is IPv6.</text>
            </description>
          </leaf>
          <leaf name="ipv6-pd">
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable the user basic service type is IPv6 PD.</text>
            </description>
          </leaf>
        </container>
        <container name="cui">
          <description>
            <text>Configure users in a domain to apply to the RADIUS server for the CUI attribute when requesting to get online, and then adds the CUI attribute to the subsequent user accounting packets.</text>
          </description>
          <leaf name="enable">
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable users in a domain to apply to the RADIUS server for the CUI attribute when requesting to get online, and then adds the CUI attribute to the subsequent user accounting packets.</text>
            </description>
          </leaf>
        </container>
        <container name="ip-pool-mode">
          <description>
            <text>Configure ip pool mode.</text>
          </description>
          <leaf name="layer2-user-priority">
            <type name="ip-pool-priority-layer2"/>
            <default value="local"/>
            <description>
              <text>The layer 2 users priority in which an IP address pool assigns IP addresses.</text>
            </description>
          </leaf>
          <leaf name="layer3-user-priority">
            <type name="ip-pool-priority-layer3"/>
            <default value="based-on-gateway-address"/>
            <description>
              <text>The layer 3 users priority in which an IP address pool assigns IP addresses.</text>
            </description>
          </leaf>
          <leaf name="user-type">
            <when condition="../layer2-user-priority='remote'"/>
            <type name="enumeration">
              <enum name="ipoe">
                <value value="1"/>
                <description>
                  <text>Ipoe.</text>
                </description>
              </enum>
              <enum name="pppoe">
                <value value="2"/>
                <description>
                  <text>Pppoe.</text>
                </description>
              </enum>
            </type>
            <description>
              <text>Specifies the type of a user.</text>
            </description>
          </leaf>
        </container>
        <container name="accounting-exclude-vlan">
          <description>
            <text>Configure the router to exclude VLAN header length from packet length when collecting statistics about user packet bytes. This command applies to both IPv4 and IPv6 packet statistics.</text>
          </description>
          <leaf name="ipoe">
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable IPoE user.</text>
            </description>
          </leaf>
          <leaf name="pppoe">
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable PPPoE user.</text>
            </description>
          </leaf>
        </container>
        <container name="annex">
          <must condition="(direction and string) or (not(direction) and not(string))"/>
          <description>
            <text>Configure string information to the left/right side of a domain name when the BRAS sends authentication and accounting request packets to the RADIUS server.</text>
          </description>
          <leaf name="direction">
            <type name="enumeration">
              <enum name="left">
                <value value="1"/>
                <description>
                  <text>Adds string information to the left side.</text>
                </description>
              </enum>
              <enum name="right">
                <value value="2"/>
                <description>
                  <text>Adds string information to the right side.</text>
                </description>
              </enum>
            </type>
            <description>
              <text>Adds string information to the left or right side.</text>
            </description>
          </leaf>
          <leaf name="string">
            <when condition="../direction"/>
            <type name="string">
              <length value="1..31"/>
            </type>
            <description>
              <text>Specifies the string added to the DHCP option 125 attribute.</text>
            </description>
          </leaf>
        </container>
        <container name="mac-authentication">
          <description>
            <text>Configure MAC address authentication.</text>
          </description>
          <leaf name="enable">
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable MAC address authentication.</text>
            </description>
          </leaf>
        </container>
        <container name="ip-pool-groups">
          <description>
            <text>List of the address pool group in a domain.</text>
          </description>
          <list name="ip-pool-group">
            <key value="name"/>
            <max-elements value="8"/>
            <description>
              <text>Configure an address pool group.</text>
            </description>
            <leaf name="name">
              <type name="leafref">
                <path value="/bras-ipv4-address-manage:bras-ipv4-address-manage/bras-ipv4-address-manage:pool-groups/bras-ipv4-address-manage:pool-group/bras-ipv4-address-manage:name"/>
              </type>
              <description>
                <text>Specifies the name of an IP address pool.</text>
              </description>
            </leaf>
          </list>
        </container>
        <container name="framed-ip-urpf">
          <description>
            <text>Configure URPF when IP addresses are delivered by the RADIUS server.</text>
          </description>
          <leaf name="enable">
            <type name="boolean"/>
            <default value="true"/>
            <description>
              <text>Enable/disable URPF when IP addresses are delivered by the radius server.</text>
            </description>
          </leaf>
        </container>
        <container name="map-priority">
          <description>
            <text>Configure a device to preferentially assign MAP-T prefixes to users.</text>
          </description>
          <leaf name="map-t-priority">
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable MAP priority in a domain.</text>
            </description>
          </leaf>
        </container>
        <container name="idle-user-control-policy">
          <description>
            <text>Configure idle user control policy.</text>
          </description>
          <leaf name="cut-time">
            <type name="uint32">
              <range value="0..2880"/>
            </type>
            <units name="min"/>
            <default value="0"/>
            <description>
              <text>Specifies the period after which the user connection is torn down.</text>
            </description>
          </leaf>
          <choice name="cut-rate">
            <default value="rate"/>
            <description>
              <text>Config cut rate.</text>
            </description>
            <case name="rate">
              <description>
                <text>Cut rate.</text>
              </description>
              <leaf name="rate">
                <type name="uint32">
                  <range value="0..76800000"/>
                </type>
                <units name="1/60 kB/s"/>
                <default value="60"/>
                <description>
                  <text>Specifies the amount of user traffic rate. If the user traffic rate is less than the amount, the user is considered idle.</text>
                </description>
              </leaf>
            </case>
            <case name="zero-rate">
              <description>
                <text>Zero cut rate.</text>
              </description>
              <leaf name="zero-rate">
                <type name="empty"/>
                <description>
                  <text>Indicates that if the time when the user traffic rate is less than 1Kbyte/min reaches the idle-time value, the system will disconnect the idle user.</text>
                </description>
              </leaf>
            </case>
          </choice>
          <leaf name="cut-flow-direction">
            <type name="enumeration">
              <enum name="inbound">
                <value value="1"/>
                <description>
                  <text>Indicates that the parameter idle takes effect only on the upstream traffic.</text>
                </description>
              </enum>
              <enum name="outbound">
                <value value="2"/>
                <description>
                  <text>Indicates that the parameter idle takes effect only on the downstream traffic.</text>
                </description>
              </enum>
            </type>
            <description>
              <text>Indicates that the parameter idle takes effect on the upstream or downstream traffic.</text>
            </description>
          </leaf>
        </container>
        <container name="quota-out">
          <description>
            <text>Configure quota out.</text>
          </description>
          <choice name="policy">
            <default value="offline"/>
            <description>
              <text>Quota out policy.</text>
            </description>
            <case name="offline">
              <description>
                <text>Quota out offline policy.</text>
              </description>
              <leaf name="offline">
                <type name="empty"/>
                <description>
                  <text>Forces the users to go offline.</text>
                </description>
              </leaf>
            </case>
            <case name="online">
              <description>
                <text>Quota out online policy.</text>
              </description>
              <leaf name="online">
                <type name="empty"/>
                <description>
                  <text>Allows the users to remain online.</text>
                </description>
              </leaf>
            </case>
            <case name="redirect">
              <description>
                <text>Quota out redirect policy.</text>
              </description>
              <leaf name="url">
                <type name="string">
                  <length value="1..200"/>
                </type>
                <description>
                  <text>Specifies the URL for forcible redirection.</text>
                </description>
              </leaf>
              <leaf name="stop-accounting">
                <when condition="../url"/>
                <type name="empty"/>
                <description>
                  <text>Indicates that a user sends an accounting-stop packet after the user is redirected because the quota of the user is used up.</text>
                </description>
              </leaf>
            </case>
            <case name="realtime-accounting">
              <description>
                <text>Quota out realtime accounting policy.</text>
              </description>
              <leaf name="enable">
                <type name="empty"/>
                <description>
                  <text>Indicates that a user sends real-time accounting packets after the quota of the user is used up.</text>
                </description>
              </leaf>
            </case>
          </choice>
        </container>
        <container name="any-address-release">
          <description>
            <text>Configure a device to forcibly log out a dual-stack user if the user releases any IP address.</text>
          </description>
          <leaf name="offline">
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable a device to forcibly log out a dual-stack user if the user releases any IP address.</text>
            </description>
          </leaf>
        </container>
        <container name="public-ip-address-pool-usage-threshold">
          <must condition="(lower and upper) or (not(lower) and not(upper) and both-cgn = 'false')"/>
          <description>
            <text>Configure threshold for public IP address pool usage in an AAA domain.</text>
          </description>
          <leaf name="lower">
            <type name="uint32">
              <range value="0..99"/>
            </type>
            <units name="%"/>
            <description>
              <text>Lower threshold for public IP address pool usage in an AAA domain.</text>
            </description>
          </leaf>
          <leaf name="upper">
            <when condition="../lower"/>
            <type name="uint32">
              <range value="1..100"/>
            </type>
            <units name="%"/>
            <must condition="../upper &gt; ../lower"/>
            <description>
              <text>Upper threshold for public IP address pool usage in an AAA domain.</text>
            </description>
          </leaf>
          <leaf name="both-cgn">
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable threshold in an AAA domain. The public IP address pool status is determined by the address pool usage in both CGN and non-CGN scenarios. By default, the public IP address pool status is safe in non-CGN scenarios.</text>
            </description>
          </leaf>
        </container>
        <container name="user-volume-quato">
          <description>
            <text>Configure a user traffic volume quota to upstream or downstream traffic.</text>
          </description>
          <leaf name="direction">
            <type name="enumeration">
              <enum name="both">
                <value value="0"/>
                <description>
                  <text>Both inbound and outbound direction.</text>
                </description>
              </enum>
              <enum name="inbound">
                <value value="1"/>
                <description>
                  <text>Inbound direction.</text>
                </description>
              </enum>
              <enum name="outbound">
                <value value="2"/>
                <description>
                  <text>Outbound direction.</text>
                </description>
              </enum>
            </type>
            <default value="both"/>
            <description>
              <text>User flow quota applying direction. By default, the user traffic volume quota applies to both upstream and downstream traffic.</text>
            </description>
          </leaf>
        </container>
        <container name="accounting-start-delay">
          <description>
            <text>Configure a device to send an accounting-start packet about a dual-stack user after a specified delay.</text>
          </description>
          <leaf name="ipv4-immediately">
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable accounting immediately when the IPv4 stack is online.</text>
            </description>
          </leaf>
          <container name="delay-policy">
            <presence value="Delay policy."/>
            <description>
              <text>Configure a device to send an accounting-start packet about a dual-stack user after a specified delay for user.</text>
            </description>
            <leaf name="time">
              <type name="uint32">
                <range value="1..300"/>
              </type>
              <units name="s"/>
              <mandatory value="true"/>
              <description>
                <text>Specifies a delay in sending accounting-start packets.</text>
              </description>
            </leaf>
            <leaf name="method">
              <type name="enumeration">
                <enum name="online">
                  <value value="1"/>
                  <description>
                    <text>The user stays online and the device sends accounting-start packets after the specified delay.</text>
                  </description>
                </enum>
                <enum name="offline">
                  <value value="2"/>
                  <description>
                    <text>The device forcibly logs out the user after the specified delay.</text>
                  </description>
                </enum>
              </type>
              <mandatory value="true"/>
              <description>
                <text>User delay process method.</text>
              </description>
            </leaf>
            <choice name="user-type">
              <mandatory value="true"/>
              <description>
                <text>User type for enable a device to send an accounting-start packet about a dual-stack user after a specified delay.</text>
              </description>
              <case name="all">
                <description>
                  <text>Enable a device to send an accounting-start packet about a dual-stack user after a specified delay all user type.</text>
                </description>
                <leaf name="all">
                  <when condition="../method='offline'"/>
                  <type name="empty"/>
                  <description>
                    <text>Delay process for All user type.</text>
                  </description>
                </leaf>
              </case>
              <case name="spec">
                <description>
                  <text>Enable a device to send an accounting-start packet about a dual-stack user after a specified delay special user type.</text>
                </description>
                <leaf name="ppp">
                  <type name="empty"/>
                  <description>
                    <text>Delay process for PPP access user.</text>
                  </description>
                </leaf>
                <leaf name="ipoe">
                  <type name="empty"/>
                  <description>
                    <text>Delay process for IPoE access user.</text>
                  </description>
                </leaf>
                <leaf name="l2tp">
                  <type name="empty"/>
                  <description>
                    <text>Delay process for L2TP access user.</text>
                  </description>
                </leaf>
                <leaf name="static">
                  <type name="empty"/>
                  <description>
                    <text>Delay process for Static access user.</text>
                  </description>
                </leaf>
              </case>
            </choice>
          </container>
          <container name="traffic">
            <description>
              <text>Configure a device to send an accounting-start packet about a dual-stack user after a specified delay for traffic.</text>
            </description>
            <leaf name="forward-before-start-accounting">
              <type name="boolean"/>
              <default value="false"/>
              <description>
                <text>Enable/disable users to access the network before starting accounting message.</text>
              </description>
            </leaf>
            <leaf name="statistics-before-start-accounting">
              <type name="boolean"/>
              <default value="false"/>
              <description>
                <text>Enable/disable the traffic of ueser accessing to network before starting accounting message.</text>
              </description>
            </leaf>
          </container>
        </container>
        <container name="radius-no-response">
          <description>
            <text>Configure the lease time of the users of an unresponsive RADIUS server.</text>
          </description>
          <leaf name="lease-time">
            <type name="uint32">
              <range value="60..65535"/>
            </type>
            <units name="s"/>
            <description>
              <text>Specifies the lease time of the users of an unresponsive RADIUS server.</text>
            </description>
          </leaf>
        </container>
        <container name="ip-usage-threshold">
          <must condition="not(upper) or not(lower) or upper &gt;= lower"/>
          <description>
            <text>Configure alarm threshold for the IP address usage.</text>
          </description>
          <leaf name="lower">
            <type name="uint32">
              <range value="0..99"/>
            </type>
            <units name="%"/>
            <description>
              <text>Specifies a lower alarm threshold for the IP address usage.</text>
            </description>
          </leaf>
          <leaf name="upper">
            <type name="uint32">
              <range value="1..100"/>
            </type>
            <units name="%"/>
            <description>
              <text>Specifies an upper alarm threshold for the IP address usage.</text>
            </description>
          </leaf>
        </container>
        <container name="nas-logic">
          <description>
            <text>Configure the logical.</text>
          </description>
          <leaf name="system-name">
            <type name="string">
              <length value="1..30"/>
            </type>
            <description>
              <text>Specifies the logical host name.</text>
            </description>
          </leaf>
        </container>
        <container name="llid-first-authentication-radius-server-group">
          <description>
            <text>Configure RADIUS server group name of LLID first authentication.</text>
          </description>
          <leaf name="name">
            <type name="leafref">
              <path value="/radius:radius/radius:radius-server-groups/radius:radius-server-group/radius:name"/>
            </type>
            <must condition="../../../../aaa:radius-server-template"/>
            <description>
              <text>RADIUS server group name.</text>
            </description>
          </leaf>
        </container>
        <container name="non-accouting-time-range">
          <description>
            <text>Configure the name of a time range for non-accounting.</text>
          </description>
          <leaf name="name">
            <type name="leafref">
              <path value="/timerange:time-range/timerange:time-range-instances/timerange:time-range-instance/timerange:name"/>
            </type>
            <description>
              <text>Specifies the name of a time range.</text>
            </description>
          </leaf>
        </container>
        <container name="pre-auth-radius-server-groups">
          <description>
            <text>List of RADIUS server group to use for pre-authentication in a domain.</text>
          </description>
          <list name="pre-auth-radius-server-group">
            <key value="radius-server-group-name"/>
            <unique tag="priority"/>
            <max-elements value="3"/>
            <description>
              <text>Configure a RADIUS server group to use for pre-authentication in a domain.</text>
            </description>
            <leaf name="radius-server-group-name">
              <type name="leafref">
                <path value="/radius:radius/radius:radius-server-groups/radius:radius-server-group/radius:name"/>
              </type>
              <description>
                <text>Specifies a RADIUS server group to use for pre-authentication.</text>
              </description>
            </leaf>
            <leaf name="priority">
              <type name="uint8">
                <range value="0..2"/>
              </type>
              <default value="0"/>
              <description>
                <text>Specifies the priority of a RADIUS server group to use for pre-authentication.</text>
              </description>
            </leaf>
          </list>
        </container>
      </container>
    </container>
  </augment>
  <augment target-node="/devm:devm/devm:mpu-boards/devm:mpu-board">
    <description>
      <text>Augment bras slot configurations.</text>
    </description>
    <container name="bras">
      <description>
        <text>Configure the bras slot.</text>
      </description>
      <uses name="flow-statistic-frequency"/>
    </container>
  </augment>
  <augment target-node="/devm:devm/devm:lpu-boards/devm:lpu-board">
    <description>
      <text>Augment bras slot configurations.</text>
    </description>
    <container name="bras">
      <description>
        <text>Configure the bras slot.</text>
      </description>
      <uses name="flow-statistic-frequency"/>
    </container>
  </augment>
</module>
