  <container name="configuration">
    <description>
      <text>The netconf module to describe the configuration of route protocol management model.</text>
    </description>
    <container name="courier">
      <description>
        <text>This model includes the description of ip max-routes configuration.</text>
      </description>
      <container name="ip-maxroutes">
        <description>
          <text>The netconf module to describe the function of set maximum routes and warning percent of global route table, this function can not limit the routes number, but only send users an alarm when the routes number reaches the threshold value.</text>
        </description>
        <presence value="ip max-routes"/>
        <leaf name="max-routes">
          <description>
            <text>The max-routes node means the maximum of the global route table.This node and warning-percent node must be set together.</text>
          </description>
          <type name="uint32">
            <range value="1..4294967295"/>
          </type>
          <mandatory value="true"/>
        </leaf>
        <leaf name="warning-percent">
          <description>
            <text>Warning threshold value. When the routes of the global route table exceeded the threshold value, an alarm will be send to users. This node and max-routes node must be set together.</text>
          </description>
          <type name="uint32">
            <range value="1..100"/>
          </type>
          <mandatory value="true"/>
        </leaf>
      </container>
      <container name="ipv6-maxroutes">
        <description>
          <text>Set maximum routes and warning percent of IPv6 global route table. This function can not limit the routes number, but only send users an alarm when the routes number reaches the threshold value.</text>
        </description>
        <presence value="ipv6 max-routes"/>
        <leaf name="max-routes">
          <description>
            <text>The max-routes node means the maximum of the IPv6 global route table. This node and warning-percent node must be set together.</text>
          </description>
          <type name="uint32">
            <range value="1..4294967295"/>
          </type>
          <mandatory value="true"/>
        </leaf>
        <leaf name="warning-percent">
          <description>
            <text>Warning threshold value. When the routes of the IPv6 global route table exceeded the threshold value, an alarm will be send to users. This node and max-routes node must be set together.</text>
          </description>
          <type name="uint32">
            <range value="1..100"/>
          </type>
          <mandatory value="true"/>
        </leaf>
        <ncd:revise-date date="2018-09-18"/>
      </container>
      <container name="entropy-label">
        <description>
          <text>Set the entropy label.</text>
        </description>
        <leaf name="ipv4">
          <description>
            <text>Set the ipv4 entropy label.</text>
          </description>
          <type name="enumeration">
            <enum name="enable">
              <value value="1"/>
            </enum>
            <enum name="disable">
              <value value="0"/>
            </enum>
          </type>
        </leaf>
        <leaf name="ipv6">
          <description>
            <text>Set the ipv6 entropy label.</text>
          </description>
          <type name="enumeration">
            <enum name="enable">
              <value value="1"/>
            </enum>
            <enum name="disable">
              <value value="0"/>
            </enum>
          </type>
        </leaf>
        <ncd:revise-date date="2018-09-19"/>
      </container>
      <container name="ipv4-maxribs">
        <description>
          <text>The netconf module to describe the function of set maximum ribs and warning percent of total ribs table, this function can not limit the ribs number, but only send users an alarm when the ribs number reaches the threshold value.</text>
        </description>
        <presence value="ip max-ribs"/>
        <leaf name="warning-percent">
          <description>
            <text>Warning threshold value. When the count of the total IPv4 rib table exceeded the threshold value, an alarm will be send to users.</text>
          </description>
          <type name="uint8">
            <range value="1..100"/>
          </type>
          <mandatory value="true"/>
        </leaf>
        <ncd:revise-date date="2019-06-19"/>
      </container>
      <container name="ipv6-maxribs">
        <description>
          <text>The netconf module to describe the function of set maximum ribs and warning percent of total ribs table, this function can not limit the ribs number, but only send users an alarm when the ribs number reaches the threshold value.</text>
        </description>
        <presence value="ipv6 max-routes"/>
        <leaf name="warning-percent">
          <description>
            <text>Warning threshold value. When the count of the total IPv6 rib table exceeded the threshold value, an alarm will be send to users.</text>
          </description>
          <type name="uint8">
            <range value="1..100"/>
          </type>
          <mandatory value="true"/>
        </leaf>
        <ncd:revise-date date="2019-06-19"/>
      </container>
      <container name="direct-route-tracks">
        <description>
          <text>The SAMGR track configurations of direct route.</text>
        </description>
        <list name="direct-route-track">
          <description>
            <text>The SAMGR track configurations of direct route.</text>
          </description>
          <key value="interface af-type"/>
          <leaf name="interface">
            <description>
              <text>The interface name of direct route.</text>
            </description>
            <type name="ncd:ifname"/>
          </leaf>
          <leaf name="af-type">
            <description>
              <text>Address family of direct route.</text>
            </description>
            <type name="enumeration">
              <enum name="ipv4">
                <value value="1"/>
              </enum>
              <enum name="ipv6">
                <value value="2"/>
              </enum>
            </type>
          </leaf>
          <leaf name="track-name">
            <description>
              <text>Name of a track object.</text>
            </description>
            <type name="string">
              <length value="1..31"/>
            </type>
            <mandatory value="true"/>
          </leaf>
          <leaf name="metric">
            <description>
              <text>Cost for direct route.</text>
            </description>
            <type name="uint32">
              <range value="1..4294967295"/>
            </type>
            <mandatory value="true"/>
          </leaf>
        </list>
        <ncd:revise-date date="2019-08-08"/>
      </container>
    </container>
    <container name="ipv4-frr">
      <description>
        <text>IPv4 fast reroute.</text>
      </description>
      <leaf name="status">
        <description>
          <text>disable:0
enable:1</text>
        </description>
        <type name="enumeration">
          <enum name="disable">
            <value value="0"/>
          </enum>
          <enum name="enable">
            <value value="1"/>
          </enum>
        </type>
      </leaf>
      <leaf name="wtr">
        <description>
          <text>WTR time. The range is 1-12(min).</text>
        </description>
        <type name="uint8">
          <range value="1..12"/>
        </type>
      </leaf>
      <ncd:revise-date date="2020-04-21"/>
    </container>
    <container name="ipv6-frr">
      <description>
        <text>IPv6 fast reroute.</text>
      </description>
      <leaf name="status">
        <description>
          <text>disable:0
enable:1</text>
        </description>
        <type name="enumeration">
          <enum name="disable">
            <value value="0"/>
          </enum>
          <enum name="enable">
            <value value="1"/>
          </enum>
        </type>
      </leaf>
      <leaf name="wtr">
        <description>
          <text>WTR time. The range is 1-12(min).</text>
        </description>
        <type name="uint8">
          <range value="1..12"/>
