<?xml version="1.0" encoding="UTF-8"?>
<module name="huawei-routing"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:rt="urn:huawei:yang:huawei-routing"
        xmlns:ni="urn:huawei:yang:huawei-network-instance"
        xmlns:l3vpn="urn:huawei:yang:huawei-l3vpn"
        xmlns:inet="urn:ietf:params:xml:ns:yang:ietf-inet-types"
        xmlns:ext="urn:huawei:yang:huawei-extension"
        xmlns:ifm="urn:huawei:yang:huawei-ifm"
        xmlns:bfd="urn:huawei:yang:huawei-bfd"
        xmlns:pub-type="urn:huawei:yang:huawei-pub-type"
        xmlns:rtp="urn:huawei:yang:huawei-routing-policy"
        xmlns:xpl="urn:huawei:yang:huawei-xpl"
        xmlns:tnlm="urn:huawei:yang:huawei-tunnel-management">
  <yang-version value="1"/>
  <namespace uri="urn:huawei:yang:huawei-routing"/>
  <prefix value="rt"/>
  <import module="huawei-network-instance">
    <prefix value="ni"/>
  </import>
  <import module="huawei-l3vpn">
    <prefix value="l3vpn"/>
  </import>
  <import module="ietf-inet-types">
    <prefix value="inet"/>
  </import>
  <import module="huawei-extension">
    <prefix value="ext"/>
  </import>
  <import module="huawei-ifm">
    <prefix value="ifm"/>
  </import>
  <import module="huawei-bfd">
    <prefix value="bfd"/>
  </import>
  <import module="huawei-pub-type">
    <prefix value="pub-type"/>
  </import>
  <import module="huawei-routing-policy">
    <prefix value="rtp"/>
  </import>
  <import module="huawei-xpl">
    <prefix value="xpl"/>
  </import>
  <import module="huawei-tunnel-management">
    <prefix value="tnlm"/>
  </import>
  <include module="huawei-routing-type"/>
  <organization>
    <text>Huawei Technologies Co., Ltd.</text>
  </organization>
  <contact>
    <text>Huawei Industrial Base
Bantian, Longgang
Shenzhen 518129
People's Republic of China
Website: http://www.huawei.com
Email: support@huawei.com</text>
  </contact>
  <description>
    <text>This YANG module defines essential components for the management
of a routing subsystem.</text>
  </description>
  <revision date="2021-08-11">
    <description>
      <text>Add grouping import-rib-policy-type routing-table etc.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <revision date="2019-04-27">
    <description>
      <text>Initial revision.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <ext:task-name value="route-base"/>
  <typedef name="af-type">
    <type name="enumeration">
      <enum name="ipv4-unicast">
        <value value="1"/>
        <description>
          <text>IPv4 unicast.</text>
        </description>
      </enum>
      <enum name="ipv6-unicast">
        <value value="2"/>
        <description>
          <text>IPv6 unicast.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>The type of address family.</text>
    </description>
  </typedef>
  <grouping name="import-rib-policy-type">
    <description>
      <text>Import rib policy configuration.</text>
    </description>
    <choice name="policy-type">
      <description>
        <text>When routes are imported from other routing protocols, the Route-Policy filter specified by the parameter can be used to filter the routes and change the route attributes.</text>
      </description>
      <case name="rtp-ref">
        <description>
          <text>Route policy.</text>
        </description>
        <leaf name="policy-name">
          <type name="leafref">
            <path value="/rtp:routing-policy/rtp:policy-definitions/rtp:policy-definition/rtp:name"/>
          </type>
          <description>
            <text>Specify an import rib route policy name.</text>
          </description>
        </leaf>
      </case>
      <case name="xpl-ref">
        <description>
          <text>Route policy.</text>
        </description>
        <leaf name="filter-name">
          <type name="leafref">
            <path value="/xpl:xpl/xpl:route-filters/xpl:route-filter/xpl:name"/>
          </type>
          <description>
            <text>Specify an import rib route filter name.</text>
          </description>
        </leaf>
        <leaf name="filter-parameter">
          <when condition="../filter-name"/>
          <type name="xpl:filter-parameter-type"/>
          <description>
            <text>Specify an import rib route filter parameter.</text>
          </description>
        </leaf>
      </case>
    </choice>
  </grouping>
  <grouping name="routing-table-non-key">
    <description>
      <text>Operational data of the routing table.</text>
    </description>
    <leaf name="nexthop">
      <type name="inet:ip-address-no-zone"/>
      <description>
        <text>Next hop.</text>
      </description>
    </leaf>
    <leaf name="frr-type">
      <type name="rt:frr-type"/>
      <description>
        <text>FRR type.</text>
      </description>
    </leaf>
    <leaf name="preference">
      <type name="uint32">
        <range value="0..255"/>
      </type>
      <description>
        <text>Priority of a route. During route selection, the route with the highest priority is selected.</text>
      </description>
    </leaf>
    <leaf name="cost">
      <type name="uint32"/>
      <description>
        <text>Cost.</text>
      </description>
    </leaf>
    <leaf name="flag">
      <type name="rt:route-flag"/>
      <description>
        <text>Flag.</text>
      </description>
    </leaf>
    <leaf name="qos-id">
      <type name="pub-type:hex-binary"/>
      <description>
        <text>QoS information.</text>
      </description>
    </leaf>
    <leaf name="active">
      <type name="boolean"/>
      <default value="false"/>
      <description>
        <text>Enable/disable the flag indicating whether routes are active.</text>
      </description>
    </leaf>
    <leaf name="tag">
      <type name="uint32"/>
      <description>
        <text>Route tag.</text>
      </description>
    </leaf>
    <leaf name="priority">
      <type name="rt:route-priority"/>
      <description>
        <text>Route convergence priority, which can be low, medium, high, or critical.</text>
      </description>
    </leaf>
    <leaf name="label">
      <type name="uint32"/>
      <description>
        <text>Label that is used for traffic forwarding along a tunnel.</text>
      </description>
    </leaf>
    <leaf name="indirect-id">
      <type name="pub-type:hex-binary"/>
      <description>
        <text>IID index of a route.</text>
      </description>
    </leaf>
    <leaf name="state">
      <type name="rt:route-state"/>
      <description>
        <text>Route states. The following route states are supported:
Active: Indicates that a route is activated.
Invalid: Indicates that a route is invalid.
Inactive: Indicates that a route is inactive.
No advertise: Indicates that route advertisement is prohibited.
Advertise: Indicates that a route can be advertised.
Relied: Indicates that a route can be iterated to the next hop or the outbound interface, or iterated to a tunnel.
Stale: Indicates that routes with this tag are used in GR.</text>
      </description>
    </leaf>
    <leaf name="neighbour">
      <type name="inet:ip-address-no-zone"/>
      <description>
        <text>Neighbor.</text>
      </description>
    </leaf>
    <leaf name="age">
      <type name="string">
        <length value="0..20"/>
      </type>
      <description>
        <text>Keepalive time. The value is a string in the format of 'xdxhxmxs', for example '1d18h10m16s'.</text>
      </description>
    </leaf>
    <leaf name="relay-nexthop">
      <type name="inet:ip-address-no-zone"/>
      <description>
        <text>Iteration next hop.</text>
      </description>
    </leaf>
  </grouping>
  <grouping name="routing-statistics-num">
    <description>
      <text>Route statistics.</text>
    </description>
    <leaf name="total-num">
      <type name="uint32"/>
      <description>
        <text>Number of routes in the routing table.</text>
      </description>
    </leaf>
    <leaf name="active-num">
      <type name="uint32"/>
      <description>
        <text>Number of active routes in the routing table.</text>
      </description>
    </leaf>
    <leaf name="added-num">
      <type name="uint32"/>
      <description>
        <text>Number of active and inactive routes added to the routing table.</text>
      </description>
    </leaf>
    <leaf name="deleted-num">
      <type name="uint32"/>
      <description>
        <text>Number of routes deleted from the routing table.</text>
      </description>
    </leaf>
    <leaf name="freed-num">
      <type name="uint32"/>
      <description>
        <text>Number of routes that are permanently released from the routing table.</text>
      </description>
    </leaf>
  </grouping>
  <container name="routing">
    <description>
      <text>Configuration parameters for the routing module.</text>
    </description>
    <container name="routing-manage">
      <description>
        <text>Configure the routing management module to store and select optimal routes.</text>
      </description>
      <container name="relay-tunnel">
        <description>
          <text>Configure route recursion to tunnel.</text>
        </description>
        <leaf name="enable">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable route recursion to tunnel. By default, this configuration takes effect for all routes. If an IP prefix list is configured, the configuration takes effect only for the routes that match the IP prefix list. If a tunnel policy is configured, routes carry the tunnel policy information when recursing to a tunnel.</text>
          </description>
        </leaf>
        <leaf name="ipv4-prefix-ref">
          <when condition="../enable='true'"/>
          <type name="leafref">
            <path value="/rtp:routing-policy/rtp:ipv4-prefix-filters/rtp:ipv4-prefix-filter/rtp:name"/>
          </type>
          <must condition="../enable='true'"/>
          <description>
            <text>Name of an IPv4 prefix list.</text>
          </description>
        </leaf>
        <leaf name="tunnel-policy-ref">
          <when condition="../enable='true'"/>
          <type name="leafref">
            <path value="/tnlm:tunnel-management/tnlm:tunnel-policys/tnlm:tunnel-policy/tnlm:name"/>
          </type>
          <must condition="../enable='true'"/>
          <description>
            <text>Name of a tunnel policy.</text>
          </description>
        </leaf>
      </container>
      <container name="relay-label">
        <description>
          <text>Configure route recursion to remotely leaked VPN routes globally.</text>
        </description>
        <leaf name="ipv4-set">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable the function of recursing IPv4 unicast routes to remotely leaked VPN routes.</text>
          </description>
        </leaf>
        <leaf name="ipv6-set">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable the function of recursing IPv6 unicast routes to remotely leaked VPN routes.</text>
          </description>
        </leaf>
      </container>
      <container name="ipv4-threshold">
        <must condition="upper-percent&gt;lower-percent"/>
        <description>
          <text>Configure thresholds (one alarm threshold and one clear alarm threshold) for the number of IPv4 route prefixes.</text>
        </description>
        <leaf name="upper-percent">
          <type name="uint8">
            <range value="2..100"/>
          </type>
          <units name="%"/>
          <default value="80"/>
          <description>
            <text>Upper limit for the number of IPv4 route prefixes.</text>
          </description>
        </leaf>
        <leaf name="lower-percent">
          <type name="uint8">
            <range value="1..99"/>
          </type>
          <units name="%"/>
          <default value="70"/>
          <description>
            <text>Lower limit for the number of IPv4 route prefixes.</text>
          </description>
        </leaf>
      </container>
      <container name="ipv6-threshold">
        <must condition="upper-percent&gt;lower-percent"/>
        <description>
          <text>Configure thresholds (one alarm threshold and one clear alarm threshold) for the number of IPv6 route prefixes.</text>
        </description>
        <leaf name="upper-percent">
          <type name="uint8">
            <range value="2..100"/>
          </type>
          <units name="%"/>
          <default value="80"/>
          <description>
            <text>Set the upper limit percent of the IPv6 prefix threshold.</text>
          </description>
        </leaf>
        <leaf name="lower-percent">
          <type name="uint8">
            <range value="1..99"/>
          </type>
          <units name="%"/>
          <default value="70"/>
          <description>
            <text>Set the lower limit percent of the IPv6 prefix threshold.</text>
          </description>
        </leaf>
      </container>
      <container name="iterative-restrain">
        <description>
          <text>Configure suppression periods for recursion.</text>
        </description>
        <leaf name="enable">
          <type name="boolean"/>
          <default value="true"/>
          <description>
            <text>Enable/disable suppression on recursion.</text>
          </description>
        </leaf>
        <leaf name="start-time">
          <type name="uint32">
            <range value="500..2000"/>
          </type>
          <units name="ms"/>
          <default value="500"/>
          <description>
            <text>Specifies the time of the first route or tunnel iteration suppression.</text>
          </description>
        </leaf>
        <leaf name="increase-time">
          <type name="uint32">
            <range value="1000..5000"/>
          </type>
          <units name="ms"/>
          <default value="1000"/>
          <description>
            <text>Specifies the incremental suppression duration of route or tunnel iteration.</text>
          </description>
        </leaf>
        <leaf name="max-time">
          <type name="uint32">
            <range value="5000..60000"/>
          </type>
          <units name="ms"/>
          <default value="30000"/>
          <description>
            <text>Specifies the maximum suppression duration of route or tunnel iteration.</text>
          </description>
        </leaf>
      </container>
      <container name="relay-default-routes">
        <description>
          <text>List of routes that recurse to the default route globally.</text>
        </description>
        <list name="relay-default-route">
          <key value="address-family protocol-type"/>
          <description>
            <text>Configure route recursion to the default route globally.</text>
          </description>
          <leaf name="address-family">
            <type name="af-type"/>
            <must condition="../address-family='ipv4-unicast' or (not(../protocol-type='msr') and ../address-family='ipv6-unicast')"/>
            <description>
              <text>Set an address family.</text>
            </description>
          </leaf>
          <leaf name="protocol-type">
            <type name="enumeration">
              <enum name="msr">
                <value value="2"/>
                <description>
                  <text>MSR protocol.</text>
                </description>
              </enum>
              <enum name="static">
                <value value="3"/>
                <description>
                  <text>Static protocol.</text>
                </description>
              </enum>
            </type>
            <must condition="../protocol-type='static' or (../protocol-type='msr' and ../address-family='ipv4-unicast')"/>
            <description>
              <text>Protocol.</text>
            </description>
          </leaf>
        </list>
      </container>
      <container name="ipv4-vpn-route-statisticss">
        <config value="false"/>
        <description>
          <text>List of statistics about routes of all VPN instances in the IPv4 routing table.</text>
        </description>
        <list name="ipv4-vpn-route-statistics">
          <key value="protocol-type"/>
          <description>
            <text>All IPv4 VPN Instance route statistics.</text>
          </description>
          <leaf name="protocol-type">
            <type name="statistic-protocol-type"/>
            <description>
              <text>Protocol.</text>
            </description>
          </leaf>
          <leaf name="total-num">
            <type name="uint32"/>
            <description>
              <text>Number of routes in the routing table.</text>
            </description>
          </leaf>
          <leaf name="active-num">
            <type name="uint32"/>
            <description>
              <text>Number of active routes in the routing table.</text>
            </description>
          </leaf>
          <leaf name="added-num">
            <type name="uint32"/>
            <description>
              <text>Number of active and inactive routes added to the routing table.</text>
            </description>
          </leaf>
          <leaf name="deleted-num">
            <type name="uint32"/>
            <description>
              <text>Number of routes deleted from the routing table.</text>
            </description>
          </leaf>
          <leaf name="freed-num">
            <type name="uint32"/>
            <description>
              <text>Number of routes that are permanently released from the routing table.</text>
            </description>
          </leaf>
        </list>
      </container>
      <container name="ipv6-vpn-route-statisticss">
        <config value="false"/>
        <description>
          <text>List of statistics about routes of all VPN instances in the IPv6 routing table.</text>
        </description>
        <list name="ipv6-vpn-route-statistics">
          <key value="protocol-type"/>
          <description>
            <text>All IPv6 VPN Instance route statistics.</text>
          </description>
          <leaf name="protocol-type">
            <type name="ipv6-statistic-protocol-type"/>
            <description>
              <text>Protocol.</text>
            </description>
          </leaf>
          <leaf name="total-num">
            <type name="uint32"/>
            <description>
              <text>Number of routes in the routing table.</text>
            </description>
          </leaf>
          <leaf name="active-num">
            <type name="uint32"/>
            <description>
              <text>Number of active routes in the routing table.</text>
            </description>
          </leaf>
          <leaf name="added-num">
            <type name="uint32"/>
            <description>
              <text>Number of active and inactive routes added to the routing table.</text>
            </description>
          </leaf>
          <leaf name="deleted-num">
            <type name="uint32"/>
            <description>
              <text>Number of routes deleted from the routing table.</text>
            </description>
          </leaf>
          <leaf name="freed-num">
            <type name="uint32"/>
            <description>
              <text>Number of routes that are permanently released from the routing table.</text>
            </description>
          </leaf>
        </list>
      </container>
      <container name="unicast-route-statistics">
        <config value="false"/>
        <description>
          <text>Statistics of unicast route of all VPN instances.</text>
        </description>
        <leaf name="ipv4-prefix-number">
          <type name="uint32"/>
          <description>
            <text>The number of IPv4 route prefixes for all VPN instances.</text>
          </description>
        </leaf>
        <leaf name="ipv6-prefix-number">
          <type name="uint32"/>
          <description>
            <text>The number of IPv6 route prefixes for all VPN instances.</text>
          </description>
        </leaf>
      </container>
    </container>
    <container name="static-routing">
      <description>
        <text>Configure a basic service package for static routes.</text>
      </description>
      <container name="ipv4-site">
        <description>
          <text>Configure parameters for IPv4 static routes.</text>
        </description>
        <leaf name="preference">
          <ext:dynamic-default/>
          <type name="uint32">
            <range value="1..255"/>
          </type>
          <description>
            <text>Default priority of IPv4 static routes. The default value can be controlled by the PAF file and varies according to hardware. To query the default value, you can perform a get operation.</text>
          </description>
        </leaf>
        <leaf name="relay-switch">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable the function of selecting static routes based on recursion depths.</text>
          </description>
        </leaf>
        <leaf name="min-tx-interval">
          <ext:dynamic-default/>
          <type name="uint32"/>
          <units name="ms"/>
          <description>
            <text>Default minimum interval expected at which IPv4 BFD packets are sent to the peer end. The default value can be controlled by the PAF file and varies according to hardware.</text>
          </description>
        </leaf>
        <leaf name="min-rx-interval">
          <ext:dynamic-default/>
          <type name="uint32"/>
          <units name="ms"/>
          <description>
            <text>Default minimum interval expected at which IPv4 BFD packets are received from the peer end. The default value can be controlled by the PAF file and varies according to hardware.</text>
          </description>
        </leaf>
        <leaf name="multiplier">
          <type name="uint32">
            <range value="3..50"/>
          </type>
          <default value="3"/>
          <description>
            <text>IPv4 Local detection multiplier.</text>
          </description>
        </leaf>
        <leaf name="relay-remote">
          <type name="boolean"/>
          <default value="true"/>
          <description>
            <text>Enable/disable the function of recursing IPv4 unicast static routes to remotely leaked VPN routes.</text>
          </description>
        </leaf>
        <leaf name="relay-arp-vlink">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable the function of recursing IPv4 unicast static routes to ARP Vlink routes.</text>
          </description>
        </leaf>
        <leaf name="inherit-cost-switch">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable the function of comparing the costs of inherited routes during static route selection.</text>
          </description>
        </leaf>
        <leaf name="current-route-num">
          <type name="uint32"/>
          <config value="false"/>
          <description>
            <text>Number of IPv4 static routes that have been configured.</text>
          </description>
        </leaf>
        <leaf name="max-route-num">
          <type name="uint32"/>
          <config value="false"/>
          <description>
            <text>Maximum number of IPv4 static routes that can be configured.</text>
          </description>
        </leaf>
        <leaf name="relay-srv6-nexthop">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable the function of recursing IPv4 unicast static routes to SRv6 routes.</text>
          </description>
        </leaf>
      </container>
      <container name="ipv4-relay-tunnel">
        <description>
          <text>Configure route recursion to tunnel.</text>
        </description>
        <leaf name="enable">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable route recursion to tunnel. By default, this configuration takes effect for all routes. If an IP prefix list is configured, the configuration takes effect only for the routes that match the IP prefix list. If a tunnel policy is configured, routes carry the tunnel policy information when recursing to a tunnel.</text>
          </description>
        </leaf>
        <leaf name="ipv4-prefix-ref">
          <when condition="../enable='true'"/>
          <type name="leafref">
            <path value="/rtp:routing-policy/rtp:ipv4-prefix-filters/rtp:ipv4-prefix-filter/rtp:name"/>
          </type>
          <description>
            <text>Name of an IPv4 prefix list.</text>
          </description>
        </leaf>
        <leaf name="tunnel-policy-ref">
          <when condition="../enable='true'"/>
          <type name="leafref">
            <path value="/tnlm:tunnel-management/tnlm:tunnel-policys/tnlm:tunnel-policy/tnlm:name"/>
          </type>
          <description>
            <text>Name of a tunnel policy.</text>
          </description>
        </leaf>
      </container>
      <container name="ipv6-site">
        <description>
          <text>Configure parameters for IPv6 static routes.</text>
        </description>
        <leaf name="preference">
          <ext:dynamic-default/>
          <type name="uint32">
            <range value="1..255"/>
          </type>
          <description>
            <text>Default priority of IPv6 static routes. The default value can be controlled by the PAF file and varies according to hardware. To query the default value, you can perform a get operation.</text>
          </description>
        </leaf>
        <leaf name="min-tx-interval">
          <ext:dynamic-default/>
          <type name="uint32"/>
          <units name="ms"/>
          <description>
            <text>Default minimum interval expected at which IPv6 BFD packets are sent to the peer end. The default value can be controlled by the PAF file and varies according to hardware.</text>
          </description>
        </leaf>
        <leaf name="min-rx-interval">
          <ext:dynamic-default/>
          <type name="uint32"/>
          <units name="ms"/>
          <description>
            <text>Default minimum interval expected at which IPv6 BFD packets are received from the peer end. The default value can be controlled by the PAF file and varies according to hardware.</text>
          </description>
        </leaf>
        <leaf name="multiplier">
          <type name="uint32">
            <range value="3..50"/>
          </type>
          <default value="3"/>
          <description>
            <text>IPv6 Local detection multiplier.</text>
          </description>
        </leaf>
        <leaf name="relay-arp-vlink6">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable the function of recursing IPv6 unicast static routes to ARP Vlink routes.</text>
          </description>
        </leaf>
        <leaf name="current-route-num">
          <type name="uint32"/>
          <config value="false"/>
          <description>
            <text>Number of IPv6 static routes that have been configured.</text>
          </description>
        </leaf>
