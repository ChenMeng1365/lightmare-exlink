  <augment target-node="/ifm:ifm/ifm:interfaces/ifm:interface">
    <description>
      <text>Adds qos configuration to ifm interface model.</text>
    </description>
    <container name="qos">
      <description>
        <text>Configure QoS apply.</text>
      </description>
      <container name="trusts">
        <description>
          <text>List of configure trust ip-dscp or 8021p.</text>
        </description>
        <list name="trust">
          <key value="type direction vlan-mode begin-vlan end-vlan"/>
          <description>
            <text>Configure trust ip-dscp or 8021p.</text>
          </description>
          <leaf name="type">
            <type name="trust-type"/>
            <description>
              <text>Trust ip-dscp or 8021p.</text>
            </description>
          </leaf>
          <leaf name="direction">
            <type name="trust-direction-type"/>
            <description>
              <text>Direction.</text>
            </description>
          </leaf>
          <leaf name="vlan-mode">
            <type name="boolean"/>
            <description>
              <text>Enable/disable VLAN mode.</text>
            </description>
          </leaf>
          <leaf name="begin-vlan">
            <type name="uint32">
              <range value="0..4094"/>
            </type>
            <description>
              <text>Begin VLAN id.</text>
            </description>
          </leaf>
          <leaf name="end-vlan">
            <type name="uint32">
              <range value="0..4094"/>
            </type>
            <description>
              <text>End VLAN id.</text>
            </description>
          </leaf>
          <leaf name="diffserv-domain-name">
            <when condition="../type='ip-dscp'"/>
            <type name="leafref">
              <path value="/qos:qos/qos:classifier-template/qos:diffserv-domains/qos:diffserv-domain/qos:name"/>
            </type>
            <mandatory value="true"/>
            <description>
              <text>Name of a diffserv domain.</text>
            </description>
          </leaf>
          <leaf name="vpn-mode">
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable VPN mode.</text>
            </description>
          </leaf>
        </list>
      </container>
      <container name="traffic-policy-applys">
        <description>
          <text>List of configure traffic policy application.</text>
        </description>
        <list name="traffic-policy-apply">
          <must condition="../../../ifm:l2-mode-enable='false' and (./identifier='no' or not(./identifier)) or ../../../ifm:l2-mode-enable='true'"/>
          <must condition="./vlan-mode='false' or (./vlan-mode='true' and not(../../../ifm:l2-mode-enable='true'))"/>
          <key value="policy-name direction layer vlan-mode pe-vlan-id"/>
          <description>
            <text>Configure traffic policy application.</text>
          </description>
          <leaf name="policy-name">
            <type name="leafref">
              <path value="/qos:qos/qos:classifier-template/qos:traffic-policys/qos:traffic-policy/qos:name"/>
            </type>
            <must condition="not(../identifier!='no' and ../identifier!='none' and ../../../../ifm:l2-mode-enable='true' and /qos:qos/qos:classifier-template/qos:traffic-policys/qos:traffic-policy[qos:name=current()]/qos:share-mode='enable')"/>
            <description>
              <text>Policy name.</text>
            </description>
          </leaf>
          <leaf name="direction">
            <type name="qos-direction-type"/>
            <description>
              <text>Direction.</text>
            </description>
          </leaf>
          <leaf name="layer">
            <type name="policy-apply-layer-type"/>
            <description>
              <text>Classification based on layer 2 or layer 3 information.</text>
            </description>
          </leaf>
          <leaf name="vlan-mode">
            <type name="boolean"/>
            <description>
              <text>Enable/disable VLAN mode.</text>
            </description>
          </leaf>
          <leaf name="pe-vlan-id">
            <type name="uint32">
              <range value="0..4094"/>
            </type>
            <must condition="(../vlan-mode='false' and ../pe-vlan-id=0) or ../vlan-mode='true'"/>
            <description>
              <text>Pe VLAN id.</text>
            </description>
          </leaf>
          <leaf name="vlan-id">
            <when condition="../vlan-mode='true'"/>
            <type name="pub-type:vlan-range">
              <pattern value="((409[0-4]|40[0-8][0-9]|[1-3][0-9]{3}|[1-9][0-9]{1,2}|[1-9])+(-(409[0-4]|40[0-8][0-9]|[1-3][0-9]{3}|[1-9][0-9]{1,2}|[1-9]))?(,((409[0-4]|40[0-8][0-9]|[1-3][0-9]{3}|[1-9][0-9]{1,2}|[1-9])(-(409[0-4]|40[0-8][0-9]|[1-3][0-9]{3}|[1-9][0-9]{1,2}|[1-9]))?))*)"/>
            </type>
            <mandatory value="true"/>
            <description>
              <text>VLAN id.</text>
            </description>
          </leaf>
          <leaf name="identifier">
            <type name="identifier-type"/>
            <default value="no"/>
            <description>
              <text>Identifier. The range supported by this node varies according to the interface type. This node does not support modification or deletion.</text>
            </description>
          </leaf>
        </list>
      </container>
      <container name="traffic-policy-multi-applys">
        <must condition="count(traffic-policy-multi-apply[direction='inbound'])&lt;=16"/>
        <must condition="count(traffic-policy-multi-apply[direction='outbound'])&lt;=16"/>
        <description>
          <text>List of configure traffic multi policy application.</text>
        </description>
        <list name="traffic-policy-multi-apply">
          <must condition="not(../../traffic-policy-applys/traffic-policy-apply[direction=current()/direction])"/>
          <key value="policy-name direction"/>
          <unique tag="direction match-precedence"/>
          <max-elements value="32"/>
          <description>
            <text>Configure traffic policy multi application.</text>
          </description>
          <leaf name="policy-name">
            <type name="leafref">
              <path value="/qos:qos/qos:classifier-template/qos:traffic-policys/qos:traffic-policy/qos:name"/>
            </type>
            <description>
              <text>Policy name.</text>
            </description>
          </leaf>
          <leaf name="direction">
            <type name="qos-direction-type"/>
            <description>
              <text>Direction.</text>
            </description>
          </leaf>
          <leaf name="match-precedence">
            <type name="uint16">
              <range value="1..1024"/>
            </type>
            <mandatory value="true"/>
            <description>
              <text>Specify the precedence of traffic policy.</text>
            </description>
          </leaf>
        </list>
      </container>
      <container name="profile-applys">
        <description>
          <text>List of configure QoS profile application.</text>
        </description>
        <list name="profile-apply">
          <must condition="./ce-begin-vlan-id &lt;= ./ce-end-vlan-id"/>
          <must condition="(./type='interface' and ./pe-vlan-id=0 and ./ce-begin-vlan-id=0 and ./ce-end-vlan-id=0) or (./type='vlan' and ./pe-vlan-id=0 and ./ce-begin-vlan-id!=0) or (./type='pe' and ./pe-vlan-id!=0 and ./ce-begin-vlan-id!=0)"/>
          <key value="profile-name direction type pe-vlan-id ce-begin-vlan-id ce-end-vlan-id identify-type"/>
          <description>
            <text>Configure QoS profile application.</text>
          </description>
          <leaf name="profile-name">
            <type name="leafref">
              <path value="/qos:qos/qos:queue-scheduler-template/qos:profiles/qos:profile/qos:name"/>
            </type>
            <description>
              <text>Qos profile name.</text>
            </description>
          </leaf>
          <leaf name="direction">
            <type name="qos-direction-type"/>
            <description>
              <text>Direction.</text>
            </description>
          </leaf>
          <leaf name="type">
            <type name="profile-apply-type"/>
            <description>
              <text>Profile apply type.</text>
            </description>
          </leaf>
          <leaf name="pe-vlan-id">
            <type name="uint32">
              <range value="0..4094"/>
            </type>
            <description>
              <text>Pe VLAN id.</text>
            </description>
          </leaf>
          <leaf name="ce-begin-vlan-id">
            <type name="uint32">
              <range value="0..4094"/>
            </type>
            <description>
              <text>Ce begin VLAN id.</text>
            </description>
          </leaf>
          <leaf name="ce-end-vlan-id">
            <type name="uint32">
              <range value="0..4094"/>
            </type>
            <description>
              <text>Ce end VLAN id.</text>
            </description>
          </leaf>
          <leaf name="identify-type">
            <type name="identify-type"/>
            <description>
              <text>Identify type. The range supported by this node varies according to the interface type.</text>
            </description>
          </leaf>
          <leaf name="linkage-bandwidth">
            <type name="linkage-bandwidth-type"/>
            <default value="disable"/>
            <description>
              <text>Linkage bandwidth.</text>
            </description>
            <ext:operation-exclude value="update">
              <description>
                <text>The 'linkage-bandwidth' cannot be updated.</text>
              </description>
            </ext:operation-exclude>
          </leaf>
          <leaf name="group-name">
            <when condition="../linkage-bandwidth='disable'"/>
            <type name="template-name"/>
            <description>
              <text>The name of user queue group.</text>
            </description>
            <ext:operation-exclude value="create|update|delete">
              <description>
                <text>After the configuration is created, the node cannot be modified and delete. When the list instance has created without group-name, and the group-name can not be created.</text>
              </description>
            </ext:operation-exclude>
          </leaf>
        </list>
      </container>
      <container name="user-queue-applys">
        <description>
          <text>List of configure user queue application.</text>
        </description>
        <list name="user-queue-apply">
          <key value="direction"/>
          <description>
            <text>Configure user queue application.</text>
          </description>
          <leaf name="direction">
            <type name="qos-direction-type"/>
            <description>
              <text>Direction.</text>
            </description>
          </leaf>
          <leaf name="cir">
            <type name="uint32">
              <range value="0 | 16..4294967294"/>
            </type>
            <units name="kbit/s"/>
            <mandatory value="true"/>
            <description>
              <text>Committed information rate.</text>
            </description>
          </leaf>
          <leaf name="pir">
            <type name="uint32">
              <range value="0 | 16..4294967294"/>
            </type>
            <units name="kbit/s"/>
            <must condition="../pir&gt;=../cir"/>
            <mandatory value="true"/>
            <description>
              <text>Peak information rate.</text>
            </description>
          </leaf>
          <leaf name="flow-queue-name">
            <type name="leafref">
              <path value="/qos:qos/qos:queue-scheduler-template/qos:flow-queues/qos:flow-queue/qos:name"/>
            </type>
            <must condition="/qos:qos/qos:queue-scheduler-template/qos:flow-queues/qos:flow-queue[qos:name=current()]/qos:mode='normal'"/>
            <description>
              <text>Flow queue.</text>
            </description>
          </leaf>
          <leaf name="flow-mapping-name">
            <type name="leafref">
              <path value="/qos:qos/qos:queue-scheduler-template/qos:flow-mappings/qos:flow-mapping/qos:name"/>
            </type>
            <description>
              <text>Flow Mapping.</text>
            </description>
          </leaf>
          <leaf name="user-group-queue-name">
            <type name="leafref">
              <path value="/qos:qos/qos:queue-scheduler-template/qos:user-group-queues/qos:user-group-queue/qos:name"/>
            </type>
            <must condition="/qos:qos/qos:queue-scheduler-template/qos:user-group-queues/qos:user-group-queue[qos:name=current()]/qos:mode='normal'"/>
            <description>
              <text>Group Queue.</text>
