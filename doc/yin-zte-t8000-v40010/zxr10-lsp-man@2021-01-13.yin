  <typedef name="te-lsp-role">
    <type name="enumeration">
      <enum name="INVALID">
        <value value="0"/>
      </enum>
      <enum name="TRANSIT">
        <value value="2"/>
      </enum>
      <enum name="EGRESS">
        <value value="3"/>
      </enum>
      <enum name="INGRESS">
        <value value="1"/>
      </enum>
    </type>
    <description>
      <text>TE LSP's role,0:INVALID,1:INGRESS,2:TRANSIT,3:EGRESS.</text>
    </description>
  </typedef>
  <typedef name="te-lsp-direction">
    <type name="enumeration">
      <enum name="POSITIVE">
