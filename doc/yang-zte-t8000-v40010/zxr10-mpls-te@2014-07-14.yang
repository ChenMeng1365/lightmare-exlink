              key name ;
              max-elements 131070 ;
              min-elements 0 ;
              leaf name {
                description 'Tunnel name' ;
                type ncd:ifname ;
              }
              leaf te-metric {
                description 'The Configuration of traffic-eng metric for Tunnel'
                          + '.'
                            ;
                type uint32 {
                  range 1..4294967295 ;
                }
