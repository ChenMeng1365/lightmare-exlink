<?xml version="1.0" encoding="UTF-8"?>
<module name="zxr10-tunnel-group"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:tunnel-group="http://www.zte.com.cn/zxr10/netconf/schema/rosng/tunnel-group">
  <namespace uri="http://www.zte.com.cn/zxr10/netconf/schema/rosng/tunnel-group"/>
  <prefix value="tunnel-group"/>
  <yang-version value="1"/>
  <organization>
    <text>ZTE Corporation</text>
  </organization>
  <contact>
    <text>ZTE Corporation  
   Nanjing Institute of ZTE Corporation 
   No.68 Zijinghua Rd. Yuhuatai District, Nanjing, China 
   Tel: +86-25-52870000</text>
  </contact>
  <description>
    <text>The netconf module to describe the tunnel group.</text>
  </description>
  <revision date="2016-11-18">
    <description>
      <text>1,Change the maxmum value of /configuration/tunnel-group/tunnel-group-configuration/type/ring-protection/ring-configuration/protect-section from 4000 to performance ID 67305544.</text>
    </description>
  </revision>
  <revision date="2016-07-22">
    <description>
      <text>Expand the maxmum value of '/state/tunnel-group-state/group-id'from performance ID 67305513 to 4294967295.</text>
    </description>
  </revision>
  <revision date="2015-07-28">
    <description>
      <text>Change config attribute of state container from TRUE to FALSE.</text>
    </description>
  </revision>
  <revision date="2013-11-04">
    <description>
      <text>1.Remove if-feature clause on container of linear-configuration and ring-configuration.
2.Remove if-feature clause on leaf of protect-type/protect-derection/protect-receiver/working-tunnel/protect-tunnel/protect-strategy/protect-section.
3.Add if-feature clause on all case clause.
4.Add if-feature clause on leaf of working-tunnel-tail-aps-id.</text>
    </description>
  </revision>
  <revision date="2013-09-04">
    <description>
      <text>Initial version.</text>
    </description>
  </revision>
  <feature name="linear-protection">
    <description>
      <text>Linear tunnel protection.</text>
    </description>
  </feature>
  <feature name="ring-protection">
    <description>
      <text>Ring protection.</text>
    </description>
  </feature>
  <feature name="ring-cross-protection">
    <description>
      <text>Ring cross protection.</text>
    </description>
  </feature>
  <typedef name="protect-type">
    <type name="enumeration">
      <enum name="1:1">
        <value value="1"/>
        <description>
          <text>1:1 protection.</text>
        </description>
      </enum>
      <enum name="1+1">
        <value value="2"/>
        <description>
          <text>1+1 protection.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Protection type of tunnel group.</text>
    </description>
  </typedef>
  <typedef name="protect-direction">
    <type name="enumeration">
      <enum name="unidirectional">
        <value value="1"/>
        <description>
          <text>Unidirectional switching.</text>
        </description>
      </enum>
      <enum name="bidirectional">
        <value value="2"/>
        <description>
          <text>Bidirectional switching.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Protection switching type of tunnel group.</text>
    </description>
  </typedef>
  <typedef name="protect-receiver">
    <type name="enumeration">
      <enum name="selective">
        <value value="1"/>
        <description>
          <text>Selective receiving.</text>
        </description>
      </enum>
      <enum name="both">
        <value value="2"/>
        <description>
          <text>Both receiving.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Protection receiving type of tunnel group.</text>
    </description>
  </typedef>
  <typedef name="protect-strategy">
    <type name="enumeration">
      <enum name="aps">
        <value value="1"/>
        <description>
          <text>Automatic protection switching.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Protection strategy of tunnel group.</text>
    </description>
  </typedef>
  <typedef name="switch-state">
    <type name="enumeration">
      <enum name="unknown">
        <value value="0"/>
        <description>
          <text>Unknown switching state.</text>
        </description>
      </enum>
      <enum name="switch">
        <value value="1"/>
        <description>
          <text>Switch to protection entity.</text>
        </description>
      </enum>
      <enum name="unswitch">
        <value value="2"/>
        <description>
          <text>Working entity is active.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Protection switching state of tunnel group.</text>
    </description>
  </typedef>
  <typedef name="tunnel-state">
    <type name="enumeration">
      <enum name="OK">
        <value value="0"/>
        <description>
          <text>Signal OK.</text>
        </description>
      </enum>
      <enum name="SF">
        <value value="1"/>
        <description>
          <text>Signal Fail.</text>
        </description>
      </enum>
      <enum name="SD">
        <value value="2"/>
        <description>
          <text>Signal degrade.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Tunnel state.</text>
    </description>
  </typedef>
  <container name="configuration">
    <description>
      <text>Configuration model information of tunnel group.</text>
    </description>
    <container name="tunnel-group">
      <description>
        <text>The configuration information of tunnel group.</text>
      </description>
      <list name="tunnel-group-configuration">
        <description>
          <text>Configure tunnel group parameters.</text>
        </description>
        <key value="group-id"/>
        <leaf name="group-id">
          <description>
            <text>Configure ID of tunnel group.It is between [1,32768].</text>
          </description>
          <type name="uint32">
            <range value="1..32768">
              <description>
                <text>Tunnel group ID.</text>
              </description>
            </range>
          </type>
        </leaf>
        <choice name="type">
          <description>
            <text>You can configure ring tunnel group or linear tunnel group.</text>
          </description>
          <case name="linear-protection">
            <description>
              <text>Configure linear tunnel group.</text>
            </description>
            <container name="linear-configuration">
              <description>
                <text>Configure linear tunnel group parameters.</text>
              </description>
              <leaf name="protect-type">
                <description>
                  <text>Configure protection type of linear tunnel group.</text>
                </description>
                <type name="protect-type"/>
                <mandatory value="true"/>
              </leaf>
              <leaf name="protect-direction">
                <description>
                  <text>Configure protection switching type of linear tunnel group.</text>
                </description>
                <type name="protect-direction"/>
                <mandatory value="true"/>
              </leaf>
              <leaf name="protect-receiver">
                <description>
                  <text>Configure protection receiving type of linear tunnel group.</text>
                </description>
                <type name="protect-receiver"/>
                <mandatory value="true"/>
              </leaf>
              <leaf name="working-tunnel">
                <description>
                  <text>Configure working tunnel of linear tunnel group.No default value, must configure.</text>
                </description>
                <type name="uint32">
                  <range value="1..131072">
                    <description>
                      <text>Working tunnel ID of linear tunnel group.0 is invalid value.</text>
                    </description>
                  </range>
                </type>
              </leaf>
              <leaf name="protect-tunnel">
                <description>
                  <text>Configure protection tunnel of linear tunnel group.No default value, must configure.</text>
                </description>
                <type name="uint32">
                  <range value="1..131072">
                    <description>
                      <text>Protection tunnel ID of linear tunnel group.0 is invalid value.</text>
                    </description>
                  </range>
                </type>
              </leaf>
              <leaf name="protect-strategy">
                <description>
                  <text>Configure protection switching strategy of linear tunnel group.No default value, must configure.</text>
                </description>
                <type name="protect-strategy"/>
              </leaf>
            </container>
          </case>
          <case name="ring-protection">
            <description>
              <text>Configure ring tunnel group.</text>
            </description>
            <container name="ring-configuration">
              <description>
                <text>Configure ring tunnel group parameters.</text>
              </description>
              <leaf name="protect-type">
                <description>
                  <text>Configure protection type of ring tunnel group.You can only configure protection type as "ring".</text>
                </description>
                <type name="enumeration">
                  <enum name="ring">
                    <value value="3"/>
                    <description>
                      <text>Ring protection.</text>
                    </description>
                  </enum>
                </type>
                <mandatory value="true"/>
              </leaf>
              <leaf name="working-tunnel">
                <description>
                  <text>Configure working tunnel of ring tunnel group.No default value, must configure.</text>
                </description>
                <type name="uint32">
                  <range value="1..131072">
                    <description>
                      <text>Working tunnel ID of ring tunnel group.</text>
                    </description>
                  </range>
                </type>
              </leaf>
              <leaf name="protect-tunnel">
                <description>
                  <text>Configure ring protection tunnel of ring tunnel group.The tunnel must be ring tunnel.No default value, must configure.</text>
                </description>
                <type name="uint32">
                  <range value="1..131072">
                    <description>
                      <text>Protection tunnel ID of ring tunnel group.</text>
                    </description>
                  </range>
                </type>
              </leaf>
              <leaf name="protect-section">
                <description>
                  <text>Configure protection section of ring tunnel group.No default value, must configure.</text>
                </description>
                <type name="uint32">
                  <range value="1..4000">
                    <description>
                      <text>Protecion section ID of ring tunnel group.</text>
                    </description>
                  </range>
                </type>
              </leaf>
              <leaf name="working-tunnel-tail-aps-id">
                <description>
                  <text>Configure APS ID of working tunnel tail node in ring tunnel group.No default value, must configure.</text>
                </description>
                <type name="uint8">
                  <range value="1..127">
                    <description>
                      <text>APS ID of Working tunnel tail node.</text>
                    </description>
                  </range>
                </type>
              </leaf>
            </container>
          </case>
        </choice>
      </list>
    </container>
  </container>
  <container name="state">
    <config value="false"/>
    <description>
      <text>State model information of tunnel group.</text>
    </description>
    <container name="tunnel-group">
      <description>
        <text>State data of tunnel group.</text>
      </description>
      <list name="tunnel-group-state">
        <description>
          <text>Query tunnel group status information, the group ID is between [1,4294967295].Support queries without conditions and at protection group ID query.</text>
        </description>
        <key value="group-id"/>
        <leaf name="group-id">
          <description>
            <text>ID of tunnel group.</text>
          </description>
          <type name="uint32">
            <range value="1..4294967295">
              <description>
                <text>ID of tunnel group.</text>
              </description>
            </range>
          </type>
        </leaf>
        <leaf name="working-tunnel-state">
          <description>
            <text>State of working tunnel.</text>
          </description>
          <type name="tunnel-state"/>
        </leaf>
        <leaf name="protect-tunnel-state">
          <description>
            <text>State of protection tunnel.</text>
          </description>
          <type name="tunnel-state"/>
        </leaf>
        <leaf name="switch-state">
          <description>
            <text>Protection switching state.</text>
          </description>
          <type name="switch-state"/>
        </leaf>
      </list>
    </container>
  </container>
</module>
