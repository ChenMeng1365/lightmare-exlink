      </enum>
      <enum name="no">
        <value value="0"/>
        <description>
          <text>No.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Enable status.</text>
    </description>
  </typedef>
  <typedef name="mdn-global-status">
    <type name="enumeration">
      <enum name="rx-only">
        <value value="2"/>
        <description>
          <text>MDN status is rx-only.</text>
        </description>
      </enum>
      <enum name="disabled">
        <value value="4"/>
        <description>
          <text>MDN status is disabled.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>MDN global status.</text>
    </description>
  </typedef>
  <container name="lldp">
    <presence value="Enables LLDP"/>
    <description>
      <text>Link layer discovery protocol.</text>
    </description>
    <container name="global-attribute">
      <description>
        <text>Configure LLDP global attribute.</text>
      </description>
      <leaf name="message-transmission-interval">
        <type name="int32">
          <range value="5..32768"/>
        </type>
        <units name="s"/>
        <must condition="../message-transmission-interval&gt;=(../message-transmission-delay*4)"/>
        <default value="30"/>
        <description>
          <text>Interval at which LLDP messages are transmitted.</text>
        </description>
      </leaf>
      <leaf name="message-transmission-delay">
        <type name="int32">
          <range value="1..8192"/>
        </type>
        <units name="s"/>
        <must condition="../message-transmission-interval&gt;=(../message-transmission-delay*4)"/>
        <default value="2"/>
        <description>
          <text>Time between two successive LLDP messages.</text>
        </description>
      </leaf>
      <leaf name="message-transmission-hold-multiplier">
        <type name="int32">
          <range value="2..10"/>
        </type>
        <default value="4"/>
        <description>
          <text>Multiplier on the message transmission interval that determines the actual TTL value used in an LLDPDU.</text>
        </description>
      </leaf>
      <leaf name="restart-delay">
        <type name="int32">
          <range value="1..10"/>
        </type>
        <units name="s"/>
        <default value="2"/>
        <description>
          <text>Delay from when LLDP interface status becomes disabled until re-initialization will be attempted.</text>
        </description>
      </leaf>
      <leaf name="trap-interval">
        <type name="int32">
          <range value="5..3600"/>
        </type>
        <units name="s"/>
        <default value="5"/>
        <description>
          <text>LLDP trap notification interval.</text>
        </description>
      </leaf>
      <leaf name="fast-count">
        <type name="int32">
          <range value="1..8"/>
        </type>
        <default value="4"/>
        <description>
          <text>Number of rapidly transmitted LLDP messages.</text>
        </description>
      </leaf>
      <leaf name="managment-address">
        <type name="leafref">
          <path value="/ifm:ifm/ifm:interfaces/ifm:interface/ip:ipv4/ip:addresses/ip:address/ip:ip"/>
        </type>
        <description>
          <text>IP address for network management, which must exist in the device.</text>
        </description>
      </leaf>
      <leaf name="mdn-status">
        <type name="mdn-global-status"/>
        <default value="disabled"/>
        <description>
          <text>MDN status.</text>
        </description>
      </leaf>
      <leaf name="trap-enable">
        <type name="global-status"/>
        <default value="disabled"/>
        <config value="false"/>
        <description>
          <text>Enable the LLDP neighbor change trap function.</text>
        </description>
      </leaf>
    </container>
  </container>
  <augment target-node="/ifm:ifm/ifm:interfaces/ifm:interface">
    <when condition="/lldp:lldp"/>
    <description>
      <text>LLDP interface.</text>
    </description>
    <container name="lldp">
      <description>
        <text>Configure LLDP interfaces.</text>
      </description>
      <container name="session">
        <description>
          <text>Configure LLDP interface.</text>
        </description>
        <leaf name="admin-status">
          <type name="admin-status"/>
          <default value="tx-rx"/>
          <description>
            <text>LLDP enable status.</text>
          </description>
        </leaf>
        <container name="tlv-enable">
          <when condition="not(../admin-status='disabled')"/>
          <description>
            <text>Configure LLDP TLV.</text>
          </description>
          <leaf name="management-address">
            <type name="boolean"/>
            <default value="true"/>
            <description>
              <text>Enable/disable the device to send the management address TLV.</text>
            </description>
          </leaf>
          <leaf name="port-description">
            <type name="boolean"/>
            <default value="true"/>
            <description>
              <text>Enable/disable the device to send the interface description TLV.</text>
            </description>
          </leaf>
          <leaf name="system-capability">
            <type name="boolean"/>
            <default value="true"/>
            <description>
              <text>Enable/disable the device to send the system capability TLV.</text>
            </description>
          </leaf>
          <leaf name="system-description">
            <type name="boolean"/>
            <default value="true"/>
            <description>
              <text>Enable/disable the device to send the system description TLV.</text>
            </description>
          </leaf>
          <leaf name="system-name">
            <type name="boolean"/>
            <default value="true"/>
            <description>
              <text>Enable/disable the device to send the system name TLV.</text>
            </description>
          </leaf>
          <leaf name="port-vlan-id">
            <when condition="(../../../../ifm:type !='Pos') and (../../../../ifm:type !='FlexE-50G') and (../../../../ifm:type !='FlexE-100G') and (../../../../ifm:type !='FlexE-200GE') and (../../../../ifm:type !='FlexE-50|100G')"/>
            <type name="boolean"/>
            <default value="true"/>
            <description>
              <text>Enable/disable the device to send the interface VLAN ID TLV.</text>
            </description>
          </leaf>
          <leaf name="protocol-vlan-id-enable">
            <when condition="(../../../../ifm:type !='Pos') and (../../../../ifm:type !='FlexE-50G') and (../../../../ifm:type !='FlexE-100G') and (../../../../ifm:type !='FlexE-200GE') and (../../../../ifm:type !='FlexE-50|100G')"/>
            <type name="boolean"/>
            <default value="true"/>
            <description>
              <text>Enable/disable the device to send the protocol VLAN ID TLV.</text>
            </description>
          </leaf>
          <leaf name="protocol-vlan-id">
            <when condition="(not(../protocol-vlan-id-enable='false')) and (../../../../ifm:type !='Pos') and (../../../../ifm:type !='FlexE-50G') and (../../../../ifm:type !='FlexE-100G') and (../../../../ifm:type !='FlexE-200GE') and (../../../../ifm:type !='FlexE-50|100G')"/>
            <type name="int32">
              <range value="1..4094"/>
            </type>
            <description>
              <text>The protocol VLAN ID is sent.</text>
            </description>
          </leaf>
          <leaf name="vlan-name-enable">
            <when condition="(../../../../ifm:type !='Pos') and (../../../../ifm:type !='FlexE-50G') and (../../../../ifm:type !='FlexE-100G') and (../../../../ifm:type !='FlexE-200GE') and (../../../../ifm:type !='FlexE-50|100G')"/>
            <type name="boolean"/>
            <default value="true"/>
            <description>
              <text>Enable/disable the device to send the VLAN name TLV.</text>
            </description>
          </leaf>
          <leaf name="vlan-name-id">
            <when condition="(not(../vlan-name-enable='false')) and (../../../../ifm:type !='Pos') and (../../../../ifm:type !='FlexE-50G') and (../../../../ifm:type !='FlexE-100G') and (../../../../ifm:type !='FlexE-200GE') and (../../../../ifm:type !='FlexE-50|100G')"/>
            <type name="int32">
              <range value="1..4094"/>
            </type>
            <description>
              <text>The VLAN ID of the VLAN name TLV is sent.</text>
            </description>
          </leaf>
          <leaf name="protocol-identity">
            <when condition="(../../../../ifm:type !='Pos') and (../../../../ifm:type !='FlexE-50G') and (../../../../ifm:type !='FlexE-100G') and (../../../../ifm:type !='FlexE-200GE') and (../../../../ifm:type !='FlexE-50|100G')"/>
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable the device to send the protocol identity TLV.</text>
            </description>
          </leaf>
          <leaf name="mac-physic">
            <when condition="(../../../../ifm:type !='Pos') and (../../../../ifm:type !='FlexE-50G') and (../../../../ifm:type !='FlexE-100G') and (../../../../ifm:type !='FlexE-200GE') and (../../../../ifm:type !='FlexE-50|100G')"/>
            <type name="boolean"/>
            <default value="true"/>
            <description>
              <text>Enable/disable the device to send the MAC/PHY TLV.</text>
            </description>
          </leaf>
          <leaf name="link-aggregation">
            <when condition="(../../../../ifm:type !='Pos') and (../../../../ifm:type !='FlexE-50G') and (../../../../ifm:type !='FlexE-100G') and (../../../../ifm:type !='FlexE-200GE') and (../../../../ifm:type !='FlexE-50|100G')"/>
            <type name="boolean"/>
            <default value="true"/>
            <description>
              <text>Enable/disable the device to send the link aggregation TLV.</text>
            </description>
          </leaf>
          <leaf name="max-frame-size">
            <when condition="(../../../../ifm:type !='Pos') and (../../../../ifm:type !='FlexE-50G') and (../../../../ifm:type !='FlexE-100G') and (../../../../ifm:type !='FlexE-200GE') and (../../../../ifm:type !='FlexE-50|100G')"/>
            <type name="boolean"/>
            <default value="true"/>
            <description>
              <text>Enable/disable the device to send the maximum frame size TLV.</text>
            </description>
          </leaf>
          <leaf name="ddp">
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable the device to send the DDP TLV.</text>
            </description>
          </leaf>
          <leaf name="identity">
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable the device to send the identity TLV.</text>
            </description>
          </leaf>
        </container>
        <container name="neighbors">
          <config value="false"/>
          <description>
            <text>List of information about LLDP neighbors.</text>
          </description>
          <list name="neighbor">
            <key value="index"/>
            <description>
              <text>Statistics of information about an LLDP neighbor.</text>
            </description>
            <leaf name="index">
              <type name="uint32">
                <range value="0..4294967295"/>
              </type>
              <description>
                <text>Neighbor index.</text>
              </description>
            </leaf>
            <leaf name="chassis-id-sub-type">
              <type name="chassis-id-sub-type"/>
              <description>
                <text>Chassis type.</text>
              </description>
            </leaf>
            <leaf name="chassis-id">
              <type name="string">
                <length value="1..765"/>
              </type>
              <description>
                <text>Chassis ID.</text>
              </description>
            </leaf>
            <leaf name="port-id-sub-type">
              <type name="port-id-sub-type"/>
              <description>
                <text>Interface ID subtype.</text>
              </description>
            </leaf>
            <leaf name="port-id">
              <type name="string">
                <length value="1..765"/>
              </type>
              <description>
                <text>Interface ID.</text>
              </description>
            </leaf>
            <leaf name="port-description">
              <type name="string">
                <length value="1..256"/>
              </type>
              <description>
                <text>Interface description.</text>
              </description>
            </leaf>
            <leaf name="system-name">
              <type name="string">
                <length value="1..256"/>
              </type>
              <description>
                <text>System name.</text>
              </description>
            </leaf>
            <leaf name="system-description">
              <type name="string">
                <length value="1..256"/>
              </type>
              <description>
                <text>System description.</text>
              </description>
            </leaf>
            <leaf name="system-capabilities-enabled">
              <type name="string">
                <length value="1..90"/>
              </type>
              <description>
                <text>System capabilities enabled.</text>
              </description>
            </leaf>
            <leaf name="system-capabilities-supported">
              <type name="string">
                <length value="1..90"/>
              </type>
              <description>
                <text>System capabilities supported.</text>
              </description>
            </leaf>
            <leaf name="expired-time">
              <type name="int32">
                <range value="0..65535"/>
              </type>
              <units name="s"/>
              <description>
                <text>Expired time.</text>
              </description>
            </leaf>
            <leaf name="port-vlan-id">
              <type name="int32">
                <range value="0..4094"/>
              </type>
              <description>
                <text>Interface VLAN ID, 0 means not supported.</text>
              </description>
            </leaf>
            <leaf name="protocol-identity">
              <type name="string">
                <length value="1..64"/>
              </type>
              <description>
                <text>Protocol supported by the device.</text>
              </description>
            </leaf>
            <leaf name="auto-negotiation-supported">
              <type name="support-status"/>
              <description>
                <text>Whether the interface supports auto-negotiation.</text>
              </description>
            </leaf>
            <leaf name="auto-negotiation-enabled">
              <type name="enable-status"/>
              <description>
                <text>Whether auto-negotiation is enabled on the interface.</text>
              </description>
            </leaf>
            <leaf name="auto-negotiation-capability">
              <type name="string">
                <length value="1..192"/>
              </type>
              <description>
                <text>Auto-negotiation capability of the interface.</text>
              </description>
            </leaf>
            <leaf name="oper-mau-type">
              <type name="dot3-mau-type"/>
              <description>
                <text>Operational MAU type.</text>
              </description>
            </leaf>
            <leaf name="link-aggregation-supported">
              <type name="support-status"/>
              <description>
                <text>Whether the interface supports link aggregation.</text>
              </description>
            </leaf>
            <leaf name="link-aggregation-enabled">
              <type name="enable-status"/>
              <description>
                <text>Whether link aggregation is enabled on the interface.</text>
              </description>
            </leaf>
            <leaf name="aggregation-port-id">
              <type name="int32">
                <range value="0..2147483647"/>
              </type>
              <description>
                <text>If link aggregation is disabled, the value is 0.</text>
              </description>
            </leaf>
            <leaf name="maximum-frame-size">
              <type name="int32">
                <range value="0..65535"/>
              </type>
              <description>
                <text>Maximum frame size supported by an interface.</text>
              </description>
            </leaf>
            <leaf name="discovered-time">
              <type name="yang:date-and-time"/>
              <description>
                <text>Time when the neighbor was discovered.</text>
              </description>
            </leaf>
            <container name="management-addresss">
              <description>
                <text>List of management addresses of LLDP neighbors.</text>
              </description>
              <list name="management-address">
                <key value="type value length"/>
                <description>
                  <text>Statistics of management address of an LLDP neighbor.</text>
                </description>
                <leaf name="type">
                  <type name="management-address-type"/>
                  <description>
                    <text>Management address type of an LLDP neighbor.</text>
                  </description>
                </leaf>
                <leaf name="value">
                  <type name="string">
                    <length value="1..93"/>
                  </type>
                  <description>
                    <text>Management address of an LLDP neighbor.</text>
                  </description>
                </leaf>
                <leaf name="length">
                  <type name="int32">
                    <range value="0..2147483647"/>
                  </type>
                  <description>
                    <text>Management address length of an LLDP neighbor.</text>
                  </description>
                </leaf>
                <leaf name="if-sub-type">
                  <type name="man-addr-if-sub-type"/>
                  <description>
                    <text>Management address interface type of an LLDP neighbor.</text>
                  </description>
                </leaf>
                <leaf name="if-id">
                  <type name="int32">
                    <range value="0..2147483647"/>
                  </type>
                  <description>
                    <text>Management address interface ID of an LLDP neighbor.</text>
                  </description>
                </leaf>
                <leaf name="oid">
                  <type name="string">
                    <length value="1..640"/>
                  </type>
                  <description>
                    <text>Management address OID of an LLDP neighbor.</text>
                  </description>
                </leaf>
              </list>
            </container>
            <container name="protocol-vlans">
              <description>
                <text>List of protocol VLANs of LLDP neighbors.</text>
              </description>
              <list name="protocol-vlan">
                <key value="id"/>
                <description>
                  <text>Statistics of protocol VLAN of an LLDP neighbor.</text>
                </description>
                <leaf name="id">
                  <type name="int32">
                    <range value="0..4094"/>
                  </type>
                  <description>
                    <text>Protocol VLAN ID, 0 means not supported.</text>
                  </description>
                </leaf>
                <leaf name="supported">
                  <type name="support-status"/>
                  <description>
                    <text>Whether the interface supports protocol VLAN.</text>
                  </description>
                </leaf>
                <leaf name="enabled">
                  <type name="enable-status"/>
                  <description>
                    <text>Whether protocol VLAN is enabled on the interface.</text>
                  </description>
                </leaf>
              </list>
            </container>
            <container name="vlan-names">
              <description>
                <text>List of VLAN names of LLDP neighbors.</text>
              </description>
              <list name="vlan-name">
                <key value="id"/>
                <description>
                  <text>Statistics of VLAN name of an LLDP neighbor.</text>
                </description>
                <leaf name="id">
                  <type name="int32">
                    <range value="0..4094"/>
                  </type>
                  <description>
                    <text>VLAN ID, 0 means not supported.</text>
                  </description>
                </leaf>
                <leaf name="value">
                  <type name="string">
                    <length value="1..33"/>
                  </type>
                  <description>
                    <text>VLAN name.</text>
                  </description>
                </leaf>
              </list>
            </container>
            <container name="unknown-tlvs">
              <description>
                <text>List of unrecognized TLVs.</text>
              </description>
              <list name="unknown-tlv">
                <key value="type"/>
                <description>
                  <text>Statistics of unrecognized TLV.</text>
                </description>
                <leaf name="type">
                  <type name="int32">
                    <range value="9..126"/>
                  </type>
                  <description>
                    <text>Unrecognized TLV type.</text>
                  </description>
                </leaf>
                <leaf name="info">
                  <type name="string">
                    <length value="1..1600"/>
                  </type>
                  <description>
                    <text>Unrecognized TLV information.</text>
                  </description>
                </leaf>
              </list>
            </container>
            <container name="unknown-organizationally-defined-tlvs">
              <description>
                <text>List of unrecognized organizationally defined TLVs.</text>
              </description>
              <list name="unknown-organizationally-defined-tlv">
                <key value="oui sub-type index"/>
                <description>
                  <text>Statistics of unrecognized organizationally defined TLV.</text>
                </description>
                <leaf name="oui">
                  <type name="string">
                    <length value="1..12"/>
                  </type>
                  <description>
                    <text>OUI of an unrecognized organizationally defined TLV.</text>
                  </description>
                </leaf>
                <leaf name="sub-type">
                  <type name="int32">
                    <range value="1..256"/>
                  </type>
                  <description>
                    <text>Subtype of an unrecognized organizationally defined TLV.</text>
                  </description>
                </leaf>
                <leaf name="index">
                  <type name="int32">
                    <range value="0..2147483647"/>
                  </type>
                  <description>
                    <text>Index of an unrecognized organizationally defined TLV.</text>
                  </description>
                </leaf>
                <leaf name="info">
                  <type name="string">
                    <length value="1..1600"/>
                  </type>
                  <description>
                    <text>Information about an unrecognized organizationally defined TLV.</text>
                  </description>
                </leaf>
              </list>
            </container>
            <container name="ddp-tlv">
              <description>
                <text>Statistics of information about neighbor DDP TLV.</text>
              </description>
              <leaf name="serial-number">
                <type name="string">
                  <length value="1..33"/>
                </type>
                <description>
                  <text>Neighbor serial number.</text>
                </description>
              </leaf>
              <leaf name="if-index">
                <type name="uint32">
                  <range value="0..4294967295"/>
                </type>
                <description>
                  <text>IfIndex of neighbor DDP TLV enabled.</text>
                </description>
              </leaf>
              <leaf name="ipv4-address">
                <type name="inet:ipv4-address-no-zone"/>
                <description>
                  <text>Neighbor IP.</text>
                </description>
              </leaf>
              <leaf name="mac-address">
                <type name="pub-type:mac-address">
                  <length value="0..14"/>
                  <pattern value="[0-9a-fA-F]{4}(-[0-9a-fA-F]{4}){2}"/>
                </type>
                <description>
                  <text>Neighbor system MAC.</text>
                </description>
              </leaf>
            </container>
            <container name="identity-tlv">
              <config value="false"/>
              <description>
                <text>Statistics of information about the neighbor identity TLV.</text>
              </description>
              <leaf name="identity">
                <type name="string">
                  <length value="1..508"/>
                </type>
                <description>
                  <text>Neighbor identity.</text>
                </description>
              </leaf>
            </container>
          </list>
        </container>
      </container>
    </container>
  </augment>
</module>
