      <description>
        <text>Description of test instance.</text>
      </description>
    </leaf>
    <leaf name="age-time">
      <type name="pub-type:time"/>
      <default value="00:00:00"/>
      <description>
        <text>Aging time of test instance.</text>
      </description>
    </leaf>
    <leaf name="path-type">
      <type name="path-type"/>
      <description>
        <text>Path type.</text>
      </description>
    </leaf>
    <leaf name="threshold-rtd">
      <type name="uint32">
        <range value="1..60000"/>
      </type>
      <description>
        <text>RTD threshold.</text>
      </description>
    </leaf>
