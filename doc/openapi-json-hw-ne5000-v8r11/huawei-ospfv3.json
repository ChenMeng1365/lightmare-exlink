    "definitions": {
        "policy-type": {
            "description": "Type of a filtering rule. The available options are ACL Name or ACL Number or IP Prefix List or Route policy.",
            "properties": {
                "policy-type": {
                    "description": "Type of a filtering rule.",
                    "properties": {
                        "basic-acl": {
                            "description": "Apply basic ACL Name or ACL Number.",
                            "properties": {
                                "name-or-num": {
                                    "type": "string",
                                    "x-path": "/acl:acl/acl:group6s/acl:group6/acl:identity",
                                    "description": "Apply basic ACL Name or ACL Number."
                                }
                            }
                        },
                        "ipv6-prefix": {
                            "description": "Set the name of an IPv6 prefix list.",
                            "properties": {
                                "ipv6-prefix": {
                                    "type": "string",
                                    "x-path": "/rtp:routing-policy/rtp:ipv6-prefix-filters/rtp:ipv6-prefix-filter/rtp:name",
                                    "description": "Set the name of an IPv6 prefix list."
                                }
                            }
                        },
                        "route-policy": {
                            "description": "Set a route policy name.",
                            "properties": {
                                "route-policy": {
                                    "type": "string",
                                    "x-path": "/rtp:routing-policy/rtp:policy-definitions/rtp:policy-definition/rtp:name",
                                    "description": "Set a route policy name."
                                }
                            }
                        },
                        "route-filter": {
                            "description": "Set a route filter name.",
                            "properties": {
                                "route-filter": {
                                    "type": "string",
                                    "x-path": "/xpl:xpl/xpl:route-filters/xpl:route-filter/xpl:name",
                                    "description": "Set a route filter name."
                                }
                            }
                        }
                    }
                }
            }
        },
        "ospfv3_schema": {
            "description": "Configure OSPFv3 config.",
            "properties": {
                "check-rt-id": {
                    "description": "Configure automatic modification of conflicted router IDs. If enable, check and resolve OSPF router ID conflict.",
                    "properties": {
                        "conflict-flag": {
                            "type": "boolean",
                            "description": "Enable/disable automatic modification of conflicted router IDs. If enable, check and resolve OSPF router ID conflict."
                        }
                    }
                },
                "session-car": {
                    "description": "Configure OSPFv3 Session CAR.",
                    "properties": {
                        "enable": {
                            "type": "boolean",
                            "description": "Enable/disable OSPFv3 Session CAR."
                        },
                        "cir-interval": {
                            "type": "string",
                            "description": "Specify the Session CAR CIR interval."
                        },
                        "cbs-interval": {
                            "type": "string",
                            "description": "Specify the Session CAR CBS interval."
                        },
                        "pir-interval": {
                            "type": "string",
                            "description": "Specify the Session CAR PIR interval."
                        },
                        "pbs-interval": {
                            "type": "string",
                            "description": "Specify the Session CAR PBS interval."
                        }
                    }
                },
                "flush-source-trace": {
                    "description": "Configure OSPF flush source trace globally.",
                    "properties": {
                        "enable": {
                            "type": "boolean",
                            "description": "Enable/disable OSPF flush source trace globally."
                        },
                        "port": {
                            "type": "string",
                            "description": "Set OSPF flush source trace port allocated by huawei-pp4.The related CLI is 'display ip port info | inc UDP'."
                        }
                    }
                }
            }
        },
        "check-rt-id_schema": {
            "description": "Configure automatic modification of conflicted router IDs. If enable, check and resolve OSPF router ID conflict.",
            "properties": {
                "conflict-flag": {
