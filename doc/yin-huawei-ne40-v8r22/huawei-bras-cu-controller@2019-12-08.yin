  <rpc name="reset-south-conflict-by-up">
    <description>
      <text>Reset south conflict configuration by up.</text>
    </description>
    <input>
      <leaf name="up-id">
        <type name="leafref">
          <path value="/bras-cu-controller:bras-cu-controller/bras-cu-controller:resource-ups/bras-cu-controller:resource-up/bras-cu-controller:up-id"/>
        </type>
        <mandatory value="true"/>
        <description>
          <text>UP ID.</text>
        </description>
      </leaf>
    </input>
  </rpc>
  <rpc name="reset-south-conflict-all-up">
    <description>
      <text>Reset south conflict configuration for all up.</text>
    </description>
  </rpc>
  <augment target-node="/devm-vnf:devm-vnf/devm-vnf:vnf-boards/devm-vnf:vnf-board">
    <description>
      <text>CP cpu defend to the devm module.</text>
    </description>
