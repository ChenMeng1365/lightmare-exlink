module: zxr10-cip
  +--rw configuration
     +--rw cip
        +--rw bind
        |  +--rw bind-interfaces* [src-interface]
        |     +--rw src-interface    ncd:ifname
        |     +--rw dst-interface    ncd:ifname
        +--rw multi-binds
           +--rw multi-bind* [cip-name channel-id]
              +--rw cip-name          ncd:ifname
              +--rw channel-id        uint8
              +--rw interface-name    ncd:ifname
