<?xml version="1.0" encoding="UTF-8"?>
<module name="huawei-netconf-proxy"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:ncp="urn:huawei:yang:huawei-netconf-proxy"
        xmlns:ext="urn:huawei:yang:huawei-extension">
  <namespace uri="urn:huawei:yang:huawei-netconf-proxy"/>
  <prefix value="ncp"/>
  <import module="huawei-extension">
    <prefix value="ext"/>
  </import>
  <organization>
    <text>Huawei Technologies Co., Ltd.</text>
  </organization>
  <contact>
    <text>Huawei Industrial Base
Bantian, Longgang
Shenzhen 518129
People's Republic of China
Website: http://www.huawei.com
Email: support@huawei.com</text>
  </contact>
  <description>
    <text>NETCONF Proxy Protocol Data Types and Protocol Operations.
This YANG module describe how to define a netconf proxy.</text>
  </description>
  <revision date="2019-09-15">
    <description>
      <text>Initial revision.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <container name="proxy">
    <config value="false"/>
    <description>
      <text>Container for NETCONF Proxy.</text>
    </description>
    <list name="target-list">
      <key value="target"/>
      <description>
        <text>Statistics of target list information.</text>
      </description>
      <leaf name="target">
        <type name="string">
          <length value="1..127"/>
        </type>
        <description>
          <text>Target name.</text>
        </description>
      </leaf>
      <leaf name="target-id">
        <ext:support-filter value="true"/>
        <type name="string">
          <length value="1..127"/>
        </type>
        <description>
          <text>Target Identifier.</text>
        </description>
      </leaf>
      <leaf name="target-type">
        <ext:support-filter value="true"/>
        <type name="string">
          <length value="1..127"/>
        </type>
        <description>
          <text>The type of the target.</text>
        </description>
      </leaf>
    </list>
  </container>
  <ext:task-name value="netconf"/>
</module>
