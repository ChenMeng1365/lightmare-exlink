  <rpc name="reset-policy-apply-statistics">
    <description>
      <text>To clear traffic policy statistics.</text>
    </description>
    <ext:node-ref value="/qos-l2vpn:qos-l2vpn/qos-l2vpn:policy-statisticss"/>
    <input>
      <leaf name="instance-name">
        <type name="leafref">
          <path value="/l2vpn:l2vpn/l2vpn:instances/l2vpn:instance/l2vpn:name"/>
        </type>
        <description>
          <text>The name of L2VPN instance.</text>
        </description>
      </leaf>
      <leaf name="direction">
        <type name="qos:in-direction-type"/>
        <description>
          <text>Direction.</text>
        </description>
      </leaf>
    </input>
  </rpc>
  <rpc name="reset-policy-apply-statistics-ac-mode">
    <description>
      <text>Clear VSI policy statistics.</text>
    </description>
    <ext:node-ref value="/qos-l2vpn:qos-l2vpn/qos-l2vpn:ac-mode-policy-statisticss"/>
    <input>
      <leaf name="instance-name">
        <type name="leafref">
          <path value="/l2vpn:l2vpn/l2vpn:instances/l2vpn:instance/l2vpn:name"/>
        </type>
        <description>
          <text>The name of L2VPN instance.</text>
        </description>
      </leaf>
      <leaf name="direction">
        <type name="qos:qos-direction-type"/>
        <mandatory value="true"/>
        <description>
          <text>Direction.</text>
        </description>
      </leaf>
    </input>
  </rpc>
  <augment target-node="/l2vpn:l2vpn/l2vpn:instances/l2vpn:instance">
    <description>
      <text>Adds QoS configuration to the instance.</text>
    </description>
    <container name="qos">
      <description>
        <text>Configure QoS application.</text>
      </description>
      <container name="policy-applys">
        <when condition="../../l2vpn:type='vpls' and ../../l2vpn:vpls/l2vpn:work-mode='normal'"/>
        <description>
          <text>List of VPN instance traffic policy network.</text>
        </description>
        <list name="policy-apply">
          <must condition="../../../l2vpn:vpls/l2vpn:p2p-vsi!='true'"/>
          <key value="direction"/>
          <max-elements value="2"/>
          <description>
            <text>Configure the application of a traffic policy.</text>
          </description>
          <leaf name="direction">
            <type name="qos:in-direction-type"/>
            <description>
              <text>Direction.</text>
            </description>
          </leaf>
          <leaf name="policy-name">
            <type name="leafref">
              <path value="/qos:qos/qos:classifier-template/qos:traffic-policys/qos:traffic-policy/qos:name"/>
            </type>
            <mandatory value="true"/>
            <description>
              <text>Policy name.</text>
            </description>
            <ext:operation-exclude value="update">
              <description>
                <text>After the configuration is created, the node cannot be modified.</text>
              </description>
            </ext:operation-exclude>
          </leaf>
        </list>
      </container>
      <container name="ac-mode-policy-applys">
        <when condition="../../l2vpn:type='vpls' and (../../l2vpn:vpls/l2vpn:work-mode='normal' or ../../l2vpn:vpls/l2vpn:work-mode='i-vsi-p2p')"/>
        <description>
          <text>List of traffic policy VSI AC mode.</text>
        </description>
        <list name="ac-mode-policy-apply">
          <key value="direction"/>
          <description>
            <text>Configure traffic policy VSI AC mode.</text>
          </description>
          <leaf name="direction">
            <type name="qos:qos-direction-type"/>
            <description>
              <text>Direction.</text>
            </description>
          </leaf>
          <leaf name="policy-name">
            <type name="leafref">
              <path value="/qos:qos/qos:classifier-template/qos:traffic-policys/qos:traffic-policy/qos:name"/>
            </type>
            <mandatory value="true"/>
            <description>
              <text>Policy name.</text>
            </description>
            <ext:operation-exclude value="update">
              <description>
                <text>After the configuration is created, the node cannot be modified.</text>
              </description>
            </ext:operation-exclude>
          </leaf>
          <leaf name="layer">
            <type name="qos:ac-mode-layer-type"/>
            <description>
              <text>Layer.</text>
            </description>
          </leaf>
        </list>
      </container>
    </container>
  </augment>
</module>
