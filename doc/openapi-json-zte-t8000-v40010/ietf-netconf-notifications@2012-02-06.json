{
    "swagger": "2.0",
    "info": {
        "description": "ietf-netconf-notifications API generated from ietf-netconf-notifications@2012-02-06.yang",
        "version": "1.0.0",
        "title": "ietf-netconf-notifications API"
    },
    "host": "localhost:8080",
    "basePath": "/restconf",
    "schemes": [
        "http"
    ],
    "paths": {
        "/streams/netconf-config-change/": {
            "get": {
                "summary": "Retrieve netconf-config-change by ID",
                "description": "Retrieve operation of resource: netconf-config-change",
                "operationId": "retrieve_netconf-config-change_by_id",
                "produces": [
                    "application/json"
                ],
                "consumes": [
                    "application/json"
                ],
                "responses": {
                    "200": {
                        "description": "Successful operation",
                        "schema": {
                            "description": "Generated when the NETCONF server detects that the\n<running> or <startup> configuration datastore\nhas been changed by a management session.\nThe notification summarizes the edits that\nhave been detected.\n\nThe server MAY choose to also generate this\nnotification while loading a datastore during the\nboot process for the device.",
                            "allOf": [
                                {
                                    "$ref": "#/definitions/changed-by-parms"
                                },
                                {
                                    "properties": {
                                        "datastore": {
                                            "type": "string",
                                            "enum": [
                                                "running",
                                                "startup"
                                            ],
                                            "description": "Indicates which configuration datastore has changed."
                                        },
                                        "edit": {
                                            "description": "An edit record SHOULD be present for each distinct\nedit operation that the server has detected on\nthe target datastore.  This list MAY be omitted\nif the detailed edit operations are not known.\nThe server MAY report entries in this list for\nchanges not made by a NETCONF session (e.g., CLI).",
                                            "type": "array",
                                            "items": {
                                                "properties": {
                                                    "target": {
                                                        "type": "string",
                                                        "description": "Topmost node associated with the configuration change.\nA server SHOULD set this object to the node within\nthe datastore that is being altered.  A server MAY\nset this object to one of the ancestors of the actual\nnode that was changed, or omit this object, if the\nexact node is not known."
                                                    },
                                                    "operation": {
                                                        "type": "string",
                                                        "description": "Type of edit operation performed.\nA server MUST set this object to the NETCONF edit\noperation performed on the target datastore."
                                                    }
                                                }
                                            }
                                        }
                                    }
                                }
                            ]
                        }
                    },
                    "400": {
                        "description": "Internal Error"
                    }
                },
                "schemes": [
                    "ws"
                ]
            }
        },
        "/streams/netconf-capability-change/": {
            "get": {
                "summary": "Retrieve netconf-capability-change by ID",
                "description": "Retrieve operation of resource: netconf-capability-change",
                "operationId": "retrieve_netconf-capability-change_by_id",
                "produces": [
                    "application/json"
                ],
                "consumes": [
                    "application/json"
                ],
                "responses": {
                    "200": {
                        "description": "Successful operation",
                        "schema": {
                            "description": "Generated when the NETCONF server detects that\nthe server capabilities have changed.\nIndicates which capabilities have been added, deleted,\nand/or modified.  The manner in which a server\ncapability is changed is outside the scope of this\ndocument.",
                            "allOf": [
                                {
                                    "$ref": "#/definitions/changed-by-parms"
                                },
                                {
                                    "properties": {
                                        "added-capability": {
                                            "type": "array",
                                            "items": {
                                                "type": "string",
                                                "description": "List of capabilities that have just been added."
                                            }
                                        },
                                        "deleted-capability": {
                                            "type": "array",
                                            "items": {
                                                "type": "string",
                                                "description": "List of capabilities that have just been deleted."
                                            }
                                        },
                                        "modified-capability": {
                                            "type": "array",
                                            "items": {
                                                "type": "string",
                                                "description": "List of capabilities that have just been modified.\nA capability is considered to be modified if the\nbase URI for the capability has not changed, but\none or more of the parameters encoded at the end of\nthe capability URI have changed.\nThe new modified value of the complete URI is returned."
                                            }
                                        }
                                    }
                                }
                            ]
                        }
                    },
                    "400": {
                        "description": "Internal Error"
                    }
                },
                "schemes": [
                    "ws"
                ]
            }
        },
        "/streams/netconf-session-start/": {
            "get": {
                "summary": "Retrieve netconf-session-start by ID",
                "description": "Retrieve operation of resource: netconf-session-start",
                "operationId": "retrieve_netconf-session-start_by_id",
                "produces": [
                    "application/json"
                ],
                "consumes": [
                    "application/json"
                ],
                "responses": {
                    "200": {
                        "description": "Successful operation",
                        "schema": {
                            "description": "Generated when a NETCONF server detects that a\nNETCONF session has started.  A server MAY generate\nthis event for non-NETCONF management sessions.\nIndicates the identity of the user that started\nthe session.",
                            "$ref": "#/definitions/common-session-parms"
                        }
                    },
                    "400": {
                        "description": "Internal Error"
                    }
                },
                "schemes": [
                    "ws"
                ]
            }
        },
        "/streams/netconf-session-end/": {
            "get": {
                "summary": "Retrieve netconf-session-end by ID",
                "description": "Retrieve operation of resource: netconf-session-end",
                "operationId": "retrieve_netconf-session-end_by_id",
                "produces": [
                    "application/json"
                ],
                "consumes": [
                    "application/json"
                ],
                "responses": {
                    "200": {
                        "description": "Successful operation",
                        "schema": {
                            "description": "Generated when a NETCONF server detects that a\nNETCONF session has terminated.\nA server MAY optionally generate this event for\nnon-NETCONF management sessions.  Indicates the\nidentity of the user that owned the session,\nand why the session was terminated.",
                            "allOf": [
                                {
                                    "$ref": "#/definitions/common-session-parms"
                                },
                                {
                                    "properties": {
                                        "killed-by": {
                                            "type": "string",
                                            "description": "The ID of the session that directly caused this session\nto be abnormally terminated.  If this session was abnormally\nterminated by a non-NETCONF session unknown to the server,\nthen this leaf will not be present."
                                        },
                                        "termination-reason": {
                                            "type": "string",
                                            "enum": [
                                                "closed",
                                                "killed",
                                                "dropped",
                                                "timeout",
                                                "bad-hello",
                                                "other"
                                            ],
                                            "description": "Reason the session was terminated."
                                        }
                                    }
                                }
                            ]
                        }
                    },
                    "400": {
                        "description": "Internal Error"
                    }
                },
                "schemes": [
                    "ws"
                ]
            }
        },
        "/streams/netconf-confirmed-commit/": {
            "get": {
                "summary": "Retrieve netconf-confirmed-commit by ID",
                "description": "Retrieve operation of resource: netconf-confirmed-commit",
                "operationId": "retrieve_netconf-confirmed-commit_by_id",
                "produces": [
                    "application/json"
                ],
                "consumes": [
                    "application/json"
                ],
                "responses": {
                    "200": {
                        "description": "Successful operation",
                        "schema": {
                            "description": "Generated when a NETCONF server detects that a\nconfirmed-commit event has occurred.  Indicates the event\nand the current state of the confirmed-commit procedure\nin progress.",
                            "allOf": [
                                {
                                    "$ref": "#/definitions/common-session-parms"
                                },
                                {
                                    "properties": {
                                        "confirm-event": {
                                            "type": "string",
                                            "enum": [
                                                "start",
                                                "cancel",
                                                "timeout",
                                                "extend",
                                                "complete"
                                            ],
                                            "description": "Indicates the event that caused the notification."
                                        },
                                        "timeout": {
                                            "type": "string",
                                            "description": "The configured timeout value if the event type\nis 'start' or 'extend'.  This value represents\nthe approximate number of seconds from the event\ntime when the 'timeout' event might occur."
                                        }
                                    }
                                }
                            ]
                        }
                    },
                    "400": {
                        "description": "Internal Error"
                    }
                },
                "schemes": [
                    "ws"
                ]
            }
        }
    },
    "definitions": {
        "common-session-parms": {
            "description": "Common session parameters to identify a\nmanagement session.",
            "properties": {
                "username": {
                    "type": "string",
                    "description": "Name of the user for the session."
                },
                "session-id": {
                    "type": "string",
                    "description": "Identifier of the session.\nA NETCONF session MUST be identified by a non-zero value.\nA non-NETCONF session MAY be identified by the value zero."
                },
                "source-host": {
                    "type": "string",
                    "description": "Address of the remote host for the session."
                }
            }
        },
        "changed-by-parms": {
            "description": "Common parameters to identify the source\nof a change event, such as a configuration\nor capability change.",
            "properties": {
                "changed-by": {
                    "description": "Indicates the source of the change.\nIf caused by internal action, then the\nempty leaf 'server' will be present.\nIf caused by a management session, then\nthe name, remote host address, and session ID\nof the session that made the change will be reported.",
                    "properties": {
                        "server-or-user": {
                            "properties": {
                                "server": {
                                    "properties": {
                                        "server": {
                                            "type": "string",
                                            "description": "If present, the change was caused\nby the server."
                                        }
                                    }
                                },
                                "by-user": {
                                    "$ref": "#/definitions/common-session-parms"
                                }
                            }
                        }
                    }
                }
            }
        }
    }
}