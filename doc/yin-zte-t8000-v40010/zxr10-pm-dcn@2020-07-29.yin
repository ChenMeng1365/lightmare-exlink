  <container name="operation">
    <config value="false"/>
    <container name="dcn">
      <description>
        <text>Configure DCN</text>
      </description>
      <container name="dcn-network-mode">
        <description>
          <text>Configure dcn network mode</text>
        </description>
        <leaf name="network-mode">
          <description>
            <text>Configure dcn network mode</text>
          </description>
          <type name="enumeration">
            <enum name="eth-p2p">
              <value value="1"/>
              <description>
                <text>Configure dcn mode to eth-p2p</text>
              </description>
            </enum>
            <enum name="eth-p2p-auto">
              <value value="3"/>
              <description>
                <text>Configure dcn mode to auto eth-p2p</text>
              </description>
            </enum>
            <enum name="pppoe-auto">
              <value value="4"/>
              <description>
                <text>Configure dcn mode to auto pppoe</text>
              </description>
            </enum>
            <enum name="eth-p2mp-auto">
              <value value="5"/>
              <description>
                <text>Configure dcn mode to auto eth-p2mp</text>
              </description>
            </enum>
          </type>
        </leaf>
      </container>
      <container name="dcn-function-status">
        <description>
          <text>Enable or disable DCN function</text>
        </description>
        <leaf name="function-status">
          <description>
            <text>Enable or disable DCN function</text>
          </description>
          <type name="enumeration">
            <enum name="enable">
              <value value="0"/>
              <description>
                <text>DCN enable</text>
              </description>
            </enum>
            <enum name="disable">
              <value value="1"/>
              <description>
                <text>DCN disable</text>
              </description>
            </enum>
          </type>
        </leaf>
      </container>
      <container name="dcn-ipv6-function-status">
        <description>
          <text>Enable or disable dcn IPv6 function</text>
        </description>
        <leaf name="ipv6-function-status">
          <description>
            <text>Enable or disable dcn IPv6 function</text>
          </description>
          <type name="enumeration">
            <enum name="ipv6-enable">
              <value value="1"/>
              <description>
                <text>DCN IPV6 enable</text>
              </description>
            </enum>
            <enum name="ipv6-disable">
              <value value="0"/>
              <description>
                <text>DCN IPV6 disable</text>
              </description>
            </enum>
          </type>
        </leaf>
        <ncd:revise-date date="2019-08-08"/>
      </container>
      <container name="dcn-mngip">
        <description>
          <text>Configure DCN network Mng IP address</text>
        </description>
