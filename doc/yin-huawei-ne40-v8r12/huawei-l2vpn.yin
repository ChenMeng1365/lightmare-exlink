      <container name="local-label-blocks">
        <config value="false"/>
        <description>
          <text>Operational data of local label blocks.</text>
        </description>
        <uses name="bgp-local-label-block-grp"/>
      </container>
      <container name="remote-sites">
        <config value="false"/>
        <description>
          <text>List of BGP multi-homing remote site information.</text>
        </description>
        <list name="remote-site">
          <key value="site-id route-distinguisher offset"/>
          <description>
            <text>Operational data of BGP multi-homing remote site information.</text>
          </description>
          <leaf name="site-id">
            <type name="uint32"/>
            <description>
              <text>BGP multi-homing remote site ID.</text>
            </description>
          </leaf>
          <leaf name="route-distinguisher">
            <type name="string">
              <length value="3..21"/>
            </type>
            <description>
              <text>Route distinguisher.</text>
            </description>
          </leaf>
          <leaf name="offset">
            <type name="uint16"/>
            <description>
              <text>BGP site offset.</text>
            </description>
          </leaf>
          <leaf name="peer-ip">
            <type name="inet:ipv4-address-no-zone"/>
            <description>
              <text>The IP address of peer.</text>
            </description>
          </leaf>
          <leaf name="mtu">
            <type name="uint32"/>
            <description>
              <text>Maximum transmission unit.</text>
            </description>
          </leaf>
          <leaf name="encapsulation-type">
            <type name="vpls-bgp-encapsulation-type"/>
            <description>
              <text>The encapsulation type of remote PW.</text>
