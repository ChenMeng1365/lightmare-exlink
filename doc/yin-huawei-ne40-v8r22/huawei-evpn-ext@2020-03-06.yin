  <typedef name="lcs-active-type">
    <type name="enumeration">
      <enum name="deactive">
        <value value="0"/>
        <description>
          <text>Specify the license active type as deactivate.</text>
        </description>
      </enum>
      <enum name="active">
        <value value="1"/>
        <description>
          <text>Specify the license active type as activate.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>License active type.</text>
    </description>
  </typedef>
  <typedef name="lcs-active-status-type">
    <type name="enumeration">
      <enum name="unallocated">
        <value value="0"/>
        <description>
          <text>Specify the license active status as unallocated.</text>
        </description>
      </enum>
      <enum name="allocated">
        <value value="1"/>
        <description>
          <text>Specify the license active status as allocated.</text>
        </description>
      </enum>
      <enum name="activated">
