        <text>List of the source port number of a RADIUS server.</text>
      </description>
      <list name="server-port">
        <key value="server-type ip-type port"/>
        <config value="false"/>
        <description>
          <text>Statistics of the source port number of a RADIUS server.</text>
        </description>
        <leaf name="server-type">
          <type name="bras-radius-server-type"/>
          <config value="false"/>
          <description>
            <text>Type of Radius Server.</text>
          </description>
        </leaf>
        <leaf name="ip-type">
          <type name="enumeration">
            <enum name="ipv4">
              <value value="0"/>
              <description>
                <text>IPv4 server.</text>
              </description>
            </enum>
            <enum name="ipv6">
              <value value="1"/>
              <description>
                <text>IPv6 server.</text>
              </description>
            </enum>
          </type>
          <config value="false"/>
          <description>
            <text>Filter port numbers based on IP.</text>
          </description>
        </leaf>
        <leaf name="port">
          <type name="uint16">
            <range value="1..65535"/>
