<?xml version="1.0" encoding="UTF-8"?>
<module name="huawei-mpls-te-auto-tunnel"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:mpls-te-auto-tunnel="urn:huawei:yang:huawei-mpls-te-auto-tunnel"
        xmlns:inet="urn:ietf:params:xml:ns:yang:ietf-inet-types"
        xmlns:pub-type="urn:huawei:yang:huawei-pub-type"
        xmlns:mpls="urn:huawei:yang:huawei-mpls"
        xmlns:mpls-te="urn:huawei:yang:huawei-mpls-te"
        xmlns:ext="urn:huawei:yang:huawei-extension">
  <namespace uri="urn:huawei:yang:huawei-mpls-te-auto-tunnel"/>
  <prefix value="mpls-te-auto-tunnel"/>
  <import module="ietf-inet-types">
    <prefix value="inet"/>
  </import>
  <import module="huawei-pub-type">
    <prefix value="pub-type"/>
  </import>
  <import module="huawei-mpls">
    <prefix value="mpls"/>
  </import>
  <import module="huawei-mpls-te">
    <prefix value="mpls-te"/>
  </import>
  <import module="huawei-extension">
    <prefix value="ext"/>
  </import>
  <organization>
    <text>Huawei Technologies Co., Ltd.</text>
  </organization>
  <contact>
    <text>Huawei Industrial Base
Bantian, Longgang
Shenzhen 518129
People's Republic of China
Website: http://www.huawei.com
Email: support@huawei.com</text>
  </contact>
  <description>
    <text>Specifies the multi-protocol label switching protocol.</text>
  </description>
  <revision date="2021-05-21">
    <description>
      <text>Initial revision.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <ext:task-name value="mpls-te"/>
  <augment target-node="/mpls:mpls/mpls-te:te">
    <description>
      <text>MPLS TE auto tunnel information augment to module MPLS TE.</text>
    </description>
    <container name="rsvp-te-auto-tunnels">
      <config value="false"/>
      <description>
        <text>List of auto-tunnels for p2p rsvp-te.</text>
      </description>
      <list name="rsvp-te-auto-tunnel">
        <key value="tunnel-name"/>
        <description>
          <text>Statistics of rsvp-te auto tunnel.</text>
        </description>
        <leaf name="tunnel-name">
          <type name="string">
            <length value="1..64"/>
          </type>
          <description>
            <text>The name of rsvp-te auto tunnel.</text>
          </description>
        </leaf>
        <leaf name="ingress-lsr-id">
          <type name="inet:ipv4-address-no-zone"/>
          <description>
            <text>Ingress LSR ID.</text>
          </description>
        </leaf>
        <leaf name="egress-lsr-id">
          <type name="inet:ipv4-address-no-zone"/>
          <description>
            <text>Egress LSR ID.</text>
          </description>
        </leaf>
        <leaf name="session-id">
          <type name="uint16">
            <range value="32769..65535"/>
          </type>
          <description>
            <text>Tunnel ID.</text>
          </description>
        </leaf>
        <leaf name="state">
          <type name="mpls-te:tunnel-state"/>
          <description>
            <text>Tunnel status.</text>
          </description>
        </leaf>
        <leaf name="tunnel-type">
          <type name="mpls-te:auto-tunnel-type"/>
          <description>
            <text>Tunnel type.</text>
          </description>
        </leaf>
        <leaf name="ip-prefix-name">
          <type name="string">
            <length value="0..169"/>
          </type>
          <description>
            <text>The name of ip prefix.</text>
          </description>
        </leaf>
        <leaf name="template-name">
          <type name="string">
            <length value="0..31"/>
          </type>
          <description>
            <text>The name of template.</text>
          </description>
        </leaf>
        <leaf name="working-lsp-type">
          <type name="mpls-te:lsp-path-type"/>
          <description>
            <text>LSP type.</text>
          </description>
        </leaf>
        <leaf name="working-lsp-id">
          <type name="uint32"/>
          <description>
            <text>The working LSP ID of a Tunnel.</text>
          </description>
        </leaf>
        <leaf name="entropy-label-enable">
          <type name="boolean"/>
          <description>
            <text>Enable/disable auto tunnel entropy label.</text>
          </description>
        </leaf>
        <container name="lsps">
          <config value="false"/>
          <description>
            <text>List of LSPs of a tunnel.</text>
          </description>
          <list name="lsp">
            <key value="ingress-lsr-id egress-lsr-id session-id lsp-id"/>
            <max-elements value="4"/>
            <description>
              <text>Statistics of LSP of a tunnel.</text>
            </description>
            <leaf name="ingress-lsr-id">
              <type name="inet:ipv4-address-no-zone"/>
              <description>
                <text>Ingress LSR ID.</text>
              </description>
            </leaf>
            <leaf name="egress-lsr-id">
              <type name="inet:ipv4-address-no-zone"/>
              <description>
                <text>Egress LSR ID.</text>
              </description>
            </leaf>
            <leaf name="session-id">
              <type name="uint16">
                <range value="0..65535"/>
              </type>
              <description>
                <text>Tunnel ID.</text>
              </description>
            </leaf>
            <leaf name="lsp-id">
              <type name="uint16">
                <range value="0..65535"/>
              </type>
              <description>
                <text>LSP ID.</text>
              </description>
            </leaf>
            <leaf name="setup-priority">
              <type name="uint8">
                <range value="0..7"/>
              </type>
              <description>
                <text>Setup priority of a LSP.</text>
              </description>
            </leaf>
            <leaf name="hold-priority">
              <type name="uint8">
                <range value="0..7"/>
              </type>
              <description>
                <text>Hold priority of a LSP.</text>
              </description>
            </leaf>
            <leaf name="record-route">
              <type name="mpls-te:record-route-mode"/>
              <description>
                <text>Route record mode.</text>
              </description>
            </leaf>
            <leaf name="dste-mode">
              <type name="boolean"/>
              <description>
                <text>Whether DS-TE is used for LSP bandwidth constraints.</text>
              </description>
            </leaf>
            <leaf name="reserved-bandwidth">
              <type name="uint32">
                <range value="0..4000000000"/>
              </type>
              <units name="kbit/s"/>
              <description>
                <text>Reserved bandwidth for non-DS-TE.</text>
              </description>
            </leaf>
            <leaf name="explicit-path-name">
              <type name="string">
                <length value="1..128"/>
              </type>
              <description>
                <text>Explicit path name of a LSP. The name is case-insensitive.</text>
              </description>
            </leaf>
            <leaf name="include-all">
              <type name="pub-type:hex-binary"/>
              <description>
                <text>Specifies the Include-all (management group attribute) of a LSP.</text>
              </description>
            </leaf>
            <leaf name="include-any">
              <type name="pub-type:hex-binary"/>
              <description>
                <text>Specifies the Include-any (management group attribute) of a LSP.</text>
              </description>
            </leaf>
            <leaf name="exclude-any">
              <type name="pub-type:hex-binary"/>
              <description>
                <text>Specifies the Exclude-any (management group attribute) of a LSP.</text>
              </description>
            </leaf>
            <leaf name="local-protect-enable">
              <type name="boolean"/>
              <description>
                <text>Specifies the enabling or disabling state of FRR for a LSP.</text>
              </description>
            </leaf>
            <leaf name="local-protect-inuse">
              <type name="mpls-te:fast-reroute-inuse-state"/>
              <description>
                <text>Specifies the FRR protection state of this LSP.</text>
              </description>
            </leaf>
            <leaf name="bypass-tunnel-name">
              <type name="string">
                <length value="1..256"/>
              </type>
              <description>
                <text>Specifies the name of the bypass tunnel that protects the LSP. The name is case-insensitive.</text>
              </description>
            </leaf>
            <leaf name="permit-bandwidth-share">
              <type name="boolean"/>
              <description>
                <text>Indicates whether the LSP permits bandwidth sharing.</text>
              </description>
            </leaf>
            <leaf name="lsp-mtu">
              <type name="uint32">
                <range value="0..32000000"/>
              </type>
              <description>
                <text>Specifies a LSP MTU.</text>
              </description>
            </leaf>
            <leaf name="oper-status">
              <type name="mpls-te:interface-status"/>
              <description>
                <text>Operation status of a LSP.</text>
              </description>
            </leaf>
            <leaf name="xcindex">
              <type name="uint32">
                <range value="0..4294967295"/>
              </type>
              <description>
                <text>Cross-connect index.</text>
              </description>
            </leaf>
            <container name="ar-hops">
              <description>
                <text>List of actual path of a LSP.</text>
              </description>
              <list name="ar-hop">
                <key value="index"/>
                <description>
                  <text>Statistics of actual hop of a LSP.</text>
                </description>
                <leaf name="index">
                  <type name="uint32"/>
                  <description>
                    <text>Actual hop index.</text>
                  </description>
                </leaf>
                <leaf name="incomming-type">
                  <type name="boolean"/>
                  <description>
                    <text>Whether the hop is an inbound interface.</text>
                  </description>
                </leaf>
                <leaf name="ip-address">
                  <type name="inet:ipv4-address-no-zone"/>
                  <description>
                    <text>IP address of the actual hop.</text>
                  </description>
                </leaf>
                <leaf name="label">
                  <type name="uint32"/>
                  <description>
                    <text>Label of the actual hop.</text>
                  </description>
                </leaf>
                <leaf name="local-protect-inuse">
                  <type name="boolean"/>
                  <description>
                    <text>FRR protection state.</text>
                  </description>
                </leaf>
                <leaf name="local-protect-type">
                  <type name="mpls-te:local-protect-type"/>
                  <description>
                    <text>FRR protection type.</text>
                  </description>
                </leaf>
                <leaf name="bandwidth-protection">
                  <type name="boolean"/>
                  <description>
                    <text>FRR bandwidth protection.</text>
                  </description>
                </leaf>
              </list>
            </container>
            <container name="c-hops">
              <config value="false"/>
              <description>
                <text>List of path calculated by CSPF according to LSP constraints.</text>
              </description>
              <list name="c-hop">
                <key value="path-index index"/>
                <description>
                  <text>Statistics of path calculated by CSPF according to LSP constraints.</text>
                </description>
                <leaf name="path-index">
                  <type name="uint32"/>
                  <description>
                    <text>Path index calculated by CSPF.</text>
                  </description>
                </leaf>
                <leaf name="index">
                  <type name="uint32"/>
                  <description>
                    <text>Hop index calculated by CSPF.</text>
                  </description>
                </leaf>
                <leaf name="include">
                  <type name="boolean"/>
                  <description>
                    <text>Hop include or exclude.</text>
                  </description>
                </leaf>
                <leaf name="hop-type">
                  <type name="mpls-te:hop-strict-type"/>
                  <description>
                    <text>Hop type calculated by CSPF. The available options are strict and loose.</text>
                  </description>
                </leaf>
                <leaf name="address-type">
                  <type name="mpls-te:hop-address-type"/>
                  <description>
                    <text>Address type calculated by CSPF. The available options are IPv4 and IPv6.</text>
                  </description>
                </leaf>
                <leaf name="ip-address">
                  <type name="inet:ipv4-address-no-zone"/>
                  <description>
                    <text>IP address calculated by CSPF.</text>
                  </description>
                </leaf>
              </list>
            </container>
          </list>
        </container>
      </list>
    </container>
    <container name="p2mp-te-auto-tunnels">
      <config value="false"/>
      <description>
        <text>List of auto-tunnels for p2mp te.</text>
      </description>
      <list name="p2mp-te-auto-tunnel">
        <key value="tunnel-name"/>
        <description>
          <text>Statistics of p2mp te auto tunnel.</text>
        </description>
        <leaf name="tunnel-name">
          <type name="string">
            <length value="1..63"/>
          </type>
          <description>
            <text>The name of rsvp-te auto tunnel.</text>
          </description>
        </leaf>
        <leaf name="template-name">
          <type name="string">
            <length value="0..31"/>
          </type>
          <description>
            <text>The name of template.</text>
          </description>
        </leaf>
        <leaf name="state">
          <type name="mpls-te:tunnel-state"/>
          <description>
            <text>Tunnel status.</text>
          </description>
        </leaf>
        <leaf name="ingress-lsr-id">
          <type name="inet:ipv4-address-no-zone"/>
          <description>
            <text>Ingress LSR ID.</text>
          </description>
        </leaf>
        <leaf name="session-id">
          <type name="uint16"/>
          <description>
            <text>Tunnel ID.</text>
          </description>
        </leaf>
        <leaf name="p2mp-id">
          <type name="inet:ipv4-address-no-zone"/>
          <description>
            <text>P2MP ID.</text>
          </description>
        </leaf>
        <leaf name="admin-status">
          <type name="mpls-te:interface-status"/>
          <description>
            <text>Interface administrative status. The available options are Up and Down.</text>
          </description>
        </leaf>
        <leaf name="oper-status">
          <type name="mpls-te:interface-status"/>
          <description>
            <text>Interface operation status. The available options are Up and Down.</text>
          </description>
        </leaf>
        <container name="lsps">
          <config value="false"/>
          <description>
            <text>List of LSPs of a tunnel.</text>
          </description>
          <list name="lsp">
            <key value="lsp-id"/>
            <max-elements value="2"/>
            <description>
              <text>Statistics of LSP of a tunnel.</text>
            </description>
            <leaf name="lsp-id">
              <type name="uint16">
                <range value="0..65535"/>
              </type>
              <description>
                <text>LSP ID.</text>
              </description>
            </leaf>
            <leaf name="modify-lsp">
              <type name="boolean"/>
              <description>
                <text>Whether the LSP is a modifying LSP.</text>
              </description>
            </leaf>
            <leaf name="setup-priority">
              <type name="uint8">
                <range value="0..7"/>
              </type>
              <description>
                <text>Setup priority of a LSP.</text>
              </description>
            </leaf>
            <leaf name="hold-priority">
              <type name="uint8">
                <range value="0..7"/>
              </type>
              <description>
                <text>Hold priority of a LSP.</text>
              </description>
            </leaf>
            <leaf name="record-route">
              <type name="mpls-te:record-route-mode"/>
              <description>
                <text>Route record mode.</text>
              </description>
            </leaf>
            <leaf name="reserved-bandwidth">
              <type name="uint32">
                <range value="0..32000000"/>
              </type>
              <units name="kbit/s"/>
              <description>
                <text>Reserved bandwidth for non-DS-TE.</text>
              </description>
            </leaf>
            <leaf name="leaf-list-name">
              <type name="string">
                <length value="1..32"/>
              </type>
              <description>
                <text>Leaf list name of a LSP.</text>
              </description>
            </leaf>
            <leaf name="include-all">
              <type name="pub-type:hex-binary"/>
              <description>
                <text>Specifies the Include-all (management group attribute) of a LSP.</text>
              </description>
            </leaf>
            <leaf name="include-any">
              <type name="pub-type:hex-binary"/>
              <description>
                <text>Specifies the Include-any (management group attribute) of a LSP.</text>
              </description>
            </leaf>
            <leaf name="exclude-any">
              <type name="pub-type:hex-binary"/>
              <description>
                <text>Specifies the Exclude-any (management group attribute) of a LSP.</text>
              </description>
            </leaf>
            <leaf name="local-protect-enable">
              <type name="boolean"/>
              <description>
                <text>Specifies the enabling or disabling state of FRR for a LSP.</text>
              </description>
            </leaf>
            <leaf name="permit-bandwidth-share">
              <type name="boolean"/>
              <description>
                <text>Indicates whether the LSP permits bandwidth sharing.</text>
              </description>
            </leaf>
            <leaf name="oper-status">
              <type name="mpls-te:mpls-te-lsp-status-type"/>
              <description>
                <text>The state of LSP.</text>
              </description>
            </leaf>
            <container name="p2mp-leafs">
              <description>
                <text>List of p2mp leaf.</text>
              </description>
              <list name="p2mp-leaf">
                <key value="leaf-address sub-group-id"/>
                <max-elements value="1024"/>
                <description>
                  <text>Statistics of p2mp leaf.</text>
                </description>
                <leaf name="leaf-address">
                  <type name="inet:ipv4-address-no-zone"/>
                  <description>
                    <text>IP address of the leaf.</text>
                  </description>
                </leaf>
                <leaf name="sub-group-id">
                  <type name="uint16">
                    <range value="0..65535"/>
                  </type>
                  <description>
                    <text>Sub-group-id of P2MP LSP.</text>
                  </description>
                </leaf>
                <leaf name="leaf-status">
                  <type name="mpls-te:p2mp-leaf-state"/>
                  <description>
                    <text>Leaf status.</text>
                  </description>
                </leaf>
                <leaf name="modify-lsp">
                  <type name="boolean"/>
                  <description>
                    <text>Whether the LSP is a modifying LSP.</text>
                  </description>
                </leaf>
                <leaf name="explicit-path-name">
                  <type name="string">
                    <length value="1..128"/>
                  </type>
                  <description>
                    <text>Explicit path name of a LSP. The name is case-insensitive.</text>
                  </description>
                </leaf>
              </list>
            </container>
          </list>
        </container>
      </list>
    </container>
    <container name="sr-te-auto-tunnels">
      <config value="false"/>
      <description>
        <text>List of auto-tunnels for sr-te.</text>
      </description>
      <list name="sr-te-auto-tunnel">
        <key value="tunnel-name"/>
        <description>
          <text>Statistics of sr-te auto tunnel.</text>
        </description>
        <leaf name="tunnel-name">
          <type name="string">
            <length value="1..64"/>
          </type>
          <description>
            <text>The name of sr-te auto tunnel.</text>
          </description>
        </leaf>
        <leaf name="ingress-lsr-id">
          <type name="inet:ipv4-address-no-zone"/>
          <description>
            <text>Ingress LSR ID.</text>
          </description>
        </leaf>
        <leaf name="egress-lsr-id">
          <type name="inet:ipv4-address-no-zone"/>
          <description>
            <text>Egress LSR ID.</text>
          </description>
        </leaf>
        <leaf name="session-id">
          <type name="uint16">
            <range value="32769..65535"/>
          </type>
          <description>
            <text>Tunnel ID.</text>
          </description>
        </leaf>
        <leaf name="state">
          <type name="mpls-te:tunnel-state"/>
          <description>
            <text>Tunnel status.</text>
          </description>
        </leaf>
        <leaf name="tunnel-type">
          <type name="mpls-te:auto-tunnel-type"/>
          <description>
            <text>Tunnel type.</text>
          </description>
        </leaf>
        <leaf name="working-lsp-type">
          <type name="mpls-te:lsp-path-type"/>
          <description>
            <text>LSP type.</text>
          </description>
        </leaf>
        <leaf name="working-lsp-id">
          <type name="uint32"/>
          <description>
            <text>The working LSP ID of a Tunnel.</text>
          </description>
        </leaf>
      </list>
    </container>
  </augment>
</module>
