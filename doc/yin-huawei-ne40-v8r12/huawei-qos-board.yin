  </augment>
  <augment target-node="/devm:devm/devm:lpu-boards/devm:lpu-board">
    <description>
      <text>Adds QoS configuration to the board.</text>
    </description>
    <container name="qos">
      <description>
        <text>Configure QoS application.</text>
      </description>
      <container name="traffic-policy-matchs">
