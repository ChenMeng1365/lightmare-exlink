  <typedef name="alarm-level">
    <type name="enumeration">
      <enum name="emergencies">
        <value value="1"/>
        <description>
          <text>Level of emergency.</text>
        </description>
      </enum>
      <enum name="alerts">
        <value value="2"/>
        <description>
          <text>Level of alert.</text>
        </description>
      </enum>
      <enum name="critical">
        <value value="3"/>
        <description>
          <text>Level of  critical.</text>
        </description>
      </enum>
      <enum name="errors">
        <value value="4"/>
        <description>
          <text>Level of error.</text>
        </description>
      </enum>
      <enum name="warnings">
        <value value="5"/>
        <description>
          <text>Level of warning.</text>
        </description>
      </enum>
      <enum name="notifications">
        <value value="6"/>
        <description>
          <text>Level of notification.</text>
        </description>
      </enum>
      <enum name="informational">
        <value value="7"/>
        <description>
          <text>Level of information.</text>
        </description>
      </enum>
      <enum name="debugging">
        <value value="8"/>
        <description>
          <text>Level of debug.</text>
        </description>
      </enum>
    </type>
    <default value="notifications"/>
    <description>
      <text>Alarm level.</text>
    </description>
  </typedef>
