  }
  revision 2020-07-20 {
    description 'Add leaf "bfd-flag" of container "ip-route".'+"\n"
              + 'Add leaf "bfd-flag" of container "dcn-ipv6-static-route".'+"\n"
              + 'Add leaf "bfd-flag" of container "query-ip-routes".'+"\n"
              + 'Add leaf "bfd-flag" of container "query-dcn-ipv6-static-routes'
