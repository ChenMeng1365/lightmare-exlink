{
    "swagger": "2.0",
    "info": {
        "description": "openconfig-system-terminal API generated from openconfig-system-terminal@2017-09-18.yang",
        "version": "1.0.0",
        "title": "openconfig-system-terminal API"
    },
    "host": "localhost:8080",
    "basePath": "/restconf",
    "schemes": [
        "http"
    ],
    "paths": {},
    "definitions": {
        "system-terminal-common-config": {
            "description": "Common configuration data for terminal services",
            "properties": {
                "timeout": {
                    "type": "string",
                    "description": "Set the idle timeout in seconds on terminal connections to\nthe system for the protocol."
                },
                "rate-limit": {
                    "type": "string",
                    "description": "Set a limit on the number of connection attempts per\nminute to the system for the protocol."
                },
                "session-limit": {
                    "type": "string",
                    "description": "Set a limit on the number of simultaneous active terminal\nsessions to the system for the protocol (e.g., ssh,\ntelnet, ...) "
                }
            }
        },
        "system-terminal-common-state": {
            "description": "Common operational state data for terminal services"
        },
        "system-terminal-common-top": {
            "description": "Top-level grouping for common terminal service data",
            "properties": {
                "terminal-servers": {
                    "description": "Top-level container for terminal services",
                    "properties": {
                        "config": {
                            "description": "Configuration data for terminal services",
                            "$ref": "#/definitions/system-terminal-common-config"
                        },
                        "state": {
                            "description": "Operational state data ",
                            "$ref": "#/definitions/system-terminal-common-state"
                        }
                    }
                }
            }
        },
        "system-ssh-server-config": {
            "description": "Configuration data for system ssh configuration",
            "allOf": [
                {
                    "$ref": "#/definitions/system-terminal-common-config"
                },
                {
                    "properties": {
                        "enable": {
                            "type": "boolean",
                            "description": "Enables the ssh server.  The ssh server is enabled by\ndefault."
                        },
                        "protocol-version": {
                            "type": "string",
                            "enum": [
                                "V2",
                                "V1",
                                "V1_V2"
                            ],
                            "description": "Set the protocol version for SSH connections to the system"
                        }
                    }
                }
            ]
        },
        "system-ssh-server-state": {
            "description": "Operational state data for ssh server"
        },
        "system-ssh-server-top": {
            "description": "Top-level grouping for ssh server data",
            "properties": {
                "ssh-server": {
                    "description": "Top-level container for ssh server",
                    "properties": {
                        "config": {
                            "description": "Configuration data for the system ssh server",
                            "$ref": "#/definitions/system-ssh-server-config"
                        },
                        "state": {
                            "description": "Operational state data for the system ssh server",
                            "$ref": "#/definitions/system-ssh-server-state"
                        }
                    }
                }
            }
        },
        "system-telnet-server-config": {
            "description": "Configuration data for telnet server",
            "allOf": [
                {
                    "$ref": "#/definitions/system-terminal-common-config"
                },
                {
                    "properties": {
                        "enable": {
                            "type": "boolean",
                            "description": "Enables the telnet server.  Telnet is disabled by\ndefault"
                        }
                    }
                }
            ]
        },
        "system-telnet-server-state": {
            "description": "Operational state data for telnet server"
        },
        "system-telnet-server-top": {
            "description": "Top-level grouping for telnet server ",
            "properties": {
                "telnet-server": {
                    "description": "Top-level container for telnet terminal servers",
                    "properties": {
                        "config": {
                            "description": "Configuration data for telnet",
                            "$ref": "#/definitions/system-telnet-server-config"
                        },
                        "state": {
                            "description": "Operational state data for telnet",
                            "$ref": "#/definitions/system-telnet-server-state"
                        }
                    }
                }
            }
        }
    }
