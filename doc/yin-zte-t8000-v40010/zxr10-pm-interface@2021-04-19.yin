  <feature name="optical-module-config"/>
  <feature name="flow-control-threshold"/>
  <container name="configuration">
    <container name="pm-if">
      <container name="eths">
        <list name="eth">
          <description>
            <text>Ethernet interface mode.</text>
          </description>
          <key value="if-name"/>
          <leaf name="if-name">
            <description>
              <text>Interface name.</text>
            </description>
            <type name="string"/>
          </leaf>
          <leaf name="holdtime">
            <description>
              <text>Configures the port hold-time.</text>
            </description>
            <type name="uint16">
              <range value="0..3600">
                <description>
                  <text>Range: 0-3600.</text>
                </description>
              </range>
            </type>
            <units name="second"/>
            <default value="120"/>
          </leaf>
          <leaf name="negotiation">
            <description>
              <text>Configures the port negotiation mode.</text>
            </description>
            <type name="enumeration">
              <enum name="negotiation-force">
                <value value="1"/>
                <description>
                  <text>Force negotiation mode.</text>
                </description>
              </enum>
              <enum name="negotiation-auto">
                <value value="2"/>
                <description>
                  <text>Auto negotiation mode.</text>
                </description>
              </enum>
            </type>
            <default value="negotiation-auto"/>
          </leaf>
          <leaf name="speed">
            <description>
              <text>Configures the rate of a port.</text>
            </description>
            <type name="enumeration">
              <enum name="speed-10M">
                <value value="1"/>
                <description>
                  <text>Port speed is 10M.</text>
                </description>
              </enum>
              <enum name="speed-100M">
                <value value="2"/>
                <description>
                  <text>Port speed is 100M.</text>
                </description>
              </enum>
              <enum name="speed-1G">
                <value value="3"/>
                <description>
                  <text>Port speed is 1G.</text>
                </description>
              </enum>
              <enum name="speed-10G">
                <value value="4"/>
                <description>
                  <text>Port speed is 10G.</text>
                </description>
              </enum>
              <enum name="speed-25G">
                <value value="10"/>
                <description>
                  <text>Port speed is 25G.</text>
                </description>
              </enum>
              <enum name="speed-40G">
                <value value="5"/>
                <description>
                  <text>Port speed is 40G.</text>
                </description>
              </enum>
              <enum name="speed-50G">
                <value value="11"/>
                <description>
                  <text>Port speed is 50G.</text>
                </description>
              </enum>
              <enum name="speed-100G">
                <value value="6"/>
                <description>
                  <text>Port speed is 100G.</text>
                </description>
              </enum>
              <enum name="speed-200G">
                <value value="12"/>
                <description>
                  <text>Port speed is 200G.</text>
                </description>
              </enum>
              <enum name="speed-400G">
                <value value="13"/>
                <description>
                  <text>Port speed is 400G.</text>
                </description>
              </enum>
              <enum name="speed-auto">
                <value value="15"/>
                <description>
                  <text>Port speed is configured to auto, the real speed depend on optical module.</text>
                </description>
              </enum>
            </type>
            <default value="speed-1G"/>
          </leaf>
          <leaf name="duplex">
            <description>
              <text>Configures the Ethernet working mode.</text>
            </description>
            <type name="enumeration">
              <enum name="duplex-full">
                <value value="1"/>
                <description>
                  <text>Full duplex mode.</text>
                </description>
              </enum>
              <enum name="duplex-half">
                <value value="2"/>
                <description>
                  <text>Half duplex mode.</text>
                </description>
              </enum>
            </type>
            <default value="duplex-full"/>
          </leaf>
          <leaf name="loopback">
            <description>
              <text>Configures the loopback method of a port.</text>
            </description>
            <type name="enumeration">
              <enum name="cancel">
                <value value="0"/>
                <description>
                  <text>Cancel loopback mode.</text>
                </description>
              </enum>
              <enum name="outer">
                <value value="1"/>
                <description>
                  <text>Line loopback mode.</text>
                </description>
              </enum>
              <enum name="inner">
                <value value="2"/>
                <description>
                  <text>Inner loopback mode.</text>
                </description>
              </enum>
              <enum name="external">
                <value value="3"/>
                <description>
                  <text>External loopback mode.</text>
                </description>
              </enum>
            </type>
            <default value="cancel"/>
          </leaf>
          <leaf name="mdimode">
            <description>
              <text>Configures the MDI/MDI-X function.</text>
            </description>
            <type name="enumeration">
              <enum name="mdimode-mdix">
                <value value="1"/>
                <description>
                  <text>Switch port to straight-through mode.</text>
                </description>
              </enum>
              <enum name="mdimode-mdi">
                <value value="2"/>
                <description>
                  <text>Switch port to crossover mode.</text>
                </description>
              </enum>
              <enum name="mdimode-auto">
                <value value="3"/>
                <description>
                  <text>Switch port to auto-cross mode.</text>
                </description>
              </enum>
            </type>
            <default value="mdimode-auto"/>
          </leaf>
          <leaf name="pemode">
            <description>
              <text>Configures the copper-fiber attribute of a port.</text>
            </description>
            <type name="enumeration">
              <enum name="media-copper">
                <value value="1"/>
                <description>
                  <text>The PEMode of the port is electric.</text>
                </description>
              </enum>
              <enum name="media-fiber">
                <value value="2"/>
                <description>
                  <text>The PEMode of the port is optical.</text>
                </description>
              </enum>
            </type>
            <default value="media-fiber"/>
          </leaf>
          <leaf name="threshold-error-packet">
            <description>
              <text>Sets the alarm threshold for port errors.</text>
            </description>
            <type name="uint16">
              <range value="1..65535">
                <description>
                  <text>The number of error packets.</text>
                </description>
              </range>
            </type>
            <default value="3"/>
          </leaf>
          <leaf name="unidirection">
            <description>
              <text>Configures an unframing channel.</text>
            </description>
            <type name="enumeration">
              <enum name="disable">
                <value value="1"/>
                <description>
                  <text>Unidirection disable.</text>
                </description>
              </enum>
              <enum name="enable">
                <value value="2"/>
                <description>
                  <text>Unidirection enable</text>
                </description>
              </enum>
            </type>
            <default value="disable"/>
          </leaf>
          <leaf name="rf2sf">
            <description>
              <text>Configures rf2sf.</text>
            </description>
            <type name="enumeration">
              <enum name="enable">
                <value value="1"/>
                <description>
                  <text>rf2sf enable</text>
                </description>
              </enum>
              <enum name="disable">
                <value value="0"/>
                <description>
                  <text>rf2sf diable</text>
                </description>
              </enum>
            </type>
            <default value="enable"/>
            <ncd:revise-date date="2018-03-09"/>
          </leaf>
          <container name="laser-status">
            <leaf name="status">
              <description>
                <text>Open/close laser or set recover mode of ALS status.</text>
              </description>
              <type name="enumeration">
                <enum name="on">
                  <value value="1"/>
                  <description>
                    <text>Open laser.</text>
                  </description>
                </enum>
                <enum name="off">
                  <value value="2"/>
                  <description>
                    <text>Close laser.</text>
                  </description>
                </enum>
                <enum name="auto-recovery">
                  <value value="3"/>
                  <description>
                    <text>Set recover mode is auto-recovery.</text>
                  </description>
                </enum>
                <enum name="manual-recovery">
                  <value value="4"/>
                  <description>
                    <text>Set recover mode is manual-recovery.</text>
                  </description>
                </enum>
                <enum name="invalid">
                  <value value="0"/>
                  <description>
                    <text>The invalid value of laser-status mode.</text>
                  </description>
                </enum>
              </type>
              <default value="on"/>
            </leaf>
            <leaf name="period">
              <description>
                <text>Start period of automatic.</text>
              </description>
              <type name="uint16">
                <range value="20..600">
                  <description>
                    <text>Range: 20-600.</text>
                  </description>
                </range>
              </type>
              <units name="second"/>
              <default value="100"/>
            </leaf>
            <leaf name="pulsewidth">
              <description>
                <text>Start pulse wide of automatic.</text>
              </description>
              <type name="uint8">
                <range value="1..20">
                  <description>
                    <text>Range: 1-20.</text>
                  </description>
                </range>
              </type>
              <units name="second"/>
              <default value="2"/>
            </leaf>
            <leaf name="testpulsewidth">
              <description>
                <text>Start pulse wide of testing.</text>
              </description>
              <type name="uint8">
                <range value="50..200">
                  <description>
                    <text>Range: 50-200.</text>
                  </description>
                </range>
              </type>
              <units name="second"/>
              <default value="90"/>
            </leaf>
            <leaf name="offtime">
              <description>
                <text>Time of laser reopen, unit is second</text>
              </description>
              <type name="uint32">
                <range value="0..65535">
                  <description>
                    <text>laser status off times (s)</text>
                  </description>
                </range>
              </type>
              <units name="second"/>
              <default value="0"/>
            </leaf>
          </container>
          <leaf name="flow-ctr-receive">
            <description>
              <text>Flow control receive.</text>
            </description>
            <type name="enumeration">
              <enum name="disable">
                <value value="1"/>
                <description>
                  <text>Disable receive flow control.</text>
                </description>
              </enum>
              <enum name="enable">
                <value value="2"/>
                <description>
                  <text>Enable receive flow control.</text>
                </description>
              </enum>
            </type>
            <default value="disable"/>
          </leaf>
          <leaf name="flow-ctrl-send">
            <description>
              <text>Flow control send.</text>
            </description>
            <type name="enumeration">
              <enum name="disable">
                <value value="1"/>
                <description>
                  <text>Disable send flow control.</text>
                </description>
              </enum>
              <enum name="enable">
                <value value="2"/>
                <description>
                  <text>Enable send flow control.</text>
                </description>
              </enum>
            </type>
            <default value="disable"/>
          </leaf>
          <leaf name="jumbo">
            <description>
              <text>Configures jumbo.</text>
            </description>
            <type name="enumeration">
              <enum name="disable">
                <value value="1"/>
                <description>
                  <text>Jumbo disable.</text>
                </description>
              </enum>
              <enum name="enable">
                <value value="2"/>
                <description>
                  <text>Jumbo enable.</text>
                </description>
              </enum>
            </type>
            <default value="enable"/>
            <ncd:revise-date date="2018-12-12"/>
          </leaf>
          <leaf name="jumbo-size">
            <description>
              <text>Configures jumbo size.</text>
            </description>
            <type name="uint32"/>
            <default value="9216"/>
            <ncd:revise-date date="2018-12-12"/>
          </leaf>
          <leaf name="master-slave-mode">
            <description>
              <text>Set clock negotiation mode of interface.</text>
            </description>
            <type name="enumeration">
              <enum name="master">
                <value value="1"/>
                <description>
                  <text>Master mode.</text>
                </description>
              </enum>
              <enum name="slave">
                <value value="2"/>
                <description>
                  <text>Slave mode.</text>
                </description>
              </enum>
              <enum name="auto">
                <value value="3"/>
                <description>
                  <text>Auto mode.</text>
                </description>
              </enum>
            </type>
            <default value="auto"/>
          </leaf>
          <leaf name="fec-eth">
            <description>
              <text>Configures fec of a port.</text>
            </description>
            <type name="enumeration">
              <enum name="enable">
                <value value="1"/>
                <description>
                  <text>enable.</text>
                </description>
              </enum>
              <enum name="disable">
                <value value="2"/>
                <description>
                  <text>disable.</text>
                </description>
              </enum>
              <enum name="auto">
                <value value="3"/>
                <description>
                  <text>auto.</text>
                </description>
              </enum>
              <enum name="invalid">
                <value value="0"/>
                <description>
                  <text>invalid</text>
                </description>
              </enum>
            </type>
            <default value="invalid"/>
            <ncd:revise-date date="2019-06-25"/>
          </leaf>
          <container name="alarm-insert">
            <description>
              <text>Configure alarm insert</text>
            </description>
            <leaf name="alarm-insert-sd">
              <description>
                <text>Alarm insert SD.</text>
              </description>
              <type name="empty"/>
            </leaf>
            <ncd:revise-date date="2017-08-01"/>
          </container>
          <container name="ipg-config">
            <description>
              <text>Configure ipg</text>
            </description>
            <leaf name="ipg">
              <description>
                <text>ipg</text>
              </description>
              <type name="uint16">
                <range value="0..255">
                  <description>
                    <text>ipg rang value and default value configured by perfcode.</text>
                  </description>
                </range>
              </type>
              <default value="12"/>
            </leaf>
            <ncd:revise-date date="2018-03-09"/>
          </container>
          <container name="loopback-time">
            <description>
              <text>Configure loopback time.</text>
            </description>
            <choice name="effective-time">
              <case name="timeliness">
                <leaf name="permanent">
                  <description>
                    <text>Loopback config permanent effective</text>
                  </description>
                  <type name="empty"/>
                </leaf>
              </case>
              <case name="date-time">
                <leaf name="begin-date">
                  <description>
                    <text>Begin date from 1-1-2001 to 12-31-2037</text>
                  </description>
                  <type name="ncd:date"/>
                  <mandatory value="true"/>
                </leaf>
                <leaf name="begin-time">
                  <description>
                    <text>Begin time</text>
                  </description>
                  <type name="ncd:time"/>
                  <mandatory value="true"/>
                </leaf>
                <leaf name="end-date">
                  <description>
                    <text>End date from 1-1-2001 to 12-31-2037</text>
                  </description>
                  <type name="ncd:date"/>
                  <mandatory value="true"/>
                </leaf>
                <leaf name="end-time">
                  <description>
                    <text>End time</text>
                  </description>
                  <type name="ncd:time"/>
                  <mandatory value="true"/>
                </leaf>
              </case>
            </choice>
          </container>
          <container name="work-mode-config">
            <description>
              <text>Configure work mode</text>
            </description>
            <leaf name="work-mode">
              <description>
                <text>Ethernet working mode.</text>
              </description>
              <type name="enumeration">
                <enum name="eth">
                  <value value="1"/>
                  <description>
                    <text>Work mode is eth</text>
                  </description>
                </enum>
                <enum name="flexe">
                  <value value="2"/>
                  <description>
                    <text>Work mode is flexe</text>
                  </description>
                </enum>
              </type>
              <default value="eth"/>
            </leaf>
            <ncd:revise-date date="2018-11-14"/>
          </container>
          <container name="optical-module-config">
            <description>
              <text>Configure optical module</text>
            </description>
            <leaf name="fec-mode">
              <description>
                <text>Configure fec mode for interface</text>
              </description>
              <type name="enumeration">
                <enum name="GFEC">
                  <value value="12"/>
                  <description>
                    <text>OTU-4 G.709 FEC</text>
                  </description>
                </enum>
                <enum name="OTNnoFEC">
                  <value value="18"/>
                  <description>
                    <text>OTU-4 Transparent mode</text>
                  </description>
                </enum>
                <enum name="HD-FEC1">
                  <value value="48"/>
                  <description>
                    <text>OTU-4 7% Staircase HD-FEC</text>
                  </description>
                </enum>
                <enum name="SD-FECy-DIFF-15">
                  <value value="53"/>
                  <description>
                    <text>15% SD-FEC Everest Compatible</text>
                  </description>
                </enum>
                <enum name="SD-FECy-NONDIFF-15">
                  <value value="85"/>
                  <description>
