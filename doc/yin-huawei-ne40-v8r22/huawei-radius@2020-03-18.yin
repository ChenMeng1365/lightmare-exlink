    <container name="server-pending-limits">
      <description>
        <text>List of the maximum number of pending packets or the limit on the number of packets sent to a RADIUS server within a specified period.</text>
      </description>
      <list name="server-pending-limit">
        <key value="server-type server-ip-address server-port vpn-name"/>
