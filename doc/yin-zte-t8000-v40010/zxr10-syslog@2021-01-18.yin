      </enum>
      <enum name="ftp">
        <value value="11"/>
        <description>
          <text>FTP process.</text>
        </description>
      </enum>
      <enum name="ntp">
        <value value="12"/>
        <description>
          <text>NTP process.</text>
        </description>
      </enum>
      <enum name="log-audit">
        <value value="13"/>
        <description>
          <text>Log audit.</text>
        </description>
      </enum>
      <enum name="log-alert">
        <value value="14"/>
        <description>
          <text>Log alert.</text>
        </description>
      </enum>
      <enum name="clock2">
        <value value="15"/>
        <description>
