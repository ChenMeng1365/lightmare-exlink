     |  +--ro ppp-chasten-info* [slot-id]
     |     +--ro slot-id                        string
     |     +--ro mac-address?                   pub-type:mac-address
     |     +--ro circuit-id?                    string
     |     +--ro remote-id?                     string
     |     +--ro vlan-mac-fail?                 string
     |     +--ro vlan-mac-qucik-offline?        string
     |     +--ro vlan-option-fail?              string
     |     +--ro vlan-option-quick?             string
     |     +--ro global-mac-fail?               string
     |     +--ro global-mac-quick-offline?      string
     |     +--ro global-option-fail?            string
     |     +--ro global-option-quick?           string
     |     +--ro global-online-fail?            string
     |     +--ro global-quick-offline?          string
     |     +--ro vlan-online-fail?              string
     |     +--ro vlan-quick-offline?            string
     |     +--ro global-online-fail-number?     string
     |     +--ro global-quick-offline-number?   string
     |     +--ro vlan-online-fail-number?       string
     |     +--ro vlan-quick-offline-number?     string
