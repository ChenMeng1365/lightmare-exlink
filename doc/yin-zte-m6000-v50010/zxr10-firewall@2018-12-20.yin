              <type name="string">
                <length value="1..32"/>
              </type>
            </leaf>
            <leaf name="ip">
              <description>
                <text>Whitelist IP address</text>
              </description>
              <type name="ncd:ipv4-address"/>
            </leaf>
            <leaf name="time-range">
              <description>
                <text>Timerange name</text>
              </description>
              <type name="string">
                <length value="0..31"/>
              </type>
              <default value=""/>
            </leaf>
          </list>
        </container>
