        <list name="service-peer">
          <key value="peer-ip"/>
          <description>
            <text>Configure the list of peers.</text>
          </description>
          <leaf name="peer-ip">
            <type name="inet:ipv4-address-no-zone"/>
            <description>
              <text>Indicates the LSR ID of the remote device.</text>
            </description>
          </leaf>
          <leaf name="ldp-mode">
            <type name="l2vpn-peer-ldp-signal-mode"/>
            <default value="default"/>
            <description>
              <text>Peer signal mode.</text>
            </description>
          </leaf>
        </list>
      </container>
      <container name="peer-no-requests">
        <description>
          <text>List of no-request peers.</text>
        </description>
        <list name="peer-no-request">
          <key value="peer-ip"/>
          <description>
            <text>Configure no-request peers.</text>
          </description>
          <leaf name="peer-ip">
            <type name="inet:ipv4-address-no-zone"/>
            <description>
              <text>Indicates the LSR ID of the remote device.</text>
            </description>
          </leaf>
        </list>
      </container>
      <container name="peer-bfd-cv-stds">
        <description>
          <text>List of BFD CV standard peers.</text>
        </description>
        <list name="peer-bfd-cv-std">
          <must condition="(enable='disable' and type='cv-bfd') or enable='enable'"/>
          <key value="peer-ip"/>
          <description>
            <text>Configure BFD CV standard peers.</text>
          </description>
          <leaf name="peer-ip">
            <type name="inet:ipv4-address-no-zone"/>
            <description>
              <text>Indicates the LSR ID of the remote device.</text>
            </description>
          </leaf>
          <leaf name="enable">
            <type name="l2vpn-common-enable-type"/>
            <default value="enable"/>
            <description>
              <text>Enable BFDCV.</text>
            </description>
          </leaf>
          <leaf name="type">
            <type name="bfd-vccv-cv-type"/>
            <default value="cv-bfd"/>
            <description>
              <text>The BFDCV type of the peer.</text>
            </description>
          </leaf>
        </list>
      </container>
      <container name="vpws-bgp-vpns">
        <description>
          <text>List of VPWS BGP VPN instances.</text>
        </description>
        <list name="vpws-bgp-vpn">
          <key value="name"/>
          <description>
            <text>Configure VPWS BGP VPN instance.</text>
          </description>
          <leaf name="name">
            <type name="string">
              <length value="1..31"/>
            </type>
            <description>
              <text>VPWS BGP VPN instance name.</text>
            </description>
          </leaf>
          <leaf name="encapsulation-type">
            <type name="vpws-bgp-vpn-encap"/>
            <mandatory value="true"/>
            <description>
              <text>VPWS BGP VPN instance encapsulation type.</text>
            </description>
          </leaf>
          <leaf name="control-word">
            <type name="enumeration">
              <enum name="disable">
                <value value="1"/>
                <description>
                  <text>The control word mode is disable.</text>
                </description>
              </enum>
              <enum name="enable">
                <value value="2"/>
                <description>
                  <text>The control word mode is enable.</text>
                </description>
              </enum>
            </type>
            <default value="disable"/>
            <description>
              <text>Control word of the VPWS BGP VPN instance. If the encapsulation type is low speed, disabling the control word causes the VPWS BGP instance to fail.</text>
            </description>
          </leaf>
          <leaf name="route-distinguisher">
            <type name="string">
              <length value="3..21"/>
            </type>
            <description>
              <text>Route distinguisher of VPWS BGP VPN instance.</text>
            </description>
          </leaf>
          <leaf name="mtu">
            <type name="uint16">
              <range value="0..16352"/>
            </type>
            <default value="1500"/>
            <description>
              <text>MTU of VPWS BGP VPN instance.</text>
            </description>
          </leaf>
          <leaf name="ignore-mtu-match">
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable the function of the ignoring MTU match of a VPWS BGP VPN instance.</text>
            </description>
          </leaf>
          <leaf name="auto-route-distinguisher">
            <type name="string">
              <length value="3..21"/>
            </type>
            <config value="false"/>
            <description>
              <text>Automatic route distinguisher (RD). If no RD is configured for an L2VPN instance, you can specify an IPv4 address using the rd-ip parameter, based on which an RD is automatically generated in the format of 32-bit prefix IP address:index, for example, 192.168.122.15:1. An index is an integer automatically allocated by the system and ranges from 1 to 65535.Each L2VPN instance must use a unique RD.</text>
            </description>
          </leaf>
          <container name="vpn-targets">
            <description>
              <text>List of route targets of VPWS BGP VPN instances.</text>
            </description>
            <list name="vpn-target">
              <key value="rt-value rt-type"/>
              <max-elements value="32"/>
              <description>
                <text>Configure a route target list of the VPWS BGP VPN instances.</text>
              </description>
              <leaf name="rt-value">
                <type name="string">
                  <length value="3..21"/>
                </type>
                <description>
                  <text>Route target value of the VPWS BGP VPN instance.</text>
                </description>
              </leaf>
              <leaf name="rt-type">
                <type name="vpn-rt-type"/>
                <description>
                  <text>Route target type of the VPWS BGP VPN instance.</text>
                </description>
              </leaf>
            </list>
          </container>
          <container name="sites">
            <description>
              <text>List of CEs in a VPWS BGP VPN instance.</text>
            </description>
            <list name="site">
              <must condition="not(id=0 and default-offset=1)"/>
              <key value="name"/>
              <unique tag="id"/>
              <description>
                <text>Configure a CE in a VPWS BGP VPN instance.</text>
              </description>
              <leaf name="name">
                <type name="vpws-bgp-ce-name-type"/>
                <description>
                  <text>Local site name.</text>
                </description>
              </leaf>
              <leaf name="id">
                <ext:operation-exclude value="update"/>
                <type name="uint16">
                  <range value="0..65534"/>
                </type>
                <mandatory value="true"/>
                <description>
                  <text>VPWS BGP VPN instance customer edge identifier.</text>
                </description>
              </leaf>
              <leaf name="range">
                <type name="uint16">
                  <range value="1..65535"/>
                </type>
                <must condition="../l2vpn:range &gt;= /l2vpn:l2vpn/l2vpn:common/l2vpn:vpws-bgp-vpns/l2vpn:vpws-bgp-vpn[l2vpn:name=current()/../../../l2vpn:name]/l2vpn:sites/l2vpn:site[l2vpn:name = current()/../l2vpn:name]/range"/>
                <default value="10"/>
                <description>
                  <text>VPWS BGP VPN instance customer edge range.</text>
                </description>
              </leaf>
              <leaf name="default-offset">
                <type name="uint16">
                  <range value="0..1"/>
                </type>
                <default value="0"/>
                <description>
                  <text>VPWS BGP VPN instance customer edge default offset.</text>
                </description>
              </leaf>
              <container name="label-blocks">
                <config value="false"/>
                <description>
                  <text>List of label blocks of a CE in a VPWS BGP VPN instance.</text>
                </description>
                <list name="label-block">
                  <key value="label-offset"/>
                  <description>
                    <text>Operational data of a label block of a CE in aVPWS BGP VPN instance.</text>
                  </description>
                  <leaf name="label-offset">
                    <type name="uint8"/>
                    <description>
                      <text>VPWS BGP VPN instance customer edge label offset.</text>
                    </description>
                  </leaf>
                  <leaf name="label-base">
                    <type name="uint32"/>
                    <description>
                      <text>VPWS BGP VPN instance customer edge label benchmark.</text>
                    </description>
                  </leaf>
                  <leaf name="ce-base">
                    <type name="uint32"/>
                    <description>
                      <text>VPWS BGP VPN instance customer edge benchmark.</text>
                    </description>
                  </leaf>
                  <leaf name="label-range">
                    <type name="uint16"/>
                    <description>
                      <text>VPWS BGP VPN instance customer edge label range.</text>
                    </description>
                  </leaf>
                </list>
              </container>
            </list>
          </container>
          <container name="remote-sites">
            <config value="false"/>
            <description>
              <text>List of remote CE information in a VPWS BGP VPN instance.</text>
            </description>
            <list name="remote-site">
              <key value="id"/>
              <description>
                <text>Operational data of remote label information of a remote CE in a VPWS BGP VPN instance.</text>
              </description>
              <leaf name="id">
                <type name="uint16"/>
                <description>
                  <text>VPWS BGP VPN instance remote customer edge info customer edge identifier.</text>
                </description>
              </leaf>
              <leaf name="peer-ip">
                <type name="inet:ipv4-address-no-zone"/>
                <description>
                  <text>VPWS BGP VPN instance remote customer edge info peer IP address.</text>
                </description>
              </leaf>
              <leaf name="route-distinguisher">
                <type name="string">
                  <length value="0..21"/>
                </type>
                <description>
                  <text>VPWS BGP VPN instance remote customer edge info remote route distinguisher.</text>
                </description>
              </leaf>
              <container name="labels">
                <description>
                  <text>List of remote label information of a remote CE in a VPWS BGP VPN instance.</text>
                </description>
                <list name="label">
                  <key value="label-offset"/>
                  <description>
                    <text>Operational data of remote label information of a remote CE in a VPWS BGP VPN instance.</text>
                  </description>
                  <leaf name="label-offset">
                    <type name="uint8"/>
                    <description>
                      <text>VPWS BGP VPN instance remote customer edge info remote label offset.</text>
                    </description>
                  </leaf>
                  <leaf name="label-base">
                    <type name="uint32"/>
                    <description>
                      <text>VPWS BGP VPN instance remote customer edge info remote label benchmark.</text>
                    </description>
                  </leaf>
                  <leaf name="label-range">
                    <type name="uint16"/>
                    <description>
                      <text>VPWS BGP VPN instance remote customer edge info remote label range.</text>
                    </description>
                  </leaf>
                </list>
              </container>
            </list>
          </container>
        </list>
      </container>
      <container name="not-match-remote-ldps">
        <config value="false"/>
        <description>
          <text>List of incongruous remote signaling.</text>
        </description>
        <list name="not-match-remote-ldp">
