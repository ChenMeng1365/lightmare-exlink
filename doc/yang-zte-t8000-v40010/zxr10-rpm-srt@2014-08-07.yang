                  description 'The mask of route' ;
                  type ncd:ipv4-address ;
                }
                leaf interface {
                  description 'The interface name of route' ;
                  type ncd:ifname {
                    length 1..31 ;
                  }
                }
                leaf preference {
                  description 'The route priority and the default value is 1. T'
                            + 'he range is 1-255.'
                              ;
                  type uint8 {
                    range 1..255 ;
                  }
                  default 1 ;
