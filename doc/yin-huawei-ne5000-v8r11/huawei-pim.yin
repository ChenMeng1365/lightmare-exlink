  <augment target-node="/ni:network-instance/ni:instances/ni:instance">
    <description>
      <text>Adds PIM configuration to the L3VPN module.</text>
    </description>
    <container name="pim">
      <description>
        <text>Configure PIM (Protocol Independent Multicast) in the instance.</text>
      </description>
      <container name="rp">
        <description>
          <text>Configure PIM RP configurations.</text>
        </description>
        <container name="c-bsr-vpns">
          <description>
            <text>List of BSR configurations.</text>
          </description>
          <list name="c-bsr-vpn">
            <key value="type"/>
            <description>
              <text>Configure BSR.</text>
            </description>
            <leaf name="type">
              <type name="addr-family-type"/>
              <must condition="../../../../pim:protocol/pim:instances/pim:instance[pim:type=current()]"/>
              <description>
                <text>Address family, which determines whether an address belongs to IPv4 or IPv6.</text>
              </description>
            </leaf>
            <leaf name="if-name">
              <when condition="../type='ipv4unicast'"/>
              <type name="leafref">
                <path value="/ifm:ifm/ifm:interfaces/ifm:interface/ifm:name"/>
              </type>
              <must condition="../../../../../ni:name=/ifm:ifm/ifm:interfaces/ifm:interface[ifm:name=current()]/ifm:vrf-name"/>
              <description>
                <text>Interface name of a C-BSR. PIM-SM must be enabled on the interface to make the C-BSR take effect.</text>
              </description>
            </leaf>
            <leaf name="if-addr">
              <when condition="../type='ipv6unicast'"/>
              <type name="inet:ipv6-address-no-zone"/>
              <description>
                <text>Global IPv6 unicast address of the C-BSR.</text>
              </description>
            </leaf>
            <leaf name="hold-time">
              <type name="uint32">
                <range value="1..214748364"/>
              </type>
              <units name="s"/>
              <default value="130"/>
              <description>
                <text>Timeout period (called holdtime) during which C-BSRs wait to receive bootstrap messages from the BSR. To prevent frequent BSR elections, set the same holdtime for all C-BSRs in the same domain. If an interval (called BS_interval) at which bootstrap messages are sent has been set for C-BSRs, ensure that the specified holdtime is larger than the BS_interval.</text>
              </description>
            </leaf>
            <leaf name="interval">
              <type name="uint32">
                <range value="1..107374177"/>
              </type>
              <units name="s"/>
              <default value="60"/>
              <description>
                <text>Interval (called BS_interval) at which a BSR continuously sends bootstrap messages. To prevent frequent BSR elections, set the same BS_interval for all C-BSRs in the same domain. If a timeout period (called holdtime) during which C-BSRs wait to receive bootstrap messages from the BSR has been set for C-BSRs, ensure that the specified BS_interval is smaller than the holdtime.</text>
              </description>
            </leaf>
            <leaf name="hash-len">
              <type name="uint32">
                <range value="0..128"/>
              </type>
              <ext:dynamic-default>
                <ext:default-value value="30">
                  <when condition="../type='ipv4unicast'"/>
                  <description>
                    <text>In IPv4, the default value is 30.</text>
                  </description>
                </ext:default-value>
                <ext:default-value value="126">
                  <when condition="../type='ipv6unicast'"/>
                  <description>
                    <text>In IPv6, the default value is 126.</text>
                  </description>
                </ext:default-value>
              </ext:dynamic-default>
              <description>
                <text>Global hash mask length for a C-BSR.
In IPv4, the value is an integer ranging from 0 to 32, and the default value is 30. In IPv6, the value is an integer ranging from 0 to 128, and the default value is 126.</text>
              </description>
            </leaf>
            <leaf name="priority">
              <type name="uint32">
                <range value="0..255"/>
              </type>
              <default value="0"/>
              <description>
                <text>Globally specify a priority for all C-BSRs on the router. The greater the value, the higher the priority.</text>
              </description>
            </leaf>
            <leaf name="policy-name">
              <when condition="../type='ipv4unicast'"/>
              <type name="leafref">
                <path value="/acl:acl/acl:groups/acl:group/acl:identity"/>
              </type>
              <must condition="/acl:acl/acl:groups/acl:group[acl:identity=current()]/acl:type='basic'"/>
              <description>
                <text>Policy for limiting the range of valid BSR addresses so that a router discards the messages received from the BSRs not in the specified address range.</text>
              </description>
            </leaf>
            <leaf name="policy-name6">
              <when condition="../type='ipv6unicast'"/>
              <type name="leafref">
                <path value="/acl:acl/acl:group6s/acl:group6/acl:identity"/>
              </type>
              <must condition="/acl:acl/acl:group6s/acl:group6[acl:identity=current()]/acl:type='basic'"/>
              <description>
                <text>Policy for limiting the range of valid BSR addresses so that a router discards the messages received from the BSRs not in the specified address range.</text>
              </description>
            </leaf>
            <leaf name="admin-scope">
              <when condition="../type='ipv4unicast'"/>
              <type name="boolean"/>
              <default value="false"/>
              <description>
                <text>Enable/disable a device to divide one PIM-SM domain into multiple BSR administrative domains to implement RP-Set advertisement. By default, there is only one BSR in the entire PIM-SM domain.</text>
              </description>
            </leaf>
            <leaf name="global-enable">
              <when condition="../type='ipv4unicast'"/>
              <type name="boolean"/>
              <default value="false"/>
              <description>
                <text>Enable/disable C-BSR in the global domain. By default, no C-BSR is configured in the global domain.</text>
              </description>
            </leaf>
            <leaf name="global-hash-len">
              <when condition="../global-enable='true'"/>
              <type name="uint32">
                <range value="0..32"/>
              </type>
              <default value="30"/>
              <description>
                <text>Hash mask length for the C-BSR in the global domain.</text>
              </description>
            </leaf>
            <leaf name="global-priority">
              <when condition="../global-enable='true'"/>
              <type name="uint32">
                <range value="0..255"/>
              </type>
              <default value="0"/>
              <description>
                <text>Priority for the C-BSR in the global domain. The greater the value, the higher the priority.</text>
              </description>
            </leaf>
            <leaf name="c-rp-hold-time">
              <type name="uint32">
                <range value="1..65535"/>
              </type>
              <units name="s"/>
              <default value="150"/>
              <description>
                <text>Timeout period during which a BSR waits to receive Advertisement messages from a C-RP.</text>
              </description>
            </leaf>
            <leaf name="c-rp-priority">
              <type name="uint32">
                <range value="0..255"/>
              </type>
              <ext:dynamic-default>
                <ext:default-value value="0">
                  <when condition="../type='ipv4unicast'"/>
                  <description>
                    <text>In IPv4, the default value is 0.</text>
                  </description>
                </ext:default-value>
                <ext:default-value value="192">
                  <when condition="../type='ipv6unicast'"/>
                  <description>
                    <text>In IPv6, the default value is 192.</text>
                  </description>
                </ext:default-value>
              </ext:dynamic-default>
              <description>
                <text>Global priority for C-RPs. The greater the value, the lower the priority. In IPv4, the value is an integer ranging from 0 to 255, and the default value is 0. In IPv6, the value is an integer ranging from 0 to 255, and the default value is 192.</text>
              </description>
            </leaf>
            <leaf name="c-rp-ad-interval">
              <type name="uint32">
                <range value="1..65535"/>
              </type>
              <units name="s"/>
              <default value="60"/>
              <description>
                <text>Interval at which a C-RP periodically sends Advertisement messages.</text>
              </description>
            </leaf>
            <leaf name="c-rp-policy">
              <when condition="../type='ipv4unicast'"/>
              <type name="leafref">
                <path value="/acl:acl/acl:groups/acl:group/acl:identity"/>
              </type>
              <must condition="/acl:acl/acl:groups/acl:group[acl:identity=current()]/acl:type='advance'"/>
              <description>
                <text>Policy for limiting the range of valid C-RP addresses and the range of multicast groups that each C-RP serves. The BSR configured with a C-RP filter policy discards messages received from the C-RPs whose addresses are not in the specified address range, thus preventing C-RP spoofing.</text>
              </description>
            </leaf>
            <leaf name="c-rp-policy6">
              <when condition="../type='ipv6unicast'"/>
              <type name="leafref">
                <path value="/acl:acl/acl:group6s/acl:group6/acl:identity"/>
              </type>
              <must condition="/acl:acl/acl:group6s/acl:group6[acl:identity=current()]/acl:type='advance'"/>
              <description>
                <text>Policy for limiting the range of valid C-RP addresses and the range of multicast groups that each C-RP serves. The BSR configured with a C-RP filter policy discards messages received from the C-RPs whose addresses are not in the specified address range, thus preventing C-RP spoofing.</text>
              </description>
            </leaf>
            <leaf name="is-fragable">
              <type name="boolean"/>
              <default value="false"/>
              <description>
                <text>Enable/disable the C-BSR message fragmentation function.</text>
              </description>
            </leaf>
          </list>
        </container>
        <container name="ipv4-c-bsr-groups">
          <when condition="../../pim:protocol/pim:instances/pim:instance[type='ipv4unicast']"/>
          <description>
            <text>List of C-BSR group ranges.</text>
          </description>
          <list name="ipv4-c-bsr-group">
            <key value="group-addr group-mask"/>
            <max-elements value="64"/>
            <description>
              <text>Configure range of C-BSR groups. PIM-SM must be enabled on the interface to make the C-BSR take effect.</text>
            </description>
            <leaf name="group-addr">
              <type name="inet:ipv4-address-no-zone">
                <pattern value="(239)(\.(25[0-5]|2[0-4]\d|1\d\d|[1-9]?\d)){3}"/>
              </type>
              <description>
                <text>Multicast group address. The value ranges from 239.0.0.0 to 239.255.255.255.</text>
              </description>
            </leaf>
            <leaf name="group-mask">
              <type name="inet:ipv4-address-no-zone"/>
              <description>
                <text>Mask of a group address. The value is in dotted decimal notation.</text>
              </description>
            </leaf>
            <leaf name="hash-length">
              <type name="uint32">
                <range value="0..32"/>
              </type>
              <default value="30"/>
              <description>
                <text>Hash length of a C-BSR.</text>
              </description>
            </leaf>
            <leaf name="priority">
              <type name="uint32">
                <range value="0..255"/>
              </type>
              <default value="0"/>
              <description>
                <text>Priority of a C-BSR in the BSR administrative domain that serves a specific multicast group. The greater the value, the higher the priority.</text>
              </description>
            </leaf>
          </list>
        </container>
        <container name="ipv4-c-rp-ifs">
          <when condition="../../pim:protocol/pim:instances/pim:instance[type='ipv4unicast']"/>
          <description>
            <text>List of C-RP IPv4 configurations.</text>
          </description>
          <list name="ipv4-c-rp-if">
            <key value="if-name"/>
            <description>
              <text>Configure C-RP IPv4 configuration. PIM-SM must be enabled on the interface to make the C-RP take effect.</text>
            </description>
            <leaf name="if-name">
              <type name="leafref">
                <path value="/ifm:ifm/ifm:interfaces/ifm:interface/ifm:name"/>
              </type>
              <must condition="../../../../../ni:name=/ifm:ifm/ifm:interfaces/ifm:interface[ifm:name=current()]/ifm:vrf-name"/>
              <description>
                <text>Name of a C-RP interface.</text>
              </description>
            </leaf>
            <leaf name="group-policy">
              <type name="leafref">
                <path value="/acl:acl/acl:groups/acl:group/acl:identity"/>
              </type>
              <must condition="/acl:acl/acl:groups/acl:group[acl:identity=current()]/acl:type='basic'"/>
              <description>
                <text>Policy for limiting the range of valid group addresses. With this policy, a router discards messages received from the addresses not in the specified range.</text>
              </description>
            </leaf>
            <leaf name="priority">
              <type name="uint32">
                <range value="0..255"/>
              </type>
              <default value="0"/>
              <description>
                <text>Priority of a C-RP. The greater the value, the lower the priority.</text>
              </description>
            </leaf>
            <leaf name="hold-time">
              <type name="uint32">
                <range value="1..65535"/>
              </type>
              <units name="s"/>
              <default value="150"/>
              <description>
                <text>Timeout period during which a BSR waits to receive Advertisement messages from a C-RP.</text>
              </description>
            </leaf>
            <leaf name="ad-interval">
              <type name="uint32">
                <range value="1..65535"/>
              </type>
              <units name="s"/>
              <default value="60"/>
              <description>
                <text>Interval at which a C-RP sends Advertisement messages.</text>
              </description>
            </leaf>
          </list>
        </container>
        <container name="ipv6-c-rps">
          <when condition="../../pim:protocol/pim:instances/pim:instance[type='ipv6unicast']"/>
          <description>
            <text>List of C-RP IPv6 configurations.</text>
          </description>
          <list name="ipv6-c-rp">
            <key value="address"/>
            <description>
              <text>Configure C-RP IPv6 configurations.</text>
            </description>
            <leaf name="address">
              <type name="inet:ipv6-address-no-zone"/>
              <description>
                <text>Address of a C-RP. The value is a valid IPv6 unicast address, such as fc00::100.</text>
              </description>
            </leaf>
            <leaf name="holdtime">
              <type name="uint32">
                <range value="1..65535"/>
              </type>
              <units name="s"/>
              <default value="150"/>
              <description>
                <text>Timeout period during which a BSR waits to receive Advertisement messages from a C-RP.</text>
              </description>
            </leaf>
            <leaf name="priority">
              <type name="uint32">
                <range value="0..255"/>
              </type>
              <default value="192"/>
              <description>
                <text>Priority of a C-RP. The greater the value, the lower the priority.</text>
              </description>
            </leaf>
            <leaf name="ad-interval">
              <type name="uint32">
                <range value="1..65535"/>
              </type>
              <units name="s"/>
              <default value="60"/>
              <description>
                <text>Interval at which a C-RP continuously sends Advertisement messages.</text>
              </description>
            </leaf>
            <leaf name="group-policy">
              <type name="leafref">
                <path value="/acl:acl/acl:group6s/acl:group6/acl:identity"/>
              </type>
              <must condition="/acl:acl/acl:group6s/acl:group6[acl:identity=current()]/acl:type='basic'"/>
              <description>
                <text>Policy for limiting the range of valid group addresses. With this policy, a router discards the messages received from the addresses not in the specified range.</text>
              </description>
            </leaf>
          </list>
        </container>
        <container name="auto-rps">
          <when condition="../../pim:protocol/pim:instances/pim:instance[type='ipv4unicast']"/>
          <presence value="Auto-RP configuration."/>
          <description>
            <text>Enable/disable Auto-RP.</text>
          </description>
        </container>
        <container name="static-rps">
          <description>
            <text>List of static RPs.</text>
          </description>
          <list name="static-rp">
            <key value="type"/>
            <description>
              <text>Configure static RP.</text>
            </description>
            <leaf name="type">
              <type name="addr-family-type"/>
              <must condition="../../../../pim:protocol/pim:instances/pim:instance[pim:type=current()]"/>
              <description>
                <text>Address family, which determines whether an address belongs to IPv4 or IPv6.</text>
              </description>
            </leaf>
            <leaf name="address">
              <type name="inet:ip-address-no-zone"/>
              <mandatory value="true"/>
              <description>
                <text>Address of a static RP. This address must be a valid unicast IP address.
In IPv4, the value is in dotted decimal notation. This address cannot be an address on the network segment 127.0.0.0/8.
In IPv6, the value is in hexadecimal notation and in the format of FFxx:xxxx:xxxx::xxxx.</text>
              </description>
            </leaf>
            <leaf name="policy">
              <when condition="../type='ipv4unicast'"/>
              <type name="leafref">
                <path value="/acl:acl/acl:groups/acl:group/acl:identity"/>
              </type>
              <must condition="/acl:acl/acl:groups/acl:group[acl:identity=current()]/acl:type='basic'"/>
              <description>
                <text>Static RP policy.</text>
              </description>
            </leaf>
            <leaf name="preference">
              <type name="rp-prefer-type"/>
              <default value="not-prefer"/>
              <description>
                <text>Whether the static RP is preferred. The value can be:
0: the static RP is not preferred.
1: the static RP is preferred.
The default value is 0.</text>
              </description>
            </leaf>
          </list>
        </container>
        <container name="elected-bsrs">
          <config value="false"/>
          <description>
            <text>List of elected BSR states.</text>
          </description>
          <list name="elected-bsr">
            <key value="type address"/>
            <description>
              <text>Operational state of elected BSRs.</text>
            </description>
            <leaf name="type">
              <type name="addr-family-type"/>
              <description>
                <text>Address family, which determines whether an address belongs to IPv4 or IPv6.</text>
              </description>
            </leaf>
            <leaf name="address">
              <type name="inet:ip-address-no-zone"/>
              <description>
                <text>Address of an elected BSR.</text>
              </description>
            </leaf>
            <leaf name="scope">
              <type name="bsr-scope"/>
              <description>
                <text>Whether the BSR is a BSR in an administrative domain.</text>
              </description>
            </leaf>
            <leaf name="group-addr">
              <type name="inet:ip-address-no-zone">
                <ext:value-meaning>
                  <ext:item value="0.0.0.0">
                    <ext:meaning value="--"/>
                  </ext:item>
                </ext:value-meaning>
              </type>
              <description>
                <text>Multicast group address. In IPv4 address, the value ranges from 239.0.0.0 to 239.255.255.255. In IPv6, the value is in hexadecimal notation and in the format of FFxx:xxxx:xxxx::xxxx.</text>
              </description>
            </leaf>
            <leaf name="group-mask-length">
              <type name="uint32">
                <range value="0..32"/>
                <ext:value-meaning>
                  <ext:item value="0">
                    <ext:meaning value="--"/>
                  </ext:item>
                </ext:value-meaning>
              </type>
              <description>
                <text>Mask length of a group address. The value is an integer ranging from 8 to 32.</text>
              </description>
            </leaf>
            <leaf name="priority">
              <type name="uint32">
                <range value="0..255"/>
              </type>
              <description>
                <text>Priority for a C-BSR to participate in BSR election. The greater the value, the higher the priority. The value is an integer ranging from 0 to 255. The default value is 0.</text>
              </description>
            </leaf>
            <leaf name="hash-lenth">
              <type name="uint32">
                <range value="0..128"/>
              </type>
              <description>
                <text>Hash length used by an elected BSR to perform RP calculation. The value is an integer ranging from 0 to 32.</text>
              </description>
            </leaf>
            <leaf name="fsm-state">
              <type name="bsr-state"/>
              <description>
                <text>Status of an elected BSR.</text>
              </description>
            </leaf>
            <leaf name="up-time">
              <type name="pub-type:time-tick"/>
              <description>
                <text>Time since the C-BSR was elected as the BSR.</text>
              </description>
            </leaf>
            <leaf name="expire-time">
              <type name="pub-type:time-tick"/>
              <description>
                <text>Period after which the elected BSR on the LAN sends the next bootstrap message.</text>
              </description>
            </leaf>
            <leaf name="candidate-count">
              <type name="uint32">
                <range value="0..4294967295"/>
              </type>
              <description>
                <text>Number of candidate RPs.</text>
              </description>
            </leaf>
          </list>
        </container>
        <container name="c-bsrs">
          <config value="false"/>
          <description>
            <text>List of C-BSR states.</text>
          </description>
          <list name="c-bsr">
            <key value="type address"/>
            <description>
              <text>Operational state of candidate bootstrap router.</text>
            </description>
            <leaf name="type">
              <type name="addr-family-type"/>
              <description>
                <text>Address family, which determines whether an address belongs to IPv4 or IPv6.</text>
              </description>
            </leaf>
            <leaf name="address">
              <type name="inet:ip-address-no-zone"/>
              <description>
                <text>Address of a C-BSR.</text>
              </description>
            </leaf>
            <leaf name="scope">
              <type name="bsr-scope"/>
              <description>
                <text>Whether the BSR is a BSR in an administrative domain.</text>
              </description>
            </leaf>
            <leaf name="group-addr">
              <type name="inet:ip-address-no-zone">
                <ext:value-meaning>
                  <ext:item value="0.0.0.0">
                    <ext:meaning value="--"/>
                  </ext:item>
                </ext:value-meaning>
              </type>
              <description>
                <text>Multicast group address. In IPv4, the value ranges from 239.0.0.0 to 239.255.255.255. In IPv6, the value is in hexadecimal notation and in the format of FFxx:xxxx:xxxx::xxxx.</text>
              </description>
            </leaf>
            <leaf name="group-mask-length">
              <type name="uint32">
                <range value="0..32"/>
                <ext:value-meaning>
                  <ext:item value="0">
                    <ext:meaning value="--"/>
                  </ext:item>
                </ext:value-meaning>
              </type>
              <description>
                <text>Mask length of a group address. The value is an integer ranging from 8 to 32.</text>
              </description>
            </leaf>
            <leaf name="priority">
              <type name="uint32">
                <range value="0..255"/>
              </type>
              <description>
                <text>Priority of a C-BSR. The greater the value, the higher the priority. The value is an integer ranging from 0 to 255. The default value is 0.</text>
              </description>
            </leaf>
            <leaf name="hash-lenth">
              <type name="uint32">
                <range value="0..32"/>
              </type>
              <description>
                <text>Mask length in RP hash calculation. The value is an integer ranging from 0 to 32.</text>
              </description>
            </leaf>
            <leaf name="fsm-state">
              <type name="bsr-state"/>
              <description>
                <text>Status of a C-BSR.</text>
              </description>
            </leaf>
            <leaf name="valid-flag">
              <type name="c-bsr-valid-type"/>
              <description>
                <text>Whether the current C-BSR is valid (0: valid; 1: invalid).</text>
              </description>
            </leaf>
          </list>
        </container>
        <container name="elected-bsr-rps">
          <config value="false"/>
          <description>
            <text>List of RP information about elected BSRs.</text>
          </description>
          <list name="elected-bsr-rp">
            <key value="type rp-addr"/>
            <description>
              <text>Elected BSR RP state.</text>
            </description>
            <leaf name="type">
              <type name="addr-family-type"/>
              <description>
                <text>Address family, which determines whether an address belongs to IPv4 or IPv6.</text>
              </description>
            </leaf>
            <leaf name="rp-addr">
              <type name="inet:ip-address-no-zone"/>
              <description>
                <text>RP address.</text>
              </description>
            </leaf>
            <leaf name="is-local-addr">
              <type name="boolean"/>
              <description>
                <text>Whether the RP address is a local address.</text>
              </description>
            </leaf>
            <leaf name="group-addr">
              <type name="inet:ip-address-no-zone"/>
              <description>
                <text>Multicast group address. In IPv4, the value ranges from 239.0.0.0 to 239.255.255.255. In IPv6, the value is in hexadecimal notation and in the format of FFxx:xxxx:xxxx::xxxx.</text>
              </description>
            </leaf>
            <leaf name="group-mask-length">
              <type name="uint32">
                <range value="0..32"/>
              </type>
              <description>
                <text>Mask length. In IPv4, the value ranges from 4 to 32. In IPv6, the value ranges from 8 to 128.</text>
              </description>
            </leaf>
            <leaf name="priority">
              <type name="uint32">
                <range value="0..255"/>
              </type>
              <description>
                <text>Priority of an RP. The greater the value, the higher the priority. The value is an integer ranging from 0 to 255. The default value is 0.</text>
              </description>
            </leaf>
            <leaf name="up-time">
              <type name="pub-type:time-tick"/>
              <units name="s"/>
              <description>
                <text>Time since the C-RP was elected as the RP.</text>
              </description>
            </leaf>
            <leaf name="expiry-time">
              <type name="pub-type:time-tick"/>
              <units name="s"/>
              <description>
                <text>Time after which an RP will expire, in seconds.</text>
              </description>
            </leaf>
            <leaf name="bidir-enable">
              <type name="boolean"/>
              <description>
                <text>Whether BIDIR-PIM is enabled on the RP.</text>
              </description>
            </leaf>
          </list>
        </container>
      </container>
      <container name="protocol">
        <description>
          <text>Configure PIM.</text>
        </description>
        <container name="instances">
          <description>
            <text>List of PIM configurations in the instance.</text>
          </description>
          <list name="instance">
            <key value="type"/>
            <description>
              <text>Configure PIM instance.</text>
            </description>
            <leaf name="type">
              <type name="addr-family-type"/>
              <description>
                <text>Address family, which determines whether an address belongs to IPv4 or IPv6.</text>
              </description>
            </leaf>
            <leaf name="assert-holdtime">
              <type name="uint32">
                <range value="7..65535"/>
              </type>
              <units name="s"/>
              <default value="180"/>
              <description>
                <text>Timeout period during which PIM interfaces wait to receive Assert messages from the forwarder.</text>
              </description>
            </leaf>
            <leaf name="jp-holdtime">
              <type name="uint32">
                <range value="1..65535"/>
              </type>
              <units name="s"/>
              <default value="210"/>
              <description>
                <text>Holdtime for a Join/Prune message sent by a PIM interface. Commonly, the holdtime is 3.5 times longer than the interval for all the interfaces to send Join/Prune messages.</text>
              </description>
            </leaf>
            <leaf name="probe-interval">
              <type name="uint32">
                <range value="1..1799"/>
              </type>
              <units name="s"/>
              <must condition="(../probe-interval&lt;../reg-suppress-interval*0.5)"/>
              <default value="5"/>
              <description>
                <text>Interval at which Probe messages are sent to an RP.</text>
              </description>
            </leaf>
            <leaf name="jp-timer-interval">
              <type name="uint32">
                <range value="1..18000"/>
              </type>
              <units name="s"/>
              <default value="60"/>
              <description>
                <text>Interval at which Join/Prune messages are sent. It must be shorter than the holdtime of Join/Prune messages.</text>
              </description>
            </leaf>
            <leaf name="dr-priority">
              <type name="uint32">
                <range value="0..4294967295"/>
              </type>
              <default value="1"/>
              <description>
                <text>DR election priority for a router. The greater the value, the higher the priority. The value is an integer ranging from 0 to 4294967295. The default value is 1.</text>
              </description>
            </leaf>
            <leaf name="hello-holdtime">
              <type name="uint16">
                <range value="1..65535"/>
              </type>
              <units name="s"/>
              <default value="105"/>
              <description>
                <text>Timeout period during which a PIM interface waits to receive Hello messages from its neighbors. It must be longer than the interval for PIM neighbors to send Hello messages.</text>
              </description>
            </leaf>
            <leaf name="hello-landelay">
              <type name="uint16">
                <range value="1..32767"/>
              </type>
              <units name="ms"/>
              <default value="500"/>
              <description>
                <text>Delay for transmitting Prune messages on a shared network segment.</text>
              </description>
            </leaf>
            <leaf name="hello-interval">
              <type name="uint32">
                <range value="1..18000"/>
              </type>
              <units name="s"/>
              <default value="30"/>
              <description>
                <text>Specifies the interval at which Hello messages are sent. It must be shorter than the timeout period of PIM neighbors.</text>
              </description>
            </leaf>
            <leaf name="hello-override">
              <type name="uint16">
                <range value="1..65535"/>
              </type>
              <units name="ms"/>
              <default value="2500"/>
              <description>
                <text>Interval at which the prune action in a Hello message is overridden.</text>
              </description>
            </leaf>
            <leaf name="reg-checksum">
              <when condition="../type='ipv4unicast'"/>
              <type name="boolean"/>
              <default value="false"/>
              <description>
                <text>Enable/disable a router to calculate the checksum based on all contents of a Register message. By default, the checksum is calculated based on the header of a Register message only.</text>
              </description>
            </leaf>
            <leaf name="reg-suppress-interval">
              <type name="uint32">
                <range value="11..3600"/>
              </type>
              <units name="s"/>
              <default value="60"/>
              <description>
                <text>Timeout period during which a router remains in the registration suppression state.</text>
              </description>
            </leaf>
            <leaf name="nbr-check-when-recv">
              <type name="boolean"/>
              <default value="false"/>
              <description>
                <text>Enable/disable the PIM neighbor check function to check whether received Join/Prune and Assert messages are sent from a PIM neighbor. If not, these messages are discarded.
By default, the PIM neighbor check function is disabled.</text>
              </description>
            </leaf>
            <leaf name="nbr-check-when-send">
              <type name="boolean"/>
              <default value="false"/>
              <description>
                <text>Enable/disable the PIM neighbor check function to check whether Join/Prune and Assert messages are to be sent to an IPv4/v6 PIM neighbor. If not, these messages are not sent. By default, the PIM neighbor check function is disabled for Join/Prune and Assert messages to be sent.</text>
              </description>
            </leaf>
            <leaf name="reg-policy-name">
              <when condition="../type='ipv4unicast'"/>
              <type name="leafref">
                <path value="/acl:acl/acl:groups/acl:group/acl:identity"/>
              </type>
              <must condition="/acl:acl/acl:groups/acl:group[acl:identity=current()]/acl:type='advance'"/>
              <description>
                <text>Policy for filtering Register messages. The value is an integer ranging from 3000 to 3999, or a string.</text>
              </description>
            </leaf>
            <leaf name="ssm-policy-name">
              <when condition="../type='ipv4unicast'"/>
              <type name="leafref">
                <path value="/acl:acl/acl:groups/acl:group/acl:identity"/>
              </type>
              <must condition="/acl:acl/acl:groups/acl:group[acl:identity=current()]/acl:type='basic'"/>
              <description>
                <text>Policy for limiting the range of valid SSM group addresses. The value is an integer ranging from 2000 to 2999, or a string.</text>
              </description>
            </leaf>
            <leaf name="src-policy-name">
              <when condition="../type='ipv4unicast'"/>
              <type name="leafref">
                <path value="/acl:acl/acl:groups/acl:group/acl:identity"/>
              </type>
              <must condition="/acl:acl/acl:groups/acl:group[acl:identity=current()]/acl:type='basic' or /acl:acl/acl:groups/acl:group[acl:identity=current()]/acl:type='advance'"/>
              <description>
                <text>Policy for filtering multicast entries based on source addresses or based on both source and group addresses.</text>
              </description>
            </leaf>
            <leaf name="source-life-time">
              <type name="uint32">
                <range value="60..65535"/>
              </type>
              <units name="s"/>
              <default value="210"/>
              <description>
                <text>Timeout period for (S, G) entries on a router.</text>
              </description>
            </leaf>
            <leaf name="spt-switch-check-interval">
              <type name="uint32">
                <range value="15..65535"/>
              </type>
              <units name="s"/>
              <default value="15"/>
              <description>
                <text>Interval for checking the multicast message rate is configured. When the multicast message rate reaches the specified threshold, a switchover from the RPT to the SPT is performed. The value is an integer ranging from 15 to 65535, in seconds.</text>
              </description>
            </leaf>
            <leaf name="packaging-jp-disable">
              <type name="boolean"/>
              <default value="false"/>
              <description>
                <text>Enable/disable the Join/Prune message packaging function. The efficiency for sending PIM Join/Prune messages in a package is higher than that for separately sending a large number of PIM Join/Prune messages. By default, a device sends PIM Join/Prune messages in a package.</text>
              </description>
            </leaf>
            <leaf name="register-source-interface">
              <when condition="../type='ipv4unicast'"/>
              <type name="leafref">
                <path value="/ifm:ifm/ifm:interfaces/ifm:interface/ifm:name"/>
              </type>
              <must condition="../../../../../ni:name=/ifm:ifm/ifm:interfaces/ifm:interface[ifm:name=current()]/ifm:vrf-name"/>
              <description>
                <text>Source address of register packets.</text>
              </description>
            </leaf>
            <leaf name="state-refresh-interval">
              <when condition="../../../../../ni:name='_public_' and ../type='ipv4unicast'"/>
              <type name="uint32">
                <range value="1..255"/>
              </type>
              <units name="s"/>
              <default value="60"/>
              <description>
                <text>The state-refresh-interval command sets the interval for sending PIM State-Refresh messages.</text>
              </description>
            </leaf>
            <leaf name="state-refresh-rate-limit">
              <when condition="../../../../../ni:name='_public_' and ../type='ipv4unicast'"/>
              <type name="uint32">
                <range value="1..65535"/>
              </type>
              <units name="s"/>
              <default value="30"/>
              <description>
                <text>The state-refresh-rate-limit command sets the minimum period to wait before receiving the next PIM State-Refresh message.</text>
              </description>
            </leaf>
            <leaf name="state-refresh-ttl">
              <when condition="../../../../../ni:name='_public_' and ../type='ipv4unicast'"/>
              <type name="uint32">
                <range value="1..255"/>
              </type>
              <default value="255"/>
              <description>
                <text>The state-refresh-ttl command sets the TTL value for sending PIM State-Refresh messages.</text>
              </description>
            </leaf>
            <leaf name="fast-reroute">
              <when condition="../type='ipv4unicast'"/>
              <type name="boolean"/>
              <default value="false"/>
              <description>
                <text>Enable/disable PIM fast reroute.</text>
              </description>
            </leaf>
            <leaf name="hello-join-attr-enable">
              <when condition="../type='ipv4unicast'"/>
              <type name="boolean"/>
              <default value="false"/>
              <description>
                <text>Enable/disable all PIM interfaces to send PIM Hello messages that carry the join attribute.</text>
              </description>
            </leaf>
            <leaf name="clone-inherit-enable">
              <when condition="../type='ipv4unicast'"/>
              <type name="boolean"/>
              <must condition="../clone-inherit-enable='false' or ../fast-reroute='true'"/>
              <default value="false"/>
              <description>
                <text>Enable/disable cloned PIM (S, G) entries to inherit the outbound interfaces of (*, G) entries.</text>
              </description>
            </leaf>
            <leaf name="gen-id-chang-enable">
              <when condition="../type='ipv4unicast'"/>
              <type name="boolean"/>
              <default value="false"/>
              <description>
                <text>Enable/disable hello nsr genid-change.</text>
              </description>
            </leaf>
            <leaf name="rpf-prune-holdtime">
              <when condition="../type='ipv4unicast'"/>
              <type name="uint16">
                <range value="1..300"/>
              </type>
              <units name="s"/>
              <description>
                <text>Set a delay in sending Prune messages to upstream devices.</text>
              </description>
            </leaf>
            <leaf name="rpf-prune-policy-ipv4">
              <when condition="../rpf-prune-holdtime"/>
              <type name="leafref">
                <path value="/acl:acl/acl:groups/acl:group/acl:identity"/>
              </type>
              <must condition="/acl:acl/acl:groups/acl:group[acl:identity=current()]/acl:type='basic' or /acl:acl/acl:groups/acl:group[acl:identity=current()]/acl:type='advance'"/>
              <description>
                <text>Indicate a matching policy for multicast entries to be pruned.</text>
              </description>
            </leaf>
            <leaf name="is-backup-rpf-switch-flow-based">
              <when condition="../type='ipv4unicast'"/>
              <type name="boolean"/>
              <default value="false"/>
              <description>
                <text>Enable/disable the function of allowing new interfaces connected to backup upstream devices to generate forwarding entries only after they receive multicast traffic from the backup upstream devices.</text>
              </description>
            </leaf>
            <container name="spt-switchs">
              <when condition="../type='ipv4unicast'"/>
              <description>
                <text>List of SPT switchover configurations.</text>
              </description>
              <container name="spt-switch-no-acl">
                <description>
                  <text>Configure SPT switchover configuration.</text>
                </description>
                <leaf name="spt-threshold-no-acl">
                  <type name="uint32">
                    <range value="1..4194304 | 4294967295"/>
                    <ext:value-meaning>
                      <ext:item value="4294967295">
                        <ext:meaning value="infinity"/>
                      </ext:item>
                    </ext:value-meaning>
                  </type>
                  <units name="kbit/s"/>
                  <description>
                    <text>Multicast packet rate threshold that triggers the switchover from the RPT to the SPT. this threshold for all group that doesn't match any group policy.</text>
                  </description>
                </leaf>
              </container>
              <list name="spt-switch">
                <key value="group-policy"/>
                <ordered-by value="user"/>
                <description>
                  <text>Configure SPT switchover configuration.</text>
                </description>
                <leaf name="group-policy">
                  <type name="leafref">
                    <path value="/acl:acl/acl:groups/acl:group/acl:identity"/>
                  </type>
                  <must condition="/acl:acl/acl:groups/acl:group[acl:identity=current()]/acl:type='basic'"/>
                  <description>
                    <text>Name of a named ACL. If the parameter is not set, the threshold is applied to all multicast groups.</text>
                  </description>
                </leaf>
                <leaf name="spt-threshold">
                  <type name="uint32">
                    <range value="1..4194304 | 4294967295"/>
                    <ext:value-meaning>
                      <ext:item value="4294967295">
                        <ext:meaning value="infinity"/>
                      </ext:item>
                    </ext:value-meaning>
                  </type>
                  <units name="kbit/s"/>
                  <mandatory value="true"/>
                  <description>
                    <text>Multicast packet rate threshold that triggers the switchover from the RPT to the SPT.</text>
                  </description>
                </leaf>
              </list>
            </container>
            <container name="anycast-rps">
              <description>
                <text>List of Anycast-RPs.</text>
              </description>
              <list name="anycast-rp">
                <key value="rp-addr"/>
                <max-elements value="4"/>
                <description>
                  <text>Configure Anycast-RP.</text>
                </description>
                <leaf name="rp-addr">
                  <type name="inet:ip-address-no-zone"/>
                  <description>
                    <text>Address of an Anycast-RP. This address must be a valid unicast IP address.
In IPv4, the value is in dotted decimal notation. This address cannot be an address on the network segment 127.0.0.0/8.
In IPv6, the value is in hexadecimal notation and in the format of FFxx:xxxx:xxxx::xxxx.</text>
                  </description>
                </leaf>
                <leaf name="local-addr">
                  <type name="inet:ip-address-no-zone"/>
                  <description>
                    <text>The Anycast-RP address is a local Address. This address must be a valid unicast IP address.
In IPv4, the value is in dotted decimal notation. This address cannot be an address on the network segment 127.0.0.0/8.
In IPv6, the value is in hexadecimal notation and in the format of FFxx:xxxx:xxxx::xxxx.</text>
                  </description>
                </leaf>
                <container name="rp-peers">
                  <description>
                    <text>List of Anycast-RP peers.</text>
                  </description>
                  <list name="rp-peer">
                    <key value="peer-ip-addr"/>
                    <max-elements value="16"/>
                    <description>
                      <text>Configure Anycast-RP peer.</text>
                    </description>
                    <leaf name="peer-ip-addr">
                      <type name="inet:ip-address-no-zone"/>
                      <description>
                        <text>Address of an Anycast-RP peer. This address must be a valid unicast IP address.
In IPv4, the value is in dotted decimal notation. This address cannot be an address on the network segment 127.0.0.0/8.
In IPv6, the value is in hexadecimal notation and in the format of FFxx:xxxx:xxxx::xxxx.</text>
                      </description>
                    </leaf>
                    <leaf name="forward-sa-msg">
                      <when condition="../../../../../type='ipv4unicast'"/>
                      <type name="boolean"/>
                      <default value="false"/>
                      <description>
                        <text>Enable/disable the function of forwarding to Anycast-RP peers source information learned through SA messages.</text>
                      </description>
                    </leaf>
                    <leaf name="forward-policy">
                      <when condition="../forward-sa-msg='true'"/>
                      <type name="leafref">
                        <path value="/acl:acl/acl:groups/acl:group/acl:identity"/>
                      </type>
                      <must condition="/acl:acl/acl:groups/acl:group[acl:identity=current()]/acl:type='basic'"/>
                      <description>
                        <text>Policy for filtering SA messages to be forwarded to Anycast-RP peers.</text>
                      </description>
                    </leaf>
                  </list>
                </container>
              </list>
            </container>
            <container name="p2mp">
              <when condition="../type='ipv4unicast'"/>
              <description>
                <text>Configure P2MP configuration.</text>
              </description>
              <leaf name="rpf-disable">
                <type name="boolean"/>
                <default value="false"/>
                <description>
                  <text>Enable/disable the RPF check for multicast entries. A policy can be configured to limit the range of multicast entries for which the RPF check is disabled. If a policy is not configured, the RPF check is disabled for all multicast entries. By default, the RPF check is enabled for all multicast entries.</text>
                </description>
              </leaf>
              <leaf name="rpf-disable-policy">
                <when condition="../rpf-disable='true'"/>
                <type name="leafref">
                  <path value="/acl:acl/acl:groups/acl:group/acl:identity"/>
                </type>
                <must condition="/acl:acl/acl:groups/acl:group[acl:identity=current()]/acl:type='advance'"/>
                <description>
                  <text>Policy for limiting the range of multicast entries for which the RPF check is disabled.</text>
                </description>
              </leaf>
              <leaf name="source-proxy-enable">
                <type name="boolean"/>
                <default value="false"/>
                <description>
                  <text>Enable/disable multicast source proxy.</text>
                </description>
              </leaf>
              <leaf name="source-proxy-policy">
                <when condition="../source-proxy-enable='true'"/>
                <type name="leafref">
                  <path value="/acl:acl/acl:groups/acl:group/acl:identity"/>
                </type>
                <must condition="/acl:acl/acl:groups/acl:group[acl:identity=current()]/acl:type='advance'"/>
                <description>
                  <text>Policy for limiting the range of multicast entries for which multicast source proxy is enabled.</text>
                </description>
              </leaf>
            </container>
            <container name="vector">
              <when condition="../type='ipv4unicast'"/>
              <description>
                <text>Configure Vector configuration.</text>
              </description>
              <leaf name="compatible-standard-enable">
                <type name="boolean"/>
                <default value="false"/>
                <description>
                  <text>Enable/disable a device to use the standard format defined in a relevant RFC to encapsulate Hello or Join/Prune messages carrying vector attributes. By default, a private format is used to encapsulate Hello or Join/Prune messages carrying vector attributes.</text>
                </description>
              </leaf>
            </container>
          </list>
        </container>
        <container name="interfaces">
          <description>
            <text>List of interface configurations.</text>
          </description>
          <list name="interface">
            <key value="type if-name"/>
            <description>
              <text>Configure PIM configuration on the interface.</text>
            </description>
            <leaf name="type">
              <type name="addr-family-type"/>
              <description>
                <text>Address family, which determines whether an address belongs to IPv4 or IPv6.</text>
              </description>
            </leaf>
            <leaf name="if-name">
              <type name="leafref">
                <path value="/ifm:ifm/ifm:interfaces/ifm:interface/ifm:name"/>
              </type>
              <must condition="../../../../../ni:name=/ifm:ifm/ifm:interfaces/ifm:interface[ifm:name=current()]/ifm:vrf-name"/>
              <description>
                <text>Interface name.</text>
              </description>
            </leaf>
            <leaf name="sm-enable">
              <type name="boolean"/>
              <default value="false"/>
              <description>
                <text>Enable/disable PIM on an interface.</text>
              </description>
            </leaf>
            <leaf name="dr-priority">
              <type name="uint32">
                <range value="0..4294967295"/>
              </type>
              <default value="1"/>
              <description>
                <text>DR priority. The greater the value, the higher the priority.</text>
              </description>
            </leaf>
            <leaf name="hello-interval">
              <type name="uint32">
                <range value="1..18000"/>
              </type>
              <units name="s"/>
              <default value="30"/>
              <description>
                <text>Interval at which Hello messages are sent. It must be shorter than the timeout period of PIM neighbors.</text>
              </description>
            </leaf>
            <leaf name="hello-holdtime">
              <type name="uint16">
                <range value="1..65535"/>
              </type>
              <units name="s"/>
              <default value="105"/>
              <description>
                <text>Timeout period during which a router waits for Hello messages sent from its PIM neighbors. It must be longer than the interval for PIM neighbors to send Hello messages.</text>
              </description>
            </leaf>
            <leaf name="hello-override">
              <type name="uint16">
                <range value="1..65535"/>
              </type>
              <units name="ms"/>
              <default value="2500"/>
              <description>
                <text>Interval at which the prune action in a Hello message is overridden.</text>
              </description>
            </leaf>
            <leaf name="hello-lan-delay">
              <type name="uint16">
                <range value="1..32767"/>
              </type>
              <units name="ms"/>
              <default value="500"/>
