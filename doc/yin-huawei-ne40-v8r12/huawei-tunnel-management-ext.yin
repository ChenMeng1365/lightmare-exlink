<?xml version="1.0" encoding="UTF-8"?>
<module name="huawei-tunnel-management-ext"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:tnlm-ext="urn:huawei:yang:huawei-tunnel-management-ext"
        xmlns:ext="urn:huawei:yang:huawei-extension"
        xmlns:tnlm="urn:huawei:yang:huawei-tunnel-management"
        xmlns:ni="urn:huawei:yang:huawei-network-instance"
        xmlns:srpolicy="urn:huawei:yang:huawei-srpolicy"
        xmlns:sr="urn:huawei:yang:huawei-segment-routing">
  <yang-version value="1"/>
  <namespace uri="urn:huawei:yang:huawei-tunnel-management-ext"/>
  <prefix value="tnlm-ext"/>
  <import module="huawei-extension">
    <prefix value="ext"/>
  </import>
  <import module="huawei-tunnel-management">
    <prefix value="tnlm"/>
  </import>
  <import module="huawei-network-instance">
    <prefix value="ni"/>
  </import>
  <import module="huawei-srpolicy">
    <prefix value="srpolicy"/>
  </import>
  <import module="huawei-segment-routing">
    <prefix value="sr"/>
  </import>
  <organization>
    <text>Huawei Technologies Co., Ltd.</text>
  </organization>
  <contact>
    <text>Huawei Industrial Base
Bantian, Longgang
Shenzhen 518129
People's Republic of China
Website: http://www.huawei.com
Email: support@huawei.com</text>
  </contact>
  <description>
    <text>Tunnel management, including management on tunnel policies and tunnel selectors.</text>
  </description>
  <revision date="2020-02-24">
    <description>
      <text>Initial revision.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <ext:task-name value="tunnel"/>
  <augment target-node="/tnlm:tunnel-management/tnlm:tunnel-policys/tnlm:tunnel-policy/tnlm:ipv4-set/tnlm:policy-type/tnlm:binding/tnlm:nexthops/tnlm:nexthop">
    <description>
      <text>Augment the huawei-tunnel-management-ext model to add tunnel-policy configuration.</text>
    </description>
    <leaf name="group-id">
      <when condition="../tnlm:bind-type='sr-te-policy-group'"/>
      <type name="leafref">
        <path value="/ni:network-instance/ni:instances/ni:instance/sr:segment-routing/srpolicy:srpolicy/srpolicy:policy-groups/srpolicy:policy-group/srpolicy:index"/>
      </type>
      <description>
        <text>SR TE policy group ID.</text>
      </description>
    </leaf>
  </augment>
</module>
