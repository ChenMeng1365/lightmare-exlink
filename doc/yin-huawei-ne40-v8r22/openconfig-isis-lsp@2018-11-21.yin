    <uses name="isis-lsdb-prefix-state"/>
  </grouping>
  <grouping name="ipv4-mt-prefix-extended-state">
    <description>
      <text>State parameters that relate to an IPv4 prefix in a
multi-topology context.</text>
    </description>
    <container name="state">
      <description>
        <text>State parameters of an IPv4 extended prefix.</text>
      </description>
      <uses name="ipv4-prefix-extended-params-state"/>
