      <container name="briefs">
        <description>
          <text>Show the brief information of sctp connections</text>
        </description>
        <presence value="Show sctp brief"/>
        <list name="brief">
          <description>
            <text>Show brief information of sctp connections</text>
          </description>
          <key value="association-id"/>
          <leaf name="association-id">
            <description>
              <text>Index of association id</text>
            </description>
            <type name="uint32">
              <range value="0..4294967295"/>
            </type>
          </leaf>
          <leaf name="local-address">
            <description>
              <text>Local ip address and port</text>
            </description>
            <type name="string">
              <length value="1..50"/>
            </type>
          </leaf>
          <leaf name="remote-address">
            <description>
              <text>Remote ip address and port</text>
            </description>
            <type name="string">
              <length value="1..50"/>
            </type>
          </leaf>
          <leaf name="state">
            <description>
              <text>State of a SCTP connection</text>
            </description>
            <type name="string">
              <length value="1..20"/>
            </type>
          </leaf>
        </list>
      </container>
      <container name="tcps">
        <description>
          <text>Show tcb information of all sctp connections</text>
        </description>
        <list name="tcp">
          <description>
            <text>Show the detail information of all sctp connections or a specific sctp connection</text>
          </description>
          <key value="association-id"/>
          <leaf name="association-id">
            <description>
              <text>Index of association id</text>
            </description>
            <type name="uint32">
              <range value="0..4294967295"/>
            </type>
          </leaf>
          <leaf name="state">
            <description>
              <text>State of association</text>
            </description>
            <type name="string">
              <length value="1..20"/>
            </type>
          </leaf>
          <leaf name="check-sum">
            <description>
              <text>Type of checksum</text>
            </description>
            <type name="uint8">
              <range value="0..255"/>
            </type>
          </leaf>
          <leaf name="local-address">
            <description>
              <text>Local ip address,port and vpnid</text>
            </description>
            <type name="string">
