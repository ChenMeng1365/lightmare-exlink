<?xml version="1.0" encoding="UTF-8"?>
<module name="huawei-ftps"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:ftps="urn:huawei:yang:huawei-ftps"
        xmlns:pub-type="urn:huawei:yang:huawei-pub-type"
        xmlns:ni="urn:huawei:yang:huawei-network-instance"
        xmlns:inet="urn:ietf:params:xml:ns:yang:ietf-inet-types"
        xmlns:ext="urn:huawei:yang:huawei-extension"
        xmlns:acl="urn:huawei:yang:huawei-acl">
  <namespace uri="urn:huawei:yang:huawei-ftps"/>
  <prefix value="ftps"/>
  <import module="huawei-pub-type">
    <prefix value="pub-type"/>
  </import>
  <import module="huawei-network-instance">
    <prefix value="ni"/>
  </import>
  <import module="ietf-inet-types">
    <prefix value="inet"/>
  </import>
  <import module="huawei-extension">
    <prefix value="ext"/>
  </import>
  <import module="huawei-acl">
    <prefix value="acl"/>
  </import>
  <organization>
    <text>Huawei Technologies Co., Ltd.</text>
  </organization>
  <contact>
    <text>Huawei Industrial Base
Bantian, Longgang
Shenzhen 518129
People's Republic of China
Website: http://www.huawei.com
Email: support@huawei.com</text>
  </contact>
  <description>
    <text>FTP server component.</text>
  </description>
  <revision date="2019-07-01">
    <description>
      <text>Initial revision.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <ext:task-name value="ftp-server"/>
  <typedef name="ftps-bool-type">
    <type name="enumeration">
      <enum name="disable">
        <value value="0"/>
        <description>
          <text>Disable the FTP server service.</text>
        </description>
      </enum>
      <enum name="enable">
        <value value="1"/>
        <description>
          <text>Enable the FTP server service.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>FTP server bool datatype for ftps server.</text>
    </description>
  </typedef>
  <container name="ftps">
    <description>
      <text>FTP server component.</text>
    </description>
    <container name="server">
      <description>
        <text>Configure FTP server statistics.</text>
      </description>
      <leaf name="server-enable">
        <type name="ftps-bool-type"/>
        <default value="disable"/>
        <description>
          <text>Whether to enable the FTP server service.</text>
        </description>
      </leaf>
      <leaf name="ipv6-server-enable">
        <type name="ftps-bool-type"/>
        <default value="disable"/>
        <description>
          <text>Whether to enable the IPv6 FTP server service.</text>
        </description>
      </leaf>
      <leaf name="listen-port-number">
        <type name="uint32">
          <range value="21..65535"/>
        </type>
        <default value="21"/>
        <description>
          <text>Listening port of the FTP server.</text>
        </description>
      </leaf>
      <leaf name="ipv6-listen-port-number">
        <type name="uint32">
          <range value="21..65535"/>
        </type>
        <default value="21"/>
        <description>
          <text>Listening port of the IPv6 FTP server.</text>
        </description>
      </leaf>
      <leaf name="idle-timeout">
        <type name="uint32">
          <range value="1..35791"/>
        </type>
        <units name="min"/>
        <default value="10"/>
        <description>
          <text>Users can set the timeout period for an idle client to connect to the FTP server.</text>
        </description>
      </leaf>
      <leaf name="ipv6-idle-timeout">
        <type name="uint32">
          <range value="1..35791"/>
        </type>
        <units name="min"/>
        <default value="10"/>
        <description>
          <text>Users can set the timeout period for an idle client to connect to the IPv6 FTP server.</text>
        </description>
      </leaf>
      <leaf name="default-dir">
        <type name="string">
          <length value="1..255"/>
        </type>
        <description>
          <text>When no root directory is configured for RADIUS/TACACS users, they can log in to the FTP server using the default root directory.</text>
        </description>
      </leaf>
      <leaf name="acl4-name-or-number">
        <type name="leafref">
          <path value="/acl:acl/acl:groups/acl:group/acl:identity"/>
        </type>
        <must condition="/acl:acl/acl:groups/acl:group[acl:identity=current()]/acl:type='basic' or /acl:acl/acl:groups/acl:group[acl:identity=current()]/acl:type='advance'"/>
        <description>
          <text>Server ACL name or number for IPv4 connections.</text>
        </description>
      </leaf>
      <leaf name="acl6-name-or-number">
        <type name="leafref">
          <path value="/acl:acl/acl:group6s/acl:group6/acl:identity"/>
        </type>
        <must condition="/acl:acl/acl:group6s/acl:group6[acl:identity=current()]/acl:type='basic' or /acl:acl/acl:group6s/acl:group6[acl:identity=current()]/acl:type='advance'"/>
        <description>
          <text>Server ACL name or number for IPv6 connections.</text>
        </description>
      </leaf>
      <leaf name="server-max-sessions">
        <type name="uint32">
          <range value="0..15"/>
        </type>
        <default value="15"/>
        <description>
          <text>User can configure maximum number of clients can be connected to server at any point of time for FTP service.</text>
        </description>
      </leaf>
    </container>
    <container name="users-status">
      <config value="false"/>
      <description>
        <text>FTP server configuration statistics.</text>
      </description>
      <leaf name="current-number-of-users">
        <type name="uint32"/>
        <config value="false"/>
        <description>
          <text>Number of FTP clients connected.</text>
        </description>
      </leaf>
      <leaf name="max-number-of-users">
        <type name="uint32"/>
        <config value="false"/>
        <description>
          <text>Maximum number of FTP clients.</text>
        </description>
      </leaf>
    </container>
    <container name="ip-block">
      <description>
        <text>Configure FTP IP block.</text>
      </description>
      <leaf name="ip-block-enable">
        <type name="ftps-bool-type"/>
        <default value="enable"/>
        <description>
          <text>Whether to enable IP block function.</text>
        </description>
      </leaf>
      <leaf name="ip-block-failed-times">
        <type name="uint32">
          <range value="1..10"/>
        </type>
        <default value="6"/>
        <description>
          <text>IP block continuous authentication failures.</text>
        </description>
      </leaf>
      <leaf name="ip-block-period">
        <type name="uint32">
          <range value="1..120"/>
        </type>
        <units name="min"/>
        <default value="5"/>
        <description>
          <text>IP block continuous authentication failure time period.</text>
        </description>
      </leaf>
      <leaf name="ip-block-react-period">
        <type name="uint32">
          <range value="1..1000"/>
        </type>
        <units name="min"/>
        <default value="5"/>
        <description>
          <text>IP block automatically unlock time.</text>
        </description>
      </leaf>
    </container>
    <container name="login-fail-alarm">
      <description>
        <text>Configure login failed frequently.</text>
      </description>
      <leaf name="upper-limit">
        <type name="uint32">
          <range value="0..100"/>
        </type>
        <must condition="../upper-limit &gt;= ../lower-limit"/>
        <default value="30"/>
        <description>
          <text>Login failed alarm report times.</text>
        </description>
      </leaf>
      <leaf name="lower-limit">
        <type name="uint32">
          <range value="0..45"/>
        </type>
        <default value="20"/>
        <description>
          <text>Login failed alarm resume times.</text>
        </description>
      </leaf>
      <leaf name="period">
        <type name="uint32">
          <range value="1..120"/>
        </type>
        <units name="min"/>
        <default value="5"/>
        <description>
          <text>Login failed alarm period.</text>
        </description>
      </leaf>
    </container>
    <container name="ipv4-addr-sources">
      <description>
        <text>List of FTP IPv4 server source address.</text>
      </description>
      <list name="ipv4-addr-source">
        <key value="source-ipv4-address"/>
        <max-elements value="5"/>
        <description>
          <text>Configure FTP IPv4 server source address.</text>
        </description>
        <leaf name="source-ipv4-address">
          <type name="inet:ipv4-address-no-zone"/>
          <description>
            <text>FTP IPv4 server source address.</text>
          </description>
        </leaf>
      </list>
    </container>
    <container name="ipv4-source-interfaces">
      <description>
        <text>List of specify the source interface.</text>
      </description>
      <list name="ipv4-source-interface">
        <key value="src-interface"/>
        <max-elements value="5"/>
        <description>
          <text>Configure specify the source interface.</text>
        </description>
        <leaf name="src-interface">
          <type name="pub-type:if-name"/>
          <description>
            <text>Specify the source interface name. The specified source interface must exist and an IP address must be configured on the interface.</text>
          </description>
        </leaf>
      </list>
    </container>
    <container name="ipv4-isolate-server-sources">
      <description>
        <text>List of the isolate source interface and source address.</text>
      </description>
      <list name="ipv4-isolate-server-source">
        <key value="src-interface src-ipv4-addr"/>
        <max-elements value="5"/>
        <description>
          <text>Configure the isolate source interface and source address.</text>
        </description>
        <leaf name="src-interface">
          <type name="pub-type:if-name"/>
          <must condition="count(/ftps/ipv4-source-interfaces/ipv4-source-interface[src-interface = current()]) = 0"/>
          <description>
            <text>Specify the source interface name.</text>
          </description>
        </leaf>
        <leaf name="src-ipv4-addr">
          <type name="inet:ipv4-address-no-zone"/>
          <description>
            <text>Specify the source IPv4 address.</text>
          </description>
        </leaf>
      </list>
    </container>
    <container name="ipv6-server-sources">
      <description>
        <text>List of FTP IPv6 server source address.</text>
      </description>
      <list name="ipv6-server-source">
        <key value="src-ipv6-addr"/>
        <max-elements value="5"/>
        <description>
          <text>Configure FTP IPv6 server source address.</text>
        </description>
        <leaf name="src-ipv6-addr">
          <type name="inet:ipv6-address-no-zone"/>
          <description>
            <text>FTP IPv6 server source IP address.</text>
          </description>
        </leaf>
        <leaf name="src-ipv6-vpn-name">
          <type name="string">
            <length value="1..31"/>
          </type>
          <description>
            <text>Specify the source VPN instance. The specified VPN instance must exist and IPv6-family must be enabled in the VPN instance.</text>
          </description>
          <ext:support-filter value="true"/>
        </leaf>
      </list>
    </container>
    <container name="ipv6-isolate-server-sources">
      <description>
        <text>List of the isolate source interface and source IPv6 address.</text>
      </description>
      <list name="ipv6-isolate-server-source">
        <key value="src-interface src-ipv6-addr"/>
        <max-elements value="5"/>
        <description>
          <text>Configure the isolate source interface and source IPv6 address.</text>
        </description>
        <leaf name="src-interface">
          <type name="pub-type:if-name"/>
          <description>
            <text>Specify the source interface name.</text>
          </description>
        </leaf>
        <leaf name="src-ipv6-addr">
          <type name="inet:ipv6-address-no-zone"/>
          <must condition="count(/ftps/ipv6-server-sources/ipv6-server-source[src-ipv6-addr = current()]) = 0"/>
          <description>
            <text>Specify the source IPv6 address.</text>
          </description>
        </leaf>
      </list>
    </container>
    <container name="all-server-source">
      <description>
        <text>Configure specify all interfaces.</text>
      </description>
      <leaf name="all-ipv4-interface">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable specify all IPv4 interfaces.</text>
        </description>
      </leaf>
      <leaf name="all-ipv6-interface">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable specify all IPv6 interfaces.</text>
        </description>
      </leaf>
    </container>
    <container name="session-car">
      <must condition="cir-interval&lt;=pir-interval"/>
      <must condition="cbs-interval&lt;=pbs-interval"/>
      <description>
        <text>Configure FTP server session CAR.</text>
      </description>
      <leaf name="enable">
        <type name="boolean"/>
        <default value="true"/>
        <description>
          <text>Enable/disable session CAR.</text>
        </description>
      </leaf>
      <leaf name="cir-interval">
        <type name="uint32">
          <range value="1..1000000"/>
        </type>
        <units name="kbit/s"/>
        <default value="100"/>
        <description>
          <text>Specify the session CAR CIR interval.</text>
        </description>
      </leaf>
      <leaf name="cbs-interval">
        <type name="uint32">
          <range value="1..10000000"/>
        </type>
        <units name="bit/s"/>
        <default value="3000000"/>
        <description>
          <text>Specify the session CAR CBS interval.</text>
        </description>
      </leaf>
      <leaf name="pir-interval">
        <type name="uint32">
          <range value="1..1000000"/>
        </type>
        <units name="kbit/s"/>
        <default value="4000"/>
        <description>
          <text>Specify the session CAR PIR interval.</text>
        </description>
      </leaf>
      <leaf name="pbs-interval">
        <type name="uint32">
          <range value="1..10000000"/>
        </type>
        <units name="bit/s"/>
        <default value="9000000"/>
        <description>
          <text>Specify the session CAR PBS interval.</text>
        </description>
      </leaf>
    </container>
  </container>
  <rpc name="activate-ip">
    <description>
      <text>Activate IP.</text>
    </description>
    <input>
      <leaf name="ip-address">
        <type name="inet:ip-address-no-zone"/>
        <mandatory value="true"/>
        <description>
          <text>IP address.</text>
        </description>
      </leaf>
      <leaf name="vpn-name">
        <type name="leafref">
          <path value="/ni:network-instance/ni:instances/ni:instance/ni:name"/>
        </type>
        <description>
          <text>VPN name.</text>
        </description>
      </leaf>
    </input>
  </rpc>
</module>
