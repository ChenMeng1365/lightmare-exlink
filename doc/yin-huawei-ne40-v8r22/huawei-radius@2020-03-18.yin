          <description>
            <text>The number of attribute in DM acknowledge packet.</text>
          </description>
        </leaf>
      </list>
    </container>
    <container name="authen-packet-statistics-ipv6s">
      <config value="false"/>
      <description>
        <text>List of authentication packet statistics on the RADIUS server with a specified IPv6 address.</text>
      </description>
      <list name="authen-packet-stat-ipv6">
        <key value="ipv6-address port"/>
        <config value="false"/>
        <description>
          <text>Statistics of authentication packets on the RADIUS server with a specified IPv6 address.</text>
        </description>
        <leaf name="ipv6-address">
          <type name="inet:ipv6-address-no-zone"/>
          <description>
            <text>Radius server IP.</text>
          </description>
        </leaf>
        <leaf name="port">
          <type name="uint16">
            <range value="1..65535"/>
          </type>
          <description>
            <text>Port number of Radius Server.</text>
          </description>
        </leaf>
        <uses name="radius-authen-packet-statistics-info"/>
      </list>
    </container>
    <container name="account-packet-statistics-ipv6s">
      <config value="false"/>
      <description>
        <text>List of accounting packet statistics on the RADIUS server with a specified IPv6 address.</text>
      </description>
      <list name="account-packet-statistics-ipv6">
        <key value="ipv6-address port"/>
        <description>
          <text>Statistics of accounting packets on the RADIUS server with a specified IPv6 address.</text>
        </description>
        <leaf name="ipv6-address">
          <type name="inet:ipv6-address-no-zone"/>
          <description>
            <text>IP address of Radius server.</text>
          </description>
        </leaf>
        <leaf name="port">
          <type name="uint16">
            <range value="1..65535"/>
          </type>
          <description>
            <text>Port number of Radius server.</text>
          </description>
        </leaf>
        <uses name="radius-account-packet-statistics-type"/>
      </list>
    </container>
    <container name="aaa-route-download-status">
      <config value="false"/>
      <description>
        <text>Statistics of the status of static routes delivered by an AAA server.</text>
      </description>
      <leaf name="status">
        <type name="string">
          <length value="1..99"/>
        </type>
        <description>
          <text>Current route update status.</text>
        </description>
      </leaf>
      <leaf name="last-download-attempt">
        <type name="string">
          <length value="1..99"/>
        </type>
        <description>
          <text>Time when the last route synchronization request was sent.</text>
        </description>
      </leaf>
      <leaf name="last-download-success">
        <type name="string">
          <length value="1..99"/>
        </type>
        <description>
          <text>Time when the last route synchronization succeeded.</text>
        </description>
      </leaf>
      <leaf name="next-scheduled-download">
        <type name="string">
          <length value="1..99"/>
        </type>
        <description>
          <text>Time of the next route synchronization.</text>
        </description>
      </leaf>
      <leaf name="is-route-advertise-delay">
        <type name="string">
          <length value="1..99"/>
        </type>
        <description>
          <text>Whether the route is waiting for advertisement.</text>
        </description>
      </leaf>
      <leaf name="route-advertise-delay-time">
        <type name="string">
          <length value="1..99"/>
        </type>
        <description>
          <text>Remaining time before routes are advertised.</text>
        </description>
      </leaf>
    </container>
    <container name="aaa-static-routes">
      <config value="false"/>
      <description>
        <text>List of Radius server static routes.</text>
      </description>
      <list name="aaa-static-route">
        <key value="vpn-name ip-address"/>
        <description>
          <text>Statistics of Radius server static routes.</text>
        </description>
        <leaf name="vpn-name">
          <type name="string">
            <length value="1..31"/>
          </type>
          <description>
            <text>VPN Name.</text>
          </description>
        </leaf>
        <leaf name="ip-address">
          <type name="string">
            <length value="1..99"/>
          </type>
          <description>
            <text>IP and length of IP mask.</text>
          </description>
        </leaf>
        <leaf name="is-installed">
          <type name="bras-radius-aaa-route-infos-type"/>
          <description>
            <text>Is route installed.</text>
          </description>
          <ext:support-filter value="true"/>
        </leaf>
        <leaf name="next-hop">
          <type name="string">
            <length value="1..99"/>
          </type>
          <description>
            <text>Next hop.</text>
          </description>
        </leaf>
        <leaf name="cost">
          <type name="uint32">
            <range value="0..65535"/>
          </type>
          <description>
            <text>Routing weights.</text>
          </description>
        </leaf>
        <leaf name="interface-name">
          <type name="string">
            <length value="1..99"/>
          </type>
          <description>
            <text>Interface name.</text>
          </description>
        </leaf>
        <leaf name="tag">
          <type name="uint32">
            <range value="0..65535"/>
          </type>
          <description>
            <text>Routing tag.</text>
          </description>
        </leaf>
        <leaf name="is-advertised">
          <type name="bras-radius-aaa-route-infos-type"/>
          <description>
            <text>Is route advertised.</text>
          </description>
        </leaf>
        <leaf name="is-rolled">
          <type name="bras-radius-aaa-route-infos-type"/>
          <description>
            <text>Is route rolled back to pre-sync state, under the radius fault scenario.</text>
          </description>
        </leaf>
      </list>
    </container>
    <container name="auth-client">
      <config value="false"/>
      <description>
        <text>Statistics of RADIUS authentication client.</text>
      </description>
      <leaf name="invalid-server-packets">
        <type name="uint32"/>
        <description>
          <text>The number of RADIUS Access-Response packets received from unknown addresses.</text>
        </description>
      </leaf>
      <leaf name="identifier">
        <type name="string">
          <length value="1..255"/>
        </type>
        <description>
          <text>The NAS-Identifier of the RADIUS authentication client.</text>
        </description>
      </leaf>
    </container>
    <container name="accounting-client">
      <config value="false"/>
      <description>
        <text>Statistics of RADIUS accounting client.</text>
      </description>
      <leaf name="response-packets">
        <type name="uint32"/>
        <description>
          <text>The number of RADIUS Accounting-Response packets received from unknown addresses.</text>
        </description>
      </leaf>
      <leaf name="identifier">
        <type name="string">
          <length value="1..255"/>
        </type>
        <description>
          <text>The NAS-Identifier of the RADIUS accounting client.</text>
        </description>
      </leaf>
    </container>
    <container name="attribute-global">
      <description>
        <text>Configure Radius attribute global.</text>
      </description>
      <leaf name="framed-ip-encap-mode">
        <type name="bras-radius-encap-method"/>
        <default value="version1"/>
        <description>
          <text>Indicates the mode of encapsulating the attributes of a user's IPv4 address.</text>
        </description>
      </leaf>
      <leaf name="delegated-ipv6-prefix-encap-mode">
        <type name="bras-radius-encap-method"/>
        <default value="version1"/>
        <description>
          <text>Indicates the mode of encapsulating the attributes of Delegated-IPv6-Prefix.</text>
        </description>
      </leaf>
      <leaf name="net-min-mask-limit">
        <type name="uint32">
          <range value="0..32"/>
        </type>
        <default value="0"/>
        <description>
          <text>Configure the minimum mask length for the Framed-Route attribute, 0 indicates that the device can parse the Framed-Route attribute with any mask length.</text>
        </description>
      </leaf>
      <leaf name="default-route-support">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable the device to generate a default route based on the route carried in the Framed-Route attribute delivered by the RADIUS server.</text>
        </description>
      </leaf>
      <leaf name="acct-term-sub-encap-type">
        <type name="enumeration">
          <enum name="string">
            <value value="2"/>
            <description>
              <text>The string encapsulation type of hw-acct-terminate-subcause.</text>
            </description>
          </enum>
          <enum name="integer">
            <value value="3"/>
            <description>
              <text>The integer encapsulation type of hw-acct-terminate-subcause.</text>
            </description>
          </enum>
        </type>
        <default value="integer"/>
        <description>
          <text>Configure the encapsulation format of the HW-Acct-terminate-subcause attribute.</text>
        </description>
      </leaf>
      <leaf name="coa-service-type-reauthorize">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable reauthentication for users in a domain and disable/enable password check for users, when a CoA message is used to deliver a service-type attribute value of 17.</text>
        </description>
      </leaf>
      <leaf name="nas-identifier-unlimited-length">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable the maximum length of the RADIUS attribute NAS-Identifier unlimited.</text>
        </description>
      </leaf>
      <container name="nas-port-id-sub-slots">
        <description>
          <text>List of the sub-card type in the subslot field of the NAS-Port-Id attribute.</text>
        </description>
        <list name="nas-port-id-sub-slot">
          <key value="sub-slot-id"/>
          <description>
            <text>Configure the sub-card type in the subslot field of the NAS-Port-Id attribute.</text>
          </description>
          <leaf name="sub-slot-id">
            <type name="leafref">
              <path value="/devm:devm/devm:lpu-boards/devm:lpu-board/devm:position"/>
            </type>
            <description>
              <text>Specifies the slot ID of the interface board.</text>
            </description>
          </leaf>
          <leaf name="base-number">
            <type name="uint8">
              <range value="1..24"/>
            </type>
            <mandatory value="true"/>
            <description>
              <text>Specifies the number of interfaces on the subcard.</text>
            </description>
          </leaf>
        </list>
      </container>
    </container>
    <container name="session-car-bandwidth">
      <must condition="cir&lt;=pir"/>
      <must condition="cbs&lt;=pbs"/>
      <description>
        <text>Configure RADIUS session-car.</text>
      </description>
      <leaf name="enable">
        <type name="boolean"/>
        <default value="true"/>
        <description>
          <text>Enable/disable the session-car function for RADIUS packets.</text>
        </description>
      </leaf>
      <leaf name="cir">
        <type name="uint32">
          <range value="0..1000000"/>
        </type>
        <units name="kbit/s"/>
        <default value="512"/>
        <description>
          <text>Specify committed information rate.</text>
        </description>
      </leaf>
      <leaf name="pir">
        <type name="uint32">
          <range value="0..9000000"/>
        </type>
        <units name="kbit/s"/>
        <default value="1500"/>
        <description>
          <text>Specify the peak information rate.</text>
        </description>
      </leaf>
      <leaf name="cbs">
        <type name="uint32">
          <range value="0..9000000"/>
        </type>
        <units name="Byte"/>
        <default value="125000"/>
        <description>
          <text>Specify the committed burst size.</text>
        </description>
      </leaf>
      <leaf name="pbs">
        <type name="uint32">
          <range value="0..9000000"/>
        </type>
        <units name="Byte"/>
        <default value="250000"/>
        <description>
          <text>Specify the peak burst size.</text>
        </description>
      </leaf>
    </container>
    <container name="authen-detect-recorver">
      <presence value="RADIUS authentication server state recovery."/>
      <description>
        <text>Configure detect for RADIUS authentication server state recovery.</text>
      </description>
      <uses name="radius-detect-recorver"/>
    </container>
    <container name="account-detect-recorver">
      <presence value="RADIUS accounting server state recovery."/>
      <description>
        <text>Configure detect for RADIUS accounting server state recovery.</text>
      </description>
      <uses name="radius-detect-recorver"/>
    </container>
    <container name="aaa-route-download-server">
      <presence value="Create aaa-route-download-server."/>
      <description>
        <text>Configure the device to periodically synchronize static routes delivered by a RADIUS server.</text>
      </description>
      <leaf name="server-group-name">
        <type name="leafref">
          <path value="/radius:radius/radius:radius-server-groups/radius:radius-server-group/radius:name"/>
        </type>
        <mandatory value="true"/>
        <description>
          <text>Radius server Group Name.</text>
        </description>
      </leaf>
      <leaf name="base-user-name">
        <type name="string">
          <length value="1..64"/>
        </type>
        <mandatory value="true"/>
        <description>
          <text>The user name of the BAS device requesting the route.</text>
        </description>
      </leaf>
      <leaf name="password">
        <type name="pub-type:password-extend">
          <length value="1..128"/>
        </type>
        <mandatory value="true"/>
        <description>
          <text>Password to request routing. The simple password length ranges from 1 to 16, and the cipher password length ranges from 1 to 128.</text>
        </description>
      </leaf>
      <leaf name="download-interval">
        <type name="uint32">
          <range value="1..1440"/>
        </type>
        <units name="min"/>
        <default value="720"/>
        <description>
          <text>Time interval for routing.</text>
        </description>
      </leaf>
      <leaf name="retry-interval">
        <type name="uint32">
          <range value="1..60"/>
        </type>
        <units name="min"/>
        <default value="10"/>
        <description>
          <text>Retransmission time interval.</text>
        </description>
      </leaf>
      <leaf name="retry-max-times">
        <type name="uint32">
          <range value="1..10"/>
        </type>
        <default value="1"/>
        <description>
          <text>Maximum retransmission times.</text>
        </description>
      </leaf>
      <leaf name="tag">
        <type name="uint32">
          <range value="1..65535"/>
        </type>
        <description>
          <text>Routing tag.</text>
        </description>
      </leaf>
      <leaf name="cost">
        <type name="uint32">
          <range value="1..65535"/>
        </type>
        <description>
          <text>Routing weights.</text>
        </description>
      </leaf>
      <leaf name="synchronization-time">
        <type name="pub-type:time"/>
        <description>
          <text>Daily fixed-point synchronization routing time.</text>
        </description>
      </leaf>
      <leaf name="recover-delay-time">
        <type name="uint32">
          <range value="1..120"/>
        </type>
        <units name="min"/>
        <description>
          <text>The delay-time of route release,unit is minute.</text>
        </description>
      </leaf>
    </container>
    <container name="account-packet-cache">
      <description>
        <text>Configure cache accounting packets.</text>
      </description>
      <leaf name="enable">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable cache accounting packets.</text>
        </description>
      </leaf>
    </container>
    <container name="admin-user">
      <description>
        <text>Configure the admin user info.</text>
      </description>
      <container name="user-name-exclude-domain">
        <description>
          <text>Configure the user names in the admin domain not to carry the domain name.</text>
        </description>
        <leaf name="enable">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable user names in the admin domain not to carry the domain name.</text>
          </description>
        </leaf>
      </container>
    </container>
    <container name="coa">
      <description>
        <text>Configure radius server coa packet info.</text>
      </description>
      <container name="update-user-name">
        <description>
          <text>Configure radius server coa update username.</text>
        </description>
        <leaf name="enable">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable Radius server coa update username.</text>
          </description>
        </leaf>
      </container>
    </container>
  </container>
</module>
