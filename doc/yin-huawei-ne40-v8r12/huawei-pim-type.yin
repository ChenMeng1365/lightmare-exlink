  <typedef name="bfd-status">
    <type name="enumeration">
      <enum name="admin-down">
        <value value="0"/>
        <description>
          <text>A session is in the administratively Down state because the shutdown command is run in the BFD session.</text>
        </description>
      </enum>
      <enum name="down">
        <value value="1"/>
        <description>
          <text>A session is in the Down state not because the shutdown command is run in the BFD session.</text>
        </description>
      </enum>
      <enum name="init">
        <value value="2"/>
        <description>
          <text>The local end can communicate with the remote end and wants the session status to be Up.</text>
        </description>
      </enum>
      <enum name="up">
        <value value="3"/>
        <description>
          <text>The BFD session is set up successfully and detection messages are periodically exchanged.</text>
        </description>
      </enum>
      <enum name="bfd-session-number-exceed">
        <value value="4"/>
        <description>
          <text>The number of BFD session discriminators exceeds the upper limit.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Status of a PIM BFD session.</text>
    </description>
  </typedef>
