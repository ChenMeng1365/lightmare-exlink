      <type name="uint8">
        <range value="1..255"/>
      </type>
      <default value="30"/>
      <description>
        <text>Time to live.</text>
      </description>
    </leaf>
    <leaf name="pass-route">
      <type name="boolean"/>
      <default value="false"/>
      <description>
        <text>Enable/disable check pass route.</text>
      </description>
    </leaf>
    <leaf name="probe-fails">
      <type name="uint32">
        <range value="1..15"/>
      </type>
      <default value="1"/>
      <description>
        <text>Packet failed threshold determining whether send traps or events.</text>
      </description>
    </leaf>
    <leaf name="test-fails">
      <type name="uint32">
        <range value="1..15"/>
      </type>
      <default value="1"/>
      <description>
        <text>Test failed threshold determining whether send traps or events.</text>
      </description>
    </leaf>
    <leaf name="send-trap-bits">
      <type name="bits">
        <bit name="probefailure">
          <position value="31"/>
          <description>
            <text>Indicates that a trap is sent when the OWD from the source to the destination exceeds the threshold. This parameter applies only to jitter test instances.</text>
          </description>
        </bit>
        <bit name="rtd">
          <position value="28"/>
          <description>
            <text>Indicates that a trap is sent when the RTD exceeds the threshold.</text>
          </description>
        </bit>
        <bit name="testcomplete">
          <position value="29"/>
          <description>
            <text>Indicates that a trap is sent when a test succeeds.</text>
          </description>
        </bit>
        <bit name="testfailure">
          <position value="30"/>
          <description>
            <text>Indicates that a trap is sent when the number of test failures reaches the threshold.</text>
          </description>
        </bit>
      </type>
      <description>
        <text>Configures conditions for sending traps.</text>
      </description>
    </leaf>
    <leaf name="compatibility">
      <type name="boolean"/>
      <default value="false"/>
      <description>
        <text>Enable/disable compatibility mode. When the ptn79 interworks with the ptn39, the no result is converted to failed and success.</text>
