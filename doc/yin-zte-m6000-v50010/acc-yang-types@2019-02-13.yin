      <text>pm granularity</text>
    </description>
  </typedef>
  <typedef name="threshold-type">
    <type name="enumeration">
      <enum name="low">
        <value value="0"/>
        <description>
          <text>low threshold-type</text>
        </description>
      </enum>
      <enum name="high">
        <value value="1"/>
        <description>
          <text>high threshold-type</text>
        </description>
