  <container name="bras-ipv6-address-manage">
    <description>
      <text>Configure the IPv6 repfix pool and IPv6 address pool.</text>
    </description>
    <container name="ipv6-prefixs">
      <description>
        <text>List of IPv6 prefix pools.</text>
      </description>
      <list name="ipv6-prefix">
        <key value="name"/>
        <max-elements value="16384"/>
        <description>
          <text>Configure an IPv6 prefix pool.</text>
        </description>
        <leaf name="name">
          <ext:case-sensitivity value="lower-only"/>
          <type name="string">
            <length value="1..32"/>
            <pattern value="[a-z0-9._-]{1,32}"/>
          </type>
          <must condition="../name != '-' and ../name != '--'"/>
          <description>
            <text>Specify the name of the IPv6 prefix pool to be created.</text>
          </description>
        </leaf>
        <leaf name="type">
          <ext:operation-exclude value="update"/>
          <type name="prefix-type"/>
          <mandatory value="true"/>
          <description>
            <text>Specify the type of the IPv6 prefix pool, and the type cannot be modified.</text>
          </description>
        </leaf>
        <container name="local">
          <when condition="../type = 'local'"/>
          <description>
            <text>Configure a local IPv6 prefix pool. The specified IPv6 address must be in the IPv6 prefix pool.</text>
          </description>
          <uses name="specified-up"/>
          <container name="prefix-address">
            <description>
              <text>Configure the IPv6 prefix address for an IPv6 prefix pool.</text>
            </description>
            <leaf name="prefix">
              <type name="inet:ipv6-prefix"/>
              <description>
                <text>Specify the address and length of an IPv6 prefix, not support modify. The prefix address must be calculated based on the prefix length.</text>
              </description>
            </leaf>
          </container>
          <uses name="vpn-instance"/>
          <container name="exclude-ipv6-addresss">
            <when condition="../prefix-address/prefix"/>
            <description>
              <text>Configure IPv6 addresses to prevent it from being allocated to users. The start IPv6 address must be smaller than or equal to the end IPv6 address.</text>
            </description>
            <uses name="exclude-ipv6-address"/>
          </container>
          <uses name="lifetime-config"/>
          <uses name="reserved-ipv6"/>
          <uses name="frame-ipv6-lease"/>
          <uses name="ipv6-constant-index"/>
        </container>
        <container name="delegation">
          <when condition="../type = 'delegation'"/>
          <must condition="./slaac-unshare-only/enable='false' and ./pd-unshare-only/enable='false' or ./slaac-unshare-only/enable='false' and not(./dhcpv6-unshare-only/enable) or ./pd-unshare-only/enable='false' and not(./dhcpv6-unshare-only/enable)"/>
          <description>
            <text>Configure a delegation IPv6 prefix pool. The specified IPv6 address must be in the IPv6 prefix pool.</text>
          </description>
          <uses name="specified-up"/>
          <container name="prefix-address">
            <description>
              <text>Configure the IPv6 prefix address for an IPv6 prefix pool.</text>
            </description>
            <leaf name="prefix">
              <type name="inet:ipv6-prefix"/>
              <description>
                <text>Specify the address and length of an IPv6 prefix, not support modify. The prefix address must be calculated based on the prefix length.</text>
              </description>
            </leaf>
            <leaf name="prefix-length">
              <when condition="../prefix"/>
              <type name="uint8">
                <range value="1..128"/>
              </type>
              <default value="64"/>
              <description>
                <text>Specify the length of the IPv6 prefix assigned by the delegating router to the requesting router. Not support modify or delete.
The delegating prefix length must be longer than or the same as IPv6 prefix length.</text>
              </description>
            </leaf>
          </container>
          <uses name="slaac-unshare-only"/>
          <uses name="pd-unshare-only"/>
          <container name="dhcpv6-unshare-only">
            <when condition="../prefix-address/prefix"/>
            <description>
              <text>Configure an IPv6 prefix pool to assign only IPv6 addresses not prefixes to users.</text>
            </description>
            <uses name="dhcpv6-unshare-only"/>
          </container>
          <uses name="vpn-instance"/>
          <container name="exclude-ipv6-addresss">
            <when condition="../dhcpv6-unshare-only/enable"/>
            <description>
              <text>Configure IPv6 addresses to prevent it from being allocated to users.</text>
            </description>
            <uses name="exclude-ipv6-address"/>
          </container>
          <uses name="lifetime-config"/>
          <uses name="reserved-ipv6"/>
          <uses name="frame-ipv6-lease"/>
          <uses name="ipv6-constant-index"/>
        </container>
        <container name="remote">
          <when condition="../type = 'remote'"/>
          <must condition="./slaac-unshare-only/enable='false' or not(./dhcpv6-only/enable)"/>
          <description>
            <text>Configure a remote IPv6 prefix pool.</text>
          </description>
          <uses name="specified-up"/>
          <uses name="vpn-instance"/>
          <container name="link-address">
            <description>
              <text>Configure a link address.</text>
            </description>
            <leaf name="ipv6-address">
              <type name="inet:ipv6-prefix"/>
              <description>
                <text>Specify a global unicast address or a site-local unicast address.</text>
              </description>
            </leaf>
          </container>
          <container name="dhcpv6-only">
            <description>
              <text>Configure a remote prefix pool for only DHCPv6 prefix or address allocation.</text>
            </description>
            <leaf name="enable">
              <type name="empty"/>
              <description>
                <text>Specify a remote prefix pool for only DHCPv6 prefix or address allocation.</text>
              </description>
            </leaf>
          </container>
          <container name="lease-management">
            <description>
              <text>Configure the lease management function for remote IPv6 prefix pool.</text>
            </description>
            <leaf name="enable">
              <type name="boolean"/>
              <default value="false"/>
              <description>
                <text>Enable/disable the lease management function for remote IPv6 prefix pool.</text>
              </description>
            </leaf>
          </container>
          <container name="reserved-ipv6">
            <description>
              <text>Configure a reservation type for the prefix pool.</text>
            </description>
            <leaf name="reserved-type">
              <type name="reserved-type"/>
              <description>
                <text>Reservation type for the IPv6 prefix or address, after the reserved type of an IPv6 prefix or address is set to mac, one-to-many mapping between one MAC address and multiple sessions cannot be enabled.</text>
              </description>
            </leaf>
          </container>
          <uses name="lock"/>
          <uses name="slaac-unshare-only"/>
          <uses name="ipv6-constant-index"/>
        </container>
        <container name="dynamic">
          <when condition="../type = 'dynamic'"/>
          <description>
            <text>Configure a dynamic IPv6 prefix pool.</text>
          </description>
          <container name="prefix-length">
            <must condition="not(./unshared-initial &gt; ./unshared-delegating or ./unshared-extend &gt; ./unshared-delegating)"/>
            <must condition="not(./slaac-unshare-only/enable = 'true' and ./unshared-delegating != 64)"/>
            <must condition="not(./slaac-unshare-only/enable = 'true' and ./pd-unshare-only/enable = 'true')"/>
            <description>
              <text>Configure the share mode for a dynamic IPv6 prefix pool to configure the prefix length.</text>
            </description>
            <choice name="share-mode">
              <description>
                <text>Select the share mode for a dynamic IPv6 prefix pool to configure the prefix length.</text>
              </description>
              <case name="shared">
                <description>
                  <text>Configure initial and subsequent prefix lengths for a shared dynamic prefix pool.</text>
                </description>
                <leaf name="shared-initial">
                  <type name="uint8">
                    <range value="3..127"/>
                  </type>
                  <mandatory value="true"/>
                  <description>
                    <text>Configure the initial prefix length.</text>
                  </description>
                </leaf>
                <leaf name="shared-extend">
                  <type name="uint8">
                    <range value="3..127"/>
                  </type>
                  <ext:dynamic-default>
                    <ext:default-value value="../shared-initial">
                      <description>
                        <text>The default value is equal to initial length.</text>
                      </description>
                    </ext:default-value>
                  </ext:dynamic-default>
                  <description>
                    <text>Configure the subsequent prefix length.</text>
                  </description>
                </leaf>
              </case>
              <case name="unshared">
                <description>
                  <text>Configure initial, subsequent and delegating prefix lengths for a unshared dynamic prefix pool.</text>
                </description>
                <leaf name="unshared-initial">
                  <type name="uint8">
                    <range value="3..127"/>
                  </type>
                  <mandatory value="true"/>
                  <description>
                    <text>Configure the initial prefix length.</text>
                  </description>
                </leaf>
                <leaf name="unshared-extend">
                  <type name="uint8">
                    <range value="3..127"/>
                  </type>
                  <ext:dynamic-default>
                    <ext:default-value value="../unshared-initial">
                      <description>
                        <text>The default value is equal to initial length.</text>
                      </description>
                    </ext:default-value>
                  </ext:dynamic-default>
                  <description>
                    <text>Configure the subsequent prefix length.</text>
                  </description>
                </leaf>
                <leaf name="unshared-delegating">
                  <type name="uint8">
                    <range value="3..127"/>
                  </type>
                  <default value="64"/>
                  <description>
                    <text>Configure the delegating prefix length.</text>
                  </description>
                </leaf>
                <container name="slaac-unshare-only">
                  <description>
                    <text>Configure the prefix pool to allocate IP addresses by using only the unshared-mode stateless address autoconfiguration (SLAAC) protocol.</text>
                  </description>
                  <leaf name="enable">
                    <type name="boolean"/>
                    <default value="false"/>
                    <description>
                      <text>Enable/disable the prefix pool to allocate IP addresses by using only the unshared-mode stateless address autoconfiguration (SLAAC) protocol.</text>
                    </description>
                  </leaf>
                </container>
                <container name="pd-unshare-only">
                  <description>
                    <text>Configure the prefix pool to allocate IP addresses by using only the unshared-mode stateless address autoconfiguration (DHCPv6 IA_PD) protocol.</text>
                  </description>
                  <leaf name="enable">
                    <type name="boolean"/>
                    <default value="false"/>
                    <description>
                      <text>Enable/disable the prefix pool to allocate IP addresses by using only the unshared-mode stateless address autoconfiguration (DHCPv6 IA_PD) protocol.</text>
                    </description>
                  </leaf>
                </container>
              </case>
            </choice>
          </container>
          <uses name="vpn-instance"/>
          <uses name="ipv6-constant-index"/>
        </container>
      </list>
    </container>
    <container name="ipv6-pools">
      <description>
        <text>List of IPv6 address pools.</text>
      </description>
      <list name="ipv6-pool">
        <key value="name"/>
        <max-elements value="16384"/>
        <description>
          <text>Configure an IPv6 address pool.</text>
        </description>
        <leaf name="name">
          <ext:case-sensitivity value="lower-only"/>
          <type name="string">
            <length value="1..32"/>
            <pattern value="[a-z0-9._-]{1,32}"/>
          </type>
          <must condition="../name != '-' and ../name != '--'"/>
          <description>
            <text>Specify the name of an IPv6 address pool.</text>
          </description>
        </leaf>
        <leaf name="type">
          <ext:operation-exclude value="update"/>
          <type name="pool-type"/>
          <mandatory value="true"/>
          <description>
            <text>Specify the type of the IPv6 address pool, and the type cannot be modified.</text>
          </description>
        </leaf>
        <leaf name="rui-slave">
          <ext:operation-exclude value="update|delete"/>
          <when condition="not(../type = 'relay')"/>
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable a hybrid IP address pool that can be used as a local address pool or remote address pool or delegation address pool. The rui-slave cannot be modified.</text>
          </description>
        </leaf>
        <container name="local">
          <when condition="../type = 'local'"/>
          <description>
            <text>Configure a local IPv6 address pool.</text>
          </description>
          <container name="bind-prefix">
            <description>
              <text>Configure binding an IPv6 prefix pool to an IPv6 address pool.</text>
            </description>
            <leaf name="prefix-name">
              <type name="leafref">
                <path value="/bras-ipv6-address-manage:bras-ipv6-address-manage/bras-ipv6-address-manage:ipv6-prefixs/bras-ipv6-address-manage:ipv6-prefix/bras-ipv6-address-manage:name"/>
              </type>
              <must condition="/bras-ipv6-address-manage:bras-ipv6-address-manage/bras-ipv6-address-manage:ipv6-prefixs/bras-ipv6-address-manage:ipv6-prefix[name=current()]/bras-ipv6-address-manage:type ='local'"/>
              <description>
