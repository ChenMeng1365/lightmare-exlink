        </description>
      </enum>
      <enum name="ascii">
        <value value="1"/>
        <description>
          <text>Authentication type value:ASCII.</text>
        </description>
      </enum>
      <enum name="pap">
        <value value="2"/>
        <description>
          <text>Authentication type value:PAP.</text>
        </description>
      </enum>
      <enum name="chap">
        <value value="3"/>
        <description>
          <text>Authentication type value:CHAP.</text>
        </description>
      </enum>
      <enum name="arap">
        <value value="4"/>
        <description>
          <text>Authentication type value:ARAP.</text>
        </description>
      </enum>
      <enum name="mschap">
        <value value="5"/>
        <description>
          <text>Authentication type value:MSCHAP.</text>
        </description>
