              +--rw local-sub-domain* [sd-id address-family]
                 +--rw sd-id             uint8
                 +--rw address-family    address-family
                 +--rw igp-type?         igp-type
                 +--rw bfr-id?           uint16
                 +--rw bfr-prefix
                 |  +--rw (bfr-prefix)?
                 |     +--:(ipv4-bfr-prefix)
