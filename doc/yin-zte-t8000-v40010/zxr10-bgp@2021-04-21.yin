                          <text>The parameters of BFD.</text>
                        </description>
                        <presence value="bfd-parameters"/>
                        <choice name="bfd-parameters">
                          <description>
                            <text>Config bfd parameters</text>
                          </description>
                          <case name="template-name">
                            <description>
                              <text>Template name</text>
                            </description>
