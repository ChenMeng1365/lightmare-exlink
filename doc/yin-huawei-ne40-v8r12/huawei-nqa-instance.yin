  <grouping name="instances-type">
    <description>
      <text>The network quality assurance.</text>
    </description>
    <list name="instance">
      <key value="admin-name test-name"/>
      <description>
        <text>Configure test instance.</text>
      </description>
      <leaf name="admin-name">
        <type name="string">
          <length value="1..32"/>
        </type>
        <description>
          <text>Administrator name.</text>
        </description>
      </leaf>
      <leaf name="test-name">
        <type name="string">
          <length value="1..32"/>
        </type>
        <description>
          <text>Test name.</text>
        </description>
      </leaf>
      <choice name="type">
        <description>
          <text>The test type of instance.</text>
        </description>
        <case name="icmp">
          <description>
            <text>ICMP NQA test.</text>
          </description>
          <container name="icmp-input">
            <must condition="not(ipv4-next-hop and (ni-name != '_public_' or fwd-if-name or path-type = 'bypass'))"/>
            <must condition="not(ipv6-next-hop and (ni-name != '_public_' or fwd-if-name or path-type = 'bypass'))"/>
            <must condition="not(path-type = 'bypass' and (pass-route = 'true' or fwd-if-name or ipv6-dest-address or ipv6-source-address or ipv6-next-hop))"/>
            <must condition="not(pass-route = 'true' and fwd-if-name)"/>
            <must condition="not(out-if-name and fwd-if-name)"/>
            <presence value="ICMP PING test instance."/>
            <description>
              <text>Enable/disable ICMP test instance input.</text>
            </description>
            <uses name="icmp-instance-input"/>
          </container>
        </case>
        <case name="trace">
          <description>
            <text>TRACE NQA test.</text>
          </description>
          <container name="trace-input">
            <must condition="first-ttl&lt;=max-ttl"/>
            <must condition="not(non-fragment = 'true' and (ipv6-dest-address or ipv6-source-address))"/>
            <presence value="Present enable trace test instance input."/>
            <description>
              <text>Enable/disable trace test instance input.</text>
            </description>
            <uses name="trace-instance-input"/>
          </container>
        </case>
        <case name="udp">
          <description>
            <text>UDP NQA test.</text>
          </description>
          <container name="udp-input">
            <must condition="not((pass-route = 'true' and ipv6-dest-address) or (pass-route = 'true' and ipv6-source-address))"/>
            <presence value="Present enable UDP test instance input."/>
            <description>
              <text>Enable/disable UDP test instance input.</text>
            </description>
            <uses name="udp-instance-input"/>
          </container>
        </case>
        <case name="tcp">
          <description>
            <text>TCP NQA test.</text>
          </description>
          <container name="tcp-input">
            <presence value="Present enable TCP test instance input."/>
            <description>
              <text>Enable/disable TCP test instance input.</text>
            </description>
            <uses name="tcp-instance-input"/>
          </container>
        </case>
        <case name="jitter">
          <description>
            <text>Jitter NQA test that tests UDP-packet jitter.</text>
          </description>
          <container name="jitter-input">
            <must condition="not(hardware-enable = 'true' and data-fill)"/>
            <must condition="not(hardware-enable = 'false' and timestamp = 'microsecond')"/>
            <must condition="(hardware-enable = 'false' and interval &gt;= 20) or (hardware-enable = 'true' and interval &gt;= 10)"/>
            <presence value="Present enable jitter test instance input"/>
            <description>
              <text>Enable/disable jitter test instance input.</text>
            </description>
            <uses name="jitter-instance-input"/>
          </container>
        </case>
        <case name="icmp-jitter">
          <description>
            <text>ICMP jitter NQA test.</text>
          </description>
          <container name="icmp-jitter-input">
            <presence value="Present enable ICMP jitter test instance input."/>
            <description>
              <text>Enable/disable ICMP jitter test instance input.</text>
            </description>
            <uses name="icmp-jitter-instance-input"/>
          </container>
        </case>
        <case name="lsp-ping">
          <description>
            <text>LSP ping NQA test.</text>
          </description>
          <container name="lsp-ping-input">
            <presence value="Present enable LSP ping test instance input."/>
            <description>
              <text>Enable/disable LSP ping test instance input.</text>
            </description>
            <uses name="lspping-instance-input"/>
          </container>
        </case>
        <case name="lsp-trace">
          <description>
            <text>LSP trace NQA test.</text>
          </description>
          <container name="lsp-trace-input">
            <presence value="Present enable LSP trace test instance input."/>
            <description>
              <text>Enable/disable LSP trace test instance input.</text>
            </description>
            <uses name="lsptrace-instance-input"/>
          </container>
        </case>
        <case name="lsp-jitter">
          <description>
            <text>LSP jitter NQA test.</text>
          </description>
          <container name="lsp-jitter-input">
            <presence value="Present enable LSP jitter test instance input"/>
            <description>
              <text>Enable/disable LSP jitter test instance input.</text>
            </description>
            <uses name="lsp-jitter-instance-input"/>
          </container>
        </case>
        <case name="pwe3-trace">
          <description>
            <text>PWE3 trace NQA test.</text>
          </description>
          <container name="pwe3-trace-input">
            <presence value="Present enable PWE3 trace test instance input"/>
            <description>
              <text>Enable/disable PWE3 trace test instance input.</text>
            </description>
            <uses name="pwe3-trace-instance-input"/>
          </container>
        </case>
        <case name="mac-ping">
          <description>
            <text>MAC ping NQA test.</text>
          </description>
          <container name="mac-ping-input">
            <must condition="(md-name and ma-name) or (not(md-name) and not(ma-name))"/>
            <must condition="not(dest-mac-address) or (dest-mac-address != '0000-0000-0000')"/>
            <presence value="Present enable MAC ping test instance input."/>
            <description>
              <text>Enable/disable MAC ping test instance input.</text>
            </description>
            <uses name="mac-ping-instance-input"/>
          </container>
        </case>
        <case name="arp-ping-ip">
          <description>
            <text>Arp ping IP NQA test.</text>
          </description>
          <container name="arp-ping-ip-input">
            <presence value="Present enable arp ping IP test intance input."/>
            <description>
              <text>Enable/disable arp ping IP test intance input.</text>
            </description>
            <uses name="arp-ping-ip-instance-input"/>
          </container>
        </case>
      </choice>
      <container name="execution-policy">
        <config value="false"/>
        <description>
          <text>Statistics of the test instance execution policy.</text>
        </description>
        <choice name="execution-policy">
          <description>
            <text>The test instance start policy.</text>
          </description>
          <case name="start">
            <description>
              <text>Start the test.</text>
            </description>
            <choice name="start-type">
              <description>
                <text>The test instance start type.</text>
              </description>
              <case name="start-now">
                <description>
                  <text>Start now.</text>
                </description>
                <leaf name="start-now">
                  <type name="start-now-type"/>
                  <description>
                    <text>Start the test instance instantly.</text>
                  </description>
                </leaf>
                <choice name="start-now-end-type">
                  <description>
                    <text>Test instance end type.</text>
                  </description>
                  <case name="at">
                    <description>
                      <text>Stop the test at the specified time.</text>
                    </description>
                    <leaf name="start-now-end-date-time">
                      <type name="yang:date-and-time"/>
                      <description>
                        <text>The test instance end time.</text>
                      </description>
                    </leaf>
                  </case>
                  <case name="delay">
                    <description>
                      <text>Stop the test after the specified delay time.</text>
                    </description>
                    <leaf name="start-now-end-delay-seconds">
                      <type name="uint32">
                        <range value="6..86399"/>
                      </type>
                      <units name="s"/>
                      <description>
                        <text>The test instance end delay seconds.</text>
                      </description>
                    </leaf>
                  </case>
                  <case name="lifetime">
                    <description>
                      <text>Stop the test after the specified life time.</text>
                    </description>
                    <leaf name="start-now-end-lifetime-seconds">
                      <type name="uint32">
                        <range value="6..86399"/>
                      </type>
                      <units name="s"/>
                      <description>
                        <text>The test instance end lifetime seconds.</text>
                      </description>
                    </leaf>
                  </case>
                </choice>
              </case>
              <case name="start-at">
                <description>
                  <text>Execute at the specified time.</text>
                </description>
                <leaf name="start-date-time">
                  <type name="yang:date-and-time"/>
                  <description>
                    <text>The test instance start time.</text>
                  </description>
                </leaf>
                <choice name="start-at-end-type">
                  <description>
                    <text>Test instance end type.</text>
                  </description>
                  <case name="at">
                    <description>
                      <text>Stop the test at the specified time.</text>
                    </description>
                    <leaf name="start-at-end-date-time">
                      <type name="yang:date-and-time"/>
                      <description>
                        <text>The test instance end time.</text>
                      </description>
                    </leaf>
                  </case>
                  <case name="delay">
                    <description>
                      <text>Stop the test after the specified delay time.</text>
                    </description>
                    <leaf name="start-at-end-delay-seconds">
                      <type name="uint32">
                        <range value="6..86399"/>
                      </type>
                      <units name="s"/>
                      <description>
                        <text>The test instance end delay seconds.</text>
                      </description>
                    </leaf>
                  </case>
                  <case name="lifetime">
                    <description>
                      <text>Stop the test after the specified life time.</text>
                    </description>
                    <leaf name="start-at-end-lifetime-seconds">
                      <type name="uint32">
                        <range value="6..86399"/>
                      </type>
                      <units name="s"/>
                      <description>
                        <text>The test instance end lifetime seconds.</text>
                      </description>
                    </leaf>
                  </case>
                </choice>
              </case>
              <case name="start-delay">
                <description>
                  <text>Delay some time then execute testcase.</text>
                </description>
                <leaf name="start-delay-seconds">
                  <type name="uint32">
                    <range value="1..86399"/>
                  </type>
                  <units name="s"/>
                  <description>
                    <text>The test instance start delay seconds.</text>
                  </description>
                </leaf>
                <choice name="start-delay-end-type">
                  <description>
                    <text>Test instance end type.</text>
                  </description>
                  <case name="at">
                    <description>
                      <text>Stop the test at the specified time.</text>
                    </description>
                    <leaf name="start-delay-end-date-time">
                      <type name="yang:date-and-time"/>
                      <description>
                        <text>The test instance end time.</text>
                      </description>
                    </leaf>
                  </case>
                  <case name="delay">
                    <description>
                      <text>Stop the test after the specified delay time.</text>
                    </description>
                    <leaf name="start-delay-end-delay-seconds">
                      <type name="uint32">
                        <range value="6..86399"/>
                      </type>
                      <units name="s"/>
                      <description>
                        <text>The test instance end delay seconds.</text>
                      </description>
                    </leaf>
                  </case>
                  <case name="lifetime">
                    <description>
                      <text>Stop the test after the specified life time.</text>
                    </description>
                    <leaf name="start-delay-end-lifetime-seconds">
                      <type name="uint32">
                        <range value="6..86399"/>
                      </type>
                      <units name="s"/>
                      <description>
                        <text>The test instance end lifetime seconds.</text>
                      </description>
                    </leaf>
                  </case>
                </choice>
              </case>
              <case name="start-daily">
                <description>
                  <text>Execute the test instance daily.</text>
                </description>
                <leaf name="daily-start-time">
                  <type name="pub-type:time"/>
                  <description>
                    <text>Start time of daily execution.</text>
                  </description>
                </leaf>
                <leaf name="daily-start-date">
                  <type name="pub-type:date"/>
                  <description>
                    <text>Start date of daily execution.</text>
                  </description>
                </leaf>
                <leaf name="daily-end-time">
                  <type name="pub-type:time"/>
                  <description>
                    <text>End time of daily execution.</text>
                  </description>
                </leaf>
                <leaf name="daily-end-date">
                  <type name="pub-type:date"/>
                  <description>
                    <text>End date of daily execution.</text>
                  </description>
                </leaf>
              </case>
            </choice>
          </case>
          <case name="stop">
            <description>
              <text>Stop the test.</text>
            </description>
            <leaf name="stop">
              <type name="stop-type"/>
              <description>
                <text>Stop the test.</text>
              </description>
            </leaf>
          </case>
        </choice>
      </container>
    </list>
  </grouping>
  <grouping name="icmp-instance-input">
    <description>
      <text>Configure ICMP test instance input.</text>
    </description>
    <choice name="address">
      <description>
        <text>Address information.</text>
      </description>
      <case name="ipv4-address">
        <description>
          <text>IPv4 address.</text>
        </description>
        <leaf name="ipv4-dest-address">
          <type name="inet:ipv4-address-no-zone"/>
          <description>
            <text>Destination IPv4 address.</text>
          </description>
        </leaf>
        <leaf name="ipv4-source-address">
          <type name="inet:ipv4-address-no-zone"/>
          <description>
            <text>Source IPv4 address.</text>
          </description>
        </leaf>
        <leaf name="ipv4-next-hop">
          <type name="inet:ipv4-address-no-zone"/>
          <description>
            <text>IPv4 next hop address.</text>
          </description>
        </leaf>
      </case>
      <case name="ipv6-address">
        <description>
          <text>IPv6 address.</text>
        </description>
        <leaf name="ipv6-dest-address">
          <type name="inet:ipv6-address-no-zone"/>
          <description>
            <text>Destination IPv6 address.</text>
          </description>
        </leaf>
        <leaf name="ipv6-source-address">
          <type name="inet:ipv6-address-no-zone"/>
          <description>
            <text>Source IPv6 address.</text>
          </description>
        </leaf>
        <leaf name="ipv6-next-hop">
          <type name="inet:ipv6-address-no-zone"/>
          <description>
            <text>IPv6 next hop address.</text>
          </description>
        </leaf>
      </case>
    </choice>
    <leaf name="ni-name">
      <type name="leafref">
        <path value="/ni:network-instance/ni:instances/ni:instance/ni:name"/>
      </type>
      <default value="_public_"/>
      <description>
        <text>Name of a VPN instance.</text>
      </description>
    </leaf>
    <leaf name="out-if-name">
      <type name="leafref">
        <path value="/ifm:ifm/ifm:interfaces/ifm:interface/ifm:name"/>
      </type>
      <description>
        <text>Out interface name.</text>
      </description>
    </leaf>
    <leaf name="fwd-if-name">
      <type name="leafref">
        <path value="/ifm:ifm/ifm:interfaces/ifm:interface/ifm:name"/>
      </type>
      <description>
        <text>Simulated inbound-interface name.</text>
      </description>
    </leaf>
    <leaf name="interval">
      <type name="uint32">
        <range value="1..60"/>
      </type>
      <units name="s"/>
      <default value="4"/>
      <description>
        <text>Interval for sending packets.</text>
      </description>
    </leaf>
    <leaf name="probe-count">
      <type name="uint32">
        <range value="1..15"/>
      </type>
      <default value="3"/>
      <description>
        <text>Number of sent packets.</text>
      </description>
    </leaf>
    <leaf name="timeout">
      <type name="uint32">
        <range value="1..60"/>
      </type>
      <units name="s"/>
      <default value="3"/>
      <description>
        <text>Time to wait for a response packet after a test packet is sent.</text>
      </description>
    </leaf>
    <leaf name="frequency">
      <type name="uint32">
        <range value="1..604800"/>
      </type>
      <units name="s"/>
      <description>
        <text>Cyclic scheduling period of test cases. The frequency must be greater than the value of the following formula; otherwise, the test results may be inaccurate:
(number of sent packets - 1) * (packet sending interval) + (timeout period).</text>
      </description>
    </leaf>
    <leaf name="fail-percent">
      <type name="uint32">
        <range value="1..100"/>
      </type>
      <units name="%"/>
      <default value="100"/>
      <description>
        <text>Indicates the packet loss ratio threshold of an NQA test instance. If the packet loss ratio exceeds the threshold, the test fails.</text>
      </description>
    </leaf>
    <leaf name="data-size">
      <type name="uint32">
        <range value="0..8100"/>
      </type>
      <units name="Byte"/>
      <default value="0"/>
      <description>
        <text>Packet size.</text>
      </description>
    </leaf>
    <leaf name="data-fill">
      <type name="string">
        <length value="1..230"/>
      </type>
      <description>
        <text>Specify an NQA test packet payload string.</text>
      </description>
    </leaf>
    <leaf name="ttl">
      <type name="uint8">
        <range value="1..255"/>
      </type>
      <default value="30"/>
      <description>
        <text>Time to live.</text>
      </description>
    </leaf>
    <leaf name="pass-route">
      <type name="boolean"/>
      <default value="false"/>
      <description>
        <text>Enable/disable the function of sending packets without searching for routes.</text>
      </description>
    </leaf>
    <leaf name="probe-fails">
      <type name="uint32">
        <range value="1..15"/>
      </type>
      <default value="1"/>
      <description>
        <text>The maximum number of probe failures of an NQA test to send trap or events.</text>
      </description>
    </leaf>
    <leaf name="test-fails">
      <type name="uint32">
        <range value="1..15"/>
      </type>
      <default value="1"/>
      <description>
        <text>The maximum number of the NQA test consecutive test failures to send trap or events.</text>
      </description>
    </leaf>
    <leaf name="send-trap-bits">
      <type name="bits">
        <bit name="probefailure">
          <position value="31"/>
          <description>
            <text>Indicates that a trap is sent when the OWD from the source to the destination exceeds the threshold.</text>
          </description>
        </bit>
        <bit name="rtd">
          <position value="28"/>
          <description>
            <text>Indicates that a trap is sent when the RTD exceeds the threshold.</text>
          </description>
        </bit>
        <bit name="testcomplete">
          <position value="29"/>
          <description>
            <text>Indicates that a trap is sent when a test succeeds.</text>
          </description>
        </bit>
        <bit name="testfailure">
          <position value="30"/>
          <description>
            <text>Indicates that a trap is sent when the number of test failures reaches the threshold.</text>
          </description>
        </bit>
      </type>
      <description>
        <text>Configure conditions for sending traps.</text>
      </description>
    </leaf>
    <leaf name="compatibility">
      <type name="boolean"/>
      <default value="false"/>
      <description>
        <text>Enable/disable compatibility mode. When the ptn79 interworks with the ptn39, the no result is converted to failed and success.</text>
      </description>
    </leaf>
    <leaf name="result-num">
      <type name="uint32">
        <range value="1..10"/>
      </type>
      <default value="5"/>
      <description>
        <text>The maximum number of test result records.</text>
      </description>
    </leaf>
    <leaf name="history-num">
      <type name="uint32">
        <range value="0..1000"/>
      </type>
      <default value="60"/>
      <description>
        <text>The maximum number of historical test records.</text>
      </description>
    </leaf>
    <leaf name="description">
      <type name="string">
        <length value="1..230"/>
      </type>
      <description>
        <text>Description of test instance.</text>
      </description>
    </leaf>
    <leaf name="age-time">
      <type name="pub-type:time"/>
      <default value="00:00:00"/>
      <description>
        <text>Aging time of test instance.</text>
      </description>
    </leaf>
    <leaf name="path-type">
      <type name="path-type"/>
      <description>
        <text>Path type.</text>
      </description>
    </leaf>
    <leaf name="threshold-rtd">
      <type name="uint32">
        <range value="1..60000"/>
      </type>
      <description>
        <text>Threshold value of round trip delay.</text>
      </description>
    </leaf>
    <choice name="priority">
      <default value="tos-value"/>
      <description>
        <text>Priority type.</text>
      </description>
      <case name="tos-value">
        <description>
          <text>ToS value.</text>
        </description>
        <leaf name="tos">
          <type name="uint16">
            <range value="0..255"/>
          </type>
          <default value="0"/>
          <description>
            <text>ToS.</text>
          </description>
        </leaf>
      </case>
      <case name="dscp-value">
        <description>
          <text>DSCP value.</text>
        </description>
        <leaf name="dscp">
          <type name="uint32">
            <range value="0..63"/>
          </type>
          <description>
            <text>DSCP value.</text>
          </description>
        </leaf>
      </case>
    </choice>
  </grouping>
  <grouping name="jitter-instance-input">
    <description>
      <text>Configure jitter test instance input.</text>
