    <container name="local-bill">
      <presence value="local-bill"/>
      <description>
        <text>Configure the local bill.</text>
      </description>
      <leaf name="bill-backup-server-ip">
        <type name="inet:ipv4-address-no-zone"/>
        <description>
          <text>Specifies the IP address of a bill server.</text>
        </description>
      </leaf>
      <leaf name="bill-backup-file-name">
        <when condition="../bill-backup-server-ip"/>
        <type name="string">
          <length value="1..30"/>
        </type>
        <mandatory value="true"/>
        <description>
          <text>Specifies a file used to store bills on a bill server.</text>
        </description>
      </leaf>
      <leaf name="bill-backup-user-name">
        <when condition="../bill-backup-server-ip"/>
        <type name="string">
          <length value="1..64"/>
        </type>
        <description>
          <text>Sets a user name used to set up an SFTP session with a bill server.</text>
        </description>
      </leaf>
      <leaf name="bill-backup-password">
        <when condition="../bill-backup-user-name"/>
        <type name="pub-type:password-extend">
          <length value="1..128"/>
        </type>
        <mandatory value="true"/>
        <description>
          <text>Sets a password used to set up an SFTP session with a bill server. The password is a string ranging from 1 to 16 characters for a simple password and 1 to 128 characters for a ciphertext password.</text>
        </description>
      </leaf>
      <leaf name="bill-backup-port-number">
        <when condition="../bill-backup-user-name"/>
        <type name="uint32">
          <range value="1..65535"/>
        </type>
        <mandatory value="true"/>
        <description>
          <text>Sets a port number used to set up an SFTP session with a bill server.</text>
        </description>
      </leaf>
      <leaf name="local-bill-pool-enable">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable a local bill pool.</text>
        </description>
      </leaf>
      <leaf name="cache-backup-interval">
        <type name="uint32">
          <range value="1..65535"/>
        </type>
        <units name="min"/>
        <default value="1440"/>
        <description>
          <text>Sets an interval for automatic local bill transfer from the local bill pool to a bill server.</text>
        </description>
      </leaf>
      <leaf name="cache-alarm-threshold">
        <type name="uint32">
          <range value="50..100"/>
        </type>
        <units name="%"/>
        <default value="75"/>
        <description>
          <text>Sets a bill alarm threshold for the usage of the local bill pool.</text>
        </description>
      </leaf>
      <leaf name="cache-backup-mode">
        <type name="enumeration">
          <enum name="none">
            <value value="1"/>
            <description>
              <text>No bill backup.</text>
            </description>
          </enum>
          <enum name="tftp">
            <value value="2"/>
            <description>
              <text>To backup bills to bill server by tftp.</text>
            </description>
          </enum>
          <enum name="cfcard">
            <value value="3"/>
            <description>
              <text>Cfcard.</text>
            </description>
          </enum>
          <enum name="sftp">
            <value value="5"/>
            <description>
              <text>To backup bills to bill server by sftp.</text>
            </description>
          </enum>
        </type>
        <default value="cfcard"/>
        <description>
          <text>Configures the transfer mode for bills in the local bill pool.</text>
        </description>
      </leaf>
      <leaf name="cfcard-backup-interval">
        <type name="uint32">
          <range value="1..65535"/>
        </type>
        <units name="min"/>
        <default value="1440"/>
        <description>
          <text>Sets an interval for automatic local bill transfer from the CF card to a bill server.</text>
        </description>
      </leaf>
      <leaf name="cfcard-alarm-threshold">
        <type name="uint32">
          <range value="50..100"/>
        </type>
        <units name="min"/>
        <default value="75"/>
        <description>
          <text>Sets a bill alarm threshold for the usage of the CF card.</text>
        </description>
      </leaf>
      <leaf name="cfcard-backup-mode">
        <type name="enumeration">
          <enum name="tftp">
            <value value="2"/>
            <description>
              <text>To backup bills to bill server by tftp.</text>
            </description>
          </enum>
          <enum name="sftp">
            <value value="5"/>
            <description>
              <text>To backup bills to bill server by sftp.</text>
            </description>
          </enum>
        </type>
        <default value="sftp"/>
        <description>
          <text>Configures a mode in which bills in the CF card are transferred to a bill server.</text>
        </description>
      </leaf>
    </container>
