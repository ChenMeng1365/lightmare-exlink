  <container name="socket">
    <description>
      <text>Socket.</text>
    </description>
    <container name="tcp-global">
      <description>
        <text>Configure global tcp attribute configuration.</text>
      </description>
      <leaf name="tcp-fin-timeout">
        <type name="uint32">
          <range value="76..3600"/>
        </type>
        <units name="s"/>
        <default value="675"/>
        <description>
          <text>TCP IPv4 fin timer.</text>
        </description>
      </leaf>
      <leaf name="tcp-syn-timeout">
        <type name="uint32">
          <range value="2..600"/>
        </type>
        <units name="s"/>
        <default value="75"/>
        <description>
          <text>TCP IPv4 synchronised timer.</text>
        </description>
      </leaf>
      <leaf name="tcp-window">
        <type name="uint32">
          <range value="1..32"/>
        </type>
        <units name="kB"/>
        <default value="8"/>
        <description>
          <text>TCP IPv4 window.</text>
        </description>
      </leaf>
      <leaf name="tcp6-fin-timeout">
        <type name="uint32">
          <range value="76..3600"/>
        </type>
        <units name="s"/>
        <default value="675"/>
        <description>
          <text>TCP IPv6 fin timer.</text>
        </description>
      </leaf>
      <leaf name="tcp6-syn-timeout">
        <type name="uint32">
          <range value="2..600"/>
        </type>
        <units name="s"/>
        <default value="75"/>
        <description>
          <text>TCP IPv6 synchronised timer.</text>
        </description>
      </leaf>
      <leaf name="tcp6-window">
        <type name="uint32">
          <range value="1..32"/>
        </type>
        <units name="kB"/>
        <default value="8"/>
        <description>
          <text>TCP IPv6 window.</text>
        </description>
      </leaf>
      <leaf name="tcp-max-mss">
        <type name="uint32">
          <range value="32..9600"/>
        </type>
        <units name="Octet"/>
        <must condition="../tcp-min-mss&lt;=../tcp-max-mss"/>
        <description>
          <text>IPv4 TCP max mss.</text>
        </description>
      </leaf>
      <leaf name="tcp6-max-mss">
        <type name="uint32">
          <range value="32..9600"/>
        </type>
        <units name="Octet"/>
        <must condition="../tcp6-min-mss&lt;=../tcp6-max-mss"/>
        <description>
          <text>IPv6 TCP max mss.</text>
        </description>
      </leaf>
      <leaf name="tcp-pathmtu-timeout">
        <type name="uint32">
          <range value="10..100"/>
        </type>
        <units name="min"/>
        <description>
          <text>TCP IPv4 path MTU timer.</text>
        </description>
      </leaf>
      <leaf name="tcp-min-mss">
        <type name="uint32">
          <range value="32..9600"/>
        </type>
        <units name="Octet"/>
        <must condition="(../tcp-min-mss&lt;=../tcp-max-mss) or not(../tcp-max-mss)"/>
        <default value="216"/>
        <description>
          <text>IPv4 TCP min mss.</text>
        </description>
      </leaf>
      <leaf name="tcp6-min-mss">
        <type name="uint32">
          <range value="32..9600"/>
        </type>
        <units name="Octet"/>
        <must condition="(../tcp6-min-mss&lt;=../tcp6-max-mss) or not(../tcp6-max-mss)"/>
        <default value="216"/>
        <description>
          <text>IPv6 TCP min mss.</text>
        </description>
      </leaf>
    </container>
    <container name="tcp-infos">
      <config value="false"/>
      <description>
        <text>List of TCP status.</text>
      </description>
      <list name="tcp-info">
        <key value="family socket-id cid"/>
        <description>
          <text>Operational data of TCP status.</text>
        </description>
        <leaf name="family">
          <type name="socket-proto-type"/>
          <description>
            <text>Family.</text>
          </description>
