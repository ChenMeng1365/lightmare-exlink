<?xml version="1.0" encoding="UTF-8"?>
<module name="huawei-oam-cfm-efm"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:oam-cfm-efm="urn:huawei:yang:huawei-oam-cfm-efm"
        xmlns:ext="urn:huawei:yang:huawei-extension"
        xmlns:oammgr="urn:huawei:yang:huawei-oammgr"
        xmlns:ifm="urn:huawei:yang:huawei-ifm"
        xmlns:y1731="urn:huawei:yang:huawei-y1731"
        xmlns:efm="urn:huawei:yang:huawei-efm">
  <namespace uri="urn:huawei:yang:huawei-oam-cfm-efm"/>
  <prefix value="oam-cfm-efm"/>
  <import module="huawei-extension">
    <prefix value="ext"/>
  </import>
  <import module="huawei-oammgr">
    <prefix value="oammgr"/>
  </import>
  <import module="huawei-ifm">
    <prefix value="ifm"/>
  </import>
  <import module="huawei-y1731">
    <prefix value="y1731"/>
  </import>
  <import module="huawei-efm">
    <prefix value="efm"/>
  </import>
  <organization>
    <text>Huawei Technologies Co., Ltd.</text>
  </organization>
  <contact>
    <text>Huawei Industrial Base
Bantian, Longgang
Shenzhen 518129
People's Republic of China
Website: http://www.huawei.com
Email: support@huawei.com</text>
  </contact>
  <description>
    <text>Configure the operation, administration and maintenance (OAM) association function.</text>
  </description>
  <revision date="2020-09-18">
    <description>
      <text>Initial revision.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <ext:task-name value="oammgr"/>
  <typedef name="cfm-bind-efm-type">
    <type name="enumeration">
      <enum name="CFM-EFM">
        <value value="1"/>
        <description>
          <text>Type that CFM transfers fault information to EFM.</text>
        </description>
      </enum>
      <enum name="EFM-CFM">
        <value value="2"/>
        <description>
          <text>Type that EFM transfers fault information to CFM.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Binding type.</text>
    </description>
  </typedef>
  <augment target-node="/oammgr:oammgr">
    <description>
      <text>Configure the operation, administration and maintenance (OAM) association function.</text>
    </description>
    <container name="cfm-bind-efms">
      <description>
        <text>List of associations between CFM and EFM.</text>
      </description>
      <list name="cfm-bind-efm">
        <key value="md-name ma-name if-name bind-type"/>
        <unique tag="md-name ma-name bind-type"/>
        <unique tag="if-name bind-type"/>
        <description>
          <text>Configure an instance of an association between CFM and EFM.</text>
        </description>
        <leaf name="md-name">
          <type name="leafref">
            <path value="/y1731:y1731/y1731:global-enable/y1731:mds/y1731:md/y1731:name"/>
          </type>
          <description>
            <text>Configure an MD name.</text>
          </description>
        </leaf>
        <leaf name="ma-name">
          <type name="leafref">
            <path value="/y1731:y1731/y1731:global-enable/y1731:mds/y1731:md[y1731:name=current()/../md-name]/y1731:mas/y1731:ma/y1731:name"/>
          </type>
          <description>
            <text>Configure an MA name.</text>
          </description>
        </leaf>
        <leaf name="if-name">
          <type name="leafref">
            <path value="/ifm:ifm/ifm:interfaces/ifm:interface/ifm:name"/>
          </type>
          <must condition="/ifm:ifm/ifm:interfaces/ifm:interface[ifm:name=current()]/efm:efm/efm:session/efm:state='enable'"/>
          <description>
            <text>Configure the name of interface which the EFM instance bound.</text>
          </description>
        </leaf>
        <leaf name="bind-type">
          <type name="cfm-bind-efm-type"/>
          <description>
            <text>Configure a binding type.</text>
          </description>
        </leaf>
      </list>
    </container>
  </augment>
</module>
