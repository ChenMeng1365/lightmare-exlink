  <typedef name="diag-state">
    <type name="enumeration">
      <enum name="none">
        <value value="0"/>
        <description>
          <text>No diagnosis reason.</text>
        </description>
      </enum>
      <enum name="detect-down">
        <value value="1"/>
        <description>
          <text>Detection control times out.</text>
        </description>
      </enum>
      <enum name="echo-down">
        <value value="2"/>
        <description>
          <text>Echo detection is Down.</text>
        </description>
      </enum>
      <enum name="neighbor-down">
        <value value="3"/>
        <description>
          <text>Neighbor advertisement is Down.</text>
        </description>
      </enum>
      <enum name="forwarding-reset">
        <value value="4"/>
        <description>
          <text>The forwarding plane is restarted.</text>
        </description>
      </enum>
      <enum name="path-down">
        <value value="5"/>
        <description>
          <text>The path is Down.</text>
        </description>
      </enum>
      <enum name="concat-path-down">
        <value value="6"/>
        <description>
          <text>The cascade path is Down.</text>
        </description>
      </enum>
      <enum name="admin-down">
        <value value="7"/>
        <description>
          <text>The session is in the AdminDown state.</text>
        </description>
      </enum>
      <enum name="reverse-concat-path-down">
        <value value="8"/>
        <description>
          <text>The reverse cascade path is Down.</text>
        </description>
      </enum>
      <enum name="receive-admin-down">
        <value value="9"/>
        <description>
          <text>The remote session is in the AdminDown state.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Bfd diag state.</text>
    </description>
  </typedef>
  <typedef name="pw-role">
    <type name="enumeration">
      <enum name="master">
