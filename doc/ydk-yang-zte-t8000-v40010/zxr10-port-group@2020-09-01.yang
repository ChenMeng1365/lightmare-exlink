  typedef if-state {
    type enumeration {
      enum ok {
        value 0 ;
        description 'Interface state of port group is ok.'
                    ;
      }
      enum sf {
        value 1 ;
        description 'Interface state of port group is sf.'
                    ;
      }
      enum sd {
        value 2 ;
        description 'Interface state of port group is sd.'
                    ;
      }
