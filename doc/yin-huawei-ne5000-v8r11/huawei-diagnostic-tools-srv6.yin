  <grouping name="srv6-ping-trace-result-type">
    <description>
      <text>List of SRV6 ping and trace tests.</text>
    </description>
    <container name="ping-results">
      <config value="false"/>
      <description>
        <text>List of test result of SRV6 ping.</text>
      </description>
      <list name="ping-result">
        <key value="test-name path-index"/>
        <config value="false"/>
        <description>
          <text>Statistics of test result of SRV6 ping.</text>
        </description>
        <leaf name="test-name">
          <type name="string">
            <length value="1..32"/>
          </type>
          <config value="false"/>
          <description>
            <text>Test name.</text>
          </description>
        </leaf>
        <leaf name="path-index">
          <type name="uint32">
            <range value="1..4294967295"/>
          </type>
          <config value="false"/>
          <description>
            <text>Path index.</text>
          </description>
        </leaf>
        <leaf name="packet-receive">
          <type name="uint32">
            <range value="0..4294967295"/>
          </type>
          <config value="false"/>
          <description>
            <text>Total number of received packets.</text>
          </description>
        </leaf>
        <leaf name="packet-send">
          <type name="uint32">
            <range value="0..4294967295"/>
          </type>
          <config value="false"/>
          <description>
            <text>Total number of sent packets.</text>
          </description>
        </leaf>
        <leaf name="loss-ratio">
          <type name="pub-type:percent">
            <range value="0..100"/>
          </type>
          <units name="%"/>
          <config value="false"/>
          <description>
            <text>Packet loss ratio of a test.</text>
          </description>
        </leaf>
        <leaf name="rtt-min">
          <type name="uint32">
            <range value="0..4294967295"/>
          </type>
          <units name="ms"/>
          <config value="false"/>
          <description>
            <text>Minimum RTT of all test packets.</text>
          </description>
        </leaf>
        <leaf name="rtt-max">
          <type name="uint32">
            <range value="0..4294967295"/>
          </type>
          <units name="ms"/>
          <config value="false"/>
          <description>
            <text>Maximum RTT of all test packets.</text>
          </description>
        </leaf>
        <leaf name="rtt-average">
          <type name="uint32">
            <range value="0..4294967295"/>
          </type>
          <units name="ms"/>
          <config value="false"/>
          <description>
            <text>Average RTT of all test packets.</text>
          </description>
        </leaf>
        <leaf name="status">
          <type name="dgntl-test-status-type"/>
          <config value="false"/>
          <description>
            <text>Test status.</text>
          </description>
        </leaf>
        <leaf name="path-status">
          <type name="dgntl-test-status-type"/>
          <config value="false"/>
          <description>
            <text>Path test status.</text>
          </description>
        </leaf>
        <leaf name="preference">
          <type name="uint32">
            <range value="1..65535"/>
          </type>
          <config value="false"/>
          <description>
            <text>Path preference.</text>
          </description>
        </leaf>
        <leaf name="path-type">
          <type name="dgntl-sr-policy-path-type"/>
          <config value="false"/>
          <description>
            <text>Path type.</text>
          </description>
        </leaf>
        <leaf name="protocol-origin">
          <type name="dgntl-lsp-protocol-origin"/>
          <config value="false"/>
          <description>
            <text>Protocol origin.</text>
          </description>
        </leaf>
        <leaf name="node-address">
          <type name="inet:ipv6-address-no-zone"/>
          <config value="false"/>
          <description>
            <text>Endpoint IP address.</text>
          </description>
        </leaf>
        <leaf name="asn-no">
          <type name="uint32">
            <range value="0..65535"/>
          </type>
          <config value="false"/>
          <description>
            <text>ASN No.</text>
          </description>
        </leaf>
        <leaf name="discriminator">
          <type name="uint32">
            <range value="0..65535"/>
          </type>
          <config value="false"/>
          <description>
            <text>Discriminator.</text>
          </description>
        </leaf>
        <leaf name="segment-list-id">
          <type name="uint32">
            <range value="0..4294967295"/>
          </type>
          <config value="false"/>
          <description>
            <text>Segment-list ID.</text>
          </description>
        </leaf>
        <leaf name="xc-index">
          <type name="uint32">
            <range value="1..65535"/>
          </type>
          <config value="false"/>
          <description>
            <text>XC index.</text>
          </description>
        </leaf>
        <leaf name="end-op">
          <type name="inet:ipv6-address-no-zone"/>
          <config value="false"/>
          <description>
            <text>End-OP segment ID.</text>
          </description>
        </leaf>
        <container name="probes">
          <config value="false"/>
          <description>
            <text>List of ping results of all packets in a test.</text>
          </description>
          <list name="probe">
            <key value="sequence"/>
            <config value="false"/>
            <description>
              <text>Statistics of ping result of a single packet in a test.</text>
            </description>
            <leaf name="sequence">
              <type name="uint32">
                <range value="0..4294967295"/>
              </type>
              <config value="false"/>
              <description>
                <text>Serial number of a test packet received by a certain hop.</text>
              </description>
            </leaf>
            <leaf name="replier">
              <type name="inet:ipv6-address-no-zone"/>
              <config value="false"/>
              <description>
                <text>Destination address of the related hop in a test.</text>
              </description>
            </leaf>
            <leaf name="data-bytes">
              <type name="uint32">
                <range value="65..9600"/>
              </type>
              <config value="false"/>
              <description>
                <text>Number of bytes in a received response packet.</text>
              </description>
            </leaf>
            <leaf name="hop-limit">
              <type name="uint8">
                <range value="1..255"/>
              </type>
              <config value="false"/>
              <description>
                <text>Hop limit of received response packets.</text>
              </description>
            </leaf>
            <leaf name="rtt">
              <type name="uint32">
                <range value="0..4294967295"/>
              </type>
              <units name="ms"/>
              <config value="false"/>
              <description>
                <text>RTT of test packets.</text>
              </description>
            </leaf>
            <leaf name="result-type">
              <type name="probe-result-type"/>
              <config value="false"/>
              <description>
                <text>Result of packet sending, which is either success or timeout.</text>
              </description>
            </leaf>
            <leaf name="is-delete">
              <type name="boolean"/>
              <config value="false"/>
              <description>
                <text>Deletion flag indicating whether to delete test results.</text>
              </description>
            </leaf>
          </list>
        </container>
      </list>
    </container>
