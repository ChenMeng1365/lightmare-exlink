          <leaf name="me-direction">
            <type name="me-direction"/>
            <config value="false"/>
            <description>
              <text>ME direction.</text>
            </description>
          </leaf>
          <leaf name="select-board-slot">
            <type name="string">
              <length value="1..19"/>
            </type>
            <config value="false"/>
            <description>
              <text>Board where the state machine resides.</text>
