  <augment target-node="/sf:said-frame">
    <description>
      <text>Unicast forward configuration to the said frame.</text>
    </description>
    <container name="ip-nices">
      <description>
        <text>List of service-stream table infomations.</text>
      </description>
      <list name="ip-nice">
        <key value="source-ip dest-ip vpn-name"/>
        <description>
          <text>Configure service-stream table infomation.</text>
        </description>
        <leaf name="source-ip">
          <type name="inet:ipv4-address-no-zone"/>
          <description>
            <text>Source IP address.</text>
          </description>
        </leaf>
        <leaf name="dest-ip">
          <type name="inet:ipv4-address-no-zone"/>
          <description>
            <text>Destination IP address.</text>
          </description>
        </leaf>
        <leaf name="vpn-name">
          <type name="leafref">
            <path value="/ni:network-instance/ni:instances/ni:instance/ni:name"/>
          </type>
          <description>
            <text>VPN instance.</text>
          </description>
        </leaf>
      </list>
    </container>
    <container name="global-uc-attribute">
      <description>
        <text>Configure global unicast attributes.</text>
      </description>
      <leaf name="ipv4-route-recovery-enable">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable fault recovery for inconsistent IPv4 unicast entries.</text>
        </description>
      </leaf>
    </container>
  </augment>
  <augment target-node="/devm-vnf:devm-vnf/devm-vnf:vnf-boards/devm-vnf:vnf-board">
    <description>
      <text>Configure unicast forward configuration to the devm module.</text>
    </description>
    <container name="unicast-forward">
      <description>
        <text>Configure unicast forward.</text>
      </description>
      <container name="ipv4-uc">
        <description>
          <text>Configure slot unicast forwarding.</text>
        </description>
        <leaf name="force-frag">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable slot IPv4 force fragment.</text>
          </description>
        </leaf>
        <leaf name="icmp-reply-fast">
          <type name="boolean"/>
          <default value="true"/>
          <description>
            <text>Enable/disable slot ICMP replay fast.</text>
          </description>
        </leaf>
      </container>
      <container name="arp-fast-reply">
        <description>
          <text>Configure slot ARP reply.</text>
        </description>
        <leaf name="enable">
          <type name="boolean"/>
          <default value="true"/>
          <description>
            <text>Enable/disable slot ARP reply fast.</text>
          </description>
        </leaf>
      </container>
      <container name="gre-ttl-mode">
        <description>
          <text>Configure slot GRE TTL mode.</text>
        </description>
        <leaf name="ttl-mode">
          <type name="ttl-mode-type"/>
          <default value="pipe"/>
          <description>
            <text>Set GRE tunnel-encapsulated mode.</text>
          </description>
        </leaf>
      </container>
    </container>
  </augment>
