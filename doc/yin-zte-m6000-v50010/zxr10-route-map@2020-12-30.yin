                <description>
                  <text>Set distance of routing.</text>
                </description>
                <type name="uint8">
                  <range value="1..255">
                    <description>
                      <text>The range of distance value is from 1 to 255.</text>
                    </description>
                  </range>
                </type>
                <mandatory value="true"/>
              </leaf>
              <ncd:revise-date date="2018-11-15"/>
            </container>
            <container name="split-horizon-scope">
              <description>
                <text>Set split horizon scope.</text>
              </description>
              <presence value="split-horizon-scope"/>
