<?xml version="1.0" encoding="UTF-8"?>
<submodule name="openconfig-ospfv2-area"
           xmlns="urn:ietf:params:xml:ns:yang:yin:1"
           xmlns:oc-ospfv2="http://openconfig.net/yang/ospfv2"
           xmlns:oc-ext="http://openconfig.net/yang/openconfig-ext"
           xmlns:oc-ospf-types="http://openconfig.net/yang/ospf-types"
           xmlns:inet="urn:ietf:params:xml:ns:yang:ietf-inet-types">
  <belongs-to module="openconfig-ospfv2">
    <prefix value="oc-ospfv2"/>
  </belongs-to>
  <import module="openconfig-extensions">
    <prefix value="oc-ext"/>
  </import>
  <import module="openconfig-ospf-types">
    <prefix value="oc-ospf-types"/>
  </import>
  <import module="ietf-inet-types">
    <prefix value="inet"/>
  </import>
  <include module="openconfig-ospfv2-area-interface"/>
  <include module="openconfig-ospfv2-lsdb"/>
  <organization>
    <text>OpenConfig working group</text>
  </organization>
  <contact>
    <text>OpenConfig working group
www.openconfig.net</text>
  </contact>
  <description>
    <text>This submodule provides OSPFv2 configuration and operational
state parameters that are specific to the area context</text>
  </description>
  <oc-ext:openconfig-version semver="0.2.1"/>
  <revision date="2019-11-05">
    <description>
      <text>Fix paths in when statements is LSDB model</text>
    </description>
    <reference>
      <text>0.2.1</text>
    </reference>
  </revision>
  <revision date="2019-07-09">
    <description>
      <text>Normalise all timeticks64 to be expressed in nanoseconds.</text>
    </description>
    <reference>
      <text>0.2.0</text>
    </reference>
  </revision>
  <revision date="2018-11-21">
    <description>
      <text>Add OpenConfig module metadata extensions.</text>
    </description>
    <reference>
      <text>0.1.3</text>
    </reference>
  </revision>
  <revision date="2018-06-05">
    <description>
      <text>Bug fixes in when statements in lsdb</text>
    </description>
    <reference>
      <text>0.1.2</text>
    </reference>
  </revision>
  <revision date="2017-08-24">
    <description>
      <text>Minor formatting fixes.</text>
    </description>
    <reference>
      <text>0.1.1</text>
    </reference>
  </revision>
  <revision date="2017-02-28">
    <description>
      <text>Initial public release of OSPFv2</text>
    </description>
    <reference>
      <text>0.1.0</text>
    </reference>
  </revision>
  <revision date="2016-06-24">
    <description>
      <text>Initial revision</text>
    </description>
    <reference>
      <text>0.0.1</text>
    </reference>
  </revision>
  <grouping name="ospfv2-area-config">
    <description>
      <text>Configuration parameters relating to an OSPF area</text>
    </description>
    <leaf name="identifier">
      <type name="oc-ospf-types:ospf-area-identifier"/>
      <description>
        <text>An identifier for the OSPFv2 area - described as either a
32-bit unsigned integer, or a dotted-quad</text>
      </description>
    </leaf>
  </grouping>
  <grouping name="ospfv2-area-mpls-config">
    <description>
      <text>Configuration parameters relating to OSPFv2 extensions for
MPLS</text>
    </description>
    <leaf name="traffic-engineering-enabled">
      <type name="boolean"/>
      <description>
        <text>Specifies whether traffic engineering extensions should be
advertised within the area</text>
      </description>
    </leaf>
  </grouping>
  <grouping name="ospfv2-area-virtual-link-config">
    <description>
      <text>Configuration parameters relating to a virtual-link within
the OSPF area</text>
    </description>
    <leaf name="remote-router-id">
      <type name="inet:ipv4-address-no-zone"/>
      <description>
        <text>The router ID of the device which terminates the remote end
of the virtual link</text>
      </description>
    </leaf>
  </grouping>
  <grouping name="ospfv2-area-structure">
    <description>
      <text>Structural grouping for configuration and operational state
parameters that relate to an individual area</text>
    </description>
    <container name="config">
      <description>
        <text>Configuration parameters relating to an OSPFv2 area</text>
      </description>
      <uses name="ospfv2-area-config"/>
    </container>
    <container name="state">
      <config value="false"/>
      <description>
        <text>Operational state parameters relating to an OSPFv2 area</text>
      </description>
      <uses name="ospfv2-area-config"/>
    </container>
    <container name="mpls">
      <description>
        <text>Configuration and operational state parameters for OSPFv2
extensions relating to MPLS</text>
      </description>
      <container name="config">
        <description>
          <text>Configuration parameters relating to MPLS extensions for
OSPFv2</text>
        </description>
        <uses name="ospfv2-area-mpls-config"/>
      </container>
      <container name="state">
        <config value="false"/>
        <description>
          <text>Operational state parameters relating to MPLS extensions
for OSPFv2</text>
        </description>
        <uses name="ospfv2-area-mpls-config"/>
      </container>
    </container>
    <uses name="ospfv2-lsdb-structure"/>
    <uses name="ospfv2-area-interfaces-structure"/>
    <container name="virtual-links">
      <description>
        <text>Configuration and state parameters relating to virtual
links from the source area to a remote router</text>
      </description>
      <list name="virtual-link">
        <key value="remote-router-id"/>
        <description>
          <text>Configuration and state parameters relating to a
virtual link</text>
        </description>
        <leaf name="remote-router-id">
          <type name="leafref">
            <path value="../config/remote-router-id"/>
          </type>
          <description>
            <text>Reference to the remote router ID</text>
          </description>
        </leaf>
        <container name="config">
          <description>
            <text>Configuration parameters relating to the OSPF virtual link</text>
          </description>
          <uses name="ospfv2-area-virtual-link-config"/>
        </container>
        <container name="state">
          <config value="false"/>
          <description>
            <text>State parameters relating to the OSPF virtual link</text>
          </description>
          <uses name="ospfv2-area-virtual-link-config"/>
          <uses name="ospfv2-area-interface-neighbor-state"/>
        </container>
      </list>
    </container>
  </grouping>
</submodule>
