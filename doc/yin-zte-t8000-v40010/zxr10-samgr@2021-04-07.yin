  <revision date="2013-10-08">
    <description>
      <text>Initial version.</text>
    </description>
  </revision>
  <feature name="prefix-list">
    <description>
      <text>Prefix list track instance.</text>
    </description>
  </feature>
  <feature name="te-ecmp-group">
    <description>
      <text>Te-ecmp group track instance.</text>
    </description>
  </feature>
  <container name="configuration">
    <description>
      <text>Configuration model info.</text>
    </description>
    <container name="samgr">
      <description>
        <text>SAMGR configuration.</text>
      </description>
      <container name="tracks">
        <description>
          <text>Configure track instance.</text>
        </description>
        <list name="track">
          <description>
            <text>Configure track instance.</text>
          </description>
          <key value="name"/>
          <leaf name="name">
            <description>
              <text>Track name.</text>
            </description>
            <type name="string">
              <length value="1..31">
                <description>
                  <text>The length of track name.</text>
                </description>
              </length>
            </type>
          </leaf>
          <choice name="type">
            <description>
              <text>Detect type.</text>
            </description>
            <mandatory value="true"/>
            <case name="cfm">
              <description>
                <text>Configure CFM track instance.</text>
              </description>
              <container name="cfm-track">
                <description>
                  <text>Configure CFM track instance.</text>
                </description>
                <leaf name="md">
                  <description>
                    <text>The maintaining index of CFM,range from 1 to 65535.</text>
                  </description>
                  <type name="uint16">
                    <range value="1..65535">
                      <description>
                        <text>The value of md.</text>
                      </description>
                    </range>
                  </type>
                  <mandatory value="true"/>
                </leaf>
                <leaf name="ma">
                  <description>
                    <text>The maintaining alliance index of CFM,range from 1 to 65535.</text>
                  </description>
                  <type name="uint16">
                    <range value="1..65535">
                      <description>
                        <text>The value of ma.</text>
                      </description>
                    </range>
                  </type>
                  <mandatory value="true"/>
                </leaf>
                <leaf name="remote-mep">
                  <description>
                    <text>The Remote Mep of CFM,range from 1 to 8191.</text>
                  </description>
                  <type name="uint16">
                    <range value="1..8191">
                      <description>
                        <text>The value of remote-mep.</text>
                      </description>
                    </range>
                  </type>
                  <mandatory value="true"/>
                </leaf>
              </container>
            </case>
            <case name="efm">
              <description>
                <text>Configure EFM Track instance.This track object is dependent on interface.If interface is deleted,the track object will be deleted.</text>
              </description>
              <container name="efm-track">
                <description>
                  <text>Configure EFM Track instance.This track object is dependent on interface.If interface is deleted,the track object will be deleted.</text>
                </description>
                <leaf name="interface-name">
                  <description>
                    <text>Interface name of EFM.</text>
                  </description>
                  <type name="ncd:ifname">
                    <length value="1..31">
                      <description>
                        <text>The length of interface name.</text>
                      </description>
                    </length>
                  </type>
                  <mandatory value="true"/>
                </leaf>
              </container>
            </case>
            <case name="ping-detect">
              <description>
                <text>Configure ping-detect track instance.</text>
              </description>
              <container name="ping-detect-track">
                <description>
                  <text>Configure ping-detect track instance.</text>
                </description>
                <leaf name="group-id">
                  <description>
                    <text>Ping-detect group ID,the upper limit of this parameter depend on the device.</text>
                  </description>
                  <type name="uint32">
                    <range value="1..4294967295">
                      <description>
                        <text>The value of group id.</text>
                      </description>
                    </range>
                  </type>
                  <mandatory value="true"/>
                </leaf>
              </container>
            </case>
            <case name="mpls-oam">
              <description>
                <text>Configure MPLS-OAM track instance.</text>
              </description>
              <container name="mpls-oam-track">
                <description>
                  <text>Configure MPLS-OAM track instance.</text>
                </description>
                <leaf name="tunnel-id">
                  <description>
                    <text>MPLS-OAM tunnel ID,the upper limit of this parameter depends on the device.</text>
                  </description>
                  <type name="uint32">
                    <range value="1..4294967295">
                      <description>
                        <text>MPLS-OAM tunnel ID, the upper limit of this parameter depends on the device.</text>
                      </description>
                    </range>
                  </type>
                  <mandatory value="true"/>
                </leaf>
                <leaf name="ingress-id">
                  <description>
                    <text>MPLS-OAM ingress ID.</text>
                  </description>
                  <type name="ncd:ipv4-address"/>
                  <mandatory value="true"/>
                </leaf>
              </container>
            </case>
            <case name="vrrp">
              <description>
                <text>Configure VRRP Track instance.This track object is dependent on interface.If interface is deleted,the track object will be deleted.</text>
              </description>
              <container name="vrrp-track">
                <description>
                  <text>Configure VRRP Track instance.This track object is dependent on interface.If interface is deleted,the track object will be deleted.</text>
                </description>
                <leaf name="interface-name">
                  <description>
                    <text>Interface name of VRRP.</text>
                  </description>
                  <type name="ncd:ifname">
                    <length value="1..31">
                      <description>
                        <text>The length of interface name.</text>
                      </description>
                    </length>
                  </type>
                  <mandatory value="true"/>
                </leaf>
                <leaf name="group-id">
                  <description>
                    <text>The virtual router ID of VRRP group,range from 1 to 255.</text>
                  </description>
                  <type name="uint8">
                    <range value="1..255">
                      <description>
                        <text>The value of group id.</text>
                      </description>
                    </range>
                  </type>
                  <mandatory value="true"/>
                </leaf>
              </container>
            </case>
            <case name="sqa">
              <description>
                <text>Configure SQA Track instance.</text>
              </description>
              <container name="sqa-track">
                <description>
                  <text>Configure SQA Track instance.</text>
                </description>
                <leaf name="instance-id">
                  <description>
                    <text>SQA instance ID, the upper limit of this parameter depends on the device.</text>
                  </description>
                  <type name="uint16">
                    <range value="1..65535">
                      <description>
                        <text>The value of instance id, and the upper limit of this parameter depends on the device.</text>
                      </description>
                    </range>
                  </type>
                  <mandatory value="true"/>
                </leaf>
              </container>
            </case>
            <case name="bfd">
              <description>
                <text>Configure BFD Track instance.</text>
              </description>
              <container name="bfd-track">
                <description>
                  <text>Configure BFD Track instance.</text>
                </description>
                <leaf name="session-name">
                  <description>
                    <text>BFD session name.</text>
                  </description>
                  <type name="string">
                    <length value="1..64">
                      <description>
                        <text>The length of session name.</text>
                      </description>
                    </length>
                  </type>
                  <mandatory value="true"/>
                </leaf>
                <leaf name="session-desc">
                  <description>
                    <text>BFD session description.</text>
                  </description>
                  <type name="string">
                    <length value="1..64">
                      <description>
                        <text>The length of bfd session description.</text>
                      </description>
                    </length>
                  </type>
                  <ncd:revise-date date="2020-05-06"/>
                </leaf>
              </container>
            </case>
            <case name="pw">
              <description>
                <text>Configure PW Track instance.</text>
              </description>
              <container name="pw-track">
                <description>
                  <text>Configure PW Track instance.</text>
                </description>
                <leaf name="pw-name">
                  <description>
                    <text>PW name.</text>
                  </description>
                  <type name="string">
                    <length value="1..31">
                      <description>
                        <text>The length of pw name.</text>
                      </description>
                    </length>
                  </type>
                  <mandatory value="true"/>
                </leaf>
              </container>
            </case>
            <case name="mpls-tp-oam">
              <description>
                <text>Configure MPLS-TP OAM Track instance.</text>
              </description>
              <container name="mpls-tp-oam-track">
                <description>
                  <text>Configure MPLS-TP OAM Track instance.</text>
                </description>
                <leaf name="meg">
                  <description>
                    <text>MPLS-TP OAM meg index, the upper limit of this parameter depends on the device.</text>
                  </description>
                  <type name="uint32">
                    <range value="1..4294967295">
                      <description>
                        <text>The value of meg ID.</text>
                      </description>
                    </range>
                  </type>
                  <mandatory value="true"/>
                </leaf>
              </container>
            </case>
            <case name="interface">
              <description>
                <text>Configure interface Track instance.This track object is dependent on interface.If interface is deleted,the track object will be deleted.</text>
              </description>
              <container name="interface-track">
                <description>
                  <text>Configure interface Track instance.This track object is dependent on interface.If interface is deleted,the track object will be deleted.</text>
                </description>
                <leaf name="interface-name">
                  <description>
                    <text>Interface name.</text>
                  </description>
                  <type name="ncd:ifname">
                    <length value="1..31">
                      <description>
                        <text>The length of interface name.</text>
                      </description>
                    </length>
                  </type>
                  <mandatory value="true"/>
                </leaf>
                <leaf name="interface-status-type">
                  <description>
                    <text>The type of interface protocol status,range from 0 to 5,the full set of status values as follows:0 Layer2 status(default);1 Layer3 status;2 layer2 exclude SD status; 3 layer3 exclude SD status; 4 bidirectional SD status; 5 RX SD status.</text>
                  </description>
                  <type name="uint8">
                    <range value="0..5">
                      <description>
                        <text>The type of interface protocol status.</text>
                      </description>
                    </range>
                  </type>
                  <default value="0"/>
                  <ncd:revise-date date="2015-07-15"/>
                </leaf>
              </container>
            </case>
            <case name="te-tunnel">
              <description>
                <text>Configure te-tunnel track instance.</text>
              </description>
              <container name="te-tunnel-track">
                <description>
                  <text>Configure te-tunnel track instance.</text>
                </description>
                <leaf name="tunnel-id">
                  <description>
                    <text>Tunnel ID,the upper limit of this parameter depends on the device.</text>
                  </description>
                  <type name="uint32">
                    <range value="1..4294967295">
                      <description>
                        <text>The value of tunnel ID, and the upper limit of this parameter depends on the device.</text>
                      </description>
                    </range>
                  </type>
                  <mandatory value="true"/>
                </leaf>
              </container>
              <ncd:revise-date date="2018-05-03"/>
            </case>
            <case name="p2mp-tunnel">
              <description>
                <text>Configure P2MP tunnel track instance.</text>
              </description>
              <container name="p2mp-tunnel-track">
                <description>
                  <text>Configure p2mp tunnel track instance.</text>
                </description>
                <leaf name="tunnel-id">
                  <description>
                    <text>tunnel ID.</text>
                  </description>
                  <type name="uint32">
                    <range value="1..4294967294"/>
                  </type>
                  <mandatory value="true"/>
                </leaf>
              </container>
              <ncd:revise-date date="2018-05-03"/>
            </case>
            <case name="prefix-list">
              <description>
                <text>Configure prefix list track instance.</text>
              </description>
              <container name="prefix-list-track">
                <description>
                  <text>Configure prefix list track instance.</text>
                </description>
                <leaf name="type">
                  <description>
                    <text>IP type.</text>
                  </description>
                  <type name="enumeration">
                    <enum name="ipv4">
                      <value value="1"/>
                      <description>
                        <text>IPv4</text>
                      </description>
                    </enum>
                    <enum name="ipv6">
                      <value value="2"/>
                      <description>
                        <text>IPv6</text>
                      </description>
                    </enum>
                  </type>
                  <mandatory value="true"/>
                </leaf>
                <leaf name="prefix-name">
                  <description>
                    <text>Name of a prefix list.

These characters's ASCII unicode must be between 0x21-0x3E / 0x40-0x7E.</text>
                  </description>
                  <type name="string">
                    <length value="1..169">
                      <description>
                        <text>Name of a prefix list.</text>
                      </description>
                    </length>
                  </type>
                  <mandatory value="true"/>
                </leaf>
                <leaf name="vrf-name">
                  <description>
                    <text>VPN Routing/Forwarding instance name.

These characters's ASCII unicode must be between 0x21-0x3E / 0x40-0x7E.</text>
                  </description>
                  <type name="string">
                    <length value="1..32">
                      <description>
                        <text>VPN Routing/Forwarding instance name.</text>
                      </description>
                    </length>
                  </type>
                </leaf>
              </container>
              <ncd:revise-date date="2020-02-06"/>
            </case>
            <case name="te-ecmp-group">
              <description>
                <text>Configure te-ecmp group track instance.</text>
              </description>
              <container name="te-ecmp-group-track">
                <description>
                  <text>Configure te-ecmp-group track instance.</text>
                </description>
                <leaf name="group-id">
                  <description>
                    <text>Group ID.</text>
                  </description>
                  <type name="uint32">
                    <range value="1..128">
                      <description>
                        <text>The value of tunnel ECMP group ID, and the upper limit of this parameter depends on the device.</text>
                      </description>
                    </range>
                  </type>
                  <mandatory value="true"/>
                </leaf>
              </container>
              <ncd:revise-date date="2021-01-19"/>
            </case>
          </choice>
        </list>
      </container>
      <container name="track-groups">
        <description>
          <text>Track-group instance.</text>
        </description>
        <list name="track-group">
          <description>
            <text>Track-group instance.</text>
          </description>
          <key value="track-group-name"/>
          <leaf name="track-group-name">
            <description>
              <text>Track-group name.</text>
            </description>
            <type name="string">
              <length value="1..31">
                <description>
                  <text>The length of track-group name.</text>
                </description>
              </length>
            </type>
          </leaf>
          <leaf name="group-policy">
            <description>
              <text>Track-group policy,range from 0 to 10.The default value is 0,when the default value is 0,and all the members state is down in the track-group, the track-group state will be down.</text>
            </description>
            <type name="uint16">
              <range value="0..10">
                <description>
                  <text>The value of group policy.</text>
                </description>
              </range>
            </type>
            <default value="0"/>
          </leaf>
          <leaf name="up-delay">
            <description>
              <text>Track-group delay up time.ranges from 0s to 600s(default:0s).</text>
            </description>
            <type name="uint16">
              <range value="0..600">
                <description>
                  <text>delay time.</text>
                </description>
              </range>
            </type>
            <default value="0"/>
          </leaf>
          <container name="group-members">
            <description>
              <text>The member object of group.when the track is added to the track-group as a member,the track must be existed.If the track is deleted ,the member track will be deleted from the track-group.</text>
            </description>
            <list name="group-member">
              <description>
                <text>The member object of group.when the track is added to the track-group as a member,the track must be existed.If the track is deleted ,the member track will be deleted from the track-group.</text>
              </description>
              <key value="track-name"/>
              <leaf name="track-name">
                <description>
                  <text>The member name.</text>
                </description>
                <type name="string">
                  <length value="1..31">
                    <description>
                      <text>The length of member name</text>
                    </description>
                  </length>
                </type>
              </leaf>
            </list>
          </container>
        </list>
      </container>
      <container name="sa-binds">
        <description>
          <text>The bind instance.when the bind instance is created,the track and track-group that is used to create the bind relation must be existed.If the track or track-group is deleted, the bind relation will be deleted.</text>
        </description>
        <list name="sa-bind">
          <description>
            <text>The bind instance.when the bind instance is created,the track and track-group that is used to create the bind relation must be existed.If the track or track-group is deleted, the bind relation will be deleted.</text>
          </description>
          <key value="passive-name"/>
          <leaf name="passive-name">
            <description>
              <text>The passive track name.</text>
            </description>
            <type name="string">
              <length value="1..31">
                <description>
                  <text>The length of passive name.</text>
                </description>
              </length>
            </type>
          </leaf>
          <choice name="active-type">
            <description>
              <text>The active type.</text>
            </description>
            <mandatory value="true"/>
            <case name="active-track">
              <description>
                <text>The active track.</text>
              </description>
              <container name="track">
                <description>
                  <text>The active track name.</text>
                </description>
                <leaf name="active-track-name">
                  <description>
                    <text>The active track name.</text>
                  </description>
                  <type name="string">
                    <length value="1..31">
                      <description>
                        <text>The length of active track name.</text>
                      </description>
                    </length>
                  </type>
                  <mandatory value="true"/>
                </leaf>
              </container>
            </case>
            <case name="active-group">
              <description>
                <text>The active group.</text>
              </description>
              <container name="group">
                <description>
                  <text>The active track-group name.</text>
                </description>
                <leaf name="active-group-name">
                  <description>
                    <text>The active track-group name.</text>
                  </description>
                  <type name="string">
                    <length value="1..31">
                      <description>
                        <text>The length of active group name.</text>
                      </description>
                    </length>
                  </type>
                  <mandatory value="true"/>
                </leaf>
              </container>
            </case>
          </choice>
        </list>
      </container>
    </container>
  </container>
  <container name="state">
    <config value="false"/>
    <description>
      <text>SAMGR state model Info.</text>
    </description>
    <container name="samgr">
      <description>
        <text>SAMGR state parameters.</text>
      </description>
      <container name="tracks">
        <description>
          <text>Track info.</text>
        </description>
        <list name="track">
          <description>
            <text>Track info.</text>
          </description>
          <key value="track-name"/>
          <leaf name="track-name">
            <description>
              <text>Track name.</text>
            </description>
            <type name="string">
              <length value="1..31">
                <description>
                  <text>The length of track name.</text>
                </description>
              </length>
            </type>
          </leaf>
          <leaf name="trans-state">
            <description>
              <text>Track transfer state.</text>
            </description>
            <type name="enumeration">
              <enum name="trans-ok">
                <value value="0"/>
                <description>
                  <text>Track trans state is ok.</text>
                </description>
              </enum>
              <enum name="trans-down">
                <value value="1"/>
                <description>
                  <text>Track trans state is down.</text>
                </description>
              </enum>
            </type>
          </leaf>
          <leaf name="track-state">
            <description>
              <text>Track state.</text>
            </description>
            <type name="enumeration">
              <enum name="unknown">
                <value value="0"/>
                <description>
                  <text>Track state is unknown.</text>
                </description>
              </enum>
              <enum name="up">
                <value value="1"/>
                <description>
                  <text>Track state is up.</text>
                </description>
              </enum>
              <enum name="local-down">
                <value value="2"/>
                <description>
                  <text>Track state is local-down.</text>
                </description>
              </enum>
              <enum name="admin-down">
                <value value="3"/>
