  <container name="configuration">
    <description>
      <text>ARP configuration commands.</text>
    </description>
    <container name="arp">
      <description>
        <text>ARP configuration commands.</text>
      </description>
      <container name="interface-container">
        <description>
          <text>Commands under interface mode about ARP.</text>
        </description>
        <list name="interface-list">
          <description>
            <text>Commands under interface mode about ARP.</text>
          </description>
          <key value="intf-name"/>
          <leaf name="intf-name">
            <description>
              <text>Interface name.</text>
            </description>
            <type name="ncd:ifname"/>
          </leaf>
          <leaf name="timeout">
            <description>
              <text>Dynamic ARP entries of aging time.</text>
            </description>
            <type name="uint32">
              <range value="1..2147483">
                <description>
                  <text>From 1 To 2147483.</text>
                </description>
              </range>
            </type>
            <units name="seconds"/>
            <default value="14400"/>
          </leaf>
          <leaf name="purge-delay">
            <description>
              <text>ARP purge delay.</text>
            </description>
            <type name="uint32">
              <range value="1..36000">
                <description>
                  <text>From 1 to 36000.</text>
                </description>
              </range>
            </type>
            <units name="seconds"/>
            <default value="600"/>
          </leaf>
          <leaf name="protect-num">
            <description>
              <text>Protect limit number.Its value depends on the performance parameters.</text>
            </description>
            <type name="uint32">
              <range value="0..262144">
                <description>
                  <text>From 0 to 262144.</text>
                </description>
              </range>
            </type>
            <default value="0"/>
          </leaf>
          <leaf name="limit-time">
            <description>
              <text>ARP limit time.</text>
            </description>
            <type name="uint32">
              <range value="0..1000">
                <description>
                  <text>From 0 to 1000.</text>
                </description>
              </range>
            </type>
            <units name="seconds"/>
            <default value="10"/>
          </leaf>
          <leaf name="port-speed">
            <description>
              <text>Packets number per second of the port.</text>
            </description>
            <type name="uint32">
              <range value="0..1000">
                <description>
                  <text>From 0 to 1000.</text>
                </description>
              </range>
            </type>
            <units name="number-per-second"/>
            <default value="0"/>
          </leaf>
          <leaf name="proxy">
            <description>
              <text>ARP proxy.</text>
            </description>
            <type name="enumeration">
              <enum name="disable">
                <value value="0"/>
                <description>
                  <text>Disable.</text>
                </description>
              </enum>
              <enum name="enable">
                <value value="1"/>
                <description>
                  <text>Enable.</text>
                </description>
              </enum>
            </type>
            <default value="disable"/>
          </leaf>
          <leaf name="learn-disable">
            <description>
              <text>Disable ARP learning function.</text>
            </description>
            <type name="enumeration">
              <enum name="disable">
                <value value="1"/>
                <description>
                  <text>Disable.</text>
                </description>
              </enum>
              <enum name="enable">
                <value value="2"/>
                <description>
                  <text>Enable.</text>
                </description>
              </enum>
            </type>
            <default value="disable"/>
          </leaf>
          <leaf name="source-filtered">
            <description>
              <text>The interface source-filtered.</text>
            </description>
            <type name="enumeration">
              <enum name="enable">
                <value value="1"/>
                <description>
                  <text>Enable.</text>
                </description>
              </enum>
              <enum name="disable">
                <value value="2"/>
                <description>
                  <text>Disable.</text>
                </description>
              </enum>
            </type>
            <default value="enable"/>
          </leaf>
          <leaf name="local-proxy-arp">
            <description>
              <text>ARP local proxy in the port.</text>
            </description>
            <type name="enumeration">
              <enum name="disable">
                <value value="0"/>
                <description>
                  <text>Disable.</text>
                </description>
              </enum>
              <enum name="enable">
                <value value="1"/>
                <description>
                  <text>Enable.</text>
                </description>
              </enum>
            </type>
            <default value="disable"/>
          </leaf>
          <leaf name="proxy-local">
            <description>
              <text>Under the same VLAN ARP proxy function.</text>
            </description>
            <type name="enumeration">
              <enum name="disable">
                <value value="0"/>
                <description>
                  <text>Disable.</text>
                </description>
              </enum>
              <enum name="enable">
                <value value="1"/>
                <description>
                  <text>Enable.</text>
                </description>
              </enum>
            </type>
            <default value="disable"/>
          </leaf>
          <leaf name="learn-limit">
            <description>
              <text>Limit ARP learning function.</text>
            </description>
            <type name="enumeration">
              <enum name="enable">
                <value value="1"/>
                <description>
                  <text>Enable.</text>
                </description>
              </enum>
              <enum name="disable">
                <value value="2"/>
                <description>
                  <text>Disable.</text>
                </description>
              </enum>
            </type>
            <default value="disable"/>
          </leaf>
          <leaf name="backupvrrp-learn-flag">
            <description>
              <text>Backup VRRP ARP learning function.</text>
            </description>
            <type name="enumeration">
              <enum name="enable">
                <value value="1"/>
                <description>
                  <text>Enable.</text>
                </description>
              </enum>
              <enum name="disable">
                <value value="2"/>
                <description>
                  <text>Disable.</text>
                </description>
              </enum>
            </type>
            <default value="disable"/>
          </leaf>
          <leaf name="gratuitous-learn-flag">
            <description>
              <text>Gratuitous ARP learning function.</text>
            </description>
            <type name="enumeration">
              <enum name="enable">
                <value value="1"/>
                <description>
                  <text>Enable.</text>
                </description>
              </enum>
              <enum name="disable">
                <value value="2"/>
                <description>
                  <text>Disable.</text>
                </description>
              </enum>
            </type>
            <default value="disable"/>
          </leaf>
          <leaf name="network-learn-flag">
            <description>
              <text>Network ARP learning function.</text>
            </description>
            <type name="enumeration">
              <enum name="enable">
                <value value="1"/>
                <description>
                  <text>Enable.</text>
                </description>
              </enum>
              <enum name="disable">
                <value value="2"/>
                <description>
                  <text>Disable.</text>
                </description>
              </enum>
            </type>
            <default value="disable"/>
          </leaf>
          <leaf name="limit-alarm-threshold">
            <description>
              <text>ARP learn-limit alarm-threshold.</text>
            </description>
            <type name="uint32">
              <range value="1..65535">
                <description>
                  <text>From 1 to 65535.</text>
                </description>
              </range>
            </type>
            <default value="300"/>
          </leaf>
          <leaf name="filter-alarm-thresshold">
            <description>
              <text>ARP source-filter alarm-threshold.</text>
            </description>
            <type name="uint32">
              <range value="1..65535">
                <description>
                  <text>From 1 to 65535.</text>
                </description>
              </range>
            </type>
            <default value="300"/>
          </leaf>
          <leaf name="temp-arp-timeout">
            <description>
              <text>Temporary ARP aging time.Its maximum which is between 10-100 and generally 10, depends on pruduct specification.</text>
            </description>
            <type name="uint32">
              <range value="1..100">
                <description>
                  <text>From 1 to 100.</text>
                </description>
              </range>
            </type>
            <units name="seconds"/>
            <default value="3"/>
          </leaf>
          <leaf name="inter-vlan-proxy">
            <description>
              <text>Set ARP proxy function between different VLANs in the same interface.</text>
            </description>
            <type name="enumeration">
              <enum name="enable">
                <value value="1"/>
                <description>
                  <text>Enable.</text>
                </description>
              </enum>
              <enum name="disable">
                <value value="2"/>
                <description>
                  <text>Disable.</text>
                </description>
              </enum>
            </type>
            <default value="disable"/>
          </leaf>
          <leaf name="global-proxy">
            <description>
              <text>ARP global proxy.</text>
            </description>
            <type name="enumeration">
              <enum name="disable">
                <value value="0"/>
                <description>
                  <text>Disable global proxy function.</text>
                </description>
              </enum>
              <enum name="enable">
                <value value="1"/>
                <description>
                  <text>Enable global proxy function.</text>
                </description>
              </enum>
            </type>
            <default value="disable"/>
            <ncd:revise-date date="2017-10-10"/>
          </leaf>
          <container name="gratuitous-proxy">
            <description>
              <text>Gratuitous proxy ARP.</text>
            </description>
            <leaf name="gratuitous-proxy-time">
              <description>
                <text>Gratuitous proxy ARP regularly send interval.</text>
              </description>
              <type name="uint32">
                <range value="1..14400">
                  <description>
                    <text>From 1 to 14400.</text>
                  </description>
                </range>
              </type>
              <units name="seconds"/>
              <default value="30"/>
            </leaf>
          </container>
          <container name="freearp">
            <description>
              <text>Regularly send gratuitous ARP message sign of the port.</text>
            </description>
            <leaf name="freearp-time">
              <description>
                <text>The time interval of send gratuitous ARP message.</text>
              </description>
              <type name="uint32">
                <range value="10..14400">
                  <description>
                    <text>From 10 to 14400.</text>
                  </description>
                </range>
              </type>
              <units name="seconds"/>
              <default value="30"/>
            </leaf>
          </container>
          <leaf name="rarp">
