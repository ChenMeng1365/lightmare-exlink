<?xml version="1.0" encoding="UTF-8"?>
<module name="huawei-mstp-notification"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:mstp-notification="urn:huawei:yang:huawei-mstp-notification"
        xmlns:pub-type="urn:huawei:yang:huawei-pub-type"
        xmlns:ext="urn:huawei:yang:huawei-extension">
  <yang-version value="1"/>
  <namespace uri="urn:huawei:yang:huawei-mstp-notification"/>
  <prefix value="mstp-notification"/>
  <import module="huawei-pub-type">
    <prefix value="pub-type"/>
  </import>
  <import module="huawei-extension">
    <prefix value="ext"/>
  </import>
  <organization>
    <text>Huawei Technologies Co., Ltd.</text>
  </organization>
  <contact>
    <text>Huawei Industrial Base
Bantian, Longgang
Shenzhen 518129
People's Republic of China
Website: http://www.huawei.com
Email: support@huawei.com</text>
  </contact>
  <description>
    <text>Notification of mstp.</text>
  </description>
  <revision date="2019-07-08">
    <description>
      <text>Initial revision.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <ext:task-name value="mstp"/>
  <notification name="tc-flap-receive">
    <description>
      <text>The local device received many TC BPDUs from neighbors.</text>
    </description>
    <leaf name="process-id">
      <type name="uint32"/>
      <description>
        <text>Process ID.</text>
      </description>
    </leaf>
    <leaf name="instance-id">
      <type name="uint32"/>
      <description>
        <text>MSTP instance ID.</text>
      </description>
    </leaf>
    <leaf name="if-index">
      <type name="uint32"/>
      <description>
        <text>Interface index.</text>
      </description>
    </leaf>
    <leaf name="if-name">
      <type name="pub-type:if-name"/>
      <description>
        <text>Interface name.</text>
      </description>
    </leaf>
    <leaf name="pw-name">
      <type name="string">
        <length value="1..15"/>
      </type>
      <description>
        <text>PW name.</text>
      </description>
    </leaf>
    <leaf name="src-mac">
      <type name="pub-type:mac-address"/>
      <description>
        <text>Source MAC address.</text>
      </description>
    </leaf>
  </notification>
  <notification name="tc-flap">
    <description>
      <text>The local device proactively detected that its STP topology was flapping.</text>
    </description>
    <leaf name="process-id">
      <type name="uint32"/>
      <description>
        <text>MSTP process ID.</text>
      </description>
    </leaf>
    <leaf name="instance-id">
      <type name="uint32"/>
      <description>
        <text>Multiple spanning tree instance (MSTI) ID.</text>
      </description>
    </leaf>
  </notification>
  <notification name="tc-flap-resume">
    <description>
      <text>The STP topology of the local device stopped flapping.</text>
    </description>
    <leaf name="process-id">
      <type name="uint32"/>
      <description>
        <text>MSTP process ID.</text>
      </description>
    </leaf>
    <leaf name="instance-id">
      <type name="uint32"/>
      <description>
        <text>Multiple spanning tree instance (MSTI) ID.</text>
      </description>
    </leaf>
  </notification>
</module>
