      <container name="global-buffer-share-queues">
        <description>
          <text>List of the shared threshold of buffer resources for specific queues.</text>
        </description>
        <list name="global-buffer-share-queue">
          <key value="service-class direction"/>
          <description>
            <text>Configure the shared threshold of buffer resources for specific queues.</text>
          </description>
          <leaf name="service-class">
            <type name="qos:qos-service-class"/>
            <description>
              <text>Class of service.</text>
            </description>
          </leaf>
          <leaf name="direction">
            <type name="qos:qos-direction-type"/>
            <description>
              <text>Direction.</text>
            </description>
          </leaf>
        </list>
      </container>
      <container name="flow-queue-backpressure-off">
        <presence value="create flow-queue-backpressure-off"/>
        <description>
          <text>Enable/disable the PIR backpressure function. When the configuration exists, the function is disable.</text>
        </description>
      </container>
      <container name="traffic-policy-match-type">
        <description>
          <text>Configure policy UCL rule match.</text>
        </description>
        <container name="destination-user">
          <presence value="create destination-user"/>
          <description>
            <text>Enable/disable traffic policy match type destination user.</text>
          </description>
        </container>
        <container name="interface-acl">
          <presence value="create interface-acl"/>
          <description>
            <text>Enable/disable traffic policy match type interface acl.</text>
          </description>
        </container>
      </container>
      <container name="user-queue-resource-4cos">
        <description>
          <text>Configure 4queue CoS size.</text>
        </description>
        <leaf name="cos-value">
          <type name="uint32">
            <range value="4..64000"/>
          </type>
          <must condition="../cos-value mod 4 = 0"/>
          <description>
            <text>Cos value.</text>
          </description>
        </leaf>
      </container>
      <container name="user-queue-resource-8cos-enhances">
        <description>
          <text>List of 8queue CoS size configuration.</text>
        </description>
        <list name="user-queue-resource-8cos-enhance">
          <key value="direction"/>
          <max-elements value="2"/>
          <description>
            <text>Configure 8queue CoS size.</text>
          </description>
          <leaf name="direction">
            <type name="qos:qos-direction-type"/>
            <description>
              <text>Direction.</text>
            </description>
          </leaf>
          <leaf name="cos-value">
            <type name="uint32">
              <range value="4..7000"/>
            </type>
            <must condition="../cos-value mod 4 = 0"/>
