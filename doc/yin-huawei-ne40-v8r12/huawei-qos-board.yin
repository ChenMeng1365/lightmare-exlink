        <leaf name="adjust-number">
          <type name="uint16">
            <range value="1..1000"/>
          </type>
          <default value="50"/>
          <description>
            <text>Adjust number value.</text>
          </description>
        </leaf>
      </container>
      <container name="over-ride-degrade-cycle">
        <description>
          <text>Configure over ride degrade cycle.</text>
        </description>
        <leaf name="degrade-cycle">
          <type name="uint8">
            <range value="1..255"/>
          </type>
          <default value="64"/>
          <description>
            <text>Cycle number.</text>
          </description>
        </leaf>
      </container>
      <container name="over-ride-upgrade-cycle">
        <description>
          <text>Configure over ride upgrade cycle.</text>
        </description>
        <leaf name="upgrade-cycle">
          <type name="uint8">
            <range value="1..255"/>
          </type>
          <default value="1"/>
          <description>
            <text>Cycle number.</text>
          </description>
        </leaf>
      </container>
      <container name="over-ride-switch">
        <must condition="./enable-percent&gt;=./disable-percent"/>
        <description>
          <text>Configure over ride switch.</text>
        </description>
        <leaf name="disable-percent">
          <type name="uint8">
            <range value="0..100"/>
          </type>
          <units name="%"/>
          <default value="50"/>
          <description>
            <text>Disable percent value.</text>
          </description>
        </leaf>
        <leaf name="enable-percent">
          <type name="uint8">
            <range value="0..100"/>
          </type>
          <units name="%"/>
          <default value="80"/>
          <description>
            <text>Enable percent value.</text>
          </description>
        </leaf>
      </container>
      <container name="over-ride-inbound-threshold-adjust">
