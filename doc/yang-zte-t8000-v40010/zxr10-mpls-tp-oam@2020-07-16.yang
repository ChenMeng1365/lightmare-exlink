                units s ;
                default 0 ;
              }
              leaf send-interval {
                description 'The sending period of on-demand LM packets, ranges'
                          + ' from 1 to 100(units:100ms, default: 10).'
                            ;
                type uint8 {
                  range 1..100 {
                    description 'The value of sending interval.'
                                ;
                  }
                }
                units 100ms ;
                default 10 ;
              }
              leaf type {
                description 'The on-demand LM type, including G.8114 and Y.1731'
                          + '. G.8114, counting the data packets; Y.1731, count'
                          + 'ing the data packets and OAM packets(default: G.81'
                          + '14).'
                            ;
                type enumeration {
                  enum g8114 {
                    value 1 ;
                    description 'Counting the data packets.'
                                ;
                  }
                  enum y1731 {
                    value 2 ;
                    description 'Counting the data packets and OAM packets.'
                                ;
                  }
                }
                default g8114 ;
              }
              leaf report-interval {
                description 'The time which is timing upsended in the function '
                          + 'of LM, when it ranges from 1s to 86400s, it means '
                          + 'timing upsend the results;  '+"\n"
                          + 'If this node assigned 4294967295, it means not ups'
                          + 'end results unless mannual stopping;'+"\n"
                          + 'The default value is 0,it means not upsend results'
                          + '.'
                            ;
                type uint32 {
                  range '0 | 1..86400 | 4294967295'
                        ;
                }
                units s ;
                default 0 ;
              }
            }
            case disable {
              description 'Turn off LM function.'
                          ;
              leaf disable {
                description 'Turn off LM function.'
                            ;
                type empty ;
                mandatory true ;
              }
            }
          }
        }
      }
      container dms {
        description 'The operation of on-demand DM function.'
                    ;
        list dm {
          description 'The operation of on-demand DM function.'
                      ;
          key meg-index ;
          leaf meg-index {
            description 'The value of MEG index,the upper limit of this paramet'
                      + 'er depends on the device.'
                        ;
            type uint32 {
              range 1..4294967295 {
                description 'The value of MEG index.'
                            ;
              }
            }
          }
          choice status {
            description 'The switch for turning on/off DM function.'
                        ;
            mandatory true ;
            case enable {
              description 'Turn off DM function.'
                          ;
              leaf enable {
                description 'Turn off DM function.'
                            ;
                type empty ;
                mandatory true ;
              }
              leaf type {
                description 'On-demand DM type, inculding one-way and two-way.'
                            ;
                type enumeration {
                  enum one-way {
                    value 1 ;
                    description 'One-way DM.'
                                ;
                  }
                  enum two-way {
                    value 2 ;
                    description 'Two-way DM.'
                                ;
                  }
                }
                mandatory true ;
              }
              leaf phb {
                description 'The PHB value carried in DM packets(default: cs7).'
                            ;
                type phb ;
                default cs7 ;
              }
              leaf send-time {
                description 'Duration time in the function of on-demand DM. It '
                          + 'ranges from 10s to 24h with the configration of pe'
                          + 'rmanence detecting. If this node assigned 0, it me'
                          + 'ans permanence detecting unless mannual stopping. '
                          + 'If assigned 10-86400, it means detecting lasting t'
                          + 'ime.'
                            ;
                type uint32 {
                  range '0 | 10..86400'
                        {
                    description '0: running all time unless mannual stopping; 1'
                              + '0-86400: the value of runnning time.'
                                ;
                  }
                }
                units s ;
                default 0 ;
              }
              leaf send-interval {
                description 'The sending period of on-demand DM packets, ranges'
                          + ' from 1 to 100(units:100ms, default: 10).'
                            ;
                type uint8 {
                  range 1..100 {
                    description 'The value of sending interval.'
                                ;
                  }
                }
                units 100ms ;
                default 10 ;
              }
              leaf tlv-size {
                description 'The TLV size of on-demand DM function, ranges from'
                          + ' 0 to 400(default: 0).If TLV size is 0, it means D'
                          + 'M packets will not carry TLV.'
                            ;
                type uint32 {
                  range 0..400 {
                    description 'The value of TLV size.'
                                ;
                  }
                }
                default 0 ;
              }
              leaf report-interval {
                description 'The time which is timing upsended in the function '
                          + 'of DM, when it ranges from 1s to 86400s, it means '
                          + 'timing upsend the results;  '+"\n"
                          + 'If this node assigned 4294967295, it means not ups'
                          + 'end results unless mannual stopping.'+"\n"
                          + 'The default value is 0,it means not upsend results'
                          + '.'
                            ;
                type uint32 {
                  range '0 | 1..86400 | 4294967295'
                        ;
                }
                units s ;
                default 0 ;
              }
            }
            case disable {
              description 'Turn off DM function.'
                          ;
              leaf disable {
                description 'Turn off DM function.'
                            ;
                type empty ;
              }
            }
          }
        }
      }
    }
  }
