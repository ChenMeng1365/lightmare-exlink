    <container name="domain-servers">
      <description>
        <text>List of domain syslog servers.</text>
      </description>
      <list name="domain-server">
        <key value="server-domain is-default-vpn vrf-name"/>
        <max-elements value="8"/>
        <description>
          <text>Configure the domain log sever that receives logs.</text>
        </description>
        <leaf name="server-domain">
          <type name="string">
            <length value="1..255"/>
          </type>
          <description>
            <text>Log server domian address.</text>
          </description>
        </leaf>
        <leaf name="is-default-vpn">
          <type name="boolean"/>
          <description>
            <text>Enable/disable the default VPN.</text>
          </description>
        </leaf>
        <leaf name="vrf-name">
          <type name="leafref">
            <path value="/ni:network-instance/ni:instances/ni:instance/ni:name"/>
          </type>
          <must condition="not(../is-default-vpn='true') or (../is-default-vpn='true' and ../vrf-name='_public_')"/>
          <description>
            <text>VPN name on a log server.</text>
          </description>
        </leaf>
        <leaf name="level">
          <type name="log-level"/>
          <default value="debugging"/>
          <description>
            <text>Level of logs saved on a log server.</text>
          </description>
        </leaf>
        <leaf name="server-port">
          <type name="uint32">
            <range value="1..65535"/>
          </type>
          <default value="514"/>
          <description>
            <text>Number of a port recving logs.</text>
          </description>
        </leaf>
        <leaf name="facility">
          <type name="loghost-facility"/>
          <default value="local7"/>
          <description>
            <text>Log record tool.</text>
          </description>
        </leaf>
        <leaf name="channel-id">
          <type name="leafref">
            <path value="/syslog:syslog/syslog:info-center-channels/syslog:info-center-channel/syslog:channel-id"/>
          </type>
          <default value="2"/>
          <description>
            <text>Channel number.</text>
          </description>
        </leaf>
        <leaf name="channel-name">
          <type name="string">
            <length value="1..30"/>
          </type>
          <default value="loghost"/>
          <config value="false"/>
          <description>
            <text>Channel name.</text>
          </description>
        </leaf>
        <leaf name="timestamp">
          <type name="timestamp-format"/>
          <default value="UTC"/>
          <description>
            <text>Log server timestamp.</text>
          </description>
        </leaf>
        <leaf name="transport-mode">
          <type name="transport-mode-type"/>
          <default value="udp"/>
          <description>
            <text>Transport mode.</text>
          </description>
        </leaf>
        <leaf name="ssl-policy-name">
          <type name="leafref">
            <path value="/ssl:ssl/ssl:ssl-policys/ssl:ssl-policy/ssl:policy-name"/>
          </type>
          <must condition="../transport-mode='tcp'"/>
          <description>
            <text>SSL policy name.</text>
          </description>
        </leaf>
        <leaf name="is-brief-format">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable Syslog Brief Format.</text>
          </description>
        </leaf>
        <leaf name="is-security-only">
          <type name="boolean"/>
          <must condition="../is-security-only='false' or (../is-security-only='true' and ../ssl-policy-name)"/>
          <default value="false"/>
          <description>
            <text>Enable/disable Syslog support security log.</text>
          </description>
        </leaf>
      </list>
