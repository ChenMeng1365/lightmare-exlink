<?xml version="1.0" encoding="UTF-8"?>
<module name="huawei-pub-type"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:pub-type="urn:huawei:yang:huawei-pub-type"
        xmlns:ext="urn:huawei:yang:huawei-extension">
  <namespace uri="urn:huawei:yang:huawei-pub-type"/>
  <prefix value="pub-type"/>
  <import module="huawei-extension">
    <prefix value="ext"/>
  </import>
  <organization>
    <text>Huawei Technologies Co., Ltd.</text>
  </organization>
  <contact>
    <text>Huawei Industrial Base
Bantian, Longgang
Shenzhen 518129
People's Republic of China
Website: http://www.huawei.com
Email: support@huawei.com</text>
  </contact>
  <description>
    <text>This YANG module define public datatypes.</text>
  </description>
  <revision date="2021-08-11">
    <description>
      <text>Modify revision data.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <revision date="2020-04-01">
    <description>
      <text>Modify hex-binary type.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <revision date="2020-02-05">
    <description>
      <text>Add password type.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <revision date="2019-09-26">
    <description>
      <text>Modify description and status.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <revision date="2019-07-18">
    <description>
      <text>Modify regular expression to mac-address.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <revision date="2019-07-08">
    <description>
      <text>Add a regular expression to mac-address.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <revision date="2019-04-27">
    <description>
      <text>Initial revision.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <typedef name="time-hhmm">
    <type name="string">
      <length value="3..5"/>
      <pattern value="(([0-1][0-9])|(2[0-3])|[0-9]):([0-5][0-9]|[0-9])"/>
    </type>
    <description>
      <text>Time in standard notation, is like string with format 'hh:mm', and hh is hour, mm is minute, range is 00:00~23:59.</text>
    </description>
  </typedef>
  <typedef name="mac-address">
    <type name="string">
      <length value="0..255"/>
      <pattern value="[0-9a-f]{4}(-[0-9a-f]{4}){2}"/>
      <ext:case-sensitivity value="lower-only"/>
    </type>
    <description>
      <text>A MAC address in standard notation.</text>
    </description>
  </typedef>
  <typedef name="ipv6-address-prefix">
    <type name="string">
      <length value="0..255"/>
    </type>
    <description>
      <text>IPv6 address prefix in standard notation.</text>
    </description>
  </typedef>
  <typedef name="block">
    <type name="string">
      <length value="0..255"/>
    </type>
    <description>
      <text>Block in standard notation.</text>
    </description>
  </typedef>
  <typedef name="slot-path">
    <type name="string">
      <length value="1..127"/>
      <pattern value="\d+/\d+"/>
    </type>
    <description>
      <text>Slot path is like '1/1'.</text>
    </description>
  </typedef>
  <typedef name="if-name">
    <type name="string">
      <length value="1..63"/>
    </type>
    <description>
      <text>Interface name is like ethernet1/1/1/1.</text>
    </description>
  </typedef>
  <typedef name="position">
    <type name="string">
      <length value="1..32"/>
    </type>
    <description>
      <text>Board position in standard notation, serial-port position is like[1/1/1/1:2:3].</text>
    </description>
  </typedef>
  <typedef name="password">
    <type name="string">
      <length value="1..128"/>
    </type>
    <description>
      <text>The password is a string ranging from 1 to 16 characters for a unencrypted password and 20 to 128 characters for a encrypted password.</text>
    </description>
  </typedef>
  <typedef name="password-extend">
    <type name="string">
      <length value="1..432"/>
    </type>
    <description>
      <text>The password is a string ranging from 1 to 255 characters for a unencrypted password and 20 to 432 characters for a encrypted password.</text>
    </description>
  </typedef>
  <typedef name="one-input-password-extend">
    <type name="string">
      <length value="1..432"/>
    </type>
    <description>
      <text>The password is a string ranging from 1 to 255 characters for a plain text password and 20 to 432 characters for a cipher text password.</text>
    </description>
  </typedef>
  <typedef name="vrf-name">
    <type name="string">
      <length value="1..31"/>
    </type>
    <description>
      <text>VRF name in standard notation.</text>
    </description>
  </typedef>
  <typedef name="time-tick">
    <type name="uint32"/>
    <description>
      <text>Time tick in standard notation, represents the time in SECOND between two epochs.</text>
    </description>
  </typedef>
  <typedef name="identifier-string">
    <type name="string">
      <pattern value="([^\s?|&gt;]*)"/>
    </type>
    <description>
      <text>Identifier string is used for identifier with strict limitation.</text>
    </description>
  </typedef>
  <typedef name="description-string">
    <type name="string">
      <pattern value="([^?]*)"/>
    </type>
    <description>
      <text>Description string is used for description with little limitation.</text>
    </description>
  </typedef>
  <typedef name="row-status">
    <type name="enumeration">
      <enum name="active">
        <value value="0"/>
        <description>
          <text>Rule status is active at the current time.</text>
        </description>
      </enum>
      <enum name="not-in-service">
        <value value="1"/>
        <description>
          <text>Rule status is not active at the current time.</text>
        </description>
      </enum>
      <enum name="not-ready">
        <value value="2"/>
        <description>
          <text>The system is unavailable at the moment.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Row-status indicates current status.</text>
    </description>
  </typedef>
  <typedef name="bits8">
    <type name="uint8"/>
    <status value="deprecated"/>
    <description>
      <text>Bits8 is like uint8. It is strongly recommended to use the standard built-in types of 'bits' instead of bits8/bits16/bits32/bits64.</text>
    </description>
  </typedef>
  <typedef name="bits16">
    <type name="uint16"/>
    <status value="deprecated"/>
    <description>
      <text>Bits16 is like uint16. It is strongly recommended to use the standard built-in types of 'bits' instead of bits8/bits16/bits32/bits64.</text>
    </description>
  </typedef>
  <typedef name="bits32">
    <type name="uint32"/>
    <status value="deprecated"/>
    <description>
      <text>Bits32 is like uint32. It is strongly recommended to use the standard built-in types of 'bits' instead of bits8/bits16/bits32/bits64.</text>
    </description>
  </typedef>
  <typedef name="bits64">
    <type name="uint64"/>
    <status value="deprecated"/>
    <description>
      <text>Bits64 is like uint64. It is strongly recommended to use the standard built-in types of 'bits' instead of bits8/bits16/bits32/bits64.</text>
    </description>
  </typedef>
  <typedef name="percent">
    <type name="uint8">
      <range value="0..100"/>
    </type>
    <units name="%"/>
    <description>
      <text>Percent is like uint8.</text>
    </description>
  </typedef>
  <typedef name="band-width">
    <type name="uint64"/>
    <units name="bit/s"/>
    <description>
      <text>The speed of interfaces.</text>
    </description>
  </typedef>
  <typedef name="app-type">
    <type name="enumeration">
      <enum name="lct">
        <value value="0"/>
        <description>
          <text>App-type is LCT.</text>
        </description>
      </enum>
      <enum name="ems">
        <value value="1"/>
        <description>
          <text>App-type is EMS.</text>
        </description>
      </enum>
      <enum name="cmd">
        <value value="2"/>
        <description>
          <text>App-type is CMD.</text>
        </description>
      </enum>
      <enum name="ftp">
        <value value="3"/>
        <description>
          <text>App-type is FTP.</text>
        </description>
      </enum>
      <enum name="unknown">
        <value value="4"/>
        <description>
          <text>App-type is UNKNOWN.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>The application types.</text>
    </description>
  </typedef>
  <typedef name="devm-timeslot-list-type">
    <type name="string">
      <length value="1..61"/>
    </type>
    <description>
      <text>Type of timeslot-list.</text>
    </description>
  </typedef>
  <typedef name="host-name">
    <type name="string">
      <length value="1..255"/>
    </type>
    <description>
      <text>Host-name is like string.</text>
    </description>
  </typedef>
  <typedef name="acl4-identity">
    <type name="string">
      <length value="1..64"/>
      <pattern value="([1-9]\d{3})|(10[0-9]{3})|(23[0-9]{3})|(99999)|([0-9a-zA-Z]([^\s]*))"/>
    </type>
    <description>
      <text>Acl4-identity is begin with a-z or A-Z or 0-9.</text>
    </description>
  </typedef>
  <typedef name="hex-binary">
    <type name="string">
      <length value="1..10"/>
      <pattern value="0[xX](0|([1-9a-fA-F][0-9a-fA-F]*))"/>
    </type>
    <ext:case-sensitivity value="upper2lower"/>
    <description>
      <text>This is a hexadecimal variable.</text>
    </description>
  </typedef>
  <typedef name="date">
    <type name="string">
      <pattern value="\d{4}-\d{2}-\d{2}"/>
    </type>
    <description>
      <text>XSD date string type, such as '2010-10-08'.</text>
    </description>
    <reference>
      <text>http://www.w3.org/TR/2004/REC-xmlschema-2-20041028/datatypes.html#date</text>
    </reference>
  </typedef>
  <typedef name="time">
    <type name="string">
      <pattern value="\d{1,2}:\d{1,2}:\d{1,2}"/>
      <pattern value="(([0-1][0-9])|(2[0-3])|[0-9]):([0-5][0-9]|[0-9]):([0-5][0-9]|[0-9])"/>
    </type>
    <description>
      <text>XSD time string type such as '20:15:30'. Range is 00:00:00~23:59:59.</text>
    </description>
    <reference>
      <text>http://www.w3.org/TR/2004/REC-xmlschema-2-20041028/datatypes.html#date</text>
    </reference>
  </typedef>
  <typedef name="id-range">
    <type name="string">
      <pattern value="([0-9]|,|\+|\-|\(|\))+"/>
    </type>
    <description>
      <text>ID list, such as '1,3,5-7',or '+1,-3,+(20-30)'.</text>
    </description>
  </typedef>
  <typedef name="vlan-range">
    <type name="id-range">
      <pattern value="((409[0-4]|40[0-8][0-9]|[1-3][0-9]{3}|[1-9][0-9]{1,2}|[1-9])?(-(409[0-4]|40[0-8][0-9]|[1-3][0-9]{3}|[1-9][0-9]{1,2}|[1-9]))?(,((409[0-4]|40[0-8][0-9]|[1-3][0-9]{3}|[1-9][0-9]{1,2}|[1-9])(-(409[0-4]|40[0-8][0-9]|[1-3][0-9]{3}|[1-9][0-9]{1,2}|[1-9]))?))*)|((([\+\-](409[0-4]|40[0-8][0-9]|[1-3][0-9]{3}|[1-9][0-9]{1,2}|[1-9]))|([\+\-]\((409[0-4]|40[0-8][0-9]|[1-3][0-9]{3}|[1-9][0-9]{1,2}|[1-9])-(409[0-4]|40[0-8][0-9]|[1-3][0-9]{3}|[1-9][0-9]{1,2}|[1-9])\)))?(,(([\+\-](409[0-4]|40[0-8][0-9]|[1-3][0-9]{3}|[1-9][0-9]{1,2}|[1-9]))|([\+\-]\((409[0-4]|40[0-8][0-9]|[1-3][0-9]{3}|[1-9][0-9]{1,2}|[1-9])-(409[0-4]|40[0-8][0-9]|[1-3][0-9]{3}|[1-9][0-9]{1,2}|[1-9])\))))*)"/>
      <ext:value-range value="1..4094"/>
    </type>
    <description>
      <text>VLAN ID list.</text>
    </description>
  </typedef>
  <typedef name="keychain-name">
    <type name="string">
      <length value="0..47"/>
    </type>
    <description>
      <text>Now keyChainName is like string.</text>
    </description>
  </typedef>
</module>
