  <typedef name="qos-policy-apply-layer">
    <type name="enumeration">
      <enum name="none">
        <value value="0"/>
        <description>
          <text>Default classification.</text>
        </description>
      </enum>
      <enum name="link">
        <value value="1"/>
        <description>
          <text>Classification based on layer 2 information.</text>
        </description>
      </enum>
      <enum name="all">
        <value value="2"/>
        <description>
          <text>Classification based on all information.</text>
        </description>
      </enum>
      <enum name="mpls">
        <value value="3"/>
        <description>
          <text>Classification based on MPLS information.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>None/link/MPLS/all.</text>
    </description>
  </typedef>
  <typedef name="operate-type">
    <type name="enumeration">
      <enum name="disable">
        <value value="0"/>
        <description>
          <text>Disable.</text>
        </description>
      </enum>
      <enum name="enable">
        <value value="1"/>
        <description>
          <text>Enable.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Disable/enable.</text>
    </description>
  </typedef>
