     |     +--rw start-ipv6-address    inet:ipv6-address-no-zone
     |     +--rw vpn-name              -> /network-instance:network-instance/instances/instance/name
     |     +--rw domain-name           -> /aaa:aaa/domains/domain/name
     |     +--rw end-ipv6-address      inet:ipv6-address-no-zone
     |     +--rw prefix-length?        uint32
     +--rw subscriber-ip-and-ipv6s
     |  +--rw subscriber-ip-and-ipv6* [start-ip-address start-ipv6-address start-ipv6-prefix vpn-name]
