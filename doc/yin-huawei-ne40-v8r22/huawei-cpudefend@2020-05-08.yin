  <grouping name="devm-group">
    <description>
      <text>Applies an attack defense policy to the board.</text>
    </description>
    <container name="cpudefend">
      <description>
        <text>Configure policies for global management and service plane protection.</text>
      </description>
      <leaf name="policy-id">
        <type name="leafref">
          <path value="/cpudefend:cpudefend/cpudefend:policys/cpudefend:policy/cpudefend:id"/>
        </type>
        <description>
          <text>Specifies the id of the attack defense policy to be applied.</text>
        </description>
      </leaf>
    </container>
    <container name="cpudefend-devm">
      <description>
        <text>Configure policies for global management and service plane protection.</text>
      </description>
      <container name="ignore-hostcar-acl">
        <description>
          <text>Configure HOST CAR and VLAN-HOST-CAR not to apply to packets matching a specified ACL rule.</text>
        </description>
        <choice name="ipv4-acl-type">
          <description>
            <text>IPv4 ACL number or name. An ACL number ranges from 3000 to 3999. An ACL name is a string of 1 to 64 characters, which begins with a-z or A-Z. IPv4 ACL dentity is a number or number must exist, otherwise it does not support delivery.</text>
          </description>
          <case name="by-identity">
            <description>
              <text>IPv4 ACL identity.</text>
            </description>
            <leaf name="ipv4-acl">
              <type name="leafref">
                <path value="/acl:acl/acl:groups/acl:group/acl:identity"/>
              </type>
              <must condition="/acl:acl/acl:groups/acl:group[acl:identity=current()]/acl:type='advance'"/>
              <description>
                <text>IPv4 ACL identity.</text>
              </description>
            </leaf>
          </case>
          <case name="by-number">
            <description>
              <text>IPv4 ACL number.</text>
            </description>
            <leaf name="ipv4-acl-number">
              <type name="leafref">
                <path value="/acl:acl/acl:groups/acl:group/acl:number"/>
              </type>
              <must condition="/acl:acl/acl:groups/acl:group[acl:number=current()]/acl:type='advance'"/>
              <description>
                <text>IPv4 ACL number.</text>
              </description>
            </leaf>
          </case>
        </choice>
        <leaf name="ipv6-acl">
          <type name="leafref">
            <path value="/acl:acl/acl:group6s/acl:group6/acl:identity"/>
          </type>
          <must condition="/acl:acl/acl:group6s/acl:group6[acl:identity=current()]/acl:type='advance'"/>
          <description>
            <text>IPv6 ACL number or name. An ACL number ranges from 3000 to 3999. An ACL name is a string of 1 to 64 characters, which begins with a-z or A-Z.</text>
          </description>
        </leaf>
      </container>
      <container name="bras-host-cars">
        <description>
          <text>List of HOST-CAR is used to perform rate limit for packets that the user side sends to the CPU.</text>
        </description>
        <list name="bras-host-car">
          <ext:generated-by value="system"/>
          <ext:operation-exclude value="create"/>
          <key value="type"/>
          <description>
            <text>Configure HOST-CAR parameters.</text>
          </description>
          <leaf name="type">
            <type name="host-car-type"/>
            <description>
              <text>HOST-CAR type, such as HOST-CAR, HTTP-HOST-CAR, VLAN-HOST-CAR.</text>
            </description>
          </leaf>
          <container name="rate-limit">
            <must condition="./pir&gt;=./cir"/>
            <description>
              <text>Configure the bandwidth limit for HOST-CAR.</text>
            </description>
            <leaf name="cir">
              <type name="uint32">
                <range value="0..4294967295"/>
              </type>
              <units name="kbit/s"/>
              <ext:dynamic-default>
                <ext:default-value value="32">
                  <when condition="../../type = 'host-car'"/>
                  <description>
                    <text>The default value is 32 when type is HOST-CAR.</text>
                  </description>
                </ext:default-value>
                <ext:default-value value="128">
                  <when condition="../../type = 'http-host-car'"/>
                  <description>
                    <text>The default value is 128 when type is HTTP-HOST-CAR.</text>
                  </description>
                </ext:default-value>
                <ext:default-value value="5120">
                  <when condition="../../type = 'vlan-host-car'"/>
                  <description>
                    <text>The default value is 5120 when type is VLAN-HOST-CAR.</text>
                  </description>
                </ext:default-value>
              </ext:dynamic-default>
              <must condition="(../../type='vlan-host-car' and ../cir&gt;=8 and ../cir&lt;=4294967295) or (../../type!='vlan-host-car')"/>
              <description>
                <text>Committed information rate. The default value in the CU scene is expanded by 5 times.</text>
              </description>
            </leaf>
            <leaf name="cbs">
              <type name="uint32">
                <range value="1000..4294967295"/>
              </type>
              <units name="Byte"/>
              <ext:dynamic-default>
                <ext:default-value value="32000">
                  <when condition="../../type = 'host-car'"/>
                  <description>
                    <text>The default value is 32000 when type is HOST-CAR.</text>
                  </description>
                </ext:default-value>
                <ext:default-value value="128000">
                  <when condition="../../type = 'http-host-car'"/>
                  <description>
                    <text>The default value is 128000 when type is HTTP-HOST-CAR.</text>
                  </description>
                </ext:default-value>
                <ext:default-value value="2560000">
                  <when condition="../../type = 'vlan-host-car'"/>
                  <description>
                    <text>The default value is 2560000 when type is VLAN-HOST-CAR.</text>
                  </description>
                </ext:default-value>
              </ext:dynamic-default>
              <must condition="(../../type='vlan-host-car' and ../cbs&gt;=10000 and ../cbs&lt;=4294967295) or (../../type!='vlan-host-car')"/>
              <description>
                <text>Committed burst size. The default value in the CU scene is expanded by 5 times.</text>
              </description>
            </leaf>
            <leaf name="pir">
              <type name="uint32">
                <range value="0..4294967295"/>
              </type>
              <units name="kbit/s"/>
              <ext:dynamic-default>
                <ext:default-value value="32">
                  <when condition="../../type = 'host-car'"/>
                  <description>
                    <text>The default value is 32 when type is HOST-CAR.</text>
                  </description>
                </ext:default-value>
                <ext:default-value value="128">
                  <when condition="../../type = 'http-host-car'"/>
                  <description>
                    <text>The default value is 128 when type is HTTP-HOST-CAR.</text>
                  </description>
                </ext:default-value>
                <ext:default-value value="5120">
                  <when condition="../../type = 'vlan-host-car'"/>
                  <description>
                    <text>The default value is 5120 when type is VLAN-HOST-CAR.</text>
                  </description>
                </ext:default-value>
              </ext:dynamic-default>
              <description>
                <text>Peak information rate. The default value in the CU scene is expanded by 5 times.</text>
              </description>
            </leaf>
            <leaf name="pbs">
              <type name="uint32">
                <range value="0..4294967295"/>
              </type>
              <units name="Byte"/>
              <ext:dynamic-default>
                <ext:default-value value="32000">
                  <when condition="../../type = 'host-car'"/>
                  <description>
                    <text>The default value is 32000 when type is HOST-CAR.</text>
                  </description>
                </ext:default-value>
                <ext:default-value value="128000">
                  <when condition="../../type = 'http-host-car'"/>
                  <description>
                    <text>The default value is 128000 when type is HTTP-HOST-CAR.</text>
                  </description>
                </ext:default-value>
                <ext:default-value value="2560000">
                  <when condition="../../type = 'vlan-host-car'"/>
                  <description>
                    <text>The default value is 2560000 when type is VLAN-HOST-CAR.</text>
                  </description>
                </ext:default-value>
              </ext:dynamic-default>
              <description>
                <text>Peak burst size. The default value in the CU scene is expanded by 5 times.</text>
              </description>
            </leaf>
            <leaf name="auto-adjust-enable">
              <type name="boolean"/>
              <ext:dynamic-default>
                <ext:default-value value="true">
                  <when condition="../../type = 'host-car'"/>
                  <description>
                    <text>The default value is true when type is HOST-CAR.</text>
                  </description>
                </ext:default-value>
                <ext:default-value value="true">
                  <when condition="../../type = 'http-host-car'"/>
                  <description>
                    <text>The default value is true when type is HTTP-HOST-CAR.</text>
                  </description>
                </ext:default-value>
                <ext:default-value value="false">
                  <when condition="../../type = 'vlan-host-car'"/>
                  <description>
                    <text>The default value is false when type is VLAN-HOST-CAR.</text>
                  </description>
                </ext:default-value>
              </ext:dynamic-default>
              <description>
                <text>Enable/disable the device to automatically adjust HOST-CAR bandwidth.</text>
              </description>
            </leaf>
