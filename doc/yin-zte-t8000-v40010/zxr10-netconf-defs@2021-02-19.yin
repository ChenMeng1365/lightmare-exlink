  <typedef name="ipv4-address-none-zero">
    <type name="string">
      <pattern value="((([0-9]|[1-9][0-9]|1[0-9][0-9]|2[0-4][0-9]|25[0-5])\.){3}([0-9]|[1-9][0-9]|1[0-9][0-9]|2[0-4][0-9]|25[0-5]))"/>
    </type>
    <description>
      <text>ipv4-address without all zero (0.0.0.0)configuration;</text>
    </description>
  </typedef>
  <typedef name="mac-address-none-zero">
    <type name="string">
      <pattern value="([0-9a-fA-F]{2}(:[0-9a-fA-F]{2}){5})"/>
    </type>
    <description>
      <text>mac-address without all zero (00:00:00:00:00:00)configuration;</text>
    </description>
  </typedef>
  <typedef name="long-text">
    <type name="string">
      <length value="0..4095"/>
    </type>
    <description>
      <text>This typedef provide the capability that user can use it to input char parameter, the length is 0-4095. Quesition mark and blank can be accepted as valid input.</text>
    </description>
  </typedef>
  <typedef name="ipv6-prefix">
    <type name="string"/>
    <description>
      <text>This typedef is used to define an ipv6 address with prefix.</text>
    </description>
  </typedef>
  <typedef name="af-name">
    <type name="string"/>
    <description>
      <text>Application forwarder name</text>
    </description>
  </typedef>
  <typedef name="string-with-chinese">
    <type name="string"/>
    <description>
      <text>This typedef provides the capability that chinese can be accepted as valid input.</text>
    </description>
  </typedef>
  <typedef name="string-with-blank-and-chinese">
    <type name="string">
      <pattern value="([^?]*)"/>
    </type>
    <description>
      <text>This typedef provides the capability that blank and chinese can be accepted as valid input.</text>
    </description>
  </typedef>
  <typedef name="string-all-with-chinese">
    <type name="string"/>
    <description>
      <text>This typedef provides the capability that quesition mark,blank and chinese can be accepted as valid input.</text>
    </description>
  </typedef>
  <typedef name="af-name-with-chinese">
    <type name="string-with-chinese"/>
  </typedef>
  <typedef name="mib-datetime">
    <type name="string"/>
  </typedef>
  <typedef name="ipv6-address-none-zero">
    <type name="string"/>
    <description>
      <text>ipv6-address without all zero configuration;</text>
    </description>
  </typedef>
  <typedef name="std-ipv6-address">
    <type name="string">
      <pattern value="(((:|[0-9a-fA-F]{0,4}):)([0-9a-fA-F]{0,4}:){0,5}((([0-9a-fA-F]{0,4}:)?(:|[0-9a-fA-F]{0,4}))|(((25[0-5]|2[0-4][0-9]|[01]?[0-9]?[0-9])\.){3}(25[0-5]|2[0-4][0-9]|[01]?[0-9]?[0-9]))))"/>
      <pattern value="((([^:]+:){6}(([^:]+:[^:]+)|(.*\..*)))|((([^:]+:)*[^:]+)?::(([^:]+:)*[^:]+)?))"/>
    </type>
    <description>
      <text>This type is standard format for embedded IPv4 address, such as:[::1000:1].
The ipv6-address defined in zxr10-netconf-defs is a brief format for embedded pv4 address, such as:[::16.0.0.1].</text>
    </description>
  </typedef>
  <typedef name="ip-address-string">
    <type name="string">
      <pattern value="(((([0-9]|[1-9][0-9]|1[0-9][0-9]|2[0-4][0-9]|25[0-5])\.){3}([0-9]|[1-9][0-9]|1[0-9][0-9]|2[0-4][0-9]|25[0-5]))|((([0-9a-fA-F]{1,4}:){7}[0-9a-fA-F]{1,4}|([0-9a-fA-F]{1,4}:){1,7}:|([0-9a-fA-F]{1,4}:){1,6}:[0-9a-fA-F]{1,4}|([0-9a-fA-F]{1,4}:){1,5}(:[0-9a-fA-F]{1,4}){1,2}|([0-9a-fA-F]{1,4}:){1,4}(:[0-9a-fA-F]{1,4}){1,3}|([0-9a-fA-F]{1,4}:){1,3}(:[0-9a-fA-F]{1,4}){1,4}|([0-9a-fA-F]{1,4}:){1,2}(:[0-9a-fA-F]{1,4}){1,5}|[0-9a-fA-F]{1,4}:((:[0-9a-fA-F]{1,4}){1,6})|:((:[0-9a-fA-F]{1,4}){1,7}|:))))"/>
    </type>
    <description>
      <text>This typedef provides the ip address in string format,can be IPv4 or IPv6.</text>
    </description>
  </typedef>
  <typedef name="host-string">
    <type name="string">
      <pattern value="(((((([0-9]|[1-9][0-9]|1[0-9][0-9]|2[0-4][0-9]|25[0-5])\.){3}([0-9]|[1-9][0-9]|1[0-9][0-9]|2[0-4][0-9]|25[0-5]))|((([0-9a-fA-F]{1,4}:){7}[0-9a-fA-F]{1,4}|([0-9a-fA-F]{1,4}:){1,7}:|([0-9a-fA-F]{1,4}:){1,6}:[0-9a-fA-F]{1,4}|([0-9a-fA-F]{1,4}:){1,5}(:[0-9a-fA-F]{1,4}){1,2}|([0-9a-fA-F]{1,4}:){1,4}(:[0-9a-fA-F]{1,4}){1,3}|([0-9a-fA-F]{1,4}:){1,3}(:[0-9a-fA-F]{1,4}){1,4}|([0-9a-fA-F]{1,4}:){1,2}(:[0-9a-fA-F]{1,4}){1,5}|[0-9a-fA-F]{1,4}:((:[0-9a-fA-F]{1,4}){1,6})|:((:[0-9a-fA-F]{1,4}){1,7}|:))))) |(((([a-zA-Z0-9_]([a-zA-Z0-9\-_]){0,61})?[a-zA-Z0-9]\.)*([a-zA-Z0-9_]([a-zA-Z0-9\-_]){0,61})?[a-zA-Z0-9]\.?)|\.))"/>
    </type>
    <description>
      <text>This typedef provides the ip address in string format,can be IPv4,IPv6 or domain.</text>
    </description>
  </typedef>
</module>
