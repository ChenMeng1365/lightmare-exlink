<?xml version="1.0" encoding="UTF-8"?>
<module name="huawei-vaccess"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:vaccess="urn:huawei:yang:huawei-vaccess"
        xmlns:ext="urn:huawei:yang:huawei-extension"
        xmlns:inet="urn:ietf:params:xml:ns:yang:ietf-inet-types">
  <namespace uri="urn:huawei:yang:huawei-vaccess"/>
  <prefix value="vaccess"/>
  <import module="huawei-extension">
    <prefix value="ext"/>
  </import>
  <import module="ietf-inet-types">
    <prefix value="inet"/>
  </import>
  <include module="huawei-vaccess-type"/>
  <organization>
    <text>Huawei Technologies Co., Ltd.</text>
  </organization>
  <contact>
    <text>Huawei Industrial Base
Bantian, Longgang
Shenzhen 518129
People's Republic of China
Website: http://www.huawei.com
Email: support@huawei.com</text>
  </contact>
  <description>
    <text>Virtual Access.</text>
  </description>
  <revision date="2020-03-24">
    <description>
      <text>Fixed some problems.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <revision date="2020-03-10">
    <description>
      <text>Add a node primary-master-ip.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <revision date="2019-11-13">
    <description>
      <text>Add some condition for vaccess container.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <revision date="2019-10-14">
    <description>
      <text>The modifications in the schema file are synchronized to the YANG file.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <revision date="2019-06-05">
    <description>
      <text>Initial revision.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <ext:task-name value="virtual-access"/>
  <container name="vaccess">
    <description>
      <text>Virtual Access.</text>
    </description>
    <container name="global">
      <must condition="not(port-extend-enable='true' and enable='true')"/>
      <description>
        <text>Configure bisic function of virtural access.</text>
      </description>
      <leaf name="enable">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable state of virtural access. Default is no configuration.</text>
        </description>
      </leaf>
      <leaf name="port-extend-enable">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable state of virtural access port-extend. Default is no configuration.</text>
        </description>
      </leaf>
      <leaf name="role">
        <when condition="../enable='true' or ../port-extend-enable='true'"/>
        <type name="vaccess-va-role"/>
        <must condition="(../role='default') or (../enable='true' and ../role='ap') or (../port-extend-enable='true' and ../role='master')"/>
        <default value="default"/>
        <description>
          <text>Role of virtual access. Default is no configuration.</text>
        </description>
      </leaf>
    </container>
    <container name="ap">
      <when condition="../global/role='ap'"/>
      <description>
        <text>Configure ap of virtual access configuration.</text>
      </description>
      <container name="master">
        <description>
          <text>Configure basic function of master.</text>
        </description>
        <leaf name="primary-master-ip">
          <type name="inet:ip-address"/>
          <description>
            <text>Primary master IP address.</text>
          </description>
        </leaf>
      </container>
    </container>
  </container>
</module>
