     |     +--rw start-ipv6-address    inet:ipv6-address-no-zone
     |     +--rw start-ipv6-prefix     inet:ipv6-address-no-zone
     |     +--rw vpn-name              -> /network-instance:network-instance/instances/instance/name
     |     +--rw domain-name           -> /aaa:aaa/domains/domain/name
     |     +--rw end-ip-address        inet:ipv4-address-no-zone
     |     +--rw end-ipv6-address      inet:ipv6-address-no-zone
     |     +--rw end-ipv6-prefix       inet:ipv6-address-no-zone
     |     +--rw prefix-length?        uint32
     +--rw subscriber-subnets
        +--rw subscriber-subnet* [start-ip-address vpn-name]
           +--rw start-ip-address    inet:ipv4-address-no-zone
           +--rw vpn-name            -> /network-instance:network-instance/instances/instance/name
           +--rw mask-length         uint32
           +--rw domain-name         -> /aaa:aaa/domains/domain/name
