  <container name="state">
    <config value="false"/>
    <description>
      <text>Information of show.</text>
    </description>
    <container name="msdp">
      <description>
        <text>Information of msdp.</text>
      </description>
      <container name="private">
        <description>
          <text>Private root node.</text>
        </description>
        <container name="peers">
          <description>
            <text>Information of MSDP peer.</text>
          </description>
          <list name="peer">
            <description>
              <text>Information of MSDP peer.</text>
            </description>
            <key value="vrf-name address"/>
            <leaf name="vrf-name">
              <description>
                <text>VRF name.</text>
              </description>
              <type name="string">
                <length value="1..32"/>
              </type>
            </leaf>
            <leaf name="address">
              <description>
                <text>Peer address.</text>
              </description>
              <type name="ncd:ipv4-address"/>
            </leaf>
            <leaf name="state">
              <description>
                <text>Peer connection status:
1 - down;
2 - shutdown;
3 - inactive;
4 - connnect;
5 - listen;
6 - up.</text>
              </description>
              <type name="enumeration">
                <enum name="down">
                  <value value="1"/>
                </enum>
                <enum name="shutdown">
                  <value value="2"/>
                </enum>
                <enum name="inactive">
                  <value value="3"/>
                </enum>
                <enum name="connect">
                  <value value="4"/>
                </enum>
                <enum name="listen">
                  <value value="5"/>
                </enum>
                <enum name="up">
                  <value value="6"/>
                </enum>
              </type>
            </leaf>
            <leaf name="reset">
              <description>
                <text>Peer reset times.</text>
              </description>
              <type name="uint32"/>
            </leaf>
            <leaf name="connect-source">
              <description>
                <text>Connect source interface name.</text>
              </description>
              <type name="ncd:ifname">
                <length value="0..31"/>
