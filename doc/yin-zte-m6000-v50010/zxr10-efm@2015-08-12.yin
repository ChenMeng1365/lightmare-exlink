  <container name="state">
    <description>
      <text>EFM state information</text>
    </description>
    <config value="false"/>
    <container name="efm">
      <description>
        <text>Ethernet in the first mile</text>
      </description>
      <container name="global">
        <description>
          <text>EFM global information</text>
        </description>
        <leaf name="adminstate">
          <description>
            <text>EFM global enable switch</text>
          </description>
          <type name="enumeration">
            <enum name="enable">
              <value value="1"/>
              <description>
                <text>Enable Ethernet OAM global function</text>
              </description>
            </enum>
            <enum name="disable">
              <value value="2"/>
              <description>
                <text>Disable Ethernet OAM global function</text>
              </description>
            </enum>
          </type>
        </leaf>
        <leaf name="linkmonitor">
          <description>
            <text>The state of linkmonitor function</text>
          </description>
          <type name="enumeration">
            <enum name="support">
              <value value="1"/>
              <description>
                <text>Support the linkmonitor function</text>
              </description>
            </enum>
            <enum name="nonsupport">
              <value value="2"/>
              <description>
                <text>Nonsupport the linkmonitor function</text>
              </description>
            </enum>
          </type>
        </leaf>
        <leaf name="mibretrieval">
          <description>
            <text>The state of mibretrieval function</text>
          </description>
          <type name="enumeration">
            <enum name="support">
              <value value="1"/>
              <description>
                <text>Support the mibretrieval function</text>
              </description>
            </enum>
            <enum name="nonsupport">
