  <grouping name="rate-limit-type">
    <description>
      <text>Configures the inbound and outbound rate for the service policy template.</text>
    </description>
    <leaf name="cir">
      <type name="uint32">
        <range value="0..4294967294"/>
      </type>
      <units name="kbit/s"/>
      <mandatory value="true"/>
      <description>
        <text>Specifies committed information rate.</text>
      </description>
    </leaf>
    <leaf name="pir">
      <type name="uint32">
        <range value="0..4294967294"/>
      </type>
      <units name="kbit/s"/>
      <must condition="not(../pir) or (../pir and ../pir &gt;=../cir)"/>
      <description>
        <text>Specifies the peak information rate.</text>
      </description>
    </leaf>
    <leaf name="cbs">
      <type name="uint32">
        <range value="0..4294967294"/>
      </type>
      <units name="kbit/s"/>
      <description>
        <text>Specifies the committed burst size.</text>
      </description>
    </leaf>
    <leaf name="pbs">
      <type name="uint32">
        <range value="0..4294967294"/>
      </type>
      <units name="kbit/s"/>
      <must condition="not(../pbs) or (../cbs and ../pbs and ../pbs &gt;=../cbs)"/>
      <description>
        <text>Specifies the peak burst size.</text>
      </description>
    </leaf>
    <leaf name="flow-queue-pbs">
      <when condition="../cbs"/>
      <type name="uint32">
        <range value="1..4194304"/>
      </type>
      <units name="Byte"/>
      <must condition="../flow-queue-pbs&gt;=../cbs"/>
      <description>
        <text>Specifies the peak burst size of flow queue.</text>
      </description>
    </leaf>
  </grouping>
