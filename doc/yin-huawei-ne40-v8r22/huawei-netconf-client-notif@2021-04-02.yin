<?xml version="1.0" encoding="UTF-8"?>
<module name="huawei-netconf-client-notif"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:netconf-client-notif="urn:huawei:yang:huawei-netconf-client-notif"
        xmlns:inet="urn:ietf:params:xml:ns:yang:ietf-inet-types"
        xmlns:ext="urn:huawei:yang:huawei-extension">
  <yang-version value="1"/>
  <namespace uri="urn:huawei:yang:huawei-netconf-client-notif"/>
  <prefix value="netconf-client-notif"/>
  <import module="ietf-inet-types">
    <prefix value="inet"/>
  </import>
  <import module="huawei-extension">
    <prefix value="ext"/>
  </import>
  <organization>
    <text>Huawei Technologies Co., Ltd.</text>
  </organization>
  <contact>
    <text>Huawei Industrial Base
Bantian, Longgang
Shenzhen 518129
People's Republic of China
Website: http://www.huawei.com
Email: support@huawei.com</text>
  </contact>
  <description>
    <text>Notification of netconf client.</text>
  </description>
  <revision date="2021-04-02">
    <description>
      <text>Initial revision.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <notification name="netconf-client-config-failed">
    <description>
      <text>Failed to deliver configurations to a peer device.</text>
    </description>
    <leaf name="peer-name">
      <type name="string">
        <length value="1..64"/>
      </type>
      <description>
        <text>Name of the peer.</text>
      </description>
    </leaf>
    <leaf name="addr-type">
      <type name="inet:ip-version"/>
      <description>
        <text>Type of the IP address.</text>
      </description>
    </leaf>
    <leaf name="address">
      <type name="inet:ipv4-address"/>
      <description>
        <text>Address of the peer.</text>
      </description>
    </leaf>
    <leaf name="feature-name">
      <type name="string">
        <length value="1..64"/>
      </type>
      <description>
        <text>App name.</text>
      </description>
    </leaf>
  </notification>
  <notification name="netconf-client-config-synchronized-success">
    <description>
      <text>Successfully synchronized configurations to a peer device.</text>
    </description>
    <leaf name="peer-name">
      <type name="string">
        <length value="1..64"/>
      </type>
      <description>
        <text>Name of the peer.</text>
      </description>
    </leaf>
    <leaf name="addr-type">
      <type name="inet:ip-version"/>
      <description>
        <text>Type of the IP address.</text>
      </description>
    </leaf>
    <leaf name="address">
      <type name="inet:ipv4-address"/>
      <description>
        <text>Address of the peer.</text>
      </description>
    </leaf>
    <leaf name="feature-name">
      <type name="string">
        <length value="1..64"/>
      </type>
      <description>
        <text>App name.</text>
      </description>
    </leaf>
  </notification>
  <notification name="netconf-client-connection-interrupt">
    <description>
      <text>Remote netconf connection is interrupted.</text>
    </description>
    <leaf name="ip-type">
      <type name="inet:ip-version"/>
      <description>
        <text>IP address type.</text>
      </description>
    </leaf>
    <leaf name="peer-ip">
      <type name="inet:ipv4-address"/>
      <description>
        <text>Peer IP address.</text>
      </description>
    </leaf>
    <leaf name="vpn-name">
      <type name="string">
        <length value="1..31"/>
      </type>
      <description>
        <text>VPN name.</text>
      </description>
    </leaf>
  </notification>
  <notification name="netconf-client-connection-resume">
    <description>
      <text>Remote netconf connection is resumed.</text>
    </description>
    <leaf name="ip-type">
      <type name="inet:ip-version"/>
      <description>
        <text>IP address type.</text>
      </description>
    </leaf>
    <leaf name="peer-ip">
      <type name="inet:ipv4-address"/>
      <description>
        <text>Peer IP address.</text>
      </description>
    </leaf>
    <leaf name="vpn-name">
      <type name="string">
        <length value="1..31"/>
      </type>
      <description>
        <text>VPN name.</text>
      </description>
    </leaf>
  </notification>
  <notification name="netconf-client-peer-connection-interrupt">
    <description>
      <text>The NETCONF connection with a peer device was torn down.</text>
    </description>
    <leaf name="peer-name">
      <type name="string">
        <length value="1..64"/>
      </type>
      <description>
        <text>Name of the peer.</text>
      </description>
    </leaf>
    <leaf name="addr-type">
      <type name="inet:ip-version"/>
      <description>
        <text>The type of IP address.</text>
      </description>
    </leaf>
    <leaf name="address">
      <type name="inet:ipv4-address"/>
      <description>
        <text>Peer IP address.</text>
      </description>
    </leaf>
    <leaf name="user-name">
      <type name="string">
        <length value="1..50"/>
      </type>
      <description>
        <text>User name.</text>
      </description>
    </leaf>
  </notification>
  <notification name="netconf-client-peer-connection-resume">
    <description>
      <text>The NETCONF connection with a peer device is resumed.</text>
    </description>
    <leaf name="peer-name">
      <type name="string">
        <length value="1..64"/>
      </type>
      <description>
        <text>Name of the peer.</text>
      </description>
    </leaf>
    <leaf name="addr-type">
      <type name="inet:ip-version"/>
      <description>
        <text>The type of IP address.</text>
      </description>
    </leaf>
    <leaf name="address">
      <type name="inet:ipv4-address"/>
      <description>
        <text>Peer IP address.</text>
      </description>
    </leaf>
    <leaf name="user-name">
      <type name="string">
        <length value="1..50"/>
      </type>
      <description>
        <text>User name.</text>
      </description>
    </leaf>
  </notification>
  <notification name="netconf-client-sync-config-failed">
    <description>
      <text>Failed to synchronize configurations to a peer device.</text>
    </description>
    <leaf name="peer-name">
      <type name="string">
        <length value="1..64"/>
      </type>
      <description>
        <text>Name of the peer.</text>
      </description>
    </leaf>
    <leaf name="addr-type">
      <type name="inet:ip-version"/>
      <description>
        <text>The type of IP address.</text>
      </description>
    </leaf>
    <leaf name="address">
      <type name="inet:ipv4-address"/>
      <description>
        <text>Peer IP address.</text>
      </description>
    </leaf>
  </notification>
  <notification name="netconf-client-sync-config-success">
    <description>
      <text>Successfully synchronized configurations to a peer device.</text>
    </description>
    <leaf name="peer-name">
      <type name="string">
        <length value="1..64"/>
      </type>
      <description>
        <text>Name of the peer.</text>
      </description>
    </leaf>
    <leaf name="addr-type">
      <type name="inet:ip-version"/>
      <description>
        <text>The type of IP address.</text>
      </description>
    </leaf>
    <leaf name="address">
      <type name="inet:ipv4-address"/>
      <description>
        <text>Peer IP address.</text>
      </description>
    </leaf>
  </notification>
  <ext:task-name value="netconfc"/>
</module>
