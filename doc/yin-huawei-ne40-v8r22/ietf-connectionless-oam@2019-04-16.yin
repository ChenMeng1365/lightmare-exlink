      <leaf name="ingress-intf-name">
        <type name="if:interface-ref"/>
        <description>
          <text>Ingress interface name.</text>
        </description>
      </leaf>
    </container>
    <leaf name="sequence-number">
      <type name="uint64"/>
      <default value="0"/>
      <description>
        <text>Sequence number in data packets.  A value of
zero indicates that no sequence number is sent.</text>
      </description>
    </leaf>
    <leaf name="hop-cnt">
      <type name="uint8"/>
      <default value="0"/>
      <description>
