<?xml version="1.0" encoding="UTF-8"?>
<module name="huawei-multicore"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:multicore="urn:huawei:yang:huawei-multicore"
        xmlns:ext="urn:huawei:yang:huawei-extension"
        xmlns:devm="urn:huawei:yang:huawei-devm">
  <yang-version value="1"/>
  <namespace uri="urn:huawei:yang:huawei-multicore"/>
  <prefix value="multicore"/>
  <import module="huawei-extension">
    <prefix value="ext"/>
  </import>
  <import module="huawei-devm">
    <prefix value="devm"/>
  </import>
  <organization>
    <text>Huawei Technologies Co., Ltd.</text>
  </organization>
  <contact>
    <text>Huawei Industrial Base
Bantian, Longgang
Shenzhen 518129
People's Republic of China
Website: http://www.huawei.com
Email: support@huawei.com</text>
  </contact>
  <description>
    <text>Global Configuration.</text>
  </description>
  <revision date="2020-04-08">
    <description>
      <text>Initial revision.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <ext:task-name value="forwarding"/>
  <augment target-node="/devm:devm">
    <description>
      <text>Forward CPU alarm.</text>
    </description>
    <container name="forward-cpu-alarm">
      <description>
        <text>Configure forward alarm for the multi-core CPU average usage on a board.</text>
      </description>
      <leaf name="threshold">
        <type name="uint8">
          <range value="40..100"/>
        </type>
        <units name="%"/>
        <default value="95"/>
        <description>
          <text>Specifies the alarm threshold for the multi-core CPU average usage on a board.</text>
        </description>
      </leaf>
      <leaf name="log-enable">
        <type name="boolean"/>
        <default value="true"/>
        <description>
          <text>Enable/disable log reporting when the multi-core CPU average usage reaches the threshold.</text>
        </description>
      </leaf>
      <leaf name="trap-enable">
        <type name="boolean"/>
        <default value="true"/>
        <description>
          <text>Enable/disable alarm reporting when the multi-core CPU average usage reaches the threshold.</text>
        </description>
      </leaf>
    </container>
    <container name="forward-vcpu-alarm">
      <description>
        <text>Configure forward alarm for the usage of the multi-core's single core.</text>
      </description>
      <leaf name="threshold">
        <type name="uint8">
          <range value="40..100"/>
        </type>
        <units name="%"/>
        <default value="95"/>
        <description>
          <text>Specifies the alarm threshold for the usage of the multi-core's single core.</text>
        </description>
      </leaf>
      <leaf name="log-enable">
        <type name="boolean"/>
        <default value="true"/>
        <description>
          <text>Enable/disable log reporting when the CPU usage of the multi-core's single core reaches the threshold.</text>
        </description>
      </leaf>
      <leaf name="trap-enable">
        <type name="boolean"/>
        <default value="true"/>
        <description>
          <text>Enable/disable alarm reporting when the CPU usage of the multi-core's single core reaches the threshold.</text>
        </description>
      </leaf>
    </container>
  </augment>
</module>
