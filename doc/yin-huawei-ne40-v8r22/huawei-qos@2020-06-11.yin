              <type name="uint32">
                <range value="1..4194304"/>
              </type>
              <units name="Byte"/>
              <description>
                <text>Peak burst size.</text>
              </description>
            </leaf>
            <leaf name="cbs">
              <when condition="../cir-value or ../cir-percent"/>
              <type name="uint32">
                <range value="1..4194304"/>
              </type>
              <units name="Byte"/>
              <description>
                <text>Committed burst size.</text>
              </description>
