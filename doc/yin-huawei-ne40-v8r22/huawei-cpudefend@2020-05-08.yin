  <grouping name="cp-rate-limit-general">
    <description>
      <text>Restricts the rate at which packets on the general interface are sent to the CPU.</text>
    </description>
    <leaf name="cir">
      <type name="uint32">
        <range value="32..1000000"/>
      </type>
      <units name="pps"/>
      <mandatory value="true"/>
      <description>
        <text>Committed information rate.</text>
      </description>
    </leaf>
    <leaf name="cbs">
      <type name="uint32">
        <range value="100..33554432"/>
      </type>
      <units name="Byte"/>
      <ext:dynamic-default>
        <ext:default-value value="../cir">
          <when condition="../cir&gt;100"/>
          <description>
            <text>The default value is the value of cir when cir is greater than the minimum of cbs.</text>
          </description>
        </ext:default-value>
        <ext:default-value value="100">
          <when condition="../cir&lt;=100"/>
          <description>
            <text>The default value is the minimum of cbs when cir is less than or equal to the minimum of cbs.</text>
          </description>
        </ext:default-value>
      </ext:dynamic-default>
      <description>
        <text>Committed burst size.</text>
      </description>
    </leaf>
    <leaf name="prior">
      <type name="boolean"/>
      <default value="false"/>
      <description>
        <text>Enable/disable specifies the packet bandwidth limit for the specified port takes precedence over blacklists, whitelists, and user-defined-flow.</text>
      </description>
    </leaf>
  </grouping>
