            <uses name="fake-as-type"/>
            <uses name="peer-timer-para-type"/>
            <container name="bfd-parameter">
              <description>
                <text>Configure BFD.</text>
              </description>
              <leaf name="block">
                <type name="boolean"/>
                <must condition="not(../block='true' and ../enable='true')"/>
                <default value="false"/>
                <description>
                  <text>Enable/disable preventing a BGP peer from inheriting the BFD configuration of the BGP peer group.</text>
                </description>
              </leaf>
              <leaf name="enable">
                <type name="boolean"/>
                <must condition="not(../block='true' and ../enable='true')"/>
                <default value="false"/>
                <description>
                  <text>Enable/disable BFD.</text>
                </description>
              </leaf>
              <choice name="mode-type">
                <description>
                  <text>Set bfd mode.</text>
                </description>
                <case name="single">
                  <description>
                    <text>Single hop mode.</text>
                  </description>
                  <leaf name="single-hop">
                    <when condition="../enable='true'"/>
                    <type name="empty"/>
                    <description>
                      <text>The single hop mode is preferred during the BFD session establishment between IBGP peers.</text>
                    </description>
                  </leaf>
                  <leaf name="compatible">
                    <when condition="../enable='true'"/>
                    <type name="empty"/>
                    <description>
                      <text>Compatible.</text>
                    </description>
                  </leaf>
                </case>
                <case name="link">
                  <description>
                    <text>The per-link-echo mode.</text>
                  </description>
                  <leaf name="per-link-echo">
                    <when condition="../enable='true'"/>
                    <type name="empty"/>
                    <description>
                      <text>The per-link one-arm-echo mode is preferred during the BFD session establishment between IBGP peers.</text>
                    </description>
                  </leaf>
                </case>
              </choice>
              <uses name="bfd-detect-parameter-type"/>
            </container>
            <container name="egress-engineer-parameter">
              <when condition="../egress-engineer='enable'"/>
