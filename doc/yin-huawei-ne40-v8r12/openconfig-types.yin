  <oc-ext:regexp-posix/>
  <oc-ext:catalog-organization org="openconfig"/>
  <oc-ext:origin origin="openconfig"/>
  <typedef name="percentage">
    <type name="uint8">
      <range value="0..100"/>
    </type>
    <description>
      <text>Integer indicating a percentage value</text>
    </description>
  </typedef>
  <typedef name="std-regexp">
    <type name="string"/>
    <description>
      <text>This type definition is a placeholder for a standard
definition of a regular expression that can be utilised in
OpenConfig models. Further discussion is required to
consider the type of regular expressions that are to be
supported. An initial proposal is POSIX compatible.</text>
    </description>
  </typedef>
  <typedef name="timeticks64">
    <type name="uint64"/>
    <units name="nanoseconds"/>
    <description>
      <text>The timeticks64 represents the time, modulo 2^64 in
nanoseconds between two epochs. The leaf using this
type must define the epochs that tests are relative to.</text>
    </description>
  </typedef>
  <typedef name="ieeefloat32">
    <type name="binary">
      <length value="4"/>
    </type>
    <description>
      <text>An IEEE 32-bit floating point number. The format of this number
is of the form:
 1-bit  sign
 8-bit  exponent
 23-bit fraction
The floating point value is calculated using:
 (-1)**S * 2**(Exponent-127) * (1+Fraction)</text>
    </description>
  </typedef>
  <typedef name="routing-password">
    <type name="string"/>
    <description>
      <text>This type is indicative of a password that is used within
a routing protocol which can be returned in plain text to the
NMS by the local system. Such passwords are typically stored
as encrypted strings. Since the encryption used is generally
well known, it is possible to extract the original value from
the string - and hence this format is not considered secure.
Leaves specified with this type should not be modified by
the system, and should be returned to the end-user in plain
text. This type exists to differentiate passwords, which
may be sensitive, from other string leaves. It could, for
example, be used by the NMS to censor this data when
viewed by particular users.</text>
    </description>
  </typedef>
  <typedef name="stat-interval">
    <type name="uint64"/>
    <units name="nanoseconds"/>
    <description>
      <text>A time interval over which a set of statistics is computed.
A common usage is to report the interval over which
avg/min/max stats are computed and reported.</text>
    </description>
  </typedef>
  <grouping name="stat-interval-state">
    <description>
      <text>Reusable leaf definition for stats computation interval</text>
    </description>
    <leaf name="interval">
      <type name="oc-types:stat-interval"/>
      <description>
        <text>If supported by the system, this reports the time interval
over which the min/max/average statistics are computed by
the system.</text>
      </description>
    </leaf>
  </grouping>
  <grouping name="min-max-time">
    <description>
      <text>Common grouping for recording the absolute time at which
the minimum and maximum values occurred in the statistics</text>
    </description>
    <leaf name="min-time">
      <type name="oc-types:timeticks64"/>
      <description>
        <text>The absolute time at which the minimum value occurred.
The value is the timestamp in nanoseconds relative to
 the Unix Epoch (Jan 1, 1970 00:00:00 UTC).</text>
      </description>
    </leaf>
    <leaf name="max-time">
      <type name="oc-types:timeticks64"/>
      <description>
        <text>The absolute time at which the maximum value occurred.
The value is the timestamp in nanoseconds relative to
 the Unix Epoch (Jan 1, 1970 00:00:00 UTC).</text>
      </description>
    </leaf>
  </grouping>
  <grouping name="avg-min-max-stats-precision1">
    <description>
      <text>Common nodes for recording average, minimum, and
maximum values for a statistic.  These values all have
fraction-digits set to 1.  Statistics are computed
and reported based on a moving time interval (e.g., the last
30s).  If supported by the device, the time interval over which
the statistics are computed is also reported.</text>
    </description>
    <leaf name="avg">
      <type name="decimal64">
        <fraction-digits value="1"/>
      </type>
      <description>
        <text>The arithmetic mean value of the statistic over the
time interval.</text>
      </description>
    </leaf>
    <leaf name="min">
      <type name="decimal64">
        <fraction-digits value="1"/>
      </type>
      <description>
        <text>The minimum value of the statistic over the time
interval.</text>
      </description>
    </leaf>
    <leaf name="max">
      <type name="decimal64">
        <fraction-digits value="1"/>
      </type>
      <description>
        <text>The maximum value of the statitic over the time
interval.</text>
      </description>
    </leaf>
    <uses name="stat-interval-state"/>
    <uses name="min-max-time"/>
  </grouping>
  <grouping name="avg-min-max-instant-stats-precision1">
    <description>
      <text>Common grouping for recording an instantaneous statistic value
in addition to avg-min-max stats</text>
    </description>
    <leaf name="instant">
      <type name="decimal64">
        <fraction-digits value="1"/>
      </type>
      <description>
        <text>The instantaneous value of the statistic.</text>
      </description>
    </leaf>
    <uses name="avg-min-max-stats-precision1"/>
  </grouping>
