                },
                "target-params": {
                    "description": "List of target parameters.",
                    "type": "array",
                    "items": {
                        "properties": {
                            "name": {
                                "type": "string"
                            },
                            "params": {
                                "description": "This choice is augmented with case nodes containing\nconfiguration parameters specific to the security model."
                            }
                        }
                    },
