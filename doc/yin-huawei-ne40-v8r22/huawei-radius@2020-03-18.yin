  <grouping name="radius-server-para-info-type">
    <description>
      <text>Configure the inbound and outbound rate for the service policy template.</text>
    </description>
    <leaf name="master-enable">
      <type name="boolean"/>
      <config value="false"/>
      <description>
        <text>Enable/disable master and backup servers.</text>
      </description>
    </leaf>
    <leaf name="server-state">
      <type name="string">
        <length value="1..32"/>
      </type>
      <config value="false"/>
      <description>
        <text>Status of a RADIUS server.</text>
      </description>
    </leaf>
    <container name="server-status-changes">
      <config value="false"/>
      <description>
        <text>List of status change information about a RADIUS server.</text>
      </description>
      <list name="server-status-change">
        <key value="record-index"/>
        <config value="false"/>
        <description>
          <text>Operational state of status change information about a RADIUS server.</text>
        </description>
        <leaf name="record-index">
          <type name="uint32">
            <range value="0..4294967295"/>
          </type>
          <config value="false"/>
          <description>
            <text>Record index of status change information about a RADIUS server.</text>
          </description>
        </leaf>
        <leaf name="change-time">
          <type name="string">
            <length value="1..31"/>
          </type>
          <config value="false"/>
          <description>
            <text>Time when the status of a RADIUS server changes.</text>
          </description>
        </leaf>
        <leaf name="change-state">
          <type name="enumeration">
            <enum name="up">
              <value value="1"/>
              <description>
                <text>Status of a RADIUS server as up.</text>
              </description>
            </enum>
            <enum name="down">
              <value value="2"/>
              <description>
                <text>Status of a RADIUS server as down.</text>
              </description>
            </enum>
          </type>
          <config value="false"/>
          <description>
            <text>Change status.</text>
          </description>
        </leaf>
      </list>
    </container>
  </grouping>
  <grouping name="radius-detect-recorver">
    <description>
      <text>Configure detect for RADIUS authentication/accounting server state recovery.</text>
    </description>
    <leaf name="user-name">
      <type name="string">
        <length value="1..253"/>
      </type>
      <mandatory value="true"/>
      <description>
        <text>User name.</text>
      </description>
    </leaf>
    <leaf name="interval">
      <type name="uint32">
        <range value="10..3600"/>
      </type>
      <units name="s"/>
      <default value="30"/>
      <description>
        <text>Detect interval.</text>
      </description>
    </leaf>
    <leaf name="threshold">
      <type name="uint32">
        <range value="1..100"/>
      </type>
      <default value="3"/>
      <description>
        <text>Detect threshold.</text>
      </description>
    </leaf>
  </grouping>
  <container name="radius">
    <description>
      <text>RADIUS.</text>
    </description>
    <container name="global-service">
      <description>
        <text>Configure RADIUS global function.</text>
      </description>
      <leaf name="radius-enable">
        <type name="boolean"/>
        <default value="true"/>
        <description>
          <text>Enable/disable the RADIUS client services.</text>
        </description>
      </leaf>
      <leaf name="dead-count">
        <type name="uint32">
          <range value="3..65535"/>
        </type>
        <default value="10"/>
        <description>
          <text>To configure the dead count for all radius servers in the virtual system.</text>
        </description>
      </leaf>
      <leaf name="dead-interval">
        <type name="uint32">
          <range value="0..60"/>
        </type>
        <units name="s"/>
        <default value="5"/>
        <description>
          <text>To configure the dead interval for all the radius servers in the virtual system.</text>
        </description>
      </leaf>
      <leaf name="dead-time">
        <type name="uint32">
          <range value="1..65535"/>
        </type>
        <units name="min"/>
        <default value="3"/>
        <description>
          <text>To configure the dead time for all the radius servers in the virtual system.</text>
        </description>
      </leaf>
      <leaf name="fail-rate">
        <type name="uint32">
          <range value="1..100"/>
        </type>
        <units name="%"/>
        <default value="100"/>
        <description>
          <text>Configure the fail rate for all the RADIUS servers in the virtual system.</text>
        </description>
      </leaf>
      <leaf name="server-recovery-on-response">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable server recovery on response for all the RADIUS servers in the virtual system.</text>
        </description>
      </leaf>
      <leaf name="authen-client-identifier">
        <type name="string">
          <length value="1..255"/>
        </type>
        <config value="false"/>
        <description>
          <text>Radius client identifier for authentication purposes.</text>
        </description>
      </leaf>
      <container name="source-interface">
        <description>
          <text>Configure RADIUS source interface.</text>
        </description>
        <leaf name="name">
          <type name="leafref">
            <path value="/ifm:ifm/ifm:interfaces/ifm:interface/ifm:name"/>
          </type>
          <must condition="(/ifm:ifm/ifm:interfaces/ifm:interface[ifm:name = current()]/ifm:type != 'Stack-Port' and /ifm:ifm/ifm:interfaces/ifm:interface[ifm:name = current()]/ifm:type != 'NULL' and /ifm:ifm/ifm:interfaces/ifm:interface[ifm:name = current()]/ifm:type != 'Sip' and /ifm:ifm/ifm:interfaces/ifm:interface[ifm:name = current()]/ifm:type != 'MTunnel' and /ifm:ifm/ifm:interfaces/ifm:interface[ifm:name = current()]/ifm:type != 'Vbdif' )"/>
          <description>
            <text>Interface name.</text>
          </description>
        </leaf>
      </container>
    </container>
    <container name="radius-server-groups">
      <description>
        <text>List of a set of RADIUS group.</text>
      </description>
      <list name="radius-server-group">
        <must condition="(auth-retransmission-count and auth-retransmission-interval) or (not(auth-retransmission-count) and not(auth-retransmission-interval))"/>
        <must condition="(acct-retransmission-count and acct-retransmission-interval) or (not(acct-retransmission-count) and not(acct-retransmission-interval))"/>
        <key value="name"/>
        <max-elements value="128"/>
        <description>
          <text>Configure RADIUS server group.</text>
        </description>
        <leaf name="name">
          <type name="string">
            <length value="1..32"/>
            <pattern value="[a-z0-9._-]{1,32}"/>
          </type>
          <must condition="../name != '-' and ../name != '--'"/>
          <description>
            <text>RADIUS server group name.</text>
          </description>
          <ext:case-sensitivity value="lower-only"/>
        </leaf>
        <leaf name="retransmission-count">
          <type name="int32">
            <range value="1..5"/>
          </type>
          <default value="3"/>
          <description>
            <text>Number of times packet retransmission will happen if server is not responding.</text>
          </description>
        </leaf>
        <leaf name="retransmission-interval">
          <type name="int32">
            <range value="3..25"/>
          </type>
          <units name="s"/>
          <default value="5"/>
          <description>
            <text>Server response timeout value.</text>
          </description>
        </leaf>
        <leaf name="auth-retransmission-count">
          <type name="int32">
            <range value="1..5"/>
          </type>
          <description>
            <text>Number of retransmission times of RADIUS authentication request packets if the server is not responding.</text>
          </description>
        </leaf>
        <leaf name="auth-retransmission-interval">
          <type name="int32">
            <range value="3..25"/>
          </type>
          <units name="s"/>
          <description>
            <text>Authentication response timeout interval of the RADIUS server.</text>
          </description>
        </leaf>
        <leaf name="acct-retransmission-count">
          <type name="int32">
            <range value="1..5"/>
          </type>
          <description>
            <text>Number of retransmission times of RADIUS accounting request packets if the server is not responding.</text>
          </description>
        </leaf>
        <leaf name="acct-retransmission-interval">
          <type name="int32">
            <range value="3..25"/>
          </type>
          <units name="s"/>
          <description>
            <text>Accounting response timeout interval of the RADIUS server.</text>
          </description>
        </leaf>
        <leaf name="shared-key">
          <type name="pub-type:password-extend">
            <length value="1..268"/>
          </type>
          <description>
            <text>Shared-key value for a particular group. The simple password length ranges from 1 to 128, and the cipher password length ranges from 1 to 268.</text>
          </description>
        </leaf>
        <leaf name="attribute-translation-enable">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable the attribute translation.</text>
          </description>
        </leaf>
        <leaf name="group-mode">
          <type name="enumeration">
            <enum name="pri-secondary">
              <value value="0"/>
              <description>
                <text>Group mode as Primary secondary.</text>
              </description>
            </enum>
            <enum name="load-balance">
              <value value="1"/>
              <description>
                <text>Group mode as Load balance.</text>
              </description>
            </enum>
          </type>
          <default value="pri-secondary"/>
          <status value="deprecated"/>
          <description>
            <text>To configure mode (primary secondary and load balance) for a particular group. You are advised to use the leaf '/radius/radius-server-groups/radius-server-group/radius-server-algorithm/radius-arithmetic'.</text>
          </description>
        </leaf>
        <leaf name="nas-ip-address">
          <type name="inet:ipv4-address-no-zone"/>
          <description>
            <text>Value of NAS-IP-Address attribute.</text>
          </description>
        </leaf>
        <leaf name="source-interface-name">
          <type name="leafref">
            <path value="/ifm:ifm/ifm:interfaces/ifm:interface/ifm:name"/>
          </type>
          <description>
            <text>To configure source interface name at group level.</text>
          </description>
        </leaf>
        <leaf name="domain-mode">
          <type name="enumeration">
            <enum name="exclude">
              <value value="0"/>
              <description>
                <text>User name excludes domain.</text>
              </description>
            </enum>
            <enum name="include">
              <value value="1"/>
              <description>
                <text>User name includes domain.</text>
              </description>
            </enum>
            <enum name="original">
              <value value="2"/>
              <description>
                <text>User name same as user input.</text>
              </description>
            </enum>
          </type>
          <default value="include"/>
          <description>
            <text>Configure domain mode of RADIUS server group.</text>
          </description>
        </leaf>
        <leaf name="alarm-enable">
          <type name="boolean"/>
          <default value="true"/>
          <description>
            <text>Enable/disable the RADIUS server alarm function.</text>
          </description>
        </leaf>
        <container name="radius-server-ipv4s">
          <description>
            <text>List of RADIUS IPv4 server.</text>
          </description>
          <list name="radius-server-ipv4">
            <key value="server-type server-ip-address server-port vpn-name"/>
            <max-elements value="32"/>
            <description>
              <text>Configure server for a particular group.</text>
            </description>
            <leaf name="server-type">
              <type name="enumeration">
                <enum name="authentication">
                  <value value="0"/>
                  <description>
                    <text>Authentication server.</text>
                  </description>
                </enum>
                <enum name="accounting">
                  <value value="1"/>
                  <description>
                    <text>Accounting server.</text>
                  </description>
                </enum>
              </type>
              <description>
                <text>Type of RADIUS server.</text>
              </description>
            </leaf>
            <leaf name="server-ip-address">
              <type name="inet:ipv4-address-no-zone"/>
              <description>
                <text>IPv4 address of configured server.</text>
              </description>
            </leaf>
            <leaf name="server-port">
              <type name="int32">
                <range value="1..65535"/>
              </type>
              <description>
                <text>Configured server port for a particular server.</text>
              </description>
            </leaf>
            <leaf name="vpn-name">
              <type name="leafref">
                <path value="/ni:network-instance/ni:instances/ni:instance/ni:name"/>
              </type>
              <must condition="/ni:network-instance/ni:instances/ni:instance[ni:name=current()]/l3vpn:afs/l3vpn:af[l3vpn:type='ipv4-unicast']"/>
              <description>
                <text>Set VPN instance.</text>
              </description>
            </leaf>
            <leaf name="shared-key">
              <type name="pub-type:password-extend">
                <length value="1..268"/>
              </type>
              <description>
                <text>To configure shared-key value for a particular server. The simple password length ranges from 1 to 128, and the cipher password length ranges from 1 to 268.</text>
              </description>
            </leaf>
            <choice name="source-ip">
              <description>
                <text>Source ip address or source interface of a RADIUS server.</text>
              </description>
              <case name="ip">
                <description>
                  <text>Specify the source IP address of a RADIUS server.</text>
                </description>
                <leaf name="source-ip-address">
                  <type name="inet:ip-address-no-zone"/>
                  <description>
                    <text>Specify the source IP address of a RADIUS server.</text>
                  </description>
                </leaf>
              </case>
              <case name="interface">
                <description>
                  <text>Specify the source interface of a RADIUS server.</text>
                </description>
                <leaf name="source-interface-name">
                  <type name="leafref">
                    <path value="/ifm:ifm/ifm:interfaces/ifm:interface/ifm:name"/>
                  </type>
                  <description>
                    <text>Name of source interface.</text>
                  </description>
                </leaf>
              </case>
            </choice>
            <leaf name="server-current-state">
              <type name="enumeration">
                <enum name="up">
                  <value value="0"/>
                  <description>
                    <text>Up.</text>
                  </description>
                </enum>
                <enum name="down">
                  <value value="1"/>
                  <description>
                    <text>Down.</text>
                  </description>
                </enum>
                <enum name="NA">
                  <value value="2"/>
                  <description>
                    <text>NA.</text>
                  </description>
                </enum>
              </type>
              <config value="false"/>
              <description>
                <text>Current state of the server.</text>
              </description>
            </leaf>
            <leaf name="is-current-server">
              <type name="boolean"/>
              <default value="false"/>
              <config value="false"/>
              <description>
                <text>Enable/disable the server as current server.</text>
              </description>
            </leaf>
            <leaf name="weight">
              <type name="uint8">
                <range value="0..100"/>
              </type>
              <default value="0"/>
              <description>
                <text>Set the weight.</text>
              </description>
            </leaf>
            <container name="ip-account-packet-statistic">
              <config value="false"/>
              <description>
                <text>Statistics of Accounting packet about a specified RADIUS server IP address.</text>
              </description>
              <leaf name="start-packet-number">
                <type name="uint32"/>
                <config value="false"/>
                <description>
                  <text>Number of Accounting Start packets in the buffer queue.</text>
                </description>
              </leaf>
              <leaf name="stop-packet-number">
                <type name="uint32"/>
                <config value="false"/>
                <description>
                  <text>Number of Accounting Stop packets in the buffer queue.</text>
                </description>
              </leaf>
              <leaf name="interim-packet-number">
                <type name="uint32"/>
                <config value="false"/>
                <description>
                  <text>Number of Accounting Interim-Update packets in the buffer queue.</text>
                </description>
              </leaf>
            </container>
          </list>
        </container>
        <container name="force-account-stop-packet">
          <description>
            <text>Configure accounting stop packets to be sent forcibly.</text>
          </description>
          <leaf name="enable">
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable accounting stop packets to be sent forcibly.</text>
            </description>
          </leaf>
        </container>
        <container name="radius-hw-dhcp-options">
          <description>
            <text>List of configuring the device to send IPv4 DHCP options in the hw-dhcp-option attribute of authentication requests.</text>
          </description>
          <list name="radius-hw-dhcp-option">
            <key value="number"/>
            <max-elements value="16"/>
            <description>
              <text>Configure the device to send IPv4 DHCP options in the hw-dhcp-option attribute of authentication requests.</text>
            </description>
            <leaf name="number">
              <type name="uint32">
                <range value="1..254"/>
              </type>
              <description>
                <text>Value of DHCP option.</text>
              </description>
            </leaf>
          </list>
        </container>
        <container name="hw-dhcpv6-options">
          <description>
            <text>List of configuring the device to send IPv6 DHCP options in the hw-dhcpv6-option attribute of authentication requests.</text>
          </description>
          <list name="hw-dhcpv6-option">
            <key value="number"/>
            <max-elements value="16"/>
            <description>
              <text>Configure the device to send IPv6 DHCP options in the hw-dhcpv6-option attribute of authentication requests.</text>
            </description>
            <leaf name="number">
              <type name="uint32">
                <range value="1..65535"/>
              </type>
              <description>
                <text>Value of IPv6 DHCP option.</text>
              </description>
            </leaf>
          </list>
        </container>
        <container name="accounting-include-hw-dhcp-option">
          <description>
            <text>Configure the device to send options in the hw-dhcp-option and hw-dhcpv6-option attribute.</text>
          </description>
          <leaf-list name="hw-dhcp-option-accounting-request">
            <type name="uint32">
              <range value="60"/>
            </type>
            <max-elements value="1"/>
            <description>
              <text>Configure the device to send IPv4 DHCP options in the hw-dhcp-option attribute of accounting requests.</text>
            </description>
          </leaf-list>
          <leaf-list name="hw-dhcpv6-option-accounting-request">
            <type name="uint32">
              <range value="16"/>
            </type>
            <max-elements value="1"/>
            <description>
              <text>Configure the device to send IPv6 DHCP options in the hw-dhcpv6-option attribute of accounting requests.</text>
            </description>
          </leaf-list>
        </container>
        <container name="version-type">
          <description>
            <text>Configure the RADIUS version used between the router and a RADIUS server.</text>
          </description>
          <leaf name="type">
            <type name="enumeration">
              <enum name="standard">
                <value value="0"/>
                <description>
                  <text>Indicates that RADIUS of the standard version that based on RFC 2865 is used between the router and the RADIUS server. This version is used to set a standard server.</text>
                </description>
              </enum>
              <enum name="plus10">
                <value value="1"/>
                <description>
                  <text>Indicates that RADIUS+1.0 is used between the router and the RADIUS server.</text>
                </description>
              </enum>
              <enum name="plus11">
                <value value="2"/>
                <description>
                  <text>Indicates that RADIUS+1.1 is used between the router and the RADIUS server.</text>
                </description>
              </enum>
            </type>
            <default value="standard"/>
            <description>
              <text>The RADIUS version used between the router and a RADIUS server.</text>
            </description>
          </leaf>
        </container>
        <container name="packet-control">
          <description>
            <text>Configure radius packet.</text>
          </description>
          <leaf name="interim-accout-resend-count">
            <type name="uint16">
              <range value="1..65535"/>
            </type>
            <description>
              <text>Enables RADIUS real-time accounting packet caching and configures the number of real-time accounting packet retransmissions.</text>
            </description>
          </leaf>
          <leaf name="start-accout-resend-count">
            <type name="uint16">
              <range value="1..65535"/>
            </type>
            <description>
              <text>Configure the number of times that cached accounting-start packets are retransmitted to the RADIUS accounting server in a RADIUS server group.</text>
            </description>
          </leaf>
          <leaf name="stop-accout-resend-count">
            <type name="uint16">
              <range value="1..65535"/>
            </type>
            <description>
              <text>Configure the number of times that cached accounting-stop packets are retransmitted to the RADIUS accounting server in a RADIUS server group.</text>
            </description>
          </leaf>
          <leaf name="packet-priority">
            <type name="uint8">
              <range value="0..63"/>
            </type>
            <description>
              <text>Sets the DSCP value for RADIUS packets sent by the device to a RADIUS server.</text>
            </description>
          </leaf>
          <leaf name="vas-account-packed-max-length">
            <type name="uint32">
              <range value="500..4096"/>
            </type>
            <description>
              <text>Sets the maximum length for a post-merging accounting packet for value-added services.</text>
            </description>
          </leaf>
        </container>
        <container name="common-attribute">
          <description>
            <text>Configure global radius server.</text>
          </description>
          <leaf name="ncp-up-send-start-account">
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable the router to send Accounting Start packets to the RADIUS server after NCP goes Up for PPPv6 users that use DHCPv6 to obtain IPv6 addresses.</text>
            </description>
          </leaf>
          <leaf name="test-aaa-send-account-start">
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable the router to send Accounting Start packets to the RADIUS server upon the test-aaa command execution.</text>
            </description>
          </leaf>
          <leaf name="encode-option61-to-attribute153">
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable the device to encapsulate the Option61 field into the No.153 proprietary attribute on the RADIUS server, hw-user-mac attribute.</text>
            </description>
          </leaf>
          <leaf name="decode-fail-ignore-hw-data-filter">
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable a device to ignore a RADIUS attribute if the device fails to parse this attribute.</text>
            </description>
          </leaf>
          <leaf name="frame-ip-mask">
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable a device to add a 32-bit Framed-IP-Netmask attribute to an Accounting-Request packet. By default, an Accounting-Request packet does not carry a 32-bit Framed-IP-Netmask attribute.</text>
            </description>
          </leaf>
          <leaf name="service-type-value">
            <type name="enumeration">
              <enum name="framed">
                <value value="0"/>
                <description>
                  <text>Service type value framed.</text>
                </description>
              </enum>
              <enum name="outbound">
                <value value="1"/>
                <description>
                  <text>Service type value Outbound.</text>
                </description>
              </enum>
            </type>
            <default value="framed"/>
            <description>
              <text>Sets service type value.</text>
            </description>
          </leaf>
          <leaf name="service-type-outbound-vas-enable">
            <when condition="../service-type-value='outbound'"/>
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable the value-added-service when the Service-Type attribute value as 5 in common IPoE users' authentication and accounting request packets sent to the RADIUS server.</text>
            </description>
          </leaf>
        </container>
        <container name="attribute-format-called-station-id">
          <must condition="(include-type!='ap-mac' and not(user-defined)) or (include-type='ap-mac' and user-defined)"/>
          <presence value="attribute-format-called-station-id."/>
          <description>
            <text>Enable/disable RADIUS attribute format of Called-Station-Id.</text>
          </description>
          <leaf name="include-type">
            <type name="enumeration">
              <enum name="ap-mac">
                <value value="1"/>
                <description>
                  <text>Indicates that the encapsulation content of the Called-station-id (30) attribute is the AP's MAC address.</text>
                </description>
              </enum>
              <enum name="ac-ip">
                <value value="3"/>
                <description>
                  <text>Indicates that the encapsulation content of the Called-station-id (30) attribute is the AC's IP address.</text>
                </description>
              </enum>
            </type>
            <mandatory value="true"/>
            <description>
              <text>Radius server attribute format of called-station-id.</text>
            </description>
          </leaf>
          <container name="user-defined">
            <must condition="include-attributes"/>
            <presence value="user-defined."/>
            <description>
              <text>Enable/disable user defined.</text>
            </description>
            <leaf name="head-delimiter">
              <type name="string">
                <length value="1"/>
                <pattern value="[b\*\-\\#$@&amp;:]"/>
              </type>
              <description>
                <text>Configure the user-defined format of the Called-Station-Id include delimiter.</text>
              </description>
            </leaf>
            <container name="include-attributes">
              <description>
                <text>List of the user-defined format of the Called-Station-Id attributes.</text>
              </description>
              <list name="include-attribute">
                <key value="attribute-type"/>
                <max-elements value="2"/>
                <ordered-by value="user"/>
                <description>
                  <text>Configure the user-defined format of the Called-Station-Id attribute.</text>
                </description>
                <leaf name="attribute-type">
                  <type name="enumeration">
                    <enum name="ap-mac">
                      <value value="1"/>
                      <description>
                        <text>Configure AP MAC content.</text>
                      </description>
                    </enum>
                    <enum name="ssid">
                      <value value="2"/>
                      <description>
                        <text>Configure SSID content.</text>
                      </description>
                    </enum>
                  </type>
                  <description>
                    <text>Configure the user-defined format of the attribute delimiter one.</text>
                  </description>
                </leaf>
                <leaf name="delimiter">
                  <type name="string">
                    <length value="1"/>
                    <pattern value="[b\*\-\\#$@&amp;:]"/>
                  </type>
                  <description>
                    <text>Configure the user-defined format of the attribute delimiter.</text>
                  </description>
                </leaf>
                <leaf name="split-format">
                  <when condition="../attribute-type='ap-mac'"/>
                  <type name="enumeration">
                    <enum name="colon">
                      <value value="1"/>
                      <description>
                        <text>Indicates that the colon (:) is used as the separator in a MAC address, such as 00:01:af:e1:16:01.</text>
                      </description>
                    </enum>
                    <enum name="hyphen">
                      <value value="2"/>
                      <description>
                        <text>Indicates that the hyphen (-) is used as the separator in a MAC address, such as 00-01-af-e1-16-01.</text>
                      </description>
                    </enum>
                  </type>
                  <default value="colon"/>
                  <description>
                    <text>MAC address split format.</text>
                  </description>
                </leaf>
              </list>
            </container>
          </container>
        </container>
        <container name="attribute-format-calling-station-id">
          <description>
            <text>Configure radius server attribute format of calling-station-id.</text>
          </description>
          <choice name="include-type">
            <description>
              <text>Radius server attribute format of calling-station-id.</text>
            </description>
            <case name="refer-option61">
              <description>
                <text>Refer to option61.</text>
              </description>
              <leaf name="refer-option61-enable">
                <type name="empty"/>
                <description>
                  <text>Refer to option61.</text>
                </description>
              </leaf>
            </case>
            <case name="vlan-binding">
              <description>
                <text>VLAN binding.</text>
              </description>
              <leaf name="vlan-binding-enable">
                <type name="empty"/>
                <description>
                  <text>VLAN binding.</text>
                </description>
              </leaf>
            </case>
            <case name="vlan-description">
              <description>
                <text>VLAN description.</text>
              </description>
              <leaf name="vlan-description-enable">
                <type name="empty"/>
                <description>
                  <text>VLAN description.</text>
                </description>
              </leaf>
            </case>
            <case name="llid">
              <description>
                <text>Logical line identifier.</text>
              </description>
              <leaf name="ppp">
                <type name="empty"/>
                <description>
                  <text>PPP access user.</text>
                </description>
              </leaf>
              <leaf name="lns">
                <type name="empty"/>
                <description>
                  <text>LNS access user.</text>
                </description>
              </leaf>
            </case>
            <case name="pevlan-before-cevlan">
              <description>
                <text>User's peVLAN priority ceVLAN.</text>
              </description>
              <leaf name="pevlan-delimiter">
                <type name="string">
                  <length value="1"/>
                  <pattern value="[b\*\-_\\#$@&amp;:]"/>
                </type>
                <description>
                  <text>Configure peVLAN delimiter of calling-station-id.</text>
                </description>
              </leaf>
              <leaf name="cevlan">
                <type name="empty"/>
                <description>
                  <text>User's ceVLAN.</text>
                </description>
              </leaf>
            </case>
            <case name="cevlan-before-pevlan">
              <description>
                <text>User's ceVLAN priority peVLAN.</text>
              </description>
              <leaf name="cevlan-delimiter">
                <type name="string">
                  <length value="1"/>
                  <pattern value="[b\*\-_\\#$@&amp;:]"/>
                </type>
                <description>
                  <text>Configure ceVLAN delimiter of calling-station-id.</text>
                </description>
              </leaf>
              <leaf name="pevlan">
                <type name="empty"/>
                <description>
                  <text>User's peVLAN.</text>
                </description>
              </leaf>
            </case>
            <case name="line-id">
              <description>
                <text>Configure line-id.</text>
              </description>
              <leaf name="line-id-enable">
                <type name="empty"/>
                <description>
                  <text>Configure line-id.</text>
                </description>
              </leaf>
            </case>
            <case name="user-defined">
              <description>
                <text>User defained.</text>
              </description>
              <leaf name="head-delimiter">
                <type name="string">
                  <length value="1"/>
                  <pattern value="[b\*\-\\#$@&amp;:]"/>
                </type>
                <description>
                  <text>Configure the user-defined format of the Calling-Station-Id include delimiter.</text>
                </description>
              </leaf>
              <container name="include-attributes">
                <description>
                  <text>List of the user-defined format of the Calling-Station-Id attributes.</text>
                </description>
                <list name="include-attribute">
                  <key value="attribute-type"/>
                  <min-elements value="1"/>
                  <max-elements value="5"/>
                  <ordered-by value="user"/>
                  <description>
                    <text>Configure the user-defined format of the Calling-Station-Id attribute.</text>
                  </description>
                  <leaf name="attribute-type">
                    <type name="enumeration">
                      <enum name="option82-or-access-line-id">
                        <value value="1"/>
                        <description>
                          <text>User's access-line-id (dhcpv4 option82/pppoe+/dhcpv6option18+37+17), the 'option82' and 'access-line-id' can not be configed together.</text>
                        </description>
                      </enum>
                      <enum name="mac">
                        <value value="3"/>
                        <description>
                          <text>Configure SSID content.</text>
                        </description>
                      </enum>
                      <enum name="sysname">
                        <value value="4"/>
                        <description>
                          <text>Configure SSID content.</text>
                        </description>
                      </enum>
                      <enum name="interface">
                        <value value="5"/>
                        <description>
                          <text>Configure SSID content.</text>
                        </description>
                      </enum>
                      <enum name="domain">
                        <value value="6"/>
                        <description>
                          <text>Configure SSID content.</text>
                        </description>
                      </enum>
                    </type>
                    <description>
                      <text>Configure the user-defined format of the attribute delimiter one.</text>
                    </description>
                  </leaf>
                  <leaf name="delimiter">
                    <type name="string">
                      <length value="1"/>
                      <pattern value="[b\*\-\\#$@&amp;:]"/>
                    </type>
                    <description>
                      <text>Configure the user-defined format of the attribute delimiter.</text>
                    </description>
                  </leaf>
                  <leaf name="option82-or-access-line-id-type">
                    <when condition="../attribute-type='option82-or-access-line-id'"/>
                    <type name="enumeration">
                      <enum name="option82">
                        <value value="1"/>
                        <description>
                          <text>User's access-line-id (dhcpv4 option82/pppoe+/dhcpv6option18+37+17), the 'option82' and 'access-line-id' can not be configed together.</text>
                        </description>
                      </enum>
                      <enum name="access-line-id">
                        <value value="2"/>
                        <description>
                          <text>User's access-line-id (dhcpv4 option82/pppoe+/dhcpv6option18+37+17), the 'option82' and 'access-line-id' can not be configed together.</text>
                        </description>
                      </enum>
                    </type>
                    <mandatory value="true"/>
                    <description>
                      <text>User's access-line-id (dhcpv4 option82/pppoe+/dhcpv6option18+37+17).</text>
                    </description>
                  </leaf>
                  <leaf name="split-format">
                    <when condition="../attribute-type='mac'"/>
                    <type name="enumeration">
                      <enum name="colon">
                        <value value="1"/>
                        <description>
                          <text>Indicates that the colon (:) is used as the separator in a MAC address, such as 00:01:af:e1:16:01.</text>
                        </description>
                      </enum>
                      <enum name="hyphen">
                        <value value="2"/>
                        <description>
                          <text>Indicates that the hyphen (-) is used as the separator in a MAC address, such as 00-01-af-e1-16-01.</text>
                        </description>
                      </enum>
                    </type>
                    <default value="colon"/>
                    <description>
                      <text>MAC address split format.</text>
                    </description>
                  </leaf>
                </list>
              </container>
            </case>
          </choice>
        </container>
        <container name="attribute-calling-station-id-lns-default">
          <description>
            <text>Configure the LNS to encapsulate the Calling-Station-Id attribute into RADIUS authentication and accounting packets in the default version1 format.</text>
          </description>
          <leaf name="lns-version">
            <type name="enumeration">
              <enum name="version1">
                <value value="1"/>
                <description>
                  <text>Configure the LNS to encapsulate the Calling-Station-Id attribute into RADIUS authentication and accounting packets in the default version1 format.</text>
                </description>
              </enum>
              <enum name="version1-force">
                <value value="2"/>
                <description>
                  <text>Configure the LNS to encapsulate the Calling-Station-Id attribute into RADIUS authentication and accounting packets in the default version1 format irrespective of whether the LAC sends the calling-number attribute to the LNS.</text>
                </description>
              </enum>
            </type>
            <description>
              <text>Configure the LNS to encapsulate the Calling-Station-Id attribute into RADIUS authentication and accounting packets in the default version1 format.</text>
            </description>
          </leaf>
        </container>
        <container name="attribute-vendor-id">
          <presence value="attribute-vendor-id."/>
          <description>
            <text>Enable/disable the ID of a vendor whose private RADIUS attribute the device can parse.</text>
          </description>
          <leaf name="vendor-number">
            <type name="uint32">
              <range value="1..65535"/>
            </type>
            <mandatory value="true"/>
            <description>
              <text>Vendor ID.</text>
            </description>
          </leaf>
        </container>
        <container name="nas-port-id-format">
          <description>
            <text>Configure the format of the NAS-Port or NAS-Port-Id attribute.</text>
          </description>
          <leaf name="format">
            <type name="enumeration">
              <enum name="version1">
                <value value="1"/>
                <description>
                  <text>Indicates encapsulating the NAS-Port-Id attribute in Huawei's default format.</text>
                </description>
              </enum>
              <enum name="version2">
                <value value="2"/>
                <description>
                  <text>Version2:format as &lt;interface-type&gt; &lt;slot&gt;/&lt;sub-slot&gt;/&lt;port&gt;.&lt;subinterface&gt; [:&lt;out-VLAN&gt;][.&lt;in-VLAN&gt;]</text>
                </description>
              </enum>
              <enum name="vendor">
                <value value="3"/>
                <description>
                  <text>Specifies the ID of a supplier.</text>
                </description>
              </enum>
              <enum name="redback-simple">
                <value value="4"/>
                <description>
                  <text>Specifies the format of Redback-simple to encapsulate the NAS-Port-Id attribute. 'VLAN-ID' is used as 'VLANID' in the format of Redback-simple.</text>
                </description>
              </enum>
              <enum name="redback-addition">
                <value value="5"/>
                <description>
                  <text>Specifies the format of redback-addition to encapsulate the NAS-Port-Id attribute.</text>
                </description>
              </enum>
            </type>
            <default value="version1"/>
            <description>
              <text>Configure the format of the NAS-Port or NAS-Port-Id attribute.</text>
            </description>
          </leaf>
          <leaf name="vendor-id">
            <when condition="../format='vendor'"/>
            <type name="uint32">
              <range value="9|2352|2636"/>
            </type>
            <mandatory value="true"/>
            <description>
              <text>Specifies the ID of a supplier.</text>
            </description>
          </leaf>
          <leaf name="vendor-version">
            <when condition="../vendor-id=2636"/>
            <type name="enumeration">
              <enum name="version1">
                <value value="1"/>
                <description>
                  <text>Version1 for vendor2636.</text>
                </description>
              </enum>
            </type>
            <description>
              <text>Indicates the version format for specified vendor to encapsulate the NAS-Port-Id attribute.</text>
            </description>
          </leaf>
        </container>
        <container name="attribute-include-nas">
          <must condition="not(nas-port-enable = 'true' and with-user-id-accounting-request = 'true')"/>
          <description>
            <text>Configure the automatic LAC receiving NAS-PORT attribute.</text>
          </description>
          <leaf name="nas-port-enable">
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable the automatic LAC receiving NAS-PORT attribute.</text>
            </description>
          </leaf>
          <leaf name="with-user-id-accounting-request">
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable encapsulate type in the nas-port Attribute.</text>
            </description>
          </leaf>
        </container>
        <container name="attribute-traffic-unit">
          <description>
            <text>Configure radius server attribute format.</text>
          </description>
          <leaf name="traffic-unit">
            <type name="enumeration">
              <enum name="byte">
                <value value="0"/>
                <description>
                  <text>Indicates that the traffic unit is byte.</text>
                </description>
              </enum>
              <enum name="kbyte">
                <value value="1"/>
                <description>
                  <text>Indicates that the traffic unit is Kbyte.</text>
                </description>
              </enum>
              <enum name="mbyte">
                <value value="2"/>
                <description>
                  <text>Indicates that the traffic unit is Mbyte.</text>
                </description>
              </enum>
              <enum name="gbyte">
                <value value="3"/>
                <description>
                  <text>Indicates that the traffic unit is Gbyte.</text>
                </description>
              </enum>
            </type>
            <default value="byte"/>
            <description>
              <text>Sets the traffic unit for a RADIUS server.</text>
            </description>
          </leaf>
        </container>
        <container name="attribute-packet-format">
          <description>
            <text>Configure the attribute packet.</text>
          </description>
          <leaf name="packet-format">
            <type name="enumeration">
              <enum name="tr-101-format">
                <value value="1"/>
                <description>
                  <text>Indicates the TR-101 format.</text>
                </description>
              </enum>
              <enum name="cn-format">
                <value value="0"/>
                <description>
                  <text>Indicates that the packets that inform the upstream device of the link ID are in the CN format.</text>
                </description>
              </enum>
            </type>
            <default value="cn-format"/>
            <description>
              <text>Configure the format of packets that inform the upstream device of the link ID.</text>
            </description>
          </leaf>
        </container>
        <container name="attribute-tunnel-password">
          <description>
            <text>Configure the attribute packet.</text>
          </description>
          <leaf name="password-type">
            <type name="enumeration">
              <enum name="simple">
                <value value="1"/>
                <description>
                  <text>Password type as simple.</text>
                </description>
              </enum>
              <enum name="cipher">
                <value value="0"/>
                <description>
                  <text>Password type as ciphertext.</text>
                </description>
              </enum>
            </type>
            <default value="cipher"/>
            <description>
              <text>Sets the mode in which the RADIUS server sends a tunnel password to simple text or cipher.</text>
            </description>
          </leaf>
        </container>
        <container name="attribute-connect">
          <description>
            <text>Configure the attribute packet.</text>
          </description>
          <leaf name="refer-option82">
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable authentication and accounting request packets to use the Connect-Info attribute to carry the uplink and downlink rates negotiated between the BRAS and DSLAM.</text>
            </description>
          </leaf>
        </container>
        <container name="attribute-sub-slot">
          <description>
            <text>Configure the attribute packet.</text>
          </description>
          <leaf name="sub-slot">
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable the system to use the interface number format of slot number/sub-slot number/interface number in the calling-station-ID and NAS-port-ID attributes.</text>
            </description>
          </leaf>
        </container>
        <container name="attribute-nas-port">
          <description>
            <text>Configure the attribute packet.</text>
          </description>
          <leaf name="nas-port-value">
            <type name="string">
              <length value="1..32"/>
            </type>
            <description>
              <text>Enables the system to use the NAS-port-ID attributes.</text>
            </description>
          </leaf>
        </container>
        <container name="attribute-access-policy">
          <description>
            <text>Configure the attribute action.</text>
          </description>
          <leaf name="if-domain-blocked">
            <type name="enumeration">
              <enum name="online">
                <value value="0"/>
                <description>
                  <text>Allows users to go online if the domain delivered by the RADIUS server is blocked.</text>
                </description>
              </enum>
              <enum name="offline">
                <value value="1"/>
                <description>
                  <text>Forbids users to go online if the domain delivered by the RADIUS server is blocked.</text>
                </description>
              </enum>
            </type>
            <default value="online"/>
            <description>
              <text>Configure an access policy for users if the domain delivered by the RADIUS server is blocked.</text>
            </description>
          </leaf>
        </container>
        <container name="attribute-user-name">
          <description>
            <text>Configure the attribute action.</text>
          </description>
          <leaf name="author-reply-user-name-policy">
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable the device to reply with a CoA/DM response packet that carries the same user name as that in a CoA/DM request packet.</text>
            </description>
          </leaf>
          <leaf name="local-coa-enable">
            <when condition="../author-reply-user-name-policy='true'"/>
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable a user name adding policy for CoA response packets.</text>
            </description>
          </leaf>
          <leaf name="local-dm-enable">
            <when condition="../author-reply-user-name-policy='true'"/>
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable a user name adding policy for DM response packets.</text>
            </description>
          </leaf>
          <leaf name="update-domain-name">
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable devices in a RADIUS server group to replace the domain name carried in a user name with the domain name delivered by the RADIUS server.</text>
            </description>
          </leaf>
        </container>
        <container name="attribute-action-hw-mng-ipv6">
          <description>
            <text>Configure the hw-mng-IPv6 attribute action.</text>
          </description>
          <leaf name="hw-mng-ipv6-motm-enable">
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable the encapsulation format and name of the hw-mng-IPv6 attribute to be encapsulated into a PPP packet's MTOM field.</text>
            </description>
          </leaf>
          <leaf name="hw-mng-ipv6-encapsulating-name">
            <when condition="../hw-mng-ipv6-motm-enable='true'"/>
            <type name="string">
              <length value="1..31"/>
            </type>
            <description>
              <text>Specifies the encapsulation format and name of the hw-mng-IPv6 attribute to be encapsulated into a PPP packet's MTOM field.</text>
            </description>
          </leaf>
          <leaf name="hw-mng-ipv6-delimiter">
            <when condition="../hw-mng-ipv6-encapsulating-name"/>
            <type name="string">
              <length value="1"/>
            </type>
            <mandatory value="true"/>
            <description>
              <text>Specifies a delimiter of the hw-mng-IPv6 attribute to be encapsulated into a PPP packet's MTOM field.</text>
            </description>
          </leaf>
          <leaf name="hw-mng-ipv6-exclude-local">
            <when condition="../hw-mng-ipv6-motm-enable='true'"/>
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable that the local configuration is excluded of the hw-mng-IPv6 attribute to be encapsulated into a PPP packet's MTOM field.</text>
            </description>
          </leaf>
        </container>
        <container name="attribute-apply">
          <description>
            <text>Configure the attribute action.</text>
          </description>
          <leaf name="ipoe-user-name-apply">
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable the router to replace the IPoE user name generated on the router with the user name delivered by the RADIUS server.</text>
            </description>
          </leaf>
          <leaf name="ipoe-priority-apply">
            <when condition="../ipoe-user-name-apply='true'"/>
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable RADIUS server delivered user name replaces the IPoE user name generated on the router with a higher priority than the user name in the Service identity policy.</text>
            </description>
          </leaf>
          <leaf name="pppoe-user-name-apply">
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable the router to replace the PPPoE user name generated on the router with the user name delivered by the RADIUS server.</text>
            </description>
          </leaf>
          <leaf name="replace-ipv6-pools-with-same-type">
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable an IPv6 address pool to be delivered by a RADIUS server using the Framed-IPv6-Pool attribute to replace the IPv6 address pools of the same type configured in a domain.</text>
            </description>
          </leaf>
        </container>
        <container name="attribute-disable-account">
          <description>
            <text>Configure accounting stop packets to be sent forcibly.</text>
          </description>
          <leaf name="flow-attributes">
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable if you specify the flow-attributes parameter. The following RADIUS attributes are all disabled: Acct-Input-Octets, Acct-Output-Octets, Acct-Input-Packets, Acct-Output-Packets, Acct-Input-Gigawords, Acct-Output-Gigawords, HW-Acct-IPv6-Input-Octets, HW-Acct-IPv6-Output-Octets, HW-Acct-IPv6-Input-Packets, HW-Acct-IPv6-Output-Packet, HW-Acct-IPv6-Input-Gigawords, HW-Acct-IPv6-Output-Gigawords.</text>
            </description>
          </leaf>
          <leaf name="hw-acct-update-address">
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable the HW-Acct-Update-Address attribute when the attribute value is equal to a certain integer.</text>
            </description>
          </leaf>
        </container>
        <container name="standard-attributes">
          <must condition="not((count(../extend-attributes/extend-attribute[operation-type='translate'])&gt;0) and (count(standard-attribute[operation-type='translate'])&gt;0))"/>
          <must condition="not((count(../extend-attributes/extend-attribute[operation-type='disable'])&gt;0) and (count(standard-attribute[operation-type='disable'])&gt;0))"/>
          <must condition="not((count(../vendor-specifics/vendor-specific[operation-type='disable'])&gt;0) and (count(standard-attribute[operation-type='disable'])&gt;0))"/>
          <description>
            <text>List of the standard RADIUS attributes.
Standard attributes cannot be disabled and extended attributes cannot be disabled on the same server at the same time.
Standard attributes cannot be translated and extended attributes cannot be translated on the same server at the same time.</text>
          </description>
          <list name="standard-attribute">
            <key value="attribute-name"/>
            <max-elements value="64"/>
            <description>
              <text>Configure RADIUS attributes.</text>
            </description>
            <leaf name="attribute-name">
              <type name="radius-attribute-table"/>
              <description>
                <text>Specifies an attribute name.</text>
              </description>
            </leaf>
            <leaf name="operation-type">
              <ext:operation-exclude value="update"/>
              <type name="enumeration">
                <enum name="disable">
                  <value value="0"/>
                  <description>
                    <text>Disable radius attributes.</text>
                  </description>
                </enum>
                <enum name="translate">
                  <value value="1"/>
                  <description>
                    <text>Translate radius attributes.</text>
                  </description>
                </enum>
              </type>
              <must condition="(../operation-type='translate' and ../translate-attribute and not(../disable-attribute)) or (../operation-type='disable' and (../disable-attribute/disable-packet or ../disable-attribute/disable-direction or ../disable-attribute/disable-value) and not(../translate-attribute))"/>
              <mandatory value="true"/>
              <description>
                <text>Operation of radius attributes.</text>
              </description>
            </leaf>
            <container name="translate-attribute">
              <when condition="../operation-type='translate'"/>
              <description>
                <text>Configure radius attributes.</text>
              </description>
              <leaf name="attribute-name">
                <type name="radius-attribute-table"/>
                <must condition="not(../../attribute-name=../attribute-name)"/>
                <mandatory value="true"/>
                <description>
                  <text>Translate attribute name.</text>
                </description>
              </leaf>
              <choice name="translate-type">
                <mandatory value="true"/>
                <description>
                  <text>Translate types.</text>
                </description>
                <case name="translate-packet-type">
                  <description>
                    <text>Configure radius translate attributes by packet type.</text>
                  </description>
                  <leaf-list name="packet-type">
                    <type name="enumeration">
                      <enum name="access-request">
                        <value value="0"/>
                        <description>
                          <text>Access request packet.</text>
                        </description>
                      </enum>
                      <enum name="access-accept">
                        <value value="1"/>
                        <description>
                          <text>Access accept packet.</text>
                        </description>
                      </enum>
                      <enum name="account">
                        <value value="2"/>
                        <description>
                          <text>Account packet.</text>
                        </description>
                      </enum>
                    </type>
                    <description>
                      <text>Configure a packet type.</text>
                    </description>
                  </leaf-list>
                </case>
                <case name="translate-direction-type">
                  <description>
                    <text>Configure radius attributes by direction type.</text>
                  </description>
                  <leaf-list name="direction-type">
                    <type name="enumeration">
                      <enum name="send">
                        <value value="0"/>
                        <description>
                          <text>Attribute action direction, NAS to RADIUS.</text>
                        </description>
                      </enum>
                      <enum name="receive">
                        <value value="1"/>
                        <description>
                          <text>Attribute action direction, RADIUS to NAS.</text>
                        </description>
                      </enum>
                    </type>
                    <description>
                      <text>Configure a direction type.</text>
                    </description>
                  </leaf-list>
                </case>
              </choice>
            </container>
            <container name="disable-attribute">
              <when condition="../operation-type='disable'"/>
              <description>
                <text>Configure radius attributes.</text>
              </description>
              <container name="disable-packet">
                <description>
                  <text>Configure radius attributes by packet type.</text>
                </description>
                <leaf-list name="type">
                  <type name="enumeration">
                    <enum name="access-request">
                      <value value="0"/>
                      <description>
                        <text>Access request packet.</text>
                      </description>
                    </enum>
                    <enum name="access-accept">
                      <value value="1"/>
                      <description>
                        <text>Access accept packet.</text>
                      </description>
                    </enum>
                    <enum name="account">
                      <value value="2"/>
                      <description>
                        <text>Account packet.</text>
                      </description>
                    </enum>
                    <enum name="account-start">
                      <value value="3"/>
                      <description>
                        <text>Start account packet.</text>
                      </description>
                    </enum>
                    <enum name="coa-request">
                      <value value="4"/>
                      <description>
                        <text>COA request packet.</text>
                      </description>
                    </enum>
                    <enum name="dm-request">
                      <value value="5"/>
                      <description>
                        <text>Dm request packet.</text>
                      </description>
                    </enum>
                  </type>
                  <must condition="not(../../disable-direction/type) and not(../../disable-value/attribute-value)"/>
                  <must condition="(../type='account-start' and ../../../attribute-name='acct-delay-time') or (../type!='account-start' and ../../../attribute-name)"/>
                  <description>
                    <text>Configure a packet type.</text>
                  </description>
                </leaf-list>
              </container>
              <container name="disable-direction">
                <description>
                  <text>Configure radius attributes by packet type.</text>
                </description>
                <leaf-list name="type">
                  <type name="enumeration">
                    <enum name="send">
                      <value value="0"/>
                      <description>
                        <text>Attribute action direction, NAS to RADIUS.</text>
                      </description>
                    </enum>
                    <enum name="receive">
                      <value value="1"/>
                      <description>
                        <text>Attribute action direction, RADIUS to NAS.</text>
                      </description>
                    </enum>
                  </type>
                  <must condition="not(../../disable-packet/type) and not(../../disable-value/attribute-value)"/>
                  <description>
                    <text>Configure a direction type.</text>
                  </description>
                </leaf-list>
              </container>
              <container name="disable-value">
                <description>
                  <text>Configure radius attributes by value.</text>
                </description>
                <leaf-list name="attribute-value">
                  <type name="string">
                    <length value="1..254"/>
                  </type>
                  <must condition="not(../../disable-packet/type) and not(../../disable-direction/type)"/>
                  <max-elements value="2"/>
                  <description>
                    <text>Configure disable attribute for attribute value.</text>
                  </description>
                </leaf-list>
              </container>
            </container>
          </list>
        </container>
        <container name="extend-attributes">
          <must condition="count(extend-attribute)+count(../vendor-specifics/vendor-specific)&lt;=64"/>
          <description>
            <text>List of the extend RADIUS attributes.</text>
          </description>
          <list name="extend-attribute">
            <key value="attribute-name"/>
            <description>
              <text>Configure extend RADIUS attributes.</text>
            </description>
            <leaf name="attribute-name">
              <type name="bras-radius-attr-ext-type"/>
              <description>
                <text>Specifies an attribute name.</text>
              </description>
            </leaf>
            <leaf name="operation-type">
              <type name="enumeration">
                <enum name="disable">
                  <value value="0"/>
                  <description>
                    <text>Disable radius attributes.</text>
                  </description>
                </enum>
                <enum name="translate">
                  <value value="1"/>
                  <description>
                    <text>Translate radius attributes.</text>
                  </description>
                </enum>
              </type>
              <must condition="(../operation-type='translate' and ((../translate-name and (../access-request='true' or ../access-accept='true' or ../account='true')) or ((../translate-vendor-id and ../translate-sub-attr-id) and ((../access-request='true' or ../account='true') and ../access-accept='false')))) or (../operation-type='disable' and not(../translate-name or ../translate-vendor-id or ../translate-sub-attr-id) and (../access-request='true' or ../access-accept='true' or ../account='true'))"/>
              <mandatory value="true"/>
              <description>
                <text>Operation type of radius attributes.</text>
              </description>
            </leaf>
            <choice name="translate">
              <description>
                <text>Translate value.</text>
              </description>
              <case name="attribute">
                <description>
                  <text>Translate attribute name.</text>
                </description>
                <leaf name="translate-name">
                  <when condition="../operation-type='translate'"/>
                  <type name="bras-radius-attr-ext-type"/>
                  <must condition="not(../attribute-name=../translate-name)"/>
                  <description>
                    <text>Attribute name.</text>
                  </description>
                </leaf>
              </case>
              <case name="vendor-specific">
                <description>
                  <text>Translate vendor specific.</text>
                </description>
                <leaf name="translate-vendor-id">
                  <when condition="../operation-type='translate'"/>
                  <type name="uint32">
                    <range value="1..4294967295"/>
                  </type>
                  <description>
                    <text>Vendor specific attribute id.</text>
                  </description>
                </leaf>
                <leaf name="translate-sub-attr-id">
                  <when condition="../operation-type='translate'"/>
                  <type name="uint32">
                    <range value="1..255"/>
                  </type>
                  <description>
                    <text>Vendor specific sub attribute id.</text>
                  </description>
                </leaf>
              </case>
            </choice>
            <leaf name="access-request">
              <type name="boolean"/>
              <default value="false"/>
              <description>
                <text>Enable/disable operate attribute for access request packet.</text>
              </description>
            </leaf>
            <leaf name="access-accept">
              <type name="boolean"/>
              <default value="false"/>
              <description>
                <text>Enable/disable operate attribute for access accept packet.</text>
              </description>
            </leaf>
            <leaf name="account">
              <type name="boolean"/>
              <default value="false"/>
              <description>
                <text>Enable/disable operate attribute for account packet.</text>
              </description>
            </leaf>
          </list>
        </container>
        <container name="vendor-specifics">
          <must condition="count(../extend-attributes/extend-attribute)+count(vendor-specific)&lt;=64"/>
          <description>
            <text>List of the vendor specific RADIUS attribute.</text>
          </description>
          <list name="vendor-specific">
            <key value="vendor-id sub-attr-id"/>
            <description>
              <text>Configure the vendor specific RADIUS attribute.</text>
            </description>
            <leaf name="vendor-id">
              <type name="uint32">
                <range value="1..4294967295"/>
              </type>
              <description>
                <text>Vendor specific attribute id.</text>
              </description>
            </leaf>
            <leaf name="sub-attr-id">
              <type name="uint32">
                <range value="1..255"/>
              </type>
              <description>
                <text>Vendor specific sub attribute id.</text>
              </description>
            </leaf>
            <leaf name="operation-type">
              <type name="enumeration">
                <enum name="disable">
                  <value value="0"/>
                  <description>
                    <text>Disable vendor specific attribute.</text>
                  </description>
                </enum>
                <enum name="translate">
                  <value value="1"/>
                  <description>
                    <text>Translate vendor specific attribute.</text>
                  </description>
                </enum>
              </type>
              <mandatory value="true"/>
              <description>
                <text>Operation type of vendor specific attribute.</text>
              </description>
            </leaf>
            <leaf name="translate-attribute">
              <when condition="../operation-type='translate'"/>
              <type name="bras-radius-attr-ext-type"/>
              <mandatory value="true"/>
              <description>
                <text>Translate attribute name.</text>
              </description>
            </leaf>
          </list>
        </container>
        <container name="radius-server-algorithm">
          <description>
            <text>Configure algorithm of a RADIUS server.</text>
          </description>
          <leaf name="radius-arithmetic">
            <type name="enumeration">
              <enum name="master-backup">
                <value value="1"/>
                <description>
                  <text>Indicates the active/standby algorithm. Only the RADIUS authentication server or accounting server configured first is the active server.</text>
                </description>
              </enum>
              <enum name="loading-share">
                <value value="2"/>
                <description>
                  <text>Indicates the load-sharing algorithm. The weight of the RADIUS authentication server or accounting server takes effect only after loading-share is used.</text>
                </description>
              </enum>
            </type>
            <default value="master-backup"/>
            <description>
              <text>Sets the algorithm of a RADIUS server.</text>
            </description>
          </leaf>
          <leaf name="master-backup-rule">
            <when condition="../radius-arithmetic='master-backup'"/>
            <type name="enumeration">
              <enum name="strict">
                <value value="1"/>
                <description>
                  <text>Indicates that the accounting server is selected based on the configured algorithm.</text>
                </description>
              </enum>
              <enum name="sequence">
                <value value="2"/>
                <description>
                  <text>The system selects a server in the sequence in which the servers are configured.</text>
                </description>
              </enum>
              <enum name="strict-and-sequence">
                <value value="3"/>
                <description>
                  <text>Indicates that the accounting server is selected based on the configured algorithm and the system selects a server in the sequence in which the servers are configured.</text>
                </description>
              </enum>
            </type>
            <description>
              <text>Indicates the rule of the active/standby algorithm.</text>
            </description>
          </leaf>
        </container>
        <container name="rollover-on-reject">
          <description>
            <text>Configure a user fails to be authenticated by a RADIUS server in the group, the router sends an Access-Request packet to another RADIUS server until the user is authenticated or fails to be authenticated by any RADIUS servers.</text>
          </description>
          <leaf name="enable">
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable if a user fails to be authenticated by a RADIUS server in the group, the router sends an Access-Request packet to another RADIUS server until the user is authenticated or fails to be authenticated by any RADIUS servers.</text>
            </description>
          </leaf>
        </container>
