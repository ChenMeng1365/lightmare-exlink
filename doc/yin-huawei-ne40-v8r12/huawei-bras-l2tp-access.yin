  <grouping name="lastest-tunnel-record-backup">
    <description>
      <text>Lastest tunnel record backup info.</text>
    </description>
    <container name="lastest-tunnel-records">
      <description>
        <text>List of l2tp tunnel backup statistics in detail.</text>
      </description>
      <list name="lastest-tunnel-record">
        <key value="index"/>
        <description>
          <text>Statistics of l2tp tunnel backup statistics in detail.</text>
        </description>
        <leaf name="index">
          <type name="uint32">
            <range value="1..100"/>
          </type>
          <description>
            <text>Index.</text>
          </description>
        </leaf>
        <leaf name="local-id">
          <type name="uint32">
            <range value="1..65535"/>
          </type>
          <description>
            <text>Local tunnel ID.</text>
          </description>
        </leaf>
        <leaf name="remote-id">
          <type name="uint32">
            <range value="1..65535"/>
          </type>
          <description>
            <text>Remote tunnel ID.</text>
          </description>
        </leaf>
        <leaf name="type">
          <type name="enumeration">
            <enum name="last-tunnel-drop">
              <value value="0"/>
              <description>
                <text>Last tunnel drop.</text>
              </description>
            </enum>
            <enum name="last-tunnel-update">
              <value value="1"/>
              <description>
                <text>Last tunnel update.</text>
              </description>
            </enum>
            <enum name="last-tunnel-create">
              <value value="2"/>
              <description>
                <text>Last tunnel create.</text>
              </description>
            </enum>
            <enum name="last-tunnel-teardown">
              <value value="3"/>
              <description>
