<?xml version="1.0" encoding="UTF-8"?>
<module name="huawei-routing-notification"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:rt-notification="urn:huawei:yang:huawei-routing-notification"
        xmlns:ext="urn:huawei:yang:huawei-extension">
  <yang-version value="1"/>
  <namespace uri="urn:huawei:yang:huawei-routing-notification"/>
  <prefix value="rt-notification"/>
  <import module="huawei-extension">
    <prefix value="ext"/>
  </import>
  <organization>
    <text>Huawei Technologies Co., Ltd.</text>
  </organization>
  <contact>
    <text>Huawei Industrial Base
Bantian, Longgang
Shenzhen 518129
People's Republic of China
Website: http://www.huawei.com
Email: support@huawei.com</text>
  </contact>
  <description>
    <text>Notification of route management.</text>
  </description>
  <revision date="2021-02-21">
    <description>
      <text>Initial revision.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <ext:task-name value="route-base"/>
  <notification name="public-ipv6-prefix-exceed">
    <description>
      <text>The number of public IPv6 prefixes exceeded the maximum value.</text>
    </description>
    <leaf name="prefix-count">
      <type name="uint32"/>
      <description>
        <text>Number of IPv6 public route prefixes in the routing table.</text>
      </description>
    </leaf>
    <leaf name="max-value">
      <type name="uint32"/>
      <description>
        <text>Maximum number of IPv6 public route prefixes that is allowed.</text>
      </description>
    </leaf>
  </notification>
  <notification name="public-ipv6-prefix-exceed-clear">
    <description>
      <text>The number of public IPv6 prefixes falls below the maximum value.</text>
    </description>
    <leaf name="prefix-count">
      <type name="uint32"/>
      <description>
        <text>Number of IPv6 public route prefixes in the routing table.</text>
      </description>
    </leaf>
    <leaf name="max-value">
      <type name="uint32"/>
      <description>
        <text>Maximum number of IPv6 public route prefixes that is allowed.</text>
      </description>
    </leaf>
  </notification>
  <notification name="vpn-ipv6-prefix-exceed">
    <description>
      <text>The number of IPv6 prefixes in the VPN instance exceeded the maximum value.</text>
    </description>
    <leaf name="vpn-name">
      <type name="string">
        <length value="1..31"/>
      </type>
      <description>
        <text>Name of a VPN instance.</text>
      </description>
    </leaf>
    <leaf name="prefix-count">
      <type name="uint32"/>
      <description>
        <text>Current number of IPv6 route prefixes in the VPN instance.</text>
      </description>
    </leaf>
    <leaf name="max-value">
      <type name="uint32"/>
      <description>
        <text>Maximum number of IPv6 route prefixes in the VPN instance.</text>
      </description>
    </leaf>
  </notification>
  <notification name="vpn-ipv6-prefix-exceed-clear">
    <description>
      <text>The number of IPv6 prefixes in the VPN instance fell below the maximum value.</text>
