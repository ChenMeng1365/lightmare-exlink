  <container name="configuration">
    <description>
      <text>Community moudle's configuration</text>
    </description>
    <container name="community">
      <description>
        <text>Add a community list entry</text>
      </description>
      <container name="standards">
        <description>
          <text>Community list number(standard)</text>
        </description>
        <presence value="standard"/>
        <list name="standard">
          <description>
            <text>Community list number(standard)</text>
          </description>
          <max-elements value="1"/>
          <key value="list-no action-type seq-no"/>
          <leaf name="list-no">
            <description>
              <text>Community list number(standard)</text>
            </description>
            <type name="uint16">
              <range value="1..99">
                <description>
                  <text>The range of value is from 1 to 99.</text>
                </description>
              </range>
            </type>
          </leaf>
          <leaf name="action-type">
            <description>
              <text>Deny:Specify community to deny;
Permit:Specify community to accept.</text>
            </description>
            <type name="enumeration">
              <enum name="deny">
                <value value="0"/>
                <description>
                  <text>Specify community to reject.</text>
                </description>
              </enum>
              <enum name="permit">
                <value value="1"/>
                <description>
                  <text>Specify community to accept.</text>
                </description>
              </enum>
            </type>
          </leaf>
          <leaf name="seq-no">
            <description>
              <text>Set community's sequence number</text>
            </description>
            <type name="uint32">
              <range value="1..4294967295">
                <description>
                  <text>The range of this value is from 1 to 4294967295.</text>
                </description>
              </range>
            </type>
          </leaf>
          <leaf name="whole-match">
            <description>
              <text>The whole community number should be matched.</text>
            </description>
            <type name="empty"/>
            <ncd:revise-date date="2020-09-23"/>
          </leaf>
          <choice name="number-choice">
            <description>
              <text>Set community number in different format(asn-nn,uint,enums etc).</text>
            </description>
            <mandatory value="true"/>
            <case name="any-case">
              <description>
                <text>Set community number with special value.</text>
              </description>
              <leaf name="any">
                <description>
                  <text>Any community number</text>
                </description>
                <type name="empty"/>
              </leaf>
            </case>
            <case name="number-case">
              <description>
                <text>Set community number in different format(asn-nn,uint,enums).</text>
              </description>
              <container name="asns">
                <description>
                  <text>Set community number in asn-nn format</text>
                </description>
                <list name="asn">
                  <description>
                    <text>Community number in aa:nn format(max quantity 10)</text>
                  </description>
                  <key value="value"/>
                  <leaf name="value">
                    <description>
                      <text>Community number in aa:nn format(max quantity 10)</text>
                    </description>
                    <type name="ncd:asn-nn">
                      <pattern value="((([0-9]{1,4})|([0-5]([0-9]{1,4}))|(6[0-4]([0-9]{1,3}))|(65[0-4]([0-9]{1,2}))|(655[0-2]([0-9]{1,1}))|(6553([0-5]{1,1}))):(([0-9]{1,4})|([0-5]([0-9]{1,4}))|(6[0-4]([0-9]{1,3}))|(65[0-4]([0-9]{1,2}))|(655[0-2]([0-9]{1,1}))|(6553([0-5]{1,1}))))"/>
                    </type>
                  </leaf>
                </list>
              </container>
              <container name="uints">
                <description>
                  <text>Set community number in uint format</text>
                </description>
                <list name="uint">
                  <description>
                    <text>Community number(max quantity 10)</text>
                  </description>
                  <key value="value"/>
                  <leaf name="value">
                    <description>
                      <text>Community number(max quantity 10)</text>
                    </description>
                    <type name="uint32">
                      <range value="1..4294967295">
                        <description>
                          <text>The range of value is from 1 to 4294967295.</text>
                        </description>
                      </range>
                    </type>
                  </leaf>
                </list>
              </container>
              <container name="enums">
                <description>
                  <text>Set community number in enum format</text>
                </description>
                <container name="intern">
                  <description>
                    <text>Internet(Well known community )</text>
                  </description>
                  <presence value="internet"/>
                  <leaf name="internet">
                    <description>
                      <text>Internet(Well known community )</text>
                    </description>
                    <type name="empty"/>
                    <mandatory value="true"/>
                  </leaf>
                </container>
                <container name="no-exp">
                  <description>
                    <text>Don't export to next AS</text>
                  </description>
                  <presence value="no-export"/>
                  <leaf name="no-export">
                    <description>
                      <text>Don't export to next AS</text>
                    </description>
                    <type name="empty"/>
                    <mandatory value="true"/>
                  </leaf>
                </container>
                <container name="no-adv">
                  <description>
                    <text>Don't advertise to any peer</text>
                  </description>
                  <presence value="no-advertise"/>
                  <leaf name="no-advertise">
                    <description>
                      <text>Don't advertise to any peer</text>
                    </description>
                    <type name="empty"/>
                    <mandatory value="true"/>
                  </leaf>
                </container>
                <container name="no-exp-sub">
                  <description>
                    <text>Don't send outside local AS</text>
                  </description>
                  <presence value="no-export-subconfed"/>
                  <leaf name="no-export-subconfed">
                    <description>
                      <text>Don't send outside local AS</text>
                    </description>
                    <type name="empty"/>
                    <mandatory value="true"/>
                  </leaf>
                </container>
              </container>
            </case>
          </choice>
        </list>
      </container>
