<?xml version="1.0" encoding="UTF-8"?>
<module name="zxr10-timerange"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:timerange="http://www.zte.com.cn/zxr10/netconf/schema/rosng/timerange"
        xmlns:ncd="http://www.zte.com.cn/zxr10/netconf/schema/rosng/netconf-defs">
  <namespace uri="http://www.zte.com.cn/zxr10/netconf/schema/rosng/timerange"/>
  <prefix value="timerange"/>
  <import module="zxr10-netconf-defs">
    <prefix value="ncd"/>
  </import>
  <organization>
    <text>ZTE Corporation</text>
  </organization>
  <contact>
    <text>ZTE Corporation  
   Nanjing Institute of ZTE Corporation 
   No.68 Zijinghua Rd. Yuhuatai District, Nanjing, China 
   Tel: +86-25-52870000</text>
  </contact>
  <description>
    <text>The netconf module to describe the TIMERANGE.</text>
  </description>
  <revision date="2015-07-28">
    <description>
      <text>Remove config attribute of all state nodes.</text>
    </description>
  </revision>
  <revision date="2015-01-19">
    <description>
      <text>Modify enum value of 'configuration/timerange/global-configuration/instances/instance/period-non-endday-rules/non-endday-rule/startday' from 0-9 to 0,8,9</text>
    </description>
  </revision>
  <revision date="2014-07-01">
    <description>
      <text>Initial version.</text>
    </description>
  </revision>
  <container name="configuration">
    <description>
      <text>The configuration model of TIMERANGE.</text>
    </description>
    <container name="timerange">
      <description>
        <text>The configuration model of TIMERANGE.</text>
      </description>
      <container name="global-configuration">
        <description>
          <text>The global configuration model of TIMERANGE.</text>
        </description>
        <container name="enable">
          <description>
            <text>To configure status of enable.</text>
          </description>
          <leaf name="status">
            <description>
              <text>Status of enable.</text>
            </description>
            <type name="enumeration">
              <enum name="off">
                <value value="0"/>
                <description>
                  <text>config disable status.</text>
                </description>
              </enum>
              <enum name="on">
                <value value="1"/>
                <description>
                  <text>config enable status.</text>
                </description>
              </enum>
            </type>
          </leaf>
        </container>
        <container name="instances">
          <description>
            <text>Instances of TIMERANGE.</text>
          </description>
          <list name="instance">
            <description>
              <text>Instance of TIMERANGE.</text>
            </description>
            <key value="name"/>
            <max-elements value="2048"/>
            <leaf name="name">
              <description>
                <text>Name of TIMERANGE instance.</text>
              </description>
              <type name="string">
                <length value="1..31"/>
              </type>
            </leaf>
            <container name="absolute-rule">
              <description>
                <text>To configure absolute time rule.</text>
              </description>
              <container name="startdate">
                <description>
                  <text>To configure start date.</text>
                </description>
                <presence value="Mandatory leafs are included."/>
                <leaf name="startday">
                  <description>
                    <text>To configure start day.</text>
                  </description>
                  <type name="ncd:date"/>
                  <mandatory value="true"/>
                </leaf>
                <leaf name="starttime">
                  <description>
                    <text>To configure start time.</text>
                  </description>
                  <type name="ncd:time"/>
                  <mandatory value="true"/>
                </leaf>
              </container>
              <container name="enddate">
                <description>
                  <text>To configure end date.</text>
                </description>
                <presence value="Mandatory leafs are included."/>
                <leaf name="endday">
                  <description>
                    <text>To configure end day.</text>
                  </description>
                  <type name="ncd:date"/>
                  <mandatory value="true"/>
                </leaf>
                <leaf name="endtime">
                  <description>
                    <text>To configure end time.</text>
                  </description>
                  <type name="ncd:time"/>
                  <mandatory value="true"/>
                </leaf>
              </container>
            </container>
            <container name="period-endday-exist-rules">
              <description>
                <text>To configure period rule which end day exists.</text>
              </description>
              <list name="endday-exist-rule">
                <description>
                  <text>To configure period rule which end day exists.</text>
                </description>
                <key value="startday daytime-begin endday daytime-end"/>
                <max-elements value="2048"/>
                <leaf name="startday">
                  <description>
                    <text>Day of start.</text>
                  </description>
                  <type name="enumeration">
                    <enum name="Monday">
                      <value value="1"/>
                    </enum>
                    <enum name="Tuesday">
                      <value value="2"/>
                    </enum>
                    <enum name="Wednesday">
                      <value value="3"/>
                    </enum>
                    <enum name="Thursday">
                      <value value="4"/>
                    </enum>
                    <enum name="Friday">
                      <value value="5"/>
                    </enum>
                    <enum name="Saturday">
                      <value value="6"/>
                    </enum>
                    <enum name="Sunday">
                      <value value="7"/>
                    </enum>
                  </type>
                </leaf>
                <leaf name="daytime-begin">
                  <description>
                    <text>Time of begin.</text>
                  </description>
                  <type name="ncd:time"/>
                </leaf>
                <leaf name="endday">
                  <description>
                    <text>Day of end.</text>
                  </description>
                  <type name="enumeration">
                    <enum name="Monday">
                      <value value="1"/>
                    </enum>
                    <enum name="Tuesday">
                      <value value="2"/>
                    </enum>
                    <enum name="Wednesday">
                      <value value="3"/>
                    </enum>
                    <enum name="Thursday">
                      <value value="4"/>
                    </enum>
                    <enum name="Friday">
                      <value value="5"/>
                    </enum>
                    <enum name="Saturday">
                      <value value="6"/>
                    </enum>
                    <enum name="Sunday">
                      <value value="7"/>
                    </enum>
                  </type>
                </leaf>
                <leaf name="daytime-end">
                  <description>
                    <text>Time of end.</text>
                  </description>
                  <type name="ncd:time"/>
                </leaf>
              </list>
            </container>
            <container name="period-non-endday-rules">
              <description>
                <text>To configure period rule which has no end day.</text>
              </description>
              <list name="non-endday-rule">
                <description>
                  <text>To configure period rule which has no end day.</text>
                </description>
                <key value="startday daytime-begin daytime-end"/>
                <max-elements value="2048"/>
                <leaf name="startday">
                  <description>
                    <text>Day of start.</text>
                  </description>
                  <type name="enumeration">
                    <enum name="Weekdays">
                      <value value="0"/>
                      <description>
                        <text>From Monday to Friday.</text>
                      </description>
                    </enum>
                    <enum name="Weekend">
                      <value value="8"/>
                      <description>
                        <text>Saturday and Sunday.</text>
                      </description>
                    </enum>
                    <enum name="Daily">
                      <value value="9"/>
                      <description>
                        <text>From Monday to Sunday.</text>
                      </description>
                    </enum>
                  </type>
                </leaf>
                <leaf name="daytime-begin">
                  <description>
                    <text>Time of begin.</text>
                  </description>
                  <type name="ncd:time"/>
                </leaf>
                <leaf name="daytime-end">
                  <description>
                    <text>Time of end.</text>
                  </description>
                  <type name="ncd:time"/>
                </leaf>
              </list>
            </container>
          </list>
        </container>
      </container>
    </container>
  </container>
  <container name="state">
    <description>
      <text>The state model of TIMERANGE.</text>
    </description>
    <config value="false"/>
    <container name="timerange">
      <description>
        <text>The state model of TIMERANGE.</text>
      </description>
      <list name="state-info">
        <description>
          <text>State of time range information.</text>
        </description>
        <key value="name"/>
        <max-elements value="2048"/>
        <leaf name="name">
          <description>
            <text>Name of time range.</text>
          </description>
          <type name="string">
            <length value="1..31"/>
          </type>
        </leaf>
        <leaf name="state">
          <description>
            <text>State of time range.</text>
          </description>
          <type name="enumeration">
            <enum name="inactive">
              <value value="0"/>
            </enum>
            <enum name="active">
              <value value="1"/>
            </enum>
          </type>
        </leaf>
      </list>
    </container>
  </container>
</module>
