  <typedef name="ls-route-type">
    <type name="enumeration">
      <enum name="node">
        <value value="1"/>
        <description>
          <text>NODE.</text>
        </description>
      </enum>
      <enum name="link">
        <value value="2"/>
        <description>
          <text>LINK.</text>
        </description>
      </enum>
