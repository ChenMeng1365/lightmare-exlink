  <feature name="permanent-attribution">
    <description>
      <text>Permanent route.</text>
    </description>
  </feature>
  <container name="configuration">
    <description>
      <text>IPv4 MSTATIC and IPv6 MSTATIC configuration.</text>
    </description>
    <container name="mstatics">
      <description>
        <text>Mstatics contain IPv4 MSTATIC for short MSTATIC and IPv6 MSTATIC.</text>
      </description>
      <presence value="mstatic"/>
      <container name="mstatic-v4">
        <description>
          <text>MSTATIC configuration.</text>
        </description>
        <presence value="mstatic-v4"/>
        <container name="public">
          <description>
            <text>Public mode of MSTATIC module configure node.</text>
          </description>
          <container name="global">
            <description>
              <text>MSTATIC global start configuration.</text>
            </description>
            <leaf name="start">
              <description>
                <text>MSTATIC start flag.</text>
              </description>
              <type name="empty"/>
            </leaf>
          </container>
          <container name="limit">
            <description>
              <text>Configure MSTATIC route limit.</text>
            </description>
            <presence value="limit"/>
            <leaf name="xg-limit">
              <description>
                <text>XG limit.</text>
              </description>
              <type name="int32">
                <range value="1..2048">
                  <description>
                    <text>The range from 1 to 2048,the max value gets from performance parameter.</text>
                  </description>
                </range>
              </type>
              <mandatory value="true"/>
            </leaf>
            <leaf name="sg-limit">
              <description>
                <text>SG limit.</text>
              </description>
              <type name="int32">
                <range value="1..2048">
                  <description>
                    <text>The range from 1 to 2048,the max value gets from performance parameter.</text>
                  </description>
                </range>
              </type>
              <mandatory value="true"/>
            </leaf>
          </container>
          <container name="oif-indexes">
            <description>
              <text>Configure MSTATIC oif list.</text>
            </description>
            <presence value="oif-indexes"/>
            <list name="oif-index">
              <description>
                <text>Configure MSTATIC oif instance.</text>
              </description>
              <key value="intf-name oif-index"/>
              <leaf name="intf-name">
                <description>
                  <text>Interface name.</text>
                </description>
                <type name="ncd:ifname">
                  <length value="1..31">
                    <description>
                      <text>The length for 1 to 31.</text>
                    </description>
                  </length>
                </type>
              </leaf>
              <leaf name="oif-index">
                <description>
                  <text>Downstream interface index.</text>
                </description>
                <type name="uint16">
                  <range value="1..100">
                    <description>
                      <text>The range from 1 to 100.</text>
                    </description>
                  </range>
                </type>
              </leaf>
            </list>
            <list name="oif-index-bier">
              <description>
                <text>Configure BFER index of MSTATIC route.</text>
              </description>
              <key value="oif-index bfer-index subdomain-id"/>
              <leaf name="oif-index">
                <description>
                  <text>Downstream forwording index.</text>
                </description>
                <type name="uint16">
                  <range value="1..100">
                    <description>
                      <text>The range from 1 to 100.</text>
                    </description>
                  </range>
                </type>
              </leaf>
              <leaf name="bfer-index">
                <description>
                  <text>BFER index.</text>
                </description>
                <type name="uint32">
                  <range value="1..100">
                    <description>
                      <text>The range from 1 to 100.</text>
                    </description>
                  </range>
                </type>
              </leaf>
              <leaf name="subdomain-id">
                <description>
                  <text>Subdomain ID.</text>
                </description>
                <type name="uint8">
                  <range value="0..255">
                    <description>
                      <text>The range from 0 to 255.</text>
                    </description>
                  </range>
                </type>
              </leaf>
              <leaf name="ipv6-bier">
                <description>
                  <text>BIER tunnel over IPv6.</text>
                </description>
                <type name="empty"/>
                <ncd:revise-date date="2020-06-20"/>
              </leaf>
              <ncd:revise-date date="2017-10-18"/>
            </list>
          </container>
          <container name="routes">
            <description>
              <text>Configure MSTATIC route.</text>
            </description>
            <presence value="routes"/>
            <list name="route">
              <description>
                <text>Configure MSTATIC route instance.</text>
              </description>
              <key value="group source"/>
              <leaf name="group">
                <description>
                  <text>Group address.</text>
                </description>
                <type name="ncd:ipv4-address"/>
              </leaf>
              <leaf name="source">
                <description>
                  <text>Source address.</text>
                </description>
                <type name="ncd:ipv4-address"/>
              </leaf>
              <leaf name="iif-name">
                <description>
                  <text>Upstream interface name.</text>
                </description>
                <type name="ncd:ifname">
                  <length value="1..31">
                    <description>
                      <text>The length for 1 to 31.</text>
                    </description>
                  </length>
                </type>
              </leaf>
              <leaf name="track-name">
                <description>
                  <text>Master upstream interface track name.</text>
                </description>
                <type name="string">
                  <length value="1..31"/>
                </type>
                <ncd:revise-date date="2015-08-28"/>
              </leaf>
              <leaf name="slave-iif-name">
                <description>
                  <text>Slave upstream interface name.</text>
                </description>
                <type name="ncd:ifname">
                  <length value="1..31"/>
                </type>
                <ncd:revise-date date="2015-08-28"/>
              </leaf>
              <leaf name="slave-track-name">
                <description>
                  <text>Slave upstream interface track name.</text>
                </description>
                <type name="string">
                  <length value="1..31"/>
                </type>
                <ncd:revise-date date="2015-08-28"/>
              </leaf>
              <leaf name="permanent">
                <description>
                  <text>Permanent route.</text>
                </description>
                <type name="empty"/>
                <ncd:revise-date date="2015-08-28"/>
              </leaf>
              <leaf name="oif-index">
                <description>
                  <text>Downstream interface index.</text>
                </description>
                <type name="uint16">
                  <range value="1..100">
                    <description>
                      <text>The range from 1 to 100.</text>
                    </description>
                  </range>
                </type>
              </leaf>
              <leaf name="inc-mask">
                <description>
                  <text>Increment mask of MSTATIC group address.</text>
                </description>
                <type name="ncd:ipv4-address">
                  <pattern value="(([1-9]|[1-9][0-9]|[1][0-9][0-9]|[2][0-4][0-9]|[2][5][0-5])\.([0-9]{1,3})\.([0-9]{1,3})\.([0-9]{1,3}))|(0\.([1-9]|[1-9][0-9]|[1][0-9][0-9]|[2][0-4][0-9]|[2][5][0-5])\.([0-9]{1,3})\.([0-9]{1,3}))|((0\.){2}([1-9]|[1-9][0-9]|[1][0-9][0-9]|[2][0-4][0-9]|[2][5][0-5])\.([0-9]{1,3}))|((0\.){3}([1-9]|[1-9][0-9]|[1][0-9][0-9]|[2][0-4][0-9]|[2][5][0-5]))"/>
                </type>
                <ncd:revise-date date="2015-12-18"/>
              </leaf>
              <leaf name="count">
                <description>
                  <text>The number of MSTATIC group addresses.</text>
                </description>
                <type name="uint32">
                  <range value="1..512"/>
                </type>
                <ncd:revise-date date="2015-12-18"/>
              </leaf>
            </list>
          </container>
          <container name="frr">
            <description>
              <text>Configure MSTATIC fast reroute.</text>
            </description>
            <presence value="frr"/>
            <leaf name="wtr">
              <description>
                <text>Time interval when slave interface change back.</text>
              </description>
              <type name="uint8">
                <range value="1..12"/>
              </type>
              <units name="minute"/>
              <mandatory value="true"/>
            </leaf>
            <ncd:revise-date date="2015-08-28"/>
          </container>
          <ncd:revise-date date="2015-07-02"/>
        </container>
        <container name="private">
          <description>
            <text>Private mode of MSTATIC module configure root node.</text>
          </description>
          <list name="vrf-mstatic">
            <description>
              <text>Private mode of MSTATIC module configure root node.</text>
            </description>
            <key value="vrf-name"/>
            <leaf name="vrf-name">
              <description>
                <text>VRF name.</text>
              </description>
              <type name="string">
                <length value="1..32"/>
              </type>
            </leaf>
            <container name="oif-indexes">
              <description>
                <text>Configure MSTATIC downstream interface.</text>
              </description>
              <list name="oif-index">
                <description>
                  <text>Configure MSTATIC downstream interface.</text>
                </description>
                <key value="intf-name index"/>
                <leaf name="intf-name">
                  <description>
                    <text>Interface name.</text>
                  </description>
                  <type name="ncd:ifname">
                    <length value="1..31">
                      <description>
                        <text>The length for 1 to 31.</text>
                      </description>
                    </length>
                  </type>
                </leaf>
                <leaf name="index">
                  <description>
                    <text>Downstream interface index.</text>
                  </description>
                  <type name="uint16">
                    <range value="1..100">
                      <description>
                        <text>The range from 1 to 100.</text>
                      </description>
                    </range>
                  </type>
                </leaf>
              </list>
            </container>
            <container name="routes">
              <description>
                <text>Configure MSTATIC route.</text>
              </description>
              <list name="route">
                <description>
                  <text>Configure MSTATIC route.</text>
                </description>
                <key value="group source"/>
                <leaf name="group">
                  <description>
                    <text>Group address.</text>
                  </description>
                  <type name="ncd:ipv4-address"/>
                </leaf>
                <leaf name="source">
                  <description>
                    <text>Source address.</text>
                  </description>
                  <type name="ncd:ipv4-address"/>
                </leaf>
                <leaf name="iif-name">
                  <description>
                    <text>Upstream interface name.</text>
                  </description>
                  <type name="ncd:ifname">
                    <length value="1..31">
                      <description>
                        <text>The length for 1 to 31.</text>
                      </description>
                    </length>
                  </type>
                </leaf>
                <leaf name="track-name">
                  <description>
                    <text>Master upstream interface track name.</text>
                  </description>
                  <type name="string">
                    <length value="1..31"/>
                  </type>
                  <ncd:revise-date date="2015-08-28"/>
                </leaf>
                <leaf name="slave-iif-name">
                  <description>
