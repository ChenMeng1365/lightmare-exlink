            </type>
            <must condition="/qos:qos/qos:queue-scheduler-template/qos:user-group-queues/qos:user-group-queue[qos:name=current()]/qos:mode='normal'"/>
            <mandatory value="true"/>
            <description>
              <text>Group queue. This node does not support modification or deletion.</text>
            </description>
            <ext:operation-exclude value="update">
              <description>
                <text>After the configuration is created, the node cannot be modified.</text>
              </description>
            </ext:operation-exclude>
          </leaf>
          <leaf name="group-name">
            <type name="template-name"/>
            <description>
              <text>The name of user queue group. This node does not support modification or deletion.</text>
            </description>
            <ext:operation-exclude value="update|delete">
              <description>
                <text>After the configuration is created, the node cannot be modified.</text>
              </description>
            </ext:operation-exclude>
          </leaf>
        </list>
      </container>
      <container name="bras-member-link-schedulers">
        <when condition="../../ifm:type='Eth-Trunk'"/>
        <description>
          <text>List of configure weight based bandwidth allocation for member interfaces of a trunk.</text>
        </description>
        <list name="bras-member-link-scheduler">
          <must condition="(user-access-type='subscriber-access' and direction='inbound') or user-access-type='leased-line-access'"/>
          <key value="user-access-type direction"/>
          <description>
            <text>Configure weight based bandwidth allocation for member interfaces of a trunk.</text>
          </description>
          <leaf name="user-access-type">
            <type name="user-access-type"/>
            <description>
              <text>User access type.</text>
            </description>
          </leaf>
          <leaf name="direction">
            <type name="qos-direction-type"/>
            <description>
              <text>Direction.</text>
            </description>
          </leaf>
        </list>
      </container>
      <container name="schedule-tree-distribute-modes">
        <when condition="(../../ifm:type='Eth-Trunk' and ../../ifm:class='main-interface') or (../../ifm:type='Ip-Trunk' and ../../ifm:class='main-interface')"/>
        <description>
          <text>List of configure QoS schedule tree distribute mode outbound.</text>
        </description>
        <list name="schedule-tree-distribute-mode">
          <key value="direction"/>
          <description>
            <text>Configure QoS schedule tree distribute mode outbound.</text>
          </description>
          <leaf name="direction">
            <type name="out-direction-type"/>
            <description>
              <text>Direction.</text>
            </description>
          </leaf>
        </list>
      </container>
      <container name="loop-mode">
        <when condition="../../ifm:type='Virtual-Ethernet'"/>
        <presence value="create loop-mode"/>
        <description>
          <text>Enable/disable QoS loop mode.</text>
        </description>
      </container>
      <container name="resource-pools">
        <description>
          <text>List of configure QoS queue resource.</text>
        </description>
        <list name="resource-pool">
          <key value="direction"/>
          <description>
            <text>Configure QoS queue resource.</text>
          </description>
          <leaf name="direction">
            <type name="qos-direction-type"/>
            <description>
              <text>Direction.</text>
            </description>
          </leaf>
          <leaf name="pool-id">
            <type name="pool-type"/>
            <mandatory value="true"/>
            <description>
              <text>Pool-id.</text>
            </description>
          </leaf>
        </list>
      </container>
      <container name="user-group-queue-applys">
        <description>
          <text>List of configure user group queue application.</text>
        </description>
        <list name="user-group-queue-apply">
          <key value="direction"/>
          <max-elements value="2"/>
          <description>
            <text>Configure user group queue application.</text>
          </description>
          <leaf name="direction">
            <type name="qos-direction-type"/>
            <description>
              <text>Group queue direction.</text>
            </description>
          </leaf>
          <leaf name="group-queue-name">
            <type name="leafref">
              <path value="/qos:qos/qos:queue-scheduler-template/qos:user-group-queues/qos:user-group-queue/qos:name"/>
            </type>
            <must condition="/qos:qos/qos:queue-scheduler-template/qos:user-group-queues/qos:user-group-queue[qos:name=current()]/qos:mode='normal'"/>
            <mandatory value="true"/>
            <description>
              <text>Group queue name.</text>
            </description>
            <ext:operation-exclude value="update">
              <description>
                <text>After the configuration is created, the node cannot be modified.</text>
              </description>
            </ext:operation-exclude>
          </leaf>
          <leaf name="group-name">
            <type name="string">
              <length value="1..31"/>
            </type>
            <description>
              <text>Group-name.</text>
            </description>
            <ext:operation-exclude value="create|update|delete">
              <description>
                <text>After the configuration is created, the node cannot be modified.</text>
              </description>
            </ext:operation-exclude>
          </leaf>
        </list>
      </container>
      <container name="mpls-l2vc">
        <description>
          <text>Configure MPLS l2vc.</text>
        </description>
        <leaf name="diffserv-domain-name">
          <type name="leafref">
            <path value="/qos:qos/qos:classifier-template/qos:diffserv-domains/qos:diffserv-domain/qos:name"/>
          </type>
          <description>
            <text>Name of a diffserv-domain.</text>
          </description>
        </leaf>
      </container>
      <container name="member-link-schedulers">
        <when condition="(../../ifm:type='Eth-Trunk' and ../../ifm:class='main-interface') or (../../ifm:type='Ip-Trunk' and ../../ifm:class='main-interface')"/>
        <description>
          <text>List of configure trunk flow split.</text>
        </description>
        <list name="member-link-scheduler">
          <key value="member-link-scheduler-type"/>
          <description>
            <text>Configure trunk flow split.</text>
          </description>
          <leaf name="member-link-scheduler-type">
            <type name="member-link-scheduler-type"/>
            <description>
              <text>Business type.</text>
            </description>
          </leaf>
          <leaf name="direction">
            <when condition="../member-link-scheduler-type='user-queue'"/>
            <type name="out-direction-type"/>
            <description>
              <text>Direction.</text>
            </description>
          </leaf>
        </list>
      </container>
      <container name="qos-lr">
        <description>
          <text>Configure limit on the rate of traffic on an interface.</text>
        </description>
        <choice name="cir-parameters">
          <description>
            <text>Specify cir-parameters.</text>
          </description>
          <case name="cir">
            <description>
              <text>Specify cir parameters choice case as cir value.</text>
            </description>
            <leaf name="cir-value">
              <type name="uint32">
                <range value="1..4294967294"/>
              </type>
              <units name="Mbit/s"/>
              <description>
                <text>Committed information rate.</text>
              </description>
            </leaf>
          </case>
          <case name="cir-percent">
            <description>
              <text>Specify cir parameters choice case as cir percentage value.</text>
            </description>
            <leaf name="cir-percent-value">
              <type name="uint8">
                <range value="1..100"/>
              </type>
              <units name="%"/>
              <description>
                <text>CIR percentage.</text>
              </description>
            </leaf>
          </case>
        </choice>
      </container>
      <container name="ldp-traffic">
        <description>
          <text>Configure ldp traffic.</text>
        </description>
        <leaf name="traffic-type">
          <type name="traffic-type"/>
          <description>
            <text>Traffic of type.</text>
          </description>
        </leaf>
      </container>
      <container name="buffer-policy-mode">
        <description>
          <text>Configure buffer policy buffer mode.</text>
        </description>
        <leaf name="buffer-mode">
          <type name="buffer-mode-type"/>
          <description>
            <text>Buffer mode.</text>
          </description>
        </leaf>
      </container>
      <container name="hard-pipe-apply">
        <presence value="Create hard pipe."/>
        <description>
          <text>Enable/disable hard pipe apply.</text>
        </description>
        <leaf name="mode">
          <type name="hard-pipe-mode"/>
          <mandatory value="true"/>
          <description>
            <text>Hard pipe mode.</text>
          </description>
        </leaf>
        <leaf name="bandwidth">
          <type name="uint32">
            <range value="1..4294967294"/>
          </type>
          <units name="Mbit/s"/>
          <mandatory value="true"/>
          <description>
            <text>Hard pipe bandwidth.</text>
          </description>
        </leaf>
      </container>
      <container name="channel-adjustment">
        <when condition="../../ifm:class='main-interface'"/>
        <presence value="create channel-adjustment"/>
        <description>
          <text>Enable/disable channel network header length adjustment.</text>
        </description>
        <leaf name="adjust-value">
          <type name="uint16">
            <range value="1..63"/>
          </type>
          <mandatory value="true"/>
          <description>
            <text>Adjustment value.</text>
          </description>
        </leaf>
      </container>
      <container name="user-vlan-profile-applys">
        <description>
          <text>List of configure QoS profile on user VLAN.</text>
        </description>
        <list name="user-vlan-profile-apply">
          <must condition="./pe-end-vlan-id&gt;=./pe-begin-vlan-id"/>
          <must condition="./ce-end-vlan-id&gt;=./ce-begin-vlan-id"/>
          <key value="direction pe-begin-vlan-id pe-end-vlan-id ce-begin-vlan-id ce-end-vlan-id"/>
          <description>
            <text>Configure QoS profile on user VLAN.</text>
          </description>
          <leaf name="direction">
            <type name="qos-direction-type"/>
            <description>
              <text>Direction.</text>
            </description>
          </leaf>
          <leaf name="pe-begin-vlan-id">
            <type name="uint16">
              <range value="0..4094"/>
            </type>
            <description>
              <text>Pe begin VLAN id.</text>
            </description>
          </leaf>
          <leaf name="pe-end-vlan-id">
            <type name="uint16">
              <range value="0..4094"/>
            </type>
            <description>
              <text>Pe end VLAN id.</text>
            </description>
          </leaf>
          <leaf name="ce-begin-vlan-id">
            <type name="uint16">
              <range value="1..4094"/>
            </type>
            <description>
              <text>Ce begin VLAN id.</text>
            </description>
          </leaf>
          <leaf name="ce-end-vlan-id">
            <type name="uint16">
              <range value="1..4094"/>
            </type>
            <description>
              <text>Ce end VLAN id.</text>
            </description>
          </leaf>
          <leaf name="profile-name">
            <type name="leafref">
              <path value="/qos:qos/qos:queue-scheduler-template/qos:profiles/qos:profile/qos:name"/>
            </type>
            <must condition="count(/qos:qos/qos:queue-scheduler-template/qos:profiles/qos:profile[qos:name=current()]/qos:cars/qos:car)&gt;=1"/>
            <mandatory value="true"/>
            <description>
              <text>Qos profile name.</text>
            </description>
            <ext:operation-exclude value="update">
              <description>
                <text>After the configuration is created, the node cannot be modified.</text>
              </description>
            </ext:operation-exclude>
          </leaf>
          <leaf name="each-vlan">
            <type name="yes-or-no"/>
            <mandatory value="true"/>
            <description>
              <text>Qos profile takes effect on the traffic of each VLAN separately or not.</text>
            </description>
            <ext:operation-exclude value="update">
              <description>
                <text>After the configuration is created, the node cannot be modified.</text>
              </description>
            </ext:operation-exclude>
          </leaf>
        </list>
      </container>
      <container name="service-template-shaping">
        <description>
          <text>Configure service template shaping.</text>
        </description>
        <container name="all-service-class">
          <description>
            <text>Configure shaping for all service class with service template.</text>
          </description>
          <leaf name="service-template-name">
            <type name="leafref">
              <path value="/qos:qos/qos:queue-scheduler-template/qos:service-templates/qos:service-template/qos:name"/>
            </type>
            <description>
              <text>Service template.</text>
            </description>
          </leaf>
        </container>
        <container name="specific-service-classs">
          <description>
            <text>List of configure shapings based on class queue with service template.</text>
          </description>
          <list name="specific-service-class">
            <key value="service-class"/>
            <description>
              <text>Configure shaping based on class queue with service template.</text>
            </description>
            <leaf name="service-class">
              <type name="qos-service-class"/>
              <description>
                <text>Class of service, some boards do not support this object.</text>
              </description>
            </leaf>
            <leaf name="service-template-name">
              <type name="leafref">
                <path value="/qos:qos/qos:queue-scheduler-template/qos:service-templates/qos:service-template/qos:name"/>
              </type>
              <mandatory value="true"/>
              <description>
                <text>Service template.</text>
              </description>
            </leaf>
          </list>
        </container>
      </container>
      <container name="default-service-class-remote">
        <description>
          <text>Configure the upstream traffic to enter the specified queue on the remote AP interface of the master device.</text>
        </description>
        <leaf name="service-class">
          <type name="qos-service-class"/>
          <description>
            <text>Service class.</text>
          </description>
        </leaf>
      </container>
      <container name="trust-remotes">
        <description>
          <text>List of configure BA classification on the interface of the master device and configure the port extension interface to deliver configurations to AP devices.</text>
        </description>
        <list name="trust-remote">
          <must condition="(./vlan-mode='false' and ./begin-vlan=0 and ./end-vlan=0) or (./vlan-mode='true' and ./begin-vlan&gt;0)"/>
          <must condition="./begin-vlan &lt;= ./end-vlan"/>
          <key value="type direction vlan-mode begin-vlan end-vlan"/>
          <description>
            <text>Configure BA classification on the interface of the master device and configure the port extension interface to deliver configurations to AP devices.</text>
          </description>
          <leaf name="type">
            <type name="remote-trust-type"/>
            <description>
              <text>Trust IP dscp or 8021p.</text>
            </description>
          </leaf>
          <leaf name="direction">
            <type name="trust-remote-direction-type"/>
            <description>
              <text>Direction.</text>
            </description>
          </leaf>
          <leaf name="vlan-mode">
            <type name="boolean"/>
            <description>
              <text>Enable/disable VLAN mode.</text>
            </description>
          </leaf>
          <leaf name="begin-vlan">
            <type name="uint16">
              <range value="0..4094"/>
            </type>
            <description>
              <text>Begin VLAN ID.</text>
            </description>
          </leaf>
          <leaf name="end-vlan">
            <type name="uint16">
              <range value="0..4094"/>
            </type>
            <description>
              <text>End VLAN ID.</text>
            </description>
          </leaf>
          <leaf name="diffserv-domain-name">
            <when condition="../type='ip-dscp'"/>
            <type name="leafref">
              <path value="/qos:qos/qos:classifier-template/qos:diffserv-domains/qos:diffserv-domain/qos:name"/>
            </type>
            <mandatory value="true"/>
            <description>
              <text>Name of a diffserv domain.</text>
            </description>
          </leaf>
          <leaf name="type-8021p">
            <when condition="../type='8021p'"/>
            <type name="type-8021p"/>
            <mandatory value="true"/>
            <description>
              <text>Trust 8021p, inner 8021p or outer 8021p.</text>
            </description>
          </leaf>
        </list>
      </container>
      <container name="wfq-shaping">
        <description>
          <text>Configure wfq shaping.</text>
        </description>
        <choice name="shaping-parameters">
          <description>
            <text>Specify shaping parameters.</text>
          </description>
          <case name="shaping">
            <description>
              <text>Specify shaping parameters choice case as shaping value.</text>
            </description>
            <leaf name="shaping-value">
              <type name="uint32">
                <range value="0..4294967294"/>
              </type>
              <units name="Mbit/s"/>
              <description>
                <text>Rate of shaping.</text>
              </description>
            </leaf>
          </case>
          <case name="shaping-percent">
            <description>
              <text>Specify shaping parameters choice case as shaping percentage value.</text>
            </description>
            <leaf name="shaping-percentage">
              <type name="uint8">
                <range value="0..100"/>
              </type>
              <units name="%"/>
              <description>
                <text>Percentage of shaping.</text>
              </description>
            </leaf>
          </case>
        </choice>
      </container>
      <container name="source-board-schedule-weights">
        <description>
          <text>List of configure weight based scheduling for a source board.</text>
        </description>
        <list name="source-board-schedule-weight">
          <key value="slot-id"/>
          <description>
            <text>Configure weight based scheduling for a source board.</text>
          </description>
          <leaf name="slot-id">
            <type name="slot-id"/>
            <description>
              <text>Slot ID.</text>
            </description>
          </leaf>
          <leaf name="weight-value">
            <type name="uint8">
              <range value="1..100"/>
            </type>
            <mandatory value="true"/>
            <description>
              <text>Weight value.</text>
            </description>
          </leaf>
        </list>
      </container>
      <container name="deficit-unit">
        <presence value="Create deficit unit."/>
        <description>
          <text>Enable/disable the chip scheduling granularity.</text>
        </description>
        <leaf name="unit">
          <type name="deficit-unit-type"/>
          <mandatory value="true"/>
          <description>
            <text>Unit.</text>
          </description>
        </leaf>
      </container>
      <container name="default-user-queue">
        <must condition="not(not(./cir) and not(./pir) and not(./cbs) and not(./pbs) and not(./weight))"/>
        <presence value="Create default user queue."/>
        <description>
          <text>Enable/disable default user queue. This object must contain at least one parameter in cir/cbs/pir/pbs/weight.</text>
        </description>
        <leaf name="direction">
          <type name="out-direction-type"/>
          <mandatory value="true"/>
          <description>
            <text>Direction.</text>
          </description>
        </leaf>
        <leaf name="cir">
          <type name="uint32">
            <range value="0 | 16..4294967294"/>
          </type>
          <units name="kbit/s"/>
          <description>
            <text>Cir.</text>
          </description>
        </leaf>
        <leaf name="cbs">
          <type name="uint32">
            <range value="1..4194304"/>
          </type>
          <units name="Byte"/>
          <description>
            <text>Cbs.</text>
          </description>
        </leaf>
        <leaf name="pir">
          <type name="uint32">
            <range value="0 | 16..4294967294"/>
          </type>
          <units name="kbit/s"/>
          <must condition="(not(../cir) and ../pir&gt;=10000) or (../cir and ../pir&gt;=../cir)"/>
          <description>
            <text>Pir.</text>
          </description>
        </leaf>
        <leaf name="pbs">
          <type name="uint32">
            <range value="1..4194304"/>
          </type>
          <units name="Byte"/>
          <must condition="not(../cbs) or (../cbs and ../pbs&gt;=../cbs)"/>
          <description>
            <text>Pbs.</text>
          </description>
        </leaf>
        <leaf name="weight">
          <type name="uint8">
            <range value="1..63"/>
          </type>
          <description>
            <text>Weight.</text>
          </description>
        </leaf>
      </container>
      <container name="vni-cfg-peers">
        <when condition="../../ifm:type='Nve'"/>
        <description>
          <text>List of configure profile peers.</text>
        </description>
        <list name="vni-cfg-peer">
          <must condition="./source-ip !=peer-ip"/>
          <key value="direction vni-id source-ip peer-ip"/>
          <description>
            <text>Configure profile peers.</text>
          </description>
          <leaf name="direction">
            <type name="qos-direction-type"/>
            <description>
              <text>Direction.</text>
            </description>
          </leaf>
          <leaf name="vni-id">
            <type name="uint32">
              <range value="1..16777215"/>
            </type>
            <description>
              <text>Vni ID.</text>
            </description>
          </leaf>
          <leaf name="source-ip">
            <type name="inet:ipv4-address-no-zone"/>
            <description>
              <text>Source IP.</text>
            </description>
          </leaf>
          <leaf name="peer-ip">
            <type name="inet:ipv4-address-no-zone"/>
            <description>
              <text>Peer IP.</text>
            </description>
          </leaf>
          <leaf name="profile-name">
            <type name="leafref">
              <path value="/qos:qos/qos:queue-scheduler-template/qos:profiles/qos:profile/qos:name"/>
            </type>
            <must condition="/qos:qos/qos:queue-scheduler-template/qos:profiles/qos:profile[qos:name=current()]/qos:mode='normal'"/>
            <mandatory value="true"/>
            <description>
              <text>Profile Name.</text>
            </description>
            <ext:operation-exclude value="update"/>
          </leaf>
        </list>
      </container>
    </container>
  </augment>
  <augment target-node="/ni:network-instance/ni:instances/ni:instance">
    <description>
      <text>Adds qos configuration to the instance.</text>
    </description>
    <container name="qos">
      <description>
        <text>Configure QoS apply.</text>
      </description>
      <container name="traffic-policy-vxlans">
        <when condition="not(../../ni:name='_public_')"/>
        <description>
          <text>List of configure application of a traffic policy.</text>
        </description>
        <list name="traffic-policy-vxlan">
          <key value="direction"/>
          <max-elements value="2"/>
          <description>
            <text>Configure application of a traffic policy.</text>
          </description>
          <leaf name="direction">
            <type name="qos-direction-type"/>
            <description>
              <text>Direction.</text>
            </description>
          </leaf>
          <leaf name="policy-name">
            <type name="leafref">
              <path value="/qos:qos/qos:classifier-template/qos:traffic-policys/qos:traffic-policy/qos:name"/>
            </type>
            <mandatory value="true"/>
            <description>
              <text>Policy name.</text>
            </description>
          </leaf>
          <leaf name="layer">
            <when condition="../direction='inbound'"/>
            <type name="policy-apply-vxlan-layer-type"/>
            <description>
              <text>Classification based on Layer 2 or Layer 3 information.</text>
            </description>
          </leaf>
        </list>
      </container>
    </container>
  </augment>
  <augment target-node="/lcs:license/lcs:license-items/lcs:license-item">
    <description>
      <text>Adds qos configuration to license model.</text>
    </description>
    <container name="port-hqoss">
      <when condition="../lcs:name='LCR5S40NHQS0P' or ../lcs:name='LCR5S40XHQS0P' or ../lcs:name='LCR5S40GHQS0P' or ../lcs:name='LCR5S40VHQS0P' or ../lcs:name='LCR5S40YHQS0P' or ../lcs:name='LTNRS69GHQS0P' or ../lcs:name='LTNRS69NHQS0P' or ../lcs:name='LTNRS69VHQS0P' or ../lcs:name='LTNRS69YHQS0P' or ../lcs:name='LTNRS69XHQS0P' or ../lcs:name='LCX6S00HQSN0P' or ../lcs:name='LCX6S00HQSX0P' or ../lcs:name='LCX6S00HQSV0P'"/>
      <description>
        <text>List of configure port hqos license.</text>
      </description>
      <list name="port-hqos">
        <key value="position"/>
        <description>
          <text>Configure port hqos license.</text>
        </description>
        <leaf name="position">
          <type name="string">
            <length value="1..32"/>
          </type>
          <description>
            <text>Port position.</text>
          </description>
        </leaf>
        <leaf name="active-status">
          <type name="lcs-active-status-type"/>
          <config value="false"/>
          <description>
            <text>Activated State.</text>
          </description>
        </leaf>
      </list>
    </container>
  </augment>
</module>
