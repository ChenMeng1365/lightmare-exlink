  <feature name="public-dest-address">
    <description>
      <text>The anycast-mac-address configuration is used to configure nexthop to be public.</text>
    </description>
  </feature>
  <feature name="ipv4-global-nh">
    <description>
      <text>This configuration is used to configure IPv4 global nexthop for IPv6 static route.</text>
    </description>
  </feature>
  <feature name="ipv6-permanent">
    <description>
      <text>This configuration is used to specify permanent IPv6 static route.</text>
    </description>
  </feature>
  <feature name="ipv6-global-nh">
    <reference>
      <text>This configuration is used to configure IPv6 global nexthop for IPv6 static route.</text>
    </reference>
  </feature>
  <feature name="ipv6-vxlan">
    <description>
      <text>The feature is for IPv6 vxlan route.</text>
    </description>
  </feature>
  <feature name="ipv4-dest-with-ipv6-global-nh">
    <description>
