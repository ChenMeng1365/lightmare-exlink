  <augment target-node="/ifm:ifm/ifm:interfaces/ifm:interface/bras-basic-access:bras">
    <description>
      <text>Configure DHCPv6 access on inferface.</text>
    </description>
    <container name="dhcpv6-access">
      <description>
        <text>Configure DHCPv6 access.</text>
      </description>
      <container name="options">
        <when condition="../../../ifm:type='Ethernet' or&#10;../../../ifm:type='GigabitEthernet' or&#10;../../../ifm:type='Eth-Trunk' or&#10;../../../ifm:type='100GE' or&#10;../../../ifm:type='10GE' or&#10;../../../ifm:type='Virtual-Ethernet' or&#10;../../../ifm:type='Global-VE' or&#10;../../../ifm:type='10x10GE' or&#10;../../../ifm:type='4x25GE' or&#10;../../../ifm:type='25GE' or&#10;../../../ifm:type='XGigabitEthernet' or&#10;../../../ifm:type='FlexE' or&#10;../../../ifm:type='PW-VE'"/>
        <description>
          <text>List of options insert to the relay-forward packet sent to the DHCPv6 server on the interfaces.</text>
        </description>
        <list name="option">
          <key value="code"/>
          <description>
            <text>Configure options insert to the relay-forward packet sent to the DHCPv6 server on the interfaces.</text>
          </description>
          <leaf name="code">
            <type name="uint16">
              <range value="79"/>
            </type>
            <description>
              <text>Dhcpv6 option code.</text>
            </description>
          </leaf>
        </list>
      </container>
      <container name="relay-option">
        <when condition="../../../ifm:type='Ethernet' or&#10;../../../ifm:type='GigabitEthernet' or&#10;../../../ifm:type='Eth-Trunk' or&#10;../../../ifm:type='100GE' or&#10;../../../ifm:type='10GE' or&#10;../../../ifm:type='Virtual-Ethernet' or&#10;../../../ifm:type='Global-VE' or&#10;../../../ifm:type='10x10GE' or&#10;../../../ifm:type='4x25GE' or&#10;../../../ifm:type='25GE' or&#10;../../../ifm:type='XGigabitEthernet' or&#10;../../../ifm:type='FlexE' or&#10;../../../ifm:type='PW-VE'"/>
        <description>
          <text>Configure options insert to the relay-forward packet sent to the DHCPv6 server on the interfaces.</text>
        </description>
        <leaf name="remote-id-type">
          <type name="enumeration">
            <enum name="neba">
              <value value="1"/>
              <description>
                <text>Specifies the remote-id padding to string NEBA.</text>
              </description>
            </enum>
            <enum name="vula">
              <value value="2"/>
              <description>
                <text>Specifies the remote-id padding to string VULA.</text>
              </description>
            </enum>
          </type>
          <description>
            <text>Specifies the remote-id padding to string NEBA or VULA.</text>
          </description>
        </leaf>
      </container>
    </container>
  </augment>
