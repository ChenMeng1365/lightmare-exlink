     |  +--ro atr-clk-class?     uint32
     |  +--ro atr-priority1?     uint32
     |  +--ro atr-priority2?     uint32
     +--rw ptp-bits-srcs
     |  +--rw ptp-bits-src* [bits-name]
     |     +--rw bits-name             string
     |     +--rw bits-switch?          boolean
     |     +--rw bits-signal?          clock-bits-type
     |     +--rw bits-direction?       ptp-bits-direction
     |     +--ro bits-status?          ptp-bits-status
