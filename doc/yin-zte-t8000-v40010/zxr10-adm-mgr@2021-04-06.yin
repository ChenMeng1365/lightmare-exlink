  <feature name="local-user-offline"/>
  <feature name="password-duration"/>
  <container name="configuration">
    <description>
      <text>The configuration model of ADM_MGR.</text>
    </description>
    <container name="adm-mgr">
      <description>
        <text>The configuration model of ADM_MGR.</text>
      </description>
      <container name="global-configuration">
        <description>
          <text>The global configuration of ADM_MGR.</text>
        </description>
        <container name="account-switch">
          <description>
            <text>Account switch.</text>
          </description>
          <choice name="switch">
            <description>
              <text>To configure the command account swith.</text>
            </description>
            <default value="switch-off"/>
            <case name="switch-on">
              <description>
                <text>To open command account function.</text>
              </description>
              <leaf name="on">
                <description>
                  <text>To open account function.</text>
                </description>
                <type name="empty"/>
                <mandatory value="true"/>
              </leaf>
              <leaf name="accounting-template">
                <description>
                  <text>To bind AAA account template</text>
                </description>
                <type name="uint16">
                  <range value="2001..2128"/>
                </type>
                <mandatory value="true"/>
              </leaf>
            </case>
            <case name="switch-off">
              <description>
                <text>To close command account function.</text>
              </description>
              <leaf name="off">
                <description>
                  <text>To close account function.</text>
                </description>
                <type name="empty"/>
              </leaf>
            </case>
          </choice>
        </container>
        <container name="login-ascii">
          <description>
            <text>To configure ASCII authentication type.</text>
          </description>
          <presence value="Mandatory leaves are included."/>
          <leaf name="ascii">
            <description>
              <text>To configure ASCII authentication type.</text>
            </description>
            <type name="empty"/>
            <mandatory value="true"/>
          </leaf>
          <leaf name="authentication-template">
            <description>
              <text>To configure the global authentication template number.</text>
            </description>
            <type name="uint8">
              <range value="1..128"/>
            </type>
            <mandatory value="true"/>
          </leaf>
          <leaf name="authortication-template">
            <description>
              <text>To configure the global authorization template number.</text>
            </description>
            <type name="uint8">
