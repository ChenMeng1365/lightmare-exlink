  <ext:task-name value="clksync"/>
  <typedef name="ssm-recovery-mode">
    <type name="enumeration">
      <enum name="revertive">
        <value value="0"/>
        <description>
          <text>Set the SSM selection mode to revertive mode to allow the system to always select the best physical clock source.</text>
        </description>
      </enum>
      <enum name="non-revertive">
        <value value="1"/>
        <description>
          <text>Set the SSM mode of the physical clock to non-revertive mode to allow the system to select a physical clock source lower than or equal to the best one in current clock sources (QL is the highest). If there is no such source, the best clock source is selected.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Configure SSM recovery mode as revertive mode or non-revertive mode.</text>
    </description>
  </typedef>
  <typedef name="ssm-level">
    <type name="enumeration">
      <enum name="unk">
        <value value="0"/>
        <description>
          <text>Unknown SSM level (UNK).</text>
        </description>
      </enum>
      <enum name="prc">
        <value value="2"/>
        <description>
          <text>SSM level of the G.811 clock (PRC).</text>
        </description>
      </enum>
      <enum name="ssua">
        <value value="4"/>
        <description>
          <text>G.812 transit clock (SSUA).</text>
        </description>
      </enum>
      <enum name="ssub">
        <value value="8"/>
        <description>
          <text>G.812 local clock (SSUB).</text>
        </description>
      </enum>
      <enum name="sec">
        <value value="11"/>
        <description>
          <text>SDH (sec).</text>
        </description>
      </enum>
      <enum name="dnu">
        <value value="15"/>
        <description>
          <text>The quality level of the clock source is unavailable (DNU).</text>
        </description>
      </enum>
      <enum name="none">
        <value value="16"/>
        <description>
          <text>Invalid SSM level.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>SSM level type.</text>
    </description>
  </typedef>
  <typedef name="sabit-type">
    <type name="enumeration">
      <enum name="sa4">
        <value value="4"/>
        <description>
          <text>Sa4 timeslot.</text>
        </description>
      </enum>
      <enum name="sa5">
        <value value="5"/>
        <description>
          <text>Sa5 timeslot.</text>
        </description>
      </enum>
      <enum name="sa6">
        <value value="6"/>
        <description>
          <text>Sa6 timeslot.</text>
        </description>
      </enum>
      <enum name="sa7">
        <value value="7"/>
        <description>
          <text>Sa7 timeslot.</text>
        </description>
      </enum>
      <enum name="sa8">
        <value value="8"/>
        <description>
          <text>Sa8 timeslot.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Sabit type.</text>
    </description>
  </typedef>
  <typedef name="source-state">
    <type name="enumeration">
      <enum name="initial">
        <value value="0"/>
        <description>
          <text>The clock source is in the initialized state.</text>
        </description>
      </enum>
      <enum name="normal">
        <value value="1"/>
        <description>
          <text>The physical source is normal.</text>
        </description>
      </enum>
      <enum name="abnormal">
        <value value="2"/>
        <description>
          <text>The clock source is abnormal.</text>
        </description>
      </enum>
      <enum name="waitwtr">
        <value value="3"/>
        <description>
          <text>The clock source is in the state of waiting for recovery.</text>
        </description>
      </enum>
      <enum name="holdoff">
        <value value="4"/>
        <description>
          <text>The clock source is in the holdoff state.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Status of a physical clock source.</text>
    </description>
  </typedef>
  <typedef name="pll-type">
    <type name="enumeration">
      <enum name="system">
        <value value="0"/>
        <description>
          <text>Select system PLL clock source.</text>
        </description>
      </enum>
      <enum name="sync2m-1">
        <value value="1"/>
        <description>
          <text>Select 2M-1 PLL clock source.</text>
        </description>
      </enum>
      <enum name="sync2m-2">
        <value value="2"/>
        <description>
          <text>Select 2M-2 PLL clock source.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>PLL type.</text>
    </description>
  </typedef>
  <typedef name="source-selection-mode">
    <type name="enumeration">
      <enum name="auto">
        <value value="0"/>
        <description>
          <text>The source selection mode is the auto mode.</text>
        </description>
      </enum>
      <enum name="manual">
        <value value="1"/>
        <description>
          <text>The physical source selection mode is manual selection.</text>
        </description>
      </enum>
      <enum name="force">
        <value value="2"/>
        <description>
          <text>The physical layer clock source selection mode is forcible selection.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Clock source selection mode: forced clock source selection, manual clock source selection, and automatic clock source selection.</text>
    </description>
  </typedef>
  <typedef name="phy-source-state">
    <type name="enumeration">
      <enum name="up">
        <value value="1"/>
        <description>
          <text>The physical layer interface is Up.</text>
        </description>
      </enum>
      <enum name="down">
        <value value="0"/>
        <description>
          <text>The physical layer interface is Down.</text>
        </description>
      </enum>
      <enum name="na">
        <value value="2"/>
        <description>
          <text>The interface physical status is not involved.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>The physical layer interface status.</text>
    </description>
  </typedef>
  <typedef name="oam-profile-type">
    <type name="enumeration">
      <enum name="none">
        <value value="0"/>
        <description>
          <text>No OAM protocol is selected.</text>
        </description>
      </enum>
      <enum name="ccsa">
        <value value="1"/>
        <description>
          <text>CCSA OAM technique requirements.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Oam profile type.</text>
    </description>
  </typedef>
  <typedef name="input-ssm-level">
    <type name="enumeration">
      <enum name="prc">
        <value value="2"/>
        <description>
          <text>G.811 clock.</text>
        </description>
      </enum>
      <enum name="ssua">
        <value value="4"/>
        <description>
          <text>G.812 transit clock.</text>
        </description>
      </enum>
      <enum name="ssub">
        <value value="8"/>
        <description>
          <text>G.812 local clock.</text>
        </description>
      </enum>
      <enum name="sec">
        <value value="11"/>
        <description>
          <text>SDH clock.</text>
        </description>
      </enum>
      <enum name="dnu">
        <value value="15"/>
        <description>
          <text>The input clock source is unavailable.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Input ssm level.</text>
    </description>
  </typedef>
  <typedef name="bits-invalid-act">
    <type name="enumeration">
      <enum name="shutdown">
        <value value="0"/>
        <description>
          <text>When no tracing source is available for the 2M ring, the output signal action is shut down.</text>
        </description>
      </enum>
      <enum name="txdnu">
        <value value="1"/>
        <description>
          <text>Set 2Mbps signal LTI action as sending DNU.</text>
        </description>
      </enum>
      <enum name="txais">
        <value value="2"/>
        <description>
          <text>Set 2Mbps signal LTI action as sending the AIS alarm.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Handling of 2M phase-locked source failure.</text>
    </description>
  </typedef>
  <typedef name="clock-bits-type">
    <type name="enumeration">
      <enum name="1pps">
        <value value="3"/>
        <description>
          <text>Bits type is 1PPS.</text>
        </description>
      </enum>
      <enum name="2mhz">
        <value value="1"/>
        <description>
          <text>Bits type is 2MHZ.</text>
        </description>
      </enum>
      <enum name="2mbps">
        <value value="0"/>
        <description>
          <text>Bits type is 2MBPS.</text>
        </description>
      </enum>
      <enum name="dcls">
        <value value="2"/>
        <description>
          <text>Bits type is DCLS.</text>
        </description>
      </enum>
      <enum name="none">
        <value value="255"/>
        <description>
          <text>NONE.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>BITS TYPE.</text>
    </description>
  </typedef>
  <typedef name="clock-run-mode">
    <type name="enumeration">
      <enum name="free">
        <value value="2"/>
        <description>
          <text>Free.</text>
        </description>
      </enum>
      <enum name="hold">
        <value value="1"/>
        <description>
          <text>Hold.</text>
        </description>
      </enum>
      <enum name="normal">
        <value value="0"/>
        <description>
          <text>Normal.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Clock run mode type.</text>
    </description>
  </typedef>
  <typedef name="dev-clk-level-type">
    <type name="enumeration">
      <enum name="SEC">
        <value value="0"/>
        <description>
          <text>Level is SEC.</text>
        </description>
      </enum>
      <enum name="ESEC">
        <value value="1"/>
        <description>
          <text>Level is eSEC.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>The Clock Level of the current device.</text>
    </description>
  </typedef>
  <container name="clocksync-phy">
    <description>
      <text>Physical layer clock synchronization management.</text>
    </description>
    <container name="common">
      <description>
        <text>Configure global information about the physical clock.</text>
      </description>
      <leaf name="system-clock-run-mode">
        <type name="clock-run-mode"/>
        <default value="normal"/>
        <description>
          <text>Running mode of a system clock.</text>
        </description>
      </leaf>
      <leaf name="ssm-control-on">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable the SSM level to participate in clock source selection.</text>
        </description>
      </leaf>
      <leaf name="ext-ssm-control-on">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable Clock ID participates in clock source selection.</text>
        </description>
      </leaf>
      <leaf name="internal-clock-id">
        <type name="uint32">
          <range value="0..15"/>
        </type>
        <default value="0"/>
        <description>
          <text>Clock ID of an internal clock source used for extended SSM control.</text>
        </description>
      </leaf>
      <leaf name="internal-clock-ssm">
        <type name="ssm-level"/>
        <default value="sec"/>
        <description>
          <text>Configure the SSM level of the internal clock source.</text>
        </description>
      </leaf>
      <leaf name="freq-check-enable">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable frequency offset detection.</text>
        </description>
      </leaf>
      <leaf name="ssm-recovery-mode">
        <type name="ssm-recovery-mode"/>
        <default value="revertive"/>
        <description>
          <text>Configure SSM recovery mode as revertive mode or non-revertive mode.</text>
        </description>
      </leaf>
      <leaf name="wtr-time">
        <type name="uint32">
          <range value="0..12"/>
        </type>
        <units name="min"/>
        <default value="5"/>
        <description>
          <text>WTR time for a Down interface.</text>
        </description>
      </leaf>
      <leaf name="freq-switch-enable">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable frequency offset switchover. After switchover triggered by frequency offset is enabled, signals are switched to another slave source on the same interface board if the frequency offset is detected abnormal.</text>
        </description>
      </leaf>
      <leaf name="holdoff-time">
        <type name="uint32">
          <range value="300..1800"/>
        </type>
        <units name="ms"/>
        <default value="1000"/>
        <description>
          <text>Holdoff time of the clock source after an interface changes from normal to abnormal.</text>
        </description>
      </leaf>
      <leaf name="ssm-unk-map">
        <type name="input-ssm-level"/>
        <default value="dnu"/>
        <description>
          <text>Map UNK to an SSM level so that the clock source with the unknown SSM level can be selected.</text>
        </description>
      </leaf>
      <leaf name="min-out-ssm-bits">
        <type name="input-ssm-level"/>
        <default value="dnu"/>
        <description>
          <text>Lowest SSM level of the clock signals output by the BITS interface.</text>
        </description>
      </leaf>
      <leaf name="max-out-ssm-system">
        <type name="ssm-level"/>
        <must condition="((../max-out-ssm-system='none' or ../max-out-ssm-system='prc' or ../max-out-ssm-system='ssua' or ../max-out-ssm-system='ssub' or ../max-out-ssm-system='sec') )"/>
        <default value="none"/>
        <description>
          <text>Highest SSM level of the output system clock signals. If the SSM level of a system clock is greater than the set highest SSM level, the system clock signals are output based on the highest SSM level.</text>
        </description>
      </leaf>
      <leaf name="max-out-ssm-2m1">
        <type name="ssm-level"/>
        <must condition="((../max-out-ssm-2m1='none' or ../max-out-ssm-2m1='prc' or ../max-out-ssm-2m1='ssua' or ../max-out-ssm-2m1='ssub' or ../max-out-ssm-2m1='sec') )"/>
        <default value="none"/>
        <description>
          <text>Highest SSM level of the output 2M-1 clock signals. If the SSM level of a 2M-1 clock is greater than the set highest SSM level, the 2M-1 clock signals are output based on the highest SSM level.</text>
        </description>
      </leaf>
      <leaf name="max-out-ssm-2m2">
        <type name="ssm-level"/>
        <must condition="((../max-out-ssm-2m2 ='none' or ../max-out-ssm-2m2 ='prc' or ../max-out-ssm-2m2 ='ssua' or ../max-out-ssm-2m2 ='ssub' or ../max-out-ssm-2m2 ='sec') )"/>
        <default value="none"/>
        <description>
          <text>Highest SSM level of the output 2M-2 clock signals. If the SSM level of a 2M-2 clock is greater than the set highest SSM level, the 2M-2 clock signals are output based on the highest SSM level.</text>
        </description>
      </leaf>
      <leaf name="clock-oam-profile">
        <type name="oam-profile-type"/>
        <default value="none"/>
        <description>
          <text>OAM protocol type.</text>
        </description>
      </leaf>
      <leaf name="freq-offset-alarm-threshold">
        <type name="uint32">
          <range value="10..92"/>
        </type>
        <default value="92"/>
        <description>
          <text>Alarm threshold for physical layer frequency offset.</text>
        </description>
      </leaf>
      <leaf name="input-ssm-alarm-threshold">
        <type name="input-ssm-level"/>
        <default value="sec"/>
        <description>
          <text>Alarm threshold for input SSM.</text>
        </description>
      </leaf>
      <leaf name="device-clk-level">
        <type name="dev-clk-level-type"/>
        <default value="SEC"/>
        <config value="false"/>
        <description>
          <text>The Clock Level of the current device.</text>
        </description>
      </leaf>
    </container>
    <container name="selection-sources">
      <description>
        <text>List of configure physical layer clock source selection.</text>
      </description>
      <list name="selection">
        <ext:generated-by value="system"/>
        <ext:operation-exclude value="create|delete"/>
        <key value="source-type"/>
        <description>
          <text>Configure physical layer clock source selection.</text>
        </description>
        <leaf name="source-type">
          <type name="pll-type"/>
          <description>
            <text>PLL type.</text>
          </description>
        </leaf>
        <leaf name="source-name">
          <when condition="not(../mode='auto')"/>
          <type name="string">
            <length value="1..49"/>
          </type>
          <must condition="((/ifm:ifm/ifm:interfaces/ifm:interface[ifm:name=current()]/clocksync-phy:clock-port-sources/clocksync-phy:source/clocksync-phy:sync-enable='true') or ((../source-type='system') and (/clocksync-phy:clocksync-phy/clocksync-phy:external-clock-sources/clocksync-phy:source[clocksync-phy:name=current()]/clocksync-phy:sync-enable='true'))) or (((../source-type='system') and not(/ifm:ifm/ifm:interfaces/ifm:interface[ifm:name=current()]/clocksync-phy:clock-port-sources/clocksync-phy:source/clocksync-phy:priority/clocksync-phy:system=0)) or ((../source-type='system') and not(/clocksync-phy:clocksync-phy/clocksync-phy:external-clock-sources/clocksync-phy:source[clocksync-phy:name=current()]/clocksync-phy:system-priority=0)))"/>
          <mandatory value="true"/>
          <description>
            <text>Name of the selected clock source in forced clock source selection and manual clock source selection.</text>
          </description>
        </leaf>
        <leaf name="mode">
          <type name="source-selection-mode"/>
          <default value="auto"/>
          <description>
            <text>Clock source selection mode: forced clock source selection, manual clock source selection, and automatic clock source selection.</text>
          </description>
        </leaf>
        <leaf name="trace-source-name">
          <type name="string">
            <length value="1..63"/>
          </type>
          <config value="false"/>
          <description>
            <text>Name of the traced clock source.</text>
          </description>
        </leaf>
      </list>
    </container>
    <container name="external-clock-sources">
      <description>
        <text>List of configure physical layer clock source.</text>
      </description>
      <list name="source">
        <ext:generated-by value="system"/>
        <ext:operation-exclude value="create|delete"/>
        <key value="name"/>
        <description>
          <text>Configure physical clock source.</text>
        </description>
        <leaf name="name">
          <type name="string">
            <length value="1..49"/>
            <pattern value="bits[0-3]((/(c?)[0-9]{1,2}){1,2}){0,1}|ptp|CESACR([0-9]{1,2}/){2,3}[0-9]{1,2}"/>
          </type>
          <description>
            <text>Name of the physical clock source.</text>
          </description>
        </leaf>
        <leaf name="state">
          <type name="source-state"/>
          <default value="initial"/>
          <config value="false"/>
          <description>
            <text>Status of a physical clock source.</text>
          </description>
        </leaf>
        <leaf name="sync-enable">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable clock synchronization for the physical clock source.</text>
          </description>
        </leaf>
        <leaf name="reference-enable">
          <type name="boolean"/>
          <default value="false"/>
          <config value="false"/>
          <description>
            <text>Whether a clock source can serve as a reference clock source.</text>
          </description>
        </leaf>
        <leaf name="phy-state">
          <type name="phy-source-state"/>
          <default value="down"/>
          <config value="false"/>
          <description>
            <text>Physical status of the clock source.</text>
          </description>
        </leaf>
        <leaf name="system-priority">
          <type name="int32">
            <range value="0..255"/>
          </type>
          <default value="0"/>
          <description>
            <text>Priority of the physical source. In physical clock source selection, you must configure the priority of clock sources for the system PLL. Otherwise, the clock sources cannot participate in the selection of the system PLL clock source.</text>
          </description>
        </leaf>
        <container name="ssm-class">
          <description>
            <text>Configure information about the SSM level, including SSM selection mode, input SSM level, and output SSM level.</text>
          </description>
          <leaf name="ssm-configured">
            <type name="ssm-level"/>
            <default value="none"/>
            <description>
              <text>Input SSM level of the physical clock source. If clock sources are configured with SSM levels, the configured SSM levels are used.</text>
            </description>
          </leaf>
          <leaf name="ssm-learn">
            <type name="ssm-level"/>
            <must condition="(((../ssm-learn='none' or ../ssm-learn='dnu' or ../ssm-learn='prc' or ../ssm-learn='sec' or ../ssm-learn='ssua' or ../ssm-learn='ssub')))"/>
            <default value="none"/>
            <config value="false"/>
            <description>
              <text>Input SSM level of the physical clock source. If clock sources are configured with SSM levels, the configured SSM levels are used.</text>
            </description>
          </leaf>
          <leaf name="ssm-output">
            <type name="ssm-level"/>
            <config value="false"/>
            <description>
              <text>Output SSM level of the physical clock source.</text>
            </description>
          </leaf>
        </container>
        <container name="clock-id">
          <description>
            <text>Configure information about the Clock ID of extended SSM control, including the clock ID selection mode, input clock ID, and output clock ID.</text>
          </description>
          <leaf name="clock-id-configured">
            <type name="uint32">
              <range value="0..15"/>
            </type>
            <default value="0"/>
            <description>
              <text>Clock ID of the physical clock source in extended SSM control.</text>
            </description>
          </leaf>
          <leaf name="clock-id-learn">
            <type name="uint32">
              <range value="0..15"/>
            </type>
            <default value="0"/>
            <config value="false"/>
            <description>
              <text>Clock ID of the physical clock source in extended SSM control.</text>
            </description>
          </leaf>
          <leaf name="clock-id-out">
            <type name="uint32">
              <range value="0..15"/>
            </type>
            <default value="0"/>
            <config value="false"/>
            <description>
              <text>Output Clock ID of the physical clock source in extended SSM control.</text>
            </description>
          </leaf>
        </container>
        <container name="bits-sources">
          <description>
            <text>Configure physical BITS clock sources.</text>
          </description>
          <leaf name="sabit">
            <type name="sabit-type"/>
            <default value="sa4"/>
            <description>
              <text>Sa timeslot used by the BITS interface to send or receive the SSM level.</text>
            </description>
          </leaf>
          <leaf name="signal-type">
            <type name="clock-bits-type"/>
            <default value="2mbps"/>
            <description>
              <text>BITS signal type.</text>
            </description>
          </leaf>
          <leaf name="invalid-act">
            <type name="bits-invalid-act"/>
            <default value="shutdown"/>
            <description>
              <text>2M phase-locked source failure handling.</text>
            </description>
          </leaf>
          <leaf name="force-cutoff-enable">
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable forcible cutoff for external clock output.</text>
            </description>
          </leaf>
        </container>
      </list>
    </container>
    <container name="detections">
      <config value="false"/>
      <description>
        <text>List of statistics of physical clock source detection.</text>
      </description>
      <list name="detection">
        <key value="source-name"/>
        <description>
          <text>Statistics of physical clock source detection.</text>
        </description>
        <leaf name="source-name">
          <type name="string">
            <length value="1..63"/>
          </type>
          <description>
            <text>Physical clock source name.</text>
          </description>
        </leaf>
        <leaf name="freq-value">
          <type name="string">
            <length value="1..50"/>
          </type>
          <description>
            <text>Detected frequency offset value of the clock source.</text>
          </description>
        </leaf>
      </list>
    </container>
  </container>
  <rpc name="recover-freq-deviation">
    <description>
      <text>Frequency offset status of the clock source to be recovered.</text>
    </description>
    <ext:node-ref value="/clocksync-phy:clocksync-phy/clocksync-phy:detections/clocksync-phy:detection"/>
    <input>
      <leaf name="recover-src-name">
        <type name="pub-type:if-name"/>
        <description>
          <text>Interface name of the clock source to be recovered.</text>
        </description>
      </leaf>
    </input>
  </rpc>
  <augment target-node="/ifm:ifm/ifm:interfaces/ifm:interface">
    <description>
      <text>Interface.</text>
    </description>
    <container name="clock-port-sources">
      <description>
        <text>Configure physical layer clock source.</text>
      </description>
      <container name="source">
        <description>
          <text>Configure physical clock source node.</text>
        </description>
        <leaf name="state">
          <type name="source-state"/>
          <default value="initial"/>
          <config value="false"/>
          <description>
            <text>Status of a physical clock source.</text>
          </description>
        </leaf>
        <leaf name="sync-enable">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable clock synchronization for the physical clock source.</text>
          </description>
        </leaf>
        <leaf name="esmc-send-disable">
          <when condition="not((/ifm:ifm/ifm:interfaces/ifm:interface[ifm:name=current()/../../../ifm:name]/ifm:type='E1') or (/ifm:ifm/ifm:interfaces/ifm:interface[ifm:name=current()/../../../ifm:name]/ifm:type='Pos') or (/ifm:ifm/ifm:interfaces/ifm:interface[ifm:name=current()/../../../ifm:name]/ifm:type='Cpos') or (/ifm:ifm/ifm:interfaces/ifm:interface[ifm:name=current()/../../../ifm:name]/ifm:type='T1'))"/>
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable ESMC packet sending on interface.</text>
          </description>
        </leaf>
        <leaf name="reference-enable">
          <type name="boolean"/>
          <default value="false"/>
          <config value="false"/>
          <description>
            <text>Enable/disable a clock source can serve as a reference clock source.</text>
          </description>
        </leaf>
        <leaf name="slave-nego-enable">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable the physical layer slave negotiation mode. Only GE electrical interfaces support this configuration.</text>
          </description>
        </leaf>
        <leaf name="phy-state">
          <type name="phy-source-state"/>
          <default value="down"/>
          <config value="false"/>
          <description>
            <text>Physical status of the clock source.</text>
          </description>
        </leaf>
        <leaf name="e1-sabit">
          <when condition="/ifm:ifm/ifm:interfaces/ifm:interface[ifm:name=current()/../../../ifm:name]/ifm:type='E1'"/>
          <type name="sabit-type"/>
          <default value="sa4"/>
          <description>
            <text>Sa timeslot used by the E1 port to send or receive the SSM level.</text>
          </description>
        </leaf>
        <leaf name="e1-ais-enable">
          <when condition="/ifm:ifm/ifm:interfaces/ifm:interface[ifm:name=current()/../../../ifm:name]/ifm:type='E1'"/>
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable association of AIS signals of E1 ports with the clock source status.</text>
          </description>
        </leaf>
        <leaf name="cpos-ais-enable">
          <when condition="/ifm:ifm/ifm:interfaces/ifm:interface[ifm:name=current()/../../../ifm:name]/ifm:type='Cpos'"/>
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable Association of the clock source status with the MS-AIS signal alarm on the CPOS port.</text>
          </description>
        </leaf>
        <leaf name="cpos-exc-enable">
          <when condition="/ifm:ifm/ifm:interfaces/ifm:interface[ifm:name=current()/../../../ifm:name]/ifm:type='Cpos'"/>
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable Association of the clock source status with the B2-EXC signal alarm on the CPOS port.</text>
          </description>
        </leaf>
        <container name="priority">
          <description>
            <text>Configure priority of the physical source.</text>
          </description>
          <leaf name="system">
            <type name="int32">
              <range value="0..255"/>
            </type>
            <default value="0"/>
            <description>
              <text>In physical clock source selection, you must configure the priority of clock sources for the system PLL. Otherwise, the clock sources cannot participate in the selection of the system PLL clock source.</text>
            </description>
          </leaf>
          <leaf name="sync2m1">
            <type name="int32">
              <range value="0..255"/>
            </type>
            <default value="0"/>
            <description>
              <text>In physical clock source selection, you must configure the priority of clock sources for the 2M-1 PLL. Otherwise, the clock sources cannot participate in the selection of the 2M-1 PLL clock source.</text>
            </description>
          </leaf>
          <leaf name="sync2m2">
            <type name="int32">
              <range value="0..255"/>
            </type>
            <default value="0"/>
            <description>
              <text>In physical clock source selection, you must configure the priority of clock sources for the 2M-2 PLL. Otherwise, the clock sources cannot participate in the selection of the 2M-2 PLL clock source.</text>
            </description>
          </leaf>
        </container>
        <container name="ssm-class">
          <description>
            <text>Configure information about the SSM level. Including SSM selection mode, input SSM level, and output SSM level.</text>
          </description>
          <leaf name="ssm-configured">
            <type name="ssm-level"/>
            <default value="none"/>
            <description>
              <text>Input SSM level of the physical clock source. If clock sources are configured with SSM levels, the configured SSM levels are used.</text>
            </description>
          </leaf>
          <leaf name="ssm-learn">
            <type name="ssm-level"/>
            <must condition="(((../ssm-learn='none' or ../ssm-learn='dnu' or ../ssm-learn='prc' or ../ssm-learn='sec' or ../ssm-learn='ssua' or ../ssm-learn='ssub')))"/>
            <default value="none"/>
            <config value="false"/>
            <description>
              <text>Input SSM level of the physical clock source. If clock sources are configured with SSM levels, the configured SSM levels are used.</text>
            </description>
          </leaf>
          <leaf name="ssm-output">
            <type name="ssm-level"/>
            <config value="false"/>
            <description>
              <text>Output SSM level of the physical clock source.</text>
            </description>
          </leaf>
        </container>
        <container name="clock-id">
          <description>
            <text>Configure information about the Clock ID of extended SSM control. Including the clock ID selection mode, input clock ID, and output clock ID.</text>
          </description>
          <leaf name="clock-id-configured">
            <type name="uint32">
              <range value="0..15"/>
            </type>
            <default value="0"/>
            <description>
              <text>Clock ID of the physical clock source in extended SSM control.</text>
            </description>
          </leaf>
          <leaf name="clock-id-learn">
            <type name="uint32">
              <range value="0..15"/>
            </type>
            <default value="0"/>
            <config value="false"/>
            <description>
              <text>Clock ID of the physical clock source in extended SSM control.</text>
            </description>
          </leaf>
          <leaf name="clock-id-out">
            <type name="uint32">
              <range value="0..15"/>
            </type>
            <default value="0"/>
            <config value="false"/>
            <description>
              <text>Output Clock ID of the physical clock source in extended SSM control.</text>
            </description>
          </leaf>
        </container>
        <leaf name="bundle-group">
          <type name="int32">
            <range value="0..64"/>
          </type>
          <default value="0"/>
          <description>
            <text>Bundling group on a port.</text>
          </description>
        </leaf>
      </container>
    </container>
  </augment>
</module>
