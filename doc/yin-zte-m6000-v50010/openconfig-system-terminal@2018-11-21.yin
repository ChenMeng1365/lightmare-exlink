<?xml version="1.0" encoding="UTF-8"?>
<module name="openconfig-system-terminal"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:oc-sys-term="http://openconfig.net/yang/system/terminal"
        xmlns:oc-ext="http://openconfig.net/yang/openconfig-ext">
  <yang-version value="1"/>
  <namespace uri="http://openconfig.net/yang/system/terminal"/>
  <prefix value="oc-sys-term"/>
  <import module="openconfig-extensions">
    <prefix value="oc-ext"/>
  </import>
  <organization>
    <text>OpenConfig working group</text>
  </organization>
  <contact>
    <text>OpenConfig working group
www.openconfig.net</text>
  </contact>
  <description>
    <text>This module defines configuration and operational state data
related to remote terminal services such as ssh and telnet.</text>
  </description>
  <oc-ext:openconfig-version semver="0.3.1"/>
  <revision date="2018-11-21">
    <description>
      <text>Add OpenConfig module metadata extensions.</text>
    </description>
    <reference>
      <text>0.3.1</text>
    </reference>
  </revision>
  <revision date="2017-09-18">
    <description>
      <text>Updated to use OpenConfig types modules</text>
    </description>
    <reference>
      <text>0.3.0</text>
    </reference>
  </revision>
  <revision date="2017-07-06">
    <description>
      <text>Move to oc-inet types, add IETF attribution, add RADIUS
counters, changed password leaf names to indicate hashed</text>
    </description>
    <reference>
      <text>0.2.0</text>
    </reference>
  </revision>
  <revision date="2017-01-29">
    <description>
      <text>Initial public release</text>
    </description>
    <reference>
      <text>0.1.0</text>
    </reference>
  </revision>
  <oc-ext:regexp-posix/>
  <oc-ext:catalog-organization org="openconfig"/>
  <oc-ext:origin origin="openconfig"/>
  <grouping name="system-terminal-common-config">
    <description>
      <text>Common configuration data for terminal services</text>
    </description>
    <leaf name="timeout">
      <type name="uint16"/>
      <units name="seconds"/>
      <description>
        <text>Set the idle timeout in seconds on terminal connections to
the system for the protocol.</text>
      </description>
    </leaf>
    <leaf name="rate-limit">
      <type name="uint16"/>
      <units name="conn/min"/>
      <description>
        <text>Set a limit on the number of connection attempts per
minute to the system for the protocol.</text>
      </description>
    </leaf>
    <leaf name="session-limit">
      <type name="uint16"/>
      <description>
        <text>Set a limit on the number of simultaneous active terminal
sessions to the system for the protocol (e.g., ssh,
telnet, ...) </text>
      </description>
    </leaf>
  </grouping>
  <grouping name="system-terminal-common-state">
    <description>
      <text>Common operational state data for terminal services</text>
    </description>
  </grouping>
  <grouping name="system-terminal-common-top">
    <description>
      <text>Top-level grouping for common terminal service data</text>
    </description>
    <container name="terminal-servers">
      <description>
        <text>Top-level container for terminal services</text>
      </description>
      <container name="config">
        <description>
          <text>Configuration data for terminal services</text>
        </description>
        <uses name="system-terminal-common-config"/>
      </container>
      <container name="state">
        <config value="false"/>
        <description>
          <text>Operational state data </text>
        </description>
        <uses name="system-terminal-common-config"/>
        <uses name="system-terminal-common-state"/>
      </container>
    </container>
  </grouping>
  <grouping name="system-ssh-server-config">
    <description>
      <text>Configuration data for system ssh configuration</text>
    </description>
    <leaf name="enable">
      <type name="boolean"/>
      <default value="true"/>
      <description>
        <text>Enables the ssh server.  The ssh server is enabled by
default.</text>
      </description>
    </leaf>
    <leaf name="protocol-version">
      <type name="enumeration">
        <enum name="V2">
          <description>
            <text>Use SSH v2 only</text>
          </description>
        </enum>
        <enum name="V1">
          <description>
            <text>Use SSH v1 only</text>
          </description>
        </enum>
        <enum name="V1_V2">
          <description>
            <text>Use either SSH v1 or v2</text>
          </description>
        </enum>
      </type>
      <default value="V2"/>
      <description>
        <text>Set the protocol version for SSH connections to the system</text>
      </description>
    </leaf>
    <uses name="system-terminal-common-config"/>
  </grouping>
  <grouping name="system-ssh-server-state">
    <description>
      <text>Operational state data for ssh server</text>
    </description>
  </grouping>
  <grouping name="system-ssh-server-top">
    <description>
      <text>Top-level grouping for ssh server data</text>
    </description>
    <container name="ssh-server">
      <description>
        <text>Top-level container for ssh server</text>
      </description>
      <container name="config">
        <description>
          <text>Configuration data for the system ssh server</text>
        </description>
        <uses name="system-ssh-server-config"/>
      </container>
      <container name="state">
        <config value="false"/>
        <description>
          <text>Operational state data for the system ssh server</text>
        </description>
        <uses name="system-ssh-server-config"/>
        <uses name="system-ssh-server-state"/>
      </container>
    </container>
  </grouping>
  <grouping name="system-telnet-server-config">
    <description>
      <text>Configuration data for telnet server</text>
    </description>
    <leaf name="enable">
      <type name="boolean"/>
      <default value="false"/>
      <description>
        <text>Enables the telnet server.  Telnet is disabled by
default</text>
      </description>
    </leaf>
    <uses name="system-terminal-common-config"/>
  </grouping>
  <grouping name="system-telnet-server-state">
    <description>
      <text>Operational state data for telnet server</text>
    </description>
  </grouping>
  <grouping name="system-telnet-server-top">
    <description>
      <text>Top-level grouping for telnet server </text>
    </description>
    <container name="telnet-server">
      <description>
        <text>Top-level container for telnet terminal servers</text>
      </description>
      <container name="config">
        <description>
          <text>Configuration data for telnet</text>
        </description>
        <uses name="system-telnet-server-config"/>
      </container>
      <container name="state">
        <config value="false"/>
        <description>
          <text>Operational state data for telnet</text>
        </description>
        <uses name="system-telnet-server-config"/>
        <uses name="system-telnet-server-state"/>
      </container>
    </container>
  </grouping>
</module>
