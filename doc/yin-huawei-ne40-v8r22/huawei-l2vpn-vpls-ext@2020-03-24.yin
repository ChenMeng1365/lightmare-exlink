  <rpc name="reset-vsi-ac-suppression-statistics">
    <description>
      <text>Reset AC suppression.</text>
    </description>
    <ext:node-ref value="/l2vpn:l2vpn/l2vpn:instances/l2vpn:instance/l2vpn:vpls"/>
    <input>
      <leaf name="interface-name">
        <type name="leafref">
          <path value="/l2vpn:l2vpn/l2vpn:instances/l2vpn:instance/l2vpn:vpls/l2vpn:acs/l2vpn:ac/l2vpn:interface-name"/>
        </type>
        <mandatory value="true"/>
        <description>
          <text>Interface name.</text>
        </description>
      </leaf>
      <leaf name="vsi-name">
        <type name="leafref">
          <path value="/l2vpn:l2vpn/l2vpn:instances/l2vpn:instance/l2vpn:name"/>
        </type>
        <mandatory value="true"/>
        <description>
          <text>VSI name.</text>
        </description>
      </leaf>
    </input>
  </rpc>
  <ext:task-name value="forwarding"/>
  <augment target-node="/l2vpn:l2vpn/l2vpn:common">
    <description>
      <text>L2VPN common parameters.</text>
    </description>
    <container name="l2vpn-ext">
      <description>
        <text>Configure the extension of L2VPN.</text>
      </description>
      <leaf name="vpls-ldp-fast-switch">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable the flag of VPLS LDP fast-switch.</text>
        </description>
      </leaf>
      <leaf name="pw-source-tunnel-check">
        <type name="boolean"/>
        <default value="true"/>
        <description>
          <text>Enable/disable the flag of L2VPN pw-source-tunnel check.</text>
        </description>
      </leaf>
      <leaf name="flow-label-hash">
        <type name="enumeration">
          <enum name="L2">
            <value value="2"/>
            <description>
              <text>L2.</text>
            </description>
          </enum>
          <enum name="L3">
            <value value="1"/>
            <description>
              <text>L3.</text>
            </description>
          </enum>
          <enum name="L4">
            <value value="0"/>
            <description>
              <text>L4.</text>
            </description>
          </enum>
        </type>
        <default value="L4"/>
        <description>
          <text>The flag of flowLabel.</text>
        </description>
      </leaf>
      <leaf name="convergence-separate-in-system">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable the flag of VPLS convergence separate in systerm.</text>
        </description>
      </leaf>
      <leaf name="vpls-tm-prune-enable">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable the flag of TM prune mode.</text>
        </description>
      </leaf>
    </container>
  </augment>
  <augment target-node="/l2vpn:l2vpn/l2vpn:instances/l2vpn:instance/l2vpn:vpls">
    <description>
      <text>Vpls instances.</text>
    </description>
    <container name="vsi-pipe">
      <description>
        <text>Configure the diffserv mode of VPLS.</text>
      </description>
      <leaf name="split-mode">
        <type name="boolean"/>
        <must condition="((../split-mode='false' and not(../ingress-pipe-mode) and not(../egress-pipe-mode) and ../pipe-mode) or (../split-mode='true' and not(../pipe-mode) and ../ingress-pipe-mode and ../egress-pipe-mode and not(../ingress-pipe-mode='uniform' and ../egress-pipe-mode='uniform')))"/>
        <default value="false"/>
        <description>
          <text>Enable/disable split mode.</text>
        </description>
      </leaf>
      <leaf name="pipe-mode">
        <when condition="../split-mode='false'"/>
        <type name="l2vpn-pipe-mode"/>
        <must condition="(../split-mode='false') and ((../pipe-mode='pipe' and (../service-class) and (../color)) or (../pipe-mode='shortPipe' and (../service-class) and (../color) and (../domain-name)) or (../pipe-mode='uniform'))"/>
        <default value="uniform"/>
        <description>
          <text>VSI pipe mode.</text>
        </description>
      </leaf>
      <leaf name="ingress-pipe-mode">
        <when condition="../split-mode='true'"/>
        <type name="l2vpn-pipe-mode"/>
        <must condition="((../split-mode='true') and (((../service-class) and (../color) and (../ingress-pipe-mode='shortPipe' or ../ingress-pipe-mode='pipe')) or (../ingress-pipe-mode='uniform' and ../egress-pipe-mode!='uniform')))"/>
        <default value="uniform"/>
        <description>
          <text>VSI ingress pipe mode.</text>
        </description>
      </leaf>
      <leaf name="egress-pipe-mode">
        <when condition="../split-mode='true'"/>
        <type name="l2vpn-pipe-mode"/>
        <must condition="((../split-mode='true') and (((../domain-name) and (../trust-type) and (../egress-pipe-mode='shortPipe')) or (../egress-pipe-mode='pipe') or (../egress-pipe-mode='uniform' and ../ingress-pipe-mode!='uniform')))"/>
        <default value="uniform"/>
        <description>
          <text>VSI egress pipe mode.</text>
        </description>
      </leaf>
      <leaf name="service-class">
        <when condition="(../pipe-mode='pipe' or ../pipe-mode='shortPipe' or ../ingress-pipe-mode='pipe' or ../ingress-pipe-mode='shortPipe')"/>
        <type name="l2vpn-service-class"/>
        <must condition="(../pipe-mode='pipe' or ../pipe-mode='shortPipe' or ../ingress-pipe-mode='pipe' or ../ingress-pipe-mode='shortPipe')"/>
        <default value="be"/>
        <description>
          <text>VSI service class.</text>
        </description>
      </leaf>
      <leaf name="color">
        <when condition="(../pipe-mode='pipe' or ../pipe-mode='shortPipe'  or ../ingress-pipe-mode='pipe' or ../ingress-pipe-mode='shortPipe')"/>
        <type name="l2vpn-color"/>
        <must condition="(../pipe-mode='pipe' or ../pipe-mode='shortPipe' or ../ingress-pipe-mode='pipe' or ../ingress-pipe-mode='shortPipe')"/>
        <default value="green"/>
        <description>
          <text>Packet color.</text>
        </description>
      </leaf>
      <leaf name="domain-name">
        <when condition="(../pipe-mode='shortPipe'  or ../egress-pipe-mode='shortPipe')"/>
        <type name="leafref">
          <path value="/qos:qos/qos:classifier-template/qos:diffserv-domains/qos:diffserv-domain/qos:name"/>
        </type>
        <must condition="(../pipe-mode='shortPipe' or ../egress-pipe-mode='shortPipe')"/>
        <default value="default"/>
        <description>
          <text>Domain name.</text>
        </description>
      </leaf>
      <leaf name="trust-type">
        <when condition="../egress-pipe-mode='shortPipe'"/>
        <type name="l2vpn-trust-type"/>
        <must condition="../egress-pipe-mode='shortPipe'"/>
        <default value="outpri"/>
        <description>
          <text>VSI trust type.</text>
        </description>
      </leaf>
    </container>
    <container name="reserve-if-switch">
      <when condition="../l2vpn:work-mode='normal'"/>
      <description>
        <text>Configure VSI separate broadcast fast switching.</text>
      </description>
      <leaf name="enable">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable the flag of VSI separate broadcast fast switching.</text>
        </description>
      </leaf>
    </container>
    <container name="vpls-ext">
      <description>
        <text>Configure VPLS common extension.</text>
      </description>
      <leaf name="vsi-multicast-switch">
        <type name="boolean"/>
        <must condition="(../vsi-multicast-switch = 'false') or ((../vsi-multicast-switch = 'true') and (../../l2vpn:work-mode='normal' or ../../l2vpn:work-mode='bd-mode' or ../../l2vpn:work-mode='b-vsi' or ../../l2vpn:work-mode='i-vsi' or ../../l2vpn:work-mode='i-vsi-p2p'))"/>
        <default value="false"/>
        <description>
          <text>Enable/disable the flag of VPLS multicast fast switching.</text>
        </description>
      </leaf>
      <leaf name="pw-packet-type-traffic-statistic">
        <type name="boolean"/>
        <must condition="(../pw-packet-type-traffic-statistic = 'false') or ((../pw-packet-type-traffic-statistic = 'true') and (../../l2vpn:work-mode='normal' or ../../l2vpn:work-mode='bd-mode'))"/>
        <default value="false"/>
        <description>
          <text>Enable/disable the function to collect packet-type traffic statistics.</text>
        </description>
      </leaf>
      <leaf name="convergence-separate">
        <type name="boolean"/>
        <must condition="(../convergence-separate ='false') or ((../convergence-separate ='true') and (../../l2vpn:work-mode='normal' or ../../l2vpn:work-mode='bd-mode') and (../../l2vpn:ldp-signaling/l2vpn:hard-pipe='false') and (/l2vpn:l2vpn/l2vpn:common/l2vpn-vpls-ext:l2vpn-ext/l2vpn-vpls-ext:convergence-separate-in-system='false'))"/>
        <default value="false"/>
        <description>
          <text>Enable/disable the flag of VPLS convergence separate in VSI.</text>
        </description>
      </leaf>
    </container>
  </augment>
  <augment target-node="/l2vpn:l2vpn/l2vpn:instances/l2vpn:instance/l2vpn:vpls/l2vpn:acs/l2vpn:ac">
