            <max-elements value="100"/>
            <description>
              <text>Configure a VNI node.</text>
            </description>
            <leaf name="value">
              <type name="uint32">
                <range value="1..16777215"/>
              </type>
              <description>
                <text>Configure a VNI value.</text>
              </description>
