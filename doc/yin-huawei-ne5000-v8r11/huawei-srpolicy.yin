            <leaf name="down-reason">
              <type name="map-down-reason-type"/>
              <description>
                <text>Reason why a mapping went down.</text>
              </description>
            </leaf>
          </list>
        </container>
      </list>
    </container>
  </grouping>
  <grouping name="policy-group-statistics">
    <description>
      <text>SR Policy group statistics.</text>
    </description>
    <container name="policy-group-statistic">
      <config value="false"/>
      <description>
        <text>Statistics of SR Policy groups.</text>
      </description>
      <leaf name="mapping-policy">
        <type name="uint32">
          <range value="0..4294967295"/>
        </type>
        <description>
          <text>Number of mapping policies.</text>
        </description>
      </leaf>
      <leaf name="sr-policy-group">
        <type name="uint32">
          <range value="0..4294967295"/>
        </type>
        <description>
          <text>Number of SR Policy groups.</text>
        </description>
      </leaf>
      <leaf name="sr-policy-group-up">
        <type name="uint32">
          <range value="0..4294967295"/>
        </type>
        <description>
          <text>Number of SR Policy groups that are up.</text>
        </description>
      </leaf>
      <leaf name="dscp-map">
        <type name="uint32">
          <range value="0..4294967295"/>
        </type>
        <description>
          <text>Number of DSCP mappings.</text>
        </description>
      </leaf>
      <leaf name="dscp-map-up">
        <type name="uint32">
          <range value="0..4294967295"/>
        </type>
        <description>
          <text>Number of DSCP mappings that are up.</text>
        </description>
      </leaf>
    </container>
  </grouping>
  <rpc name="reset-srv6-te-policy-traffic-statistics">
    <description>
      <text>Reset SRv6 TE Policy traffic statistics.</text>
    </description>
    <input>
      <uses name="reset-traffic-statistics"/>
    </input>
  </rpc>
  <rpc name="reset-sr-mpls-te-policy-traffic-statistics">
    <description>
      <text>Reset SR-MPLS TE Policy traffic statistics.</text>
    </description>
    <input>
      <uses name="reset-traffic-statistics"/>
    </input>
  </rpc>
  <augment target-node="/ni:network-instance/ni:instances/ni:instance/srv6:srv6">
    <description>
      <text>Augment for Segment Routing Policy.</text>
    </description>
    <container name="srpolicy">
      <when condition="../srv6:enable='true'"/>
      <description>
        <text>Configure SR Policy parameters.</text>
      </description>
      <uses name="srv6-policy-attributes"/>
      <uses name="srv6-policys"/>
      <uses name="traffic-statistics"/>
      <uses name="srv6-policy-infos"/>
      <uses name="dynamic-policy-group"/>
      <uses name="dynamic-policy-group-down-reason"/>
      <uses name="policy-group-statistics"/>
      <uses name="sr-template-infos"/>
      <uses name="srv6-source-sids"/>
      <uses name="srv6-sid-statistics"/>
    </container>
  </augment>
  <augment target-node="/ni:network-instance/ni:instances/ni:instance/sr:segment-routing">
    <description>
      <text>Augment for SR Policy.</text>
    </description>
    <container name="srpolicy">
      <when condition="../sr:enable='true'"/>
      <description>
        <text>Configure SR Policy parameters.</text>
      </description>
      <uses name="srv4-policy-attributes"/>
      <uses name="srv4-policys"/>
      <uses name="srv4-policy-infos"/>
      <uses name="policy-groups"/>
      <uses name="traffic-statistics"/>
      <uses name="sr-template-infos"/>
    </container>
  </augment>
  <augment target-node="/ni:network-instance/ni:instances/ni:instance/srv6:srv6/srv6:mapping-policys/srv6:mapping-policy">
    <description>
      <text>Augment for SR Policy mappings.</text>
    </description>
    <uses name="policy-maps"/>
  </augment>
</module>
