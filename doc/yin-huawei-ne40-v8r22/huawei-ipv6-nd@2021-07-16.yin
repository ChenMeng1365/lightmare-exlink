          </leaf>
          <leaf name="dest-address">
            <type name="inet:ipv6-address-no-zone"/>
            <description>
              <text>Destination IPv6 address of a packet.</text>
            </description>
          </leaf>
          <leaf name="target-address">
            <type name="inet:ipv6-address-no-zone"/>
            <description>
              <text>Target IPv6 address of a packet.</text>
            </description>
          </leaf>
          <leaf name="source-mac">
            <type name="pub-type:mac-address"/>
            <description>
              <text>Source MAC address of a packet.</text>
            </description>
          </leaf>
          <leaf name="dest-mac">
            <type name="pub-type:mac-address"/>
            <description>
              <text>Destination MAC address of a packet.</text>
            </description>
          </leaf>
          <leaf name="pe-vlan">
            <type name="uint32">
              <range value="1..4094"/>
            </type>
            <description>
              <text>Outer VLAN tag.</text>
            </description>
          </leaf>
          <leaf name="ce-vlan">
            <type name="uint32">
              <range value="1..4094"/>
            </type>
            <description>
              <text>Inner VLAN tag.</text>
            </description>
          </leaf>
          <leaf name="attack-times">
            <type name="uint32"/>
            <description>
              <text>Attacked times.</text>
            </description>
          </leaf>
          <leaf name="last-attack-time">
            <type name="yang:date-and-time"/>
            <description>
              <text>Last attacked time.</text>
            </description>
          </leaf>
          <leaf name="dropped-reason">
            <type name="string">
              <length value="1..63"/>
            </type>
            <description>
              <text>Dropped reason.</text>
            </description>
          </leaf>
        </list>
      </container>
      <container name="miss-records">
        <description>
          <text>List of query miss anti-attack record.</text>
        </description>
        <list name="miss-record">
          <key value="if-name target-address"/>
          <description>
            <text>Statistics of query miss anti-attack record.</text>
          </description>
          <leaf name="if-name">
            <type name="leafref">
              <path value="/ifm:ifm/ifm:interfaces/ifm:interface/ifm:name"/>
            </type>
            <description>
              <text>Interface name.</text>
            </description>
          </leaf>
          <leaf name="target-address">
            <type name="inet:ipv6-address-no-zone"/>
            <description>
              <text>Target IPv6 address of a miss message.</text>
            </description>
          </leaf>
          <leaf name="source-address">
            <type name="inet:ipv6-address-no-zone"/>
            <description>
              <text>Source IPv6 address of a miss message.</text>
            </description>
          </leaf>
        </list>
      </container>
    </container>
  </container>
  <rpc name="nd-pkt-clear-all">
    <description>
      <text>To clear all ND statistics.</text>
    </description>
    <ext:node-ref value="/ipv6-nd:ipv6-nd/ipv6-nd:nd-pkt-stats"/>
  </rpc>
  <rpc name="ipv6-neighbor-clear">
    <description>
      <text>To clear IPv6 dynamic neighbors.</text>
    </description>
    <ext:node-ref value="/ipv6-nd:ipv6-nd/ipv6-nd:ipv6-neighbors"/>
    <input>
      <choice name="clear-type">
        <description>
          <text>IPv6 neighbor clear type.</text>
        </description>
        <case name="if-ip">
          <description>
            <text>Clear by specified Interface name or Interface name and IPv6 address.</text>
          </description>
          <leaf name="if-name">
            <type name="leafref">
              <path value="/ifm:ifm/ifm:interfaces/ifm:interface/ifm:name"/>
            </type>
            <mandatory value="true"/>
            <description>
              <text>Interface name.</text>
            </description>
          </leaf>
          <leaf name="ipv6-addr">
            <type name="inet:ipv6-address-no-zone"/>
            <description>
              <text>IPv6 address.</text>
            </description>
          </leaf>
        </case>
      </choice>
    </input>
  </rpc>
  <rpc name="anti-attack-packet-record-clear">
    <description>
      <text>To clear packet anti-attack records.</text>
    </description>
    <ext:node-ref value="/ipv6-nd:ipv6-nd/ipv6-nd:anti-attack-records/ipv6-nd:packet-records"/>
    <input>
      <leaf name="if-name">
        <type name="leafref">
          <path value="/ifm:ifm/ifm:interfaces/ifm:interface/ifm:name"/>
        </type>
        <description>
          <text>Interface name.</text>
        </description>
      </leaf>
    </input>
  </rpc>
  <rpc name="anti-attack-miss-record-clear">
    <description>
      <text>To clear miss anti-attack records.</text>
    </description>
    <ext:node-ref value="/ipv6-nd:ipv6-nd/ipv6-nd:anti-attack-records/ipv6-nd:miss-records"/>
    <input>
      <leaf name="if-name">
        <type name="leafref">
          <path value="/ifm:ifm/ifm:interfaces/ifm:interface/ifm:name"/>
        </type>
        <description>
          <text>Interface name.</text>
        </description>
      </leaf>
    </input>
  </rpc>
  <augment target-node="/bd:bd/bd:instances/bd:instance">
    <description>
      <text>List of Bridge-Domain Neighbor Multicast-Suppress Information.</text>
    </description>
    <container name="nd-bd-property">
      <must condition="not(unicast-forward and proxy-reply)"/>
      <description>
        <text>Configure Bridge-Domain neighbor multicast-suppress information.</text>
      </description>
      <leaf name="unicast-forward">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable Bridge-Domain unicast forward.</text>
        </description>
      </leaf>
      <leaf name="proxy-reply">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable Bridge-Domain proxy reply.</text>
        </description>
      </leaf>
      <leaf name="mismatch-discard">
        <when condition="../unicast-forward='true' or ../proxy-reply='true'"/>
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable Bridge-Domain mismatch discard.</text>
        </description>
      </leaf>
    </container>
  </augment>
  <augment target-node="/ifm:ifm/ifm:interfaces/ifm:interface/ip:ipv6">
    <when condition="../ifm:type!='Tunnel' and ../ifm:type!='LoopBack' and ../ifm:type!='ServiceIf' and ../ifm:type!='Virtual-if'"/>
    <description>
      <text>All configurations of the ND under the interface.</text>
    </description>
    <container name="nd-collection">
      <description>
        <text>Configure ND interface configurations.</text>
      </description>
      <container name="if-property">
        <description>
          <text>Configure ND interface configurations.</text>
        </description>
        <leaf name="retrans-timer">
          <when condition="../../../../ifm:type!='Serial'"/>
          <type name="uint32">
            <range value="1000..4294967295"/>
          </type>
          <units name="ms"/>
          <default value="1000"/>
          <description>
            <text>To set the retransmission timer of a router.</text>
          </description>
        </leaf>
        <leaf name="nud-reach-time">
          <when condition="../../../../ifm:type!='Ip-Trunk' and ../../../../ifm:type!='Pos' and ../../../../ifm:type!='Mp-group' and ../../../../ifm:type!='Serial'"/>
          <type name="uint32">
            <range value="1..3600000"/>
          </type>
          <units name="ms"/>
          <default value="1200000"/>
          <description>
            <text>To set the reachable time of a neighbor.</text>
          </description>
        </leaf>
        <leaf name="attempts-value">
          <when condition="../../../../ifm:type!='Serial'"/>
          <type name="uint32">
            <range value="0..600"/>
          </type>
          <default value="1"/>
          <description>
            <text>To set the number of sent NS messages during DAD.</text>
          </description>
        </leaf>
        <leaf name="ns-mcast-flag">
          <when condition="../../../../ethernet:ethernet/ethernet:l3-sub-interface/ethernet:dot1q-termination or ../../../../ethernet:ethernet/ethernet:l3-sub-interface/ethernet:qinq-termination"/>
          <type name="nd-switch-state"/>
          <default value="off"/>
          <description>
            <text>Flag of enabling multicast.</text>
          </description>
        </leaf>
        <leaf name="max-dyn-nb-num">
          <when condition="../../../../ifm:type!='Ip-Trunk' and ../../../../ifm:type!='Pos' and ../../../../ifm:type!='Mp-group' and ../../../../ifm:type!='Serial'"/>
          <type name="uint64">
            <range value="0..147456"/>
          </type>
          <default value="0"/>
          <description>
            <text>The maximum of limitation of dynamic neighbor.</text>
          </description>
        </leaf>
        <leaf name="dad-timeout">
          <when condition="../../../../ethernet:ethernet/ethernet:l3-sub-interface/ethernet:dot1q-termination or ../../../../ethernet:ethernet/ethernet:l3-sub-interface/ethernet:qinq-termination"/>
          <type name="uint32">
            <range value="1..60"/>
          </type>
          <units name="s"/>
          <default value="1"/>
          <description>
            <text>Timeout of DAD.</text>
          </description>
        </leaf>
        <leaf name="nud-attempts">
          <when condition="../../../../ifm:type!='Ip-Trunk' and ../../../../ifm:type!='Pos' and ../../../../ifm:type!='Mp-group' and ../../../../ifm:type!='Serial'"/>
          <type name="uint8">
            <range value="1..10"/>
          </type>
          <default value="3"/>
          <description>
            <text>ND entries NUD attempts.</text>
          </description>
        </leaf>
        <leaf name="na-glean">
          <when condition="../../../../ifm:type!='Ip-Trunk' and ../../../../ifm:type!='Pos' and ../../../../ifm:type!='Mp-group' and ../../../../ifm:type!='Serial'"/>
          <type name="nd-switch-state"/>
          <default value="off"/>
          <description>
            <text>Flag of generating NB when receiving NA packet.</text>
          </description>
        </leaf>
        <leaf name="ma-flag">
          <when condition="../../../../ifm:type!='MEth' and ../../../../ifm:type!='Serial'"/>
          <type name="nd-switch-state"/>
          <default value="off"/>
          <description>
            <text>Flag of obtaining the routable address through state-based automatic configuration.</text>
          </description>
        </leaf>
        <leaf name="o-flag">
          <when condition="../../../../ifm:type!='MEth' and ../../../../ifm:type!='Serial'"/>
          <type name="nd-switch-state"/>
          <default value="off"/>
          <description>
            <text>Flag of setting state-based automatic configuration.</text>
          </description>
        </leaf>
        <leaf name="ra-halt-flag">
          <when condition="../../../../ifm:type!='MEth'"/>
          <type name="nd-switch-state"/>
          <default value="on"/>
          <description>
            <text>To suppress the advertisement of RA packets.</text>
          </description>
        </leaf>
        <leaf name="max-interval">
          <when condition="../../../../ifm:type!='MEth'"/>
          <type name="uint32">
            <range value="4..1800"/>
          </type>
          <units name="s"/>
          <default value="600"/>
          <description>
            <text>The maximum interval between periodic RA packets sent on this interface.</text>
          </description>
        </leaf>
        <leaf name="min-interval">
          <when condition="../../../../ifm:type!='MEth'"/>
          <type name="uint32">
            <range value="3..1350"/>
          </type>
          <units name="s"/>
          <must condition="../max-interval &gt;= ../min-interval and ../min-interval &lt;= 0.75 * ../max-interval"/>
          <description>
            <text>The minimum interval between periodic RA packets sent on this interface.</text>
          </description>
        </leaf>
        <leaf name="ra-lifetime">
          <when condition="../../../../ifm:type!='MEth'"/>
          <type name="uint32">
            <range value="0|4..9000"/>
          </type>
          <units name="s"/>
          <must condition="../ra-lifetime = 0 or (../ra-lifetime != 0 and ../ra-lifetime &gt;= ../max-interval)"/>
          <description>
            <text>TTL of RA packets. If the value is 0, it implies that this node is not a router. And the value from 1 to 3 is invalid.</text>
          </description>
        </leaf>
        <leaf name="ra-preference">
          <when condition="../../../../ifm:type!='MEth'"/>
          <type name="nd-route-preference"/>
          <default value="medium"/>
          <description>
            <text>RA router preference.</text>
          </description>
        </leaf>
        <leaf name="ra-prefix-flag">
          <when condition="../../../../ifm:type!='MEth'"/>
          <type name="nd-switch-state"/>
          <default value="on"/>
          <description>
            <text>Flag of RA packet carry prefix information.</text>
          </description>
        </leaf>
        <leaf name="ra-mtu-flag">
          <when condition="../../../../ifm:type!='MEth'"/>
          <type name="nd-switch-state"/>
          <default value="on"/>
          <description>
            <text>Flag of RA packet carry MTU option.</text>
          </description>
        </leaf>
        <leaf name="strict-flag">
          <when condition="../../../../ifm:type!='Mp-group' and ../../../../ifm:type!='Serial'"/>
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable strict security mode.</text>
          </description>
        </leaf>
        <leaf name="ts-fuzz-factor">
          <when condition="../../../../ifm:type!='Serial'"/>
          <type name="uint16">
            <range value="0..1000"/>
          </type>
          <units name="s"/>
          <default value="1"/>
          <description>
            <text>Fuzz factor for timestamp option.</text>
          </description>
        </leaf>
        <leaf name="ts-clock-drift">
          <when condition="../../../../ifm:type!='Serial'"/>
          <type name="uint8">
            <range value="0..100"/>
          </type>
          <default value="1"/>
          <description>
            <text>Drift for timestamp option.</text>
          </description>
        </leaf>
        <leaf name="ts-delta">
          <when condition="../../../../ifm:type!='Serial'"/>
          <type name="uint16">
            <range value="0..1000"/>
          </type>
          <units name="s"/>
          <default value="300"/>
          <description>
            <text>Delta for timestamp option.</text>
          </description>
        </leaf>
        <leaf name="rsa-min-key-len">
          <when condition="../../../../ifm:type!='Serial'"/>
          <type name="uint16">
            <range value="384..4096"/>
          </type>
          <units name="bit"/>
          <must condition="(../rsa-min-key-len&lt;=../rsa-max-key-len)"/>
          <default value="512"/>
          <description>
            <text>The minimum length of acceptable RSA key pair.</text>
          </description>
        </leaf>
        <leaf name="rsa-max-key-len">
          <when condition="../../../../ifm:type!='Serial'"/>
          <type name="uint16">
            <range value="384..4096"/>
          </type>
          <units name="bit"/>
          <default value="2048"/>
          <description>
            <text>The maximum length of acceptable RSA key pair.</text>
          </description>
        </leaf>
        <leaf name="stale-time">
          <when condition="../../../../ifm:type!='Ip-Trunk' and ../../../../ifm:type!='Pos' and ../../../../ifm:type!='Mp-group' and ../../../../ifm:type!='Serial'"/>
          <type name="uint32">
            <range value="60..172800"/>
          </type>
          <units name="s"/>
          <description>
            <text>Aging time of the stale state.</text>
          </description>
        </leaf>
        <leaf name="collect-host-flag">
          <when condition="../../../../ifm:type='Vbdif'"/>
          <type name="nd-switch-state"/>
          <default value="off"/>
          <description>
            <text>Flag of collecting host Information.</text>
          </description>
        </leaf>
        <leaf name="generate-rd-enable">
          <when condition="../../../../ifm:type='Vbdif'"/>
          <type name="nd-switch-state"/>
          <default value="off"/>
          <description>
            <text>Flag of generating redirect ND table.</text>
          </description>
        </leaf>
        <leaf name="nud-interval">
          <when condition="../../../../ifm:type!='Ip-Trunk' and ../../../../ifm:type!='Pos' and ../../../../ifm:type!='Mp-group' and ../../../../ifm:type!='Serial'"/>
          <type name="uint32">
            <range value="1000..4294967295"/>
          </type>
          <units name="ms"/>
          <default value="5000"/>
          <description>
            <text>ND entries NUD interval.</text>
          </description>
        </leaf>
        <leaf name="hop-limit">
          <type name="uint8">
            <range value="0..255"/>
          </type>
          <description>
            <text>IPv6 packet hop limit.</text>
          </description>
        </leaf>
        <leaf name="link-down-trigger-del-flag">
          <when condition="../../../../ifm:type='Vlanif'"/>
          <type name="nd-switch-state"/>
          <default value="off"/>
          <description>
            <text>Flag of link down trigger fast deletion of ND entries.</text>
          </description>
        </leaf>
      </container>
      <container name="proxys">
        <when condition="../../../ifm:type!='MEth' and ../../../ifm:type!='Ip-Trunk' and ../../../ifm:type!='Serial' and ../../../ifm:type!='Pos' and ../../../ifm:type!='Mp-group'"/>
        <description>
          <text>Configure list of ND proxy.</text>
        </description>
        <leaf name="route-proxy">
          <when condition="../../../../ifm:type!='PW-VE'"/>
          <type name="nd-switch-state"/>
          <default value="off"/>
          <description>
            <text>Router proxy.</text>
          </description>
        </leaf>
        <leaf name="inner-vlan-proxy">
          <when condition="(../../../../ifm:class='sub-interface' and ../../../../ifm:type!='PW-VE') or ../../../../ifm:type='Vlanif'"/>
          <type name="nd-switch-state"/>
          <default value="off"/>
          <description>
            <text>InnerVLAN proxy.</text>
          </description>
        </leaf>
        <leaf name="inter-vlan-proxy">
          <when condition="(../../../../ifm:class='sub-interface' and ../../../../ifm:type!='PW-VE') or ../../../../ifm:type='Vlanif'"/>
          <type name="nd-switch-state"/>
          <default value="off"/>
          <description>
            <text>InterVLAN proxy.</text>
          </description>
        </leaf>
        <leaf name="anyway-proxy">
          <type name="nd-switch-state"/>
          <default value="off"/>
          <description>
            <text>Anyway proxy.</text>
          </description>
        </leaf>
        <leaf name="local-proxy">
          <when condition="../../../../ifm:type='Vbdif'"/>
          <type name="nd-switch-state"/>
          <default value="off"/>
          <description>
            <text>Local proxy.</text>
          </description>
        </leaf>
      </container>
      <container name="anti-attack">
        <when condition="../../../ifm:type!='Serial'"/>
        <description>
          <text>Configure ND anti-attack for interface.</text>
        </description>
        <container name="receive-rate-limits">
          <description>
            <text>List of rate limits for receiving ND packets.</text>
          </description>
          <list name="receive-rate-limit">
            <key value="type"/>
            <description>
              <text>Configure the rate limit for receiving ND packet or ND miss.</text>
            </description>
            <leaf name="type">
              <type name="nd-pkt-evt-type"/>
              <description>
                <text>ND packet type or ND miss.</text>
              </description>
            </leaf>
            <leaf name="value">
              <type name="uint16">
                <range value="0..5000"/>
              </type>
              <units name="pps"/>
              <mandatory value="true"/>
              <description>
                <text>Rate limit of receiving ND packet or ND miss.</text>
              </description>
            </leaf>
          </list>
        </container>
        <container name="source-ip-receive">
          <description>
            <text>Configure the ND anti-attack based on packet or miss source IP address.</text>
          </description>
          <uses name="nd-ip-rate-limits-type"/>
        </container>
      </container>
      <container name="static-neighbor">
        <description>
          <text>Configure static neighbor entry.</text>
        </description>
        <container name="common-ifs">
          <when condition="../../../../ifm:type!='Vlanif' and ../../../../ifm:type!='Vbdif'"/>
          <description>
            <text>List of static neighbor entry on common interface.</text>
          </description>
          <list name="common-if">
            <key value="ipv6-addr"/>
            <description>
              <text>Configure static neighbor entry on common interface.</text>
            </description>
            <leaf name="ipv6-addr">
              <type name="inet:ipv6-address-no-zone"/>
              <description>
                <text>IPv6 address of a static neighbor entry.</text>
              </description>
            </leaf>
            <leaf name="mac-addr">
              <type name="pub-type:mac-address"/>
              <mandatory value="true"/>
              <description>
                <text>MAC address.</text>
              </description>
            </leaf>
          </list>
        </container>
        <container name="dot1q-termination-ifs">
          <when condition="../../../../ethernet:ethernet/ethernet:l3-sub-interface/ethernet:dot1q-termination"/>
          <description>
            <text>List of static neighbor entry on dot1q termination interface.</text>
          </description>
          <list name="dot1q-termination-if">
            <key value="ipv6-addr"/>
            <description>
              <text>Configure static neighbor entry on dot1q termination interface.</text>
            </description>
            <leaf name="ipv6-addr">
              <type name="inet:ipv6-address-no-zone"/>
              <description>
                <text>IPv6 address of a static neighbor entry.</text>
              </description>
            </leaf>
            <leaf name="mac-addr">
              <type name="pub-type:mac-address">
                <pattern value="[0-9a-fA-F][0|2|4|6|8|aA|cC|eE][0-9a-fA-F]{2}(-[0-9a-fA-F]{4}){2}"/>
              </type>
              <mandatory value="true"/>
              <description>
                <text>MAC address.</text>
              </description>
            </leaf>
            <leaf name="pe-vlan">
              <type name="uint16">
                <range value="1..4094"/>
              </type>
              <mandatory value="true"/>
              <description>
                <text>Outer VLAN tag.</text>
              </description>
            </leaf>
          </list>
        </container>
        <container name="qinq-termination-ifs">
          <when condition="../../../../ethernet:ethernet/ethernet:l3-sub-interface/ethernet:qinq-termination"/>
          <description>
            <text>List of static neighbor entry on qinq termination interface.</text>
          </description>
          <list name="qinq-termination-if">
            <key value="ipv6-addr"/>
            <description>
              <text>Configure static neighbor entry on qinq termination interface.</text>
            </description>
            <leaf name="ipv6-addr">
              <type name="inet:ipv6-address-no-zone"/>
              <description>
                <text>IPv6 address of a static neighbor entry.</text>
              </description>
            </leaf>
            <leaf name="mac-addr">
              <type name="pub-type:mac-address">
                <pattern value="[0-9a-fA-F][0|2|4|6|8|aA|cC|eE][0-9a-fA-F]{2}(-[0-9a-fA-F]{4}){2}"/>
              </type>
              <mandatory value="true"/>
              <description>
                <text>MAC address.</text>
              </description>
            </leaf>
            <leaf name="pe-vlan">
              <type name="uint16">
                <range value="1..4094"/>
              </type>
              <mandatory value="true"/>
              <description>
                <text>Outer VLAN tag.</text>
              </description>
            </leaf>
            <leaf name="ce-vlan">
              <type name="uint16">
                <range value="1..4094"/>
              </type>
              <mandatory value="true"/>
              <description>
                <text>Inner VLAN tag.</text>
              </description>
            </leaf>
          </list>
        </container>
        <container name="vlan-ifs">
          <when condition="../../../../ifm:type='Vlanif'"/>
          <description>
            <text>List of static neighbor entry on VLAN interface.</text>
          </description>
          <list name="vlan-if">
            <key value="ipv6-addr"/>
            <description>
              <text>Configure static neighbor entry on VLAN interface.</text>
            </description>
            <leaf name="ipv6-addr">
              <type name="inet:ipv6-address-no-zone"/>
              <description>
                <text>IPv6 address of a static neighbor entry.</text>
              </description>
            </leaf>
            <leaf name="mac-addr">
              <type name="pub-type:mac-address">
                <pattern value="[0-9a-fA-F][0|2|4|6|8|aA|cC|eE][0-9a-fA-F]{2}(-[0-9a-fA-F]{4}){2}"/>
              </type>
              <mandatory value="true"/>
              <description>
                <text>MAC address.</text>
              </description>
            </leaf>
            <leaf name="vlan-id">
              <type name="uint16">
                <range value="1..4094"/>
              </type>
              <mandatory value="true"/>
              <description>
                <text>VLAN ID.</text>
              </description>
              <ext:support-filter value="true"/>
            </leaf>
            <leaf name="port-name">
              <type name="leafref">
                <path value="/ifm:ifm/ifm:interfaces/ifm:interface/ifm:name"/>
              </type>
              <mandatory value="true"/>
              <description>
                <text>Port name.</text>
              </description>
            </leaf>
          </list>
        </container>
      </container>
      <container name="send-rate-limit">
        <when condition="../../../ifm:type!='Serial'"/>
        <description>
          <text>Configure ND send rate limit for interface.</text>
        </description>
        <container name="packet-rate-limits">
          <description>
            <text>List of ND packet sending rate limits.</text>
          </description>
          <list name="packet-rate-limit">
            <key value="packet-type"/>
            <description>
              <text>Configure the rate limit of sending ND packet.</text>
            </description>
            <leaf name="packet-type">
              <type name="nd-pkt-type"/>
              <must condition="not((../packet-type = 'RA') and (../../../../../../ifm:type='MEth'))"/>
              <description>
                <text>ND packet type.</text>
              </description>
            </leaf>
            <leaf name="value">
              <type name="uint16">
                <range value="1..10240"/>
              </type>
              <units name="pps"/>
              <must condition="((../packet-type = 'NA') and (../value &lt;= 10240)) or (not(../packet-type = 'NA') and (../value &lt;= 1000))"/>
              <mandatory value="true"/>
              <description>
                <text>Rate limit of sending ND packet, when the packet type is NS, RS or RA, the maximum value is 1000.</text>
              </description>
            </leaf>
          </list>
        </container>
        <container name="multicast-rate-limits">
          <description>
            <text>List of ND multicast packet sending rate limits.</text>
          </description>
          <list name="multicast-rate-limit">
            <key value="packet-type"/>
            <description>
              <text>Configure the rate limit of sending ND multicast packet.</text>
            </description>
            <leaf name="packet-type">
              <type name="nd-pkt-type"/>
              <must condition="not((../packet-type = 'RA') and (../../../../../../ifm:type='MEth'))"/>
              <description>
                <text>ND packet type.</text>
              </description>
            </leaf>
            <leaf name="value">
              <type name="uint16">
                <range value="1..10240"/>
              </type>
              <units name="pps"/>
              <must condition="((../packet-type = 'NA') and (../value &lt;= 10240)) or (not(../packet-type = 'NA') and (../value &lt;= 1000))"/>
              <mandatory value="true"/>
              <description>
                <text>Rate limit of sending ND multicast packet, when the packet type is NS, RS or RA, the maximum value is 1000.</text>
              </description>
            </leaf>
          </list>
        </container>
      </container>
      <container name="ra-property">
        <when condition="../../../ifm:type!='MEth'"/>
        <description>
          <text>Configure ND RA configurations under the interface.</text>
        </description>
        <container name="ra-control">
          <description>
            <text>Configure ND RA configurations.</text>
          </description>
          <leaf name="unicast-send">
            <type name="nd-switch-state"/>
            <default value="off"/>
            <description>
              <text>Flag of Sending RA unicast packet when receive a RS packet.</text>
            </description>
          </leaf>
        </container>
        <container name="ra-routes">
          <description>
            <text>List of RA route information option.</text>
          </description>
          <list name="ra-route">
            <key value="prefix-addr prefix-len"/>
            <description>
              <text>Configure RA route information option.</text>
            </description>
            <leaf name="prefix-addr">
              <type name="inet:ipv6-address-no-zone"/>
              <description>
                <text>The IPv6 prefix address of RA route information.</text>
              </description>
            </leaf>
            <leaf name="prefix-len">
              <type name="uint32">
                <range value="0..128"/>
              </type>
              <description>
                <text>The prefix length of RA route information.</text>
              </description>
            </leaf>
            <leaf name="lifetime">
              <type name="uint32"/>
              <units name="s"/>
              <mandatory value="true"/>
              <description>
                <text>The lifetime of RA route information.</text>
              </description>
            </leaf>
            <leaf name="preference">
              <type name="nd-route-preference"/>
              <default value="medium"/>
              <description>
                <text>The preference of RA route information.</text>
              </description>
            </leaf>
          </list>
        </container>
        <container name="dns-server-addresses">
          <description>
            <text>List of DNS server address.</text>
          </description>
          <list name="dns-server-address">
            <key value="ipv6-address"/>
            <description>
              <text>Configure the DNS server address.</text>
            </description>
            <leaf name="ipv6-address">
              <type name="inet:ipv6-address-no-zone"/>
              <description>
                <text>Address of IPv6 recursive DNS server. The address cannot be a multicast address or an unspecified address.</text>
              </description>
            </leaf>
            <leaf name="lifetime">
              <type name="uint32">
                <range value="1..max"/>
              </type>
              <units name="s"/>
              <description>
                <text>The maximum time over which this RDNSS address may be used for name resolution.</text>
              </description>
            </leaf>
          </list>
        </container>
        <container name="dns-search-suffixes">
          <description>
            <text>List of DNS search list.</text>
          </description>
          <list name="dns-search-suffix">
            <key value="suffix-name"/>
            <description>
              <text>Configure DNS suffix name.</text>
            </description>
            <leaf name="suffix-name">
              <type name="string">
                <length value="1..63"/>
              </type>
              <ext:case-sensitivity value="lower-or-upper"/>
              <description>
                <text>Domain name of DNS search list.</text>
              </description>
            </leaf>
            <leaf name="lifetime">
              <type name="uint32">
                <range value="1..max"/>
              </type>
              <units name="s"/>
              <description>
                <text>The maximum time over which this DNSSL domain name may be used for name resolution.</text>
              </description>
            </leaf>
          </list>
        </container>
      </container>
      <container name="strict-prefix-learn">
        <when condition="../../../ifm:type!='Vlanif' and ../../../ifm:type!='Vbdif' and ../../../ifm:type!='Global-VE'"/>
        <description>
          <text>Configure strict prefix learning of interface.</text>
        </description>
        <leaf name="switch">
          <type name="nd-switch-state"/>
          <description>
            <text>ND prefix learning strict.</text>
          </description>
        </leaf>
      </container>
    </container>
  </augment>
  <augment target-node="/ifm:ifm/ifm:interfaces/ifm:interface">
    <description>
      <text>List of ND configurations under ifm interface.</text>
    </description>
    <container name="nd-dyn-nb-limit">
      <description>
        <text>Configure the maximum of limitation of dynamic neighbor.</text>
      </description>
      <container name="vlan-limits">
        <description>
          <text>List of the maximum of limitation of dynamic neighbor based on VLAN.</text>
        </description>
        <list name="vlan-limit">
          <key value="vlan-id"/>
          <description>
            <text>Configure the maximum of limitation of dynamic neighbor based on VLAN.</text>
          </description>
          <leaf name="vlan-id">
            <type name="uint16">
              <range value="1..4094"/>
            </type>
            <description>
              <text>VLAN ID.</text>
            </description>
          </leaf>
          <leaf name="max-dyn-nb-num">
            <type name="uint32">
              <range value="0..147456"/>
            </type>
            <mandatory value="true"/>
            <description>
              <text>The maximum of limitation of dynamic neighbor.</text>
            </description>
          </leaf>
        </list>
      </container>
    </container>
  </augment>
  <ext:task-name value="nd"/>
</module>
