  <container name="configuration">
    <description>
      <text>Configuration.</text>
    </description>
    <container name="dhcp">
      <description>
        <text>DHCP config.</text>
      </description>
      <container name="global">
        <description>
          <text>DHCP global common config.</text>
        </description>
        <leaf name="enable">
          <description>
            <text>Enable DHCP process.</text>
          </description>
          <type name="enumeration">
            <enum name="disable">
              <value value="0"/>
              <description>
                <text>DHCP disable.</text>
              </description>
            </enum>
            <enum name="enable">
              <value value="1"/>
              <description>
                <text>DHCP enable.</text>
              </description>
            </enum>
          </type>
          <default value="disable"/>
        </leaf>
        <leaf name="ramble">
          <description>
            <text>Enable DHCP user ramble.</text>
          </description>
          <type name="enumeration">
            <enum name="ramble-off">
              <value value="0"/>
              <description>
                <text>Turn off DHCP ramble.</text>
              </description>
            </enum>
            <enum name="ramble-on">
              <value value="1"/>
              <description>
                <text>Turn on DHCP ramble.</text>
              </description>
            </enum>
          </type>
          <default value="ramble-off"/>
        </leaf>
        <leaf name="check-option82">
          <description>
            <text>Enable or disable check option82 in request packet.</text>
          </description>
          <type name="enumeration">
            <enum name="off">
              <value value="0"/>
              <description>
                <text>Turn off DHCP check option82.</text>
              </description>
            </enum>
            <enum name="on">
              <value value="1"/>
              <description>
                <text>Turn on DHCP check option82.</text>
              </description>
            </enum>
          </type>
          <default value="off"/>
          <ncd:revise-date date="2016-12-20"/>
        </leaf>
        <leaf name="max-hops">
          <description>
            <text>Max hops of DHCP message.[1,16].</text>
          </description>
          <type name="uint32">
            <range value="1..16"/>
          </type>
          <default value="4"/>
        </leaf>
        <leaf name="suppress-nak">
          <description>
            <text>Suppress DHCPNAK message.</text>
          </description>
          <type name="enumeration">
            <enum name="suppress-nak-off">
              <value value="0"/>
              <description>
                <text>Turn off suppress-nak.</text>
              </description>
            </enum>
            <enum name="suppress-nak-on">
              <value value="1"/>
              <description>
                <text>Turn on suppress-nak.</text>
              </description>
            </enum>
          </type>
          <default value="suppress-nak-off"/>
        </leaf>
        <leaf name="log">
          <description>
            <text>Log on or off.</text>
          </description>
          <type name="enumeration">
            <enum name="off">
              <value value="0"/>
              <description>
                <text>Turn off log.</text>
              </description>
            </enum>
            <enum name="on">
              <value value="1"/>
              <description>
                <text>Turn on log.</text>
              </description>
            </enum>
          </type>
          <default value="off"/>
          <ncd:revise-date date="2016-12-20"/>
        </leaf>
        <leaf name="force-reply-broadcast">
          <description>
            <text>Suppress force reply broadcast packet.</text>
          </description>
          <type name="enumeration">
            <enum name="off">
              <value value="0"/>
              <description>
                <text>Turn off force reply broadcast.</text>
              </description>
            </enum>
            <enum name="on">
              <value value="1"/>
              <description>
                <text>Turn on force reply broadcast.</text>
              </description>
            </enum>
          </type>
          <default value="off"/>
          <ncd:revise-date date="2016-12-20"/>
        </leaf>
        <container name="upgrade-leasetime">
          <description>
            <text>DHCP upgrade leasetime</text>
          </description>
          <presence value="upgrade-leasetime"/>
          <leaf name="lease-days">
            <description>
              <text>DHCP upgrade leasetime days.</text>
            </description>
            <type name="uint16">
              <range value="0..365"/>
            </type>
            <mandatory value="true"/>
          </leaf>
          <leaf name="lease-hours">
            <description>
              <text>DHCP upgrade leasetime hours.</text>
            </description>
            <type name="uint8">
              <range value="0..23"/>
            </type>
            <mandatory value="true"/>
          </leaf>
