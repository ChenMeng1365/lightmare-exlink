                    <units name="Byte"/>
                    <ext:dynamic-default>
                      <ext:default-value value="../cir">
                        <when condition="../cir&gt;100"/>
                        <description>
                          <text>The default value is the value of cir when cir is greater than the minimum of cbs.</text>
                        </description>
                      </ext:default-value>
                      <ext:default-value value="100">
                        <when condition="../cir&lt;=100"/>
                        <description>
                          <text>The default value is the minimum of cbs when cir is less than or equal to the minimum of cbs.</text>
                        </description>
                      </ext:default-value>
                    </ext:dynamic-default>
                    <description>
                      <text>Committed burst size.</text>
                    </description>
                  </leaf>
                </list>
              </container>
            </case>
          </choice>
        </container>
      </container>
      <container name="mac-filter">
        <must condition="./whitelist-enable != 'true' or ./blacklist-enable != 'true'"/>
        <description>
          <text>Configure a filtering policy for the MAC-based blacklist or whitelist function.</text>
        </description>
        <leaf name="whitelist-enable">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable the MAC-based whitelist function.</text>
          </description>
        </leaf>
        <leaf name="blacklist-enable">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable the MAC-based blacklist function.</text>
          </description>
        </leaf>
        <container name="mac-addresss">
          <description>
            <text>List of specifies a MAC address.</text>
          </description>
          <list name="mac-address">
            <key value="mac"/>
            <max-elements value="256"/>
            <description>
              <text>Configure specifies a MAC address. A maximum of 256 MAC addresses can be specified for the MAC-based blacklist or whitelist function on a device.</text>
            </description>
            <leaf name="mac">
              <type name="pub-type:mac-address"/>
              <description>
                <text>MAC address.</text>
              </description>
            </leaf>
          </list>
        </container>
      </container>
    </container>
  </augment>
</module>
