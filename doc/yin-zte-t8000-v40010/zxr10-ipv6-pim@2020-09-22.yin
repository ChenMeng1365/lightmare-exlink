  <feature name="keepalive-period">
    <description>
      <text>Only for the use of 15K PC</text>
    </description>
  </feature>
  <feature name="pim-statistics">
    <description>
      <text>Only for the use of T8000, PC and M6000.</text>
    </description>
  </feature>
  <feature name="hello-track-support">
    <description>
      <text>IPv6 PIM hello packet sended with the T bit in the LAN Prune Delay option, this indicates the neighbor's capability to disable Join message suppression.</text>
    </description>
  </feature>
  <feature name="trust-reg-source">
    <description>
      <text>Only for the use of T8000, 15K and PC.</text>
    </description>
  </feature>
  <container name="configuration">
    <description>
      <text>Configuration model.</text>
    </description>
    <container name="ipv6-router-pim">
      <description>
        <text>Configure IPv6 PIM</text>
      </description>
      <container name="vrf-pim6s">
        <description>
          <text>Configure VRF instance of IPv6 PIM</text>
        </description>
        <list name="vrf-pim6">
          <description>
            <text>IPv6 Protocol Independent Multicast (IPv6 PIM)</text>
          </description>
          <key value="vrf-name"/>
          <leaf name="vrf-name">
            <description>
              <text>VPN Routing/Forwarding instance name</text>
            </description>
            <type name="string">
              <length value="1..32">
                <description>
                  <text>VPN Routing/Forwarding instance name</text>
                </description>
              </length>
            </type>
          </leaf>
          <leaf name="erp-disable">
            <description>
              <text>IPv6 PIM Embedded-RP disable</text>
            </description>
            <type name="empty"/>
          </leaf>
          <leaf name="spt-infinity">
            <description>
              <text>Never switch</text>
            </description>
            <type name="empty"/>
          </leaf>
          <leaf name="accept-rp-acl">
            <description>
              <text>IPv6 PIM RP accept filter</text>
            </description>
            <type name="string">
              <length value="1..31">
                <description>
                  <text>Access list name (1-31 characters)</text>
                </description>
              </length>
            </type>
          </leaf>
          <leaf name="reg-filter-acl">
            <description>
              <text>IPv6 PIM registers accept filter</text>
            </description>
            <type name="string">
              <length value="1..31">
                <description>
                  <text>Access list name (1-31 characters)</text>
                </description>
              </length>
            </type>
          </leaf>
          <leaf name="data-filter-acl">
            <description>
              <text>IPv6 PIM data filter</text>
            </description>
            <type name="string">
              <length value="1..31">
                <description>
                  <text>Access list name (1-31 characters)</text>
                </description>
              </length>
            </type>
          </leaf>
          <leaf name="arp-local-addr">
            <description>
              <text>IPv6 PIM anycast-RP local address</text>
            </description>
            <type name="ncd:ipv6-address">
              <pattern value="([0-9a-fA-F]{1,4}:){7}([0-9a-fA-F]{0,4})$|^([0-9a-fA-F]{1,4}:){1,6}(:[0-9a-fA-F]{0,4}){1,6}$|^([:]{2})(([0-9a-fA-F]{1,4}:){0,6})([0-9a-fA-F]{1,4})$|^([0-9a-fA-F]{1,4}:){6}([0-9]{1,3}.){3}([0-9]{1,3})$|^([0-9a-fA-F]{1,4}:){1,5}(:[0-9a-fA-F]{0,4}){1,5}([0-9]{1,3}.){3}([0-9]{1,3})$|^([:]{2})(([0-9a-fA-F]{1,4}:){0,5})([0-9]{1,3}.){3}([0-9]{1,3})"/>
            </type>
          </leaf>
          <leaf name="jp-holdtime">
            <description>
              <text>IPv6 PIM router join-prune holdtime</text>
            </description>
            <type name="uint16">
              <range value="210..65535">
                <description>
                  <text>Join-prune holdtime in seconds</text>
                </description>
              </range>
            </type>
            <units name="second"/>
            <default value="210"/>
          </leaf>
          <leaf name="reg-suppression-interval">
            <description>
              <text>IPv6 PIM router register suppression interval</text>
            </description>
            <type name="uint16">
              <range value="60..3600">
                <description>
                  <text>Register suppression interval in seconds</text>
                </description>
              </range>
            </type>
            <units name="second"/>
            <default value="60"/>
          </leaf>
          <leaf name="reg-probe-interval">
            <description>
              <text>IPv6 PIM router register probe interval</text>
            </description>
            <type name="uint16">
              <range value="1..210">
                <description>
                  <text>Register probe interval in seconds</text>
                </description>
              </range>
            </type>
            <units name="second"/>
            <default value="5"/>
          </leaf>
          <leaf name="bsm-unicast">
            <description>
              <text>IPv6 PIM unicast bootstrap messages</text>
            </description>
            <type name="empty"/>
          </leaf>
          <leaf name="srp-override">
            <description>
              <text>IPv6 PIM static RP override</text>
            </description>
            <type name="empty"/>
          </leaf>
          <leaf name="bidir-pim">
            <description>
              <text>Enable IPv6 BIDIR-PIM</text>
            </description>
            <type name="empty"/>
          </leaf>
          <leaf name="jp-interval">
            <description>
              <text>IPv6 PIM router join-prune interval</text>
            </description>
            <type name="uint16">
              <range value="10..6000">
                <description>
                  <text>Join prune interval in seconds</text>
                </description>
              </range>
            </type>
            <units name="second"/>
            <default value="60"/>
          </leaf>
          <leaf name="reg-holdtime">
            <description>
              <text>IPv6 PIM router register-state holdtime without receiving register stop packet</text>
            </description>
            <type name="uint16">
              <range value="1..600">
                <description>
                  <text>Register holdtime without receiving register stop in seconds</text>
                </description>
              </range>
            </type>
            <units name="second"/>
          </leaf>
          <leaf name="assert-holdtime">
            <description>
              <text>configure assert holdtime</text>
            </description>
            <type name="uint16">
              <range value="60..65535"/>
            </type>
            <units name="second"/>
            <default value="180"/>
          </leaf>
          <leaf name="graft-retry-time">
            <description>
              <text>configure graft retry time</text>
            </description>
            <type name="uint16">
              <range value="1..65535"/>
            </type>
            <units name="second"/>
            <default value="3"/>
          </leaf>
          <leaf name="keepalive-period">
            <description>
              <text>The configuration of keepalive period. It's validated minimum may be related to the performance of the  certain product.</text>
            </description>
            <type name="uint16">
              <range value="60..65535"/>
            </type>
            <units name="second"/>
            <default value="210"/>
          </leaf>
          <leaf name="join-prune-speed">
            <description>
              <text>configure join prune speed</text>
            </description>
            <type name="uint16">
              <range value="1000..50000"/>
            </type>
            <default value="10000"/>
          </leaf>
          <leaf name="cbsr-holdtime">
            <description>
              <text>PIM router candidate BSR holdtime</text>
            </description>
            <type name="uint16">
              <range value="130..65535"/>
            </type>
            <units name="second"/>
            <default value="130"/>
            <ncd:revise-date date="2015-06-01"/>
          </leaf>
          <leaf name="cbsr-interval">
            <description>
              <text>PIM router candidate BSR interval</text>
            </description>
            <type name="uint16">
              <range value="10..65535"/>
            </type>
            <units name="second"/>
            <default value="60"/>
            <ncd:revise-date date="2015-06-01"/>
          </leaf>
          <leaf name="mofrr-filter-acl">
            <description>
              <text>IPv6 PIM MoFRR filter</text>
            </description>
            <type name="string">
              <length value="1..31"/>
            </type>
            <ncd:revise-date date="2015-10-26"/>
          </leaf>
          <leaf name="pim-statistics-acl">
            <description>
              <text>IPv6 PIM packet statistics filter.</text>
            </description>
            <type name="string">
              <length value="1..31"/>
            </type>
            <ncd:revise-date date="2016-03-18"/>
          </leaf>
          <leaf name="dr-switch-delay-value">
            <description>
              <text>PIM DR switch back delay</text>
            </description>
            <type name="uint16">
              <range value="1..300"/>
            </type>
            <units name="second"/>
            <ncd:revise-date date="2019-07-15"/>
          </leaf>
          <leaf name="rp-proxy">
            <description>
              <text>PIM RP proxy</text>
            </description>
            <type name="empty"/>
            <ncd:revise-date date="2019-07-15"/>
          </leaf>
          <leaf name="srp-smartswitch">
            <description>
              <text>PIM RP smart switch</text>
            </description>
            <type name="empty"/>
            <ncd:revise-date date="2019-07-15"/>
          </leaf>
          <leaf name="reg-data-forward">
            <description>
              <text>PIM router register data forward.</text>
            </description>
            <type name="empty"/>
            <ncd:revise-date date="2019-07-15"/>
          </leaf>
          <leaf name="bsr-policy-acl">
            <description>
              <text>PIM BSR filter.</text>
            </description>
            <type name="string">
              <length value="1..31"/>
            </type>
            <ncd:revise-date date="2019-07-15"/>
          </leaf>
          <leaf name="interface-identifier">
            <description>
              <text>Enable interface identifier option.</text>
            </description>
            <type name="empty"/>
            <ncd:revise-date date="2019-08-29"/>
          </leaf>
          <leaf name="ecmp-redirect">
            <description>
              <text>Enable ECMP redirect.</text>
            </description>
            <type name="empty"/>
            <ncd:revise-date date="2019-08-29"/>
          </leaf>
          <container name="ssm-ranges">
            <description>
              <text>IPv6 PIM SSM range</text>
            </description>
            <choice name="ssm-range">
              <description>
                <text>IPv6 PIM ssm range</text>
              </description>
              <case name="ssm-default">
                <description>
                  <text>Use FF3X::/32 group range for SSM</text>
                </description>
                <leaf name="default">
                  <description>
                    <text>Use FF3X::/32 group range for SSM</text>
                  </description>
                  <type name="empty"/>
                </leaf>
              </case>
              <case name="ssm-acl">
                <description>
                  <text>ACL for group range to be used for SSM</text>
                </description>
                <leaf name="acl">
                  <description>
                    <text>ACL for group range to be used for SSM</text>
                  </description>
                  <type name="string">
                    <length value="1..31"/>
                  </type>
                </leaf>
              </case>
            </choice>
          </container>
          <container name="intfs">
            <description>
              <text>IPv6 PIM interface</text>
            </description>
            <list name="intf">
              <description>
                <text>IPv6 PIM interface</text>
              </description>
              <key value="intf-name"/>
              <leaf name="intf-name">
                <description>
                  <text>Interface name</text>
                </description>
                <type name="ncd:ifname">
                  <length value="1..31"/>
                </type>
              </leaf>
              <leaf name="pimsm">
                <description>
                  <text>Enable IPv6 PIM-SM operation on the interface</text>
                </description>
                <type name="empty"/>
              </leaf>
              <leaf name="pimdm">
                <description>
                  <text>Enable IPv6 PIM-DM operation on the interface</text>
                </description>
                <type name="empty"/>
                <ncd:revise-date date="2019-06-25"/>
              </leaf>
              <leaf name="bsr-border">
                <description>
                  <text>Border of IPv6 PIM domain</text>
                </description>
                <type name="empty"/>
              </leaf>
              <leaf name="hello-interval">
                <description>
                  <text>IPv6 PIM router hello interval</text>
                </description>
                <type name="uint16">
                  <range value="1..65535"/>
                </type>
                <units name="second"/>
                <default value="30"/>
              </leaf>
              <leaf name="dr-priority">
                <description>
                  <text>IPv6 PIM router DR priority</text>
                </description>
                <type name="uint32">
                  <range value="0..4294967295"/>
                </type>
                <default value="1"/>
              </leaf>
              <leaf name="nbr-filter-acl">
                <description>
                  <text>IPv6 PIM peering filter</text>
                </description>
                <type name="string">
