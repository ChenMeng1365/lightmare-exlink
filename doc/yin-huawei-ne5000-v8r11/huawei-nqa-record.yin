          </description>
        </leaf>
        <leaf name="status">
          <type name="test-finsh-status-type"/>
          <description>
            <text>Test finished status.</text>
          </description>
        </leaf>
        <leaf name="completions">
          <type name="result-status-type"/>
          <description>
            <text>Running results.</text>
          </description>
        </leaf>
        <leaf name="disconnects">
          <type name="uint32">
            <range value="0..65535"/>
          </type>
          <description>
            <text>Number of consecutive failures.</text>
          </description>
        </leaf>
        <leaf name="test-attempts">
          <type name="uint32">
            <range value="1..100000"/>
          </type>
          <description>
            <text>Attempts times (the value is 1).</text>
          </description>
        </leaf>
        <leaf name="timeouts">
          <type name="uint32">
            <range value="0..65535"/>
          </type>
          <units name="s"/>
          <description>
            <text>Timeout period.</text>
          </description>
        </leaf>
        <leaf name="busies">
          <type name="uint32">
            <range value="0..65535"/>
          </type>
          <description>
            <text>Number of times that system is busy.</text>
          </description>
        </leaf>
        <leaf name="no-connections">
          <type name="uint32">
            <range value="0..65535"/>
          </type>
          <description>
            <text>Number of connection errors.</text>
          </description>
        </leaf>
        <leaf name="sequence-errors">
          <type name="uint32">
