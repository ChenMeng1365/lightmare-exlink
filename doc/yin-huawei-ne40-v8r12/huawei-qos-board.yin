            <type name="qos:qos-service-class"/>
            <description>
              <text>Class of service.</text>
            </description>
          </leaf>
          <choice name="discard-type">
            <mandatory value="true"/>
            <description>
              <text>Specify discard type.</text>
            </description>
            <case name="bytes">
              <description>
                <text>Specify discard type choice case as discard-byte.</text>
              </description>
              <leaf name="bytes">
                <type name="uint32">
                  <range value="1..4294967295"/>
                </type>
                <units name="Byte"/>
                <mandatory value="true"/>
                <description>
                  <text>The discarded bytes.</text>
                </description>
              </leaf>
            </case>
            <case name="packets">
              <description>
                <text>Specify discard type choice case as discard packet.</text>
              </description>
              <leaf name="packets">
                <type name="uint32">
                  <range value="1..4294967295"/>
                </type>
                <units name="packet"/>
                <mandatory value="true"/>
                <description>
                  <text>The discarded packets.</text>
                </description>
              </leaf>
            </case>
            <case name="packet-ratio">
              <description>
                <text>Specify discard type choice case as discard packet ratio.</text>
              </description>
              <leaf name="packet-coefficient">
                <type name="uint8">
                  <range value="1..9"/>
                </type>
                <mandatory value="true"/>
                <description>
                  <text>Ratio coefficient of the discard packets.</text>
                </description>
              </leaf>
              <leaf name="packet-exponent">
                <type name="uint8">
                  <range value="1..5"/>
                </type>
                <mandatory value="true"/>
                <description>
                  <text>Ratio exponent of the discard packets.</text>
                </description>
              </leaf>
            </case>
          </choice>
          <leaf name="interval-time">
            <type name="uint16">
              <range value="15..3600"/>
            </type>
            <units name="s"/>
            <default value="30"/>
            <description>
              <text>Monitoring time of the queue discard alarm.</text>
            </description>
          </leaf>
        </list>
      </container>
      <container name="user-queue-burst">
        <presence value="create user-queue-burst"/>
        <description>
          <text>Enable/disable the minimum default burst size and time on a board.</text>
        </description>
        <leaf name="minimum-burst-size">
          <type name="uint32">
            <range value="1000..1000000"/>
          </type>
          <units name="Byte"/>
          <mandatory value="true"/>
          <description>
            <text>Specifies a minimum default burst size.</text>
          </description>
        </leaf>
        <leaf name="time">
          <type name="uint32">
            <range value="100..100000"/>
          </type>
          <units name="us"/>
          <mandatory value="true"/>
          <description>
            <text>Specifies a burst time.</text>
          </description>
        </leaf>
      </container>
      <container name="burst-sizes">
        <description>
          <text>List of the burst size.</text>
        </description>
        <list name="burst-size">
          <key value="service-class"/>
          <max-elements value="8"/>
          <description>
            <text>Configure the burst size of a chip.</text>
          </description>
          <leaf name="service-class">
            <type name="qos:qos-service-class"/>
            <description>
              <text>Class of service.</text>
            </description>
          </leaf>
          <leaf name="value">
            <type name="uint8">
              <range value="0..64"/>
            </type>
            <mandatory value="true"/>
            <description>
              <text>Burst size.</text>
            </description>
          </leaf>
        </list>
      </container>
      <container name="global-buffer-share-thresholds">
        <description>
          <text>List of the global shared threshold of buffer resources.</text>
        </description>
        <list name="global-buffer-share-threshold">
          <key value="direction"/>
          <description>
            <text>Configure the global shared threshold of buffer resources.</text>
          </description>
          <leaf name="direction">
            <type name="qos:qos-direction-type"/>
            <description>
              <text>Direction.</text>
            </description>
          </leaf>
          <leaf name="threshold">
            <type name="uint16">
              <range value="1..500"/>
            </type>
            <mandatory value="true"/>
            <description>
              <text>Specifies the shared threshold value.</text>
            </description>
          </leaf>
        </list>
      </container>
      <container name="global-buffer-share-queues">
        <description>
          <text>List of the shared threshold of buffer resources for specific queues.</text>
        </description>
        <list name="global-buffer-share-queue">
          <key value="service-class direction"/>
          <description>
            <text>Configure the shared threshold of buffer resources for specific queues.</text>
          </description>
          <leaf name="service-class">
            <type name="qos:qos-service-class"/>
            <description>
              <text>Class of service.</text>
            </description>
          </leaf>
          <leaf name="direction">
            <type name="qos:qos-direction-type"/>
            <description>
              <text>Direction.</text>
            </description>
          </leaf>
        </list>
      </container>
      <container name="flow-queue-backpressure-off">
        <presence value="create flow-queue-backpressure-off"/>
        <description>
          <text>Enable/disable the PIR backpressure function. When the configuration exists, the function is disable.</text>
        </description>
      </container>
      <container name="traffic-policy-match-type">
        <description>
          <text>Configure policy UCL rule match.</text>
        </description>
        <container name="destination-user">
          <presence value="create destination-user"/>
          <description>
            <text>Enable/disable traffic policy match type destination user.</text>
          </description>
        </container>
        <container name="interface-acl">
          <presence value="create interface-acl"/>
          <description>
            <text>Enable/disable traffic policy match type interface acl.</text>
          </description>
        </container>
      </container>
      <container name="user-queue-resource-4cos">
        <description>
          <text>Configure 4queue CoS size.</text>
        </description>
        <leaf name="cos-value">
          <type name="uint32">
            <range value="4..64000"/>
          </type>
          <must condition="../cos-value mod 4 = 0"/>
          <description>
            <text>Cos value.</text>
          </description>
        </leaf>
      </container>
      <container name="user-queue-resource-8cos-enhances">
        <description>
          <text>List of 8queue CoS size configuration.</text>
        </description>
        <list name="user-queue-resource-8cos-enhance">
          <key value="direction"/>
          <max-elements value="2"/>
          <description>
            <text>Configure 8queue CoS size.</text>
          </description>
          <leaf name="direction">
            <type name="qos:qos-direction-type"/>
            <description>
              <text>Direction.</text>
            </description>
          </leaf>
          <leaf name="cos-value">
            <type name="uint32">
              <range value="4..7000"/>
            </type>
            <must condition="../cos-value mod 4 = 0"/>
            <mandatory value="true"/>
            <description>
              <text>CoS value.</text>
            </description>
          </leaf>
        </list>
      </container>
    </container>
  </augment>
  <augment target-node="/lcs:license">
    <description>
      <text>Adds vm bandwidth configuration to license model.</text>
    </description>
    <container name="vm-bandwidths">
      <description>
        <text>List of configuration to set the bandwidth or set no rate limit for a VM.</text>
      </description>
      <list name="vm-bandwidth">
        <key value="vnfc-name slot-id"/>
        <description>
          <text>Configure the bandwidth or configure no rate limit for a VM.</text>
        </description>
        <ext:generated-by value="system">
          <description>
            <text>The all instances are generated automatically.</text>
          </description>
        </ext:generated-by>
        <ext:operation-exclude value="create|delete">
          <description>
            <text>The all instances cannot be created or deleted.</text>
          </description>
        </ext:operation-exclude>
        <ext:operation-exclude value="update">
          <when condition="not(../lcs:license-items/lcs:license-item[lcs:name='VR9K00BW0'])"/>
          <description>
            <text>The all instances cannot be updated when license-item 'VR9K00BW0' does not exist.</text>
          </description>
        </ext:operation-exclude>
        <leaf name="vnfc-name">
          <type name="leafref">
            <path value="/devm-vnf:devm-vnf/devm-vnf:vnf-boards/devm-vnf:vnf-board/devm-vnf:vnfc-name"/>
          </type>
          <must condition="../vnfc-name = 'brasnls'"/>
          <description>
            <text>Virtualized network function component (VNFC) name.</text>
          </description>
        </leaf>
        <leaf name="slot-id">
          <type name="leafref">
            <path value="/devm-vnf:devm-vnf/devm-vnf:vnf-boards/devm-vnf:vnf-board[devm-vnf:vnfc-name=current()/../vnfc-name]/devm-vnf:id"/>
          </type>
          <description>
            <text>Slot ID.</text>
          </description>
        </leaf>
        <leaf name="is-bandwidth-limited">
          <type name="limit-type"/>
          <default value="limited"/>
          <description>
            <text>Is bandwidth limited.</text>
          </description>
        </leaf>
        <leaf name="bandwidth">
          <when condition="../is-bandwidth-limited='limited'"/>
          <type name="uint32">
            <range value="0..1000000"/>
          </type>
          <units name="Mbit/s"/>
          <default value="1"/>
          <description>
            <text>Bandwidth.</text>
          </description>
        </leaf>
        <leaf name="current-bandwidth">
          <type name="uint32"/>
          <units name="Mbit/s"/>
          <config value="false"/>
          <description>
            <text>Current Bandwidth.</text>
          </description>
        </leaf>
      </list>
    </container>
  </augment>
</module>
