  </grouping>
  <container name="bras-vas">
    <description>
      <text>Bras value-added services.</text>
    </description>
    <container name="global">
      <description>
        <text>Configure value-added-service.</text>
      </description>
      <container name="value-added-service">
        <description>
          <text>Configure value-added-service enable list.</text>
        </description>
        <leaf name="enable">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable value-added-service.</text>
          </description>
        </leaf>
      </container>
      <container name="account-individual">
        <description>
          <text>Configure account individual.</text>
        </description>
        <leaf name="enable">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable account individual.</text>
          </description>
        </leaf>
      </container>
      <container name="policy-name-case-sensitive">
        <description>
          <text>Configure service policy name case sensitive.</text>
        </description>
        <leaf name="enable">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable service policy name case sensitive.</text>
          </description>
        </leaf>
      </container>
      <container name="quota-out">
        <description>
          <text>Configure the Quota type.</text>
        </description>
        <leaf name="quota-type">
          <type name="bras-vas-quota-out-online-offline-type"/>
          <default value="online"/>
          <description>
            <text>Quota type.</text>
          </description>
        </leaf>
        <leaf name="diameter-link-down">
          <type name="bras-vas-quota-out-online-offline-type"/>
          <default value="online"/>
          <description>
            <text>Quota type of diameter link down.</text>
          </description>
        </leaf>
      </container>
      <container name="daa-resource-insufficient">
        <description>
          <text>Configure Resource Application Failures Policy.</text>
        </description>
        <leaf name="type">
          <type name="enumeration">
            <enum name="online">
              <value value="0"/>
              <description>
                <text>By default, DAA users remain online after they fail to apply for resources.</text>
              </description>
            </enum>
            <enum name="inbound-offline">
              <value value="1"/>
              <description>
                <text>Log out DAA users who fail to apply for upstream resources.</text>
              </description>
            </enum>
            <enum name="outbound-offline">
              <value value="2"/>
              <description>
                <text>Log out DAA users who fail to apply for downstream resources.</text>
              </description>
            </enum>
            <enum name="inbound-or-outbound-offline">
              <value value="3"/>
              <description>
                <text>Log out DAA users who fail to apply for upstream or downstream resources.</text>
              </description>
            </enum>
            <enum name="both-offline">
              <value value="4"/>
              <description>
                <text>Log out DAA users who fail to apply for both upstream and downstream resources.</text>
              </description>
            </enum>
          </type>
          <default value="online"/>
          <description>
            <text>Policy Upon Resource Application Failures.</text>
          </description>
        </leaf>
      </container>
      <container name="edsg-queue">
        <description>
          <text>Configure delayed EDSG user activation.</text>
        </description>
        <leaf name="enable">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable a device to adjust the access rate of users who go online by configuring the edsg service to enter the activation queue and suspend the activation.</text>
          </description>
        </leaf>
      </container>
      <container name="edsg-run-info">
        <config value="false"/>
        <description>
          <text>Statistics of the global service policy configuration.</text>
        </description>
        <leaf name="edsg-run-service-number">
          <type name="uint32"/>
          <config value="false"/>
          <description>
            <text>Number of EDSG resources that are in use, this item is displayed only if the EDSG function license is deactivated.</text>
          </description>
        </leaf>
      </container>
      <container name="edsg-centralized-ds-lite">
        <description>
          <text>Configure value-added services on the centralized DS-Lite device.</text>
        </description>
        <leaf name="enable">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable the function of matching EDSG services based on inner IPv4 packets.</text>
          </description>
        </leaf>
      </container>
      <container name="daa-report-dropped-flow">
        <description>
          <text>Configure the function to report DAA users whose packets are dropped.</text>
        </description>
        <leaf name="enable">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable the function to report DAA users whose packets are dropped.</text>
          </description>
        </leaf>
      </container>
    </container>
    <container name="vas-policys">
      <description>
        <text>List of the vas Policy.</text>
      </description>
      <list name="vas-policy">
        <key value="name"/>
        <description>
          <text>Configure a vas policy.</text>
        </description>
        <leaf name="name">
          <type name="string">
            <length value="1..63"/>
            <pattern value="[!-~]{1,63}"/>
            <pattern value="([^A-Z&quot;%'*/:&lt;&gt;\?@|]{1,63})"/>
          </type>
          <must condition="../name != '-' and ../name != '--'"/>
          <description>
            <text>Vas policy name.</text>
          </description>
          <ext:case-sensitivity value="lower-only"/>
        </leaf>
        <leaf name="type">
          <ext:operation-exclude value="update"/>
          <type name="bras-vas-policy-type"/>
          <mandatory value="true"/>
          <description>
            <text>Vas policy type.</text>
          </description>
        </leaf>
        <container name="qos-profile">
          <when condition="../type='bod'"/>
          <description>
            <text>Configure qos profile.</text>
          </description>
          <leaf name="name">
            <type name="leafref">
              <path value="/qos:qos/qos:queue-scheduler-template/qos:profiles/qos:profile/qos:name"/>
            </type>
            <must condition="/qos:qos/qos:queue-scheduler-template/qos:profiles/qos:profile[qos:name=current()]/qos:mode='normal'"/>
            <description>
              <text>Name of qos profile.</text>
            </description>
          </leaf>
        </container>
        <container name="accounting-scheme">
          <description>
            <text>Configure accounting scheme.</text>
          </description>
          <leaf name="name">
            <type name="leafref">
              <path value="/aaa:aaa/aaa:accounting-schemes/aaa:accounting-scheme/aaa:acct-scheme-name"/>
            </type>
            <default value="default1"/>
            <description>
              <text>Name of accounting scheme.</text>
            </description>
          </leaf>
        </container>
        <container name="daa-service-enable">
          <when condition="not(../type='bod')"/>
          <description>
            <text>Configure daa service.</text>
          </description>
          <leaf name="user-accounting-together">
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable the DAA service traffic counts into user traffic.</text>
            </description>
          </leaf>
          <leaf name="traffic-separate">
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable the DAA service separate function.</text>
            </description>
          </leaf>
          <leaf name="tariff-queue-mapping">
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable priority scheduling based on tariff levels.</text>
            </description>
          </leaf>
          <leaf name="accounting-together">
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable uniform accounting for DAA services.</text>
            </description>
          </leaf>
        </container>
        <container name="dual-stack-accounting-mode">
          <when condition="../type='daa'"/>
          <description>
            <text>Configure separate traffic statistics mode for dual-stack daa users.</text>
          </description>
          <leaf name="user-queue-separate">
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable the DAA service separate traffic statistics for dual-stack based on user queues.</text>
            </description>
          </leaf>
        </container>
        <container name="bind-user-group">
          <description>
            <text>Configure an ACL user group to a value-added service policy.</text>
          </description>
          <leaf name="name">
            <type name="leafref">
              <path value="/bras-basic-access:bras-basic-access/bras-basic-access:user-groups/bras-basic-access:user-group/bras-basic-access:name"/>
            </type>
            <description>
              <text>Specifies the name of a user group, and the user group must not be added to a user group list or a domain without binding nat instance.</text>
            </description>
          </leaf>
        </container>
        <container name="bind-tariff-levels">
          <when condition="../type='daa'"/>
          <description>
            <text>List of the tariff level for DAA accounting.</text>
          </description>
          <list name="bind-tariff-level">
            <key value="tariff-level"/>
            <description>
              <text>Configure a tariff level for DAA accounting.</text>
            </description>
            <leaf name="tariff-level">
              <type name="uint8">
                <range value="1..8"/>
              </type>
              <description>
                <text>Tariff level for DAA accounting.</text>
              </description>
            </leaf>
            <leaf name="qos-profile-name">
              <type name="leafref">
                <path value="/qos:qos/qos:queue-scheduler-template/qos:profiles/qos:profile/qos:name"/>
              </type>
              <must condition="/qos:qos/qos:queue-scheduler-template/qos:profiles/qos:profile[qos:name=current()]/qos:mode='normal'"/>
              <mandatory value="true"/>
              <description>
                <text>Specifies the name of a bound QoS profile.</text>
              </description>
            </leaf>
          </list>
        </container>
        <container name="tariff-level-accountings">
          <when condition="../type='daa'"/>
          <description>
            <text>List of the accounting status or IP type for a DAA tariff level.</text>
          </description>
          <list name="tariff-level-accounting">
            <must condition="./accounting-off = 'true' or ./ip-type-ipv6 = 'true'"/>
            <key value="level"/>
            <description>
              <text>Configure the accounting status or IP type for a DAA tariff level.</text>
            </description>
            <leaf name="level">
              <type name="uint8">
                <range value="1..8"/>
              </type>
              <description>
                <text>Specifies a DAA tariff level.</text>
              </description>
            </leaf>
            <leaf name="accounting-off">
              <type name="boolean"/>
              <default value="false"/>
              <description>
                <text>Enable/disable the accounting status of a DAA tariff level as disabled.</text>
              </description>
            </leaf>
            <leaf name="ip-type-ipv6">
              <type name="boolean"/>
              <default value="false"/>
              <description>
                <text>Enable/disable an IP type for a DAA tariff level as IPv6.</text>
              </description>
            </leaf>
          </list>
        </container>
        <container name="user-traffic-statistic">
          <when condition="../type='daa'"/>
          <description>
            <text>Configure the statistics mode for user traffic. After CAR is performed on a DAA service flow, packets in the DAA service flow are re-marked with different priorities and are scheduled into SQs. In SQ-based traffic statistics, however, DAA traffic and non-DAA traffic are not differentiated. Specify the FQs of which traffic statistics are collected as user traffic statistics.</text>
          </description>
          <container name="inbound">
            <description>
              <text>Configure the statistics mode for user traffic when SQ scheduling is applied to DAA services.</text>
            </description>
            <uses name="bras-vas-user-traffic-type"/>
          </container>
          <container name="outbound">
            <description>
              <text>Configure the statistics mode for user traffic when SQ scheduling is applied to DAA services.</text>
            </description>
            <uses name="bras-vas-user-traffic-type"/>
          </container>
        </container>
        <container name="accounting-service-policy">
          <when condition="../type='daa'"/>
          <description>
            <text>Configure user traffic from to match a DAA service policy.</text>
          </description>
          <leaf name="inbound">
            <type name="enumeration">
              <enum name="disable">
                <value value="0"/>
                <description>
                  <text>Disables user traffic from matching a DAA service policy.</text>
                </description>
              </enum>
              <enum name="enable">
                <value value="1"/>
                <description>
                  <text>Enables user traffic from to match a DAA service policy.</text>
                </description>
              </enum>
              <enum name="auto">
                <value value="2"/>
                <description>
                  <text>Automatically matches user traffic with a DAA service policy.</text>
                </description>
              </enum>
            </type>
            <default value="enable"/>
            <description>
              <text>Upstream user traffic.</text>
            </description>
          </leaf>
          <leaf name="outbound">
            <type name="enumeration">
              <enum name="disable">
                <value value="0"/>
                <description>
                  <text>Disables user traffic from matching a DAA service policy.</text>
                </description>
              </enum>
              <enum name="enable">
                <value value="1"/>
                <description>
                  <text>Enables user traffic from to match a DAA service policy.</text>
                </description>
              </enum>
              <enum name="auto">
                <value value="2"/>
                <description>
                  <text>Automatically matches user traffic with a DAA service policy.</text>
                </description>
              </enum>
            </type>
            <default value="enable"/>
            <description>
              <text>Downstream user traffic.</text>
            </description>
          </leaf>
        </container>
        <container name="rate-limit-mode">
          <when condition="../type='daa'"/>
          <description>
            <text>Configure a QoS rate limit mode for downstream DAA services on a board with the eTM subcard installed.</text>
          </description>
          <leaf name="outbond">
            <type name="bras-vas-out-rate-limit-mode"/>
            <description>
              <text>QoS Rate Limit Mode.</text>
            </description>
          </leaf>
        </container>
        <container name="user-group-bind-nats">
          <when condition="../type='daa'"/>
          <description>
            <text>List of NAT instances with a user group in a value-added-service policy.</text>
          </description>
          <list name="user-group-bind-nat">
            <key value="user-group-name"/>
            <max-elements value="8"/>
            <description>
              <text>Configure NAT instances with a user group in a value-added-service policy.</text>
            </description>
            <leaf name="user-group-name">
              <type name="leafref">
                <path value="/bras-basic-access:bras-basic-access/bras-basic-access:user-groups/bras-basic-access:user-group/bras-basic-access:name"/>
              </type>
              <must condition="../../../bind-user-group/name != ../user-group-name"/>
              <must condition="count(/bras-basic-access:bras-basic-access/bras-basic-access:user-group-lists/bras-basic-access:user-group-list/bras-basic-access:user-groups/bras-basic-access:user-group[bras-basic-access:name=current()])=0"/>
              <description>
                <text>Specifies the name of a user group, and the user group must not be added to a user group list or a domain without binding nat instance.</text>
              </description>
            </leaf>
            <leaf name="nat-instance-name">
              <type name="string">
                <length value="1..31"/>
              </type>
              <mandatory value="true"/>
              <description>
                <text>Specifies the name of a NAT instance.</text>
              </description>
            </leaf>
          </list>
        </container>
        <container name="policy-info">
          <config value="false"/>
          <description>
            <text>Statistics of a specified DAA or BOD value-added service policy.</text>
          </description>
          <leaf name="used-number">
            <type name="uint32">
              <range value="0..4294967295"/>
            </type>
            <config value="false"/>
            <description>
              <text>Number of services that reference a value-added service policy.</text>
            </description>
          </leaf>
          <leaf name="user-number">
            <type name="uint32">
              <range value="0..4294967295"/>
            </type>
            <config value="false"/>
            <description>
              <text>Number of users for whom a value-added service policy is used.</text>
            </description>
          </leaf>
        </container>
      </list>
    </container>
    <container name="hw-policy-name-attribute">
      <description>
        <text>Configure the HW-Policy-Name attribute to carry the EDSG service policy name.</text>
      </description>
      <leaf name="support-edsg">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable the HW-Policy-Name attribute to carry the EDSG service policy name.</text>
        </description>
      </leaf>
    </container>
    <container name="accounting-merge-cache">
      <description>
        <text>Configure a post-merging accounting packet that fails to be sent for value-added services to enter the accounting packet cache. By default, a post-merging accounting packet that fails to be sent for value-added services is disabled from entering the accounting packet cache.</text>
      </description>
      <leaf name="enable">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable a post-merging accounting packet that fails to be sent for value-added services to enter the accounting packet cache.</text>
        </description>
      </leaf>
    </container>
    <container name="bod-portal-reserved">
      <description>
        <text>Configure portal reserved when the type of value added service is bod.</text>
      </description>
      <leaf name="enable">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable portal reserved when the type of value added service is bod.</text>
        </description>
      </leaf>
    </container>
    <container name="service-policy-download">
      <presence value="service-policy-download."/>
      <description>
        <text>Configure a mode in which an EDSG service policy is obtained.</text>
      </description>
      <leaf name="download-type">
        <type name="enumeration">
          <enum name="local">
            <value value="0"/>
            <description>
              <text>Local configuration.</text>
            </description>
          </enum>
          <enum name="radius">
            <value value="1"/>
            <description>
              <text>Download from an RADIUS server.</text>
            </description>
          </enum>
          <enum name="local-radius">
            <value value="2"/>
            <description>
              <text>First from the local configuration and then from an RADIUS server.</text>
            </description>
          </enum>
          <enum name="radius-local">
            <value value="3"/>
            <description>
              <text>First from an RADIUS server and then from the local configuration.</text>
            </description>
          </enum>
        </type>
        <default value="local"/>
        <description>
          <text>Sets a mode in which an EDSG service policy is obtained.</text>
        </description>
      </leaf>
      <leaf name="radius-server-group-name">
        <when condition="../download-type ='radius' or ../download-type ='local-radius' or ../download-type ='radius-local'"/>
        <type name="leafref">
          <path value="/radius:radius/radius:radius-server-groups/radius:radius-server-group/radius:name"/>
        </type>
        <mandatory value="true"/>
        <description>
          <text>Specifies the name of a RADIUS server group.</text>
        </description>
      </leaf>
      <leaf name="password">
        <when condition="../download-type ='radius' or ../download-type ='local-radius' or ../download-type ='radius-local'"/>
        <type name="pub-type:password-extend">
          <length value="1..128"/>
        </type>
        <mandatory value="true"/>
        <description>
          <text>Specifies an unencrypted/encrypted password.</text>
        </description>
      </leaf>
    </container>
    <container name="service-groups">
      <description>
        <text>List of the service group.</text>
      </description>
      <list name="service-group">
        <key value="name"/>
        <max-elements value="4096"/>
        <description>
          <text>Configure a service group.</text>
        </description>
        <leaf name="name">
          <type name="string">
            <length value="1..31"/>
          </type>
          <must condition="../name!='a' and ../name!='an' and ../name!='any'"/>
          <description>
            <text>Service group name.</text>
          </description>
        </leaf>
        <container name="service-group-status">
          <config value="false"/>
          <description>
            <text>Operational state of service-group.</text>
          </description>
          <leaf name="is-used">
            <type name="enumeration">
              <enum name="no">
                <value value="0"/>
                <description>
                  <text>The service-group is unused.</text>
                </description>
              </enum>
              <enum name="yes">
                <value value="1"/>
                <description>
                  <text>The service-group is used.</text>
                </description>
              </enum>
            </type>
            <config value="false"/>
            <description>
              <text>Whether the service-group is used or not.</text>
            </description>
          </leaf>
          <leaf name="group-type">
            <type name="string">
              <length value="1..19"/>
            </type>
            <config value="false"/>
            <description>
              <text>The type of service-group.</text>
            </description>
          </leaf>
        </container>
      </list>
    </container>
    <container name="coa-hw-policy-name">
      <description>
        <text>Configure DAA or EDSG service templates delivered by the RADIUS server through Huawei proprietary attributes in COA messages.</text>
      </description>
      <leaf name="daa-same-policy-reply-ack">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable a device to reply with an ACK message when the RADIUS server uses a COA message's HW-Policy-Name (26-95) attribute to deliver the same DAA service policy as that configured on the device in uniform accounting scenarios.</text>
        </description>
      </leaf>
      <leaf name="daa-coexist-with-user">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable the user attributes also to take effect if ehty are delivered with a DAA template in the HW-Policy-Name (26-95) attribute through the same COA message which may also carry other DAA service attributes.</text>
        </description>
      </leaf>
    </container>
    <container name="prepaid-profiles">
      <description>
        <text>List of the prepaid service templates.</text>
      </description>
      <list name="prepaid-profile">
        <key value="name"/>
        <max-elements value="1024"/>
        <description>
          <text>Configure a prepaid service template.</text>
        </description>
        <leaf name="name">
          <type name="string">
            <length value="1..31"/>
            <pattern value="[a-z0-9._-]{1,31}"/>
          </type>
          <must condition="../name != '-' and ../name != '--'"/>
          <description>
            <text>Specifies the name of a prepaid service template.</text>
          </description>
          <ext:case-sensitivity value="lower-only"/>
        </leaf>
        <container name="accounting-scheme">
          <description>
            <text>Configure the accounting scheme for the current pre-paid profile. The scheme must be a configured accounting scheme.</text>
          </description>
          <leaf name="name">
            <type name="leafref">
              <path value="/aaa:aaa/aaa:accounting-schemes/aaa:accounting-scheme/aaa:acct-scheme-name"/>
            </type>
            <default value="default1"/>
            <description>
              <text>Specifies the name of the accounting scheme adopted by the pre-paid profile.</text>
            </description>
          </leaf>
        </container>
        <container name="authen-scheme">
          <description>
            <text>Configure the authentication scheme for the current pre-paid profile. The scheme must be a configured authentication scheme.</text>
          </description>
          <leaf name="name">
            <type name="leafref">
              <path value="/aaa:aaa/aaa:authentication-schemes/aaa:authentication-scheme/aaa:authen-scheme-name"/>
            </type>
            <default value="default1"/>
            <description>
              <text>Specifies the name of the authentication scheme.</text>
            </description>
          </leaf>
        </container>
        <container name="password">
          <description>
            <text>Configure the password used for the quotas application sent to the server by the prepaid services.</text>
          </description>
          <leaf name="password">
            <type name="pub-type:password-extend">
              <length value="1..128"/>
            </type>
            <description>
              <text>The password is a string ranging from 1 to 16 characters for a simple password and 1 to 128 characters for a ciphertext password.</text>
            </description>
          </leaf>
        </container>
        <container name="threshold">
          <description>
            <text>Configure the threshold that determines when the pre-paid service sends the application for quotas.</text>
          </description>
          <leaf name="time-value">
            <type name="uint32"/>
            <units name="s"/>
            <default value="0"/>
            <description>
              <text>Uses a certain period of time as the threshold.</text>
            </description>
          </leaf>
          <leaf name="volume-value">
            <type name="uint32"/>
            <default value="0"/>
            <description>
              <text>Uses a certain quantity of volume as the threshold.</text>
            </description>
          </leaf>
          <leaf name="volume-unit">
            <type name="enumeration">
              <enum name="bytes">
                <value value="1"/>
                <description>
                  <text>The unit is bytes.</text>
                </description>
              </enum>
              <enum name="kbytes">
                <value value="2"/>
                <description>
                  <text>The unit is kbytes.</text>
                </description>
              </enum>
              <enum name="mbytes">
                <value value="3"/>
                <description>
                  <text>The unit is mbytes.</text>
                </description>
              </enum>
            </type>
            <default value="bytes"/>
            <description>
              <text>Unit of the volume.</text>
            </description>
          </leaf>
        </container>
        <container name="service-volume-quota-apply">
          <description>
            <text>Configure the traffic direction to which the EDSG service quota applies.</text>
          </description>
          <leaf name="direction">
            <type name="enumeration">
              <enum name="both">
                <value value="0"/>
                <description>
                  <text>The direction is both inbound and outbound.</text>
                </description>
              </enum>
              <enum name="inbound">
                <value value="1"/>
                <description>
                  <text>The direction is inbound.</text>
                </description>
              </enum>
              <enum name="outbound">
                <value value="2"/>
                <description>
                  <text>The direction is outbound.</text>
                </description>
              </enum>
            </type>
            <default value="both"/>
            <description>
              <text>The direction of volume Apply.</text>
            </description>
          </leaf>
        </container>
        <container name="profile-info">
          <config value="false"/>
          <description>
            <text>Operational state of prepaid profile.</text>
          </description>
          <leaf name="reference-count">
            <type name="uint32"/>
            <config value="false"/>
            <description>
              <text>Number of times that a prepaid profile is referenced by service instances.</text>
            </description>
          </leaf>
        </container>
        <container name="radius-server-group">
          <description>
            <text>Configure the RADIUS server group for prepaid profile.</text>
          </description>
          <leaf name="name">
            <type name="leafref">
              <path value="/radius:radius/radius:radius-server-groups/radius:radius-server-group/radius:name"/>
            </type>
            <description>
              <text>Specifies the name of the RADIUS server group.</text>
            </description>
          </leaf>
        </container>
      </list>
    </container>
    <container name="edsg-accounting-interim">
      <description>
        <text>Configure a device to send a real-time accounting packet carrying the HW-Acct-Update-Address (26-159) attribute with a value of 1 for EDSG services when the user address changes. By default, when the user address changes, a device does not send a real-time accounting packet carrying the HW-Acct-Update-Address (26-159) attribute with a value of 1 for EDSG services.</text>
      </description>
      <leaf name="send-update-user-ip-enable">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable a device to send a real-time accounting packet carrying the HW-Acct-Update-Address (26-159) attribute with a value of 1 for EDSG services when the user address changes.</text>
        </description>
      </leaf>
    </container>
    <container name="edsg-coexist-attribute">
      <description>
        <text>Configure a specified attribute to take effect upon delivery in EDSG service scenarios.</text>
      </description>
      <leaf name="filter-id">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable the attribute of filter-id.</text>
        </description>
      </leaf>
      <leaf name="traffic-policy">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable the attribute of traffic-Policy.</text>
        </description>
      </leaf>
      <leaf name="hw-data-filter">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable the attribute of HW-Data-Filter.</text>
        </description>
      </leaf>
      <leaf name="class">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable the attribute of class.</text>
        </description>
      </leaf>
    </container>
    <container name="service-record">
      <description>
        <text>Configure the service activation failure or deactivation record.</text>
      </description>
      <leaf name="active-fail">
        <type name="boolean"/>
        <default value="true"/>
        <description>
          <text>Enable/disable the service activation failure record function.</text>
        </description>
      </leaf>
      <leaf name="deactive">
        <type name="boolean"/>
        <default value="true"/>
        <description>
          <text>Enable/disable the service deactivation record function.</text>
        </description>
      </leaf>
    </container>
    <container name="traffic-policy">
      <description>
        <text>Configure a DAA traffic policy globally.</text>
      </description>
      <leaf name="name">
        <type name="leafref">
          <path value="/qos:qos/qos:classifier-template/qos:traffic-policys/qos:traffic-policy/qos:name"/>
        </type>
        <description>
          <text>Specifies the name of a DAA traffic policy.</text>
        </description>
      </leaf>
    </container>
    <container name="policy-cache-update-interval">
      <description>
        <text>Configure an interval at which the downloaded EDSG service policy template is updated.</text>
      </description>
      <leaf name="interval">
        <type name="uint16">
          <range value="5..65535"/>
        </type>
        <units name="min"/>
        <description>
          <text>Specifies the interval at which the downloaded EDSG service policy template is updated.</text>
        </description>
      </leaf>
    </container>
    <container name="service-no-server-policy">
      <description>
        <text>Configure an activation policy for RADIUS servers when an EDSG service policy is not bound to any RADIUS server group.</text>
      </description>
      <leaf name="server-activation-mode">
        <type name="bras-vas-online-offline-type"/>
        <default value="offline"/>
        <description>
          <text>Configures an activation policy for RADIUS servers when an EDSG service policy is not bound to any RADIUS server group.</text>
        </description>
      </leaf>
    </container>
    <container name="tariff-queue-mapping">
      <ext:generated-by value="system"/>
      <presence value="Tariff queue mapping."/>
      <description>
        <text>Configure the queue scheduling can be performed on different value-added services to ensure that services with higher priorities are forwarded first. It is used to configure the mapping relationship between tariff levels of DAA and flow queues.</text>
      </description>
      <leaf-list name="tariff-level">
        <ext:generated-by value="system"/>
        <type name="bras-vas-flow-queue-type"/>
        <min-elements value="8"/>
        <max-elements value="8"/>
        <ordered-by value="user"/>
        <description>
          <text>Configure a packet type.</text>
        </description>
      </leaf-list>
    </container>
    <container name="service-policys">
      <description>
        <text>List of the value-added service policy templates.</text>
      </description>
      <list name="service-policy">
        <key value="name"/>
        <max-elements value="4096"/>
        <description>
          <text>Configure a value-added service policy template.</text>
        </description>
        <leaf name="name">
          <type name="string">
            <length value="1..64"/>
            <pattern value="[a-zA-Z0-9._-]{1,64}"/>
          </type>
          <must condition="../name != '-' and ../name != '--'"/>
          <description>
            <text>Specify the name of a value-added service policy. When the node of policy-name-case-sensitive is enabled, the service policy name is case-sensitive. When the node of policy-name-case-sensitive is disabled, the service policy name cannot contain uppercase letters.</text>
          </description>
        </leaf>
        <leaf name="mode">
          <ext:operation-exclude value="update"/>
          <type name="enumeration">
            <enum name="edsg">
              <value value="0"/>
              <description>
                <text>Indicates an enhanced dynamic service gateway (EDSG) service policy.</text>
              </description>
            </enum>
            <enum name="portal">
              <value value="1"/>
              <description>
                <text>Indicates a portal service policy.</text>
              </description>
            </enum>
            <enum name="mirror">
              <value value="4"/>
              <description>
                <text>Indicates a mirror service policy.</text>
              </description>
            </enum>
          </type>
          <mandatory value="true"/>
          <description>
            <text>Indicates a service policy. This leaf cannot be modified. If you need to modify it, delete the policy and then create a new type of policy.</text>
          </description>
        </leaf>
        <container name="service-group">
          <presence value="Service-group."/>
          <description>
            <text>Configure a service group for a service policy.</text>
          </description>
          <leaf name="group-name">
            <type name="leafref">
              <path value="../../../../service-groups/service-group/name"/>
            </type>
            <mandatory value="true"/>
            <description>
              <text>Specifies the name of a service group.</text>
            </description>
          </leaf>
          <leaf name="direction">
            <when condition="../../mode='edsg'"/>
            <type name="enumeration">
              <enum name="inbound">
                <value value="1"/>
                <description>
                  <text>Inbound.</text>
                </description>
              </enum>
              <enum name="outbound">
                <value value="2"/>
                <description>
                  <text>Outbound.</text>
                </description>
              </enum>
              <enum name="both">
                <value value="3"/>
                <description>
                  <text>Both.</text>
                </description>
              </enum>
            </type>
            <default value="both"/>
            <description>
              <text>Indicates the direction for a service group.</text>
            </description>
          </leaf>
          <leaf name="priority">
            <when condition="../../mode='edsg'"/>
            <type name="uint32">
              <range value="0..1000"/>
            </type>
            <default value="0"/>
            <description>
              <text>Specifies a priority value that a service matches.</text>
            </description>
          </leaf>
        </container>
        <container name="accounting-scheme">
          <when condition="../mode='edsg'"/>
          <description>
            <text>Configure the accounting scheme for the current service policy. The scheme must be a configured accounting scheme.</text>
          </description>
          <leaf name="name">
            <type name="leafref">
              <path value="/aaa:aaa/aaa:accounting-schemes/aaa:accounting-scheme/aaa:acct-scheme-name"/>
            </type>
            <default value="default1"/>
            <description>
              <text>Specifies the name of the accounting scheme adopted by the pre-paid profile.</text>
            </description>
          </leaf>
        </container>
        <container name="radius-server-group">
          <when condition="../mode='edsg'"/>
          <description>
            <text>Configure the RADIUS server group for service policy. The server group must be a configured server group.</text>
          </description>
          <leaf name="name">
            <type name="leafref">
              <path value="/radius:radius/radius:radius-server-groups/radius:radius-server-group/radius:name"/>
            </type>
            <description>
              <text>Specifies the name of the RADIUS server group.</text>
            </description>
          </leaf>
        </container>
        <container name="traffic-match">
          <when condition="../mode='edsg'"/>
          <description>
            <text>Configure EDSG service traffic to match a user group.</text>
          </description>
          <leaf name="direction">
            <type name="enumeration">
              <enum name="none">
                <value value="0"/>
                <description>
                  <text>None.</text>
                </description>
              </enum>
              <enum name="inbound">
                <value value="1"/>
                <description>
                  <text>Specifies inbound direction of reporting dropped SQ traffic.</text>
                </description>
              </enum>
              <enum name="outbound">
                <value value="2"/>
                <description>
                  <text>Specifies outbound direction of reporting dropped SQ traffic.</text>
                </description>
              </enum>
              <enum name="both">
                <value value="3"/>
                <description>
                  <text>Specifies inbound and outbound direction of reporting dropped SQ traffic.</text>
                </description>
              </enum>
            </type>
            <default value="none"/>
            <description>
              <text>Indicates stream direction of EDSG service traffic.</text>
            </description>
          </leaf>
        </container>
        <container name="statistic-ip-type">
          <when condition="../mode='edsg'"/>
          <description>
            <text>Configure a statistic IP type for EDSG services. The default statistic type is IPv4 for EDSG services.</text>
          </description>
          <leaf name="type">
            <type name="bras-vas-service-ip-type"/>
            <default value="ipv4"/>
            <description>
              <text>Statistic IP type for EDSG services.</text>
            </description>
          </leaf>
        </container>
        <container name="service-class">
          <when condition="../mode='edsg'"/>
          <description>
            <text>Configure upstream or downstream service classes.</text>
          </description>
          <leaf name="inbound-class">
            <type name="bras-vas-flow-queue-type"/>
            <description>
              <text>Specifies a service class of inbound direction.</text>
            </description>
          </leaf>
          <leaf name="outbound-class">
            <type name="bras-vas-flow-queue-type"/>
            <description>
              <text>Specifies a service class of outbound direction.</text>
            </description>
          </leaf>
        </container>
        <container name="authentication-scheme">
          <when condition="../mode='edsg'"/>
          <description>
            <text>Configure an authentication scheme for the current service policy. The scheme must be a configured authentication scheme.</text>
          </description>
          <leaf name="name">
            <type name="leafref">
              <path value="/aaa:aaa/aaa:authentication-schemes/aaa:authentication-scheme/aaa:authen-scheme-name"/>
            </type>
            <description>
              <text>Specifies the name of the authentication scheme.</text>
            </description>
          </leaf>
        </container>
        <container name="rate-limit">
          <when condition="../mode='edsg'"/>
          <description>
            <text>Configure the inbound and outbound rate for the service policy template.</text>
          </description>
          <container name="inbound">
            <presence value="Create inbound rate-limit."/>
            <description>
              <text>Configure the inbound rate for the service policy template.</text>
            </description>
            <uses name="rate-limit-type"/>
          </container>
          <container name="outbound">
            <presence value="Create outbound rate-limit."/>
            <description>
              <text>Configure the outbound rate for the service policy template.</text>
            </description>
            <uses name="rate-limit-type"/>
          </container>
        </container>
        <container name="diameter-monitor-key">
          <when condition="../mode='edsg'"/>
          <description>
            <text>Configure the Characters monitoring key values for the policy business.</text>
          </description>
          <leaf name="key">
            <type name="uint32">
              <range value="1..4294967294"/>
            </type>
            <description>
              <text>Monitoring key values for the policy business.</text>
            </description>
          </leaf>
        </container>
        <container name="policy-info">
          <config value="false"/>
          <description>
            <text>Statistics of the value-added-service policy.</text>
          </description>
          <leaf name="reference-count">
            <type name="uint32">
              <range value="0..4294967295"/>
            </type>
            <config value="false"/>
            <description>
              <text>Number of services that reference a service policy.</text>
            </description>
          </leaf>
        </container>
      </list>
    </container>
    <container name="service-policy-groups">
      <description>
        <text>List of value-added service policy template groups.</text>
      </description>
      <list name="service-policy-group">
        <key value="name"/>
        <max-elements value="1024"/>
        <description>
          <text>Configure the value-added service policy template group.</text>
        </description>
        <leaf name="name">
          <type name="string">
            <length value="1..63"/>
            <pattern value="[!-~]{1,63}"/>
            <pattern value="([^&quot;%'*/:&lt;&gt;\?@|]{1,63})"/>
          </type>
          <must condition="../name != '-' and ../name != '--'"/>
          <description>
            <text>The name of service policy group.</text>
          </description>
        </leaf>
        <container name="service-policys">
          <description>
            <text>List of value-added service policy templates.</text>
          </description>
          <list name="service-policy">
            <key value="name"/>
            <max-elements value="12"/>
            <description>
              <text>Configure the value-added service policy template.</text>
            </description>
            <leaf name="name">
              <type name="leafref">
                <path value="../../../../../service-policys/service-policy/name"/>
              </type>
              <must condition="../../../../../service-policys/service-policy[name=current()]/mode='edsg'"/>
              <description>
                <text>The name of service policy.</text>
              </description>
            </leaf>
          </list>
        </container>
      </list>
    </container>
    <container name="mirror-service-policy-rules">
      <description>
        <text>List of the mapping relation between service policy and option82.</text>
      </description>
      <list name="mirror-service-policy-rule">
        <key value="number"/>
        <description>
          <text>Configure the mapping relation between service policy and option82.</text>
        </description>
        <leaf name="number">
          <type name="uint32">
            <range value="0..65535"/>
          </type>
          <description>
            <text>The number of the rule.</text>
          </description>
        </leaf>
        <leaf name="service-policy-name">
          <type name="leafref">
            <path value="../../../service-policys/service-policy/name"/>
