  <revision date="2020-02-29">
    <description>
      <text>Default value of stale-time is deleted.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <revision date="2020-02-26">
    <description>
      <text>Default value.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <revision date="2019-12-30">
    <description>
      <text>Init revision.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <typedef name="nd-flag">
    <type name="enumeration">
      <enum name="false">
        <value value="0"/>
        <description>
          <text>NO.</text>
        </description>
      </enum>
      <enum name="true">
        <value value="1"/>
        <description>
          <text>YES.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Description of nd-flag.</text>
    </description>
  </typedef>
  <typedef name="nd-switch-state">
    <type name="enumeration">
      <enum name="off">
        <value value="0"/>
        <description>
          <text>Off.</text>
        </description>
      </enum>
      <enum name="on">
        <value value="1"/>
        <description>
          <text>On.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>The state of the switch.</text>
    </description>
  </typedef>
  <typedef name="nd-ngb-state">
    <type name="enumeration">
      <enum name="incomplete">
        <value value="0"/>
        <description>
          <text>Address resolution is being performed on the entry.</text>
        </description>
      </enum>
      <enum name="reachable">
        <value value="1"/>
        <description>
          <text>The neighbor is known to have been reachable recently.</text>
        </description>
      </enum>
      <enum name="stale">
        <value value="2"/>
        <description>
          <text>The neighbor is no longer known to be reachable, but until traffic is sent to the neighbor no attempt should be made to verify its reachability.</text>
        </description>
      </enum>
      <enum name="delay">
        <value value="3"/>
        <description>
          <text>The neighbor is no longer known to be reachable, and traffic has recently been sent to the neighbor. Rather than probe the neighbor immediately, however,
 delay sending probes for a short while in order to give upper-layer protocols a chance to provide reachability confirmation.</text>
        </description>
      </enum>
      <enum name="probe">
        <value value="4"/>
        <description>
          <text>The neighbor is no longer known to be reachable, and unicast neighbor solicitation probes are being sent to verify reachability.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>The neighbor unreachability detection state.</text>
    </description>
    <reference>
      <text>RFC 4861: Neighbor Discovery for IP version 6 (IPv6) Section 7.3.2</text>
    </reference>
  </typedef>
  <typedef name="nd-route-preference">
    <type name="enumeration">
      <enum name="medium">
        <value value="0"/>
        <description>
          <text>Preference medium.</text>
        </description>
      </enum>
      <enum name="high">
        <value value="1"/>
        <description>
          <text>Preference high.</text>
        </description>
      </enum>
      <enum name="low">
        <value value="3"/>
        <description>
          <text>Preference low.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>The RA router preference level.</text>
    </description>
  </typedef>
  <typedef name="nd-nb-type">
    <type name="enumeration">
      <enum name="remote">
        <value value="0"/>
        <description>
          <text>Remote pushed.</text>
        </description>
      </enum>
      <enum name="static">
        <value value="1"/>
        <description>
          <text>Statically configured.</text>
        </description>
      </enum>
      <enum name="dynamic">
        <value value="2"/>
        <description>
          <text>Dynamically resolved.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>The origin of neighbor entry.</text>
    </description>
  </typedef>
  <typedef name="nd-pkt-type">
    <type name="enumeration">
      <enum name="RS">
        <value value="0"/>
        <description>
          <text>Router solicitation packet.</text>
        </description>
      </enum>
      <enum name="RA">
        <value value="1"/>
        <description>
          <text>Router advertisement packet.</text>
        </description>
      </enum>
      <enum name="NS">
        <value value="2"/>
        <description>
          <text>Neighbor solicitation packet.</text>
        </description>
      </enum>
      <enum name="NA">
        <value value="3"/>
        <description>
          <text>Neighbor advertisement packet.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>The ND packet type value.</text>
    </description>
  </typedef>
  <typedef name="nd-pkt-evt-type">
    <type name="enumeration">
      <enum name="RS">
        <value value="0"/>
        <description>
          <text>Router solicitation packet.</text>
        </description>
      </enum>
      <enum name="RA">
        <value value="1"/>
        <description>
          <text>Router advertisement packet.</text>
        </description>
      </enum>
      <enum name="NS">
        <value value="2"/>
        <description>
          <text>Neighbor solicitation packet.</text>
        </description>
      </enum>
      <enum name="NA">
        <value value="3"/>
        <description>
          <text>Neighbor advertisement packet.</text>
        </description>
      </enum>
      <enum name="NDMISS">
        <value value="4"/>
        <description>
          <text>ND miss.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>The ND packet type or event value.</text>
    </description>
  </typedef>
  <typedef name="nd-tgt-pkt-type">
    <type name="enumeration">
      <enum name="NS">
        <value value="2"/>
        <description>
          <text>Neighbor solicitation packet.</text>
        </description>
      </enum>
      <enum name="NA">
        <value value="3"/>
        <description>
          <text>Neighbor advertisement packet.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>The type value of ND packet that supports the target address.</text>
    </description>
  </typedef>
  <grouping name="nd-ip-rate-limits-type">
    <description>
      <text>Rate limits for receiving ND packets or ND miss based on specified source IP address.</text>
    </description>
    <container name="ip-rate-limits">
      <description>
        <text>List of rate limits for receiving ND packets or ND miss based on specified source IP address.</text>
      </description>
      <list name="ip-rate-limit">
        <key value="packet-type ipv6-addr"/>
        <description>
          <text>Configure the rate limit for receiving ND packets or ND miss based on specified source IP address.</text>
        </description>
        <leaf name="packet-type">
          <type name="nd-pkt-evt-type"/>
          <description>
            <text>ND packet type or ND miss.</text>
          </description>
        </leaf>
        <leaf name="ipv6-addr">
          <type name="inet:ipv6-address-no-zone"/>
          <description>
            <text>Source IPv6 address, the address cannot be a multicast address, that is, cannot start with FFXX or ffXX, X represents hexadecimal character. When the type is NDMISS, the source address cannot be the unspecified address.</text>
          </description>
        </leaf>
        <leaf name="value">
          <type name="uint16">
            <range value="0..5000"/>
          </type>
          <units name="pps"/>
          <mandatory value="true"/>
          <description>
            <text>Rate limit of receiving ND packet or ND miss.</text>
          </description>
        </leaf>
      </list>
    </container>
  </grouping>
  <container name="ipv6-nd">
    <description>
      <text>ND management.</text>
    </description>
    <container name="global">
      <description>
        <text>Configure global configuration of ND.</text>
      </description>
      <leaf name="ns-send-rate-limit">
        <type name="uint16">
          <range value="1..1000"/>
        </type>
        <units name="pps"/>
        <default value="550"/>
        <description>
          <text>Rate limit of sending NS packet.</text>
        </description>
      </leaf>
      <leaf name="na-send-rate-limit">
        <type name="uint16">
          <range value="1..10240"/>
        </type>
        <units name="pps"/>
        <default value="5000"/>
        <description>
          <text>Rate limit of sending NA packet.</text>
        </description>
      </leaf>
      <leaf name="rs-send-rate-limit">
        <type name="uint16">
          <range value="1..1000"/>
        </type>
        <units name="pps"/>
        <default value="550"/>
        <description>
          <text>Rate limit of sending RS packet.</text>
        </description>
      </leaf>
      <leaf name="ra-send-rate-limit">
        <type name="uint16">
          <range value="1..1000"/>
        </type>
        <units name="pps"/>
        <default value="550"/>
        <description>
          <text>Rate limit of sending RA packet.</text>
        </description>
      </leaf>
      <leaf name="dad-detect">
        <type name="nd-flag"/>
        <default value="true"/>
        <description>
          <text>The switch of DAD enhanced detection.</text>
        </description>
      </leaf>
      <leaf name="mac-check-flag">
        <type name="nd-flag"/>
        <default value="false"/>
        <description>
          <text>Enable IPv6 ND MAC check.</text>
        </description>
      </leaf>
      <leaf name="auto-detect-flag">
        <type name="nd-switch-state"/>
        <default value="on"/>
        <description>
          <text>To enable the automatic detection of ND entries.</text>
        </description>
      </leaf>
      <leaf name="prefix-strict-flag">
        <type name="nd-switch-state"/>
        <default value="on"/>
        <description>
          <text>ND prefix learning strict.</text>
        </description>
      </leaf>
      <leaf name="pre-detect-flag">
        <type name="nd-switch-state"/>
        <default value="off"/>
        <description>
          <text>To determine whether to enable pre-detection.</text>
        </description>
      </leaf>
      <leaf name="hop-limit">
        <type name="uint8">
          <range value="1..255"/>
        </type>
        <default value="64"/>
        <description>
          <text>IPv6 packet hoplimit.</text>
        </description>
      </leaf>
      <leaf name="stale-time">
        <type name="uint32">
          <range value="60..172800"/>
        </type>
        <units name="s"/>
        <default value="1200"/>
        <description>
          <text>Aging time of the global stale state.</text>
        </description>
      </leaf>
      <leaf name="rsa-calculate-rate-limit">
        <type name="uint32">
          <range value="0..100"/>
          <ext:value-meaning>
            <ext:item value="0">
              <ext:meaning value="disabling the function of rsa calculation limit"/>
            </ext:item>
          </ext:value-meaning>
        </type>
        <default value="0"/>
        <description>
          <text>Rate limit for RSA option calculation.</text>
        </description>
      </leaf>
      <leaf name="log-trap-time">
        <type name="uint32">
          <range value="60..3600"/>
        </type>
        <units name="s"/>
        <default value="600"/>
        <description>
          <text>The interval for sending speed-limit logs and alarms.</text>
        </description>
      </leaf>
      <leaf name="rs-receive-rate-limit">
        <type name="uint16">
          <range value="1..5000"/>
        </type>
        <units name="pps"/>
        <ext:dynamic-default/>
        <description>
          <text>Rate limit of receiving RS packet.</text>
        </description>
      </leaf>
      <leaf name="ra-receive-rate-limit">
        <type name="uint16">
          <range value="1..5000"/>
        </type>
        <units name="pps"/>
        <ext:dynamic-default/>
        <description>
          <text>Rate limit of receiving RA packet.</text>
        </description>
      </leaf>
      <leaf name="ns-receive-rate-limit">
        <type name="uint16">
          <range value="1..5000"/>
        </type>
        <units name="pps"/>
        <ext:dynamic-default/>
        <description>
          <text>Rate limit of receiving NS packet.</text>
        </description>
      </leaf>
      <leaf name="na-receive-rate-limit">
        <type name="uint16">
          <range value="1..5000"/>
        </type>
        <units name="pps"/>
        <ext:dynamic-default/>
        <description>
          <text>Rate limit of receiving NA packet.</text>
        </description>
      </leaf>
      <leaf name="miss-receive-rate-limit">
        <type name="uint16">
          <range value="1..5000"/>
        </type>
        <units name="pps"/>
        <ext:dynamic-default/>
        <description>
          <text>Rate limit of receiving ND miss.</text>
        </description>
      </leaf>
      <leaf name="dad-detect-rate-limit">
        <type name="uint16">
          <range value="1..100"/>
        </type>
        <units name="pps"/>
        <default value="50"/>
        <description>
          <text>Rate limit of sending DAD detect packet.</text>
        </description>
      </leaf>
      <leaf name="dad-detect-attempts">
        <type name="uint16">
          <range value="1..5"/>
        </type>
        <default value="3"/>
        <description>
          <text>Number of attempts to resolve address conflicts.</text>
        </description>
      </leaf>
    </container>
    <container name="m-lag-global">
      <description>
        <text>Configure M-LAG global configuration of ND.</text>
      </description>
      <leaf name="ipv6-neighbor-backup-rate">
        <type name="uint32">
          <range value="1..2000"/>
        </type>
        <description>
          <text>Specifies the number of IPv6 neighbor entries synchronized per second through M-LAG.</text>
        </description>
        <ext:dynamic-default/>
      </leaf>
    </container>
    <container name="ipv6-neighbors">
      <config value="false"/>
      <description>
        <text>List of information about neighbor entry query.</text>
      </description>
      <list name="ipv6-neighbor">
        <key value="if-name ipv6-addr"/>
        <description>
          <text>Statistics of query neighbor entries which can be static, or dynamic.</text>
        </description>
        <leaf name="if-name">
          <type name="leafref">
            <path value="/ifm:ifm/ifm:interfaces/ifm:interface/ifm:name"/>
          </type>
          <description>
            <text>Interface name.</text>
          </description>
        </leaf>
        <leaf name="ipv6-addr">
          <type name="inet:ipv6-address-no-zone"/>
          <description>
            <text>IPv6 address of a neighbor entry.</text>
          </description>
        </leaf>
        <leaf name="slot-id">
          <type name="string">
            <length value="1..24"/>
          </type>
          <description>
            <text>Slot ID.</text>
          </description>
          <ext:support-filter value="true"/>
        </leaf>
        <leaf name="mac-addr">
          <type name="pub-type:mac-address"/>
          <description>
            <text>MAC address.</text>
          </description>
        </leaf>
        <leaf name="vpn-name">
          <type name="leafref">
            <path value="/ni:network-instance/ni:instances/ni:instance/ni:name"/>
          </type>
          <description>
            <text>VPN name.</text>
          </description>
          <ext:support-filter value="true"/>
        </leaf>
        <leaf name="vlan-id">
          <type name="uint32">
            <range value="1..4094"/>
          </type>
          <description>
            <text>VLAN ID.</text>
          </description>
          <ext:support-filter value="true"/>
        </leaf>
        <leaf name="state">
          <type name="nd-ngb-state"/>
          <description>
            <text>Status of a neighbor entry.</text>
          </description>
        </leaf>
        <leaf name="static-flag">
          <type name="nd-nb-type"/>
          <description>
            <text>Type of a neighbor entry. The available options are static, dynamic or remote.</text>
          </description>
        </leaf>
        <leaf name="age">
          <type name="uint32">
            <range value="0..4294967293"/>
          </type>
          <units name="min"/>
          <description>
            <text>Time when a neighbor entry is established.</text>
          </description>
        </leaf>
        <leaf name="is-router">
          <type name="nd-flag"/>
          <description>
            <text>Is a router.</text>
          </description>
        </leaf>
        <leaf name="cevid">
          <type name="uint32">
            <range value="1..4094"/>
          </type>
          <description>
            <text>Inner VLAN tag. If this parameter is not set, the value of the VLAN ID is considered to be 0. If this parameter is set, it indicates that the ND entry is in the associated VLAN.</text>
          </description>
        </leaf>
        <leaf name="strict-flag">
          <type name="boolean"/>
          <description>
            <text>Enable/disable strict security mode.</text>
          </description>
        </leaf>
        <leaf name="nickname">
          <type name="uint32">
            <range value="1..65535"/>
          </type>
          <description>
            <text>Nickname which uniquely identifies an RB on a TRILL network.</text>
          </description>
        </leaf>
        <leaf name="bd-id">
          <type name="uint32">
            <range value="1..16777215"/>
          </type>
          <description>
            <text>Bridge-domain ID.</text>
          </description>
        </leaf>
        <leaf name="vni">
          <type name="uint32">
            <range value="1..16777215"/>
          </type>
          <description>
            <text>Vxlan VNI.</text>
          </description>
        </leaf>
        <leaf name="source-addr">
          <type name="inet:ip-address-no-zone"/>
          <description>
            <text>Tunnel SourceIPAddr.</text>
          </description>
        </leaf>
        <leaf name="peer-addr">
          <type name="inet:ip-address-no-zone"/>
          <description>
            <text>Tunnel PeerIPAddr.</text>
          </description>
        </leaf>
      </list>
    </container>
    <container name="nd-pkt-stats">
      <config value="false"/>
      <description>
        <text>Statistics of ND packets.</text>
      </description>
      <container name="ns-stats">
        <description>
          <text>Statistics of NS packets.</text>
        </description>
        <container name="ns-global">
          <description>
            <text>Statistics of global NS packets.</text>
          </description>
          <leaf name="send-total">
            <type name="uint32"/>
            <units name="packet"/>
            <description>
              <text>Number of sent NS packets.</text>
            </description>
          </leaf>
          <leaf name="send-dip-ucast">
            <type name="uint32"/>
            <units name="packet"/>
            <description>
              <text>Number of sent NS packets with the destination address as a unicast address.</text>
            </description>
          </leaf>
          <leaf name="send-dip-mcast">
            <type name="uint32"/>
            <units name="packet"/>
            <description>
              <text>Number of sent NS packets with the destination address as a multicast address.</text>
            </description>
          </leaf>
          <leaf name="send-target-linklocal">
            <type name="uint32"/>
            <units name="packet"/>
            <description>
              <text>Number of sent NS packets with the target address as a link-local address.</text>
            </description>
          </leaf>
          <leaf name="send-target-global">
            <type name="uint32"/>
            <units name="packet"/>
            <description>
              <text>Number of sent NS packets with the target address as a global address.</text>
            </description>
          </leaf>
          <leaf name="recv-total">
            <type name="uint32"/>
            <units name="packet"/>
            <description>
              <text>Number of received NS packets.</text>
            </description>
          </leaf>
          <leaf name="recv-valid-total">
            <type name="uint32"/>
            <units name="packet"/>
            <description>
              <text>Number of received valid NS packets.</text>
            </description>
          </leaf>
          <leaf name="recv-dip-ucast">
            <type name="uint32"/>
            <units name="packet"/>
            <description>
              <text>Number of received NS packets with the destination address as a unicast address.</text>
            </description>
          </leaf>
          <leaf name="recv-dip-mcast">
            <type name="uint32"/>
            <units name="packet"/>
            <description>
              <text>Number of NS packets with the destination address as a multicast address.</text>
            </description>
          </leaf>
          <leaf name="recv-target-linklocal">
            <type name="uint32"/>
            <units name="packet"/>
            <description>
              <text>Number of NS packets with the target address as a link-local address.</text>
            </description>
          </leaf>
          <leaf name="recv-target-global">
            <type name="uint32"/>
            <units name="packet"/>
            <description>
              <text>Number of NS packets with the target address as a global address.</text>
            </description>
          </leaf>
          <leaf name="drop-total">
            <type name="uint32"/>
            <units name="packet"/>
            <description>
              <text>Number of discarded NS packets.</text>
            </description>
          </leaf>
          <leaf name="drop-checksum-err">
            <type name="uint32"/>
            <units name="packet"/>
            <description>
              <text>Number of NS packets discarded due to checksum errors.</text>
            </description>
          </leaf>
          <leaf name="drop-bad-len">
            <type name="uint32"/>
            <units name="packet"/>
            <description>
              <text>Number of NS packets discarded due to bad packet length.</text>
            </description>
          </leaf>
          <leaf name="drop-bad-code">
            <type name="uint32"/>
