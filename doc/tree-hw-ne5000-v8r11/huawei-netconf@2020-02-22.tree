module: huawei-netconf
  +--rw netconf
     +--rw operation-log-switch
     |  +--rw get?   boolean
     +--ro active-modules
        +--ro active-module* [module-name]
           +--ro module-name    string

  rpcs:
    +---x activate-module
       +---w input
          +---w module-name    -> /netconf/active-modules/active-module/module-name
