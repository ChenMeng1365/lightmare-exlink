            </leaf>
            <leaf name="group-query">
              <description>
                <text>The number of sending group-query packets.</text>
              </description>
              <type name="uint32"/>
            </leaf>
            <leaf name="group-report-v1">
              <description>
                <text>The number of sending group-report-v1 packets.</text>
              </description>
              <type name="uint32"/>
            </leaf>
            <leaf name="group-reduce">
              <description>
                <text>The number of sending group-reduce packets.</text>
              </description>
              <type name="uint32"/>
            </leaf>
            <leaf name="group-report-v2">
              <description>
                <text>The number of sending group-report-v2 packets.</text>
              </description>
              <type name="uint32"/>
            </leaf>
            <leaf name="rs">
              <description>
