<?xml version="1.0" encoding="UTF-8"?>
<module name="huawei-system"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:system="urn:huawei:yang:huawei-system"
        xmlns:pub-type="urn:huawei:yang:huawei-pub-type"
        xmlns:ext="urn:huawei:yang:huawei-extension">
  <namespace uri="urn:huawei:yang:huawei-system"/>
  <prefix value="system"/>
  <import module="huawei-pub-type">
    <prefix value="pub-type"/>
  </import>
  <import module="huawei-extension">
    <prefix value="ext"/>
  </import>
  <organization>
    <text>Huawei Technologies Co., Ltd.</text>
  </organization>
  <contact>
    <text>Huawei Industrial Base
Bantian, Longgang
Shenzhen 518129
People's Republic of China
Website: http://www.huawei.com
Email: support@huawei.com</text>
  </contact>
  <description>
    <text>System.</text>
  </description>
  <revision date="2020-03-02">
    <description>
      <text>Initial revision.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <ext:task-name value="system"/>
  <typedef name="risk-level-type">
    <type name="enumeration">
      <enum name="high">
        <value value="0"/>
        <description>
          <text>High level.</text>
        </description>
      </enum>
      <enum name="medium">
        <value value="1"/>
        <description>
          <text>Medium level.</text>
        </description>
      </enum>
      <enum name="low">
        <value value="2"/>
        <description>
          <text>Low level.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Security risk level.</text>
    </description>
  </typedef>
  <typedef name="security-risk-type">
    <type name="enumeration">
      <enum name="insecure-algorithm">
        <value value="0"/>
        <description>
          <text>Insecure algorithm type.</text>
        </description>
      </enum>
      <enum name="insecure-protocol">
        <value value="1"/>
        <description>
          <text>Insecure protocol type.</text>
        </description>
      </enum>
      <enum name="insecure-configuration">
        <value value="2"/>
        <description>
          <text>Insecure configuration type.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Security risk type.</text>
    </description>
  </typedef>
  <container name="system">
    <description>
      <text>System.</text>
    </description>
    <container name="system-info">
      <description>
        <text>Configure system information.</text>
      </description>
      <leaf name="sys-name">
        <type name="string">
          <length value="1..246"/>
        </type>
        <default value="HUAWEI"/>
        <description>
          <text>System name.</text>
        </description>
      </leaf>
      <leaf name="sys-contact">
        <type name="string">
          <length value="1..255"/>
        </type>
        <default value="R&amp;D Beijing, Huawei Technologies co.,Ltd."/>
        <description>
          <text>System contact information.</text>
        </description>
      </leaf>
      <leaf name="sys-location">
        <type name="string">
          <length value="1..255"/>
        </type>
        <default value="Beijing China"/>
        <description>
          <text>System location information.</text>
        </description>
      </leaf>
      <leaf name="sys-desc">
        <type name="string">
          <length value="0..255"/>
        </type>
        <config value="false"/>
        <description>
          <text>Textual description of an entity.</text>
        </description>
      </leaf>
      <leaf name="sys-object-id">
        <type name="string">
          <length value="0..255"/>
        </type>
        <config value="false"/>
        <description>
          <text>Entity OID.</text>
        </description>
      </leaf>
      <leaf name="system-gmt-time">
        <type name="uint32"/>
        <config value="false"/>
        <description>
          <text>Current system time in UTC seconds.</text>
        </description>
      </leaf>
      <leaf name="sys-uptime">
        <type name="pub-type:time-tick"/>
        <config value="false"/>
        <description>
          <text>Time elapses since the system is running.</text>
        </description>
      </leaf>
      <leaf name="sys-service">
        <type name="bits">
          <bit name="physical-layer">
            <position value="0"/>
            <description>
              <text>Physical layer.</text>
            </description>
          </bit>
          <bit name="data-link-layer">
            <position value="1"/>
            <description>
              <text>Data Link layer.</text>
            </description>
          </bit>
          <bit name="network-layer">
            <position value="2"/>
            <description>
              <text>Network layer.</text>
            </description>
          </bit>
          <bit name="transport-layer">
            <position value="3"/>
            <description>
              <text>Transport layer.</text>
            </description>
          </bit>
          <bit name="session-layer">
            <position value="4"/>
            <description>
              <text>Session layer.</text>
            </description>
          </bit>
          <bit name="presentation-layer">
            <position value="5"/>
            <description>
              <text>Presentation layer.</text>
            </description>
          </bit>
          <bit name="application-layer">
            <position value="6"/>
            <description>
              <text>Application layer.</text>
            </description>
          </bit>
        </type>
        <config value="false"/>
        <description>
          <text>Integer value which indicates the set of services that this entity offers.</text>
        </description>
      </leaf>
      <leaf name="platform-name">
        <type name="string">
          <length value="0..255"/>
        </type>
        <config value="false"/>
        <description>
          <text>Name of a platform. For example, Huawei Versatile Routing Platform.</text>
        </description>
      </leaf>
      <leaf name="platform-version">
        <type name="string">
          <length value="1..255"/>
        </type>
        <config value="false"/>
        <description>
          <text>Version of a platform. For example, VRP (R) Software, Version 8.10.</text>
        </description>
      </leaf>
      <leaf name="product-name">
        <type name="string">
          <length value="0..255"/>
        </type>
        <config value="false"/>
        <description>
          <text>Name of a product. For example, Quidway NetEngine 5000E.</text>
        </description>
      </leaf>
      <leaf name="product-version">
        <type name="string">
          <length value="1..255"/>
        </type>
        <config value="false"/>
        <description>
          <text>Product version. The version format is VxxRxxCxxSPxxx, for example, V800R002C01SPC001.</text>
        </description>
      </leaf>
      <leaf name="uname">
        <type name="string">
          <length value="1..52"/>
        </type>
        <config value="false"/>
        <description>
          <text>The array device shall contain a name that identifies the hardware that the system is running on.</text>
        </description>
      </leaf>
      <leaf name="patch-version">
        <type name="string">
          <length value="1..255"/>
        </type>
        <config value="false"/>
        <description>
          <text>Latest patch version. The version format is CPxxx/HPxxx, for example, CP001.</text>
        </description>
      </leaf>
      <leaf name="esn">
        <type name="string">
          <length value="0..511"/>
        </type>
        <config value="false"/>
        <description>
          <text>Product ESN number.</text>
        </description>
      </leaf>
      <leaf name="mac">
        <type name="pub-type:mac-address"/>
        <config value="false"/>
        <description>
          <text>System MAC address.</text>
        </description>
      </leaf>
      <leaf name="software-name">
        <type name="string">
          <length value="4..255"/>
        </type>
        <config value="false"/>
        <description>
          <text>System software name. For example, VRPV800R019C10B210D1111_nos.cc.</text>
        </description>
      </leaf>
    </container>
    <container name="security-risks">
      <config value="false"/>
      <description>
        <text>List of security risk information.</text>
      </description>
      <list name="security-risk">
        <key value="level feature-name type content"/>
        <description>
          <text>Statistics of security risk.</text>
        </description>
        <leaf name="level">
          <type name="risk-level-type"/>
          <description>
            <text>Security risk Level.</text>
          </description>
        </leaf>
        <leaf name="feature-name">
          <type name="string">
            <length value="1..31"/>
          </type>
          <description>
            <text>Feature name.</text>
          </description>
        </leaf>
        <leaf name="type">
          <type name="security-risk-type"/>
          <description>
            <text>Security risk type.</text>
          </description>
        </leaf>
        <leaf name="content">
          <type name="string">
            <length value="1..511"/>
          </type>
          <description>
            <text>Risk information.</text>
          </description>
        </leaf>
        <leaf name="repair-action">
          <type name="string">
            <length value="1..511"/>
          </type>
          <description>
            <text>Repair action.</text>
          </description>
        </leaf>
      </list>
    </container>
  </container>
</module>
