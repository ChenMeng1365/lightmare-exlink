<?xml version="1.0" encoding="UTF-8"?>
<module name="huawei-said-frame"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:sf="urn:huawei:yang:huawei-said-frame"
        xmlns:ext="urn:huawei:yang:huawei-extension"
        xmlns:devm="urn:huawei:yang:huawei-devm">
  <yang-version value="1"/>
  <namespace uri="urn:huawei:yang:huawei-said-frame"/>
  <prefix value="sf"/>
  <import module="huawei-extension">
    <prefix value="ext"/>
  </import>
  <import module="huawei-devm">
    <prefix value="devm"/>
  </import>
  <organization>
    <text>Huawei Technologies Co., Ltd.</text>
  </organization>
  <contact>
    <text>Huawei Industrial Base
Bantian, Longgang
Shenzhen 518129
People's Republic of China
Website: http://www.huawei.com
Email: support@huawei.com
</text>
  </contact>
  <description>
    <text>SAID frame configurations.</text>
  </description>
  <revision date="2020-03-10">
    <description>
      <text>Init revision.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <typedef name="state-type">
    <type name="enumeration">
      <enum name="init">
        <value value="0"/>
        <description>
          <text>Init state.</text>
        </description>
      </enum>
      <enum name="detecting">
        <value value="1"/>
        <description>
          <text>Detecting state.</text>
        </description>
      </enum>
      <enum name="diagnosing">
        <value value="2"/>
        <description>
          <text>Diagnosing state.</text>
        </description>
      </enum>
      <enum name="invalid-diagnose">
        <value value="3"/>
        <description>
          <text>Invalid diagnose state.</text>
        </description>
      </enum>
      <enum name="recovering">
        <value value="4"/>
        <description>
          <text>Recovering state.</text>
        </description>
      </enum>
      <enum name="judging">
        <value value="5"/>
        <description>
          <text>Judging state.</text>
        </description>
      </enum>
      <enum name="exp-judging">
        <value value="6"/>
        <description>
          <text>Exception judging state.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>SAID node running states.</text>
    </description>
  </typedef>
  <typedef name="operation-type">
    <type name="enumeration">
      <enum name="common">
        <value value="0"/>
        <description>
          <text>Common operation type.</text>
        </description>
      </enum>
      <enum name="prevention">
        <value value="1"/>
        <description>
          <text>Prevented operation type.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>SAID node operating types.</text>
    </description>
  </typedef>
  <typedef name="said-name-lpu">
    <type name="enumeration">
      <enum name="ping">
        <value value="2"/>
        <description>
          <text>SAID ping node.</text>
        </description>
      </enum>
      <enum name="ping-trunk">
        <value value="3"/>
        <description>
          <text>SAID ping trunk node.</text>
        </description>
      </enum>
      <enum name="traffic-fail">
        <value value="4"/>
        <description>
          <text>SAID traffic fail node.</text>
        </description>
      </enum>
      <enum name="seu-igp">
        <value value="5"/>
        <description>
          <text>SAID seuigp node.</text>
        </description>
      </enum>
      <enum name="aaa">
        <value value="8"/>
        <description>
          <text>SAID aaa node.</text>
        </description>
      </enum>
      <enum name="seu-trunk">
        <value value="20"/>
        <description>
          <text>SAID seutrunk node.</text>
        </description>
      </enum>
      <enum name="seuloop">
        <value value="22"/>
        <description>
          <text>SAID seuloop node.</text>
        </description>
      </enum>
      <enum name="reset-board">
        <value value="25"/>
        <description>
          <text>SAID reset board node.</text>
        </description>
      </enum>
      <enum name="cfc">
        <value value="26"/>
        <description>
          <text>SAID cfc node.</text>
        </description>
      </enum>
      <enum name="ipnice">
        <value value="28"/>
        <description>
          <text>SAID ipnice node.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>SAID node name in LPU.</text>
    </description>
  </typedef>
  <typedef name="said-name-mpu">
    <type name="enumeration">
      <enum name="ping">
        <value value="2"/>
        <description>
          <text>SAID ping node.</text>
        </description>
      </enum>
      <enum name="ping-trunk">
        <value value="3"/>
        <description>
          <text>SAID ping trunk node.</text>
        </description>
      </enum>
      <enum name="traffic-fail">
        <value value="4"/>
        <description>
          <text>SAID traffic fail node.</text>
        </description>
      </enum>
      <enum name="seu-igp">
        <value value="5"/>
        <description>
          <text>SAID seuigp node.</text>
        </description>
      </enum>
      <enum name="aaa">
        <value value="8"/>
        <description>
          <text>SAID aaa node.</text>
        </description>
      </enum>
      <enum name="seu-trunk">
        <value value="20"/>
        <description>
          <text>SAID seutrunk node.</text>
        </description>
      </enum>
      <enum name="seuloop">
        <value value="22"/>
        <description>
          <text>SAID seuloop node.</text>
        </description>
      </enum>
      <enum name="reset-board">
        <value value="25"/>
        <description>
          <text>SAID reset board node.</text>
        </description>
      </enum>
      <enum name="cfc">
        <value value="26"/>
        <description>
          <text>SAID cfc node.</text>
        </description>
      </enum>
      <enum name="ipnice">
        <value value="28"/>
        <description>
          <text>SAID ipnice node.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>SAID node name in MPU.</text>
    </description>
  </typedef>
  <typedef name="disable-flag">
    <type name="enumeration">
      <enum name="undodisable">
        <value value="0"/>
        <description>
          <text>SAID node is enable.</text>
        </description>
      </enum>
      <enum name="disable">
        <value value="1"/>
        <description>
          <text>SAID node is disable.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>SAID node disable flag.</text>
    </description>
  </typedef>
  <typedef name="enable-flag">
    <type name="enumeration">
      <enum name="disable">
        <value value="0"/>
        <description>
          <text>SAID node is disable.</text>
        </description>
      </enum>
      <enum name="enable">
        <value value="1"/>
        <description>
          <text>SAID node is enable.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>SAID node enable flag.</text>
    </description>
  </typedef>
  <ext:task-name value="saidframe"/>
  <container name="said-frame">
    <description>
      <text>SAID frame.</text>
    </description>
    <container name="global-switch">
      <description>
        <text>Configure SAID global switch.</text>
      </description>
      <leaf name="disable">
        <type name="disable-flag"/>
        <default value="undodisable"/>
        <description>
          <text>SAID global disable flag.</text>
        </description>
      </leaf>
    </container>
    <container name="lpu-nodes">
      <config value="false"/>
      <description>
        <text>List of SAID node information in all LPUs.</text>
      </description>
      <list name="lpu-node">
        <key value="said-name slot-id"/>
        <description>
          <text>Statistics of SAID node information in a LPU.</text>
        </description>
        <leaf name="said-name">
          <type name="said-name-lpu"/>
          <description>
            <text>SAID node name.</text>
          </description>
        </leaf>
        <leaf name="slot-id">
          <type name="string">
            <length value="1..16"/>
          </type>
          <description>
            <text>Indicates the slot ID of a board.</text>
          </description>
        </leaf>
        <leaf name="operate">
          <type name="operation-type"/>
          <description>
            <text>SAID node operating type.</text>
          </description>
        </leaf>
        <leaf name="state">
          <type name="state-type"/>
          <description>
            <text>SAID node running state.</text>
          </description>
        </leaf>
        <leaf name="cycle">
          <type name="uint32"/>
          <description>
            <text>Detecting cycle.</text>
          </description>
        </leaf>
        <leaf name="enable">
          <type name="enable-flag"/>
          <description>
            <text>SAID node enable flag.</text>
          </description>
        </leaf>
      </list>
    </container>
    <container name="lpunodecfgs">
      <description>
        <text>List of SAID node configurations in all LPUs.</text>
      </description>
      <list name="lpunodecfg">
        <key value="said-name slot-id"/>
        <description>
          <text>Configure SAID node configurations in a LPU.</text>
        </description>
        <leaf name="said-name">
          <type name="said-name-lpu"/>
          <description>
            <text>SAID node name.</text>
          </description>
        </leaf>
        <leaf name="slot-id">
          <type name="string">
            <length value="1..16"/>
          </type>
          <description>
            <text>Indicates the slot ID of a board.</text>
          </description>
        </leaf>
        <leaf name="disable">
          <type name="disable-flag"/>
          <mandatory value="true"/>
          <description>
            <text>Disable flag.</text>
          </description>
        </leaf>
      </list>
    </container>
    <container name="mpu-nodes">
      <config value="false"/>
      <description>
        <text>List of SAID node information in all MPUs.</text>
      </description>
      <list name="mpu-node">
        <key value="said-name"/>
        <description>
          <text>Statistics of SAID node information in a MPU.</text>
        </description>
        <leaf name="said-name">
          <type name="said-name-mpu"/>
          <description>
            <text>SAID node name.</text>
          </description>
        </leaf>
        <leaf name="slot-id">
          <type name="leafref">
            <path value="/devm:devm/devm:mpu-boards/devm:mpu-board/devm:position"/>
          </type>
          <description>
            <text>Slot ID.</text>
          </description>
        </leaf>
        <leaf name="operate">
          <type name="operation-type"/>
          <description>
            <text>SAID node operating type.</text>
          </description>
        </leaf>
        <leaf name="state">
          <type name="state-type"/>
          <description>
            <text>SAID node running state.</text>
          </description>
        </leaf>
        <leaf name="cycle">
          <type name="uint32"/>
          <description>
            <text>Detecting cycle.</text>
          </description>
        </leaf>
        <leaf name="enable">
          <type name="enable-flag"/>
          <description>
            <text>SAID node enable flag.</text>
          </description>
        </leaf>
      </list>
    </container>
    <container name="mpunodecfgs">
      <description>
        <text>List of SAID node configurations in all MPUs.</text>
      </description>
      <list name="mpunodecfg">
        <key value="said-name"/>
        <description>
          <text>Configure SAID node configurations in a MPU.</text>
        </description>
        <leaf name="said-name">
          <type name="said-name-mpu"/>
          <description>
            <text>SAID node name.</text>
          </description>
        </leaf>
        <leaf name="disable">
          <type name="disable-flag"/>
          <mandatory value="true"/>
          <description>
            <text>Disable flag.</text>
          </description>
        </leaf>
      </list>
    </container>
  </container>
</module>
