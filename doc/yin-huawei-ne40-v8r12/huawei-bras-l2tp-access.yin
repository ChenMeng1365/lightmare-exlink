  <augment target-node="/aaa:aaa/aaa:domains/aaa:domain/bras-basic-access:bas">
    <description>
      <text>Adds l2tp configuration to aaa domain model.</text>
    </description>
    <container name="l2tp">
      <description>
        <text>Configure l2tp to aaa domain model.</text>
      </description>
      <container name="domain-radius-force-enable">
        <description>
          <text>Configure the L2TP attributes delivered by the RADIUS server for the domain users.</text>
        </description>
        <leaf name="radius-force-enable">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable the L2TP attributes delivered by the RADIUS server for the domain users.</text>
          </description>
        </leaf>
      </container>
      <container name="authorization">
        <description>
          <text>Configure the domain name-based authorization for L2TP users.</text>
        </description>
        <leaf name="password">
          <type name="pub-type:password-extend">
            <length value="1..128"/>
          </type>
          <description>
            <text>The password is a string ranging from 1 to 16 characters for a plaintext password and 1 to 128 characters for a ciphertext password.</text>
          </description>
        </leaf>
      </container>
      <container name="groups">
        <description>
          <text>Configure an L2TP group in the domain on the LAC.</text>
        </description>
        <leaf-list name="name">
          <type name="leafref">
            <path value="/bras-l2tp-access:bras-l2tp-access/bras-l2tp-access:l2tp-groups/bras-l2tp-access:l2tp-group/bras-l2tp-access:name"/>
          </type>
          <max-elements value="8"/>
          <description>
            <text>Configure the name of an L2TP group.</text>
          </description>
        </leaf-list>
      </container>
      <container name="qos-profile">
        <description>
          <text>Configure a QoS profile to an AAA domain for L2TP users.</text>
        </description>
        <leaf name="inbound-profile-name">
          <type name="leafref">
            <path value="/qos:qos/qos:queue-scheduler-template/qos:profiles/qos:profile/qos:name"/>
          </type>
          <must condition="/qos:qos/qos:queue-scheduler-template/qos:profiles/qos:profile[qos:name=current()]/qos:mode='normal'"/>
          <description>
            <text>Specifies the name of a QoS profile to the incoming traffic of an AAA domain for L2TP users.</text>
          </description>
        </leaf>
        <leaf name="outbound-profile-name">
          <type name="leafref">
            <path value="/qos:qos/qos:queue-scheduler-template/qos:profiles/qos:profile/qos:name"/>
          </type>
          <must condition="/qos:qos/qos:queue-scheduler-template/qos:profiles/qos:profile[qos:name=current()]/qos:mode='normal'"/>
          <description>
            <text>Specifies the name of a QoS profile to the outgoing traffic of an AAA domain for L2TP users.</text>
          </description>
        </leaf>
      </container>
      <container name="dslam-connect-speed">
        <description>
          <text>Configure an LAC to encapsulate the DSLAM's upstream and downstream rates respectively into the AVP24 and AVP3 attributes of packets to be sent to an LNS.</text>
        </description>
        <leaf name="connect-speed-enable">
          <type name="empty"/>
          <description>
            <text>Enables an LAC to encapsulate the DSLAM's upstream and downstream rates respectively into the AVP24 and AVP3 attributes of packets to be sent to an LNS.</text>
          </description>
        </leaf>
      </container>
      <container name="tunnel-account-rfc-2867">
        <description>
          <text>Configure that after the accounting defined by RFC 2867 for tunnel users is enabled, the accounting packets for the tunnel and users in the tunnel are sent to the authentication server based on the format defined by RFC 2867.</text>
        </description>
        <leaf name="account-enable">
          <type name="empty"/>
          <description>
            <text>Enables the accounting defined by RFC 2867 for tunnel users.</text>
          </description>
        </leaf>
      </container>
      <container name="priority-car-of-tcp-ack-no-payload">
        <must condition="not(./lns-edsg-ipv4='false' and ./lns-edsg-ipv4-and-ipv6='true')"/>
        <description>
          <text>Configure the priority car of TCP ACK packet for the no payload.</text>
        </description>
        <leaf name="lns-priority-car">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable the priority car of TCP ACK packet for the no payload of LNS users.</text>
          </description>
        </leaf>
        <leaf name="lns-edsg-ipv4">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable the priority car of TCP ACK packet for the no payload of LNS users, who set edsg service and whose IP type is IPv4.</text>
          </description>
        </leaf>
        <leaf name="lns-edsg-ipv4-and-ipv6">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable the priority car of TCP ACK packet for the no payload of LNS users, who set edsg service and whose IP type is IPv4/IPv6.</text>
          </description>
        </leaf>
      </container>
    </container>
  </augment>
  <augment target-node="/ifm:ifm/ifm:interfaces/ifm:interface">
