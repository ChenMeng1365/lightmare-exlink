  <grouping name="pwe3-ping-trace-result-type">
    <description>
      <text>PWE3 test result.</text>
    </description>
    <container name="ping-results">
      <config value="false"/>
      <description>
        <text>List of PWE3 ping test results.</text>
      </description>
      <list name="ping-result">
        <key value="test-name"/>
        <description>
          <text>Statistics of PWE3 ping test result.</text>
        </description>
        <leaf name="test-name">
          <type name="string">
            <length value="1..32"/>
          </type>
          <description>
            <text>Test name.</text>
          </description>
        </leaf>
        <leaf name="pw-type">
          <type name="pw-type"/>
          <description>
            <text>PW type.</text>
          </description>
        </leaf>
        <leaf name="pw-id">
          <type name="uint32">
            <range value="1..4294967295"/>
          </type>
          <description>
            <text>PW ID.</text>
          </description>
        </leaf>
        <leaf name="peer-address">
          <type name="inet:ipv4-address-no-zone"/>
          <description>
            <text>Peer IP address.</text>
          </description>
        </leaf>
        <leaf name="sender-ve-id">
          <type name="uint32">
            <range value="0..65534"/>
          </type>
          <description>
            <text>Sender VE ID.</text>
          </description>
        </leaf>
        <leaf name="remote-ve-id">
          <type name="uint32">
            <range value="0..65534"/>
          </type>
          <description>
            <text>Remote VE ID.</text>
          </description>
        </leaf>
        <leaf name="packet-send">
          <type name="uint32"/>
          <description>
            <text>Total number of sent packets.</text>
          </description>
        </leaf>
        <leaf name="packet-receive">
          <type name="uint32"/>
          <description>
            <text>Total number of received packets.</text>
          </description>
        </leaf>
        <leaf name="loss-ratio">
          <type name="pub-type:percent"/>
          <units name="%"/>
          <description>
            <text>Packet loss ratio of a test.</text>
          </description>
        </leaf>
        <leaf name="rtt-min">
          <type name="uint32"/>
          <units name="ms"/>
          <description>
            <text>Minimum RTT of all test packets.</text>
          </description>
        </leaf>
        <leaf name="rtt-max">
          <type name="uint32"/>
          <units name="ms"/>
          <description>
            <text>Maximum RTT of all test packets.</text>
          </description>
        </leaf>
        <leaf name="rtt-average">
          <type name="uint32"/>
          <units name="ms"/>
          <description>
            <text>Average RTT of all test packets.</text>
          </description>
        </leaf>
        <leaf name="status">
          <type name="dgntl-test-status-type"/>
          <description>
            <text>Test status.</text>
          </description>
        </leaf>
        <container name="probes">
          <description>
            <text>List of PWE3 ping results of all packets in a test.</text>
          </description>
          <list name="probe">
            <key value="sequence"/>
            <description>
              <text>Statistics of probe result of a single packet in a PWE3 ping test.</text>
            </description>
            <leaf name="sequence">
              <type name="uint32"/>
              <description>
                <text>Serial number of a test packet received.</text>
              </description>
            </leaf>
            <leaf name="replier">
              <type name="inet:ipv4-address-no-zone"/>
              <description>
                <text>Reply address in a received response packet.</text>
              </description>
            </leaf>
            <leaf name="data-bytes">
              <type name="uint32"/>
              <description>
                <text>Number of bytes in a received response packet.</text>
              </description>
            </leaf>
            <leaf name="rtt">
              <type name="uint32"/>
              <units name="ms"/>
              <description>
                <text>RTT of test packets, expressed in milliseconds.</text>
              </description>
            </leaf>
            <leaf name="ttl">
              <type name="uint8">
                <range value="1..255"/>
              </type>
              <description>
                <text>TTL of received response packets.</text>
              </description>
            </leaf>
            <leaf name="return-code">
              <type name="uint32"/>
              <description>
                <text>Return code.</text>
              </description>
            </leaf>
            <leaf name="return-subcode">
              <type name="uint32"/>
              <description>
                <text>Return sub-code.</text>
              </description>
            </leaf>
            <leaf name="result-type">
              <type name="probe-result-type"/>
              <description>
                <text>Result of packet sending, which is either Success or Timeout.</text>
              </description>
            </leaf>
            <leaf name="is-delete">
              <type name="boolean"/>
              <ext:support-filter value="true"/>
              <description>
                <text>Enable/disable the function of deleting test results.</text>
              </description>
            </leaf>
          </list>
        </container>
      </list>
    </container>
    <container name="trace-results">
      <config value="false"/>
      <description>
        <text>List of PWE3 trace test results.</text>
      </description>
      <list name="trace-result">
        <key value="test-name"/>
        <description>
          <text>Statistics of PWE3 trace test result.</text>
        </description>
        <leaf name="test-name">
          <type name="string">
            <length value="1..32"/>
          </type>
          <description>
            <text>Test name.</text>
          </description>
        </leaf>
        <leaf name="pw-type">
          <type name="pw-type"/>
          <description>
            <text>PW type.</text>
          </description>
        </leaf>
        <leaf name="pw-id">
          <type name="uint32"/>
          <description>
            <text>PW index.</text>
          </description>
        </leaf>
        <leaf name="peer-address">
          <type name="inet:ipv4-address-no-zone"/>
          <description>
            <text>Peer IPv4 address.</text>
          </description>
        </leaf>
        <leaf name="sender-ve-id">
          <type name="uint16"/>
          <description>
            <text>Sender VE ID.</text>
          </description>
        </leaf>
        <leaf name="remote-ve-id">
          <type name="uint16"/>
          <description>
            <text>Remote VE ID.</text>
          </description>
        </leaf>
        <leaf name="status">
          <type name="dgntl-test-status-type"/>
          <description>
            <text>Test status.</text>
          </description>
        </leaf>
        <container name="hops">
          <description>
            <text>List of PWE3 trace result of each packet in a test.</text>
          </description>
          <list name="hop">
            <key value="hop-index"/>
            <description>
              <text>Statistics of PWE3 trace result of a single packet in a test.</text>
            </description>
            <leaf name="hop-index">
              <type name="uint32">
                <range value="1..255"/>
              </type>
              <description>
                <text>The index of the hop.</text>
              </description>
            </leaf>
            <leaf name="ttl">
              <type name="uint8"/>
              <description>
                <text>TTL of received response packet.</text>
              </description>
            </leaf>
            <leaf name="replier">
              <type name="inet:ipv4-address-no-zone"/>
              <description>
                <text>Reply address of the related hop in a test.</text>
              </description>
            </leaf>
            <leaf name="rtt">
              <type name="uint32"/>
              <description>
                <text>RTT of test packets, expressed in milliseconds.</text>
              </description>
            </leaf>
            <leaf name="transmit-type">
              <type name="transmit-type"/>
              <description>
                <text>Node type.</text>
              </description>
            </leaf>
            <leaf name="ds-address">
              <type name="inet:ipv4-address-no-zone"/>
              <description>
                <text>Downstream address of the related hop in a test.</text>
              </description>
            </leaf>
            <leaf name="return-code">
              <type name="uint32"/>
              <description>
                <text>Return code.</text>
              </description>
            </leaf>
            <leaf name="result-type">
              <type name="probe-result-type"/>
              <description>
                <text>Result of packet sending, which is either Success or Timeout.</text>
              </description>
            </leaf>
            <leaf name="is-delete">
              <type name="boolean"/>
              <ext:support-filter value="true"/>
              <description>
                <text>Enable/disable the function of deleting test results.</text>
              </description>
            </leaf>
            <container name="ds-label-lists">
              <description>
                <text>List of downstream information.</text>
              </description>
              <list name="ds-label-list">
                <key value="label-index"/>
                <description>
                  <text>Statistics of downstream information.</text>
                </description>
                <leaf name="label-index">
                  <type name="uint32">
                    <range value="1..4294967295"/>
                  </type>
                  <description>
                    <text>The index of the downstream label.</text>
                  </description>
                </leaf>
                <leaf name="ds-label">
                  <type name="uint32"/>
                  <description>
                    <text>Downstream label.</text>
                  </description>
                </leaf>
              </list>
            </container>
          </list>
        </container>
      </list>
    </container>
  </grouping>
  <rpc name="pwe3-start-ping">
    <description>
      <text>Start pwe3 ping.</text>
    </description>
    <ext:node-ref value="/diagnostic-tools:diagnostic-tools/diagnostic-tools:pwe3/diagnostic-tools:ping-results"/>
    <input>
      <leaf name="test-name">
        <type name="string">
          <length value="1..32"/>
        </type>
        <mandatory value="true"/>
        <description>
          <text>Test name.</text>
        </description>
      </leaf>
      <leaf name="label-type">
        <type name="pwe3-label-type"/>
        <mandatory value="true"/>
        <description>
          <text>Label type.</text>
        </description>
      </leaf>
      <choice name="detection-type">
        <mandatory value="true"/>
        <description>
          <text>Detection type.</text>
        </description>
        <case name="pw-id">
          <description>
            <text>Detecting by PW ID.</text>
          </description>
          <leaf name="pw-type">
            <type name="pw-type"/>
            <mandatory value="true"/>
            <description>
              <text>PW type.</text>
            </description>
          </leaf>
          <leaf name="pw-id">
            <type name="uint32">
              <range value="1..4294967295"/>
            </type>
            <mandatory value="true"/>
            <description>
              <text>PW ID.</text>
            </description>
          </leaf>
          <leaf name="peer-address">
            <type name="inet:ipv4-address-no-zone"/>
            <description>
              <text>Peer IP address.</text>
            </description>
          </leaf>
          <leaf name="remote-pw-id">
            <type name="uint32">
              <range value="1..4294967295"/>
            </type>
            <description>
              <text>Remote PW ID.</text>
            </description>
          </leaf>
          <leaf name="remote-address">
            <type name="inet:ipv4-address-no-zone"/>
            <description>
              <text>Remote IP address.</text>
            </description>
          </leaf>
          <leaf name="sender-address">
            <type name="inet:ipv4-address-no-zone"/>
            <description>
              <text>Set the source address of the targeted session.</text>
            </description>
          </leaf>
          <leaf name="ttl">
            <type name="uint8">
              <range value="1..255"/>
            </type>
            <description>
              <text>Time to live.</text>
            </description>
          </leaf>
          <leaf name="ttl-mode">
            <type name="ttl-mode"/>
            <description>
              <text>TTL mode (uniform or pipe).</text>
            </description>
          </leaf>
        </case>
        <case name="vpws-bgp-vpn-instance">
          <description>
            <text>Detecting by VPWS BGP VPN instance.</text>
          </description>
          <leaf name="vpws-bgp-vpn-name">
            <type name="string">
              <length value="1..31"/>
            </type>
            <mandatory value="true"/>
            <description>
              <text>VPWS BGP VPN instance name.</text>
            </description>
          </leaf>
          <leaf name="local-ce-id">
            <type name="uint32">
              <range value="0..65534"/>
            </type>
            <mandatory value="true"/>
            <description>
              <text>Local CE ID.</text>
            </description>
          </leaf>
          <leaf name="remote-ce-id">
            <type name="uint32">
              <range value="0..65534"/>
            </type>
            <mandatory value="true"/>
            <description>
              <text>Remote CE ID.</text>
            </description>
          </leaf>
        </case>
      </choice>
      <leaf name="exp">
        <type name="uint32">
          <range value="0..7"/>
        </type>
        <default value="0"/>
        <description>
          <text>EXP value in the label field of MPLS packets. The EXP value is used to distinguish the service types of MPLS packets.</text>
        </description>
      </leaf>
      <leaf name="reply-mode">
        <type name="reply-mode-pwe3"/>
        <default value="udp"/>
        <description>
          <text>Mode for responding to test packets in a test.</text>
        </description>
      </leaf>
      <leaf name="packet-size">
        <type name="uint32">
          <range value="65..9600"/>
        </type>
        <default value="100"/>
        <description>
          <text>Number of bytes in a sent test packet.</text>
        </description>
      </leaf>
      <leaf name="count">
        <type name="uint32">
          <range value="1..4294967295"/>
        </type>
        <default value="5"/>
        <description>
          <text>Number of sent test packets.</text>
        </description>
      </leaf>
      <leaf name="interval">
        <type name="uint32">
          <range value="1..10000"/>
        </type>
        <units name="ms"/>
        <default value="2000"/>
        <description>
          <text>Interval for sending test packets.</text>
        </description>
      </leaf>
      <leaf name="timeout">
        <type name="uint32">
          <range value="0..65535"/>
        </type>
        <units name="ms"/>
        <default value="2000"/>
        <description>
          <text>Time to wait for a response packet after a test packet is sent. If no response packet is received within the timeout period, the system considers that the test packet times out.</text>
        </description>
      </leaf>
      <leaf name="bypass-source-if-name">
        <type name="leafref">
          <path value="/ifm:ifm/ifm:interfaces/ifm:interface/ifm:name"/>
        </type>
        <description>
          <text>Set the specified interface as the source interface of ping packet.</text>
        </description>
      </leaf>
      <leaf name="fragment">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable packet fragmentation function.</text>
        </description>
      </leaf>
      <leaf name="no-control-word">
        <type name="boolean"/>
        <description>
          <text>Enable/disable the function of not carrying control word.</text>
        </description>
      </leaf>
    </input>
  </rpc>
  <rpc name="pwe3-stop-ping">
    <description>
      <text>Stop pwe3 ping.</text>
    </description>
    <ext:node-ref value="/diagnostic-tools:diagnostic-tools/diagnostic-tools:pwe3/diagnostic-tools:ping-results"/>
    <input>
      <leaf name="test-name">
        <type name="leafref">
          <path value="/diagnostic-tools:diagnostic-tools/diagnostic-tools:pwe3/diagnostic-tools:ping-results/diagnostic-tools:ping-result/diagnostic-tools:test-name"/>
        </type>
        <mandatory value="true"/>
        <description>
          <text>Test name.</text>
        </description>
      </leaf>
    </input>
  </rpc>
  <rpc name="pwe3-delete-ping">
    <description>
      <text>Delete pwe3 ping.</text>
    </description>
    <ext:node-ref value="/diagnostic-tools:diagnostic-tools/diagnostic-tools:pwe3/diagnostic-tools:ping-results"/>
    <input>
      <leaf name="test-name">
        <type name="leafref">
          <path value="/diagnostic-tools:diagnostic-tools/diagnostic-tools:pwe3/diagnostic-tools:ping-results/diagnostic-tools:ping-result/diagnostic-tools:test-name"/>
        </type>
        <mandatory value="true"/>
        <description>
          <text>Test name.</text>
        </description>
      </leaf>
    </input>
  </rpc>
  <rpc name="pwe3-start-trace">
    <description>
      <text>Start PWE3 trace.</text>
    </description>
    <ext:node-ref value="/diagnostic-tools:diagnostic-tools/diagnostic-tools:pwe3/diagnostic-tools:trace-results"/>
    <input>
      <leaf name="test-name">
        <type name="string">
          <length value="1..32"/>
        </type>
        <mandatory value="true"/>
        <description>
          <text>Test name.</text>
        </description>
      </leaf>
      <choice name="detection-type">
        <mandatory value="true"/>
        <description>
          <text>Detection type.</text>
        </description>
        <case name="pw-id">
          <description>
            <text>Detecting by PW ID.</text>
          </description>
          <leaf name="pw-type">
            <type name="pw-type"/>
            <mandatory value="true"/>
            <description>
              <text>PW type.</text>
            </description>
          </leaf>
          <leaf name="pw-id">
            <type name="uint32">
              <range value="1..4294967295"/>
            </type>
            <mandatory value="true"/>
            <description>
              <text>PW ID.</text>
            </description>
          </leaf>
          <leaf name="peer-address">
            <type name="inet:ipv4-address-no-zone"/>
            <description>
              <text>Peer IPv4 address.</text>
            </description>
          </leaf>
        </case>
        <case name="vpws-bgp-vpn-instance">
          <description>
            <text>Detecting by VPWS BGP VPN instance.</text>
          </description>
          <leaf name="vpws-bgp-vpn-name">
            <type name="string">
              <length value="1..31"/>
            </type>
            <mandatory value="true"/>
            <description>
              <text>VPWS BGP VPN instance name.</text>
            </description>
          </leaf>
          <leaf name="local-ce-id">
            <type name="uint32">
              <range value="0..65534"/>
            </type>
            <mandatory value="true"/>
            <description>
              <text>Local VC ID.</text>
            </description>
          </leaf>
          <leaf name="remote-ce-id">
            <type name="uint32">
              <range value="0..65534"/>
            </type>
            <mandatory value="true"/>
            <description>
              <text>Remote VC ID.</text>
            </description>
          </leaf>
        </case>
      </choice>
      <leaf name="label-type">
        <type name="pwe3-label-type"/>
        <mandatory value="true"/>
        <description>
          <text>Label type.</text>
        </description>
      </leaf>
      <leaf name="no-control-word">
        <type name="empty"/>
        <description>
          <text>No control word.</text>
        </description>
      </leaf>
      <leaf name="remote-address">
        <type name="inet:ipv4-address-no-zone"/>
        <description>
          <text>Remote address.</text>
        </description>
      </leaf>
      <leaf name="exp">
        <type name="uint32">
          <range value="0..7"/>
        </type>
        <default value="0"/>
        <description>
          <text>EXP value in the label field of MPLS packets. The EXP value is used to distinguish the service types of MPLS packets.</text>
        </description>
      </leaf>
      <leaf name="first-ttl">
        <type name="uint8">
          <range value="1..255"/>
        </type>
        <default value="1"/>
        <description>
          <text>First time to live.</text>
        </description>
      </leaf>
      <leaf name="max-ttl">
        <type name="uint8">
          <range value="1..255"/>
        </type>
        <default value="30"/>
        <description>
          <text>Maximum time to live.</text>
        </description>
      </leaf>
      <leaf name="reply-mode">
        <type name="reply-mode-pwe3"/>
        <default value="udp"/>
        <description>
          <text>Mode for responding to test packets in a test.</text>
        </description>
      </leaf>
      <leaf name="timeout">
        <type name="uint32">
          <range value="0..65535"/>
        </type>
        <units name="ms"/>
        <default value="2000"/>
        <description>
          <text>Time to wait for a response packet after a test packet is sent. If no response packet is received within the timeout period, the system considers that the test packet times out.</text>
        </description>
      </leaf>
      <leaf name="fragment">
        <type name="empty"/>
        <description>
          <text>Set the Fragment flag in packet.</text>
        </description>
      </leaf>
      <leaf name="ptn-mode">
        <type name="empty"/>
        <description>
          <text>PTN-Interworking mode.</text>
        </description>
      </leaf>
      <leaf name="ttl-mode">
        <type name="ttl-mode"/>
        <description>
          <text>TTL mode.</text>
        </description>
      </leaf>
      <leaf name="detail">
        <type name="empty"/>
        <description>
          <text>Detail information.</text>
        </description>
      </leaf>
      <leaf name="bypass-source-if-name">
        <type name="leafref">
          <path value="/ifm:ifm/ifm:interfaces/ifm:interface/ifm:name"/>
        </type>
        <description>
          <text>Set the interface as the source interface of tracert packet when specifying PW bypass LSP.</text>
        </description>
      </leaf>
    </input>
  </rpc>
  <rpc name="pwe3-stop-trace">
    <description>
      <text>Stop PWE3 trace.</text>
    </description>
    <ext:node-ref value="/diagnostic-tools:diagnostic-tools/diagnostic-tools:pwe3/diagnostic-tools:trace-results"/>
    <input>
      <leaf name="test-name">
        <type name="leafref">
          <path value="/diagnostic-tools:diagnostic-tools/diagnostic-tools:pwe3/diagnostic-tools:trace-results/diagnostic-tools:trace-result/diagnostic-tools:test-name"/>
        </type>
        <description>
          <text>Test name.</text>
        </description>
      </leaf>
    </input>
  </rpc>
  <rpc name="pwe3-delete-trace">
    <description>
      <text>Delete PWE3 trace.</text>
    </description>
    <ext:node-ref value="/diagnostic-tools:diagnostic-tools/diagnostic-tools:pwe3/diagnostic-tools:trace-results"/>
    <input>
      <leaf name="test-name">
        <type name="leafref">
          <path value="/diagnostic-tools:diagnostic-tools/diagnostic-tools:pwe3/diagnostic-tools:trace-results/diagnostic-tools:trace-result/diagnostic-tools:test-name"/>
        </type>
        <description>
          <text>Test name.</text>
        </description>
      </leaf>
    </input>
  </rpc>
  <augment target-node="/diagnostic-tools:diagnostic-tools">
    <description>
      <text>List of PWE3 ping and trace tests.</text>
    </description>
    <container name="pwe3">
      <config value="false"/>
      <description>
        <text>Statistics of PWE3 ping and trace tests.</text>
      </description>
