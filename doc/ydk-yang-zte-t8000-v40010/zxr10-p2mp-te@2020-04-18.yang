            type ncd:ipv4-address ;
          }
          leaf backuptunnel-index {
            description 'The index of backup tunnel.'
                        ;
            type uint16 ;
          }
          leaf protected-ifname {
            description 'The name of protected interface.'
                        ;
            type string {
              length 0..32 ;
            }
          }
          leaf inner-label {
            description 'Inner lable.'
                        ;
            type uint32 ;
          }
          leaf frrstatus {
            description 'The status of frr.'
                        ;
            type enumeration {
              enum ready {
                value 1 ;
              }
              enum active {
                value 2 ;
              }
            }
          }
          ncd:select 'protected-if-index pritunnel-index pritunnel-p2mpId pritu'
                   + 'nnel-ingressid pritunnel-lspid pritunnel-prevhop pritunne'
                   + 'l-nexthop'
                     ;
        }
      }
      container leaf-tunnel {
        description 'The state of leaf tunnel.'
                    ;
        list tunnel {
          description 'Leaf tunnel.'
                      ;
          key tunnel-id ;
          leaf tunnel-id {
            description 'The index of leaf tunnel.'
                        ;
            type uint16 {
              range 1..65535 ;
            }
          }
          leaf tnnl-name {
            description 'The name of tunnel.'
                        ;
            type ncd:ifname ;
            default null ;
          }
          leaf operstatus {
            description 'The operstatus of tunnel.'
                        ;
            type enumeration {
              enum down {
                value 0 ;
              }
              enum up {
                value 1 ;
              }
            }
            default down ;
          }
        }
      }
      container static-mtunnel {
        description 'The state of the static P2MP tunnel.'
                    ;
        list tunnel {
          description 'The static P2MP tunnel list.'
                      ;
          key tunnel-id ;
          leaf tunnel-id {
            description 'The tunnel id of the static p2mp tunnel.'
                        ;
            type uint32 ;
          }
          leaf admin_status {
            description 'The adminstatus of tunnel.'
                        ;
            type enumeration {
              enum down {
                value 1 ;
              }
              enum up {
                value 0 ;
              }
            }
          }
          leaf oper_status {
            description 'The operstatus of tunnel.'
                        ;
            type enumeration {
              enum down {
                value 1 ;
              }
              enum up {
                value 0 ;
              }
            }
          }
        }
        ncd:revise-date 2016-03-11 ;
      }
    }
