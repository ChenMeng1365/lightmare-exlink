        <description>
          <text>Calculate the active/standby mode according to the active link number.</text>
        </description>
      </enum>
      <enum name="bandwidth">
        <value value="2"/>
        <description>
          <text>Calculate the active/standby mode according to the available bandwidth.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>E-Trunk select mode.</text>
    </description>
  </typedef>
  <typedef name="bfd-state">
    <type name="enumeration">
      <enum name="up">
        <value value="2"/>
        <description>
          <text>The BFD state is up.</text>
        </description>
      </enum>
      <enum name="down">
        <value value="1"/>
        <description>
          <text>The BFD state is down.</text>
        </description>
      </enum>
      <enum name="unknown">
        <value value="0"/>
        <description>
          <text>The BFD state is unknown.</text>
        </description>
      </enum>
