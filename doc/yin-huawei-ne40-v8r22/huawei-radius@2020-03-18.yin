        <key value="server-type server-ip-address server-port vpn-name"/>
        <description>
          <text>Configure the maximum number of pending packets or the limit on the number of packets sent to a RADIUS server within a specified period.</text>
        </description>
        <leaf name="server-type">
          <type name="enumeration">
            <enum name="authentication">
              <value value="1"/>
              <description>
                <text>Radius authentication server.</text>
              </description>
            </enum>
            <enum name="accounting">
              <value value="2"/>
              <description>
                <text>Radius accounting server.</text>
              </description>
            </enum>
          </type>
          <description>
            <text>Configure accounting server or authentication server.</text>
          </description>
        </leaf>
        <leaf name="server-ip-address">
          <type name="leafref">
            <path value="../../../radius-server-groups/radius-server-group/radius-server-ipv4s/radius-server-ipv4[server-type=current()/../server-type]/server-ip-address"/>
          </type>
          <description>
            <text>IP address of the server.</text>
          </description>
        </leaf>
        <leaf name="server-port">
          <type name="leafref">
            <path value="../../../radius-server-groups/radius-server-group/radius-server-ipv4s/radius-server-ipv4[server-type=current()/../server-type][server-ip-address = current()/../server-ip-address]/server-port"/>
          </type>
          <description>
            <text>Port of the server.</text>
          </description>
        </leaf>
        <leaf name="vpn-name">
          <type name="leafref">
            <path value="../../../radius-server-groups/radius-server-group/radius-server-ipv4s/radius-server-ipv4[server-type=current()/../server-type][server-ip-address = current()/../server-ip-address][server-port=current()/../server-port]/vpn-name"/>
          </type>
          <description>
            <text>Vpn instance name of the server.</text>
          </description>
        </leaf>
        <leaf name="limit-value">
          <type name="uint32">
            <range value="1..65535"/>
          </type>
          <units name="packet"/>
          <mandatory value="true"/>
