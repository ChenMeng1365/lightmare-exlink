  <feature name="global-vpnid">
    <description>
      <text>The VPN ID as specified in [RFC2685].</text>
    </description>
  </feature>
  <feature name="traffic-statistics">
    <description>
      <text>L3vpn instances traffic statistics function.</text>
    </description>
  </feature>
  <feature name="deactive">
    <description>
      <text>VRF deactivation function.</text>
    </description>
  </feature>
  <feature name="ttl-mode">
    <description>
      <text>TTL mode.</text>
    </description>
  </feature>
  <feature name="ds-mode">
    <description>
      <text>DS mode.</text>
    </description>
  </feature>
  <feature name="rate-limit">
    <description>
      <text>The rate limit strategy of VRF.</text>
    </description>
  </feature>
  <feature name="import-multicast-route-ipv6">
    <description>
      <text>VRF route import for multicast in ipv6.</text>
    </description>
  </feature>
  <feature name="protection-ipv6">
    <description>
      <text>Protection for local prefixes in ipv6.</text>
    </description>
  </feature>
  <feature name="nm-private">
    <description>
      <text>Only for the use of network management.</text>
    </description>
  </feature>
  <feature name="entroy-label">
    <description>
      <text>The entroy label capability of VRF.</text>
    </description>
  </feature>
  <feature name="evpn-rt2-import-double-label">
    <description>
      <text>vBRAS-c does not need evpn-rt2-import-double-label.</text>
    </description>
  </feature>
  <feature name="reserved-vrf">
    <description>
      <text>Specific certain VRF instance for reserved virtual network,such as management net.</text>
    </description>
  </feature>
  <feature name="default-value-common">
    <description>
      <text>Default value is not applicable for v6000.</text>
    </description>
  </feature>
  <typedef name="global-vpnid-type">
    <type name="string">
      <length value="3..15"/>
      <pattern value="([0-9a-fA-F]{1,6}):([0-9a-fA-F]{1,8})">
        <description>
          <text>The purpose of a VPN-ID is to uniquely identify a VPN.The Global VPN Identifier format is: 3 octet VPN Authority, Organizationally Unique Identifier followed by 4 octet VPN index identifying VPN according to OUI.</text>
        </description>
      </pattern>
    </type>
    <description>
      <text>The type of global-vpnid.Format:(hex) &lt;3 bytes OUI:4 bytes VPN_Index&gt; (3-15 characters).</text>
    </description>
  </typedef>
  <container name="configuration">
    <container name="vrf">
      <description>
        <text>This portion represents the MPLS L3VPNs that are configured.The Virtual Routing and Forwarding (VRF) that is configured at a particular device represents an instance of some VPN.</text>
      </description>
      <container name="vrf-instances">
        <description>
          <text>VRF configuration.Besides name,description,global-vpnid and traffic-stat, other configurations of VRF are dependent on rd.</text>
        </description>
        <list name="vrf-inst">
          <description>
            <text>VRF configuration.Besides name,description,global-vpnid and traffic-stat, other configurations of VRF are dependent on rd.</text>
          </description>
          <max-elements value="16384"/>
          <key value="name"/>
          <leaf name="name">
            <description>
              <text>The human-readable name of this VPN.</text>
            </description>
            <type name="string">
              <length value="1..32"/>
            </type>
          </leaf>
          <leaf name="vpnid">
            <description>
              <text>The VPN identifier of VRF instance,it cannot be modified and removed unless VRF is deleted.</text>
            </description>
            <type name="uint32">
              <range value="1..16384"/>
            </type>
            <ncd:revise-date date="2017-07-03"/>
          </leaf>
          <leaf name="reserved-tag">
            <description>
              <text>Tagged for a certain reserved vrf.</text>
            </description>
            <type name="enumeration">
              <enum name="mng">
                <value value="1"/>
                <description>
                  <text>The VRF instance is used as mng.</text>
                </description>
              </enum>
            </type>
            <ncd:revise-date date="2020-03-02"/>
          </leaf>
          <leaf name="description">
            <description>
              <text>The human-readable description of this VRF.</text>
            </description>
            <type name="ncd:string-with-blank">
              <length value="1..104"/>
            </type>
          </leaf>
          <leaf name="global-vpnid">
            <description>
              <text>The VPN ID as specified in [RFC2685]. The global-vpnid format is (hex)&lt;3 bytes OUI:4 bytes VPN_Index&gt;.</text>
            </description>
            <type name="global-vpnid-type">
              <length value="3..15"/>
            </type>
          </leaf>
          <leaf name="traffic-stat">
            <description>
              <text>L3vpn statistic status of a VPN instance.The traffic-stat of VRF can be changed by the configuration of global-statistics.</text>
            </description>
            <type name="enumeration">
              <enum name="disable">
                <value value="0"/>
                <description>
                  <text>Disable statistic.</text>
                </description>
              </enum>
              <enum name="enable">
                <value value="1"/>
                <description>
                  <text>Enable statistic.</text>
                </description>
              </enum>
            </type>
            <default value="disable"/>
          </leaf>
          <container name="rd">
            <description>
              <text>The route distinguisher for a VRF.Once rd configured for a VRF,it cannot be modified and removed unless delete the vrf.</text>
            </description>
            <presence value="RD configuration."/>
            <leaf name="rd">
              <description>
                <text>The route distinguisher for this VRF.Once rd configured for a VRF,it cannot be modified and removed unless delete the VRF.</text>
              </description>
              <type name="union">
                <type name="ncd:asn-nn"/>
                <type name="ncd:asnd-nn"/>
                <type name="ncd:ipadd-nn"/>
              </type>
              <mandatory value="true"/>
            </leaf>
          </container>
          <container name="added-property">
            <description>
              <text>Other additional attributes for VRF instance.</text>
            </description>
            <leaf name="active-status">
              <description>
                <text>Status of VRF.</text>
              </description>
              <type name="enumeration">
                <enum name="deactive">
                  <value value="1"/>
                  <description>
                    <text>VRF is deactive.</text>
                  </description>
                </enum>
                <enum name="active">
                  <value value="2"/>
                  <description>
                    <text>VRF is active.</text>
                  </description>
                </enum>
              </type>
              <default value="active"/>
            </leaf>
            <leaf name="static-inlabel">
              <description>
                <text>Static in-label.The maximum range of static in-label is from 16 to 1048575,in fact,it is a dynamic range.Static in-label just takes effect when mpls-label-mode is per-vrf.</text>
              </description>
              <type name="uint32">
                <range value="16..1048575"/>
              </type>
            </leaf>
            <leaf name="ttl-mode">
              <description>
                <text>Ttl mode.The default values for different device types may be different, usually the value defined here.</text>
              </description>
              <type name="enumeration">
                <enum name="uniform">
                  <value value="1"/>
                  <description>
                    <text>Select uniform inherit mode.</text>
                  </description>
                </enum>
                <enum name="pipe">
                  <value value="2"/>
                  <description>
                    <text>Select pipe inherit mode.</text>
                  </description>
                </enum>
              </type>
              <default value="uniform"/>
            </leaf>
            <leaf name="nat-type">
              <description>
                <text>The type of NAT integration with MPLS VPN,if it is set as inside,the traffic in MPLS VPN will be processed with NAT.</text>
              </description>
              <type name="enumeration">
                <enum name="inside">
                  <value value="1"/>
                  <description>
                    <text>Nat-type is inside.</text>
                  </description>
                </enum>
                <enum name="none">
                  <value value="2"/>
                  <description>
                    <text>Nat-type is none.</text>
                  </description>
                </enum>
              </type>
              <default value="none"/>
            </leaf>
            <choice name="ds-mode">
              <description>
                <text>Ds-mode configuration.</text>
              </description>
              <default value="pipe"/>
              <case name="pipe">
                <description>
                  <text>Ds-mode is pipe.</text>
                </description>
                <container name="ds-mode-pipe">
                  <description>
                    <text>Configuration of VRF when the mode of ds-mode is pipe.</text>
                  </description>
                  <leaf name="ds-mode">
                    <description>
                      <text>Ds-mode of VRF.</text>
                    </description>
                    <type name="enumeration">
                      <enum name="pipe">
                        <value value="2"/>
                        <description>
                          <text>The mode of ds-mode is pipe.</text>
                        </description>
                      </enum>
                    </type>
                  </leaf>
                  <leaf name="service-class">
                    <description>
                      <text>Ds-mode service class.</text>
                    </description>
                    <type name="enumeration">
                      <enum name="be">
                        <value value="1"/>
                        <description>
                          <text>Best effort.</text>
                        </description>
                      </enum>
                      <enum name="af1">
                        <value value="2"/>
                        <description>
                          <text>Assured forwarding class 1.</text>
                        </description>
                      </enum>
                      <enum name="af2">
                        <value value="3"/>
                        <description>
                          <text>Assured forwarding class 2.</text>
                        </description>
                      </enum>
                      <enum name="af3">
                        <value value="4"/>
                        <description>
                          <text>Assured forwarding class 3.</text>
                        </description>
                      </enum>
                      <enum name="af4">
                        <value value="5"/>
                        <description>
                          <text>Assured forwarding class 4.</text>
                        </description>
                      </enum>
