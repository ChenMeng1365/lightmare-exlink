<?xml version="1.0" encoding="UTF-8"?>
<module name="huawei-unicast-forward"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:unicast-forward="urn:huawei:yang:huawei-unicast-forward"
        xmlns:ext="urn:huawei:yang:huawei-extension"
        xmlns:inet="urn:ietf:params:xml:ns:yang:ietf-inet-types"
        xmlns:ifm="urn:huawei:yang:huawei-ifm"
        xmlns:devm="urn:huawei:yang:huawei-devm"
        xmlns:lcs="urn:huawei:yang:huawei-license"
        xmlns:ni="urn:huawei:yang:huawei-network-instance"
        xmlns:devm-vnf="urn:huawei:yang:huawei-devm-vnf"
        xmlns:sf="urn:huawei:yang:huawei-said-frame">
  <yang-version value="1"/>
  <namespace uri="urn:huawei:yang:huawei-unicast-forward"/>
  <prefix value="unicast-forward"/>
  <import module="huawei-extension">
    <prefix value="ext"/>
  </import>
  <import module="ietf-inet-types">
    <prefix value="inet"/>
  </import>
  <import module="huawei-ifm">
    <prefix value="ifm"/>
  </import>
  <import module="huawei-devm">
    <prefix value="devm"/>
  </import>
  <import module="huawei-license">
    <prefix value="lcs"/>
  </import>
  <import module="huawei-network-instance">
    <prefix value="ni"/>
  </import>
  <import module="huawei-devm-vnf">
    <prefix value="devm-vnf"/>
  </import>
  <import module="huawei-said-frame">
    <prefix value="sf"/>
  </import>
  <organization>
    <text>Huawei Technologies Co., Ltd.</text>
  </organization>
  <contact>
    <text>Huawei Industrial Base
Bantian, Longgang
Shenzhen 518129
People's Republic of China
Website: http://www.huawei.com
Email: support@huawei.com</text>
  </contact>
  <description>
    <text>Unicast forward.</text>
  </description>
  <revision date="2020-06-11">
    <description>
      <text>Add ND-MAC linkage node.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <revision date="2019-12-21">
    <description>
      <text>Initial revision.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <typedef name="ttl-mode-type">
    <type name="enumeration">
      <enum name="pipe">
        <value value="0"/>
        <description>
          <text>Pipe mode.</text>
        </description>
      </enum>
      <enum name="uniform">
        <value value="1"/>
        <description>
          <text>Uniform mode.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>GRE ttl mode type.</text>
    </description>
  </typedef>
  <typedef name="l3-lcs-service-type">
    <type name="enumeration">
      <enum name="fib">
        <value value="0"/>
        <description>
          <text>Fib service.</text>
        </description>
      </enum>
      <enum name="l3vpn">
        <value value="1"/>
        <description>
          <text>L3vpn service.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>L3 license service type.</text>
    </description>
  </typedef>
  <typedef name="global-gre-mode">
    <type name="enumeration">
      <enum name="through">
        <value value="1"/>
        <description>
          <text>Through mode.</text>
        </description>
      </enum>
      <enum name="loopback">
        <value value="0"/>
        <description>
          <text>Loopback mode.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Type of global GRE mode.</text>
    </description>
  </typedef>
  <typedef name="lcs-act-status-type">
    <type name="enumeration">
      <enum name="unallocated">
        <value value="0"/>
        <description>
          <text>The license status is unallocated.</text>
        </description>
      </enum>
      <enum name="allocated">
        <value value="1"/>
        <description>
          <text>The license status is allocated.</text>
        </description>
      </enum>
      <enum name="activated">
        <value value="2"/>
        <description>
          <text>The license status is activated.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>License active status type.</text>
    </description>
  </typedef>
  <typedef name="mpls-6pe-ttl-mode-type">
    <type name="enumeration">
      <enum name="pipe">
        <value value="0"/>
        <description>
          <text>Pipe mode.</text>
        </description>
      </enum>
      <enum name="uniform">
        <value value="1"/>
        <description>
          <text>Uniform mode.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>MPLS 6pe ttl-mode type.</text>
    </description>
  </typedef>
  <ext:task-name value="forwarding"/>
  <container name="unicast-forward">
    <description>
      <text>Unicast forward.</text>
    </description>
    <container name="global-uc-attribute">
      <description>
        <text>Configure global atrributes.</text>
      </description>
      <leaf name="fib-miss">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable fib miss.</text>
        </description>
      </leaf>
      <leaf name="icmp-reply-fast">
        <type name="boolean"/>
        <default value="true"/>
        <description>
          <text>Enable/disable ICMP reply fast.</text>
        </description>
      </leaf>
      <leaf name="nexthop-spt">
        <type name="boolean"/>
        <default value="true"/>
        <description>
          <text>Enable/disable single-nexthop BGP fast-refresh.</text>
        </description>
      </leaf>
      <leaf name="ipv4-force-frag">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable IPv4 force fragment.</text>
        </description>
      </leaf>
      <leaf name="mtu-force-frag">
        <type name="boolean"/>
        <default value="true"/>
        <description>
          <text>Enable/disable IPv4 MTU force fragment.</text>
        </description>
      </leaf>
      <leaf name="trunk-nd-mode">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable Trunk ND global mode.</text>
        </description>
      </leaf>
      <leaf name="mpls-6pe-ttl-mode">
        <type name="mpls-6pe-ttl-mode-type"/>
        <default value="pipe"/>
        <description>
          <text>Enable/disable MPLS 6pe ttl-mode.</text>
        </description>
      </leaf>
    </container>
    <container name="high-re-modes">
      <description>
        <text>List of LPUF50A High-RE-Mode.</text>
      </description>
      <list name="high-re-mode">
        <key value="position"/>
        <description>
          <text>Configure LPUF50A High-RE-Mode.</text>
        </description>
        <leaf name="position">
          <type name="string">
            <length value="1..32"/>
          </type>
          <description>
            <text>Slot ID.</text>
          </description>
        </leaf>
      </list>
    </container>
    <container name="global-nexthop-spt">
      <config value="false"/>
      <description>
        <text>Statistics of single-next hop BGP route fast update.</text>
      </description>
      <leaf name="state">
        <type name="string">
          <length value="1..127"/>
        </type>
        <config value="false"/>
        <description>
          <text>The state statistics for single-next hop BGP route fast update.</text>
        </description>
      </leaf>
    </container>
    <container name="na-anti-attack-statistics">
      <config value="false"/>
      <description>
        <text>List of na anti-attack message statistic on a specified board.</text>
      </description>
      <list name="na-anti-attack-statistic">
        <key value="slot-id"/>
        <config value="false"/>
        <description>
          <text>Statistics of NA anti-attack message for a specified board.</text>
        </description>
        <leaf name="slot-id">
          <type name="string">
            <length value="1..32"/>
          </type>
          <config value="false"/>
          <description>
            <text>Slot ID of an interface board. The value range varies according to router hardware. NULL is for board on atn.</text>
          </description>
        </leaf>
        <leaf name="na-tocpu-count">
          <type name="uint64"/>
          <config value="false"/>
          <description>
            <text>Na message statistic of sending to cpu.</text>
          </description>
        </leaf>
        <leaf name="na-drop-count">
          <type name="uint64"/>
          <config value="false"/>
          <description>
            <text>Na message statistic of dropping.</text>
          </description>
        </leaf>
      </list>
    </container>
    <container name="vxlan-central-reassemble">
      <presence value="Enable centralized inter-board reassembly on VXLAN tunnels."/>
      <description>
        <text>Enable/disable centralized inter-board reassembly on VXLAN tunnels.</text>
      </description>
    </container>
    <container name="global-gre">
      <description>
        <text>Configure global unicast attributes.</text>
      </description>
      <leaf name="forward-mode">
        <type name="global-gre-mode"/>
        <default value="through"/>
        <description>
          <text>Global GRE forward mode.</text>
        </description>
      </leaf>
    </container>
  </container>
  <rpc name="reset-na-anti-attack-statistic">
    <description>
      <text>Reset na anti-attack message statistic information.</text>
    </description>
    <input>
      <leaf name="slot-id">
        <type name="string">
          <length value="1..32"/>
        </type>
        <mandatory value="true"/>
        <description>
          <text>Slot ID of an interface board. The value range varies according to router hardware. NULL is for board on ATN.</text>
        </description>
      </leaf>
    </input>
  </rpc>
  <augment target-node="/ifm:ifm/ifm:interfaces/ifm:interface">
    <description>
      <text>Unicast forward configuration to the ifm module.</text>
    </description>
    <container name="unicast-forward">
      <description>
        <text>Configure unicast forward.</text>
      </description>
      <container name="poison-reverse">
        <when condition="../../ifm:type = 'GigabitEthernet' or ../../ifm:type = 'XGigabitEthernet' or ../../ifm:type = 'Ip-Trunk' or ../../ifm:type = 'Eth-Trunk' or ../../ifm:type = 'Ethernet' or ../../ifm:type = 'Pos' or ../../ifm:type = '100GE' or ../../ifm:type = '400GE' or ../../ifm:type = '40GE' or ../../ifm:type = '50|100GE' or ../../ifm:type = '50GE' or ../../ifm:type = '25GE' or ../../ifm:type = '200GE' or ../../ifm:type = '10GE' or ../../ifm:type = 'FlexE' or ../../ifm:type = '4x10GE' or ../../ifm:type = '10x10GE' or ../../ifm:type = '3x40GE' or ../../ifm:type = '4x25GE'"/>
        <presence value="Enable poison reverse on an interface"/>
        <description>
          <text>Enable/disable poison reverse.</text>
        </description>
      </container>
      <container name="ipv6-redirect">
        <when condition="../../ifm:type = 'GigabitEthernet' or ../../ifm:type = 'XGigabitEthernet' or ../../ifm:type = 'Ip-Trunk' or ../../ifm:type = 'Eth-Trunk' or ../../ifm:type = 'Ethernet' or ../../ifm:type = 'Pos' or ../../ifm:type = '100GE' or ../../ifm:type = '400GE' or ../../ifm:type = '40GE' or ../../ifm:type = '50|100GE' or ../../ifm:type = '50GE' or ../../ifm:type = '25GE' or ../../ifm:type = '200GE' or ../../ifm:type = '10GE' or ../../ifm:type = 'FlexE' or ../../ifm:type = '4x10GE' or ../../ifm:type = '10x10GE' or ../../ifm:type = '3x40GE' or ../../ifm:type = '4x25GE'"/>
        <presence value="Enable ipv6 redirect on an interface"/>
        <description>
          <text>Enable/disable IPv6 redirect.</text>
        </description>
      </container>
      <container name="ipv6-na-anti-attack">
        <when condition="../../ifm:type = 'GigabitEthernet' or ../../ifm:type = 'XGigabitEthernet' or ../../ifm:type = 'Ip-Trunk' or ../../ifm:type = 'Eth-Trunk' or ../../ifm:type = 'Ethernet' or ../../ifm:type = '100GE' or ../../ifm:type = '400GE' or ../../ifm:type = '40GE' or ../../ifm:type = 'Vlanif' or ../../ifm:type = '50|100GE' or ../../ifm:type = '50GE' or ../../ifm:type = '25GE' or ../../ifm:type = '200GE' or ../../ifm:type = '10GE' or ../../ifm:type = 'FlexE' or ../../ifm:type = '4x10GE' or ../../ifm:type = '10x10GE' or ../../ifm:type = '3x40GE' or ../../ifm:type = '4x25GE'"/>
        <presence value="Enable ipv6 nd na anti-attack on an interface"/>
        <description>
          <text>Enable/disable IPv6 ND NA anti-attack.</text>
        </description>
      </container>
      <container name="ip-priority-select">
        <when condition="../../ifm:type = 'Vlanif'"/>
        <description>
          <text>Configure unicast VLANIF priority port.</text>
        </description>
        <leaf name="if-name">
          <type name="leafref">
            <path value="../../../ifm:name"/>
          </type>
          <must condition="../../../ifm:type = 'GigabitEthernet' or ../../../ifm:type = '50|100GE' or ../../../ifm:type = '50GE' or ../../../ifm:type = '400GE' or ../../../ifm:type = '25GE' or ../../../ifm:type = '100GE' or ../../../ifm:type = '200GE' or ../../../ifm:type = '10GE' or ../../../ifm:type = 'XGigabitEthernet' or ../../../ifm:type = 'Eth-Trunk' or ../../../ifm:type = 'FlexE' or ../../../ifm:type = 'FlexE-50G' or ../../../ifm:type = 'FlexE-200GE' or ../../../ifm:type = 'FlexE-100G' or ../../../ifm:type = 'FlexE-50|100G' or ../../../ifm:type = '4x10GE' or ../../../ifm:type = '10x10GE' or ../../../ifm:type = '3x40GE' or ../../../ifm:type = '4x25GE' or ../../../ifm:type = 'Ethernet' or ../../../ifm:type = '40GE'"/>
          <description>
            <text>Priority port.</text>
          </description>
        </leaf>
      </container>
      <container name="mac-change-notify-arp">
        <when condition="../../ifm:type = 'Vlanif'"/>
        <description>
          <text>Configure ARP-MAC association function.</text>
        </description>
        <leaf name="enable">
          <type name="boolean"/>
          <default value="true"/>
          <description>
            <text>Enable/disable ARP-MAC association.</text>
          </description>
        </leaf>
      </container>
      <container name="mac-change-notify-nd-disable">
        <when condition="../../ifm:type = 'Vlanif'"/>
        <presence value="Disable ND-MAC association function"/>
        <description>
          <text>Enable/disable ND-MAC association function.</text>
        </description>
      </container>
    </container>
  </augment>
  <augment target-node="/devm:devm/devm:lpu-boards/devm:lpu-board">
    <description>
      <text>Configure Unicast forward to the devm module.</text>
    </description>
    <container name="unicast-forward">
      <description>
        <text>Configure unicast forward.</text>
      </description>
      <container name="ipv4-uc">
        <description>
          <text>Configure slot unicast forwarding.</text>
        </description>
        <leaf name="force-frag">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable slot IPv4 force fragment.</text>
          </description>
        </leaf>
        <leaf name="icmp-reply-fast">
          <type name="boolean"/>
          <default value="true"/>
          <description>
            <text>Enable/disable slot ICMP replay fast.</text>
          </description>
        </leaf>
      </container>
      <container name="arp-fast-reply">
        <description>
          <text>Configure slot ARP reply.</text>
        </description>
        <leaf name="enable">
          <type name="boolean"/>
          <default value="true"/>
          <description>
            <text>Enable/disable slot ARP reply fast.</text>
          </description>
        </leaf>
      </container>
      <container name="gre-ttl-mode">
        <description>
          <text>Configure GRE ttl mode configuration.</text>
        </description>
        <leaf name="ttl-mode">
          <type name="ttl-mode-type"/>
          <default value="pipe"/>
          <description>
            <text>Set GRE tunnel-encapsulated mode.</text>
          </description>
        </leaf>
      </container>
      <container name="l3-lcs-capacitys">
        <description>
          <text>List of slot L3 forwarding capacity configurations.</text>
        </description>
        <list name="slot-l3-lcs-capacity">
          <key value="service-type"/>
          <description>
            <text>Configure slot L3 forwarding capacity.</text>
          </description>
          <leaf name="service-type">
            <type name="l3-lcs-service-type"/>
            <description>
              <text>Service type.</text>
            </description>
          </leaf>
          <leaf name="capacity-enable">
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable slot L3 forwarding capacity enhance.</text>
            </description>
            <ext:support-filter value="true"/>
          </leaf>
        </list>
      </container>
      <container name="nd-fast-reply">
        <description>
          <text>Configure ND fast-reply enable.</text>
        </description>
        <leaf name="enable">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable slot ND fast-reply.</text>
          </description>
        </leaf>
      </container>
    </container>
  </augment>
  <augment target-node="/devm:devm/devm:mpu-boards/devm:mpu-board">
    <description>
      <text>Unicast forward configuration to the devm module.</text>
    </description>
    <container name="unicast-forward">
      <description>
        <text>Configure unicast forward.</text>
      </description>
      <container name="ipv4-uc">
        <description>
          <text>Configure slot unicast Forwarding.</text>
        </description>
        <leaf name="force-frag">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable slot IPv4 force fragment.</text>
          </description>
        </leaf>
        <leaf name="icmp-reply-fast">
          <type name="boolean"/>
          <default value="true"/>
          <description>
            <text>Enable/disable slot ICMP replay fast.</text>
          </description>
        </leaf>
      </container>
      <container name="arp-fast-reply">
        <description>
          <text>Configure slot ARP reply.</text>
        </description>
        <leaf name="enable">
          <type name="boolean"/>
          <default value="true"/>
          <description>
            <text>Enable/disable slot ARP reply fast.</text>
          </description>
        </leaf>
      </container>
      <container name="gre-ttl-mode">
        <description>
          <text>Configure slot GRE ttl mode.</text>
        </description>
        <leaf name="ttl-mode">
          <type name="ttl-mode-type"/>
          <default value="pipe"/>
          <description>
            <text>Set GRE tunnel-encapsulated mode.</text>
          </description>
        </leaf>
      </container>
      <container name="nd-fast-reply">
        <description>
          <text>Configure ND fast-reply enable.</text>
        </description>
        <leaf name="enable">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable slot ND fast-reply.</text>
          </description>
        </leaf>
      </container>
    </container>
  </augment>
  <augment target-node="/lcs:license/lcs:license-items/lcs:license-item">
    <description>
      <text>Unicast forward configuration to the license module.</text>
    </description>
    <container name="port-l3vpns">
      <when condition="../lcs:name='LCR5S40NL3V0P' or ../lcs:name='LCR5S40XL3V0P' or ../lcs:name='LCR5S40GL3V0P' or ../lcs:name='LCX6S00NL3V0P' or ../lcs:name='LCX6S00XL3V0P' or ../lcs:name='LCX6S00GL3V0P' or ../lcs:name='LTNRS69NL3V0P' or ../lcs:name='LTNRS69XL3V0P' or ../lcs:name='LTNRS69GL3V0P' or ../lcs:name='LCR5S5KNL3V0P' or ../lcs:name='LCR5S5KXL3V0P' or ../lcs:name='LCR9S9KNL3V0P' or ../lcs:name='LCR9S9KXL3V0P' or ../lcs:name='LCX6S00NL3VAP' or ../lcs:name='LCX6S00XL3VAP' or ../lcs:name='LCR6S00NL3VAQ' or ../lcs:name='LCR9S9KNL3V0L' or ../lcs:name='LCR9S9KXL3V0L' or ../lcs:name='LCR5S40VL3V0P' or ../lcs:name='LCR5S40YL3V0P' or ../lcs:name='LCX6S00VL3V0P' or ../lcs:name='LCX6S00YL3V0P' or ../lcs:name='LTNRS00VL3V0P' or ../lcs:name='LTNRS00YL3V0P' or ../lcs:name='LCR9S9KNL3V0Q' or ../lcs:name='LCR8S00L3VPC2' or ../lcs:name='LCR8S00L3VPC4' or ../lcs:name='LCR8SF0L3VPC4' or ../lcs:name='LCR8SF0L3VPC1' or ../lcs:name='LTNRS00L3VPC2' or ../lcs:name='LTNRS00L3VPC4' or ../lcs:name='LCR5S00L3VPC2' or ../lcs:name='LCR5S00L3VPC4' or ../lcs:name='LCR8S0VL3VNC3' or ../lcs:name='LCR8S0XL3VNC3' or ../lcs:name='LCR8S0VL3VNC0' or ../lcs:name='LCR8S0XL3VNC0' or ../lcs:name='LCR5S5KKL3V0P'"/>
      <description>
        <text>List of L3VPN port license.</text>
      </description>
      <list name="port-l3vpn">
        <key value="position"/>
        <description>
          <text>Configure L3VPN port license.</text>
        </description>
        <leaf name="position">
          <type name="string">
            <length value="1..32"/>
          </type>
          <description>
            <text>Port position.</text>
          </description>
        </leaf>
      </list>
    </container>
    <container name="port-l3vpn-evpns">
      <when condition="../lcs:name='LCR5S03L3VP12' or ../lcs:name='LCR5S03L3VP13' or ../lcs:name='LCR5S03L3VP14' or ../lcs:name='LCR5S03L3VP15' or ../lcs:name='LCR6S62L3VP12' or ../lcs:name='LCR6S62L3VP13' or ../lcs:name='LCR6S62L3VP14' or ../lcs:name='LCR5S0M1L3V13' or ../lcs:name='LCX6S6M1L3V13'"/>
      <description>
        <text>List of port license entries for the L3VPN and EVPN functions.</text>
      </description>
      <list name="port-l3vpn-evpn">
        <key value="position"/>
        <description>
          <text>Configure a port license entry for the L3VPN and EVPN functions.</text>
        </description>
        <leaf name="position">
          <type name="string">
            <length value="1..32"/>
          </type>
          <description>
            <text>Port position.</text>
          </description>
        </leaf>
      </list>
    </container>
    <container name="port-service-enhances">
      <when condition="../lcs:name='LCR5S0M1SEN13' or ../lcs:name='LCX6S6M1SEN13' or ../lcs:name='LANNS0AVAND01' or ../lcs:name='LANNS0AVAND02' or ../lcs:name='LANNS0AVAND03' or ../lcs:name='LANNS0AVAND04' or ../lcs:name='LANHS00SADV50' or ../lcs:name='LANHS00SADV10'"/>
      <description>
        <text>List of port license entries in the service enhance scenario.</text>
      </description>
      <list name="port-service-enhance">
        <key value="position"/>
        <description>
          <text>Configure a port license entry in the service enhance scenario.</text>
        </description>
        <leaf name="position">
          <type name="string">
            <length value="1..32"/>
          </type>
          <description>
            <text>Port position.</text>
          </description>
        </leaf>
      </list>
    </container>
    <container name="port-aggregations">
      <when condition="../lcs:name='LCR5S03CRAG12' or ../lcs:name='LCR5S03CRAG13' or ../lcs:name='LCR5S03CRAG14' or ../lcs:name='LCR6S62CRAG12' or ../lcs:name='LCR6S62CRAG13' or ../lcs:name='LCR6S62CRAG14'"/>
