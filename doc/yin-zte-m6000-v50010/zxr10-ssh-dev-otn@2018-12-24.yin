<?xml version="1.0" encoding="UTF-8"?>
<module name="zxr10-ssh-dev-otn"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:ssh-dev-otn="http://www.zte.com.cn/zxr10/netconf/schema/rosng/zxr10-ssh-dev-otn"
        xmlns:ssh="http://www.zte.com.cn/zxr10/netconf/schema/rosng/ssh">
  <namespace uri="http://www.zte.com.cn/zxr10/netconf/schema/rosng/zxr10-ssh-dev-otn"/>
  <prefix value="ssh-dev-otn"/>
  <import module="zxr10-ssh">
    <prefix value="ssh"/>
  </import>
  <organization>
    <text>ZTE Corporation</text>
  </organization>
  <contact>
    <text>ZTE Corporation  
   Nanjing Institute of ZTE Corporation 
   No.68 Zijinghua Rd. Yuhuatai District, Nanjing, China 
   Tel: +86-25-52870000</text>
  </contact>
  <revision date="2018-12-24">
    <description>
      <text>1.Add deviation /ssh:configuration/ssh:ssh/ssh:ssh-server/ssh:authentication/ssh:user/ssh:key-name.
2.Add devation /ssh:state/ssh:ssh/ssh:public-key/ssh:key-name.
3.Add devation /ssh:operation/ssh:ssh/ssh:clear/ssh:public-key/ssh:key-name.</text>
    </description>
  </revision>
  <deviation target-node="/ssh:configuration/ssh:ssh/ssh:ssh-server/ssh:authentication/ssh:user/ssh:key-name">
    <description>
      <text>Replace this node range,from 1-31 to 1-64.</text>
    </description>
    <deviate value="replace">
      <type name="string">
        <length value="1..64"/>
      </type>
    </deviate>
  </deviation>
  <deviation target-node="/ssh:state/ssh:ssh/ssh:public-key/ssh:key-name">
    <description>
      <text>Replace this node range,from 1-31 to 1-64.</text>
    </description>
    <deviate value="replace">
      <type name="string">
        <length value="1..64"/>
      </type>
    </deviate>
  </deviation>
  <deviation target-node="/ssh:operation/ssh:ssh/ssh:clear/ssh:public-key/ssh:key-name">
    <description>
      <text>Replace this node range,from 1-31 to 1-64.</text>
    </description>
    <deviate value="replace">
      <type name="string">
        <length value="1..64"/>
      </type>
    </deviate>
  </deviation>
</module>
