          </container>
        </container>
        <container name="udp">
          <description>
            <text>UDP statistics data.</text>
          </description>
          <leaf name="receive-total">
            <description>
              <text>The number of receiving packets.</text>
            </description>
            <type name="uint32"/>
          </leaf>
          <leaf name="nosum">
            <description>
              <text>The number of receiving packets whose checksum is zero.</text>
            </description>
            <type name="uint32"/>
          </leaf>
          <leaf name="bad-checksum">
            <description>
              <text>The number of receiving packet whose checksum is wrong.</text>
            </description>
            <type name="uint32"/>
          </leaf>
          <leaf name="bad-lenth">
            <description>
              <text>The num of receiving packets whose length is wrong.</text>
            </description>
            <type name="uint32"/>
          </leaf>
          <leaf name="noport">
            <description>
              <text>The number of receiving packets whose port is wrong.</text>
            </description>
            <type name="uint32"/>
          </leaf>
          <leaf name="send-total">
            <description>
              <text>The num of sending packets.</text>
            </description>
            <type name="uint32"/>
          </leaf>
        </container>
        <container name="tcp">
          <description>
            <text>TCP statistics data.</text>
          </description>
          <leaf name="receive-total">
            <description>
              <text>The number of receiving packets.</text>
            </description>
            <type name="uint32"/>
          </leaf>
          <leaf name="bad-checksum">
            <description>
              <text>The number of receiving packet whose checksum is wrong.</text>
            </description>
            <type name="uint32"/>
          </leaf>
          <leaf name="send-total">
            <description>
              <text>The num of sending packets.</text>
            </description>
            <type name="uint32"/>
          </leaf>
        </container>
      </container>
      <container name="pmtu-info">
        <description>
          <text>IPv6 PMTU.</text>
        </description>
        <list name="pmtu">
          <key value="src-ip dst-ip vpn"/>
          <leaf name="src-ip">
            <description>
              <text>The source IP of the PMUT item.</text>
            </description>
            <type name="ncd:ipv6-address"/>
          </leaf>
          <leaf name="dst-ip">
            <description>
              <text>The destination IP of the PMUT item.</text>
            </description>
            <type name="ncd:ipv6-address"/>
          </leaf>
          <leaf name="vpn">
            <description>
              <text>The vpn of the PMUT item.</text>
            </description>
            <type name="string">
              <length value="0..36"/>
            </type>
          </leaf>
          <leaf name="mtu">
            <description>
              <text>The mtu of the PMUT item.</text>
            </description>
            <type name="uint16"/>
          </leaf>
          <leaf name="time">
            <description>
              <text>The left time of the PMUT item.</text>
            </description>
            <type name="uint16"/>
            <units name="minutes"/>
          </leaf>
        </list>
      </container>
    </container>
  </container>
  <container name="operation">
    <config value="false"/>
    <description>
      <text>Operation entry.</text>
    </description>
    <container name="ipv6">
      <description>
        <text>IPv6 operation commonds.</text>
      </description>
      <container name="traffic">
        <description>
          <text>IPv6 statistics.</text>
        </description>
        <leaf name="clear-traffic">
          <description>
            <text>Clear statistics data.</text>
          </description>
          <type name="empty"/>
        </leaf>
      </container>
    </container>
  </container>
</module>
