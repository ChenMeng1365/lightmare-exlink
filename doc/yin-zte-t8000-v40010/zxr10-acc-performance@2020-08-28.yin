<?xml version="1.0" encoding="UTF-8"?>
<module name="zxr10-acc-performance"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:zxr10-acc-pm="http://www.zte.com.cn/zxr10/netconf/schema/rosng/zxr10-acc-performance"
        xmlns:yang="urn:ietf:params:xml:ns:yang:ietf-yang-types"
        xmlns:acc-type="urn:ccsa:yang:acc-yang-types"
        xmlns:ncd="http://www.zte.com.cn/zxr10/netconf/schema/rosng/netconf-defs">
  <namespace uri="http://www.zte.com.cn/zxr10/netconf/schema/rosng/zxr10-acc-performance"/>
  <prefix value="zxr10-acc-pm"/>
  <yang-version value="1"/>
  <import module="ietf-yang-types">
    <prefix value="yang"/>
  </import>
  <import module="acc-yang-types">
    <prefix value="acc-type"/>
  </import>
  <import module="zxr10-netconf-defs">
    <prefix value="ncd"/>
  </import>
  <organization>
    <text>ZTE Corporation</text>
  </organization>
  <contact>
    <text>ZTE Corporation 
         Nanjing Institute of ZTE Corporation
         No.68 Zijinghua Rd. Yuhuatai District, Nanjing, China
         Tel: +86-25-52870000</text>
  </contact>
  <description>
    <text>This module defines an interface for managing performances.</text>
  </description>
  <revision date="2020-08-28">
    <description>
      <text>Initial revision.</text>
    </description>
  </revision>
  <container name="state">
    <description>
      <text>State info.</text>
    </description>
    <config value="false"/>
    <container name="history-performances">
      <description>
        <text>History performances.</text>
      </description>
      <list name="performance">
        <description>
          <text>Performance list.</text>
        </description>
        <key value="object-name pm-parameter-name granularity start-time end-time"/>
        <leaf name="object-name">
          <description>
            <text>Object name.</text>
          </description>
          <type name="ncd:string-all">
            <length value="1..64"/>
          </type>
        </leaf>
        <leaf name="pm-parameter-name">
          <description>
            <text>Performance parameter name.</text>
          </description>
          <type name="ncd:string-all">
            <length value="1..64"/>
          </type>
        </leaf>
        <leaf name="granularity">
          <description>
            <text>Granularity.</text>
          </description>
          <type name="acc-type:granularity"/>
        </leaf>
        <leaf name="start-time">
          <description>
            <text>Start time.</text>
          </description>
          <type name="yang:date-and-time"/>
        </leaf>
        <leaf name="end-time">
          <description>
            <text>End time.</text>
          </description>
          <type name="yang:date-and-time"/>
        </leaf>
        <leaf name="object-type">
          <description>
            <text>Object type.</text>
          </description>
          <type name="ncd:string-all">
            <length value="1..64"/>
          </type>
          <mandatory value="true"/>
        </leaf>
        <choice name="digital-or-analog">
          <description>
            <text>Digital or analog performance.</text>
          </description>
          <case name="digital">
            <description>
              <text>Digital performance.</text>
            </description>
            <leaf name="digital-pm-value">
              <description>
                <text>Digital performance value.</text>
              </description>
              <type name="acc-type:real"/>
            </leaf>
          </case>
          <case name="analog">
            <description>
              <text>Analog performance.</text>
            </description>
            <container name="analog-pm-value">
              <description>
                <text>Analog performance value.</text>
              </description>
              <leaf name="max-value">
                <description>
                  <text>Max value.</text>
                </description>
                <type name="acc-type:real"/>
              </leaf>
              <leaf name="min-value">
                <description>
                  <text>Min value.</text>
                </description>
                <type name="acc-type:real"/>
              </leaf>
              <leaf name="average-value">
                <description>
                  <text>Average value.</text>
                </description>
                <type name="acc-type:real"/>
              </leaf>
              <leaf name="current-value">
                <description>
                  <text>Current value.</text>
                </description>
                <type name="acc-type:real"/>
                <mandatory value="true"/>
              </leaf>
            </container>
          </case>
        </choice>
      </list>
    </container>
  </container>
</module>
