    <container name="session-statuses">
      <config value="false"/>
      <description>
        <text>List of Session statistic.</text>
      </description>
      <list name="session-status">
        <key value="session-num"/>
        <config value="false"/>
        <max-elements value="21"/>
        <description>
          <text>Session statistics.</text>
        </description>
        <leaf name="session-num">
          <type name="int32">
            <range value="0..1024"/>
          </type>
          <config value="false"/>
          <description>
            <text>Unique identification number for the session.</text>
          </description>
        </leaf>
        <leaf name="service-index">
          <type name="string">
            <length value="0..12"/>
          </type>
          <config value="false"/>
          <description>
            <text>The connection index of the session belonging to particular service type.</text>
          </description>
        </leaf>
        <leaf name="version">
          <type name="string">
            <length value="0..8"/>
          </type>
          <config value="false"/>
          <description>
            <text>The current SSH protocol version of the session.</text>
          </description>
        </leaf>
        <leaf name="conn-state">
          <type name="connstate"/>
          <config value="false"/>
          <description>
            <text>The state of the current connection.</text>
          </description>
        </leaf>
        <leaf name="user-name">
          <type name="string">
            <length value="0..253"/>
          </type>
          <config value="false"/>
          <description>
            <text>Name of the user for the current session.</text>
          </description>
        </leaf>
        <leaf name="ctos-cipher">
          <type name="string">
            <length value="0..32"/>
          </type>
          <config value="false"/>
          <description>
            <text>The encryption algorithm name used for transmission from client to server.</text>
          </description>
        </leaf>
        <leaf name="stoc-cipher">
          <type name="string">
            <length value="0..32"/>
          </type>
          <config value="false"/>
          <description>
            <text>The encryption algorithm name used for transmission from server to client.</text>
          </description>
        </leaf>
        <leaf name="ctos-compress">
          <type name="string">
            <length value="0..10"/>
          </type>
          <config value="false"/>
          <description>
            <text>The compression algorithm name used for transmission from client to server.</text>
          </description>
        </leaf>
        <leaf name="stoc-compress">
          <type name="string">
            <length value="0..10"/>
          </type>
          <config value="false"/>
          <description>
            <text>The compression algorithm name used for transmission from server to client.</text>
          </description>
        </leaf>
        <leaf name="ctos-hmac">
          <type name="string">
            <length value="0..32"/>
          </type>
          <config value="false"/>
          <description>
            <text>The MAC algorithm name chosen for the current client to server transmission for integrity.</text>
          </description>
        </leaf>
        <leaf name="stoc-hmac">
          <type name="string">
            <length value="0..32"/>
          </type>
          <config value="false"/>
          <description>
            <text>The MAC algorithm name chosen for the current server to client transmission for integrity.</text>
          </description>
        </leaf>
        <leaf name="key-exchange">
          <type name="string">
            <length value="0..64"/>
          </type>
          <config value="false"/>
          <description>
            <text>The key negotiation algorithm chosen for the current session.</text>
          </description>
        </leaf>
        <leaf name="service-type">
          <type name="string">
            <length value="0..32"/>
          </type>
          <config value="false"/>
          <description>
            <text>The current session service type.</text>
          </description>
        </leaf>
        <leaf name="session-retry">
          <type name="int32">
            <range value="0..1024"/>
          </type>
          <config value="false"/>
          <description>
            <text>The number of times the user have retired authenticating to the server before successful connection for the current session.</text>
          </description>
        </leaf>
        <leaf name="session-auth-type">
          <type name="string">
            <length value="0..32"/>
          </type>
          <config value="false"/>
          <description>
            <text>The authentication type used for the current session.</text>
          </description>
        </leaf>
        <leaf name="connection-port">
          <type name="int32">
            <range value="22..65535"/>
          </type>
          <config value="false"/>
          <description>
            <text>Port used for the current session.</text>
          </description>
        </leaf>
        <leaf name="session-key-type">
          <type name="sshs-pubkey-type"/>
          <config value="false"/>
          <description>
            <text>Public key type used for current session.</text>
          </description>
        </leaf>
      </list>
    </container>
    <container name="call-homes">
      <description>
        <text>List of Callhome configurations.</text>
      </description>
      <list name="call-home">
        <key value="call-home-name"/>
        <max-elements value="1"/>
        <description>
          <text>Configure Callhome instance.</text>
        </description>
        <leaf name="call-home-name">
          <type name="string">
            <length value="1..19"/>
          </type>
          <description>
            <text>Callhome name.</text>
          </description>
        </leaf>
        <leaf name="reconnect-interval">
          <type name="uint16">
            <range value="5..300"/>
          </type>
          <units name="s"/>
          <default value="5"/>
          <description>
            <text>Callhome reconnect interval.</text>
          </description>
        </leaf>
        <container name="end-points">
          <description>
            <text>List of the callhome connection instance configures.</text>
          </description>
          <list name="end-point">
            <key value="end-point-name"/>
            <max-elements value="1"/>
            <description>
              <text>Configure the callhome connection instance.</text>
            </description>
            <leaf name="end-point-name">
              <type name="string">
                <length value="1..64"/>
              </type>
              <description>
                <text>The callhome connection instance name.</text>
              </description>
            </leaf>
            <leaf name="address">
              <type name="inet:ip-address-no-zone"/>
              <must condition="../port"/>
              <description>
                <text>The peer IP address.</text>
              </description>
            </leaf>
            <leaf name="port">
              <when condition="../address"/>
              <type name="uint16">
                <range value="1..65535"/>
              </type>
              <description>
                <text>The peer port number.</text>
              </description>
            </leaf>
            <leaf name="is-public-net">
              <when condition="(../address) and (../port)"/>
              <type name="boolean"/>
              <description>
                <text>Enable/disable the public network connection.</text>
              </description>
            </leaf>
            <leaf name="vpn-name">
              <when condition="../is-public-net != 'true' and (../address) and (../port)"/>
              <type name="string">
                <length value="1..31"/>
              </type>
              <description>
                <text>The peer VPN name.</text>
              </description>
            </leaf>
            <leaf name="src-ipaddress">
              <when condition="(../address) and (../port)"/>
              <type name="inet:ip-address-no-zone"/>
              <description>
                <text>The source IP address of the callhome.</text>
              </description>
            </leaf>
            <leaf name="keepalive-interval">
              <type name="uint32">
                <range value="0..60"/>
              </type>
              <units name="min"/>
              <default value="0"/>
              <description>
                <text>The interval of the callhome keepalive packet.</text>
              </description>
            </leaf>
            <leaf name="keepalive-count">
              <type name="uint32">
                <range value="1..30"/>
              </type>
              <default value="3"/>
              <description>
                <text>The callhome keepalive maximum count.</text>
              </description>
            </leaf>
          </list>
        </container>
      </list>
    </container>
  </container>
  <rpc name="activate-ip">
    <description>
      <text>Activate ip.</text>
    </description>
    <input>
      <leaf name="ip-address">
        <type name="inet:ip-address-no-zone"/>
        <mandatory value="true"/>
        <description>
          <text>IP address.</text>
        </description>
      </leaf>
      <leaf name="vpn-name">
        <type name="leafref">
          <path value="/ni:network-instance/ni:instances/ni:instance/ni:name"/>
        </type>
        <description>
          <text>VPN name.</text>
        </description>
      </leaf>
    </input>
  </rpc>
</module>
