  revision 2018-11-05 {
    description '1.Add a new list vni-statistics-config on zxr10-vxlan/configur'
              + 'ation/vxlan/tunnel-statistics-configs;'+"\n"
              + '2.Add a new container vxlan-statistics on zxr10-vxlan/state/vx'
              + 'lan;'+"\n"
              + '3.Add a new list vxlan-statistics on zxr10-vxlan/operation/vxl'
              + 'an/clear-vxlan-tunnel-statistics.'
                ;
  }
  revision 2018-02-06 {
    description '1.Add a new list node of vxlan-tunnel-ipv6 on zxr10-vxlan/stat'
              + 'e/vxlan/vxlan-tunnels.'
                ;
  }
  revision 2017-11-15 {
    description '1.Delete SWITCH & C600 labels in feature mac-flooding;'+"\n"
              + '2.Delete SWITCH & C600 labels in feature vxlan-instance-mode;'+"\n"
              + '3.Delete SWITCH & C600 labels in feature static-tunnel;'+"\n"
              + '4.Delete SWITCH & C600 labels in feature redundancy-group-bind'
              + ';'+"\n"
              + '5.Delete SWITCH & C600 labels in feature vtep-bind;'+"\n"
              + '6.Delete SWITCH & C600 labels in feature move-dampening.'
                ;
  }
  revision 2017-09-20 {
    description '1.Add a new container node of move-dampening-policy on zxr10-v'
              + 'xlan/configuration/vxlan/vxlan-instances/vxlan-instance;'+"\n"
              + '2.The feature of move-dampening is defined.'
                ;
  }
  revision 2017-09-15 {
    description '1.The feature of vtep-bind is defined.'+"\n"
              + '2.The feature of vtep-instance-mode is validated to 15K and T8'
              + 'K. '+"\n"
              + '3.All features in the yang of revison 2017-9-15 are validated '
              + 'to 89 and 89-GM.'
                ;
  }
  revision 2017-05-04 {
    description '1.Add a new container node of mac-policy on zxr10-vxlan/config'
              + 'uration/vxlan/vxlan-instances/vxlan-instance;'+"\n"
              + '2.Add a leaf node of evpn-remote-mac-learning on zxr10-vxlan/c'
              + 'onfiguration/vxlan/vxlan-instances/vxlan-instance/mac-policy;'+"\n"
              + '3.Add a leaf node of local-mac-learning on zxr10-vxlan/configu'
              + 'ration/vxlan/vxlan-instances/vxlan-instance/mac-policy;'+"\n"
              + '4.Add a leaf node of evpn-remote-mac-receiving on zxr10-vxlan/c'
              + 'onfiguration/vxlan/vxlan-instances/vxlan-instance/mac-policy;'+"\n"
              + '5.Add a leaf node of evpn-local-mac-advertising on zxr10-vxlan'
              + '/configuration/vxlan/vxlan-instances/vxlan-instance/mac-policy'
              + ';'
                ;
  }
  revision 2017-03-30 {
    description '1.Add a new choice of ingress-adjust on zxr10-vxlan/configurat'
              + 'ion/vxlan/vxlan-instances/vxlan-instance/vxlan-map-tables/vxla'
              + 'n-map-l2interface;'+"\n"
              + '2.delete a leaf node of ingress-adjust-tag-as-payload on zxr10'
              + '-vxlan/configuration/vxlan/vxlan-instances/vxlan-instance/vxla'
              + 'n-map-tables/vxlan-map-l2interface;'
                ;
  }
  revision 2017-01-16 {
    description '1.Add a leaf node /vxlan:configuration/vxlan:vxlan/vxlan:vxlan'
              + '-instances/vxlan:vxlan-instance/vxlan:vxlan-route-targets/vxla'
              + 'n:vxlan-route-target/vxlan:split-horizon-group.'
                ;
  }
  revision 2017-01-10 {
    description '1.Add four new leaves on vxlan:state/vxlan:vxlan/vxlan:vxlan-t'
              + 'unnels/vxlan:vxlan-tunnel/vxlan:statistics/vxlan:tunnel-statis'
              + 'tics/'+"\n"
              + '2.Add four new leaves on vxlan:state/vxlan:vxlan/vxlan:vxlan-t'
              + 'unnels/vxlan:vxlan-tunnel/vxlan:statistics/vxlan:tunnel-vni-st'
              + 'atistics/vxlan:tunnel-vni-statistic/'
                ;
  }
  revision 2016-08-18 {
    description '1.Delete the container node of map-l2interace on zxr10-vxlan/c'
              + 'onfiguration/vxlan/vxlan-instances/vxlan-instance/vxlan-map-ta'
              + 'bles/vxlan-map-l2interface;'+"\n"
              + '2.Add a new list node of map-l2interace on zxr10-vxlan/configu'
              + 'ration/vxlan/vxlan-instances/vxlan-instance/vxlan-map-tables/v'
              + 'xlan-map-l2interface;'
                ;
  }
  revision 2016-07-05 {
    description "(1)Add feature node mac-flooding, VXLAN-instance-mode, vtep-in"
              + "stance-mode, static-tunnel, redundancy-group-bind.
\n(2)Add fe"
              + "ature attribute mac-flooding on container 'configuration/vxlan"
              + "/mac-floodings'.
\n(3)Add feature attribute VXLAN-instance-mod"
              + "e on container 'configuration/vxlan/vxlan-instances'.
\n(4)Add"
              + " feature attribute vtep-instance-mode on container 'configurat"
              + "ion/vxlan/vtep-instances' and 'configuration/vxlan/vtep-binds'"
              + ".
\n(5)Add feature attribute static-tunnel on container 'confi"
              + "guration/vxlan/static-tunnel-binds'.
\n(6)Add feature attribut"
              + "e redundancy-group-bind on container 'configuration/vxlan/redu"
              + "ndancy-group-binds'."
                ;
  }
  revision 2016-04-26 {
    description '1.Add range of /vxlan:state/vxlan:vxlan/vxlan:vxlan-tunnels/vx'
              + 'lan:vxlan-tunnel/vxlan:statistics/vxlan:tunnel-vni-statistics/'
              + 'vxlan:tunnel-vni-statistic/vxlan:vxlan-id.'+"\n"
              + '2.Change range of /vxlan:state/vxlan:vxlan/vxlan:vxlan-tunnels'
              + '/vxlan:vxlan-tunnel/vxlan:statistics/vxlan:tunnel-statistics/v'
              + 'xlan:in-bytes,/vxlan:state/vxlan:vxlan/vxlan:vxlan-tunnels/vxl'
              + 'an:vxlan-tunnel/vxlan:statistics/vxlan:tunnel-statistics/vxlan'
              + ':out-bytes,/vxlan:state/vxlan:vxlan/vxlan:vxlan-tunnels/vxlan:'
              + 'vxlan-tunnel/vxlan:statistics/vxlan:tunnel-statistics/vxlan:in'
              + '-packets and /vxlan:state/vxlan:vxlan/vxlan:vxlan-tunnels/vxla'
              + 'n:vxlan-tunnel/vxlan:statistics/vxlan:tunnel-statistics/vxlan:'
              + 'out-packets.'
                ;
  }
  revision 2016-04-20 {
    description '1.Add a new container on zxr10-vxlan/configuration/vxlan/stati'
              + 'c-tunnel-binds;'+"\n"
              + '2.Add a new container on zxr10-vxlan/configuration/vxlan/tunne'
              + 'l-statistics-configs;'+"\n"
              + '3.Add a new container on zxr10-vxlan/configuration/vxlan/redun'
              + 'dancy-group-binds;'+"\n"
              + '4.Add a new leaf on zxr10-vxlan/state/vxlan/vxlan-tunnels/vxla'
              + 'n-tunnel/static-tunnel-id;'+"\n"
              + '5.Add a new container on zxr10-vxlan/state/vxlan/vxlan-tunnels'
              + '/vxlan-tunnel/statistics;'+"\n"
              + '6.Add a new container on zxr10-vxlan/operation.'
                ;
  }
  revision 2016-04-05 {
    description '1.Delete the node /vxlan:state/vxlan:vxlan/vxlan:vxlan-tunnels'
              + '/vxlan:vxlan-tunnel/vxlan:tunnel-type.'+"\n"
              + '2.Change the name of node /vxlan:state/vxlan:vxlan/vxlan:vxlan'
              + '-tunnels/vxlan:vxlan-tunnel/vxlan:destination-ip to /vxlan:sta'
              + 'te/vxlan:vxlan/vxlan:vxlan-tunnels/vxlan:vxlan-tunnel/vxlan:re'
              + 'mote-ipv4.'+"\n"
              + '3.Change the name of node /vxlan:state/vxlan:vxlan/vxlan:vxlan'
              + '-tunnels/vxlan:vxlan-tunnel/vxlan:source-ip to /vxlan:state/vx'
              + 'lan:vxlan/vxlan:vxlan-tunnels/vxlan:vxlan-tunnel/vxlan:local-i'
              + 'pv4.'+"\n"
              + '4.Change the name of node /vxlan:state/vxlan:vxlan/vxlan:vxlan'
              + '-tunnels/vxlan:vxlan-tunnel/vxlan:tunnel-id to /vxlan:state/vx'
              + 'lan:vxlan/vxlan:vxlan-tunnels/vxlan:vxlan-tunnel/vxlan:evpn-tu'
              + 'nnel-id.'
                ;
  }
  revision 2016-03-16 {
    description '1.Change the name of node /vxlan:configuration/vxlan:vxlan/vxl'
              + 'an:vxlan-instances/vxlan:vxlan-instance/vxlan:vxlan-route-targ'
              + 'ets/vxlan:vxlan-route-target/vxlan:rd to rt-value,change the d'
              + 'escription of this node.'
                ;
  }
  revision 2016-01-08 {
    description '1.Add container /vxlan:configuration/vxlan:vxlan/vxlan:mac-flo'
              + 'odings.'+"\n"
              + '2.Add leaf /vxlan:configuration/vxlan:vxlan/vxlan:vxlan-instan'
              + 'ces/vxlan:vxlan-instance/vxlan:description.'+"\n"
              + '3.Add leaf /vxlan:configuration/vxlan:vxlan/vxlan:vxlan-instan'
              + 'ces/vxlan:vxlan-instance/vxlan:unknow-unicast-drop.'+"\n"
              + '4.Add leaf /vxlan:configuration/vxlan:vxlan/vxlan:vxlan-instan'
              + 'ces/vxlan:vxlan-instance/vxlan:filter-vrrp.'+"\n"
              + '5.Add container /vxlan:configuration/vxlan:vxlan/vxlan:vxlan-i'
              + 'nstances/vxlan:vxlan-instance/vxlan:vxlan-rd.'+"\n"
              + '6.Add container /vxlan:configuration/vxlan:vxlan/vxlan:vxlan-i'
              + 'nstances/vxlan:vxlan-instance/vxlan:vxlan-route-targets.'+"\n"
              + '7.Add container /vxlan:state.'+"\n"
              + '8.Obsolete container /vxlan:configuration/vxlan:vxlan/vxlan:vx'
              + 'lan-instances/vxlan:vxlan-instance/vxlan:vxlan-multicast-ip.'+"\n"
              + '9.Obsolete container /vxlan:configuration/vxlan:vxlan/vxlan:vx'
              + 'lan-instances/vxlan:vxlan-instance/vxlan:vxlan-multicast-ipv6.'
                ;
  }
  revision 2015-11-25 {
    description 'Add container /vxlan:configuration/vxlan:vxlan/vxlan:vxlan-ins'
              + 'tances/vxlan:vxlan-instance/vxlan:vxlan-multicast-ipv6.'
                ;
  }
  revision 2014-09-16 {
    description 'Initial version.'
                ;
  }
  feature mac-flooding {
    description 'This node is used to distinguish whether the MAC flooding func'
              + 'tion can be enabled.'
                ;
  }
  feature vxlan-instance-mode {
    description 'This node is used to distinguish whether the VXLAN instance mo'
              + 'de can be enabled.'
                ;
  }
  feature vtep-instance-mode {
    description 'This node is used to distinguish whether the vtep instance mod'
              + 'e can be enabled.'
                ;
  }
  feature static-tunnel {
    description 'This node is used to distinguish whether static tunnel functio'
              + 'n can be enabled.'
                ;
  }
  feature redundancy-group-bind {
    description 'This node is used to distinguish whether redundancy group bind'
              + ' table function can be enabled.'
                ;
  }
  feature vtep-bind {
    description 'This node is used to distinguish whether the vtep bind can be '
              + 'enabled.'
                ;
  }
  feature move-dampening {
    description 'This node is used to distinguish whether the move dampening po'
              + 'licy can be enabled.'
                ;
  }
  container configuration {
    description 'Configuration model info.'
                ;
    container vxlan {
      description 'Configure VXLAN parameters.'
                  ;
      container global-config {
        description 'Global enable configuration of VXLAN.'
                    ;
        presence global-config ;
        leaf global-enable {
          description 'VXLAN global enble.'
                      ;
          type empty ;
          mandatory true ;
        }
      }
      container mac-floodings {
        // if-feature mac-flooding ;
        description 'Configuration of MAC flooding.'
                    ;
        list mac-flooding {
          description 'Configuration of MAC flooding.'
                      ;
          key 'mac-address vlan-id'
              ;
          leaf mac-address {
            description 'Specific MAC address of MAC flooding.'
                        ;
            type ncd:mac-address ;
          }
          leaf vlan-id {
            description 'VLAN ID of MAC flooding.'
                        ;
            type uint16 {
              range 1..4094 {
                description 'The value of VLAN,it must be between 1 to 4094.'
                            ;
              }
            }
          }
        }
        ncd:revise-date 2016-01-08 ;
      }
      container vxlan-instances {
        // if-feature vxlan-instance-mode ;
        description 'Configuration of VXLAN instances.'
                    ;
        list vxlan-instance {
          description 'Configuration of VXLAN instance.'
                      ;
          key vxlan-id ;
          leaf vxlan-id {
            description 'VXLAN ID of VXLAN instance.'
                        ;
            type uint32 {
              range 1..16777215 {
                description 'The value of VXLAN,it must be between 1 to 1677721'
                          + '5.'
                            ;
              }
            }
          }
          leaf description {
            description 'The description information of VXLAN instance.'
                        ;
            type string {
              length 0..64 {
                description 'VXLAN instance description information.'
                            ;
              }
            }
            default ''
                    ;
            ncd:revise-date 2016-01-08 ;
          }
          leaf unknow-unicast-drop {
            description 'Unknow unicast drop configuration of VXLAN instance.'
                        ;
            type enumeration {
              enum enable {
                value 1 ;
                description 'Unknown unicast drop enable.'
                            ;
              }
              enum disable {
                value 2 ;
                description 'Unknown unicast drop disable.'
                            ;
              }
            }
            default enable ;
            ncd:revise-date 2016-01-08 ;
          }
          leaf filter-vrrp {
            description 'VRRP packets filter configuration of VXLAN instance.'
                        ;
            type enumeration {
              enum enable {
                value 1 ;
                description 'VRRP packets filter.'
                            ;
              }
              enum disable {
                value 2 ;
                description 'VRRP packets not filter.'
                            ;
              }
            }
            default enable ;
            ncd:revise-date 2016-01-08 ;
          }
          container vxlan-multicast-ip {
            status obsolete {
              ncd:revise-date 2016-01-08 ;
            }
            description 'VXLAN multicast IP address.'
                        ;
            presence vxlan-multicast-ip ;
            leaf multicast-ip {
              status obsolete {
                ncd:revise-date 2016-01-08 ;
              }
              description 'VXLAN multicast IP address.'
                          ;
              type ncd:ipv4-address ;
              mandatory true ;
            }
          }
          container vxlan-multicast-ipv6 {
            status obsolete {
              ncd:revise-date 2016-01-08 ;
            }
            description 'VXLAN multicast IPv6 address.'
                        ;
            presence vxlan-multicast-ipv6 ;
            leaf multicast-ipv6 {
              status obsolete {
                ncd:revise-date 2016-01-08 ;
              }
              description 'VXLAN multicast IPv6 address.'
                          ;
              type ncd:ipv6-address ;
              mandatory true ;
            }
            ncd:revise-date 2015-11-25 ;
          }
          container vxlan-map-tables {
            description 'VXLAN map tables.'
                        ;
            presence vxlan-map-tables ;
            choice vxlan-map-types {
              description 'VXLAN map type choice.'
                          ;
              case vxlan-map-vlan {
                description 'VXLAN map VLAN.'
                            ;
                leaf map-type-vlan {
                  description 'VXLAN map VLAN.'
                              ;
                  type enumeration {
                    enum map-type-vlan1to1 {
                      value 1 ;
                      description 'Map type VLAN1:1.'
                                  ;
                    }
                    enum map-type-vlan1ton {
                      value 2 ;
                      description 'Map type VLAN1:n.'
                                  ;
                    }
                  }
                  default map-type-vlan1to1 ;
                }
                list map-vlan {
                  description 'VXLAN map VLAN.'
                              ;
                  key vlan-id ;
                  leaf vlan-id {
                    description 'VLAN ID.'
                                ;
                    type uint16 {
                      range 1..4094 {
                        description 'The value of VLAN,it must be between 1 to '
                                  + '4094.'
                                    ;
                      }
                    }
                  }
                }
              }
              case vxlan-map-mac {
                description 'VXLAN map MAC.'
                            ;
                leaf map-type-mac {
                  description 'VXLAN map MAC.'
                              ;
                  type empty ;
                }
                container map-mac {
                  description 'VXLAN map MAC.'
                              ;
                  presence map-mac ;
                  leaf mac {
                    description 'MAC address.'
                                ;
                    type ncd:mac-address ;
                    mandatory true ;
                  }
                }
              }
              case vxlan-map-l2interface {
                description 'VXLAN map L2 interface.'
                            ;
                leaf map-type-l2interface {
                  description 'VXLAN map L2 interface.'
                              ;
                  type empty ;
                }
                list map-l2interface {
                  description 'VXLAN map L2 interface.'
                              ;
                  key interface-name ;
                  leaf interface-name {
                    description 'L2 interface name.'
                                ;
                    type ncd:ifname ;
                  }
                  leaf ingress-adjust-tag-as-payload {
                    status obsolete {
                      ncd:revise-date 2017-03-30 ;
                    }
                    description 'The ingress tag as payload flag of the etherne'
                              + 't parameter.'
                                ;
                    type enumeration {
                      enum default {
                        value 0 ;
                        description 'Sub-interface means pTag. The others means'
                                  + ' uTag.'
                                    ;
                      }
                      enum all {
                        value 1 ;
                        description 'The ingress tag as payload flag is from ou'
                                  + 'ter most.'
                                    ;
                      }
                      enum from-sublayer {
                        value 2 ;
                        description 'The ingress tag as payload flag is from su'
                                  + 'b layer.'
                                    ;
                      }
                    }
                    default default ;
                  }
                  choice ingress-adjust {
                    description 'Configure ingress-adjustment operation'
                                ;
                    case tag-as-payload {
                      description 'The ingress tag as payload flag of the ether'
                                + 'net parameter.'
                                  ;
                      leaf tag-as-payload {
                        description 'The ingress tag as payload flag of the eth'
                                  + 'ernet parameter.'
                                    ;
                        type enumeration {
                          enum default {
                            value 0 ;
                            description 'Sub-interface means pTag. The others means uTag.'
                                        ;
                          }
                          enum all {
                            value 1 ;
                            description 'The ingress tag as payload flag is from outer most.'
                                        ;
                          }
                          enum from-sublayer {
                            value 2 ;
                            description 'The ingress tag as payload flag is from sub layer.'
                                        ;
                          }
                        }
                        default default ;
                      }
                    }
                    ncd:revise-date 2017-03-30 ;
                  }
                  ncd:revise-date 2016-08-18 ;
                }
              }
              case vxlan-map-l3interface {
                description 'VXLAN map L3 interface.'
                            ;
                leaf map-type-l3interface {
                  description 'VXLAN map L3 interface.'
                              ;
                  type empty ;
                }
                list map-l3interface {
                  description 'VXLAN map L3 interface.'
                              ;
                  key interface-name ;
                  leaf interface-name {
                    description 'L3 interface name.'
                                ;
                    type ncd:ifname {
                      length 1..31 {
                        description 'The length of interface name must be betwe'
                                  + 'en 1 to 31.'
                                    ;
                      }
                    }
                  }
                }
              }
            }
          }
          container vxlan-rd {
            description 'Information of route distinguisher.'
                        ;
            presence vxlan-rd ;
            leaf rd {
              description 'The route distinguisher parameter.'
                          ;
              type union {
                type ncd:asn-nn ;
                type ncd:ipadd-nn ;
              }
              mandatory true ;
            }
            ncd:revise-date 2016-01-08 ;
          }
          container vxlan-route-targets {
            description 'The configuration of route target.'
                        ;
            presence vxlan-route-targets ;
            list vxlan-route-target {
              description 'The configuration of route target.'
                          ;
              key 'effect-type rt-value'
                  ;
              leaf effect-type {
                description 'The effect type of route target.'
                            ;
                type enumeration {
                  enum export {
                    value 2 ;
                    description 'The route target effect type is export.'
                                ;
                  }
                  enum import {
                    value 3 ;
                    description 'The route target effect type is import.'
                                ;
                  }
                }
              }
              leaf rt-value {
                description 'The value of RT attribution.'
                            ;
                type union {
                  type ncd:asn-nn ;
                  type ncd:ipadd-nn ;
                }
                ncd:revise-date 2016-03-16 ;
              }
              leaf split-horizon-group {
                description 'Split-horizon group,the range is A-Z.'
                            ;
                type string ;
                ncd:revise-date 2017-01-16 ;
              }
            }
            ncd:revise-date 2016-01-08 ;
          }
          container mac-policy {
            description 'Information of MAC learning policy.'
                        ;
            presence mac-policy ;
            leaf evpn-remote-mac-learning {
              description 'Remote MAC of EVPN tunnel learning enable.'
                          ;
              type enumeration {
                enum enable {
                  value 1 ;
                  description Enable ;
                }
                enum disable {
                  value 2 ;
                  description Disable ;
                }
              }
              default enable ;
            }
            leaf evpn-remote-mac-receiving {
              description 'Remote EVPN tunnel advertise MAC receiving enable.'
                          ;
              type enumeration {
                enum enable {
                  value 1 ;
                  description Enable ;
                }
                enum disable {
                  value 2 ;
                  description Disable ;
                }
              }
              default enable ;
            }
            leaf evpn-local-mac-advertising {
              description 'EVPN local MAC advertising enable.'
                          ;
              type enumeration {
                enum enable {
                  value 1 ;
                }
                enum disable {
                  value 2 ;
                }
              }
              default enable ;
            }
            leaf local-mac-learning {
              description 'Local MAC learning enable of VXLAN instance.'
                          ;
              type enumeration {
                enum enable {
                  value 1 ;
                  description Enable ;
                }
                enum disable {
                  value 2 ;
                  description Disable ;
                }
              }
              default enable ;
            }
            ncd:revise-date 2017-05-04 ;
          }
          container move-dampening-policy {
            description 'The detector of MAC/IP routing drift.'
                        ;
            leaf move-dampening {
              description 'The detector switch of MAC/IP routing drift.'
                          ;
              type enumeration {
                enum enable {
                  value 1 ;
