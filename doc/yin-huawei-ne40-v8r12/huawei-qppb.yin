  <typedef name="direction-type">
    <type name="enumeration">
      <enum name="inbound">
        <value value="0"/>
        <description>
          <text>Inbound.</text>
        </description>
      </enum>
      <enum name="outbound">
        <value value="1"/>
        <description>
          <text>Outbound.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Direction type.</text>
    </description>
  </typedef>
  <typedef name="qppb-direction-type">
    <type name="enumeration">
      <enum name="inbound">
        <value value="2"/>
        <description>
          <text>Inbound.</text>
        </description>
      </enum>
      <enum name="outbound">
        <value value="3"/>
        <description>
          <text>Outbound.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>QPPB direction type.</text>
    </description>
  </typedef>
  <typedef name="behavior-state">
    <type name="enumeration">
      <enum name="active">
        <value value="0"/>
        <description>
          <text>All active.</text>
        </description>
      </enum>
      <enum name="active-p">
        <value value="1"/>
        <description>
          <text>Part active.</text>
        </description>
      </enum>
      <enum name="inactive">
        <value value="2"/>
        <description>
          <text>Inactive.</text>
        </description>
