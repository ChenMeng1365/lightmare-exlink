    "definitions": {
        "bras-said_schema": {
            "description": "Bras said.",
            "properties": {
                "said-exclude-fail-codes": {
                    "description": "Configure the excluding of login failures with a specified cause code from being detected or diagnosed by SAID nodes. By default, a login failure is not excluded from the detection or diagnosis of SAID nodes.",
                    "properties": {
                        "visible-code": {
                            "type": "array",
                            "items": {
                                "type": "string",
                                "description": "Configure the visible on-line failure cause type."
                            }
                        },
                        "invisible-code": {
                            "type": "array",
                            "items": {
                                "type": "string",
                                "description": "Configure the sxternal invisible on-line failure cause type."
                            }
                        }
                    }
                },
                "said-parameter": {
                    "description": "Configure the user Access.",
                    "properties": {
                        "increase-number-of-online-fail": {
                            "type": "string",
                            "description": "Sets the sepical increase number of online-fail."
                        },
                        "recover-interval": {
                            "type": "string",
                            "description": "Sets the special days of interval between recover operations."
                        },
                        "online-fail-number": {
                            "type": "string",
                            "description": "The sepical increase number of online-fail."
                        },
                        "online-user-reduce-number": {
                            "type": "string",
                            "description": "The sepical reduce of online-user number."
                        },
                        "online-success-rate": {
                            "type": "string",
                            "description": "The sepical rate of online success, unit is percent."
                        },
                        "detect-by-user-number-reduce-ratio": {
                            "type": "string",
                            "description": "Accords to the number of users decreased the percentage of detection when the specified value. Only when the number of users is detected to reach the specified drop rate, it is considered that the SAID condition is satisfied and the diagnosis and recovery are continued."
                        },
                        "detect-by-flow-speed-reduce-ratio": {
                            "type": "string",
                            "description": "Accords to the percentage drop in the flow rate when the specified value, Only when the traffic rate reduction is detected reaches the specified drop rate, it is considered that the SAID condition is satisfied and the diagnosis and recovery are continued."
                        },
                        "said-enable": {
                            "type": "boolean",
                            "description": "Enable/disable SAID function."
                        }
                    }
                }
            }
        },
