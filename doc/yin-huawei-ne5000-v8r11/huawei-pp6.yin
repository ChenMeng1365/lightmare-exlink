  <augment target-node="/ifm:ifm/ifm:interfaces/ifm:interface/ip:ipv6">
    <when condition="../ifm:type!='ServiceIf' and ../ifm:type!='Virtual-if'"/>
    <description>
      <text>All configurations of the PP6 under the interface.</text>
    </description>
    <container name="pp6-collection">
      <description>
        <text>Configure PP6 interface attribute configurations.</text>
      </description>
      <leaf name="permit-incomplete-fragment">
        <type name="boolean"/>
        <description>
          <text>Enable/disable flag of allowing first fragment with incomplete IPv6 header chain in IPv6 enabled interfaces.</text>
        </description>
      </leaf>
    </container>
    <container name="extension-header-filter">
      <when condition="../../ifm:type!='NULL' and ../../ifm:type!='ServiceIf' and ../../ifm:type!='Virtual-if' and ../../ifm:type!='Cpos' and ../../ifm:type!='Cpos-Trunk' and ../../ifm:type!='Atm-Bundle' and ../../ifm:type!='Serial' and ../../ifm:type!='Trunk-Serial' and ../../ifm:type!='Global-Mp-Group' and ../../ifm:type!='Gmpls-Uni'"/>
      <description>
        <text>Configure interface IPv6 extension header filter configurations.</text>
      </description>
      <container name="hop-by-hop-switch">
        <description>
          <text>Configure the IPv6 packet with hop-by-hop options extension header switch.</text>
        </description>
        <leaf name="all-options">
          <type name="filter-rule-type"/>
          <description>
            <text>All options of IPv6 extension header switch.</text>
          </description>
        </leaf>
        <container name="options">
          <description>
            <text>List of some options of IPv6 extension header switch.</text>
          </description>
          <list name="option">
            <key value="option-code"/>
            <description>
              <text>Configure some options of IPv6 extension header switch.</text>
            </description>
            <leaf name="option-code">
              <type name="uint8">
                <range value="0..255"/>
              </type>
              <description>
                <text>Option code of IPv6 extension header.</text>
              </description>
            </leaf>
            <leaf name="filter-rule">
              <type name="filter-rule-type"/>
              <mandatory value="true"/>
              <description>
                <text>Filter policy of IPv6 packet.</text>
              </description>
              <ext:support-filter value="true"/>
            </leaf>
          </list>
        </container>
      </container>
      <container name="routing-switch">
        <description>
          <text>Configure the IPv6 packet with routing options extension header switches.</text>
        </description>
        <leaf name="all-options">
          <type name="filter-rule-type"/>
          <description>
            <text>All options of IPv6 extension header switch.</text>
          </description>
        </leaf>
        <container name="options">
          <description>
            <text>List of some options of IPv6 extension header switch.</text>
          </description>
          <list name="option">
            <key value="option-code"/>
            <description>
              <text>Configure some options of IPv6 extension header switch.</text>
            </description>
            <leaf name="option-code">
              <type name="uint8">
                <range value="0..255"/>
              </type>
              <description>
                <text>Option code of IPv6 extension header.</text>
              </description>
            </leaf>
            <leaf name="filter-rule">
              <type name="filter-rule-type"/>
              <mandatory value="true"/>
              <description>
                <text>Filter policy of IPv6 packet.</text>
              </description>
              <ext:support-filter value="true"/>
            </leaf>
          </list>
        </container>
      </container>
      <container name="destination-switch">
        <description>
          <text>Configure the IPv6 packet with destination options extension header switches.</text>
        </description>
        <leaf name="all-options">
          <type name="filter-rule-type"/>
          <description>
            <text>All options of IPv6 extension header switch.</text>
          </description>
        </leaf>
        <container name="options">
          <description>
            <text>List of some options of IPv6 extension header switch.</text>
          </description>
          <list name="option">
            <key value="option-code"/>
            <description>
              <text>Configure some options of IPv6 extension header switch.</text>
            </description>
            <leaf name="option-code">
              <type name="uint8">
                <range value="0..255"/>
              </type>
              <description>
                <text>Option code of IPv6 extension header.</text>
              </description>
            </leaf>
            <leaf name="filter-rule">
              <type name="filter-rule-type"/>
              <mandatory value="true"/>
              <description>
                <text>Filter policy of IPv6 packet.</text>
              </description>
              <ext:support-filter value="true"/>
            </leaf>
          </list>
        </container>
      </container>
      <container name="no-option-exh-switchs">
        <description>
          <text>List of the IPv6 packet with no options extension header switches.</text>
        </description>
        <list name="no-option-exh-switch">
          <key value="exh-type"/>
          <description>
            <text>Configure the IPv6 packet with hop-by-hop options extension header switch.</text>
          </description>
          <leaf name="exh-type">
            <type name="extension-header-type"/>
            <description>
              <text>IPv6 extension header type which have no option.</text>
            </description>
          </leaf>
          <leaf name="filter-rule">
            <type name="filter-rule-type"/>
            <mandatory value="true"/>
            <description>
              <text>Filter policy of IPv6 packet.</text>
            </description>
            <ext:support-filter value="true"/>
          </leaf>
        </list>
      </container>
    </container>
  </augment>
  <augment target-node="/ni:network-instance/ni:instances/ni:instance">
    <description>
      <text>All configurations of the PP6 under the network instance.</text>
    </description>
    <container name="icmp6-source-address">
      <presence value="Configure a loopback interface address as a source IP address for an ICMP6 error packet."/>
      <description>
        <text>Configure a loopback interface address as a source IP address for an ICMP6 error packet.</text>
      </description>
      <leaf name="if-name">
        <type name="leafref">
          <path value="/ifm:ifm/ifm:interfaces/ifm:interface/ifm:name"/>
        </type>
        <must condition="/ifm:ifm/ifm:interfaces/ifm:interface[ifm:name=current()]/ifm:type='LoopBack'"/>
        <must condition="/ifm:ifm/ifm:interfaces/ifm:interface/ip:ipv6"/>
        <must condition="/ifm:ifm/ifm:interfaces/ifm:interface[ifm:name=current()]/ifm:vrf-name = ../../ni:name"/>
        <mandatory value="true"/>
        <description>
          <text>Interface name.</text>
        </description>
      </leaf>
      <leaf name="pkt-type">
        <type name="icmp-pkt-type"/>
        <mandatory value="true"/>
        <description>
          <text>Packet type of ICMPv6.</text>
        </description>
      </leaf>
    </container>
  </augment>
  <ext:task-name value="ip-stack"/>
</module>
