  <grouping name="routing-table-rib-only-type">
    <description>
      <text>Disable Route Delivery to the IP Routing Table configuration.</text>
    </description>
    <container name="routing-table-rib-only">
      <description>
        <text>Configure disable route delivery to the IP routing table.</text>
      </description>
      <leaf name="enable">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable prevent BGP routes from being added to the IP routing table.</text>
        </description>
      </leaf>
      <choice name="policy-type">
        <description>
          <text>Set route policy or filter.</text>
        </description>
        <case name="rtp-ref">
          <description>
            <text>Route policy.</text>
          </description>
          <leaf name="policy-name">
            <when condition="../enable = 'true'"/>
            <type name="leafref">
              <path value="/rtp:routing-policy/rtp:policy-definitions/rtp:policy-definition/rtp:name"/>
            </type>
            <description>
              <text>When routes are imported from other routing protocols, the route-policy filter can be used to filter the routes and change route attributes.</text>
            </description>
          </leaf>
        </case>
        <case name="xpl-ref">
          <description>
            <text>Route filter.</text>
          </description>
          <leaf name="filter-name">
            <when condition="../enable = 'true'"/>
            <type name="leafref">
              <path value="/xpl:xpl/xpl:route-filters/xpl:route-filter/xpl:name"/>
            </type>
            <description>
              <text>Specify a route-filter name.</text>
            </description>
          </leaf>
          <leaf name="filter-parameter">
            <when condition="../filter-name"/>
            <type name="xpl:filter-parameter-type"/>
            <description>
              <text>Specify a route-filter parameter.</text>
            </description>
          </leaf>
        </case>
      </choice>
    </container>
  </grouping>
  <grouping name="group-graceful-restart-type">
    <description>
      <text>Configure graceful restart.</text>
    </description>
    <leaf name="enable">
      <type name="boolean"/>
      <default value="false"/>
      <description>
        <text>Enable/disable graceful restart capability.</text>
      </description>
    </leaf>
    <leaf name="time-wait-for-rib">
      <type name="uint32">
        <range value="3..3000"/>
      </type>
      <units name="s"/>
      <default value="600"/>
      <description>
        <text>Period of waiting for the End-of-RIB flag. The value is an integer. When a BGP session is established or re-established, the router enabled with GR should receive the End-of-RIB flag in this period. If the router does not receive the End-of-RIB flag in this period, ensure that the router can exit from the GR process.</text>
      </description>
    </leaf>
    <leaf name="restart-time">
      <type name="uint16">
        <range value="3..3600"/>
      </type>
      <units name="s"/>
      <default value="150"/>
      <description>
        <text>Specifies the maximum time during which a peer waits for a BGP session reestablishment.</text>
      </description>
    </leaf>
  </grouping>
  <grouping name="peer-graceful-restart-type">
    <description>
      <text>Configure graceful restart.</text>
    </description>
    <leaf name="enable">
      <type name="peer-comm-config-type"/>
      <default value="default"/>
      <description>
        <text>Enable/disable graceful restart capability.</text>
      </description>
    </leaf>
    <leaf name="time-wait-for-rib">
      <type name="uint32">
        <range value="3..3000"/>
      </type>
      <units name="s"/>
      <description>
        <text>Period of waiting for the End-of-RIB flag. The value is an integer. When a BGP session is established or re-established, the router enabled with GR should receive the End-of-RIB flag in this period. If the router does not receive the End-of-RIB flag in this period, ensure that the router can exit from the GR process.</text>
      </description>
    </leaf>
    <leaf name="restart-time">
      <type name="uint16">
        <range value="3..3600"/>
      </type>
      <units name="s"/>
      <description>
        <text>Specifies the maximum time during which a peer waits for a BGP session reestablishment.</text>
      </description>
    </leaf>
    <leaf name="effect-restart-time">
      <type name="uint16">
        <range value="3..3600"/>
      </type>
      <units name="s"/>
      <config value="false"/>
      <description>
        <text>The maximum time during which a peer waits for a BGP session reestablishment.</text>
      </description>
