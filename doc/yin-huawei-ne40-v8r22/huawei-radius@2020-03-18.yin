            <default value="false"/>
            <description>
              <text>Enable/disable CoA query ACK packets to carry the Session-Timeout attribute to indicate the remaining duration of a user service.</text>
            </description>
          </leaf>
          <leaf name="acct-session-time">
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable CoA query ACK packets to carry the Acct-Session-Time attribute to indicate the online duration of a user.</text>
            </description>
          </leaf>
          <leaf name="filter-id">
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable CoA query ACK packets to carry the Filter-Id attribute to indicate the user group information.</text>
            </description>
          </leaf>
        </container>
        <container name="access-request-attribute">
          <description>
            <text>Configure a attribute to be carried in a RADIUS access request packet.</text>
          </description>
          <leaf name="hw-gateway-address">
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable access authentication request packets of Layer 3 DHCPv4 access users to carry user gateway addresses when the packets are sent to a RADIUS server.</text>
            </description>
          </leaf>
          <leaf name="hw-avpair-link-address">
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable access authentication request packets of Layer 3 DHCPv6 access users to carry the HW-AVPAIR attribute to indicate the user link-address information when the packets are sent to a RADIUS server.</text>
            </description>
          </leaf>
        </container>
        <container name="coa-logon-ack-attribute">
          <description>
            <text>Configure the attributes to be carried in the ACK packets that are replied to the RADIUS server when CoA-based pre-authentication to authentication domain switchover is performed successfully.</text>
          </description>
          <leaf name="reply-message">
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable ACK packets to carry the Reply-Message attribute.</text>
            </description>
          </leaf>
        </container>
        <container name="attribute-action-class">
          <presence value="Create attribute-action-class."/>
          <description>
            <text>Enable/disable the attribute action.</text>
          </description>
          <leaf name="class-partial-match">
            <type name="string">
              <length value="1..31"/>
              <pattern value="[^A-Z]{1,31}"/>
            </type>
            <mandatory value="true"/>
            <description>
              <text>Specifies the character string for approximate string matching of the class attribute to be encapsulated into a PPP packet's MTOM field.</text>
            </description>
            <ext:case-sensitivity value="lower-only"/>
          </leaf>
          <leaf name="class-encode-format-name">
            <type name="string">
              <length value="1..31"/>
            </type>
            <description>
              <text>Specifies the encapsulation format and name of the class attribute to be encapsulated into a PPP packet's MTOM field.</text>
            </description>
          </leaf>
          <leaf name="class-delimiter">
            <when condition="../class-encode-format-name"/>
            <type name="string">
              <length value="1"/>
            </type>
            <mandatory value="true"/>
            <description>
              <text>Specifies a delimiter of the class attribute to be encapsulated into a PPP packet's MTOM field.</text>
            </description>
          </leaf>
          <leaf name="class-exclude-local">
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable that the local configuration is excluded of the class attribute to be encapsulated into a PPP packet's MTOM field.</text>
            </description>
          </leaf>
        </container>
        <container name="nas-port-id">
          <description>
            <text>Configure the nas-port-id attribute of the RADIUS server can carry the IP address of the DSLAM recorded by the physical port description. After the configuration, the nas-port-id attribute of the RADIUS server is encapsulated according to the configuration format.</text>
          </description>
          <container name="include-attributes">
            <description>
              <text>List of the user-defined format of the nas-port-id attributes.</text>
            </description>
            <list name="include-attribute">
              <key value="attribute-type"/>
              <max-elements value="3"/>
              <ordered-by value="user"/>
              <description>
                <text>Configure the user-defined format of the nas-port-id attribute.</text>
              </description>
              <leaf name="attribute-type">
                <type name="enumeration">
                  <enum name="interface-description">
                    <value value="1"/>
                    <description>
                      <text>Interface description.</text>
                    </description>
                  </enum>
                  <enum name="pe-vlan">
                    <value value="2"/>
                    <description>
                      <text>User's peVLAN.</text>
                    </description>
                  </enum>
                  <enum name="ce-vlan">
                    <value value="3"/>
                    <description>
                      <text>User's ceVLAN.</text>
                    </description>
                  </enum>
                </type>
                <description>
                  <text>Configures the user-defined format of the attribute delimiter one.</text>
                </description>
              </leaf>
              <leaf name="delimiter">
                <type name="string">
                  <length value="1"/>
                  <pattern value="[b\*\-\\#$@&amp;:]"/>
                </type>
                <description>
                  <text>Configures the user-defined format of the attribute delimiter.</text>
                </description>
              </leaf>
            </list>
          </container>
          <leaf name="head-delimiter">
            <type name="string">
              <length value="1"/>
              <pattern value="[b\*\-\\#$@&amp;:]"/>
            </type>
            <must condition="count(../include-attributes/include-attribute) &gt;= 1"/>
            <description>
              <text>Configures the user-defined format of the nas-port-id include delimiter.</text>
            </description>
          </leaf>
        </container>
        <container name="lns-nas-ip-address">
          <description>
            <text>Configure a LNS or LTS to encapsulate the remote IP address into the NAS-Ip-Address attribute of a RADIUS packet.</text>
          </description>
          <leaf name="remote-ip-enable">
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable a LNS or LTS to encapsulate the remote IP address into the NAS-Ip-Address attribute of a RADIUS packet.</text>
            </description>
          </leaf>
        </container>
      </list>
    </container>
    <container name="radius-test-groups">
      <description>
        <text>List of a set of RADIUS test group.</text>
      </description>
      <list name="radius-test-group">
        <key value="name"/>
        <max-elements value="4"/>
        <description>
          <text>Configure RADIUS test group.</text>
        </description>
        <leaf name="name">
          <type name="string">
            <length value="1..32"/>
            <pattern value="[a-z0-9._-]{1,32}"/>
          </type>
          <must condition="../name != '-' and ../name != '--'"/>
          <description>
            <text>RADIUS test group name.</text>
          </description>
          <ext:case-sensitivity value="lower-only"/>
        </leaf>
        <container name="carry-attributes">
          <description>
            <text>List of carry RADIUS attribute.</text>
          </description>
          <list name="carry-attribute">
            <key value="attribute-name packet-type"/>
            <description>
              <text>Configure carry RADIUS attribute.</text>
            </description>
            <leaf name="attribute-name">
              <type name="radius-attribute-table"/>
              <must condition="../attribute-name != 'in-kb-after-t-switch' and ../attribute-name != 'in-kb-before-t-switch' and ../attribute-name !='input-average-rate' and ../attribute-name !='input-peak-rate' and ../attribute-name != 'out-kb-after-t-switch' and ../attribute-name != 'out-kb-before-t-switch' and ../attribute-name != 'output-average-rate' and ../attribute-name != 'output-peak-rate' and ../attribute-name != 'remanent-volume' and ../attribute-name != 'tariff-switch-interval'"/>
              <description>
                <text>Specifies an attribute name.</text>
              </description>
            </leaf>
            <leaf name="packet-type">
              <type name="enumeration">
                <enum name="authentication">
                  <value value="0"/>
                  <description>
                    <text>Authentication packet.</text>
                  </description>
                </enum>
                <enum name="accounting">
                  <value value="1"/>
                  <description>
                    <text>Accounting packet.</text>
                  </description>
                </enum>
              </type>
              <description>
                <text>Packet type.</text>
              </description>
            </leaf>
            <leaf name="carry-type">
              <type name="enumeration">
                <enum name="include">
                  <value value="0"/>
                  <description>
                    <text>Include attribute.</text>
                  </description>
                </enum>
                <enum name="exclude">
                  <value value="1"/>
                  <description>
                    <text>Exclude attribute.</text>
                  </description>
                </enum>
              </type>
              <mandatory value="true"/>
              <description>
                <text>Include attribute or exclude attribute.</text>
              </description>
            </leaf>
            <leaf name="attribute-value">
              <when condition="../carry-type = 'include' and ../attribute-name != 'user-password' and ../attribute-name != 'chap-password'"/>
              <type name="string">
                <length value="1..506"/>
              </type>
              <description>
                <text>Attribute value.</text>
              </description>
            </leaf>
            <leaf name="attribute-password-value">
              <when condition="../carry-type = 'include' and (../attribute-name = 'user-password' or ../attribute-name = 'chap-password')"/>
              <type name="pub-type:password-extend">
                <length value="1..268"/>
              </type>
              <description>
                <text>Attribute value for attribute of password type.</text>
              </description>
            </leaf>
          </list>
        </container>
      </list>
    </container>
    <container name="radius-server-authorizations">
      <description>
        <text>List of the authorization servers in a group.</text>
      </description>
      <list name="radius-server-authorization">
        <key value="ipv4-address vpn-name"/>
        <max-elements value="1024"/>
        <description>
          <text>Configure a particular group.</text>
        </description>
        <leaf name="ipv4-address">
          <type name="inet:ipv4-address-no-zone"/>
          <description>
            <text>Specifies the IP address of a RADIUS authorization server.</text>
          </description>
        </leaf>
        <leaf name="vpn-name">
          <type name="leafref">
            <path value="/ni:network-instance/ni:instances/ni:instance/ni:name"/>
          </type>
          <must condition="/ni:network-instance/ni:instances/ni:instance[ni:name=current()]/l3vpn:afs/l3vpn:af[l3vpn:type='ipv4-unicast']"/>
          <description>
            <text>Indicates the VPN instance to which the RADIUS authorization server belongs.</text>
          </description>
        </leaf>
        <leaf name="shared-key">
          <type name="pub-type:password-extend">
            <length value="1..268"/>
          </type>
          <mandatory value="true"/>
          <description>
            <text>Specifies the shared key for the RADIUS server. The simple password length ranges from 1 to 128, and the cipher password length ranges from 1 to 268.</text>
          </description>
        </leaf>
        <leaf name="destination-ip-address">
          <type name="inet:ipv4-address-no-zone"/>
          <description>
            <text>Specifies the IP address of dynamic authorization packets.</text>
          </description>
        </leaf>
        <leaf name="destination-port">
          <type name="uint32">
            <range value="1645..1646|1812..1813|3799"/>
          </type>
          <description>
            <text>Specifies the port number of dynamic authorization packets.</text>
          </description>
        </leaf>
        <leaf name="server-group-name">
          <type name="leafref">
            <path value="../../../radius-server-groups/radius-server-group/name"/>
          </type>
          <description>
            <text>Specifies the name of the RADIUS server group corresponding to the RADIUS authorization server.</text>
          </description>
        </leaf>
        <leaf name="ack-reserved-interval">
          <type name="uint32">
            <range value="0..300"/>
          </type>
          <units name="s"/>
          <default value="0"/>
          <description>
            <text>Specifies the period when the authorization acknowledgment packets are saved.</text>
          </description>
        </leaf>
      </list>
    </container>
    <container name="packet-max-length">
      <description>
        <text>Configure the default maximum length of RADIUS packets that can be sent by the router so that the router can communicate with devices of other vendors.</text>
      </description>
      <leaf name="enable">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable changing the default maximum length of RADIUS packets that can be sent by the router so that the router can communicate with devices of other vendors.</text>
        </description>
      </leaf>
    </container>
    <container name="account-cache-retransmit">
      <description>
        <text>Configure an interval at which cached RADIUS accounting packets are retransmitted and the number of users for each packet retransmission.</text>
      </description>
      <leaf name="user-number">
        <type name="uint32">
          <range value="1..30"/>
        </type>
        <default value="1"/>
        <description>
          <text>Specifies the number of users for each packet retransmission.</text>
        </description>
      </leaf>
      <leaf name="time-out">
        <type name="uint32">
          <range value="1..10"/>
        </type>
        <units name="s"/>
        <default value="10"/>
        <description>
          <text>Specifies an interval at which cached RADIUS accounting packets are retransmitted.</text>
        </description>
      </leaf>
    </container>
    <container name="account-cache-warning-threshold">
      <must condition="./lower-limit &lt; ./upper-limit"/>
      <presence value="Create account-cache-warning-threshold."/>
      <description>
        <text>Enable/disable the accounting packet cache alarm function and Configure an alarm threshold and a clear alarm threshold. If the accounting packet cache usage reaches the configured alarm threshold, an alarm is reported.</text>
      </description>
      <leaf name="upper-limit">
        <type name="uint32">
          <range value="1..100"/>
        </type>
        <units name="%"/>
        <mandatory value="true"/>
        <description>
          <text>Specifies an alarm threshold.</text>
        </description>
      </leaf>
      <leaf name="lower-limit">
        <type name="uint32">
          <range value="0..99"/>
        </type>
        <units name="%"/>
        <mandatory value="true"/>
        <description>
          <text>Specifies a clear alarm threshold.</text>
        </description>
      </leaf>
    </container>
    <container name="account-cache-memory-threshold">
      <description>
        <text>Configure a memory usage threshold for the master main control board.</text>
      </description>
      <leaf name="threshold">
        <type name="uint32">
          <range value="50..85"/>
        </type>
        <units name="%"/>
        <default value="75"/>
        <description>
          <text>Specifies a memory usage threshold for the master main control board.</text>
        </description>
      </leaf>
    </container>
    <container name="account-cache-max-packet">
      <description>
        <text>Configure the maximum number of accounting packets that can be cached.</text>
      </description>
      <leaf name="number">
        <type name="uint32">
          <range value="8192..256000"/>
        </type>
        <units name="packet"/>
        <default value="8192"/>
        <description>
          <text>Specifies the maximum number of accounting packets that can be cached.</text>
        </description>
      </leaf>
    </container>
    <container name="access-line-id-length-extend">
      <description>
        <text>Configure the maximum length of the Agent-Circuit-Id or Agent-Remote-Id attribute carried in RADIUS packets when a device trusts the Option 82 field in user packets as 198 bytes.</text>
      </description>
      <leaf name="enable">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable the maximum length of the Agent-Circuit-Id or Agent-Remote-Id attribute carried in RADIUS packets when a device trusts the Option 82 field in user packets as 198 bytes.</text>
        </description>
      </leaf>
    </container>
    <container name="account-stop-packet-optimize">
      <description>
        <text>Configure the BRAS not to count this packet retransmission into the number of RADIUS accounting stop packet retransmissions if a BRAS fails to transmit a RADIUS accounting stop packet due to a seed application failure and retransmits the packet.</text>
      </description>
      <leaf name="enable">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable the BRAS not to count this packet retransmission into the number of RADIUS accounting stop packet retransmissions if a BRAS fails to transmit a RADIUS accounting stop packet due to a seed application failure and retransmits the packet.</text>
        </description>
      </leaf>
    </container>
    <container name="send-packet-optimize">
      <description>
        <text>Configure a device to adjust the sending of RADIUS real-time accounting packets based on the system status.</text>
      </description>
      <leaf name="enable">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable a device to adjust the sending of RADIUS real-time accounting packets based on the system status.</text>
        </description>
      </leaf>
    </container>
    <container name="extended-source-port-number">
      <must condition="(start-port and ((port-number + start-port) &lt; 55537)) or not(start-port)"/>
      <presence value="Create extended-source-port-number"/>
      <description>
        <text>Enable/disable the extended source interfaces that the device uses to send and receive RADIUS packets, this operation will take effect about 30 seconds later.</text>
      </description>
      <leaf name="port-number">
        <type name="uint32">
          <range value="1..32"/>
        </type>
        <mandatory value="true"/>
        <description>
          <text>Specifies the number of extended source ports.</text>
        </description>
      </leaf>
      <leaf name="start-port">
        <type name="uint32">
          <range value="50000..55535"/>
        </type>
        <description>
          <text>Specifies the start port number.</text>
        </description>
      </leaf>
    </container>
    <container name="authentication-packet-statistics">
      <config value="false"/>
      <description>
        <text>List of authentication packet statistics of the RADIUS server.</text>
      </description>
      <list name="authentication-packet-statistic">
        <key value="ipv4-address vpn-name server-port"/>
        <config value="false"/>
        <description>
          <text>Statistics of the RADIUS server authentication packets.</text>
        </description>
        <leaf name="ipv4-address">
          <type name="inet:ipv4-address-no-zone"/>
          <config value="false"/>
          <description>
            <text>IPv4 address of the RADIUS server.</text>
          </description>
        </leaf>
        <leaf name="vpn-name">
          <type name="string">
            <length value="1..31"/>
          </type>
          <config value="false"/>
          <description>
            <text>Name of the VPN instance to which the RADIUS server belongs.</text>
          </description>
        </leaf>
        <leaf name="server-port">
          <type name="uint16"/>
          <config value="false"/>
          <description>
            <text>Port number of RADIUS server.</text>
          </description>
        </leaf>
        <uses name="radius-authen-packet-statistics-info"/>
      </list>
