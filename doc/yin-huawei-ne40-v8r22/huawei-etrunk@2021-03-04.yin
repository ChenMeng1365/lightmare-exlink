  <ext:task-name value="etrunk"/>
  <typedef name="etrunk-state">
    <type name="enumeration">
      <enum name="init">
        <value value="1"/>
        <description>
          <text>The E-Trunk state is init.</text>
        </description>
      </enum>
      <enum name="backup">
        <value value="2"/>
        <description>
          <text>The E-Trunk state is backup.</text>
        </description>
      </enum>
      <enum name="master">
        <value value="3"/>
        <description>
          <text>The E-Trunk state is master.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>E-Trunk status.</text>
    </description>
  </typedef>
  <typedef name="ethtrunk-state">
    <type name="enumeration">
      <enum name="negotiation">
        <value value="3"/>
        <description>
          <text>Negotiation state.</text>
        </description>
      </enum>
      <enum name="master">
        <value value="2"/>
        <description>
          <text>Master state.</text>
        </description>
      </enum>
      <enum name="backup">
        <value value="1"/>
        <description>
          <text>Backup state.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Eth-Trunk status.</text>
    </description>
  </typedef>
  <typedef name="etrunk-causation">
    <type name="enumeration">
      <enum name="priority-change">
        <value value="1"/>
        <description>
          <text>The E-Trunk state is changed because the priority is changed.</text>
        </description>
      </enum>
      <enum name="timeout">
        <value value="2"/>
        <description>
          <text>The E-Trunk state is changed because the local device fails to receive any hello packet before the timeout period expires.</text>
        </description>
      </enum>
      <enum name="bfd-down">
        <value value="3"/>
        <description>
          <text>The E-Trunk state is changed because the BFD is down on the local device.</text>
        </description>
      </enum>
      <enum name="peer-timeout">
        <value value="4"/>
        <description>
          <text>The E-Trunk state is changed because the peer device fails to receive any hello packet before the timeout period expires.</text>
        </description>
      </enum>
      <enum name="peer-bfd-down">
        <value value="5"/>
        <description>
          <text>The E-Trunk state is changed because the BFD is down on the peer device.</text>
        </description>
      </enum>
      <enum name="init-all-member-down">
        <value value="6"/>
        <description>
          <text>The E-Trunk state is changed because all members are down and the E-Trunk enters the initialized state.</text>
        </description>
      </enum>
      <enum name="init">
        <value value="7"/>
        <description>
          <text>The E-Trunk state is changed because the E-Trunk is initialized.</text>
        </description>
      </enum>
      <enum name="peer-node-down">
        <value value="8"/>
        <description>
          <text>The E-Trunk state is changed because the peer device is down.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>E-Trunk status reason.</text>
    </description>
  </typedef>
  <typedef name="ethtrunk-causation">
    <type name="enumeration">
      <enum name="force-backup">
        <value value="1"/>
        <description>
          <text>Forcible backup.</text>
        </description>
      </enum>
      <enum name="force-master">
        <value value="2"/>
        <description>
          <text>Forcible master.</text>
        </description>
      </enum>
      <enum name="etrunk-init">
        <value value="3"/>
        <description>
          <text>The E-Trunk state is initialized.</text>
        </description>
      </enum>
      <enum name="etrunk-backup">
        <value value="4"/>
        <description>
          <text>The E-Trunk state is backup.</text>
        </description>
      </enum>
      <enum name="etrunk-master">
        <value value="5"/>
        <description>
          <text>The E-Trunk state is master.</text>
        </description>
      </enum>
      <enum name="peer-member-down">
        <value value="6"/>
        <description>
          <text>The physical state of the peer member is down.</text>
        </description>
      </enum>
      <enum name="peer-member-up">
        <value value="7"/>
        <description>
          <text>The physical state of the peer member is up.</text>
        </description>
      </enum>
      <enum name="active-active">
        <value value="8"/>
        <description>
          <text>The physical state of members at both ends is up.</text>
        </description>
      </enum>
      <enum name="degraded-active">
        <value value="9"/>
        <description>
          <text>The physical state of E-Trunk members (Eth-Trunk interfaces) is degraded active.</text>
        </description>
      </enum>
      <enum name="peerlink-down">
        <value value="10"/>
        <description>
          <text>The peer link is down.</text>
        </description>
      </enum>
      <enum name="link-down">
        <value value="11"/>
        <description>
          <text>The local link is down.</text>
        </description>
      </enum>
      <enum name="all-active">
        <value value="12"/>
        <description>
          <text>The redundancy mode of EVPN instance that E-Trunk member is bound to is all-active.</text>
        </description>
      </enum>
      <enum name="peer-bandwidth-large">
        <value value="13"/>
        <description>
          <text>The available bandwidth of peer member is large.</text>
        </description>
      </enum>
      <enum name="local-bandwidth-large">
        <value value="14"/>
        <description>
          <text>The available bandwidth of local member is large.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Eth-Trunk status reason.</text>
    </description>
  </typedef>
