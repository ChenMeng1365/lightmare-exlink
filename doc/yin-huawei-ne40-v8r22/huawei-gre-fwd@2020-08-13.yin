<?xml version="1.0" encoding="UTF-8"?>
<module name="huawei-gre-fwd"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:gre-fwd="urn:huawei:yang:huawei-gre-fwd"
        xmlns:gre="urn:huawei:yang:huawei-gre"
        xmlns:tnlm="urn:huawei:yang:huawei-tunnel-management"
        xmlns:ext="urn:huawei:yang:huawei-extension"
        xmlns:ifm="urn:huawei:yang:huawei-ifm">
  <yang-version value="1"/>
  <namespace uri="urn:huawei:yang:huawei-gre-fwd"/>
  <prefix value="gre-fwd"/>
  <import module="huawei-gre">
    <prefix value="gre"/>
  </import>
  <import module="huawei-tunnel-management">
    <prefix value="tnlm"/>
  </import>
  <import module="huawei-extension">
    <prefix value="ext"/>
  </import>
  <import module="huawei-ifm">
    <prefix value="ifm"/>
  </import>
  <organization>
    <text>Huawei Technologies Co., Ltd.</text>
  </organization>
  <contact>
    <text>Huawei Industrial Base
Bantian, Longgang
Shenzhen 518129
People's Republic of China
Website: http://www.huawei.com
Email: support@huawei.com</text>
  </contact>
  <description>
    <text>Unicast forward GRE configuration.</text>
  </description>
  <revision date="2020-08-13">
    <description>
      <text>Initial revision.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <ext:task-name value="forwarding"/>
  <augment target-node="/ifm:ifm/ifm:interfaces/ifm:interface/tnlm:tunnel-protocol/tnlm:type/gre:gre/gre:gre-tunnel">
    <description>
      <text>Unicast forward configuration to the GRE module.</text>
    </description>
    <container name="unicast-forward">
      <description>
        <text>Configure GRE unicast forward.</text>
      </description>
      <leaf name="tcp-mss">
        <type name="uint16">
          <range value="32..9500"/>
        </type>
        <units name="Byte"/>
        <description>
          <text>Maximum Segment Size of an interface.</text>
        </description>
      </leaf>
    </container>
  </augment>
</module>
