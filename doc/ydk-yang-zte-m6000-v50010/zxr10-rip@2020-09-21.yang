                          ;
              type string {
                length 1..200 ;
              }
            }
            leaf route-policy {
              description 'Apply route-policy to redistribute.'
                          ;
              type string {
                length 1..200 ;
              }
            }
          }
        }
        container redistribute-others {
          description 'Redistribute information from another routing protocol.'
                      ;
          list redistribute-other {
            description 'Redistribute information from another routing protocol.'
                        ;
            key 'vrf-name protocol'
                ;
            leaf vrf-name {
              description 'VPN Routing/Forwarding instance name (1-32 character'
                        + 's).'
                          ;
              type string {
                length 1..32 ;
              }
            }
            leaf protocol {
              description 'Routing protocol type.'
                          ;
              type enumeration {
                enum connected {
                  value 0 ;
                  description 'Connected routes.'
                              ;
                }
                enum static {
                  value 1 ;
                  description 'Static routes.'
                              ;
                }
                enum bgp-ext {
                  value 4 ;
                  description 'External border gateway protocol (EBGP) routes.'
                              ;
                }
                enum bgp-int {
                  value 5 ;
                  description 'Internal border gateway protocol (IBGP) routes.'
                              ;
                }
                enum nat {
                  value 8 ;
                  description 'NAT routes.'
                              ;
                }
                enum natpt {
                  value 9 ;
                  description 'Port-address Translation routes.'
                              ;
                }
                enum subscriber-host {
                  value 10 ;
                  description "Bras subscriber's host routes."
                              ;
                }
                enum subscriber-aggregation {
                  value 11 ;
                  description "Bras subscriber's aggregation routes."
                              ;
                }
                enum user-special {
                  value 14 ;
                  description 'User-special routes.'
                              ;
                }
                enum ipsec {
                  value 15 ;
                  description 'IPsec routes.'
                              ;
                }
