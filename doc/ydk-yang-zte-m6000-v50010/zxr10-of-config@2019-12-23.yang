                            ;
                type binary {
                  length 1..255 ;
                }
                mandatory true ;
              }
            }
            ncd:revise-date 2014-10-17 ;
          }
          container resources {
            description "The list of identifiers of all resources of the
\nOpen"
                      + "Flow Capable Switch that the OpenFlow Logical Switch
\n"
                      + "has exclusive or non-exclusive access to. A resource i"
                      + "s
\nidentified by the value of its resource-identifier"
                      + " element.
\nFor each resource identifier value in this"
                      + " list, there MUST
\nbe an element with a matching reso"
                      + "urce identifier value in
\nthe resources list of the O"
                      + "penFlow Capable Switch.
\nIdentifiers ofthis list are "
                      + "contained in elements
\nindicating the type of resourc"
                      + "e: 'port', 'queue',
\n'certificate', or 'flow-table'. "
                      + "Depending on the type,
\ndifferent constraints apply. "
                      + "These are specified in
\nseparate descriptions per typ"
                      + "e.
\nAt present the elements in this lists are not con"
                      + "figurable
\nandcan only be retrieved by NETCONF <get> "
                      + "or <get-config>
\noperations. Attemps to modify this e"
                      + "lement and its children
\nwith a NETCONF <edit-config>"
                      + " operation MUST result in an
\n'operation-not-supporte"
                      + "d' error with type 'application'."
                        ;
            list port {
              description 'The list of a port of the OpenFlow Capable Switch th'
                        + 'at the OpenFlow Logical Switch has exclusive access '
                        + 'to.'+"\n"
                        + 'port: This element is the name of a port that is bou'
                        + 'nd by logic switch, and must be a unique.'+"\n"
                        + 'number: This element is the number of a port that is'
                        + ' bound by logic switch. And it can be assigned optio'
                        + 'nally.'
                          ;
              key id ;
              leaf id {
                description 'A resource identifier of a port of the OpenFlow Ca'
                          + 'pable Switch that the OpenFlow Logical Switch has '
                          + 'exclusive access to.'+"\n"
                          + 'The elements in this list MUST refer to elements a'
                          + 't the following path:'+"\n"
                          + '/capable-switch/resources/port/resource-id'+"\n"
                          + 'Elements in this list MUST be unique. This means e'
                          + 'ach port element can only be referenced once.'
                            ;
                type leafref {
                  path /configuration/capable-switch/resources/port/resource-id ;
                }
              }
              leaf number {
                description 'The port number assigned by logic switch optionall'
                          + 'y when the logic switch bind the port.The range is'
                          + ' from 1 to 65535.'
                            ;
                type uint32 {
                  range 1..65535 ;
                }
                ncd:revise-date 2017-08-02 ;
              }
            }
            ncd:revise-date 2015-01-15 ;
          }
          container ofls-attributes {
            list ofls-attribute {
              key id ;
              leaf id {
                type leafref {
                  path /configuration/capable-switch/resources/port/resource-id ;
                }
              }
              container vlans {
                leaf vlan-bitmap {
                  type ncd:bitmap {
                    length 1..4094 ;
                  }
                }
              }
            }
            ncd:revise-date 2018-04-26 ;
          }
          container max-backoff {
            leaf time {
              type uint8 {
                range 1..90 ;
              }
            }
            ncd:revise-date 2017-10-23 ;
          }
          container probe-interval {
            description 'The local probe interval time and probe count of one O'
                      + 'penFlow Logical Switches.'
                        ;
            presence local-probe-interval ;
            leaf time {
              description 'The local probe interval time (s) of one OpenFlow Lo'
                        + 'gical Switches, range from 1s to 90s.'
                          ;
              type uint8 {
                range 1..90 ;
              }
              mandatory true ;
            }
            leaf count {
              description 'The local probe count of one OpenFlow Logical Switch'
                        + 'es, range from 1 to 10.'
                          ;
              type uint8 {
                range 1..10 ;
              }
            }
            ncd:revise-date 2018-06-22 ;
          }
          container tcp-mss {
            description 'TCP initial maximum segment size.'
                        ;
            presence ofls-tcp-mss ;
            leaf size {
              description 'The MSS size, range from 68 to 10000.'
                          ;
              type uint32 {
                range 68..10000 ;
              }
              mandatory true ;
            }
            ncd:revise-date 2019-12-23 ;
          }
          ncd:select id ;
          ncd:select datapath-id ;
        }
      }
      container resources {
        description "The list of identifiers of all resources of the
\nOpenFlow"
                  + " Capable Switch that the OpenFlow Logical Switch
