<?xml version="1.0" encoding="UTF-8"?>
<module name="huawei-unicast-fwd-notification"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:unicast-fwd-notification="urn:huawei:yang:huawei-unicast-fwd-notification"
        xmlns:ifm="urn:huawei:yang:huawei-ifm"
        xmlns:inet="urn:ietf:params:xml:ns:yang:ietf-inet-types"
        xmlns:ni="urn:huawei:yang:huawei-network-instance"
        xmlns:ext="urn:huawei:yang:huawei-extension">
  <yang-version value="1"/>
  <namespace uri="urn:huawei:yang:huawei-unicast-fwd-notification"/>
  <prefix value="unicast-fwd-notification"/>
  <import module="huawei-ifm">
    <prefix value="ifm"/>
  </import>
  <import module="ietf-inet-types">
    <prefix value="inet"/>
  </import>
  <import module="huawei-network-instance">
    <prefix value="ni"/>
  </import>
  <import module="huawei-extension">
    <prefix value="ext"/>
  </import>
  <organization>
    <text>Huawei Technologies Co., Ltd.</text>
  </organization>
  <contact>
    <text>Huawei Industrial Base
Bantian, Longgang
Shenzhen 518129
People's Republic of China
Website: http://www.huawei.com
Email: support@huawei.com</text>
  </contact>
  <description>
    <text>Notification of unicast-fwd-notification.</text>
  </description>
  <revision date="2020-02-28">
    <description>
      <text>Update the nodes.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <revision date="2019-12-18">
    <description>
      <text>Update the name of the nodes.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <revision date="2019-07-08">
    <description>
      <text>Init revision.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <ext:task-name value="forwarding"/>
  <notification name="forward-arp-alarm">
    <description>
      <text>The ARP Miss event continuously exists.</text>
    </description>
    <leaf name="slot">
      <type name="string">
        <length value="0..15"/>
      </type>
      <description>
        <text>Slot ID.</text>
      </description>
    </leaf>
    <leaf name="ifname">
      <type name="leafref">
        <path value="/ifm:ifm/ifm:interfaces/ifm:interface/ifm:name"/>
      </type>
      <description>
        <text>Interface index.</text>
      </description>
    </leaf>
  </notification>
  <notification name="forward-arp-alarm-resume">
    <description>
      <text>The ARP Miss alarm has resumed.</text>
    </description>
    <leaf name="slot">
      <type name="string">
        <length value="0..15"/>
      </type>
      <description>
        <text>Slot ID.</text>
      </description>
    </leaf>
    <leaf name="ifname">
      <type name="leafref">
        <path value="/ifm:ifm/ifm:interfaces/ifm:interface/ifm:name"/>
      </type>
      <description>
        <text>Interface index.</text>
      </description>
    </leaf>
  </notification>
  <notification name="forward-fib-alarm">
    <description>
      <text>The board in [slot] does not support import of [route-description] static routes between public and VPN instances.</text>
    </description>
    <leaf name="slot">
      <type name="string">
        <length value="0..15"/>
      </type>
      <description>
        <text>Slot ID.</text>
      </description>
    </leaf>
    <leaf name="route-description">
      <type name="string">
        <length value="0..15"/>
      </type>
      <description>
        <text>IPv4/IPv6 route flag.</text>
      </description>
    </leaf>
  </notification>
  <notification name="forward-fib-alarm-resume">
    <description>
      <text>The alarm is cleared, and the device becomes normal.</text>
    </description>
    <leaf name="slot">
      <type name="string">
        <length value="0..15"/>
      </type>
      <description>
        <text>Slot ID.</text>
      </description>
    </leaf>
    <leaf name="route-description">
      <type name="string">
        <length value="0..15"/>
      </type>
      <description>
        <text>IPv4/IPv6 route flag.</text>
      </description>
    </leaf>
  </notification>
  <notification name="forward-resource-lack-for-license">
    <description>
      <text>The number of used resources on the board has exceeded the upper threshold. To continue using these resources, purchase and activate corresponding license resource items as soon as possible.</text>
    </description>
    <leaf name="license-item">
      <type name="string">
        <length value="0..63"/>
      </type>
      <description>
        <text>License type.</text>
      </description>
    </leaf>
    <leaf name="slot">
      <type name="string">
        <length value="0..15"/>
      </type>
      <description>
        <text>Slot ID.</text>
      </description>
    </leaf>
    <leaf name="resource-name">
      <type name="string">
        <length value="0..127"/>
      </type>
      <description>
        <text>Resource item name.</text>
      </description>
    </leaf>
  </notification>
  <notification name="forward-resource-lack-for-license-resume">
    <description>
      <text>The license resource alarm has resumed.</text>
    </description>
    <leaf name="license-item">
      <type name="string">
        <length value="0..63"/>
      </type>
      <description>
        <text>License type.</text>
      </description>
    </leaf>
    <leaf name="slot">
      <type name="string">
        <length value="0..15"/>
      </type>
      <description>
        <text>Slot ID.</text>
      </description>
    </leaf>
    <leaf name="resource-name">
      <type name="string">
        <length value="0..127"/>
      </type>
      <description>
        <text>Resource item name.</text>
      </description>
    </leaf>
  </notification>
  <notification name="forward-fail-for-license">
    <description>
      <text>Because the license named [license-item] was not activated for the board in the slot [slot],the operation is failed.</text>
    </description>
    <leaf name="license-item">
      <type name="string">
        <length value="0..63"/>
      </type>
      <description>
        <text>License name.</text>
      </description>
    </leaf>
    <leaf name="slot">
      <type name="string">
        <length value="0..15"/>
      </type>
      <description>
        <text>Slot ID.</text>
      </description>
    </leaf>
    <leaf name="operation">
      <type name="uint32">
        <range value="0..64"/>
      </type>
      <description>
        <text>Operation ID.</text>
      </description>
    </leaf>
  </notification>
  <notification name="forward-fail-for-license-resume">
    <description>
      <text>The license named [license-item] was activated for the board in the slot [slot].</text>
    </description>
    <leaf name="license-item">
      <type name="string">
        <length value="0..63"/>
      </type>
      <description>
        <text>License name.</text>
      </description>
    </leaf>
    <leaf name="slot">
      <type name="string">
        <length value="0..15"/>
      </type>
      <description>
        <text>Slot ID.</text>
      </description>
    </leaf>
    <leaf name="operation">
      <type name="uint32">
        <range value="0..64"/>
      </type>
      <description>
        <text>Operation ID.</text>
      </description>
    </leaf>
  </notification>
  <notification name="seu-igp-down-risking">
    <description>
      <text>A SEU logic error occurred on the subcard of the current interface, causing the IGP interruption.</text>
    </description>
    <leaf name="card">
      <type name="uint32">
        <range value="0..64"/>
      </type>
      <description>
        <text>Card ID.</text>
      </description>
    </leaf>
    <leaf name="port">
      <type name="uint32">
        <range value="0..256"/>
      </type>
      <description>
        <text>Port ID.</text>
      </description>
    </leaf>
    <leaf name="ifname">
      <type name="leafref">
        <path value="/ifm:ifm/ifm:interfaces/ifm:interface/ifm:name"/>
      </type>
      <description>
        <text>Interface name.</text>
      </description>
    </leaf>
    <leaf name="protocol">
      <type name="string">
        <length value="0..63"/>
      </type>
      <description>
        <text>Protocol name.</text>
      </description>
    </leaf>
  </notification>
  <notification name="seu-igp-down-risking-resume">
    <description>
      <text>The alarm was cleared because the IGP went Up again or the soft failure of the SEU logic on the subcard was rectified.</text>
    </description>
    <leaf name="card">
      <type name="uint32">
        <range value="0..64"/>
      </type>
      <description>
        <text>Card ID.</text>
      </description>
    </leaf>
    <leaf name="port">
      <type name="uint32">
        <range value="0..256"/>
      </type>
      <description>
        <text>Port ID.</text>
      </description>
    </leaf>
    <leaf name="ifname">
      <type name="leafref">
        <path value="/ifm:ifm/ifm:interfaces/ifm:interface/ifm:name"/>
      </type>
      <description>
        <text>Interface name.</text>
      </description>
    </leaf>
    <leaf name="protocol">
      <type name="string">
        <length value="0..63"/>
      </type>
      <description>
        <text>Protocol name.</text>
      </description>
    </leaf>
  </notification>
  <notification name="nd-proxy-not-support-alarm">
    <description>
      <text>The board does not support proxy ND on VLANIF and BDIF interfaces.</text>
    </description>
    <leaf name="slot">
      <type name="string">
        <length value="0..15"/>
      </type>
      <description>
        <text>Slot ID.</text>
      </description>
    </leaf>
  </notification>
  <notification name="nd-proxy-not-support-alarm-resume">
    <description>
      <text>The alarm is cleared because the proxy ND configuration has been deleted on the current VLANIF and BDIF interfaces or the board has been replaced with one that supports proxy ND on VLANIF and BDIF interfaces.</text>
    </description>
    <leaf name="slot">
      <type name="string">
        <length value="0..15"/>
      </type>
      <description>
        <text>Slot ID.</text>
      </description>
    </leaf>
  </notification>
  <notification name="forward-entry-inconsistent">
    <description>
      <text>The entry on the control plane became inconsistent with that on the forwarding plane.</text>
    </description>
    <leaf name="service-type">
      <type name="uint32"/>
      <description>
        <text>Service type.</text>
      </description>
    </leaf>
    <leaf name="vpn-name">
      <type name="leafref">
        <path value="/ni:network-instance/ni:instances/ni:instance/ni:name"/>
      </type>
      <description>
        <text>VPN name.</text>
      </description>
    </leaf>
    <leaf name="destination">
      <type name="inet:ipv4-address-no-zone"/>
      <description>
        <text>Destination IP address.</text>
      </description>
    </leaf>
    <leaf name="destination-mask">
      <type name="uint32">
        <range value="1..32"/>
      </type>
      <description>
        <text>Destination IP address mask.</text>
      </description>
    </leaf>
    <leaf name="index">
      <type name="uint32"/>
      <description>
        <text>Reserved parameter.</text>
      </description>
    </leaf>
    <leaf name="description">
      <type name="string">
        <length value="1..64"/>
      </type>
      <description>
        <text>Detailed description of forwarding information.</text>
      </description>
    </leaf>
  </notification>
  <notification name="forward-entry-inconsistent-resume">
    <description>
      <text>The entry on the control plane became consistent with that on the forwarding plane.</text>
    </description>
    <leaf name="service-type">
      <type name="uint32"/>
      <description>
        <text>Service type.</text>
      </description>
    </leaf>
    <leaf name="vpn-name">
      <type name="leafref">
        <path value="/ni:network-instance/ni:instances/ni:instance/ni:name"/>
      </type>
      <description>
        <text>VPN name.</text>
      </description>
    </leaf>
    <leaf name="destination">
      <type name="inet:ipv4-address-no-zone"/>
      <description>
        <text>Destination IP address.</text>
      </description>
    </leaf>
    <leaf name="destination-mask">
      <type name="uint32">
        <range value="1..32"/>
      </type>
      <description>
        <text>Destination IP address mask.</text>
      </description>
    </leaf>
    <leaf name="index">
      <type name="uint32"/>
      <description>
        <text>Reserved parameter.</text>
      </description>
    </leaf>
    <leaf name="description">
      <type name="string">
        <length value="1..64"/>
      </type>
      <description>
        <text>Detailed description of forwarding information.</text>
      </description>
    </leaf>
  </notification>
</module>
