                       +--rw dirc-nexthop
                       |  +--rw (dirc-nexthop)?
                       |     +--:(dirc-ipv4-nexthop)
                       |     |  +--rw dirc-ipv4-nexthop?   ncd:ipv4-address-none-zero
                       |     +--:(dirc-ipv6-nexthop)
                       |        +--rw dirc-ipv6-nexthop?   ncd:ipv6-address-none-zero
                       +--rw bsl?             static-bsl
                       +--rw encapsulation?   encapsulation-type
                       +--rw bift-encoding?   uint8
                       +--rw bift-value?      uint32
