        </type>
        <units name="packet"/>
        <description>
          <text>Configure the pending number of the packets send by radius.</text>
        </description>
      </leaf>
      <leaf name="account-pending-limit">
        <type name="uint32">
          <range value="1..255"/>
        </type>
        <units name="packet"/>
        <description>
          <text>Configure the pending number of the packets send by radius.</text>
        </description>
      </leaf>
