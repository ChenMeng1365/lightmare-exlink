      container vrf-offset-lists {
        description 'Apply offset lists to RIP.'
                    ;
        container for-insts {
          description 'Apply offset lists to RIP instance.'
                      ;
          presence without-ifs ;
          list for-inst {
            description 'Apply offset lists to RIP instance.'
                        ;
            key 'vrf-name in-or-out'
                ;
            leaf vrf-name {
              description 'VPN Routing/Forwarding instance name (1-32 character'
                        + 's).'
                          ;
              type string {
                length 1..32 ;
              }
            }
            leaf in-or-out {
              description 'Specify the direction of offset list.'
                          ;
              type enumeration {
                enum in {
                  value 1 ;
                  description 'Inbound packets.'
                              ;
                }
                enum out {
                  value 2 ;
                  description 'Outbound packets.'
                              ;
                }
              }
            }
            leaf acl-name {
              description 'Access list of networks to apply offset list(1-31 ch'
                        + 'aracters).'
                          ;
              type string {
                length 1..31 ;
              }
              mandatory true ;
            }
            leaf metric {
              description 'Offset list metric.'
                          ;
              type uint8 {
                range 0..16 ;
              }
              mandatory true ;
            }
          }
        }
        container for-ifs {
          description 'Apply offset lists to RIP interface.'
                      ;
          presence with-ifs ;
          list for-if {
            description 'Apply offset lists to RIP interface.'
                        ;
            key 'vrf-name in-or-out interface'
                ;
            leaf vrf-name {
              description 'VPN Routing/Forwarding instance name (1-32 character'
                        + 's).'
                          ;
              type string {
                length 1..32 ;
              }
            }
            leaf in-or-out {
              description 'Specify the direction of offset list.'
                          ;
              type enumeration {
                enum in {
                  value 1 ;
                  description 'Inbound packets.'
                              ;
                }
                enum out {
                  value 2 ;
                  description 'Outbound packets.'
                              ;
                }
              }
            }
            leaf interface {
              description 'Interface name.'
                          ;
              type ncd:ifname {
                length 1..31 ;
              }
            }
            leaf acl-name {
              description 'Access list of networks to apply offset (1-31 charac'
                        + 'ters).'
                          ;
              type string {
                length 1..31 ;
              }
              mandatory true ;
            }
            leaf metric {
              description 'Offset list metric.'
                          ;
              type uint8 {
                range 0..16 ;
              }
              mandatory true ;
            }
          }
        }
      }
      container auth-keychains {
        description 'Configure authentication keychain in RIP module.'
                    ;
        list auth-keychain {
          description 'Configure authentication keychain in RIP module.'
                      ;
          key 'if-name key-id'
              ;
          leaf if-name {
            description 'Interface name.'
                        ;
            type ncd:ifname ;
          }
          leaf key-id {
            description 'Key identifier.'
                        ;
            type uint8 {
              range 1..255 ;
            }
          }
          choice auth-key {
            description 'Specify the authentication key.'
                        ;
            case auth-key-clear {
              description 'Set the authentication key in clear text.'
                          ;
              container auth-key-clear {
                description 'Set the authentication key in clear text.'
                            ;
                leaf key-clear {
                  description 'Set the authentication key in clear text.'
                              ;
                  type string {
                    length 1..16 ;
                  }
                  mandatory true ;
                }
              }
            }
            case auth-key-encrypted {
              description 'Set the authentication key in encrypted text.'
                          ;
              container auth-key-encrypted {
                description 'Set the authentication key in encrypted text.'
                            ;
                leaf key-encrypted {
                  description 'Set the authentication key in encrypted text.'
                              ;
                  type string {
                    length 1..84 ;
                  }
                  mandatory true ;
                }
              }
            }
          }
        }
      }
    }
  }
  container state {
    config false ;
    description 'The querying information processing of RIP module.'
                ;
    container rip {
      description 'The querying information processing of RIP module.'
                  ;
      container neighbors {
        description 'The querying information of RIP neighbors.'
                    ;
        list neighbor {
          description 'The list of RIP neighbor querying information.'+"\n"
                    + 'For all RIP neighbors of public network, need select obj'
                    + '-type(1).'+"\n"
                    + 'For the special RIP neighbor of public network, need sel'
                    + 'ect obj-type(1) and neighbor-addr.'+"\n"
                    + 'For all RIP neighbors of one VPN, need select obj-type(2'
                    + ') and vrf-name.'+"\n"
                    + 'For the special RIP neighbor of one VPN, need select obj'
                    + '-type(2), vrf-name and neighbor-addr.'
                      ;
          key 'obj-type vrf-name neighbor-addr'
              ;
          leaf obj-type {
            description 'The type of network. The "global" means the public net'
                      + 'work, the "vpn" means private network.'
                        ;
            type select-mode-def ;
          }
          leaf vrf-name {
            description 'VPN Routing/Forwarding instance name (1-32 characters).'
                        ;
            type string {
              length 0..32 ;
            }
          }
          leaf neighbor-addr {
            description 'Neighbor IP address.'
                        ;
            type ncd:ipv4-address ;
          }
          leaf if-name {
            description 'Interface name.'
                        ;
            type ncd:ifname {
              length 0..31 ;
            }
          }
          ncd:select obj-type ;
          ncd:select 'obj-type vrf-name'
                     ;
          ncd:select 'obj-type neighbor-addr'
                     ;
          ncd:select 'obj-type vrf-name neighbor-addr'
                     ;
        }
      }
      container interfaces {
        description 'The querying information of RIP interfaces.'
                    ;
        list interface {
          description 'The list of RIP interface querying information.'+"\n"
                    + 'For all RIP interfaces of public network, need select ob'
                    + 'j-type(1).'+"\n"
                    + 'For all RIP interface of one VPN, need select obj-type(2'
                    + '), vrf-name.'+"\n"
                    + 'For the special RIP interface, need select if-name.'
                      ;
          key 'obj-type vrf-name if-name if-ip-addr'
              ;
          leaf obj-type {
            description 'The type of network. The "global" means the public net'
                      + 'work, the "vpn" means private network.'
                        ;
            type select-mode-def ;
          }
          leaf vrf-name {
            description 'VPN Routing/Forwarding instance name (1-32 characters).'
                        ;
            type string {
              length 0..32 ;
            }
          }
          leaf if-name {
            description 'Interface name.'
                        ;
            type ncd:ifname ;
          }
          leaf if-ip-addr {
            description 'The IP address on the interface.'
                        ;
            type ncd:ipv4-address ;
          }
          leaf if-ip-masklen {
            description 'The prefix length on the interface.'
                        ;
            type uint8 {
              range 0..32 ;
            }
          }
          leaf status {
            description 'The status of the interface.'
                        ;
            type enable-or-disable ;
          }
          leaf recieve-version {
            description 'The version of RIP message which can be recieved on th'
                      + 'e interface.'
                        ;
            type enumeration {
              enum v1 {
                value 1 ;
              }
              enum v2 {
                value 2 ;
              }
              enum all {
                value 0 ;
              }
            }
          }
          leaf send-version {
            description 'The version of RIP message sent on the interface,inclu'
                      + 'de the send mode in RIP-2.'
                        ;
            type enumeration {
              enum v1 {
                value 1 ;
                description RIP-1 ;
              }
              enum v2-multicast {
                value 2 ;
                description 'RIP-2 messages are multicast.'
                            ;
              }
              enum v2-broadcast {
                value 3 ;
                description 'RIP-2 messages are broadcast.'
                            ;
              }
            }
          }
          leaf split-horizon {
            description 'Split horizen status.'
                        ;
            type enable-or-disable ;
          }
          leaf poison-reverse {
            description 'Poison reverse status.'
                        ;
            type enable-or-disable ;
          }
          leaf subnet-only {
            description 'Only send subnet information about the interface.'
                        ;
            type enable-or-disable ;
          }
          leaf notify-default-route {
            description 'Notify default route on the interface.'
                        ;
            type enumeration {
              enum disable {
                value 0 ;
                description 'Can not notify default route on this interface.'
                            ;
              }
              enum enable {
                value 1 ;
                description 'Notify default route on this interface.'
                            ;
              }
              enum only {
                value 2 ;
                description 'Only notify default route on this interface.'
                            ;
              }
            }
          }
          leaf originate-default-route {
            description 'Originate default route on the interface.'
                        ;
            type enumeration {
              enum disable {
                value 0 ;
                description 'Can not originate default route on this interface.'
                            ;
              }
              enum enable {
                value 1 ;
                description 'Originate default route and send it on this interf'
                          + 'ace.'
                            ;
              }
              enum only {
                value 2 ;
                description 'Originate default route and only send it on this i'
                          + 'nterface.'
                            ;
              }
            }
          }
          leaf neighbor-strict {
            description 'The interface restrict mode of communicate with neighb'
                      + 'ors.'
                        ;
            type enable-or-disable ;
          }
          leaf if-mode {
            description 'The mode of the interface,include active(only send),pa'
                      + 'ssive(only recieve) and normal.'
                        ;
            type enumeration {
              enum normal {
                value 0 ;
                description 'This RIP interface both send and receive.'
                            ;
              }
              enum active {
                value 1 ;
                description 'This RIP interface only send.'
                            ;
              }
              enum passive {
                value 2 ;
                description 'This RIP interface only receive.'
                            ;
              }
            }
          }
          leaf auth-mode {
            description 'The authentication mode of the interface.'
                        ;
            type enumeration {
              enum none {
                value 0 ;
                description 'No authentication.'
                            ;
              }
              enum text {
                value 1 ;
                description 'Simple text authentication.'
                            ;
              }
              enum md5 {
                value 2 ;
                description 'MD5 authentication.'
                            ;
              }
            }
          }
          leaf bfd {
            description BFD. ;
            type enable-or-disable ;
          }
          leaf keychain-name {
            description 'The name of keychain.'
                        ;
            type string {
              length 0..31 ;
            }
          }
          leaf route-policy-name {
            description 'The name of route policy.'
                        ;
            type string {
              length 0..31 ;
            }
          }
          ncd:select if-name ;
          ncd:select obj-type ;
          ncd:select 'obj-type vrf-name'
                     ;
        }
      }
      container statistics {
        description 'The querying information of RIP statistics.'
                    ;
        list statistic {
          description 'The list of RIP statistic querying information.'+"\n"
                    + 'For RIP statistic of public instance, need select obj-ty'
                    + 'pe(1).'+"\n"
                    + 'For RIP statistic of VRF instance, need select obj-type('
                    + '2) and vrf-name.'
                      ;
          key 'obj-type vrf-name'
              ;
          leaf obj-type {
            description 'The type of network. The "global" means the public net'
                      + 'work, the "vpn" means private network.'
                        ;
            type select-mode-def ;
          }
          leaf vrf-name {
            description 'VPN Routing/Forwarding instance name (1-32 characters).'
                        ;
            type string {
              length 0..32 ;
            }
          }
          leaf total-packets-sent {
            description 'The total of packets sent.'
                        ;
            type uint32 {
              range 0..4294967295 ;
            }
          }
          leaf request-packets-sent {
            description 'The total of request packets sent.'
                        ;
            type uint32 {
              range 0..4294967295 ;
            }
          }
          leaf response-packets-sent {
            description 'The total of response packets sent.'
                        ;
            type uint32 {
              range 0..4294967295 ;
            }
          }
          leaf total-packets-recieved {
            description 'The total of packets recieved.'
                        ;
            type uint32 {
              range 0..4294967295 ;
            }
          }
          leaf request-packets-recieved {
            description 'The total of request packets recieved.'
                        ;
            type uint32 {
              range 0..4294967295 ;
            }
          }
          leaf response-packets-recieved {
            description 'The total of response packets recieved.'
                        ;
            type uint32 {
              range 0..4294967295 ;
            }
          }
          ncd:select obj-type ;
          ncd:select 'obj-type vrf-name'
                     ;
        }
      }
      container routes {
        description 'The querying information of RIP routes.'
                    ;
        list route {
          description 'The list of RIP route querying information.'+"\n"
                    + 'For all RIP routes of public network, need select obj-ty'
                    + 'pe(1).'+"\n"
                    + 'For the special RIP route of public network, need select'
                    + ' obj-type(1), route-addr and route-mask-len.'+"\n"
                    + 'For all RIP routes of one VPN, need select obj-type(2) a'
                    + 'nd vrf-name.'+"\n"
                    + 'For the special RIP route of one VPN, need select obj-ty'
                    + 'pe(2), vrf-name, route-addr and route-mask-len.'
                      ;
          key 'obj-type vrf-name dest-addr dest-mask-len serial-number nexthop-'
            + 'addr'
              ;
          leaf obj-type {
            description 'The type of network. The "global" means the public net'
                      + 'work, the "vpn" means private network.'
                        ;
            type select-mode-def ;
          }
          leaf vrf-name {
            description 'VPN Routing/Forwarding instance name (1-32 characters).'
                        ;
            type string {
              length 0..32 ;
            }
          }
          leaf dest-addr {
            description 'The destination IP address of route.'
                        ;
            type ncd:ipv4-address ;
          }
          leaf dest-mask-len {
            description 'The mask length of route.'
                        ;
            type uint8 {
              range 0..32 ;
            }
          }
          leaf serial-number {
            description 'The serial number of route.'
                        ;
            type uint32 {
              range 0..4294967295 ;
            }
          }
          leaf nexthop-addr {
            description 'The nexthop address of route.'
                        ;
            type ncd:ipv4-address ;
          }
          leaf route-owner {
            description 'Route owner.'
                        ;
            type route-owner-def ;
          }
          leaf route-stauts {
            description 'Router status.'
                        ;
            type route-status-def ;
          }
          leaf metric {
            description 'The metric of route.'
                        ;
            type uint32 {
              range 0..16 ;
            }
          }
          leaf distance {
            description 'The distance of route.'
                        ;
            type uint8 {
              range 0..255 ;
            }
          }
          leaf tag {
            description 'The tag of route.'
                        ;
            type uint32 {
              range 0..4294967295 ;
            }
          }
          leaf instance-id {
            description 'The instance ID of route.'
                        ;
            type uint32 {
              range 0..4294967295 ;
            }
          }
          leaf time {
            description 'Route time.'
                        ;
            type ncd:time {
              length 0..11 ;
            }
          }
          leaf time-status {
            description 'Route time status.'
                        ;
            type route-time-status-def ;
          }
          ncd:select obj-type ;
          ncd:select 'obj-type dest-addr dest-mask-len'
                     ;
          ncd:select 'obj-type vrf-name'
                     ;
          ncd:select 'obj-type vrf-name dest-addr dest-mask-len'
                     ;
        }
      }
      container route-statistics {
        description 'The querying information of RIP route statistics.'
                    ;
        list route-statistic {
          description 'The list of RIP route statistic querying information.'+"\n"
                    + 'For RIP route statistic of public instance, need select '
                    + 'obj-type(1).'+"\n"
                    + 'For RIP route statistic of VRF instance, need select obj'
                    + '-type(2) and vrf-name.'
                      ;
          key 'obj-type vrf-name'
              ;
          leaf obj-type {
            description 'The type of network. The "global" means the public net'
                      + 'work, the "vpn" means private network.'
                        ;
            type select-mode-def ;
          }
          leaf vrf-name {
            description 'VPN Routing/Forwarding instance name (1-32 characters).'
                        ;
            type string {
              length 0..32 ;
            }
          }
          leaf rip {
            description 'The statistic of RIP route.'
                        ;
            type uint32 {
              range 0..4294967295 ;
            }
          }
          leaf rip-discard {
            description 'The statistic of RIP discard route.'
                        ;
            type uint32 {
              range 0..4294967295 ;
            }
          }
          leaf rip-connected {
            description 'The statistic of RIP connected route.'
                        ;
            type uint32 {
              range 0..4294967295 ;
            }
          }
          leaf redistributed {
            description 'The statistic of redistributed route.'
                        ;
            type uint32 {
              range 0..4294967295 ;
            }
          }
          leaf total {
            description 'The total number of routes.'
                        ;
            type uint32 {
              range 0..4294967295 ;
            }
          }
          ncd:select obj-type ;
          ncd:select 'obj-type vrf-name'
                     ;
        }
      }
    }
  }
}
