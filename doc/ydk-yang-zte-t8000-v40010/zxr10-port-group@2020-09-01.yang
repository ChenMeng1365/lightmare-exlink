  typedef group-type {
    type enumeration {
      enum msp {
        value 1 ;
        description 'Port group type is msp.'
                    ;
      }
      enum mc-aps {
        value 2 ;
        description 'Port group type is mc-aps.'
                    ;
      }
    }
    description 'Port group type.'
                ;
  }
  typedef protect-direction {
    type enumeration {
      enum unidirection {
        value 1 ;
        description 'Port group protection direction is unidirection.'
                    ;
      }
      enum bidirection {
        value 2 ;
        description 'Port group protection direction is bidirection.'
                    ;
      }
    }
    description 'Protection direction of port group.'
                ;
  }
  typedef protect-receiver {
    type enumeration {
      enum single-receiver {
        value 1 ;
        description 'Protection receiver of port group is single receiver.'
                    ;
      }
    }
    description 'Protection receiver of port group.'
                ;
  }
  typedef protect-strategy {
    type enumeration {
      enum aps {
        value 1 ;
        description 'Protection strategy of port group is aps.'
