      </container>
      <container name="multilink-ppp">
        <when condition="../../ifm:type='Mp-group' or ../../ifm:type='Global-Mp-Group'"/>
        <description>
          <text>Configure the interface that bundles multiple interfaces into a logical interface.</text>
        </description>
        <container name="mp-base">
          <description>
            <text>Configure multilink PPP base.</text>
          </description>
          <leaf name="fragment-size">
            <type name="mp-fragment-size"/>
            <default value="no-fragment"/>
            <description>
              <text>Set the size of an MP-group fragment.</text>
            </description>
          </leaf>
          <leaf name="minimum-threshold">
            <type name="uint8">
              <range value="1..32"/>
            </type>
            <default value="1"/>
            <description>
              <text>Minimum number of member links in an MP-group.</text>
            </description>
          </leaf>
          <leaf name="mrru">
            <type name="uint32">
              <range value="328..9216"/>
            </type>
            <default value="1500"/>
            <description>
              <text>Set the MP Max-Receive-Reconstructed Unit (MRRU) for an MP-group.</text>
            </description>
          </leaf>
          <leaf name="short-sequence">
            <when condition="../../../../ifm:type='Mp-group'"/>
            <type name="ppp-enable"/>
            <default value="disable"/>
            <description>
              <text>Enable/disable short-sequence negotiation.</text>
            </description>
          </leaf>
          <leaf name="discriminator">
            <type name="ppp-enable"/>
            <default value="enable"/>
            <description>
              <text>Enable/disable the MP discriminator negotiation.</text>
            </description>
          </leaf>
        </container>
        <container name="damping">
          <when condition="../../../ifm:type='Mp-group'"/>
          <must condition="detect-time &lt; damping-time"/>
          <presence value="Enable the function of suppressing the links in an MP-group."/>
          <description>
            <text>Enable/disable the function of suppressing the links in an MP-group.</text>
          </description>
          <leaf name="detect-time">
            <type name="uint32">
              <range value="30..3600"/>
            </type>
            <default value="30"/>
            <description>
              <text>MP-group detect time.</text>
            </description>
          </leaf>
          <leaf name="flap-count">
            <type name="uint16">
              <range value="1..64"/>
            </type>
            <default value="1"/>
            <description>
              <text>Maximum number of flappings allowed in an MP-group within the detect time.</text>
            </description>
          </leaf>
          <leaf name="damping-time">
            <type name="uint32">
              <range value="60..86400"/>
            </type>
            <default value="60"/>
            <description>
              <text>Time during which the MP-group links must be Up before being unsuppressed.</text>
            </description>
          </leaf>
        </container>
        <container name="mp-members">
          <description>
            <text>List of MP-group member interfaces.</text>
          </description>
          <list name="mp-member">
            <key value="member-interface"/>
            <max-elements value="32"/>
            <description>
              <text>Configure MP-group member interface.</text>
            </description>
            <leaf name="member-interface">
              <type name="leafref">
                <path value="/ifm:ifm/ifm:interfaces/ifm:interface/ifm:name"/>
              </type>
              <must condition="(/ifm:ifm/ifm:interfaces/ifm:interface[ifm:name = current()][ifm:link-protocol='ppp']/ifm:type='Serial' and ../../../../../ifm:type='Mp-group') or (/ifm:ifm/ifm:interfaces/ifm:interface[ifm:name = current()][ifm:link-protocol='ppp']/ifm:type='Trunk-Serial' and ../../../../../ifm:type='Global-Mp-Group')"/>
              <description>
                <text>Name of an MP-group member interface.</text>
              </description>
            </leaf>
            <leaf name="physical-status">
              <type name="mp-member-physical-state"/>
              <config value="false"/>
              <description>
                <text>Physical status of an MP-group member interface.</text>
              </description>
            </leaf>
            <leaf name="protocol-status">
              <type name="mp-member-protocol-state"/>
              <config value="false"/>
              <description>
                <text>Protocol status of an MP-group member interface.</text>
              </description>
            </leaf>
          </list>
        </container>
        <container name="mp-group-info">
          <config value="false"/>
          <description>
            <text>Statistics of an MP-group interface.</text>
          </description>
          <leaf name="valid-member-links">
            <type name="uint32"/>
            <description>
              <text>Number of valid MP-group member interfaces.</text>
            </description>
          </leaf>
          <leaf name="valid-member-slots">
            <type name="uint32"/>
            <description>
              <text>Number of valid MP-group member slots.</text>
            </description>
          </leaf>
          <leaf name="lost-fragments">
            <type name="uint32"/>
            <description>
              <text>Number of lost received fragments.</text>
            </description>
          </leaf>
          <leaf name="reordered-fragments">
            <type name="uint32"/>
            <description>
              <text>Number of reassembled received packets.</text>
            </description>
          </leaf>
          <leaf name="unassigned-fragments">
            <type name="uint32"/>
            <description>
              <text>Number of received fragments waiting to be reassembled.</text>
            </description>
          </leaf>
          <leaf name="interleaved-fragments">
            <type name="uint32"/>
            <description>
              <text>Number of sent LFI fragments.</text>
            </description>
          </leaf>
          <leaf name="received-sequences">
            <type name="uint32"/>
            <description>
              <text>Number of received sequences.</text>
            </description>
          </leaf>
          <leaf name="sent-sequences">
            <type name="uint32"/>
            <description>
              <text>Number of sent sequences.</text>
            </description>
          </leaf>
        </container>
      </container>
    </container>
  </augment>
</module>
