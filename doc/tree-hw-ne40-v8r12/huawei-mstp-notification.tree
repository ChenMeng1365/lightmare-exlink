module: huawei-mstp-notification

  notifications:
    +---n tc-flap-receive
    |  +--ro process-id?    uint32
    |  +--ro instance-id?   uint32
    |  +--ro if-index?      uint32
    |  +--ro if-name?       pub-type:if-name
    |  +--ro pw-name?       string
    |  +--ro src-mac?       pub-type:mac-address
    +---n tc-flap
    |  +--ro process-id?    uint32
    |  +--ro instance-id?   uint32
    +---n tc-flap-resume
       +--ro process-id?    uint32
       +--ro instance-id?   uint32
