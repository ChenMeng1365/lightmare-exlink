  <feature name="keepalive-period">
    <description>
      <text>Only for the use of 15K PC</text>
    </description>
  </feature>
  <feature name="pim-statistics">
    <description>
      <text>Only for the use of T8000, PC and M6000.</text>
    </description>
  </feature>
  <feature name="hello-track-support">
    <description>
      <text>PIM hello packet sended with the T bit in the LAN Prune Delay option, this indicates the neighbor's capability to disable Join message suppression.</text>
    </description>
  </feature>
  <feature name="trust-reg-source">
    <description>
      <text>Only for the use of T8000,15K and PC.</text>
    </description>
  </feature>
  <container name="configuration">
    <description>
      <text>PIM module configuration info.</text>
    </description>
    <container name="router-pim">
      <description>
        <text>PIM module configure root node.</text>
      </description>
      <container name="vrf-pims">
        <description>
          <text>Private mode of PIM module configure root node.</text>
        </description>
        <list name="vrf-pim">
          <description>
            <text>Private mode of PIM module configure root node.</text>
          </description>
          <key value="vrf-name"/>
          <leaf name="vrf-name">
            <description>
              <text>VPN Routing/Forwarding instance name</text>
            </description>
            <type name="string">
              <length value="1..32"/>
            </type>
          </leaf>
          <leaf name="srp-override">
            <description>
              <text>Static RP override</text>
            </description>
            <type name="empty"/>
          </leaf>
          <leaf name="srp-smartswitch">
            <description>
              <text>PIM RP smart switch</text>
            </description>
            <type name="empty"/>
          </leaf>
          <leaf name="bsm-unicast">
            <description>
              <text>PIM unicast bootstrap messages</text>
            </description>
            <type name="empty"/>
          </leaf>
          <leaf name="ssm-enable">
            <description>
              <text>SSM enable</text>
            </description>
            <type name="empty"/>
          </leaf>
          <leaf name="dr-switch-delay-value">
            <description>
              <text>PIM DR switch back delay</text>
            </description>
            <type name="uint16">
              <range value="1..300"/>
            </type>
            <units name="second"/>
          </leaf>
          <leaf name="accept-rp-acl">
            <description>
              <text>PIM RP accept filter</text>
            </description>
            <type name="string">
              <length value="1..31"/>
            </type>
          </leaf>
          <leaf name="reg-filter-acl">
            <description>
              <text>PIM registers accept filter</text>
            </description>
            <type name="string">
              <length value="1..31"/>
            </type>
          </leaf>
          <leaf name="data-filter-acl">
            <description>
              <text>PIM data filter</text>
            </description>
            <type name="string">
              <length value="1..31"/>
            </type>
          </leaf>
          <leaf name="jp-holdtime">
            <description>
              <text>PIM router join-prune holdtime</text>
            </description>
            <type name="uint16">
              <range value="210..65535"/>
            </type>
            <units name="second"/>
            <default value="210"/>
          </leaf>
          <leaf name="rp-proxy">
            <description>
              <text>PIM RP proxy</text>
            </description>
            <type name="empty"/>
          </leaf>
          <leaf name="reg-suppression-interval">
            <description>
              <text>PIM router register suppression interval</text>
            </description>
            <type name="uint16">
              <range value="60..3600"/>
            </type>
            <units name="second"/>
            <default value="60"/>
          </leaf>
          <leaf name="reg-probe-interval">
            <description>
              <text>PIM router register probe interval</text>
            </description>
            <type name="uint16">
              <range value="1..210"/>
            </type>
            <units name="second"/>
            <default value="5"/>
          </leaf>
          <leaf name="bidir-pim">
            <description>
              <text>Enable BIDIR-PIM</text>
            </description>
            <type name="empty"/>
          </leaf>
          <leaf name="jp-interval">
            <description>
              <text>PIM router join-prune interval</text>
            </description>
            <type name="uint16">
              <range value="10..6000"/>
            </type>
            <units name="second"/>
            <default value="60"/>
          </leaf>
          <leaf name="reg-holdtime">
            <description>
              <text>Register holdtime without receiving register stop</text>
            </description>
            <type name="uint16">
              <range value="1..600"/>
            </type>
            <units name="second"/>
          </leaf>
          <leaf name="mofrr-filter-acl">
            <description>
              <text>PIM MoFRR filter</text>
            </description>
            <type name="string">
              <length value="1..31"/>
            </type>
          </leaf>
          <leaf name="mldp-signal-enable">
            <description>
              <text>mldp-signal enable</text>
            </description>
            <type name="empty"/>
          </leaf>
          <leaf name="mldp-signal-filter">
            <description>
              <text>mldp-signal ACL</text>
            </description>
            <type name="string">
              <length value="1..31"/>
            </type>
          </leaf>
          <leaf name="assert-holdtime">
            <description>
              <text>configure assert holdtime</text>
            </description>
            <type name="uint16">
              <range value="60..65535"/>
            </type>
            <units name="second"/>
            <default value="180"/>
          </leaf>
          <leaf name="graft-retry-time">
            <description>
              <text>configure graft retry time</text>
            </description>
            <type name="uint16">
              <range value="1..65535"/>
            </type>
            <units name="second"/>
            <default value="3"/>
          </leaf>
          <leaf name="keepalive-period">
            <description>
              <text>The configuration of keepalive period. It's validated minimum may be related to the performance of the  certain product.</text>
            </description>
            <type name="uint16">
              <range value="60..65535"/>
            </type>
            <units name="second"/>
            <default value="210"/>
          </leaf>
          <leaf name="join-prune-speed">
            <description>
              <text>configure join prune speed</text>
            </description>
            <type name="uint16">
              <range value="1000..50000"/>
            </type>
            <default value="10000"/>
          </leaf>
          <leaf name="cbsr-holdtime">
            <description>
              <text>PIM router candidate BSR holdtime</text>
            </description>
            <type name="uint16">
              <range value="130..65535"/>
            </type>
            <units name="second"/>
            <default value="130"/>
            <ncd:revise-date date="2015-06-01"/>
          </leaf>
          <leaf name="cbsr-interval">
            <description>
              <text>PIM router candidate BSR interval</text>
            </description>
            <type name="uint16">
              <range value="10..65535"/>
            </type>
            <units name="second"/>
            <default value="60"/>
            <ncd:revise-date date="2015-06-01"/>
          </leaf>
          <leaf name="pim-statistics-acl">
            <description>
              <text>PIM packet statistics filter.</text>
            </description>
            <type name="string">
              <length value="1..31"/>
            </type>
            <ncd:revise-date date="2016-03-18"/>
          </leaf>
          <leaf name="reg-data-forward">
            <description>
              <text>PIM router register data forward.</text>
            </description>
            <type name="empty"/>
            <ncd:revise-date date="2019-07-15"/>
          </leaf>
          <leaf name="bsr-policy-acl">
            <description>
              <text>PIM BSR filter.</text>
            </description>
            <type name="string">
              <length value="1..31"/>
            </type>
            <ncd:revise-date date="2019-07-15"/>
          </leaf>
          <leaf name="interface-identifier">
            <description>
              <text>Enable interface identifier option.</text>
            </description>
            <type name="empty"/>
            <ncd:revise-date date="2019-08-29"/>
          </leaf>
          <leaf name="ecmp-redirect">
            <description>
              <text>Enable ECMP redirect.</text>
            </description>
            <type name="empty"/>
            <ncd:revise-date date="2019-08-29"/>
          </leaf>
          <container name="bsr">
            <description>
              <text>PIM candidate bootstrap router (candidate BSR)</text>
            </description>
            <presence value="The leaf intf_name of this container should be mandatory, and other leafs are not."/>
            <leaf name="intf-name">
              <description>
                <text>Interface name</text>
              </description>
              <type name="ncd:ifname">
                <length value="1..31"/>
              </type>
              <mandatory value="true"/>
            </leaf>
            <leaf name="priority">
              <description>
                <text>Priority value for candidate BSR</text>
              </description>
              <type name="uint8">
                <range value="0..255"/>
              </type>
              <default value="0"/>
            </leaf>
            <leaf name="hash-len">
              <description>
                <text>Hash mask length for candidate BSR</text>
              </description>
              <type name="uint8">
                <range value="0..32"/>
              </type>
              <default value="30"/>
            </leaf>
          </container>
          <container name="spt">
            <description>
              <text>PIM source-tree switching threshold</text>
            </description>
            <presence value="The leaf infinity of this container should be mandatory, and other leafs are not."/>
            <leaf name="acl">
              <description>
                <text>Access list name for group prefix</text>
              </description>
              <type name="string">
                <length value="1..31"/>
              </type>
            </leaf>
            <leaf name="infinity">
              <description>
                <text>Never switch to source-tree</text>
              </description>
              <type name="empty"/>
              <mandatory value="true"/>
            </leaf>
          </container>
          <container name="intfs">
            <description>
              <text>PIM interface</text>
            </description>
            <list name="intf">
              <description>
                <text>PIM interface</text>
              </description>
              <key value="intf-name"/>
              <leaf name="intf-name">
                <description>
                  <text>Interface name</text>
                </description>
                <type name="ncd:ifname">
                  <length value="1..31"/>
                </type>
              </leaf>
              <leaf name="pimsm">
                <description>
                  <text>Enable PIM-SM operation on the interface</text>
                </description>
                <type name="empty"/>
              </leaf>
              <leaf name="pimdm">
                <description>
                  <text>Enable PIM-DM operation on the interface</text>
                </description>
                <type name="empty"/>
                <ncd:revise-date date="2019-06-25"/>
              </leaf>
              <leaf name="assert-disable">
                <description>
                  <text>PIM assert disable</text>
                </description>
                <type name="empty"/>
              </leaf>
              <leaf name="bfd-enable">
                <description>
                  <text>PIM BFD enable</text>
                </description>
                <type name="empty"/>
              </leaf>
              <leaf name="bfd-template">
                <description>
                  <text>Set template name for bfd</text>
                </description>
                <type name="string">
                  <length value="1..31"/>
                </type>
                <ncd:revise-date date="2020-05-19"/>
              </leaf>
              <leaf name="bsr-border">
                <description>
                  <text>Border of PIM domain</text>
                </description>
                <type name="empty"/>
              </leaf>
              <leaf name="dr-priority">
                <description>
                  <text>PIM router DR priority</text>
                </description>
                <type name="uint32">
                  <range value="0..4294967295"/>
                </type>
                <default value="1"/>
              </leaf>
              <leaf name="nbr-filter-acl">
                <description>
                  <text>PIM peering filter</text>
                </description>
                <type name="string">
                  <length value="1..31"/>
                </type>
              </leaf>
              <leaf name="hello-interval">
                <description>
                  <text>PIM router hello interval</text>
                </description>
                <type name="uint16">
                  <range value="1..65535"/>
                </type>
                <units name="second"/>
                <default value="30"/>
              </leaf>
              <leaf name="silent">
                <description>
                  <text>PIM silent</text>
                </description>
                <type name="empty"/>
              </leaf>
              <leaf name="dr-ignore">
                <description>
                  <text>PIM-SM DR ignore</text>
                </description>
                <type name="empty"/>
              </leaf>
              <leaf name="frr-enable">
                <description>
                  <text>PIM FRR enable</text>
                </description>
                <type name="empty"/>
              </leaf>
              <leaf name="propagation-delay">
                <description>
                  <text>PIM propagation delay</text>
                </description>
                <type name="uint16">
                  <range value="1..100"/>
                </type>
                <units name="100millisecond"/>
                <default value="5"/>
              </leaf>
              <leaf name="override-interval">
                <description>
                  <text>PIM override interval</text>
                </description>
                <type name="uint16">
                  <range value="1..600"/>
                </type>
                <units name="100millisecond"/>
                <default value="25"/>
              </leaf>
              <leaf name="jp-max-packet-length">
                <description>
                  <text>configure the max length of Join/Prune packet</text>
                </description>
                <type name="uint16">
                  <range value="100..8100"/>
                </type>
                <units name="byte"/>
                <default value="1400"/>
              </leaf>
              <leaf name="hello-dr-addr">
                <description>
                  <text>PIM hello packet sended with hello DR address</text>
                </description>
                <type name="empty"/>
                <ncd:revise-date date="2015-06-19"/>
              </leaf>
              <leaf name="hello-track-support">
                <description>
                  <text>PIM hello packet sended with the T bit in the LAN Prune Delay option, this indicates the neighbor's capability to disable Join message suppression.</text>
                </description>
                <type name="empty"/>
                <ncd:revise-date date="2016-12-08"/>
              </leaf>
              <leaf name="triggered-hello-delay">
                <description>
                  <text>Triggered hello delay time.</text>
                </description>
                <type name="uint16">
                  <range value="1..5"/>
                </type>
                <units name="second"/>
                <default value="5"/>
                <ncd:revise-date date="2019-07-15"/>
              </leaf>
              <leaf name="pfm-border">
                <description>
                  <text>PFM border.</text>
                </description>
                <type name="empty"/>
                <ncd:revise-date date="2019-07-15"/>
              </leaf>
              <container name="ecmp-bundle">
                <description>
                  <text>ECMP bundle.</text>
                </description>
                <presence value="ecmp-bundle"/>
                <leaf name="name">
                  <description>
                    <text>ECMP bundle name.</text>
