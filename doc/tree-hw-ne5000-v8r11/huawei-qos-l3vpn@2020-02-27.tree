        +--ro policy-vxlan-statistics* [instance-name direction slot-id]
           +--ro instance-name             -> /ni:network-instance/instances/instance/name
           +--ro direction                 qos:qos-direction-type
           +--ro slot-id                   string
           +--ro policy-name?              -> /qos:qos/classifier-template/traffic-policys/traffic-policy/name
