  <grouping name="isis-global-config">
    <description>
      <text>This grouping defines lobal configuration options for ISIS router.</text>
    </description>
    <leaf name="instance">
      <type name="string"/>
      <default value="0"/>
      <description>
        <text>ISIS Instance.</text>
      </description>
    </leaf>
    <leaf-list name="net">
      <type name="oc-isis-types:net"/>
      <description>
        <text>ISIS network entity title (NET). The first 8 bits are usually
49 (private AFI), next 16 bits represent area, next 48 bits represent
system id and final 8 bits are set to 0.</text>
      </description>
      <reference>
        <text>International Organization for Standardization, Information
technology - Open Systems Interconnection-Network service
Definition - ISO/ IEC 8348:2002.</text>
      </reference>
    </leaf-list>
    <leaf name="maximum-area-addresses">
      <type name="uint8"/>
      <default value="3"/>
      <description>
        <text>Maximum areas supported.</text>
      </description>
    </leaf>
    <leaf name="level-capability">
      <type name="oc-isis-types:level-type"/>
      <default value="LEVEL_1_2"/>
      <description>
        <text>ISIS level capability(level-1, level-2,vlevel-1-2).</text>
      </description>
    </leaf>
    <leaf name="max-ecmp-paths">
      <type name="uint8"/>
      <description>
        <text>ISIS max-paths count.</text>
      </description>
    </leaf>
    <leaf name="poi-tlv">
      <type name="boolean"/>
      <default value="false"/>
      <description>
        <text>ISIS purge TLV. When set to true, a TLV is added to purges to record
the system ID  of the IS generating the purge.</text>
      </description>
      <reference>
        <text>RFC6232: Purge Originator Identification TLV for IS-IS. TLV 13.</text>
      </reference>
    </leaf>
    <leaf name="iid-tlv">
      <type name="boolean"/>
      <default value="false"/>
      <description>
        <text>ISIS Instance Identifier TLV. When set to trues, the IID-TLV identifies
the unique instance as well as the topology/topologies to which the
PDU applies.</text>
      </description>
      <reference>
        <text>RFC6822: IS-IS Multi-Instance. TLV 7</text>
      </reference>
    </leaf>
    <leaf name="fast-flooding">
      <type name="boolean"/>
      <default value="true"/>
      <description>
        <text>When set to true, IS will always flood the LSP that triggered an SPF
before the router actually runs the SPF computation.</text>
      </description>
    </leaf>
  </grouping>
  <grouping name="admin-config">
    <description>
      <text>Re-usable grouping to enable or disable a particular IS-IS feature.</text>
    </description>
    <leaf name="enabled">
      <type name="boolean"/>
      <default value="false"/>
      <description>
        <text>When set to true, the functionality within which this leaf is
defined is enabled, when set to false it is explicitly disabled.</text>
      </description>
    </leaf>
  </grouping>
  <grouping name="isis-bfd-config">
    <description>
      <text>This grouping defines Bidirectionl-Forwarding-Detection
configuration.</text>
    </description>
    <leaf name="bfd-tlv">
      <type name="boolean"/>
      <description>
        <text>When set to true, BFD TLV is used. This enables support for the IS-IS
BFD TLV options, which specify that a BFD session must be established
before an IS-IS adjacency can transition to the established state.
This option should be enabled on all IS-IS neighbors on a shared
interface.</text>
      </description>
      <reference>
        <text>RFC6213. TLV 148</text>
      </reference>
    </leaf>
    <reference>
      <text>RFC5880: Bidirectional Forwarding Detection (BFD).</text>
    </reference>
  </grouping>
  <grouping name="isis-authentication-check-config">
    <description>
      <text>This grouping defines ISIS authentication check.</text>
    </description>
    <leaf name="authentication-check">
      <type name="boolean"/>
      <default value="true"/>
      <description>
        <text>When set to true, reject all ISIS protocol PDUs that either have a mismatch
in authentication-type or authentication-key.</text>
      </description>
    </leaf>
  </grouping>
  <grouping name="isis-metric-style-config">
    <description>
      <text>This grouping defines ISIS metric style.</text>
    </description>
    <leaf name="metric-style">
      <type name="oc-isis-types:metric-style"/>
      <description>
        <text>ISIS metric style types(narrow, wide).</text>
      </description>
    </leaf>
  </grouping>
  <grouping name="authentication-key-config">
    <description>
      <text>This grouping defines authentication key configuration.</text>
    </description>
    <leaf name="auth-password">
      <type name="oc-types:routing-password"/>
      <description>
        <text>Authentication key string.</text>
      </description>
    </leaf>
  </grouping>
  <grouping name="keychain-base-group">
    <description>
      <text>This grouping defines keychain configuration.</text>
    </description>
    <container name="keychain">
      <description>
        <text>This container defines keychain parameters.</text>
      </description>
    </container>
  </grouping>
  <grouping name="isis-authentication-config">
    <description>
      <text>This grouping defines ISIS authentication configuration.</text>
    </description>
    <leaf name="csnp-authentication">
      <type name="boolean"/>
      <default value="false"/>
      <description>
        <text>Enable or disable for IS-IS CSNPs.</text>
      </description>
    </leaf>
    <leaf name="psnp-authentication">
      <type name="boolean"/>
      <default value="false"/>
      <description>
        <text>Enable or disable authentication for IS-IS PSNPs.</text>
      </description>
    </leaf>
    <leaf name="lsp-authentication">
      <type name="boolean"/>
      <default value="false"/>
      <description>
        <text>Enable or disable authentication for IS-IS LSPs.</text>
      </description>
    </leaf>
  </grouping>
  <grouping name="isis-authentication-group">
    <description>
      <text>This grouping defines ISIS authentication.</text>
    </description>
    <container name="config">
      <description>
        <text>This container defines ISIS authentication configuration.</text>
      </description>
      <uses name="isis-authentication-config"/>
    </container>
    <container name="state">
      <config value="false"/>
      <description>
        <text>This container defines ISIS authentication state.</text>
      </description>
      <uses name="isis-authentication-config"/>
    </container>
    <container name="key">
      <description>
        <text>This container defines ISIS authentication key</text>
      </description>
      <container name="config">
        <description>
          <text>This container defines ISIS authentication key configuration.</text>
        </description>
        <uses name="authentication-key-group-config"/>
      </container>
      <container name="state">
        <config value="false"/>
        <description>
          <text>This container defines ISIS authentication key state.</text>
        </description>
        <uses name="authentication-key-group-config"/>
      </container>
    </container>
    <uses name="keychain-base-group"/>
  </grouping>
  <grouping name="isis-hello-authentication-config">
    <description>
      <text>Configuration options for IS-IS hello authentication.</text>
    </description>
    <leaf name="hello-authentication">
      <type name="boolean"/>
      <default value="false"/>
      <description>
        <text>Enabled or disable ISIS Hello authentication.</text>
      </description>
    </leaf>
  </grouping>
  <grouping name="isis-hello-authentication-group">
    <description>
      <text>This grouping defines ISIS hello-authentication.</text>
    </description>
    <container name="config">
      <description>
        <text>This container defines ISIS authentication configuration.</text>
      </description>
      <uses name="isis-hello-authentication-config"/>
    </container>
    <container name="state">
      <config value="false"/>
      <description>
        <text>This container defines ISIS authentication state.</text>
      </description>
      <uses name="isis-hello-authentication-config"/>
    </container>
    <container name="key">
      <description>
        <text>This container defines ISIS authentication key</text>
      </description>
      <container name="config">
        <description>
          <text>This container defines ISIS authentication key configuration.</text>
        </description>
        <uses name="authentication-key-group-config"/>
      </container>
      <container name="state">
        <config value="false"/>
        <description>
          <text>This container defines ISIS authentication key state.</text>
        </description>
        <uses name="authentication-key-group-config"/>
      </container>
    </container>
    <uses name="keychain-base-group"/>
  </grouping>
  <grouping name="isis-ldp-igp-config">
    <description>
      <text>This grouping defines ISIS/LDP Synchronization configuration.</text>
    </description>
    <leaf name="enabled">
      <type name="boolean"/>
      <default value="true"/>
      <description>
        <text>When set to true, rely on IGP/LDP synchronization. IGP cost for
link is maintained at max until LDP adjacencies are established </text>
      </description>
      <reference>
        <text>RFC5443: LDP IGP Synchronization.</text>
      </reference>
    </leaf>
    <leaf name="post-session-up-delay">
      <type name="uint16"/>
      <units name="seconds"/>
      <description>
        <text>Specifies a delay, expressed in units of seconds,
between the LDP session to the IGP neighbor being established, and
it being considered synchronized by the IGP.</text>
      </description>
    </leaf>
  </grouping>
  <grouping name="isis-te-config">
    <description>
      <text>This grouping defines ISIS Traffic Engineering configuration.</text>
    </description>
    <leaf name="ipv4-router-id">
      <type name="inet:ipv4-address-no-zone"/>
      <description>
        <text>IPv4 MPLS Traffic Engineering Router-ID.</text>
      </description>
    </leaf>
    <leaf name="ipv6-router-id">
      <type name="inet:ipv6-address-no-zone"/>
      <description>
        <text>IPv6 MPLS Traffic Engineering Router-ID.</text>
      </description>
    </leaf>
  </grouping>
  <grouping name="isis-reference-bandwidth-config">
    <description>
      <text>This grouping defines ISIS Reference Bandwidth Configuration.</text>
    </description>
    <leaf name="reference-bandwidth">
      <type name="uint32"/>
      <description>
        <text>ISIS Reference Bandwidth value</text>
      </description>
    </leaf>
  </grouping>
  <grouping name="isis-overload-bit-set-config">
    <description>
      <text>This grouping defines ISIS Overload Bit.</text>
    </description>
    <leaf name="set-bit">
      <type name="boolean"/>
      <default value="false"/>
      <description>
        <text>When set to true, IS-IS overload bit is set.</text>
      </description>
    </leaf>
    <leaf name="set-bit-on-boot">
      <type name="boolean"/>
      <default value="false"/>
      <description>
        <text>When set to true, the IS-IS overload bit is set on system boot.</text>
      </description>
    </leaf>
    <leaf name="advertise-high-metric">
      <type name="boolean"/>
      <default value="false"/>
      <description>
        <text>When set to true, the local IS advertises links with the highest
available metric regardless of their configured metric. The metric
value is based on the metric style - if wide metrics are utilised
the metric is advertised as 16777214, otherwise they are advertised
with a value of 63.</text>
      </description>
    </leaf>
  </grouping>
  <grouping name="isis-overload-bit-reset-config">
    <description>
      <text>This grouping defines ISIS Overload Bit Reset Triggers</text>
    </description>
    <leaf name="reset-trigger">
      <type name="identityref">
        <base name="oc-isis-types:OVERLOAD_RESET_TRIGGER_TYPE"/>
      </type>
      <description>
        <text>In the case that the system sets the overload bit on start, the
system should reset the bit (i.e., clear the overload bit) upon
the specified trigger.</text>
      </description>
    </leaf>
    <leaf name="delay">
      <type name="uint16"/>
      <units name="seconds"/>
      <description>
        <text>If a reset trigger is specified, the system should delay resetting
the overload bit for the specified number of seconds after the
trigger occurs.</text>
      </description>
    </leaf>
  </grouping>
  <grouping name="isis-attached-bit-config">
    <description>
      <text>This grouping defines ISIS Attached Bit</text>
    </description>
    <leaf name="ignore-bit">
      <type name="boolean"/>
      <default value="false"/>
      <description>
        <text>When set to true, if the attached bit is set on an incoming Level 1
IS-IS, the local system ignores it. In this case the local system
does not set a default route to the L1L2 router advertising the PDU
with the attached bit set.</text>
      </description>
    </leaf>
    <leaf name="suppress-bit">
      <type name="boolean"/>
      <default value="false"/>
      <description>
        <text>When set to true, if the local IS acts as a L1L2 router, then the
attached bit is not advertised in locally generated PDUs.</text>
      </description>
    </leaf>
  </grouping>
  <grouping name="overload-bit-group">
    <description>
      <text>This grouping defines ISIS Overload Bit.</text>
    </description>
    <container name="config">
      <description>
        <text>This container defines ISIS Overload Bit configuration.</text>
      </description>
      <uses name="isis-overload-bit-set-config"/>
    </container>
    <container name="state">
      <config value="false"/>
      <description>
        <text>This container defines state for ISIS Overload Bit.</text>
      </description>
      <uses name="isis-overload-bit-set-config"/>
    </container>
    <container name="reset-triggers">
      <description>
        <text>This container defines state for ISIS Overload Bit reset triggers</text>
      </description>
      <list name="reset-trigger">
        <key value="reset-trigger"/>
        <description>
          <text>This list describes ISIS Overload reset trigger reasons.</text>
        </description>
        <leaf name="reset-trigger">
          <type name="leafref">
            <path value="../config/reset-trigger"/>
          </type>
          <description>
            <text>Reference to the reset trigger reason</text>
          </description>
        </leaf>
        <container name="config">
          <description>
            <text>This container defines ISIS Overload Bit reset trigger
configuration.</text>
          </description>
          <uses name="isis-overload-bit-reset-config"/>
        </container>
        <container name="state">
          <config value="false"/>
          <description>
            <text>This container defines state for ISIS Overload Bit reset
triggers.</text>
          </description>
          <uses name="isis-overload-bit-reset-config"/>
        </container>
      </list>
    </container>
  </grouping>
  <grouping name="isis-base-level-config">
    <description>
      <text>This grouping defines ISIS Level configuration.</text>
    </description>
    <leaf name="level-number">
      <type name="oc-isis-types:level-number"/>
      <description>
        <text>ISIS level number (level-1, level-2).</text>
      </description>
    </leaf>
  </grouping>
  <grouping name="isis-interface-level-config">
    <description>
      <text>This grouping defines ISIS Interface Level configuration.</text>
    </description>
    <leaf name="level-number">
      <type name="oc-isis-types:level-number"/>
      <description>
        <text>ISIS level number(level-1, level-2).</text>
      </description>
    </leaf>
    <leaf name="passive">
      <type name="boolean"/>
      <default value="false"/>
      <description>
        <text>ISIS passive interface admin enable/disable function.</text>
      </description>
    </leaf>
    <leaf name="priority">
      <type name="uint8">
        <range value="0 .. 127"/>
      </type>
      <description>
        <text>ISIS neighbor priority(LAN hello PDU only).</text>
      </description>
    </leaf>
  </grouping>
  <grouping name="isis-hello-timers-config">
    <description>
      <text>This grouping defines ISIS hello timers configuration.</text>
    </description>
    <leaf name="hello-interval">
      <type name="uint32"/>
      <description>
        <text>ISIS hello-interval value.</text>
      </description>
    </leaf>
    <leaf name="hello-multiplier">
      <type name="uint8"/>
      <description>
        <text>ISIS hello-multiplier value.</text>
      </description>
    </leaf>
  </grouping>
  <grouping name="isis-interface-config">
    <description>
      <text>This grouping defines ISIS interface configuration.</text>
    </description>
    <leaf name="interface-id">
      <type name="oc-if:interface-id"/>
      <description>
        <text>Interface for which ISIS configuration is to be applied.</text>
      </description>
    </leaf>
    <leaf name="passive">
      <type name="boolean"/>
      <default value="false"/>
      <description>
        <text>When set to true, the referenced interface is a passive interface
such that it is not eligible to establish adjacencies with other
systems, but is advertised into the IS-IS topology.</text>
      </description>
    </leaf>
    <leaf name="hello-padding">
      <type name="oc-isis-types:hello-padding-type"/>
      <description>
        <text>This leaf controls padding type for IS-IS Hello PDUs.</text>
      </description>
    </leaf>
    <leaf name="circuit-type">
      <type name="oc-isis-types:circuit-type"/>
      <description>
        <text>ISIS circuit type (p2p, broadcast).</text>
      </description>
    </leaf>
  </grouping>
  <grouping name="isis-adaptive-timers-state">
    <description>
      <text>This grouping defines ISIS adaptive timers state</text>
    </description>
    <leaf name="adaptive-timer">
      <type name="oc-isis-types:adaptive-timer-type"/>
      <description>
        <text>ISIS adaptive timer types (linear, exponential).</text>
      </description>
    </leaf>
  </grouping>
  <grouping name="isis-lsp-generation-timers-config">
    <description>
      <text>This grouping defines ISIS LSP Generation timers configuration</text>
    </description>
    <leaf name="lsp-max-wait-interval">
      <type name="uint64"/>
      <units name="milliseconds"/>
      <description>
        <text>Time interval in milliseconds that specifies max interval between
two consecutive occurrences of an LSP being generated.</text>
      </description>
    </leaf>
    <leaf name="lsp-first-wait-interval">
      <type name="uint64"/>
      <units name="milliseconds"/>
      <description>
        <text>Time interval in milliseconds that specifies the first LSP generation
delay.</text>
      </description>
    </leaf>
    <leaf name="lsp-second-wait-interval">
      <type name="uint64"/>
      <units name="milliseconds"/>
      <description>
        <text>Time interval in milliseconds that specifies the millisecond LSP
generation delay.</text>
      </description>
    </leaf>
  </grouping>
  <grouping name="isis-lsp-timers-config">
    <description>
      <text>This grouping defines ISIS LSP timers configuration</text>
    </description>
    <leaf name="lsp-lifetime-interval">
      <type name="uint16"/>
      <units name="seconds"/>
      <default value="1200"/>
      <description>
        <text>Time interval in seconds that specifies how long an LSP remains in
LSDB without being refreshed.</text>
      </description>
    </leaf>
    <leaf name="lsp-refresh-interval">
      <type name="uint16"/>
      <units name="seconds"/>
      <description>
        <text>Time interval in seconds that specifies how often route topology
that a device originates is transmitted in LSPs.</text>
      </description>
    </leaf>
  </grouping>
  <grouping name="isis-spf-timers-config">
    <description>
      <text>This grouping defines ISIS SPF timers configuration.</text>
    </description>
    <leaf name="spf-hold-interval">
      <type name="uint64"/>
      <units name="milliseconds"/>
      <default value="5000"/>
      <description>
        <text>SPF Hold Down time interval in milliseconds.</text>
      </description>
    </leaf>
    <leaf name="spf-first-interval">
      <type name="uint64"/>
      <units name="milliseconds"/>
      <description>
        <text>Time interval in milliseconds between the
detection of topology change and when the SPF algorithm runs.</text>
      </description>
    </leaf>
    <leaf name="spf-second-interval">
      <type name="uint64"/>
      <units name="milliseconds"/>
      <description>
        <text>Time interval in milliseconds between the first and second
SPF calculation.</text>
      </description>
