<?xml version="1.0" encoding="UTF-8"?>
<module name="zxr10-system-oc"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:zte-sys="http://www.zte.com.cn/zxr10/netconf/schema/rosng/zxr10-system-oc"
        xmlns:oc-sys="http://openconfig.net/yang/system">
  <yang-version value="1"/>
  <namespace uri="http://www.zte.com.cn/zxr10/netconf/schema/rosng/zxr10-system-oc"/>
  <prefix value="zte-sys"/>
  <import module="openconfig-system">
    <prefix value="oc-sys"/>
  </import>
  <organization>
    <text>ZTE Corporation</text>
  </organization>
  <contact>
    <text>ZTE Corporation  
   Nanjing Institute of ZTE Corporation 
   No.68 Zijinghua Rd. Yuhuatai District, Nanjing, China 
   Tel: +86-25-52870000</text>
  </contact>
  <description>
    <text>The netconf module to describe the System Management.</text>
  </description>
  <revision date="2020-07-07">
    <description>
      <text>Initial revision.</text>
    </description>
  </revision>
  <grouping name="system-operational-state">
    <description>
      <text>The operational state data for the system.</text>
    </description>
    <leaf name="uptime">
      <type name="uint32"/>
      <description>
        <text>The system uptime.</text>
      </description>
    </leaf>
  </grouping>
  <augment target-node="/oc-sys:system/oc-sys:state">
    <description>
      <text>The system uptime.</text>
    </description>
    <uses name="system-operational-state"/>
  </augment>
</module>
