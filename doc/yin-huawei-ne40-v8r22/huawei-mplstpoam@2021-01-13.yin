          <presence value="te-mip presence container"/>
          <description>
            <text>Configure an ME to monitor a static bidirectional co-routed transit LSP.</text>
          </description>
          <leaf name="lsp-name">
            <ext:operation-exclude value="update"/>
            <type name="leafref">
              <path value="/mpls:mpls/mpls-te:te/mpls-te-cr-static:bidirection-static-cr-lsps/mpls-te-cr-static:bidirection-static-cr-lsp/mpls-te-cr-static:lsp-name"/>
            </type>
            <must condition="/mpls:mpls/mpls-te:te/mpls-te-cr-static:bidirection-static-cr-lsps/mpls-te-cr-static:bidirection-static-cr-lsp[mpls-te-cr-static:lsp-name = current()]/mpls-te-cr-static:transit"/>
            <mandatory value="true"/>
            <description>
              <text>Configure the LSP name of an ME bound to a static bidirectional co-routed transit LSP.</text>
            </description>
          </leaf>
          <leaf name="mip-id">
            <ext:operation-exclude value="update"/>
            <type name="uint32">
              <range value="1..8191"/>
            </type>
            <description>
              <text>Configure the MIP ID of an ME.</text>
            </description>
          </leaf>
          <leaf name="me-index">
            <type name="uint32"/>
            <config value="false"/>
            <description>
              <text>ME index.</text>
            </description>
          </leaf>
          <leaf name="me-direction">
            <type name="me-direction"/>
            <config value="false"/>
            <description>
              <text>ME direction.</text>
            </description>
          </leaf>
          <leaf name="select-board-slot">
            <type name="string">
              <length value="1..19"/>
            </type>
            <config value="false"/>
            <description>
              <text>Board where the state machine resides.</text>
            </description>
          </leaf>
          <leaf name="me-status">
            <type name="me-state"/>
            <config value="false"/>
            <description>
              <text>The status of an ME.</text>
            </description>
          </leaf>
        </container>
        <container name="cc">
          <when condition="(../co-route-tunnel/tunnel-name) or (../pw/pw-vll/l2vpn-name) or (../pw/pw-vpls/vpls-peer-ip)"/>
          <description>
            <text>Configure the CC function.</text>
          </description>
          <leaf name="exp">
            <type name="uint8">
              <range value="0..7"/>
            </type>
            <default value="7"/>
            <description>
              <text>Configure the CCM exp.</text>
            </description>
          </leaf>
          <leaf name="interval">
            <type name="ccm-interval"/>
            <units name="ms"/>
            <must condition="(not (../interval='3.3ms') and not (../interval='10ms') and ../../test-instance/dual-lm/dlm-enable='true') or (../../test-instance/dual-lm/dlm-enable='false')"/>
            <must condition="(../interval='600000ms' and ../detect-mode='manu') or not (../interval='600000ms')"/>
            <default value="1000ms"/>
            <description>
              <text>Configure the CCM interval.</text>
            </description>
          </leaf>
          <leaf name="send-enable">
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable to configure the CC send enable flag.</text>
            </description>
          </leaf>
          <leaf name="receive-enable">
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable to configure the CC receive enable flag.</text>
            </description>
          </leaf>
          <leaf name="detect-mode">
            <type name="detect-mode"/>
            <must condition="(../detect-mode='auto' and ../receive-enable='true' and not (../interval='600000ms')) or ../detect-mode='manu'"/>
            <default value="manu"/>
            <description>
              <text>Configure the detect mode of CC.</text>
            </description>
          </leaf>
        </container>
        <container name="oam-lm">
