    "definitions": {
        "messages-config": {
            "description": "Configuration data for defining Syslog message severity.",
            "properties": {
                "severity": {
                    "type": "string",
                    "enum": [
                        "EMERGENCY",
                        "ALERT",
                        "CRITICAL",
                        "ERROR",
                        "WARNING",
                        "NOTICE",
                        "INFORMATIONAL",
                        "DEBUG"
                    ],
                    "description": "Specifies that only messages of the given severity (or\ngreater severity) are sent over the RPC.\n\nThis is analogous to differentiating which severity is to be\nsent to legacy Syslog servers, as opposed to local buffer or\nfiles."
                }
            }
        },
        "messages-state": {
            "description": "Operational state data for Syslog messages.",
            "properties": {
                "message": {
                    "description": "Syslog messages the client is Subscribing to. This is all\nmessages currently configured to be sent according to\nsyslog-severity.",
                    "properties": {
                        "msg": {
                            "type": "string",
                            "description": "Message payload. If other leafs within this container not\nsupported, this leaf MAY include the entire message,\ninclding pri, procid, app-name etc.."
                        },
                        "priority": {
                            "type": "string",
                            "description": "The Priority value (PRIVAL) represents both the\nFacility and Severity."
                        },
                        "app-name": {
                            "type": "string",
                            "description": "The APP-NAME field SHOULD identify the device or\napplication that originated the message."
                        },
                        "procid": {
                            "type": "string",
                            "description": "PROCID is a value that is included in the message, having\nno interoperable meaning, except that a change in the value\nindicates there has been a discontinuity in syslog\nreporting."
                        },
                        "msgid": {
                            "type": "string",
                            "description": "The MSGID SHOULD identify the type of message. For\nexample, a firewall might use the MSGID 'TCPIN' for\nincoming TCP traffic and the MSGID 'TCPOUT' for outgoing\nTCP traffic."
                        }
                    }
                }
            }
        },
        "debug-messages-config": {
            "description": "Configuration data for enabling debug messages.",
            "properties": {
                "service": {
                    "type": "string",
                    "description": "Enumeration of all services which can have debugging enabled.\nVendors are to augment this base identity with their platform\nor OS specific debug options."
                },
                "enabled": {
                    "type": "boolean",
                    "description": "Enable and disable debugging."
                }
            }
        },
        "debug-messages-top": {
            "description": "Configuration data for enabling Syslog debug messages.",
            "properties": {
                "debug-entries": {
                    "description": "Enclosing container for list of debugs to enable.",
                    "properties": {
                        "debug-service": {
                            "description": "List of debugging entries.",
                            "type": "array",
                            "items": {
                                "properties": {
                                    "service": {
                                        "type": "string",
                                        "x-path": "../config/service",
                                        "description": "Reference to the debug-enable service key."
                                    },
                                    "config": {
                                        "description": "Configuration data for debug service entries.",
                                        "$ref": "#/definitions/debug-messages-config"
                                    },
                                    "state": {
                                        "description": "Operational state data for enabled debugs.",
                                        "$ref": "#/definitions/debug-messages-config"
                                    }
                                }
                            },
                            "x-key": "service"
                        }
                    }
                }
            }
        },
        "messages-top": {
            "description": "Top-level grouping for Syslog messages.",
            "properties": {
                "messages": {
                    "description": "Top-level container for Syslog messages.",
                    "allOf": [
                        {
                            "$ref": "#/definitions/debug-messages-top"
                        },
                        {
                            "properties": {
                                "config": {
                                    "description": "Configuration data for Syslog messages.",
                                    "$ref": "#/definitions/messages-config"
                                },
                                "state": {
                                    "description": "Operational state data for a Syslog messages.",
                                    "$ref": "#/definitions/messages-state"
                                }
                            }
                        }
                    ]
                }
            }
        },
        "message_schema": {
            "description": "Syslog messages the client is Subscribing to. This is all\nmessages currently configured to be sent according to\nsyslog-severity.",
            "properties": {
                "msg": {
                    "type": "string",
                    "description": "Message payload. If other leafs within this container not\nsupported, this leaf MAY include the entire message,\ninclding pri, procid, app-name etc.."
                },
                "priority": {
                    "type": "string",
                    "description": "The Priority value (PRIVAL) represents both the\nFacility and Severity."
                },
                "app-name": {
                    "type": "string",
                    "description": "The APP-NAME field SHOULD identify the device or\napplication that originated the message."
                },
                "procid": {
                    "type": "string",
                    "description": "PROCID is a value that is included in the message, having\nno interoperable meaning, except that a change in the value\nindicates there has been a discontinuity in syslog\nreporting."
                },
                "msgid": {
                    "type": "string",
                    "description": "The MSGID SHOULD identify the type of message. For\nexample, a firewall might use the MSGID 'TCPIN' for\nincoming TCP traffic and the MSGID 'TCPOUT' for outgoing\nTCP traffic."
                }
            }
        },
        "debug-entries_schema": {
            "description": "Enclosing container for list of debugs to enable.",
            "properties": {
                "debug-service": {
                    "description": "List of debugging entries.",
                    "type": "array",
                    "items": {
                        "properties": {
                            "service": {
                                "type": "string",
                                "x-path": "../config/service",
                                "description": "Reference to the debug-enable service key."
                            },
                            "config": {
                                "description": "Configuration data for debug service entries.",
                                "$ref": "#/definitions/debug-messages-config"
