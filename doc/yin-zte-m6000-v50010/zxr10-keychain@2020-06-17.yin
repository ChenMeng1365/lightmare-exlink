  <typedef name="encryptAlgorithmType">
    <type name="enumeration">
      <enum name="3des">
        <value value="1"/>
        <description>
          <text>3DES algorithm</text>
        </description>
      </enum>
      <enum name="des">
        <value value="2"/>
        <description>
          <text>DES algorithm</text>
        </description>
      </enum>
      <enum name="aes-128">
        <value value="3"/>
        <description>
          <text>AES-128 algorithm</text>
        </description>
      </enum>
      <enum name="aes-192">
        <value value="4"/>
        <description>
          <text>AES-192 algorithm</text>
        </description>
      </enum>
      <enum name="aes-256">
        <value value="5"/>
        <description>
          <text>AES-256 algorithm</text>
        </description>
      </enum>
      <enum name="null">
        <value value="7"/>
        <description>
          <text>NULL algorithm</text>
        </description>
