<?xml version="1.0" encoding="UTF-8"?>
<module name="huawei-nd-notification"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:nd-notification="urn:huawei:yang:huawei-nd-notification"
        xmlns:pub-type="urn:huawei:yang:huawei-pub-type"
        xmlns:inet="urn:ietf:params:xml:ns:yang:ietf-inet-types"
        xmlns:ext="urn:huawei:yang:huawei-extension">
  <yang-version value="1"/>
  <namespace uri="urn:huawei:yang:huawei-nd-notification"/>
  <prefix value="nd-notification"/>
  <import module="huawei-pub-type">
    <prefix value="pub-type"/>
  </import>
  <import module="ietf-inet-types">
    <prefix value="inet"/>
  </import>
  <import module="huawei-extension">
    <prefix value="ext"/>
  </import>
  <organization>
    <text>Huawei Technologies Co., Ltd.</text>
  </organization>
  <contact>
    <text>Huawei Industrial Base
Bantian, Longgang
Shenzhen 518129
People's Republic of China
Website: http://www.huawei.com
Email: support@huawei.com</text>
  </contact>
  <description>
    <text>Notification of IPv6-nd-notification.</text>
  </description>
  <revision date="2020-05-28">
    <description>
      <text>The description of the source-ip leaf is modified.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <revision date="2020-04-17">
    <description>
      <text>Notification of anti-attack-rate-limit is added.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <revision date="2019-12-01">
    <description>
      <text>Initial revision.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <ext:task-name value="nd"/>
  <typedef name="nd-pkt-evt-disp-type">
    <type name="enumeration">
      <enum name="RS">
        <value value="1"/>
        <description>
          <text>Router solicitation packet.</text>
        </description>
      </enum>
      <enum name="RA">
        <value value="2"/>
        <description>
          <text>Router advertisement packet.</text>
        </description>
      </enum>
      <enum name="NS">
        <value value="3"/>
        <description>
          <text>Neighbor solicitation packet.</text>
        </description>
      </enum>
      <enum name="NA">
        <value value="4"/>
        <description>
          <text>Neighbor advertisement packet.</text>
        </description>
      </enum>
      <enum name="NDMISS">
        <value value="5"/>
        <description>
          <text>ND miss.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>ND packet type or event value.</text>
    </description>
  </typedef>
  <typedef name="nd-suppress-type">
    <type name="enumeration">
      <enum name="SRCIP">
        <value value="1"/>
        <description>
          <text>Source IP address.</text>
        </description>
      </enum>
      <enum name="SRCMAC">
        <value value="2"/>
        <description>
          <text>Source MAC address.</text>
        </description>
      </enum>
      <enum name="INTERFACE">
        <value value="3"/>
        <description>
          <text>Interface.</text>
        </description>
      </enum>
      <enum name="INTERFACESRCIP">
        <value value="4"/>
        <description>
          <text>Interface source IP address.</text>
        </description>
      </enum>
      <enum name="VSGLOBAL">
        <value value="5"/>
        <description>
          <text>Virtual system.</text>
        </description>
      </enum>
      <enum name="LSGLOBAL">
        <value value="6"/>
        <description>
          <text>Logic system.</text>
        </description>
      </enum>
      <enum name="DSTIP">
        <value value="7"/>
        <description>
          <text>Destination IP address.</text>
        </description>
      </enum>
      <enum name="TGTIP">
        <value value="8"/>
        <description>
          <text>Target IP address.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>ND packet type or event value.</text>
    </description>
  </typedef>
  <notification name="dad-not-finished">
    <description>
      <text>DAD is not finished.</text>
    </description>
    <leaf name="if-name">
      <type name="pub-type:if-name"/>
      <description>
        <text>Interface name.</text>
      </description>
    </leaf>
    <leaf name="dad-addr">
      <type name="inet:ipv6-address-no-zone"/>
      <description>
        <text>DAD address.</text>
      </description>
    </leaf>
    <leaf name="dad-timeout">
      <type name="uint32"/>
      <units name="s"/>
      <description>
        <text>Dad timeout.</text>
      </description>
    </leaf>
  </notification>
  <notification name="anti-attack-rate-limit">
    <description>
      <text>ND anti-attack rate limit.</text>
    </description>
    <leaf name="packet-type">
      <type name="nd-pkt-evt-disp-type"/>
      <mandatory value="true"/>
      <description>
        <text>Type of packets that are suppressed.</text>
      </description>
    </leaf>
    <leaf name="suppress-value">
      <type name="uint32"/>
      <units name="pps"/>
      <mandatory value="true"/>
      <description>
        <text>Rate limit of ND packets or ND miss messages.</text>
      </description>
    </leaf>
    <leaf name="current-value">
      <type name="uint32"/>
      <units name="pps"/>
      <mandatory value="true"/>
      <description>
        <text>Current rate of ND packets or ND miss messages.</text>
      </description>
    </leaf>
    <leaf name="suppress-type">
      <type name="nd-suppress-type"/>
      <mandatory value="true"/>
      <description>
        <text>Suppress type.</text>
      </description>
    </leaf>
    <leaf name="if-name">
      <type name="pub-type:if-name"/>
      <mandatory value="true"/>
      <description>
        <text>Interface name.</text>
      </description>
    </leaf>
    <leaf name="work-if-name">
      <type name="pub-type:if-name"/>
      <description>
        <text>Work interface name.</text>
      </description>
    </leaf>
    <leaf name="source-mac">
      <type name="pub-type:mac-address"/>
      <description>
        <text>Source MAC address.</text>
      </description>
    </leaf>
    <leaf name="destination-mac">
      <type name="pub-type:mac-address"/>
      <description>
        <text>Destination MAC address.</text>
      </description>
    </leaf>
    <leaf name="source-ip">
      <type name="inet:ipv6-address-no-zone"/>
      <description>
        <text>Source IP address, when the packet type is not NS or RS, the source address is invalid if it is an unspecified address.</text>
      </description>
    </leaf>
    <leaf name="destination-ip">
      <type name="inet:ipv6-address-no-zone"/>
      <description>
        <text>Destination IP address.</text>
      </description>
    </leaf>
    <leaf name="target-ip">
      <type name="inet:ipv6-address-no-zone"/>
      <description>
        <text>Target IP address.</text>
      </description>
    </leaf>
    <leaf name="pe-vlan">
      <type name="uint32"/>
      <description>
        <text>Outer VLAN ID.</text>
      </description>
    </leaf>
    <leaf name="ce-vlan">
      <type name="uint32"/>
      <description>
        <text>Inner VLAN ID.</text>
      </description>
    </leaf>
  </notification>
</module>
