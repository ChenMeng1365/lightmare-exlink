  <grouping name="evpn-ping-trace-result-type">
    <description>
      <text>Description of evpn-ping-trace-result-type.</text>
    </description>
    <container name="mac-ping-results">
      <config value="false"/>
      <description>
        <text>List of test result.</text>
      </description>
      <list name="mac-ping-result">
        <key value="test-name"/>
        <description>
          <text>Statistics of test result of a certain hop in the test path. For a ping-type test, there is only one test result.</text>
        </description>
        <leaf name="test-name">
          <type name="string">
            <length value="1..32"/>
          </type>
          <config value="false"/>
          <description>
            <text>Test name.</text>
          </description>
        </leaf>
        <leaf name="evpn-name">
          <type name="string">
            <length value="1..31"/>
          </type>
          <config value="false"/>
          <description>
            <text>EVPN instance name.</text>
          </description>
        </leaf>
        <leaf name="bd-id">
          <type name="uint32">
            <range value="1..16777215"/>
          </type>
          <config value="false"/>
          <description>
            <text>The ID of bridge-domain.</text>
          </description>
        </leaf>
        <leaf name="mac">
          <type name="pub-type:mac-address"/>
          <config value="false"/>
          <description>
            <text>Mac address.</text>
          </description>
        </leaf>
        <leaf name="packet-send">
          <type name="uint32"/>
          <config value="false"/>
          <description>
            <text>Total number of sent packets.</text>
          </description>
        </leaf>
        <leaf name="packet-receive">
          <type name="uint32"/>
          <config value="false"/>
          <description>
            <text>Total number of received packets.</text>
          </description>
        </leaf>
        <leaf name="loss-ratio">
          <type name="pub-type:percent">
            <range value="0..100"/>
          </type>
          <units name="%"/>
          <config value="false"/>
          <description>
            <text>Packet loss ratio of a test.</text>
          </description>
        </leaf>
        <leaf name="rtt-min">
          <type name="uint32"/>
          <units name="ms"/>
          <config value="false"/>
          <description>
            <text>Minimum RTT of all test packets.</text>
          </description>
        </leaf>
        <leaf name="rtt-max">
          <type name="uint32"/>
          <units name="ms"/>
          <config value="false"/>
          <description>
            <text>Maximum RTT of all test packets.</text>
          </description>
        </leaf>
        <leaf name="rtt-average">
          <type name="uint32"/>
          <units name="ms"/>
          <config value="false"/>
          <description>
            <text>Average RTT of all test packets.</text>
          </description>
        </leaf>
        <leaf name="status">
          <type name="dgntl-test-status-type"/>
          <config value="false"/>
          <description>
            <text>Test status.</text>
          </description>
        </leaf>
        <container name="probes">
          <config value="false"/>
          <description>
            <text>List of EVPN MAC ping results of all packets in a test.</text>
          </description>
          <list name="probe">
            <key value="sequence"/>
            <config value="false"/>
            <description>
              <text>Statistics of get the response result of a single packet in a EVPN MAC ping test.</text>
            </description>
            <leaf name="sequence">
              <type name="uint32"/>
              <config value="false"/>
              <description>
                <text>Serial number of a test packet received.</text>
              </description>
            </leaf>
            <leaf name="replier">
              <type name="inet:ip-address-no-zone"/>
              <config value="false"/>
              <description>
                <text>Reply address in a received response packet.</text>
              </description>
            </leaf>
            <leaf name="data-bytes">
              <type name="uint32"/>
              <config value="false"/>
              <description>
                <text>Number of bytes in a received response packet.</text>
              </description>
            </leaf>
            <leaf name="rtt">
              <type name="uint32"/>
              <units name="ms"/>
              <config value="false"/>
              <description>
                <text>RTT of test packets.</text>
              </description>
            </leaf>
            <leaf name="result-type">
              <type name="probe-result-type"/>
              <config value="false"/>
              <description>
                <text>Result of packet sending, which is either Success or Timeout.</text>
              </description>
            </leaf>
            <leaf name="is-delete">
              <ext:support-filter value="true"/>
              <type name="boolean"/>
              <config value="false"/>
              <description>
                <text>Enable/disable the function of deleting test results.</text>
              </description>
            </leaf>
          </list>
        </container>
      </list>
    </container>
