<?xml version="1.0" encoding="UTF-8"?>
<module name="huawei-bras-license"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:bras-license="urn:huawei:yang:huawei-bras-license"
        xmlns:ext="urn:huawei:yang:huawei-extension"
        xmlns:devm="urn:huawei:yang:huawei-devm"
        xmlns:lcs="urn:huawei:yang:huawei-license">
  <namespace uri="urn:huawei:yang:huawei-bras-license"/>
  <prefix value="bras-license"/>
  <import module="huawei-extension">
    <prefix value="ext"/>
  </import>
  <import module="huawei-devm">
    <prefix value="devm"/>
  </import>
  <import module="huawei-license">
    <prefix value="lcs"/>
  </import>
  <organization>
    <text>Huawei Technologies Co., Ltd.</text>
  </organization>
  <contact>
    <text>Huawei Industrial Base
Bantian, Longgang
Shenzhen 518129
People's Republic of China
Website: http://www.huawei.com
Email: support@huawei.com</text>
  </contact>
  <description>
    <text>Bras license.</text>
  </description>
  <revision date="2020-12-15">
    <description>
      <text>Initial revision.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <ext:task-name value="license"/>
  <augment target-node="/lcs:license">
    <description>
      <text>Bas license configurations.</text>
    </description>
    <container name="bras-license">
      <description>
        <text>Configure Bras license.</text>
      </description>
      <container name="active-bas-slots">
        <description>
          <text>List of activate BAS on a board.</text>
        </description>
        <list name="active-bas-slot">
          <key value="slot-id"/>
          <description>
            <text>Configure activate BAS on a board.</text>
          </description>
          <leaf name="slot-id">
            <type name="leafref">
              <path value="/devm:devm/devm:lpu-boards/devm:lpu-board/devm:position"/>
            </type>
            <description>
              <text>Specifies the slot ID of a board.</text>
            </description>
          </leaf>
        </list>
      </container>
    </container>
  </augment>
</module>
