  <container name="isis">
    <description>
      <text>Intermediate System to Intermediate System (ISIS) routes.</text>
    </description>
    <container name="global">
      <description>
        <text>Configure ISIS global config.</text>
      </description>
      <leaf name="lsp-seq-over-flag">
        <type name="boolean"/>
        <default value="true"/>
        <description>
          <text>Enable/disable to recover ISIS system ID automatically when the sequence number of the local lsp overflowed.</text>
        </description>
      </leaf>
      <leaf name="sys-id-conflict-flag">
        <type name="boolean"/>
        <default value="true"/>
        <description>
          <text>Enable/disable to recover ISIS system ID automatically when the system ID conflicted.</text>
        </description>
      </leaf>
      <leaf name="purge-lsp-protect-enable">
        <type name="boolean"/>
        <default value="true"/>
        <description>
          <text>Enable/disable automatic ISIS purge LSP protection.</text>
        </description>
      </leaf>
      <leaf name="purge-source-trace-enable">
        <type name="boolean"/>
        <default value="true"/>
        <description>
          <text>Enable/disable purge LSP source tracing.</text>
        </description>
      </leaf>
      <leaf name="purge-source-trace-port">
        <type name="uint32">
          <range value="1025..65535"/>
        </type>
        <default value="50121"/>
        <description>
          <text>Configure the port of purge LSP source tracing allocated by huawei-pp4.The related CLI is 'display ip port info | inc UDP'.</text>
        </description>
      </leaf>
      <leaf name="suppress-flap-intf-disable">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable the ISIS interface flapping suppression function.</text>
        </description>
      </leaf>
      <leaf name="intf-limit-enable">
        <type name="boolean"/>
        <default value="true"/>
        <description>
          <text>Enable/disable the limit of the number of ISIS interfaces.</text>
        </description>
      </leaf>
      <leaf name="cpu-overload-ctrl-enable">
        <type name="boolean"/>
        <default value="true"/>
        <description>
          <text>Enable/disable ISIS cpu-overload control.</text>
        </description>
      </leaf>
      <leaf name="memory-overload-ctrl-enable">
        <type name="boolean"/>
        <default value="true"/>
        <description>
          <text>Enable/disable ISIS memory-overload control.</text>
        </description>
      </leaf>
      <leaf name="memory-overload-exception-discard-lsp">
        <when condition="(../memory-overload-ctrl-enable='true')"/>
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable ISIS memory-overload exception-threshold discard new-lsp.</text>
        </description>
      </leaf>
      <leaf name="route-loop-detect-enable">
        <type name="boolean"/>
        <default value="true"/>
        <description>
          <text>Enable/disable ISIS route loop-detect control.</text>
        </description>
      </leaf>
      <container name="session-car">
        <must condition="cir&lt;=pir"/>
        <must condition="cbs&lt;=pbs"/>
        <description>
          <text>Configure global ISIS session CAR.</text>
        </description>
        <leaf name="switch-flag">
          <type name="switch-type"/>
          <default value="enable"/>
          <description>
            <text>Session CAR switch flag.</text>
          </description>
        </leaf>
        <leaf name="cir">
          <type name="uint32">
            <range value="0..1000000"/>
          </type>
          <units name="kbit/s"/>
          <default value="8"/>
          <description>
            <text>Session CAR CIR value.</text>
          </description>
        </leaf>
        <leaf name="cbs">
          <type name="uint32">
            <range value="0..1000000"/>
          </type>
          <units name="Byte"/>
          <default value="100000"/>
          <description>
            <text>Session CAR CBS value.</text>
          </description>
        </leaf>
        <leaf name="pir">
          <type name="uint32">
            <range value="0..1000000"/>
          </type>
          <units name="kbit/s"/>
          <default value="20000"/>
          <description>
            <text>Session CAR PIR value.</text>
          </description>
        </leaf>
        <leaf name="pbs">
          <type name="uint32">
            <range value="0..10000000"/>
          </type>
          <units name="Byte"/>
          <default value="9000000"/>
          <description>
            <text>Session CAR PBS value.</text>
          </description>
        </leaf>
      </container>
      <container name="micro-isolation-car">
        <must condition="cir&lt;=pir"/>
        <must condition="cbs&lt;=pbs"/>
        <description>
          <text>Configure global ISIS Micro Isolation CAR.</text>
        </description>
        <leaf name="switch-flag">
          <type name="switch-type"/>
          <default value="enable"/>
          <description>
            <text> Micro Isolation CAR switch flag.</text>
          </description>
        </leaf>
        <leaf name="cir">
          <type name="uint32">
            <range value="0..1000000"/>
          </type>
          <units name="kbit/s"/>
          <default value="8"/>
          <description>
            <text> Micro Isolation CAR CIR value.</text>
          </description>
        </leaf>
        <leaf name="cbs">
          <type name="uint32">
            <range value="0..1000000"/>
          </type>
          <units name="Byte"/>
          <default value="100000"/>
          <description>
            <text> Micro Isolation CAR CBS value.</text>
          </description>
        </leaf>
        <leaf name="pir">
          <type name="uint32">
            <range value="0..1000000"/>
          </type>
          <units name="kbit/s"/>
          <default value="20000"/>
          <description>
            <text> Micro Isolation CAR PIR value.</text>
          </description>
        </leaf>
        <leaf name="pbs">
          <type name="uint32">
            <range value="0..10000000"/>
          </type>
          <units name="Byte"/>
          <default value="9000000"/>
          <description>
            <text> Micro Isolation CAR PBS value.</text>
          </description>
        </leaf>
      </container>
    </container>
    <container name="global-stat">
      <config value="false"/>
      <description>
        <text>Statistics of ISIS global state.</text>
      </description>
      <leaf name="node-number">
        <type name="uint32">
          <range value="0..4294967295"/>
        </type>
        <description>
          <text>Support to get ISIS area public nodes.</text>
        </description>
      </leaf>
      <leaf name="peer-number">
        <type name="uint32">
          <range value="0..4294967295"/>
        </type>
        <description>
          <text>Support to get ISIS area public peers.</text>
        </description>
      </leaf>
    </container>
  </container>
  <rpc name="reset-isis-all">
    <ext:node-ref value="/ni:network-instance/ni:instances/ni:instance/isis:isis/isis:sites/isis:site"/>
    <description>
      <text>Reset ISIS Process.</text>
    </description>
    <input>
      <leaf name="process-id">
        <type name="uint32">
          <range value="1..4294967295"/>
        </type>
        <description>
          <text>Specify the ISIS process which needs to be reset.</text>
        </description>
      </leaf>
    </input>
  </rpc>
  <rpc name="reset-isis-peer">
    <ext:node-ref value="/ni:network-instance/ni:instances/ni:instance/isis:isis/isis:sites/isis:site/isis:peers/isis:peer"/>
    <description>
      <text>Reset an ISIS neighbor.</text>
    </description>
    <input>
      <leaf name="system-id">
        <type name="string">
          <length value="1..15"/>
          <pattern value="((([0-9]|[a-f]|[A-F]){4}\.){2}([0-9]|[a-f]|[A-F]){4})"/>
        </type>
        <mandatory value="true"/>
        <description>
          <text>Set the system ID of the neighbor. The duration for the system to keep the overload bit is set according to the status of the neighbor specified by the system ID. The length of the value is 6 bytes, in the format of XXXX.XXXX.XXXX, in which X is a hexadecimal case-insensitive character.</text>
        </description>
      </leaf>
      <leaf name="process-id">
        <type name="uint32">
          <range value="1..4294967295"/>
        </type>
        <description>
          <text>Specify the ISIS process which needs to be reset.</text>
        </description>
      </leaf>
    </input>
  </rpc>
  <rpc name="reset-suppress-flapping-peer">
    <ext:node-ref value="/ni:network-instance/ni:instances/ni:instance/isis:isis/isis:sites/isis:site/isis:circuits/isis:circuit/isis:suppress-flapping/isis:peer"/>
    <description>
      <text>Reset neighbor flapping suppression.</text>
    </description>
    <input>
      <leaf name="process-id">
        <type name="uint32">
          <range value="1..4294967295"/>
        </type>
        <mandatory value="true"/>
        <description>
          <text>Specify the ISIS process which needs to be reset.</text>
        </description>
      </leaf>
      <leaf name="interface">
        <type name="leafref">
          <path value="/ifm:ifm/ifm:interfaces/ifm:interface/ifm:name"/>
        </type>
        <description>
          <text>Set the interface name.</text>
        </description>
      </leaf>
      <leaf name="notify-peer">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Set whether the neighbor is notified.</text>
        </description>
      </leaf>
    </input>
  </rpc>
  <rpc name="clear-loop-detect-alarm-state">
    <description>
      <text>Clear ISIS route loop-detect alarm state.</text>
    </description>
  </rpc>
  <augment target-node="/ni:network-instance/ni:instances/ni:instance/l3vpn:afs/l3vpn:af">
    <description>
      <text>ISIS augment to L3VPN.</text>
    </description>
    <container name="isis-import-rts">
      <description>
        <text>List of import route information from another routing protocol.</text>
      </description>
      <list name="isis-import-rt">
        <key value="src-vrf process-id"/>
        <description>
          <text>Configure import route information from another routing protocol.</text>
        </description>
        <leaf name="src-vrf">
          <type name="leafref">
            <path value="/ni:network-instance/ni:instances/ni:instance/ni:name"/>
          </type>
          <must condition="../src-vrf!=../../../../../ni:name"/>
          <description>
            <text>VPN instance route information.</text>
          </description>
        </leaf>
        <leaf name="process-id">
          <type name="uint32">
            <range value="1..4294967295"/>
          </type>
          <description>
            <text>Process ID.</text>
          </description>
        </leaf>
        <leaf name="valid-route">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable the valid routes.</text>
          </description>
        </leaf>
        <choice name="policy-type">
          <description>
            <text>Type of import route rule.</text>
          </description>
          <case name="route-policy">
            <description>
              <text>Set a route policy name.</text>
            </description>
            <leaf name="route-policy">
              <type name="leafref">
                <path value="/rtp:routing-policy/rtp:policy-definitions/rtp:policy-definition/rtp:name"/>
              </type>
              <description>
                <text>Set a route policy name.</text>
              </description>
            </leaf>
          </case>
          <case name="route-filter">
            <description>
              <text>Set a route filter name.</text>
            </description>
            <leaf name="route-filter">
              <type name="leafref">
                <path value="/xpl:xpl/xpl:route-filters/xpl:route-filter/xpl:name"/>
              </type>
              <description>
                <text>Set a route filter name.</text>
              </description>
            </leaf>
          </case>
        </choice>
      </list>
    </container>
  </augment>
  <augment target-node="/ni:network-instance/ni:instances/ni:instance">
    <description>
      <text>ISIS augment to L3VPN.</text>
    </description>
    <container name="isis">
      <description>
        <text>Configure ISIS configurations.</text>
      </description>
      <container name="sites">
        <description>
          <text>List of ISIS instance.</text>
        </description>
        <list name="site">
          <must condition="../../../l3vpn:afs/l3vpn:af[l3vpn:type='ipv4-unicast'] or ../../../l3vpn:afs/l3vpn:af[l3vpn:type='ipv6-unicast']"/>
          <key value="id"/>
          <description>
            <text>Configure ISIS instance.</text>
          </description>
          <leaf name="id">
            <type name="uint32">
              <range value="1..4294967295"/>
            </type>
            <description>
              <text>Set the process ID. If the process ID does not exist, you can create a process. Otherwise, the system fails to create a process.</text>
            </description>
          </leaf>
          <leaf name="description">
            <type name="string">
              <length value="1..80"/>
              <pattern value="([^?]*)"/>
            </type>
            <description>
              <text>Set a description information text.</text>
            </description>
          </leaf>
          <leaf name="multi-iid">
            <type name="uint16">
              <range value="1..65535"/>
            </type>
            <description>
              <text>Set the multi instance ID. The leafs of support iid only include p2pEnable, cost, incrCost, peerMaxCostIntv and tilfaDisable on interface.</text>
            </description>
          </leaf>
          <leaf name="level">
            <type name="level-type"/>
            <default value="level-1-2"/>
            <description>
              <text>Set the level of a router.</text>
            </description>
          </leaf>
          <leaf name="shutdown">
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable ISIS.</text>
            </description>
          </leaf>
          <leaf name="cost-style">
            <type name="cost-style"/>
            <default value="narrow"/>
            <description>
              <text>Set the cost type for the ISIS router to receive and send routes.</text>
            </description>
          </leaf>
          <leaf name="relax-spf-limit">
            <when condition="(../cost-style='transition' or ../cost-style='ntransition')"/>
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable the device to receive the routes with a cost greater than 1023.</text>
            </description>
          </leaf>
          <leaf name="is-name">
            <type name="string">
              <length value="1..64"/>
            </type>
            <description>
              <text>Set a dynamic host name for the local system.</text>
            </description>
          </leaf>
          <leaf name="direct-inherit">
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable inherit the direct route property.</text>
            </description>
          </leaf>
          <leaf name="adv-one-intf-addr">
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable advertise one interface address.</text>
            </description>
          </leaf>
          <leaf name="bfd-session-check">
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable BFD session state check.</text>
            </description>
          </leaf>
          <container name="lsp-fragments-extend">
            <description>
              <text>Configure LSP fragment extension.</text>
            </description>
            <leaf name="enable-level1">
              <type name="boolean"/>
              <default value="false"/>
              <description>
                <text>Enable/disable the level 1 LSP fragment extension.</text>
              </description>
            </leaf>
            <leaf name="enable-level2">
              <type name="boolean"/>
              <default value="false"/>
              <description>
                <text>Enable/disable the level 2 LSP fragment extension.</text>
              </description>
            </leaf>
            <leaf name="mode">
              <when condition="(../enable-level1='true' or ../enable-level2='true')"/>
              <type name="lsp-frag-mode"/>
              <default value="mode-1"/>
              <description>
                <text>Indicates a mode to enable LSP fragment extension.</text>
              </description>
            </leaf>
          </container>
          <container name="poi-tlv">
            <description>
              <text>Configure purge originator identification TLV.</text>
            </description>
            <leaf name="enable">
              <type name="boolean"/>
              <default value="false"/>
              <description>
                <text>Enable/disable purge originator identification.</text>
              </description>
            </leaf>
            <leaf name="always">
              <when condition="../enable='true'"/>
              <type name="boolean"/>
              <default value="false"/>
              <description>
                <text>Enable/disable purge originator identification always with POI TLV.</text>
              </description>
            </leaf>
          </container>
          <container name="lsdb-limit">
            <description>
              <text>Configure LSDB Limit.</text>
            </description>
            <leaf name="enable">
              <type name="boolean"/>
              <default value="false"/>
              <description>
                <text>Enable/disable limit the number of the LSDBs.</text>
              </description>
            </leaf>
            <leaf name="max-number">
              <when condition="../enable='true'"/>
              <type name="uint32">
                <range value="1..100000"/>
              </type>
              <default value="100000"/>
              <description>
                <text>Maximum number of LSDBs.</text>
              </description>
            </leaf>
            <leaf name="threshold-upper">
              <when condition="../enable='true'"/>
              <type name="uint8">
                <range value="1..100"/>
              </type>
              <must condition="(../threshold-lower&lt;=../threshold-upper)"/>
              <default value="80"/>
              <description>
                <text>Threshold upper percentage of the maximum number of LSDBs, used to trigger an alarm.</text>
              </description>
            </leaf>
            <leaf name="threshold-lower">
              <when condition="../enable='true'"/>
              <type name="uint8">
                <range value="1..100"/>
              </type>
              <must condition="(../threshold-lower&lt;=../threshold-upper)"/>
              <default value="70"/>
              <description>
                <text>Threshold lower percentage of the maximum number of LSDBs, used to trigger a clear alarm.</text>
              </description>
            </leaf>
          </container>
          <container name="suppress-flapping">
            <description>
              <text>Configure flapping suppression.</text>
            </description>
            <container name="peer">
              <description>
                <text>Configure neighbor relationship flapping suppression.</text>
              </description>
              <leaf name="enable">
                <type name="boolean"/>
                <default value="true"/>
                <description>
                  <text>Enable/disable the neighbor relationship flapping suppression function.</text>
                </description>
              </leaf>
            </container>
            <container name="lsp-flood">
              <description>
                <text>Configure the delay timer for LSP flood.</text>
              </description>
              <choice name="lsp-flood">
                <default value="timer"/>
                <description>
                  <text>Set the delay timer for LSP flood.</text>
                </description>
                <case name="disable">
                  <description>
                    <text>Disable suppress flapping for LSP.</text>
                  </description>
                  <leaf name="disable">
                    <type name="empty"/>
                    <description>
                      <text>Disable suppress flapping for LSP.</text>
                    </description>
                  </leaf>
                </case>
                <case name="timer">
                  <description>
                    <text>Set the ISIS suppress flapping timer.</text>
                  </description>
                  <leaf name="interval">
                    <type name="uint32">
                      <range value="1..600"/>
                    </type>
                    <units name="s"/>
                    <default value="10"/>
                    <description>
                      <text>Set the maximum interval of delay timer.</text>
                    </description>
                  </leaf>
                  <leaf name="threshold">
                    <type name="uint16">
                      <range value="3..100"/>
                    </type>
                    <default value="30"/>
                    <description>
                      <text>Set the Threshold of LSPs to be suppressed.</text>
                    </description>
                  </leaf>
                </case>
              </choice>
            </container>
            <container name="route-calculate">
              <description>
                <text>Configure the delay timer for LSP process.</text>
              </description>
              <choice name="route-calculate">
                <default value="timer"/>
                <description>
                  <text>Set the delay timer for LSP process.</text>
                </description>
                <case name="disable">
                  <description>
                    <text>Disable suppress flapping for LSP.</text>
                  </description>
                  <leaf name="disable">
                    <type name="empty"/>
                    <description>
                      <text>Disable suppress flapping for LSP.</text>
                    </description>
                  </leaf>
                </case>
                <case name="timer">
                  <description>
                    <text>Set the ISIS suppress flapping timer.</text>
                  </description>
                  <leaf name="interval">
                    <type name="uint32">
                      <range value="1..600"/>
                    </type>
                    <units name="s"/>
                    <default value="10"/>
                    <description>
                      <text>Set the maximum interval of delay timer.</text>
                    </description>
                  </leaf>
                  <leaf name="threshold">
                    <type name="uint16">
                      <range value="3..100"/>
                    </type>
                    <default value="30"/>
                    <description>
                      <text>Set the Threshold of LSPs to be suppressed.</text>
                    </description>
                  </leaf>
                </case>
              </choice>
            </container>
            <container name="lsp-generation">
              <description>
                <text>Configure the delay timer for LSP generation.</text>
              </description>
              <choice name="lsp-generation">
                <default value="timer"/>
                <description>
                  <text>Set the delay timer for LSP generation.</text>
                </description>
                <case name="disable">
                  <description>
                    <text>Disable suppress flapping for LSP.</text>
                  </description>
                  <leaf name="disable">
                    <type name="empty"/>
                    <description>
                      <text>Disable suppress flapping for LSP.</text>
                    </description>
                  </leaf>
                </case>
                <case name="timer">
                  <description>
                    <text>Set the ISIS suppress flapping timer.</text>
                  </description>
                  <leaf name="interval">
                    <type name="uint32">
                      <range value="1..600"/>
                    </type>
                    <units name="s"/>
                    <default value="10"/>
                    <description>
                      <text>Set the maximum interval of delay timer.</text>
                    </description>
                  </leaf>
                  <leaf name="threshold">
                    <type name="uint16">
                      <range value="3..100"/>
                    </type>
                    <default value="30"/>
                    <description>
                      <text>Set the Threshold of LSPs to be suppressed.</text>
                    </description>
                  </leaf>
                </case>
              </choice>
            </container>
          </container>
          <container name="timer">
            <description>
              <text>Configure the timer of LSP.</text>
            </description>
            <leaf name="lsp-max">
              <type name="uint32">
                <range value="2..65535"/>
              </type>
              <units name="s"/>
              <must condition="(../lsp-max&gt;../lsp-refresh)"/>
              <default value="1200"/>
              <description>
                <text>Set the maximum validity period of the LSP generated by the current ISIS process.</text>
              </description>
            </leaf>
            <leaf name="lsp-refresh">
              <type name="uint32">
                <range value="1..65534"/>
              </type>
              <units name="s"/>
              <must condition="(../lsp-max&gt;../lsp-refresh)"/>
              <default value="900"/>
              <description>
                <text>Set the period for refreshing LSPs.</text>
              </description>
            </leaf>
            <leaf name="purge-lsp-delay">
              <type name="uint32">
                <range value="0..65535"/>
              </type>
              <units name="s"/>
              <default value="20"/>
              <description>
                <text>Set the delay time of routing calculation after receiving purge LSP.</text>
              </description>
            </leaf>
            <container name="spf">
              <description>
                <text>Configure time for route calculation.</text>
              </description>
              <leaf name="spf-type">
                <type name="spf-type"/>
                <must condition="../spf-type='intelligent-timer' or (../spf-type='no-incr-timer' and ../max and ../init) or (../spf-type='one-trigger-timer'and ../max)"/>
                <default value="intelligent-timer"/>
                <description>
                  <text>Set the SPF timer type for route calculation. When SPF type is intelligent-timer, max, incr, and init can be configured. When SPF type is set to no-incr-timer, max and init can be configured. When SPF type is set to one-trigger-timer, only max can be configured.</text>
                </description>
              </leaf>
              <leaf name="max">
                <type name="uint32">
                  <range value="1..120"/>
                </type>
                <units name="s"/>
                <ext:dynamic-default>
                  <ext:default-value value="5">
                    <when condition="../spf-type = 'intelligent-timer'"/>
                    <description>
                      <text>The default value is 5 when SPF type is intelligent-timer.</text>
                    </description>
                  </ext:default-value>
                </ext:dynamic-default>
                <description>
                  <text>Set the maximum delay time for route calculation.</text>
                </description>
              </leaf>
              <leaf name="init">
                <when condition="../spf-type!='one-trigger-timer'"/>
                <type name="uint32">
                  <range value="1..60000"/>
                </type>
                <units name="ms"/>
                <must condition="(../init&lt;../max*1000)"/>
                <ext:dynamic-default>
                  <ext:default-value value="50">
                    <when condition="../spf-type = 'intelligent-timer'"/>
                    <description>
                      <text>The default value is 50 when SPF type is intelligent-timer.</text>
                    </description>
                  </ext:default-value>
                </ext:dynamic-default>
                <description>
                  <text>Set the delay for the first route calculation.</text>
                </description>
              </leaf>
              <leaf name="incr">
                <when condition="../spf-type='intelligent-timer'"/>
                <type name="uint32">
                  <range value="1..60000"/>
                </type>
                <units name="ms"/>
                <must condition="(../incr&lt;../max*1000)"/>
                <default value="200"/>
                <description>
                  <text>Set the incremental delay between two route calculations. The delay doubles the previous one each time.</text>
                </description>
              </leaf>
            </container>
            <container name="lsp-generation">
              <description>
                <text>Configure an intelligent timer to generate LSPs.</text>
              </description>
              <leaf name="max-level1">
                <type name="uint32">
                  <range value="1..120"/>
                </type>
                <units name="s"/>
                <default value="2"/>
                <description>
                  <text>Set the maximum delay for generating level 1 LSPs with the same LSP ID.</text>
                </description>
              </leaf>
              <leaf name="max-level2">
                <type name="uint32">
                  <range value="1..120"/>
                </type>
                <default value="2"/>
                <description>
                  <text>Set the maximum delay for generating level 2 LSPs with the same LSP ID.</text>
                </description>
              </leaf>
              <leaf name="init-level1">
                <type name="uint32">
                  <range value="1..60000"/>
                </type>
                <must condition="(../init-level1&lt;../max-level1*1000)"/>
                <description>
                  <text>Set the delay for generating level 1 LSPs with the same LSP ID for the first time.</text>
                </description>
              </leaf>
              <leaf name="init-level2">
                <type name="uint32">
                  <range value="1..60000"/>
                </type>
                <must condition="(../init-level2&lt;../max-level2*1000)"/>
                <description>
                  <text>Set the delay for generating level 2 LSPs with the same LSP ID for the first time.</text>
                </description>
              </leaf>
              <leaf name="incr-level1">
                <when condition="../init-level1&gt;0"/>
                <type name="uint32">
                  <range value="1..60000"/>
                </type>
                <must condition="(../incr-level1&lt;../max-level1*1000)"/>
                <description>
                  <text>Set the incremental delay for generating two level 1 LSPs with the same LSP ID. The interval doubles the previous one each time.</text>
                </description>
              </leaf>
              <leaf name="incr-level2">
                <when condition="../init-level2&gt;0"/>
                <type name="uint32">
                  <range value="1..60000"/>
                </type>
                <must condition="(../incr-level2&lt;../max-level2*1000)"/>
                <description>
                  <text>Set the incremental delay for generating two level 2 LSPs with the same LSP ID. The interval doubles the previous one each time.</text>
                </description>
              </leaf>
            </container>
          </container>
          <container name="lsp-length">
            <description>
              <text>Configure LSPs length.</text>
            </description>
            <leaf name="originate-level1">
              <type name="uint32">
                <range value="512..16384"/>
              </type>
              <must condition="(../originate-level1&lt;=../receive) and ((../originate-level1=../originate-level2))"/>
              <default value="1497"/>
              <description>
                <text>Set the maximum size of the level 1 LSPs that are generated locally.</text>
              </description>
            </leaf>
            <leaf name="originate-level2">
              <type name="uint32">
                <range value="512..16384"/>
              </type>
              <must condition="(../originate-level2&lt;=../receive) and (../originate-level1=../originate-level2)"/>
              <default value="1497"/>
              <description>
                <text>Set the maximum size of the level 2 LSPs that are generated locally.</text>
              </description>
            </leaf>
            <leaf name="receive">
              <type name="uint32">
                <range value="512..16384"/>
              </type>
              <must condition="(../originate-level1&lt;=../receive) and (../originate-level2&lt;=../receive)"/>
              <default value="1497"/>
              <description>
                <text>Set the maximum size of the LSPs that can be received.</text>
              </description>
            </leaf>
          </container>
          <container name="set-overload">
            <description>
              <text>Configure overload.</text>
            </description>
            <leaf name="type">
              <type name="overload-type"/>
              <default value="no-set"/>
              <description>
                <text>Set the router to notify others that it is overloaded.</text>
              </description>
            </leaf>
            <leaf name="wait-type">
              <when condition="../type='on-startup'"/>
              <type name="wait-type"/>
              <default value="no-wait"/>
              <description>
                <text>Set the overload type.</text>
              </description>
            </leaf>
            <leaf name="nbr-system-id">
              <when condition="../wait-type='start-from-nbr'"/>
              <type name="string">
                <length value="1..15"/>
                <pattern value="((([0-9]|[a-f]|[A-F]){4}\.){2}([0-9]|[a-f]|[A-F]){4})"/>
              </type>
              <mandatory value="true"/>
              <description>
                <text>Set the system ID of the neighbor. The duration for the system to keep the overload bit is set according to the status of the neighbor specified by the system ID. The length of the value is 6 bytes, in the format of XXXX.XXXX.XXXX, in which X is a hexadecimal case-insensitive character.</text>
              </description>
            </leaf>
            <leaf name="timeout1">
              <when condition="../wait-type='start-from-nbr'"/>
              <type name="uint32">
                <range value="5..86400"/>
              </type>
              <units name="s"/>
              <default value="1200"/>
              <description>
                <text>If the specified neighbor does not become up before the neighbor holdtime expires, the holdtime of the system overload flag is the neighbor holdtime.</text>
              </description>
            </leaf>
            <leaf name="timeout2">
              <when condition="../type='on-startup'"/>
              <type name="uint32">
                <range value="5..86400"/>
              </type>
              <units name="s"/>
              <default value="600"/>
              <description>
                <text>If the specified neighbor becomes up before the neighbor holdtime expires, the system overload flag keeps the neighbor timeout period.</text>
              </description>
            </leaf>
            <leaf name="inter-level">
              <when condition="not(../type='no-set')"/>
              <type name="boolean"/>
              <default value="false"/>
              <description>
                <text>Enable/disable determine whether a device can advertise the IP address prefixes learned from the ISIS routes at different levels.</text>
              </description>
            </leaf>
            <leaf name="external">
              <when condition="not(../type='no-set')"/>
              <type name="boolean"/>
              <default value="false"/>
              <description>
                <text>Enable/disable determine whether a device can advertise the IP address prefixes learned from other protocols.</text>
              </description>
            </leaf>
            <leaf name="send-sa-bit">
              <when condition="../type='on-startup'"/>
              <type name="boolean"/>
              <default value="false"/>
              <description>
                <text>Enable/disable the SA bit in Hello packets.</text>
              </description>
            </leaf>
            <leaf name="sa-bit-time">
              <when condition="../send-sa-bit='true'"/>
              <type name="uint32">
                <range value="5..600"/>
              </type>
              <units name="s"/>
              <default value="30"/>
              <description>
                <text>The SA bit advertisement period is carried in Hello packet.</text>
              </description>
            </leaf>
            <leaf name="rt-delay-time">
              <when condition="../type='on-startup'"/>
              <type name="uint32">
                <range value="5..1000"/>
              </type>
              <units name="s"/>
              <must condition="../rt-delay-time&lt;=../timeout2"/>
              <description>
                <text>The route delay distribute period.</text>
              </description>
            </leaf>
            <leaf name="rt-max-metric">
              <when condition="../type='on-startup'"/>
              <type name="boolean"/>
              <default value="false"/>
              <description>
                <text>Enable/disable publish the metric of route to maximum.</text>
              </description>
            </leaf>
          </container>
          <container name="te-set-subtlv">
            <description>
              <text>Configure TE subtlv.</text>
            </description>
            <leaf name="bw-constraint">
              <type name="uint8">
                <range value="19..254"/>
              </type>
              <description>
                <text>Change the value of bandwidth constraints subtlv.</text>
              </description>
            </leaf>
            <leaf name="lo-multiplier">
              <type name="uint8">
                <range value="19..254"/>
              </type>
              <description>
                <text>Change the value of local overbooking multipliers subtlv.</text>
              </description>
            </leaf>
            <leaf name="unresv-bw-sub-pool">
              <type name="uint8">
                <range value="19..254"/>
              </type>
              <description>
                <text>Change the value of unreserved sub pool bandwidth subtlv.</text>
              </description>
            </leaf>
          </container>
          <container name="lsp-lifetime">
            <description>
              <text>Configure refresh the remaining lifetime field of a LSP.</text>
            </description>
            <leaf name="enable">
              <type name="boolean"/>
              <default value="true"/>
              <description>
                <text>Enable/disable refresh remaining lifetime field.</text>
              </description>
            </leaf>
            <choice name="refresh">
              <default value="max-age"/>
              <description>
                <text>Set remaining lifetime refresh value.</text>
              </description>
              <case name="max-age">
                <description>
                  <text>Use max age as remaining lifetime refresh value.</text>
                </description>
                <leaf name="max-age">
                  <when condition="../enable='true'"/>
                  <type name="empty"/>
                  <description>
                    <text>Use max age as remaining lifetime refresh value.</text>
                  </description>
                </leaf>
              </case>
              <case name="value">
                <description>
                  <text>Set remaining lifetime refresh value.</text>
                </description>
                <leaf name="value">
                  <when condition="../enable='true'"/>
                  <type name="uint16">
                    <range value="2..65535"/>
                  </type>
                  <must condition="../value&gt;=../../timer/lsp-max"/>
                  <description>
                    <text>Set remaining lifetime refresh value.</text>
                  </description>
                </leaf>
              </case>
            </choice>
          </container>
          <container name="bgp-ls">
            <when condition="../../../../ni:name='_public_'"/>
            <description>
              <text>Configure the BGP LS.</text>
            </description>
            <leaf name="enable-level1">
              <type name="boolean"/>
              <default value="false"/>
              <description>
                <text>Enable/disable Level 1 BGP LS.</text>
              </description>
            </leaf>
            <leaf name="enable-level2">
              <type name="boolean"/>
              <must condition="not(../ignore-level2-leak-level1='true' and ../enable-level2='true')"/>
              <default value="false"/>
              <description>
                <text>Enable/disable Level 2 BGP LS.</text>
              </description>
            </leaf>
            <leaf name="enable-ipv6-level1">
              <type name="boolean"/>
              <must condition="../enable-ipv6-level1='false' or ../../afs/af[type='afipv6']"/>
              <default value="false"/>
              <description>
                <text>Enable/disable IPv6 Level 1 BGP LS.</text>
              </description>
            </leaf>
            <leaf name="enable-ipv6-level2">
              <type name="boolean"/>
              <must condition="../enable-ipv6-level2='false' or ../../afs/af[type='afipv6']"/>
              <default value="false"/>
              <description>
                <text>Enable/disable IPv6 Level 2 BGP LS.</text>
              </description>
            </leaf>
            <leaf name="identifier">
              <type name="uint64">
                <range value="2..65535"/>
              </type>
              <description>
                <text>Set BGP LS identifier.</text>
              </description>
            </leaf>
            <leaf name="ignore-level2-leak-level1">
              <when condition="../enable-level1='true'"/>
              <type name="boolean"/>
              <must condition="not(../enable-level2='true' and ../ignore-level2-leak-level1='true')"/>
              <default value="false"/>
              <description>
                <text>Enable/disable the function of ignoring the routes that are leaked from the Level 2 to the Level 1 during IS-IS Level 1 topology information advertisement to BGP-LS.</text>
              </description>
            </leaf>
          </container>
          <container name="bier">
            <when condition="../../../../ni:name='_public_'"/>
            <description>
              <text>Configure the bier.</text>
            </description>
            <leaf name="enable">
              <type name="boolean"/>
              <must condition="../enable='false' or (../../cost-style='wide' or ../../cost-style='transition' or ../../cost-style='wtransition')"/>
              <default value="false"/>
              <description>
                <text>Enable/disable bier.</text>
              </description>
            </leaf>
          </container>
          <container name="intelligent-convergence">
            <when condition="../../../../ni:name='_public_'"/>
            <description>
              <text>Configure the intelligent convergence.</text>
            </description>
            <leaf name="enable">
              <type name="boolean"/>
              <default value="false"/>
              <description>
                <text>Enable/disable intelligent convergence.</text>
              </description>
            </leaf>
          </container>
          <container name="extern-ability">
            <description>
              <text>Configure expansibility.</text>
            </description>
            <leaf name="adj-strict-check">
              <type name="boolean"/>
              <default value="false"/>
              <description>
                <text>Enable/disable strict check on the neighbor.</text>
              </description>
            </leaf>
            <leaf name="ecmp-prefer">
              <type name="ecmp-prefer-type"/>
              <default value="none"/>
              <description>
                <text>Configure whether the routes with a TE tunnel interface as the outbound interface or the routes with an IPv4 interface as the outbound interface are preferentially selected to load balance ISIS traffic.</text>
              </description>
            </leaf>
            <leaf name="opt-checksum">
              <type name="boolean"/>
              <default value="false"/>
              <description>
                <text>Enable/disable option checksum.</text>
              </description>
            </leaf>
            <leaf name="virtual-cluster">
              <type name="boolean"/>
              <must condition="../virtual-cluster='false'  or ((../../bgp-ls/enable-level1='false' and ../../bgp-ls/enable-level2='false' and ../../bgp-ls/enable-ipv6-level1='false' and ../../bgp-ls/enable-ipv6-level2='false') and not(../../afs/af/traffic-eng) and not(../../lsp-auths) and not(../../circuits/circuit/hello-auths))"/>
              <default value="false"/>
              <description>
                <text>Enable/disable virtual cluster on the system. Conflict with segment routing MPLS and virtual access.</text>
              </description>
            </leaf>
            <leaf name="virtual-access">
              <type name="boolean"/>
              <must condition="../virtual-access='false' or ((../../cost-style='wide' or ../../cost-style='wtransition') and (../../bgp-ls/enable-level1='false' and ../../bgp-ls/enable-level2='false' and ../../bgp-ls/enable-ipv6-level1='false' and ../../bgp-ls/enable-ipv6-level2='false') and not(../../afs/af/traffic-eng) and not(../../lsp-auths) and not(../../circuits/circuit/hello-auths))"/>
              <default value="false"/>
              <description>
                <text>Enable/disable virtual access on the system. Conflict with segment routing MPLS and virtual cluster.</text>
              </description>
            </leaf>
          </container>
          <container name="local-mt">
            <when condition="../../../../ni:name='_public_'"/>
            <description>
              <text>Configure a local multicast topology.</text>
            </description>
            <leaf name="enable">
              <type name="boolean"/>
              <must condition="../enable='false' or ../../afs/af[type='afipv4']/traffic-eng/enable-level1='true' or ../../afs/af[type='afipv4']/traffic-eng/enable-level2='true'"/>
              <default value="false"/>
              <description>
                <text>Enable/disable local multicast topology.</text>
              </description>
            </leaf>
            <choice name="policy-type">
              <description>
                <text>Mode of the policy used for local multicast topology.</text>
              </description>
              <case name="basic-acl">
                <description>
                  <text>Apply basic ACL name or ACL number.</text>
                </description>
                <leaf name="name-or-num">
                  <type name="leafref">
                    <path value="/acl:acl/acl:groups/acl:group/acl:identity"/>
                  </type>
                  <must condition="/acl:acl/acl:groups/acl:group[acl:identity=current()]/acl:type='basic'"/>
                  <description>
                    <text>Apply basic ACL name or ACL number.</text>
                  </description>
                </leaf>
              </case>
              <case name="ip-prefix">
                <description>
                  <text>Set the name of an IP prefix list.</text>
                </description>
                <leaf name="ip-prefix">
                  <type name="leafref">
                    <path value="/rtp:routing-policy/rtp:ipv4-prefix-filters/rtp:ipv4-prefix-filter/rtp:name"/>
                  </type>
                  <description>
                    <text>Set the name of an IP prefix list.</text>
                  </description>
                </leaf>
              </case>
            </choice>
          </container>
          <container name="net-entitys">
            <description>
              <text>List of NET.</text>
            </description>
            <list name="net-entity">
              <key value="value"/>
              <max-elements value="3"/>
              <description>
                <text>Configure NET.</text>
              </description>
              <leaf name="value">
                <type name="string">
                  <length value="0..83"/>
                  <pattern value="\s*((([0-9a-f]\.?){2}){7,19}0\.?0)\s*"/>
                </type>
                <description>
                  <text>Set a NET. The NET is in the format of X...X.XXXX.XXXX.XXXX.00, in which X...X indicates the area address, the 12 Xs in the middle indicate the system ID, and the last 00 indicates the SEL. When configure multiple NET, must ensure that system IDs are the same.</text>
                </description>
              </leaf>
            </list>
          </container>
          <container name="virtual-systems">
            <when condition="../net-entitys/net-entity/value"/>
            <description>
              <text>List of virtual system ID.</text>
            </description>
            <list name="virtual-system">
              <key value="id"/>
              <max-elements value="99"/>
              <description>
                <text>Configure virtual system ID. Used to set the ISIS process. Without a system ID, extended LSPs are not generated.</text>
              </description>
              <leaf name="id">
                <type name="string">
                  <length value="12..19"/>
                  <pattern value="((([0-9]|[a-f]){4}\.){2}([0-9]|[a-f]){4})"/>
                </type>
                <description>
                  <text>Set a virtual system ID for an ISIS process in the format of XXXX.XXXX.XXXX.</text>
                </description>
              </leaf>
            </list>
          </container>
          <container name="lsp-auths">
            <description>
              <text>List of routing packet authentication.</text>
            </description>
            <list name="lsp-auth">
              <key value="cmd-type"/>
              <max-elements value="2"/>
              <description>
                <text>Configure routing packet authentication. Authenticate received routing packets (LSPs or SNPs) based on the specified authentication mode and password in an ISIS area or domain.</text>
              </description>
              <leaf name="cmd-type">
                <type name="auth-cmd-type"/>
                <description>
                  <text>Set the command type.</text>
                </description>
              </leaf>
              <leaf name="pw-type">
                <type name="auth-type"/>
                <mandatory value="true"/>
                <description>
                  <text>Set the authentication type.</text>
                </description>
              </leaf>
              <leaf name="simple">
                <when condition="../pw-type='simple'"/>
                <type name="pub-type:password"/>
                <mandatory value="true"/>
                <description>
                  <text>Set the simple authentication. The password is a string ranging from 1 to 16 characters for a unencrypted password and 24 to 128 characters for a encrypted password.</text>
                </description>
              </leaf>
              <leaf name="md5">
                <when condition="../pw-type='hmac-sha256' or ../pw-type='md5'"/>
                <type name="pub-type:password-extend"/>
                <mandatory value="true"/>
                <description>
                  <text>Set the encrypted authentication password.</text>
                </description>
              </leaf>
              <leaf name="key-chain">
                <when condition="../pw-type='keychain'"/>
                <type name="string">
                  <length value="1..47"/>
                  <pattern value="[^A-Z]+"/>
                </type>
                <ext:case-sensitivity value="lower-only"/>
                <mandatory value="true"/>
                <description>
                  <text>Keychain name.</text>
                </description>
              </leaf>
              <leaf name="service">
                <when condition="../pw-type='md5' or ../pw-type='simple'"/>
                <type name="service-type"/>
                <default value="osi"/>
                <description>
                  <text>Set the authentication service type.</text>
                </description>
              </leaf>
              <leaf name="usage">
                <type name="auth-usage-type"/>
                <default value="usage-default"/>
                <description>
                  <text>Set the authentication mode. The available options are as follows:
authentication-avoid: In this mode, the authentication information is not encapsulated for the generated SNPs, and the received SNPs are not checked. The authentication information is encapsulated for only the generated LSPs, and the received LSPs are checked.
send-only: In this mode, the authentication information is encapsulated for the generated LSPs and SNPs. Only the received LSPs are checked.
all-send-only: In this mode, the authentication information is encapsulated for the generated LSPs and SNPS. The received LSPs and SNPs, however, are not checked.</text>
                </description>
              </leaf>
              <leaf name="key-id">
                <when condition="../pw-type='hmac-sha256'"/>
                <type name="uint16">
                  <range value="0..65535"/>
                </type>
                <default value="0"/>
                <description>
                  <text>Set a value for key ID.</text>
                </description>
              </leaf>
            </list>
          </container>
          <container name="host-names">
            <description>
              <text>List of name mapping information.</text>
            </description>
            <list name="host-name">
              <key value="system-id"/>
              <max-elements value="500"/>
              <description>
                <text>Configure the ability to identify the host names in LSPs. On the local end, a mapping name (an alias) is configured for the ISIS system on the remote end. The mapping name is not advertised.</text>
              </description>
              <leaf name="system-id">
                <type name="string">
                  <length value="12..31"/>
                  <pattern value="((([0-9]|[a-f]){4}\.){2}([0-9]|[a-f]){4}(\.([0-9]|[a-f]){2})?)"/>
                </type>
                <ext:case-sensitivity value="lower-only"/>
                <description>
                  <text>ID of the ISIS system on the remote end or the pseudo node ID. The value of a pseudo node cannot be 0.</text>
                </description>
              </leaf>
              <leaf name="value">
                <type name="string">
                  <length value="1..64"/>
                  <pattern value=".*\S.*"/>
                </type>
                <mandatory value="true"/>
                <description>
                  <text>Name of the ISIS system on the remote end.</text>
                </description>
              </leaf>
            </list>
          </container>
          <container name="circuits">
            <description>
              <text>List of ISIS interface.</text>
            </description>
            <list name="circuit">
              <key value="name"/>
              <description>
                <text>Configure ISIS interface.</text>
              </description>
              <leaf name="name">
                <type name="leafref">
                  <path value="/ifm:ifm/ifm:interfaces/ifm:interface/ifm:name"/>
                </type>
                <must condition="../../../../../../ni:name=/ifm:ifm/ifm:interfaces/ifm:interface[ifm:name=current()]/ifm:vrf-name"/>
                <description>
                  <text>Interface name.</text>
                </description>
              </leaf>
              <leaf name="level">
                <type name="level-type"/>
                <default value="level-1-2"/>
                <description>
                  <text>Set a level for an interface.</text>
                </description>
              </leaf>
              <leaf name="p2p-enable">
                <type name="boolean"/>
                <default value="false"/>
                <description>
                  <text>Enable/disable emulate an interface as a P2P interface. This command takes effect only when the interface is of the broadcast type. Loopback is P2P interface.</text>
                </description>
              </leaf>
              <leaf name="hello-mode">
                <type name="pad-type"/>
                <default value="normal"/>
                <description>
                  <text>Set the hello packet padding type.</text>
                </description>
              </leaf>
              <leaf name="peer-hold-max">
                <type name="uint32">
                  <range value="100..1000000"/>
                </type>
                <units name="ms"/>
                <description>
                  <text>Config peer hold max cost interval.</text>
                </description>
              </leaf>
              <leaf name="silent-enable">
                <type name="boolean"/>
                <default value="false"/>
                <description>
                  <text>Enable/disable prevent an interface from receiving and sending ISIS packets.</text>
                </description>
              </leaf>
              <leaf name="purge-source-trace-block">
                <type name="boolean"/>
                <default value="false"/>
                <description>
                  <text>Enable/disable block purge LSP source tracing on this interface.</text>
                </description>
              </leaf>
              <leaf name="silent-cost">
                <when condition="../silent-enable='true'"/>
                <type name="boolean"/>
                <default value="false"/>
                <description>
                  <text>Enable/disable advertise the silent cost.</text>
                </description>
              </leaf>
              <leaf name="dis-name">
                <type name="string">
                  <length value="1..64"/>
                </type>
                <description>
                  <text>Set a name for the DIS on the LAN.</text>
                </description>
              </leaf>
              <container name="srlgs">
                <description>
                  <text>List of share risk link group.</text>
                </description>
                <list name="srlg">
                  <key value="group-id"/>
                  <description>
                    <text>Configure share risk link group.</text>
                  </description>
                  <leaf name="group-id">
                    <type name="uint32">
                      <range value="0..4294967295"/>
                    </type>
                    <description>
                      <text>Set the srlg group ID.</text>
                    </description>
                  </leaf>
                </list>
              </container>
              <container name="mesh-group">
                <description>
                  <text>Configure mesh group state on an interface.</text>
                </description>
                <leaf name="state">
                  <type name="mesh-group-state"/>
                  <default value="inactive"/>
                  <description>
                    <text>If the state is block, the interface cannot flood received LSPs.</text>
                  </description>
                </leaf>
                <leaf name="number">
                  <when condition="../state='set'"/>
                  <type name="uint32">
                    <range value="1..4294967295"/>
                  </type>
                  <mandatory value="true"/>
                  <description>
                    <text>Set an ID for a mesh group.</text>
                  </description>
                </leaf>
              </container>
              <container name="suppress-flapping">
                <description>
                  <text>Configure flapping suppression.</text>
                </description>
                <container name="peer">
                  <description>
                    <text>Configure neighbor relationship flapping suppression.</text>
                  </description>
                  <leaf name="enable">
                    <type name="boolean"/>
                    <default value="true"/>
                    <description>
                      <text>Enable/disable the neighbor relationship flapping suppression function.</text>
                    </description>
                  </leaf>
                  <leaf name="detect-interval">
                    <type name="uint32">
                      <range value="1..300"/>
                    </type>
                    <units name="s"/>
                    <must condition="../resume-interval&gt;../detect-interval"/>
                    <default value="60"/>
                    <description>
                      <text>Set the detection interval of neighbor relationship flapping suppression. If the neighbor relationship goes down twice within the interval, a flapping event is recorded.</text>
                    </description>
                  </leaf>
                  <leaf name="threshold">
                    <type name="uint32">
                      <range value="1..1000"/>
                    </type>
                    <default value="10"/>
                    <description>
                      <text>Set the threshold of ISIS neighbor relationship flapping suppression. When the flapping count reaches or exceeds the threshold, flapping suppression takes effect.</text>
                    </description>
                  </leaf>
                  <leaf name="resume-interval">
                    <type name="uint32">
                      <range value="2..1000"/>
                    </type>
                    <units name="s"/>
                    <must condition="../resume-interval&gt;../detect-interval"/>
                    <default value="120"/>
                    <description>
                      <text>Set the interval for exiting from ISIS neighbor relationship flapping suppression. If the interval between successive neighbor down events is greater than resume-interval, the flapping count is reset. In hold max cost mode, if the neighbor relationship does not go down within two successive resume intervals or the interval between successive neighbor Down events is greater than resume interval, the interface exits from hold max cost mode.</text>
                    </description>
                  </leaf>
                  <leaf name="hold-down">
                    <type name="uint32">
                      <range value="1..600"/>
                    </type>
                    <units name="s"/>
                    <description>
                      <text>Set the duration of the hold down mode.</text>
                    </description>
                  </leaf>
                  <leaf name="hold-max-cost">
                    <type name="boolean"/>
                    <default value="true"/>
                    <description>
                      <text>Enable/disable hold max cost mode.</text>
                    </description>
                  </leaf>
                </container>
              </container>
              <container name="timer">
                <description>
                  <text>Configure timer.</text>
                </description>
                <leaf name="csnp-interval-level1">
                  <type name="uint32">
                    <range value="1..65535"/>
                  </type>
                  <units name="s"/>
                  <default value="10"/>
                  <description>
                    <text>Set the interval at which level 1 CSNPs are sent.</text>
                  </description>
                </leaf>
                <leaf name="csnp-interval-level2">
                  <type name="uint32">
                    <range value="1..65535"/>
                  </type>
                  <units name="s"/>
                  <default value="10"/>
                  <description>
                    <text>Set the interval at which level 2 CSNPs are sent.</text>
                  </description>
                </leaf>
                <leaf name="throttle-interval">
                  <type name="uint32">
                    <range value="1..10000"/>
                  </type>
                  <units name="ms"/>
                  <default value="50"/>
                  <description>
                    <text>Set the minimum interval at which LSPs are sent.</text>
                  </description>
                </leaf>
                <leaf name="throttle-count">
                  <type name="uint32">
                    <range value="1..1000"/>
                  </type>
                  <default value="10"/>
                  <description>
                    <text>Set the maximum number of LSPs that are sent during the interval.</text>
                  </description>
                </leaf>
              </container>
              <container name="broadcast">
                <when condition="../p2p-enable='false'"/>
                <description>
                  <text>Configure broadcast.</text>
                </description>
                <leaf name="dis-priority-level1">
                  <type name="uint32">
                    <range value="0..127"/>
                  </type>
                  <default value="64"/>
                  <description>
                    <text>Set a priority for level 1 DIS election. The greater the value, the higher the priority.</text>
                  </description>
                </leaf>
                <leaf name="dis-priority-level2">
                  <type name="uint32">
                    <range value="0..127"/>
                  </type>
                  <default value="64"/>
                  <description>
                    <text>Set a priority for level 2 DIS election. The greater the value, the higher the priority.</text>
                  </description>
                </leaf>
                <leaf name="hello-timer-level1">
                  <type name="uint32">
                    <range value="1..255"/>
                  </type>
                  <units name="s"/>
                  <default value="10"/>
                  <description>
                    <text>Set the interval at which level 1 hello packets are sent.</text>
                  </description>
                </leaf>
                <leaf name="hello-timer-level2">
                  <type name="uint32">
                    <range value="1..255"/>
                  </type>
                  <units name="s"/>
                  <default value="10"/>
                  <description>
                    <text>To set the interval for sending level 2 hello packets.</text>
                  </description>
                </leaf>
                <leaf name="hold-multiplier-level1">
                  <type name="uint32">
                    <range value="3..1000"/>
                  </type>
                  <default value="3"/>
                  <description>
                    <text>Set the number of hello packets that the local ISIS router fails to receive from a level 1 neighbor before the local router declares the neighbor down.</text>
                  </description>
                </leaf>
                <leaf name="hold-multiplier-level2">
                  <type name="uint32">
                    <range value="3..1000"/>
                  </type>
                  <default value="3"/>
                  <description>
                    <text>Set the number of hello packets that the local ISIS router fails to receive from a level 2 neighbor before the local router declares the neighbor down.</text>
                  </description>
                </leaf>
                <leaf name="conser-level1">
                  <type name="boolean"/>
                  <default value="false"/>
                  <description>
                    <text>Enable/disable level1 hello interval conservative flag.</text>
                  </description>
                </leaf>
                <leaf name="conser-level2">
                  <type name="boolean"/>
                  <default value="false"/>
                  <description>
                    <text>Enable/disable level2 hello interval conservative flag.</text>
                  </description>
                </leaf>
              </container>
              <container name="p2p">
                <when condition="../p2p-enable='true'"/>
                <description>
                  <text>Configure P2P.</text>
                </description>
                <leaf name="snpa-check">
                  <type name="boolean"/>
                  <default value="false"/>
                  <description>
                    <text>Enable/disable the SNPA check of LSP and SNP.</text>
                  </description>
                </leaf>
                <leaf name="peer-ip-ignore">
                  <when condition="/ifm:ifm/ifm:interfaces/ifm:interface[ifm:name=current()/../../isis:name]/ifm:type='Pos'"/>
                  <type name="boolean"/>
                  <default value="false"/>
                  <description>
                    <text>Enable/disable ignore the IP address check when P2P hello packets are received.</text>
                  </description>
                </leaf>
                <leaf name="ppp-negotiation">
                  <type name="p2p-negotiation"/>
                  <default value="3-way"/>
                  <description>
                    <text>Set the P2P neighbor negotiation type.</text>
                  </description>
                </leaf>
                <leaf name="hello-timer">
                  <type name="uint32">
                    <range value="1..255"/>
                  </type>
                  <units name="s"/>
                  <default value="10"/>
                  <description>
                    <text>Set the interval at which P2P hello packets are sent.</text>
                  </description>
                </leaf>
                <leaf name="hold-multiplier">
                  <type name="uint32">
                    <range value="3..1000"/>
                  </type>
                  <default value="3"/>
                  <description>
                    <text>Set the number of P2P hello packets that the local ISIS router fails to receive from a P2P neighbor before the local router declares the neighbor down.</text>
                  </description>
                </leaf>
                <leaf name="lsp-retransmit-interval">
                  <type name="uint32">
                    <range value="1..300"/>
                  </type>
                  <units name="s"/>
                  <default value="5"/>
                  <description>
                    <text>Set the interval for retransmitting LSPs on the P2P link.</text>
                  </description>
                </leaf>
                <leaf name="ppp-osicp-check">
                  <type name="boolean"/>
                  <default value="false"/>
                  <description>
                    <text>Enable/disable check the OSICP negotiation status on the interfaces with PPP link protocols. The negotiation status affects the status of the interfaces in ISIS.</text>
                  </description>
                </leaf>
                <leaf name="conser">
                  <type name="boolean"/>
                  <default value="false"/>
                  <description>
                    <text>Enable/disable p2p hello interval conservative flag.</text>
                  </description>
                </leaf>
              </container>
              <container name="hello-auths">
                <description>
                  <text>List of hello packet authentication.</text>
                </description>
                <list name="hello-auth">
                  <key value="level"/>
                  <max-elements value="2"/>
                  <description>
                    <text>Configure hello packet authentication.</text>
                  </description>
                  <leaf name="level">
                    <type name="level-type-two"/>
                    <description>
                      <text>Set the level type for authentication. This field is not required for P2P interfaces. Display may according to the interface level for P2P.</text>
                    </description>
                  </leaf>
                  <leaf name="pw-type">
                    <type name="auth-type"/>
                    <mandatory value="true"/>
                    <description>
                      <text>Set the authentication password type of hello packets.</text>
                    </description>
                  </leaf>
                  <leaf name="simple">
                    <when condition="../pw-type='simple'"/>
                    <type name="pub-type:password"/>
                    <mandatory value="true"/>
                    <description>
                      <text>Set the authentication password. The password is a string ranging from 1 to 16 characters for a unencrypted password and 24 to 128 characters for a encrypted password.</text>
                    </description>
                  </leaf>
                  <leaf name="key-chain">
                    <when condition="../pw-type='keychain'"/>
                    <type name="string">
                      <length value="1..47"/>
                      <pattern value="[^A-Z]+"/>
                    </type>
                    <ext:case-sensitivity value="lower-only"/>
                    <mandatory value="true"/>
                    <description>
                      <text>Keychain name.</text>
                    </description>
                  </leaf>
                  <leaf name="md5">
                    <when condition="../pw-type='hmac-sha256' or ../pw-type='md5'"/>
                    <type name="pub-type:password-extend"/>
                    <mandatory value="true"/>
                    <description>
                      <text>Set the encrypted authentication password.</text>
                    </description>
                  </leaf>
                  <leaf name="service">
                    <when condition="../pw-type='md5' or ../pw-type='simple'"/>
                    <type name="service-type"/>
                    <default value="osi"/>
                    <description>
                      <text>Set the authentication service type.</text>
                    </description>
                  </leaf>
                  <leaf name="send-only">
                    <type name="boolean"/>
                    <default value="false"/>
                    <description>
                      <text>Enable/disable encapsulate the authentication information in only the hello packets to be sent.</text>
                    </description>
                  </leaf>
                  <leaf name="key-id">
                    <when condition="../pw-type='hmac-sha256'"/>
                    <type name="uint16">
                      <range value="0..65535"/>
                    </type>
                    <default value="0"/>
                    <description>
                      <text>Set a value for key ID.</text>
                    </description>
                  </leaf>
                </list>
              </container>
              <container name="afs">
                <description>
                  <text>List of interface address family of IPv4 and IPv6.</text>
                </description>
                <list name="af">
                  <key value="type"/>
                  <min-elements value="1"/>
                  <max-elements value="2"/>
                  <description>
                    <text>Configure interface address family of IPv4 and IPv6.</text>
                  </description>
                  <leaf name="type">
                    <type name="af-type"/>
                    <must condition="../../../../../afs/af[isis:type=current()]"/>
                    <must condition="(../type='afipv4') or (../type='afipv6' and /ifm:ifm/ifm:interfaces/ifm:interface[ifm:name=current()/../../../isis:name]/ip:ipv6)"/>
                    <description>
                      <text>Address family type of the multi topology.</text>
                    </description>
                  </leaf>
                  <leaf name="cost-level1">
                    <type name="uint32">
                      <range value="1..16777215"/>
                    </type>
                    <must condition="(../cost-level1&lt;=63) or (not(../../../../../cost-style!='wide' and ../../../../../cost-style!='wtransition') and ../cost-level1&lt;=16777215)"/>
                    <description>
                      <text>Link cost of an interface used for the level 1 SPF calculation.</text>
                    </description>
                  </leaf>
                  <leaf name="cost-level2">
                    <type name="uint32">
                      <range value="1..16777215"/>
                    </type>
                    <must condition="(../cost-level2&lt;=63) or (not(../../../../../cost-style!='wide' and ../../../../../cost-style!='wtransition') and ../cost-level2&lt;=16777215)"/>
                    <description>
                      <text>Link cost of an interface used for the level 2 SPF calculation.</text>
                    </description>
                  </leaf>
                  <leaf name="tag-level1">
                    <type name="uint32">
                      <range value="0..4294967295"/>
                    </type>
                    <default value="0"/>
                    <description>
                      <text>Set the level 1 tag value for direct routes on the interface. A value of 0 indicates disable.</text>
                    </description>
                  </leaf>
                  <leaf name="tag-level2">
                    <type name="uint32">
                      <range value="0..4294967295"/>
                    </type>
                    <default value="0"/>
                    <description>
                      <text>Set the level 2 tag value for direct routes on the interface. A value of 0 indicates disable.</text>
                    </description>
                  </leaf>
                  <leaf name="suppress-reach">
                    <type name="level-type-zero"/>
                    <default value="no-set"/>
                    <description>
                      <text>Suppress the advertisement of direct route.</text>
                    </description>
                  </leaf>
                  <leaf name="incr-cost">
                    <type name="uint32">
                      <range value="1..16777214"/>
                    </type>
                    <description>
                      <text>Set the cost to be added if the link quality is low.</text>
                    </description>
                  </leaf>
                  <leaf name="bfd-bit-error">
                    <type name="bfd-bit-err-type"/>
                    <must condition="../bfd-bit-error='no-set' or ../type='afipv4'"/>
                    <default value="no-set"/>
                    <description>
                      <text>Enable or block BFD bit error on an interface. Not support Loopback and tunnel.</text>
                    </description>
                  </leaf>
                  <container name="mpls-ldp">
                    <description>
                      <text>Configure MPLS LDP configuration.</text>
                    </description>
                    <leaf name="auto-flag-block">
                      <when condition="../../type='afipv4'"/>
                      <type name="boolean"/>
                      <default value="false"/>
                      <description>
                        <text>Enable/disable automatic MPLS LDP configuration on an interface.</text>
                      </description>
                    </leaf>
                  </container>
                  <container name="frr">
                    <description>
                      <text>Configure ISIS FRR on interface.</text>
                    </description>
                    <leaf name="lfa-block-level1">
                      <type name="boolean"/>
                      <default value="false"/>
                      <description>
                        <text>Enable/disable prevent an interface from participating Level 1 LFA backup interface calculation in an IPv4 base topology.</text>
                      </description>
                    </leaf>
                    <leaf name="lfa-block-level2">
                      <type name="boolean"/>
                      <default value="false"/>
                      <description>
                        <text>Enable/disable prevent an interface from participating Level 2 LFA backup interface calculation in an IPv4 base topology.</text>
                      </description>
                    </leaf>
                    <leaf name="rlfa-disable-level1">
                      <type name="boolean"/>
                      <must condition="../rlfa-disable-level1='false' or ../../type='afipv4'"/>
                      <default value="false"/>
                      <description>
                        <text>Enable/disable level 1 RLFA of the interface.</text>
                      </description>
                    </leaf>
                    <leaf name="rlfa-disable-level2">
                      <type name="boolean"/>
                      <must condition="../rlfa-disable-level2='false' or ../../type='afipv4'"/>
                      <default value="false"/>
                      <description>
                        <text>Enable/disable level 2 RLFA of the interface.</text>
                      </description>
                    </leaf>
                    <leaf name="tilfa-disable-level1">
                      <type name="boolean"/>
                      <must condition="../tilfa-disable-level1='false' or (../../type='afipv4' or ../../../../../../../../../ni:name='_public_')"/>
                      <default value="false"/>
                      <description>
                        <text>Enable/disable an interface is prevented from being calculated as a valid backup interface during level 1 LFA calculation.</text>
                      </description>
                    </leaf>
                    <leaf name="tilfa-disable-level2">
                      <type name="boolean"/>
                      <must condition="../tilfa-disable-level2='false' or (../../type='afipv4' or ../../../../../../../../../ni:name='_public_')"/>
                      <default value="false"/>
                      <description>
                        <text>Enable/disable an interface is prevented from being calculated as a valid backup interface during level 2 LFA calculation.</text>
                      </description>
                    </leaf>
                  </container>
                  <container name="bfd">
                    <description>
                      <text>Configure ISIS BFD parameters on interface. Not support Loopback and tunnel.</text>
                    </description>
                    <choice name="type">
                      <description>
                        <text>Set BFD type.</text>
                      </description>
                      <case name="enable">
                        <description>
                          <text>Type is enable.</text>
                        </description>
                        <leaf name="enable">
                          <type name="empty"/>
                          <description>
                            <text>Enable dynamic BFD on an interface.</text>
                          </description>
                        </leaf>
                      </case>
                      <case name="static">
                        <description>
                          <text>Type is static.</text>
                        </description>
                        <leaf name="static">
                          <type name="empty"/>
                          <description>
                            <text>Enable static BFD on an interface.</text>
                          </description>
                        </leaf>
                      </case>
                      <case name="block">
                        <description>
                          <text>Type is block.</text>
                        </description>
                        <leaf name="block">
                          <type name="empty"/>
                          <description>
                            <text>Disable dynamic BFD from an interface.</text>
                          </description>
                        </leaf>
                      </case>
                    </choice>
                    <leaf name="min-rx">
                      <type name="uint32">
                        <range value="3..20000"/>
                      </type>
                      <units name="ms"/>
                      <default value="10"/>
                      <description>
                        <text>Set the minimum interval at which BFD packets are received.</text>
                      </description>
                    </leaf>
                    <leaf name="min-tx">
                      <type name="uint32">
                        <range value="3..20000"/>
                      </type>
                      <units name="ms"/>
                      <default value="10"/>
                      <description>
                        <text>Set the minimum interval at which BFD packets are sent.</text>
                      </description>
                    </leaf>
                    <leaf name="detect-multiplier">
                      <type name="uint32">
                        <range value="3..50"/>
                      </type>
                      <default value="3"/>
                      <description>
                        <text>Set the detection multiplier of a BFD session.</text>
                      </description>
                    </leaf>
                    <leaf name="tos-exp">
                      <type name="uint8">
                        <range value="0..7"/>
                      </type>
                      <description>
                        <text>Set the priority of BFD packets.</text>
                      </description>
                    </leaf>
                    <leaf name="frr-binding">
                      <type name="boolean"/>
                      <default value="false"/>
                      <description>
                        <text>Enable/disable bind the BFD status to the link status of an interface.</text>
                      </description>
                    </leaf>
                    <choice name="bfd-incr-cost">
                      <description>
                        <text>Set the BFD increasing cost value for the interface.</text>
                      </description>
                      <case name="value">
                        <description>
                          <text>Set the BFD increasing cost value for the interface.</text>
                        </description>
                        <leaf name="value">
                          <type name="uint32">
                            <range value="1..16777214"/>
                          </type>
                          <description>
                            <text>Set the BFD increasing cost value for the interface.</text>
                          </description>
                        </leaf>
                      </case>
                      <case name="cost-block">
                        <description>
                          <text>Enable/disable block the BFD sync cost for the interface.</text>
                        </description>
                        <leaf name="cost-block">
                          <type name="empty"/>
                          <description>
                            <text>Enable/disable block the BFD sync cost for the interface.</text>
                          </description>
                        </leaf>
                      </case>
                    </choice>
                  </container>
                  <container name="ldp-sync">
                    <when condition="../type='afipv4'"/>
                    <description>
                      <text>Configure LDP and ISIS synchronization state on an interface.</text>
                    </description>
                    <leaf name="enable">
                      <type name="ldp-sync-state"/>
                      <default value="none"/>
                      <description>
                        <text>LDP and ISIS synchronization state on an interface.</text>
                      </description>
                    </leaf>
                    <leaf name="hold-down-timer">
                      <type name="uint32">
                        <range value="0..65535"/>
                      </type>
                      <units name="s"/>
                      <description>
                        <text>Set a value for the LDP and ISIS synchronization holddown timer.</text>
                      </description>
                    </leaf>
                    <choice name="hold-cost-max">
                      <description>
                        <text>Set a value for the LDP and ISIS synchronization timer.</text>
                      </description>
                      <case name="timer">
                        <description>
                          <text>Set a value for the LDP and ISIS synchronization timer.</text>
                        </description>
                        <leaf name="timer">
                          <type name="uint32">
                            <range value="0..65535"/>
                          </type>
                          <units name="s"/>
                          <description>
                            <text>Set a value for the LDP and ISIS synchronization timer.</text>
                          </description>
                        </leaf>
                      </case>
                      <case name="infinite">
                        <description>
                          <text>Enable infinite LDP and ISIS synchronization.</text>
                        </description>
                        <leaf name="infinite">
                          <type name="empty"/>
                          <description>
                            <text>Enable infinite LDP and ISIS synchronization. The LDP and ISIS synchronization is kept before the LDP session goes up.</text>
                          </description>
                        </leaf>
                      </case>
                    </choice>
                  </container>
                  <container name="link-group-ports">
                    <description>
                      <text>List of link-group interface.</text>
                    </description>
                    <list name="link-group-port">
                      <key value="level-type"/>
                      <description>
                        <text>Configure link-group interface.</text>
                      </description>
                      <leaf name="level-type">
                        <type name="level-type-two"/>
                        <description>
                          <text>Level type.</text>
                        </description>
                      </leaf>
                      <leaf name="name">
                        <ext:operation-exclude value="update"/>
                        <type name="leafref">
                          <path value="/ni:network-instance/ni:instances/ni:instance/isis:isis/isis:sites/isis:site/isis:link-groups/isis:link-group/isis:name"/>
                        </type>
                        <must condition="not(../../link-group-port[name!=current()])"/>
                        <mandatory value="true"/>
                        <description>
                          <text>To set link-group name.</text>
                        </description>
                      </leaf>
                    </list>
                  </container>
                  <container name="topologys">
                    <when condition="../../../../../../../../ni:name='_public_'"/>
                    <description>
                      <text>List of interface multi topology.</text>
                    </description>
                    <list name="topology">
                      <must condition="((../../isis:type='afipv4') and (../../../../../../../../../l3vpn:afs/l3vpn:af[l3vpn:type='ipv4-unicast']/rt:routing/rt:direct-routing/rt:interfaces/rt:interface[rt:name=current()/../../../../isis:name]/rt:binding-topologys/rt:binding-topology[rt:topology-name=current()/../../../../../../isis:afs/isis:af[isis:type=current()/../../isis:type]/isis:topologys/isis:topology[isis:mt-id=current()/isis:mt-id]/isis:topo-name])) or ((../../isis:type='afipv6') and (../../../../../../../../../l3vpn:afs/l3vpn:af[l3vpn:type='ipv6-unicast']/rt:routing/rt:direct-routing/rt:interfaces/rt:interface[rt:name=current()/../../../../isis:name]/rt:binding-topologys/rt:binding-topology[rt:topology-name=current()/../../../../../../isis:afs/isis:af[isis:type=current()/../../isis:type]/isis:topologys/isis:topology[isis:mt-id=current()/isis:mt-id]/isis:topo-name]))"/>
                      <key value="mt-id"/>
                      <description>
                        <text>Configure interface multi topology.</text>
                      </description>
                      <leaf name="mt-id">
                        <type name="leafref">
                          <path value="/ni:network-instance/ni:instances/ni:instance/isis:isis/isis:sites/isis:site/isis:afs/isis:af/isis:topologys/isis:topology/isis:mt-id"/>
                        </type>
                        <description>
                          <text>ISIS multi topology ID.</text>
                        </description>
                      </leaf>
                      <leaf name="cost-level1">
                        <type name="uint32">
                          <range value="1..16777215"/>
                        </type>
                        <must condition="(../cost-level1&lt;=63) or ((../../../../../../../cost-style='wide' or ../../../../../../../cost-style='wtransition') and ../cost-level1&lt;=16777215)"/>
                        <description>
                          <text>Link cost of an interface used for the level 1 SPF calculation.</text>
                        </description>
                      </leaf>
                      <leaf name="cost-level2">
                        <type name="uint32">
                          <range value="1..16777215"/>
                        </type>
                        <must condition="(../cost-level2&lt;=63) or ((../../../../../../../cost-style='wide' or ../../../../../../../cost-style='wtransition') and ../cost-level2&lt;=16777215)"/>
                        <description>
                          <text>Link cost of an interface used for the level 2 SPF calculation.</text>
                        </description>
                      </leaf>
                      <leaf name="tag-level1">
                        <type name="uint32">
                          <range value="0..4294967295"/>
                        </type>
                        <default value="0"/>
                        <description>
                          <text>Set the level 1 tag value for direct routes on the interface. A value of 0 indicates disable.</text>
                        </description>
                      </leaf>
                      <leaf name="tag-level2">
                        <type name="uint32">
                          <range value="0..4294967295"/>
                        </type>
                        <default value="0"/>
                        <description>
                          <text>Set the level 2 tag value for direct routes on the interface. A value of 0 indicates disable.</text>
                        </description>
                      </leaf>
                      <leaf name="incr-cost">
                        <when condition="../../../type='afipv4'"/>
                        <type name="uint32">
                          <range value="1..16777214"/>
                        </type>
                        <description>
                          <text>Set the cost to be added if the link quality is low.</text>
                        </description>
                      </leaf>
                      <container name="link-group-ports">
                        <description>
                          <text>List of link-group interface.</text>
                        </description>
                        <list name="link-group-port">
                          <key value="level-type"/>
                          <description>
                            <text>Configure link-group interface.</text>
                          </description>
                          <leaf name="level-type">
                            <type name="level-type-two"/>
                            <description>
                              <text>Level type.</text>
                            </description>
                          </leaf>
                          <leaf name="name">
                            <ext:operation-exclude value="update"/>
                            <type name="leafref">
                              <path value="/ni:network-instance/ni:instances/ni:instance/isis:isis/isis:sites/isis:site/isis:link-groups/isis:link-group/isis:name"/>
                            </type>
                            <must condition="not(../../link-group-port[name!=current()])"/>
                            <mandatory value="true"/>
                            <description>
                              <text>To set link-group name.</text>
                            </description>
                          </leaf>
                        </list>
                      </container>
                    </list>
                  </container>
                  <container name="delay-normalize">
                    <description>
                      <text>Configure metric-delay normalization.</text>
                    </description>
                    <leaf name="interval">
                      <type name="uint32">
                        <range value="1..16777215"/>
                      </type>
                      <must condition="(../offset&lt;../interval)"/>
                      <description>
                        <text>Normalization interval value.</text>
                      </description>
                    </leaf>
                    <leaf name="offset">
                      <when condition="../interval"/>
                      <type name="uint32">
                        <range value="0..16777214"/>
                      </type>
                      <must condition="(../offset&lt;../interval)"/>
                      <default value="0"/>
                      <description>
                        <text>Normalization offset value.</text>
                      </description>
                    </leaf>
                  </container>
                </list>
              </container>
              <container name="disp-data">
                <config value="false"/>
                <description>
                  <text>Statistics of interface data.</text>
                </description>
                <leaf name="index">
                  <type name="uint8"/>
                  <description>
                    <text>Circuit ID of an interface.</text>
                  </description>
                </leaf>
                <leaf name="mtu">
                  <type name="uint32"/>
                  <description>
                    <text>MTU value of an interface.</text>
                  </description>
                </leaf>
                <leaf name="dis-level1">
                  <type name="boolean"/>
                  <description>
                    <text>Determine whether the interface is a level 1 DIS.</text>
                  </description>
                </leaf>
                <leaf name="dis-level2">
                  <type name="boolean"/>
                  <description>
                    <text>Determine whether the interface is a level 2 DIS.</text>
                  </description>
                </leaf>
                <leaf name="v4-status">
                  <type name="string">
                    <length value="1..40"/>
                    <ext:value-meaning>
                      <ext:item value="UP">
                        <ext:meaning value="UP"/>
                      </ext:item>
                    </ext:value-meaning>
                  </type>
                  <description>
                    <text>IPv4 status of the interface, up or down.</text>
                  </description>
                </leaf>
                <leaf name="mtu-state">
                  <type name="boolean"/>
                  <description>
                    <text>MTU status of an interface, up or down.</text>
                  </description>
                </leaf>
                <leaf name="link-state">
                  <type name="boolean"/>
                  <description>
                    <text>Link status of an interface, up or down.</text>
                  </description>
                </leaf>
                <leaf name="ip-state">
                  <type name="boolean"/>
                  <description>
                    <text>IPv4 address status of an interface, up or down.</text>
                  </description>
                </leaf>
                <leaf name="v6-status">
                  <type name="string">
                    <length value="1..40"/>
                    <ext:value-meaning>
                      <ext:item value="UP">
                        <ext:meaning value="UP"/>
                      </ext:item>
                    </ext:value-meaning>
                  </type>
                  <description>
                    <text>ISIS IPv6 status of an interface, up or down.</text>
                  </description>
                </leaf>
                <leaf name="mtu-v6-state">
                  <type name="boolean"/>
                  <description>
                    <text>MTU IPv6 status of an interface, up or down.</text>
                  </description>
                </leaf>
                <leaf name="link-v6-state">
                  <type name="boolean"/>
                  <description>
                    <text>Link IPv6 status of an interface, up or down.</text>
                  </description>
                </leaf>
                <leaf name="ip-v6-state">
                  <type name="boolean"/>
                  <description>
                    <text>IPv6 address status of an interface, up or down.</text>
                  </description>
                </leaf>
                <leaf name="vc-state">
                  <type name="boolean"/>
                  <description>
                    <text>Virtual cluster status of an interface, up or down.</text>
                  </description>
                </leaf>
                <leaf name="peer-flap-status">
                  <type name="peer-flap-status-type"/>
                  <description>
                    <text>Display the neighbor relationship flapping suppression status.</text>
                  </description>
                </leaf>
                <leaf name="peer-flap-count">
                  <type name="uint32"/>
                  <description>
                    <text>Display the current flapping count.</text>
                  </description>
                </leaf>
                <leaf name="peer-flap-threshold">
                  <type name="uint32"/>
                  <description>
                    <text>Display threshold of neighbor relationship flapping suppression.</text>
                  </description>
                </leaf>
                <leaf name="peer-flap-timer">
                  <type name="string">
                    <length value="1..40"/>
                  </type>
                  <description>
                    <text>Display the time when neighbor relationship flapping suppression took effect.</text>
                  </description>
                </leaf>
                <leaf name="peer-flap-remain-timer">
                  <type name="uint32"/>
                  <units name="s"/>
                  <description>
                    <text>Display the remaining time of neighbor relationship flapping suppression.</text>
                  </description>
                </leaf>
                <leaf name="osi-state">
                  <type name="boolean"/>
                  <description>
                    <text>OSI state of the interface, up or down.</text>
                  </description>
                </leaf>
                <leaf name="snpa-state">
                  <type name="boolean"/>
                  <description>
                    <text>SNPA state of the interface, up or down.</text>
                  </description>
                </leaf>
                <leaf name="bandwidth-state">
                  <type name="boolean"/>
                  <description>
                    <text>Bandwidth state of the interface, up or down.</text>
                  </description>
                </leaf>
                <leaf name="enable-state">
                  <type name="boolean"/>
                  <description>
                    <text>ISIS enable state of the interface, up or down.</text>
                  </description>
                </leaf>
                <leaf name="v6-osi-state">
                  <type name="boolean"/>
                  <description>
                    <text>IPv6 OSI state of the interface, up or down.</text>
                  </description>
                </leaf>
                <leaf name="v6-snpa-state">
                  <type name="boolean"/>
                  <description>
                    <text>IPv6 SNPA state of the interface, up or down.</text>
                  </description>
                </leaf>
                <leaf name="v6-bandwidth-state">
                  <type name="boolean"/>
                  <description>
                    <text>IPv6 bandwidth state of the interface, up or down.</text>
                  </description>
                </leaf>
                <leaf name="v6-enable-state">
                  <type name="boolean"/>
                  <description>
                    <text>ISIS IPv6 enable state of the interface, up or down.</text>
                  </description>
                </leaf>
              </container>
            </list>
          </container>
          <container name="tlv-fragments">
            <description>
              <text>Configure IS-IS TLV fragmentation.</text>
            </description>
            <leaf name="disable">
              <type name="boolean"/>
              <default value="false"/>
              <description>
                <text>Enable/disable IS-IS TLV fragmentation.</text>
              </description>
            </leaf>
          </container>
          <container name="afs">
            <description>
              <text>List of address family of IPv4 and IPv6.</text>
            </description>
            <list name="af">
              <key value="type"/>
              <max-elements value="2"/>
              <ext:generated-by value="system">
                <ext:filter value="type='afipv4'"/>
              </ext:generated-by>
              <description>
                <text>Configure address family of IPv4 and IPv6. IPv4 base topo is generated by the ISIS process, which is not allowed to be created separately.</text>
              </description>
              <leaf name="type">
                <type name="af-type"/>
                <must condition="(../type='afipv4' and ../../../../../../l3vpn:afs/l3vpn:af[l3vpn:type='ipv4-unicast']) or (../type='afipv6' and ../../../../../../l3vpn:afs/l3vpn:af[l3vpn:type='ipv6-unicast'])"/>
                <description>
                  <text>Address family type of the multi topology.</text>
                </description>
              </leaf>
              <leaf name="ipv6-base-topo">
                <when condition="../type='afipv6'"/>
                <type name="base-topo-v6"/>
                <default value="ipv6"/>
                <description>
                  <text>Type of IPv6 enabling.</text>
                </description>
              </leaf>
              <leaf name="apply-qppb">
                <type name="boolean"/>
                <must condition="../apply-qppb='false' or ../type='afipv6'"/>
                <default value="false"/>
                <description>
                  <text>Enable/disable IPv6 qppb.</text>
                </description>
              </leaf>
              <leaf name="max-load-balancing">
                <type name="uint32">
                  <range value="1..128"/>
                </type>
                <description>
                  <text>Set the maximum number of equal cost routes for load balancing.</text>
                </description>
                <ext:dynamic-default/>
              </leaf>
              <leaf name="auto-cost">
                <type name="boolean"/>
                <default value="false"/>
                <description>
                  <text>Enable/disable automatic cost calculation for the system.</text>
                </description>
              </leaf>
              <leaf name="auto-cost-compatible">
                <when condition="../auto-cost='true'"/>
                <type name="boolean"/>
                <default value="false"/>
                <description>
                  <text>Enable/disable automatic cost calculation compatibly for the system.</text>
                </description>
              </leaf>
              <leaf name="bandwidth">
                <type name="uint32">
                  <range value="1..2147483648"/>
                </type>
                <units name="Mbit/s"/>
                <default value="100"/>
                <description>
                  <text>Set the bandwidth reference used for the automatic cost calculation on an interface.</text>
                </description>
              </leaf>
              <leaf name="cost-level1">
                <type name="uint32">
                  <range value="1..16777215"/>
                </type>
                <must condition="(../cost-level1&lt;=63) or (not(../../../cost-style!='wide' and ../../../cost-style!='wtransition') and ../cost-level1&lt;=16777215)"/>
                <description>
                  <text>Set the level 1 cost for an interface.</text>
                </description>
              </leaf>
              <leaf name="cost-level2">
                <type name="uint32">
                  <range value="1..16777215"/>
                </type>
                <must condition="(../cost-level2&lt;=63) or (not(../../../cost-style!='wide' and ../../../cost-style!='wtransition') and ../cost-level2&lt;=16777215)"/>
                <description>
                  <text>Set the level 2 cost for an interface.</text>
                </description>
              </leaf>
              <leaf name="bfd-bit-error">
                <type name="boolean"/>
                <must condition="../bfd-bit-error='false' or ../type='afipv4'"/>
                <default value="false"/>
                <description>
                  <text>Enable/disable BFD bit error on all interfaces.</text>
                </description>
              </leaf>
              <leaf name="default-tag-level1">
                <type name="uint32">
                  <range value="1..4294967295"/>
                </type>
                <description>
                  <text>Indicates the administrative tag for a Level-1 interface on which IS-IS process is enabled.</text>
                </description>
              </leaf>
              <leaf name="default-tag-level2">
                <type name="uint32">
                  <range value="1..4294967295"/>
                </type>
                <description>
                  <text>Indicates the administrative tag for a Level-2 interface on which IS-IS process is enabled.</text>
                </description>
              </leaf>
              <container name="attach-bit">
                <when condition="../type!='afipv6'"/>
                <description>
                  <text>Configure LSPs with the ATT bit.</text>
                </description>
                <leaf name="adv-control">
                  <type name="att-adv-mode"/>
                  <default value="no-type"/>
                  <description>
                    <text>Control to advertise the ATT bit.</text>
                  </description>
                </leaf>
                <leaf name="avoid-learn">
                  <type name="boolean"/>
                  <must condition="../avoid-learn='false' or ../../type='afipv4'"/>
                  <default value="false"/>
                  <description>
                    <text>Enable/disable prevent the device from learning the LSPs with the ATT bit.</text>
                  </description>
                </leaf>
              </container>
              <container name="preference">
                <description>
                  <text>Configure a priority for ISIS.</text>
                </description>
                <leaf name="value">
                  <type name="uint32">
                    <range value="1..255"/>
                  </type>
                  <default value="15"/>
                  <description>
                    <text>Set the priority for protocols. The smaller the value, the higher the priority.</text>
                  </description>
                </leaf>
                <choice name="policy-type">
                  <description>
                    <text>Type of preference rule.</text>
                  </description>
                  <case name="route-policy">
                    <description>
                      <text>Set a route policy name.</text>
                    </description>
                    <leaf name="route-policy">
                      <type name="leafref">
                        <path value="/rtp:routing-policy/rtp:policy-definitions/rtp:policy-definition/rtp:name"/>
                      </type>
                      <description>
                        <text>Set a route policy name.</text>
                      </description>
                    </leaf>
                  </case>
                  <case name="route-filter">
                    <description>
                      <text>Set a route filter name.</text>
                    </description>
                    <leaf name="route-filter">
                      <type name="leafref">
                        <path value="/xpl:xpl/xpl:route-filters/xpl:route-filter/xpl:name"/>
                      </type>
                      <description>
                        <text>Set a route filter name.</text>
                      </description>
                    </leaf>
                  </case>
                </choice>
              </container>
              <container name="default-route">
                <presence value="Enable default routes."/>
                <description>
                  <text>Configure the level 1 and level 2 routers to generate default routes.</text>
                </description>
                <leaf name="mode">
                  <type name="default-mode"/>
                  <must condition="../mode!='match-any' or (../mode='match-any' and (../route-policy-name or ../route-filter-name))"/>
                  <default value="always"/>
                  <description>
                    <text>Set the default routes mode. When the mode is not match any, route policy name and route filter name cannot be configured.</text>
                  </description>
                </leaf>
                <choice name="policy-type">
                  <description>
                    <text>Mode of the policy used for default route.</text>
                  </description>
                  <case name="route-policy-name">
                    <description>
                      <text>Name of a routing policy.</text>
                    </description>
                    <leaf name="route-policy-name">
                      <when condition="../mode='match-any'"/>
                      <type name="leafref">
                        <path value="/rtp:routing-policy/rtp:policy-definitions/rtp:policy-definition/rtp:name"/>
                      </type>
                      <description>
                        <text>Set a route policy name for default routes.</text>
                      </description>
                    </leaf>
                  </case>
                  <case name="route-filter-name">
                    <description>
                      <text>Name of a routing filter.</text>
                    </description>
                    <leaf name="route-filter-name">
                      <when condition="../mode='match-any'"/>
                      <type name="leafref">
                        <path value="/xpl:xpl/xpl:route-filters/xpl:route-filter/xpl:name"/>
                      </type>
                      <description>
                        <text>Set a xpl route policy name entity for default routes.</text>
                      </description>
                    </leaf>
                  </case>
                </choice>
                <leaf name="cost">
                  <type name="uint32">
                    <range value="0..4261412864"/>
                  </type>
                  <must condition="../cost&lt;=63 or ../../type='afipv4' and (../../../../cost-style='wide' or ../../../../cost-style='wtransition')"/>
                  <default value="0"/>
                  <description>
                    <text>Set a cost for default routes.</text>
                  </description>
                </leaf>
                <leaf name="tag">
                  <type name="uint32">
                    <range value="0..4294967295"/>
                  </type>
                  <default value="0"/>
                  <description>
                    <text>Set a tag for default routes.</text>
                  </description>
                </leaf>
                <leaf name="level-type">
                  <type name="level-type"/>
                  <default value="level-2"/>
                  <description>
                    <text>Set a level for default routes.</text>
                  </description>
                </leaf>
                <leaf name="avoid-learning">
                  <type name="boolean"/>
                  <must condition="(../avoid-learning='false' or ../learning-avoid-loop='false')"/>
                  <default value="false"/>
                  <description>
                    <text>Enable/disable to prevent ISIS processes from learning default routes and adding the default routes to the routing table. If the default routes that already exist in the routing table are active, the status of the route is set to inactive.</text>
                  </description>
                </leaf>
                <leaf name="learning-avoid-loop">
                  <type name="boolean"/>
                  <must condition="(../learning-avoid-loop='false') or (../avoid-learning='false' and ../../type='afipv6')"/>
                  <default value="false"/>
                  <description>
                    <text>Enable/disable to prevent learning of the default route into a loop.</text>
                  </description>
                </leaf>
              </container>
              <container name="import-routes">
                <description>
                  <text>List of import route of other routing protocols.</text>
                </description>
                <list name="import-route">
                  <key value="protocol process-id"/>
                  <description>
                    <text>Configure import route of other routing protocols.</text>
                  </description>
                  <leaf name="protocol">
                    <type name="import-protocol"/>
                    <must condition="(../../../type='afipv4' and not(../protocol='ospfv3' or ../protocol='ripng')) or (../../../type='afipv6' and not(../protocol='ospf' or ../protocol='rip'))"/>
                    <description>
                      <text>Set the routing protocol from which routes are imported.</text>
                    </description>
                  </leaf>
                  <leaf name="process-id">
                    <type name="uint32">
                      <range value="0..4294967295"/>
                    </type>
                    <must condition="(../process-id=0 and (../protocol='direct' or ../protocol='static' or ../protocol='bgp' or ../protocol='unr')) or (../process-id&gt;0 and (../protocol='ospf' or ../protocol='isis' or ../protocol='rip' or ../protocol='ospfv3' or ../protocol='ripng'))"/>
                    <must condition="not (../process-id=../../../../../id and ../protocol='isis')"/>
                    <description>
                      <text>Set the process ID. The process ID is required when the routing protocol is RIP, OSPF, or ISIS.</text>
                    </description>
                  </leaf>
                  <leaf name="cost-type">
                    <when condition="../../../type='afipv4' and ../inherit-cost='false'"/>
                    <type name="is-import-cost"/>
                    <default value="external"/>
                    <description>
                      <text>Set the cost type for imported routes.</text>
                    </description>
                  </leaf>
                  <leaf name="cost">
                    <when condition="../inherit-cost='false'"/>
                    <type name="uint32">
                      <range value="0..4261412864"/>
                    </type>
                    <must condition="(../cost&lt;=63) or (not(../../../../../cost-style!='wide' and ../../../../../cost-style!='wtransition') and ../cost&lt;=4261412864)"/>
                    <default value="0"/>
                    <description>
                      <text>Set a cost for imported routes. If the ISIS cost type is narrow, narrow compatible, or compatible, the value ranges from 1 to 63.</text>
                    </description>
                  </leaf>
                  <leaf name="tag">
                    <type name="uint32">
                      <range value="0..4294967295"/>
                    </type>
                    <default value="0"/>
                    <description>
                      <text>Set a tag for imported routes. A value of 0 indicates disable.</text>
                    </description>
                  </leaf>
                  <choice name="policy-type">
                    <description>
                      <text>Mode of the policy used for import route.</text>
                    </description>
                    <case name="route-policy-name">
                      <description>
                        <text>Set a route policy name.</text>
                      </description>
                      <leaf name="route-policy-name">
                        <type name="leafref">
                          <path value="/rtp:routing-policy/rtp:policy-definitions/rtp:policy-definition/rtp:name"/>
                        </type>
                        <description>
                          <text>Set a route policy name.</text>
                        </description>
                      </leaf>
                    </case>
                    <case name="route-filter-name">
                      <description>
                        <text>Name of a routing filter.</text>
                      </description>
                      <leaf name="route-filter-name">
                        <type name="leafref">
                          <path value="/xpl:xpl/xpl:route-filters/xpl:route-filter/xpl:name"/>
                        </type>
                        <description>
                          <text>Specify a route filter.</text>
                        </description>
                      </leaf>
                      <leaf name="parameter">
                        <when condition="../route-filter-name"/>
                        <type name="xpl:filter-parameter-type"/>
                        <description>
                          <text>Specify an import route-filter parameter.</text>
                        </description>
                      </leaf>
                    </case>
                  </choice>
                  <leaf name="level-type">
                    <type name="level-type"/>
                    <default value="level-2"/>
                    <description>
                      <text>Import routes to the level 1 or (and) level 2 routing table.</text>
                    </description>
                  </leaf>
                  <leaf name="inherit-cost">
                    <type name="boolean"/>
                    <must condition="(../inherit-cost='false') or (../protocol!='static' and not(../cost-type))"/>
                    <default value="false"/>
                    <description>
                      <text>Enable/disable keep the original cost of a route when importing the route to ISIS.</text>
                    </description>
                  </leaf>
                  <leaf name="permit-ibgp">
                    <type name="boolean"/>
                    <must condition="../permit-ibgp='false' or ../protocol='bgp'"/>
                    <default value="false"/>
                    <description>
                      <text>Enable/disable the device to import IBGP routes.</text>
                    </description>
                  </leaf>
                  <leaf name="nosidflag">
                    <type name="boolean"/>
                    <must condition="../nosidflag='false' or (../protocol='isis' or ../protocol='ospf')"/>
                    <default value="false"/>
                    <description>
                      <text>Enable/disable prohibit sid when import other process or protocol routes.</text>
                    </description>
                  </leaf>
                </list>
              </container>
              <container name="nexthop-weights">
                <when condition="../type='afipv4'"/>
                <description>
                  <text>List of next hop weight.</text>
                </description>
                <list name="nexthop-weight">
                  <key value="ip-address"/>
                  <max-elements value="131072"/>
                  <description>
                    <text>Configure next hop weight.</text>
                  </description>
                  <leaf name="ip-address">
                    <type name="inet:ipv4-address-no-zone"/>
                    <description>
                      <text>Set a next hop address.</text>
                    </description>
                  </leaf>
                  <leaf name="weight">
                    <type name="uint32">
                      <range value="1..254"/>
                    </type>
                    <mandatory value="true"/>
                    <description>
                      <text>Set a weight value for the next hop.</text>
                    </description>
                  </leaf>
                </list>
              </container>
              <container name="leak-route-level2-to-level1">
                <presence value="Leak route level2 to level1."/>
                <description>
                  <text>Configure to control ISIS to perform route leaking from level 2 to level 1. This command needs to be set on the level 1 2 router connected to the external areas.</text>
                </description>
                <leaf name="tag">
                  <type name="uint32">
                    <range value="0..4294967295"/>
                  </type>
                  <default value="0"/>
                  <description>
                    <text>Management label value assigned for imported routes. The label value is the name of route filtering during route leaking. A value of 0 indicates disable.</text>
                  </description>
                </leaf>
                <choice name="policy-type">
                  <description>
                    <text>Mode of the policy used for route leaking.</text>
                  </description>
                  <case name="basic-acl4">
                    <description>
                      <text>Apply basic ACL name or ACL number.</text>
                    </description>
                    <leaf name="name-or-num-v4">
                      <when condition="../../type='afipv4'"/>
                      <type name="leafref">
                        <path value="/acl:acl/acl:groups/acl:group/acl:identity"/>
                      </type>
                      <must condition="/acl:acl/acl:groups/acl:group[acl:identity=current()]/acl:type='basic'"/>
                      <description>
                        <text>Apply basic ACL name or ACL number.</text>
                      </description>
                    </leaf>
                  </case>
                  <case name="basic-acl6">
                    <description>
                      <text>Apply basic ACL6 name or ACL6 number.</text>
                    </description>
                    <leaf name="name-or-num-v6">
                      <when condition="../../type='afipv6'"/>
                      <type name="leafref">
                        <path value="/acl:acl/acl:group6s/acl:group6/acl:identity"/>
                      </type>
                      <must condition="/acl:acl/acl:group6s/acl:group6[acl:identity=current()]/acl:type='basic'"/>
                      <description>
                        <text>Apply basic ACL6 name or ACL6 number.</text>
                      </description>
                    </leaf>
                  </case>
                  <case name="ipv4-prefix">
                    <description>
                      <text>Set the name of an IP prefix list.</text>
                    </description>
                    <leaf name="ipv4-prefix">
                      <when condition="../../type='afipv4'"/>
                      <type name="leafref">
                        <path value="/rtp:routing-policy/rtp:ipv4-prefix-filters/rtp:ipv4-prefix-filter/rtp:name"/>
                      </type>
                      <description>
                        <text>Set the name of an IP prefix list.</text>
                      </description>
                    </leaf>
                  </case>
                  <case name="ipv6-prefix">
                    <description>
                      <text>Set the name of an IPv6 prefix list.</text>
                    </description>
                    <leaf name="ipv6-prefix">
                      <when condition="../../type='afipv6'"/>
                      <type name="leafref">
                        <path value="/rtp:routing-policy/rtp:ipv6-prefix-filters/rtp:ipv6-prefix-filter/rtp:name"/>
                      </type>
                      <description>
                        <text>Set the name of an IPv6 prefix list.</text>
                      </description>
                    </leaf>
                  </case>
                  <case name="route-policy-name">
                    <description>
                      <text>Set a route policy name.</text>
                    </description>
                    <leaf name="route-policy-name">
                      <type name="leafref">
                        <path value="/rtp:routing-policy/rtp:policy-definitions/rtp:policy-definition/rtp:name"/>
                      </type>
                      <description>
                        <text>Name of the route filtering policy used during route leaking.</text>
                      </description>
                    </leaf>
                  </case>
                  <case name="route-filter-name">
                    <description>
                      <text>Set a route filter name.</text>
                    </description>
                    <leaf name="route-filter-name">
                      <type name="leafref">
                        <path value="/xpl:xpl/xpl:route-filters/xpl:route-filter/xpl:name"/>
                      </type>
                      <description>
                        <text>Set a route filter name.</text>
                      </description>
                    </leaf>
                  </case>
                </choice>
                <leaf name="allow-filter">
                  <type name="boolean"/>
                  <default value="false"/>
                  <description>
                    <text>Enable/disable the name of a route policy which filters routes based on the tag or other protocol parameters.</text>
                  </description>
                </leaf>
                <leaf name="allow-updown">
                  <type name="boolean"/>
                  <default value="false"/>
                  <description>
                    <text>Enable/disable the Up/Down bit for routes.</text>
                  </description>
                </leaf>
                <leaf name="nosidflag">
                  <type name="boolean"/>
                  <default value="false"/>
                  <description>
                    <text>Enable/disable prohibit sid when leak routes from level 2 to level 1.</text>
                  </description>
                </leaf>
              </container>
              <container name="leak-route-level1-to-level2">
                <description>
                  <text>Configure the leaking of the routes from a level 1 area to a level 2 area. This is convenient for route management. This command needs to be configured on the level 1 2 router connected to external areas.</text>
                </description>
                <leaf name="enable">
                  <type name="boolean"/>
                  <default value="true"/>
                  <description>
                    <text>Enable/disable level 1 to level 2 route filtering.</text>
                  </description>
                </leaf>
                <choice name="policy-type">
                  <description>
                    <text>Type of filter policy rule.</text>
                  </description>
                  <case name="basic-acl4">
                    <description>
                      <text>Apply basic ACL name or ACL number.</text>
                    </description>
                    <leaf name="name-or-num-v4">
                      <when condition="../enable='true' and ../../type='afipv4'"/>
                      <type name="leafref">
                        <path value="/acl:acl/acl:groups/acl:group/acl:identity"/>
                      </type>
                      <must condition="/acl:acl/acl:groups/acl:group[acl:identity=current()]/acl:type='basic'"/>
                      <description>
                        <text>Apply basic ACL name or ACL number.</text>
                      </description>
                    </leaf>
                  </case>
                  <case name="basic-acl6">
                    <description>
                      <text>Apply basic ACL6 name or ACL6 number.</text>
                    </description>
                    <leaf name="name-or-num-v6">
                      <when condition="../enable='true' and ../../type='afipv6'"/>
                      <type name="leafref">
                        <path value="/acl:acl/acl:group6s/acl:group6/acl:identity"/>
                      </type>
                      <must condition="/acl:acl/acl:group6s/acl:group6[acl:identity=current()]/acl:type='basic'"/>
                      <description>
                        <text>Apply basic ACL6 name or ACL6 number.</text>
                      </description>
                    </leaf>
                  </case>
                  <case name="ipv4-prefix">
                    <description>
                      <text>Set the name of an IP prefix list.</text>
                    </description>
                    <leaf name="ipv4-prefix">
                      <when condition="../enable='true' and ../../type='afipv4'"/>
                      <type name="leafref">
                        <path value="/rtp:routing-policy/rtp:ipv4-prefix-filters/rtp:ipv4-prefix-filter/rtp:name"/>
                      </type>
                      <description>
                        <text>Set the name of an IP prefix list.</text>
                      </description>
                    </leaf>
                  </case>
                  <case name="ipv6-prefix">
                    <description>
                      <text>Set the name of an IPv6 prefix list.</text>
                    </description>
                    <leaf name="ipv6-prefix">
                      <when condition="../enable='true' and ../../type='afipv6'"/>
                      <type name="leafref">
                        <path value="/rtp:routing-policy/rtp:ipv6-prefix-filters/rtp:ipv6-prefix-filter/rtp:name"/>
                      </type>
                      <description>
                        <text>Set the name of an IPv6 prefix list.</text>
                      </description>
                    </leaf>
                  </case>
                  <case name="route-policy">
                    <description>
                      <text>Set a route policy name.</text>
                    </description>
                    <leaf name="route-policy">
                      <when condition="../enable='true'"/>
                      <type name="leafref">
                        <path value="/rtp:routing-policy/rtp:policy-definitions/rtp:policy-definition/rtp:name"/>
                      </type>
                      <description>
                        <text>Set a route policy name.</text>
                      </description>
                    </leaf>
                  </case>
                  <case name="route-filter">
                    <description>
                      <text>Set a route filter name.</text>
                    </description>
                    <leaf name="route-filter">
                      <when condition="../enable='true'"/>
                      <type name="leafref">
                        <path value="/xpl:xpl/xpl:route-filters/xpl:route-filter/xpl:name"/>
                      </type>
                      <description>
                        <text>Set a route filter name.</text>
                      </description>
                    </leaf>
                  </case>
                </choice>
                <leaf name="tag">
                  <when condition="../enable='true'"/>
                  <type name="uint32">
                    <range value="0..4294967295"/>
                  </type>
                  <default value="0"/>
                  <description>
                    <text>Administrative tag assigned for imported routes. A value of 0 indicates disable.</text>
                  </description>
                </leaf>
                <leaf name="allow-filter">
                  <when condition="../enable='true'"/>
                  <type name="boolean"/>
                  <default value="false"/>
                  <description>
                    <text>Enable/disable the name of a route policy which filters routes based on the tag or other protocol parameters.</text>
                  </description>
                </leaf>
                <leaf name="nosidflag">
                  <type name="boolean"/>
                  <default value="false"/>
                  <description>
                    <text>Enable/disable prohibit sid when leak routes from level 1 to level 2.</text>
                  </description>
                </leaf>
              </container>
              <container name="summary-prefixs">
                <description>
                  <text>List of summary routes.</text>
                </description>
                <list name="summary-prefix">
                  <key value="prefix network-mask"/>
                  <description>
                    <text>Configure summary route.</text>
                  </description>
                  <leaf name="prefix">
                    <type name="inet:ip-address-no-zone"/>
                    <description>
                      <text>Set a summary address. Prefix is the address after mask.</text>
                    </description>
                  </leaf>
                  <leaf name="network-mask">
                    <type name="uint8">
                      <range value="0..128"/>
                    </type>
                    <description>
                      <text>Set the summary address mask. If summary address is the type of IPv4, the max value is 32. If summary address is the type of IPv6, the max value is 128.</text>
                    </description>
                  </leaf>
                  <leaf name="level-type">
                    <type name="level-type"/>
                    <default value="level-2"/>
                    <description>
                      <text>Set the level for summary routes.</text>
                    </description>
                  </leaf>
                  <leaf name="tag">
                    <type name="uint32">
                      <range value="0..4294967295"/>
                    </type>
                    <default value="0"/>
                    <description>
                      <text>Set a tag for summary routes.</text>
                    </description>
                  </leaf>
                  <leaf name="gen-null0-route">
                    <type name="boolean"/>
                    <default value="false"/>
                    <description>
                      <text>Enable/disable generate a Null 0 route to prevent routing loops.</text>
                    </description>
                  </leaf>
                  <leaf name="avoid-feadback">
                    <type name="boolean"/>
                    <default value="false"/>
                    <description>
                      <text>Enable/disable avoid learning summary routes.</text>
                    </description>
                  </leaf>
                  <leaf name="cost">
                    <when condition="../../../type='afipv4'"/>
                    <type name="uint32">
                      <range value="1..16777215"/>
                    </type>
                    <description>
                      <text>Set a cost for summarized routes. If the ISIS cost type is narrow, narrow-compatible, or compatible, the value ranges from 1 to 63. If the ISIS cost type is wide or wide-compatible, the value ranges from 1 to 16777215.</text>
                    </description>
                  </leaf>
                  <leaf name="learning-avoid-loop">
                    <type name="boolean"/>
                    <must condition="(../learning-avoid-loop='false' or ../../../type='afipv6')"/>
                    <default value="false"/>
                    <description>
                      <text>Enable/disable to prevent learning of the same summary route into a loop.</text>
                    </description>
                  </leaf>
                </list>
              </container>
              <container name="traffic-eng">
                <description>
                  <text>Configure ISIS level enabled with the TE capability.</text>
                </description>
                <leaf name="enable-level1">
                  <type name="boolean"/>
                  <must condition="../enable-level1='false' or ../../../../cost-style='wide' or ../../../../cost-style='wtransition' or ../../../../cost-style='transition'"/>
                  <default value="false"/>
                  <description>
                    <text>Enable/disable traffic eng enable level 1.</text>
                  </description>
                </leaf>
                <leaf name="enable-level2">
                  <type name="boolean"/>
                  <must condition="../enable-level2='false' or ../../../../cost-style='wide' or ../../../../cost-style='wtransition' or ../../../../cost-style='transition'"/>
                  <default value="false"/>
                  <description>
                    <text>Enable/disable traffic eng enable level 2.</text>
                  </description>
                </leaf>
                <container name="metric-delay">
                  <when condition="(../enable-level1='true' or ../enable-level2='true')"/>
                  <description>
                    <text>Configure metric delay in a ISIS process.</text>
                  </description>
                  <leaf name="adv-level1">
                    <type name="boolean"/>
                    <default value="false"/>
                    <description>
                      <text>Enable/disable metric delay advertisement.</text>
                    </description>
                  </leaf>
                  <leaf name="adv-level2">
                    <type name="boolean"/>
                    <default value="false"/>
                    <description>
                      <text>Enable/disable metric delay advertisement.</text>
                    </description>
                  </leaf>
                  <leaf name="suppress-timer">
                    <when condition="../adv-level1='true' or ../adv-level2='true'"/>
                    <type name="uint32">
                      <range value="0..600"/>
                    </type>
                    <units name="s"/>
                    <default value="120"/>
                    <description>
                      <text>Metric delay suppress timer.</text>
                    </description>
                  </leaf>
                  <leaf name="suppress-percent">
                    <when condition="../adv-level1='true' or ../adv-level2='true'"/>
                    <type name="uint8">
                      <range value="0..100"/>
                    </type>
                    <default value="10"/>
                    <description>
                      <text>Metric delay suppress percent threshold.</text>
                    </description>
                  </leaf>
                  <leaf name="suppress-absolute">
                    <when condition="../adv-level1='true' or ../adv-level2='true'"/>
                    <type name="uint32">
                      <range value="0..10000"/>
                    </type>
                    <default value="1000"/>
                    <description>
                      <text>Metric delay suppress absolute threshold.</text>
                    </description>
                  </leaf>
                </container>
                <container name="metric-link-losses">
                  <when condition="(../enable-level1='true' or ../enable-level2='true')"/>
                  <description>
                    <text>Configure metric link loss in a ISIS process.</text>
                  </description>
                  <leaf name="adv-level1">
                    <type name="boolean"/>
                    <default value="false"/>
                    <description>
                      <text>Enable/disable metric link loss advertisement.</text>
                    </description>
                  </leaf>
                  <leaf name="adv-level2">
                    <type name="boolean"/>
                    <default value="false"/>
                    <description>
                      <text>Enable/disable metric link loss advertisement.</text>
                    </description>
                  </leaf>
                  <leaf name="suppress-timer">
                    <when condition="../adv-level1='true' or ../adv-level2='true'"/>
                    <type name="uint32">
                      <range value="0..600"/>
                    </type>
                    <units name="s"/>
                    <default value="120"/>
                    <description>
                      <text>Metric link loss suppress timer.</text>
                    </description>
                  </leaf>
                  <leaf name="suppress-percent">
                    <when condition="../adv-level1='true' or ../adv-level2='true'"/>
                    <type name="uint8">
                      <range value="0..100"/>
                    </type>
                    <default value="10"/>
                    <description>
                      <text>Metric link loss suppress percent threshold.</text>
                    </description>
                  </leaf>
                  <leaf name="suppress-absolute">
                    <when condition="../adv-level1='true' or ../adv-level2='true'"/>
                    <type name="uint32">
                      <range value="0..10000"/>
                    </type>
                    <default value="1000"/>
                    <description>
                      <text>Metric link loss suppress absolute threshold.</text>
                    </description>
                  </leaf>
                </container>
              </container>
              <container name="bfd">
                <description>
                  <text>Configure ISIS BFD parameters.</text>
                </description>
                <leaf name="enable">
                  <type name="boolean"/>
                  <default value="false"/>
                  <description>
                    <text>Enable/disable BFD on all interfaces.</text>
                  </description>
                </leaf>
                <leaf name="frr-binding">
                  <type name="boolean"/>
                  <default value="false"/>
                  <description>
                    <text>Enable/disable bind the BFD status to the link status of an interface.</text>
                  </description>
                </leaf>
                <leaf name="min-rx">
                  <type name="uint32">
                    <range value="3..20000"/>
                  </type>
                  <units name="ms"/>
                  <description>
                    <text>Set the minimum interval at which BFD packets are received. The default value and range controlled by PAF.</text>
                  </description>
                  <ext:dynamic-default/>
                </leaf>
                <leaf name="min-tx">
                  <type name="uint32">
                    <range value="3..20000"/>
                  </type>
                  <units name="ms"/>
                  <description>
                    <text>Set the minimum interval at which BFD packets are sent. The default value and range controlled by PAF.</text>
                  </description>
                  <ext:dynamic-default/>
                </leaf>
                <leaf name="detect-multiplier">
                  <type name="uint32">
                    <range value="3..50"/>
                  </type>
                  <default value="3"/>
                  <description>
                    <text>Set the detection multiplier of a BFD session.</text>
                  </description>
                </leaf>
                <leaf name="tos-exp">
                  <type name="uint8">
                    <range value="0..7"/>
                  </type>
                  <description>
                    <text>Set the priority of BFD packets.</text>
                  </description>
                </leaf>
                <leaf name="bfd-incr-cost">
                  <type name="uint32">
                    <range value="1..16777214"/>
                  </type>
                  <description>
                    <text>Set the increasing cost value for bfd.</text>
                  </description>
                </leaf>
              </container>
              <container name="ldp-sync">
                <when condition="../type='afipv4'"/>
                <description>
                  <text>Configure LDP and ISIS synchronization.</text>
                </description>
                <leaf name="enable">
                  <type name="boolean"/>
                  <default value="false"/>
                  <description>
                    <text>Enable/disable LDP and ISIS synchronization on an interface.</text>
                  </description>
                </leaf>
                <leaf name="mpls-binding">
                  <when condition="../enable='true'"/>
                  <type name="boolean"/>
                  <default value="false"/>
                  <description>
                    <text>Enable/disable MPLS LDP association.</text>
                  </description>
                </leaf>
                <leaf name="hold-down-timer">
                  <type name="uint32">
                    <range value="0..65535"/>
                  </type>
                  <units name="s"/>
                  <description>
                    <text>Set a value for the LDP and ISIS synchronization holddown timer.</text>
                  </description>
                </leaf>
                <choice name="hold-cost-max">
                  <default value="infinite"/>
                  <description>
                    <text>Set a value for the LDP and ISIS synchronization timer.</text>
                  </description>
                  <case name="timer">
                    <description>
                      <text>Set a value for the LDP and ISIS synchronization timer.</text>
                    </description>
                    <leaf name="timer">
                      <type name="uint32">
                        <range value="0..65535"/>
                      </type>
                      <units name="s"/>
                      <description>
                        <text>Set a value for the LDP and ISIS synchronization timer.</text>
                      </description>
                    </leaf>
                  </case>
                  <case name="infinite">
                    <description>
                      <text>Enable infinite LDP and ISIS synchronization.</text>
                    </description>
                    <leaf name="infinite">
                      <type name="empty"/>
                      <description>
                        <text>Enable infinite LDP and ISIS synchronization. The LDP and ISIS synchronization is kept before the LDP session goes up.</text>
                      </description>
                    </leaf>
                  </case>
                </choice>
              </container>
              <container name="mpls-ldp-global">
                <description>
                  <text>Configure MPLS LDP configuration.</text>
                </description>
                <leaf name="auto-flag">
                  <when condition="../../type='afipv4'"/>
                  <type name="boolean"/>
                  <default value="false"/>
                  <description>
                    <text>Enable/disable auto MPLS LDP configuration on an interface.</text>
                  </description>
                </leaf>
              </container>
              <container name="advertise-link-attributes">
                <description>
                  <text>Configure ISIS advertise attribute configuration.</text>
                </description>
                <leaf name="enable">
                  <type name="boolean"/>
                  <must condition="../enable='false' or ../../../../cost-style='wide' or ../../../../cost-style='transition' or ../../../../cost-style='wtransition'"/>
                  <default value="false"/>
                  <description>
                    <text>Enable/disable the ISIS process to advertise link attribute related TLVs on the LSP.</text>
                  </description>
                </leaf>
              </container>
              <container name="prefix-prioritys">
                <description>
                  <text>List of route priority.</text>
                </description>
                <list name="prefix-priority">
                  <key value="level priority"/>
                  <description>
                    <text>Configure route priority.</text>
                  </description>
                  <leaf name="level">
                    <type name="level-type-two"/>
                    <description>
                      <text>Set a level type for the route priority.</text>
                    </description>
                  </leaf>
                  <leaf name="priority">
                    <type name="priority-type"/>
                    <description>
                      <text>Set a route priority.</text>
                    </description>
                  </leaf>
                  <choice name="policy-flag">
                    <mandatory value="true"/>
                    <description>
                      <text>Set a filtering policy for the route priority.</text>
                    </description>
                    <case name="tag">
                      <description>
                        <text>Set a tag for the route priority.</text>
                      </description>
                      <leaf name="tag">
                        <type name="uint32">
                          <range value="1..4294967295"/>
                        </type>
                        <description>
                          <text>Set a tag for the route priority.</text>
                        </description>
                      </leaf>
                    </case>
                    <case name="ip-prefix">
                      <description>
                        <text>Set the name of an IP prefix list.</text>
                      </description>
                      <leaf name="ip-prefix">
                        <when condition="../../../type='afipv4'"/>
                        <type name="leafref">
                          <path value="/rtp:routing-policy/rtp:ipv4-prefix-filters/rtp:ipv4-prefix-filter/rtp:name"/>
                        </type>
                        <description>
                          <text>Set the name of an IP prefix list.</text>
                        </description>
                      </leaf>
                    </case>
                    <case name="ipv6-prefix">
                      <description>
                        <text>Set the name of an IPv6 prefix list.</text>
                      </description>
                      <leaf name="ipv6-prefix">
                        <when condition="../../../type='afipv6'"/>
                        <type name="leafref">
                          <path value="/rtp:routing-policy/rtp:ipv6-prefix-filters/rtp:ipv6-prefix-filter/rtp:name"/>
                        </type>
                        <description>
                          <text>Set the name of an IPv6 prefix list.</text>
                        </description>
                      </leaf>
                    </case>
                  </choice>
                </list>
              </container>
              <container name="frr">
                <description>
                  <text>Configure FRR.</text>
                </description>
                <leaf name="enable">
                  <type name="boolean"/>
                  <default value="false"/>
                  <description>
                    <text>Enable/disable FRR.</text>
                  </description>
                </leaf>
                <choice name="policy-type">
                  <description>
                    <text>Type of frr policy route.</text>
                  </description>
                  <case name="route-policy">
                    <description>
                      <text>Set a route policy name.</text>
                    </description>
                    <leaf name="route-policy">
                      <when condition="../enable='true'"/>
                      <type name="leafref">
                        <path value="/rtp:routing-policy/rtp:policy-definitions/rtp:policy-definition/rtp:name"/>
                      </type>
                      <description>
                        <text>Set a route policy name.</text>
                      </description>
                    </leaf>
                  </case>
                  <case name="route-filter">
                    <description>
                      <text>Set a route filter name.</text>
                    </description>
                    <leaf name="route-filter">
                      <when condition="../enable='true'"/>
                      <type name="leafref">
                        <path value="/xpl:xpl/xpl:route-filters/xpl:route-filter/xpl:name"/>
                      </type>
                      <description>
                        <text>Set a route filter name.</text>
                      </description>
                    </leaf>
                  </case>
                </choice>
                <leaf name="lfa-level1">
                  <when condition="../enable='true'"/>
                  <type name="boolean"/>
                  <default value="false"/>
                  <description>
                    <text>Enable/disable ISIS auto FRR of Level 1 and calculate loop free alternate routes using the LFA arithmetic.</text>
                  </description>
                </leaf>
                <leaf name="lfa-level2">
                  <when condition="../enable='true'"/>
                  <type name="boolean"/>
                  <default value="false"/>
                  <description>
                    <text>Enable/disable ISIS auto FRR of Level 2 and calculate loop free alternate routes using the LFA arithmetic.</text>
                  </description>
                </leaf>
                <leaf name="ecmp-level1">
                  <when condition="../enable='true'"/>
                  <type name="boolean"/>
                  <default value="true"/>
                  <description>
                    <text>Enable/disable Level 1 ECMP FRR and calculate backup paths for load sharing paths.</text>
                  </description>
                </leaf>
                <leaf name="ecmp-level2">
                  <when condition="../enable='true'"/>
                  <type name="boolean"/>
                  <default value="true"/>
                  <description>
                    <text>Enable/disable Level 2 ECMP FRR and calculate backup paths for load sharing paths.</text>
                  </description>
                </leaf>
                <container name="remote-lfa">
                  <when condition="../../../../../../../ni:name='_public_'"/>
                  <description>
                    <text>Configure RLFA.</text>
                  </description>
                  <leaf name="rlfa-level1">
                    <type name="boolean"/>
                    <must condition="../rlfa-level1='false' or (../../../type='afipv6' and ../../../../../afs/af[type='afipv4']/frr/remote-lfa/rlfa-level1='true') or (../../../type='afipv4' and ../../lfa-level1='true')"/>
                    <default value="false"/>
                    <description>
                      <text>Enable/disable level 1 remote LFA.</text>
                    </description>
                  </leaf>
                  <leaf name="rlfa-level2">
                    <type name="boolean"/>
                    <must condition="../rlfa-level2='false' or (../../../type='afipv6' and ../../../../../afs/af[type='afipv4']/frr/remote-lfa/rlfa-level2='true') or (../../../type='afipv4' and ../../lfa-level2='true')"/>
                    <default value="false"/>
                    <description>
                      <text>Enable/disable level 2 remote LFA.</text>
                    </description>
                  </leaf>
                  <leaf name="max-cost-level1">
                    <when condition="../../../type='afipv4' and ../rlfa-level1='true'"/>
                    <type name="uint32">
                      <range value="1..4261412864"/>
                    </type>
                    <default value="4261412864"/>
                    <description>
                      <text>Remote-lfa tunnel ldp maximum-reachable-cost level-1.</text>
                    </description>
                  </leaf>
                  <leaf name="max-cost-level2">
                    <when condition="../../../type='afipv4' and ../rlfa-level2='true'"/>
                    <type name="uint32">
                      <range value="1..4261412864"/>
                    </type>
                    <default value="4261412864"/>
                    <description>
                      <text>Remote-lfa tunnel ldp maximum-reachable-cost level-2.</text>
                    </description>
                  </leaf>
                  <leaf name="ip-prefix-level1">
                    <when condition="../../../type='afipv4' and ../../enable='true'"/>
                    <type name="leafref">
                      <path value="/rtp:routing-policy/rtp:ipv4-prefix-filters/rtp:ipv4-prefix-filter/rtp:name"/>
                    </type>
                    <description>
                      <text>Filter PQ node. Those nodes which don't pass filter can't be PQ node.</text>
                    </description>
                  </leaf>
                  <leaf name="ip-prefix-level2">
                    <when condition="../../../type='afipv4' and ../../enable='true'"/>
                    <type name="leafref">
                      <path value="/rtp:routing-policy/rtp:ipv4-prefix-filters/rtp:ipv4-prefix-filter/rtp:name"/>
                    </type>
                    <description>
                      <text>Filter PQ node. Those nodes which don't pass filter can't be PQ node.</text>
                    </description>
                  </leaf>
                </container>
                <container name="tiebreaker">
                  <description>
                    <text>Configure a Rule of Selecting a Backup Path for IS-IS Auto FRR.</text>
                  </description>
                  <leaf name="node-protect-level1">
                    <type name="uint8">
                      <range value="1..255"/>
                    </type>
                    <must condition="../../lfa-level1='true' or ../node-protect-level1=40"/>
                    <default value="40"/>
                    <description>
                      <text>Set the value of level1 node protect prefer, the default value is 40.</text>
                    </description>
                  </leaf>
                  <leaf name="node-protect-level2">
                    <type name="uint8">
                      <range value="1..255"/>
                    </type>
                    <must condition="../../lfa-level2='true' or ../node-protect-level2=40"/>
                    <default value="40"/>
                    <description>
                      <text>Set the value of level2 node protect prefer, the default value is 40.</text>
                    </description>
                  </leaf>
                  <leaf name="lowest-cost-level1">
                    <type name="uint8">
                      <range value="1..255"/>
                    </type>
                    <must condition="../../lfa-level1='true' or ../lowest-cost-level1=20"/>
                    <default value="20"/>
                    <description>
                      <text>Set the value of level1 lowcost prefer, the default value is 20.</text>
                    </description>
                  </leaf>
                  <leaf name="lowest-cost-level2">
                    <type name="uint8">
                      <range value="1..255"/>
                    </type>
                    <must condition="../../lfa-level2='true' or ../lowest-cost-level2=20"/>
                    <default value="20"/>
                    <description>
                      <text>Set the value of level2 lowcost prefer, the default value is 20.</text>
                    </description>
                  </leaf>
                  <leaf name="non-ecmp-level1">
                    <type name="uint8">
                      <range value="1..255"/>
                    </type>
                    <must condition="../../lfa-level1='true' or ../non-ecmp-level1=10"/>
                    <default value="10"/>
                    <description>
                      <text>Set the value of level1 non ecmp path prefer, the default value is 10.</text>
                    </description>
                  </leaf>
                  <leaf name="non-ecmp-level2">
                    <type name="uint8">
                      <range value="1..255"/>
                    </type>
                    <must condition="../../lfa-level2='true' or ../non-ecmp-level2=10"/>
                    <default value="10"/>
                    <description>
                      <text>Set the value of level2 non ecmp path prefer, the default value is 10.</text>
                    </description>
                  </leaf>
                  <leaf name="srlg-disjoint-level1">
                    <type name="uint8">
                      <range value="1..255"/>
                    </type>
                    <must condition="../../lfa-level1='true' or ../srlg-disjoint-level1=5"/>
                    <default value="5"/>
                    <description>
                      <text>Set the value of level1 srlg-disjoint path prefer, the default value is 5.</text>
                    </description>
                  </leaf>
                  <leaf name="srlg-disjoint-level2">
                    <type name="uint8">
                      <range value="1..255"/>
                    </type>
                    <must condition="../../lfa-level2='true' or ../srlg-disjoint-level2=5"/>
                    <default value="5"/>
                    <description>
                      <text>Set the value of level2 srlg-disjoint path prefer, the default value is 5.</text>
                    </description>
                  </leaf>
                  <leaf name="hold-max-cost-level1">
                    <type name="uint8">
                      <range value="1..255"/>
                    </type>
                    <must condition="../../lfa-level1='true' or ../hold-max-cost-level1=15"/>
                    <default value="15"/>
                    <description>
                      <text>Set the value of level1 hold max cost link prefer.</text>
                    </description>
                  </leaf>
                  <leaf name="hold-max-cost-level2">
                    <type name="uint8">
                      <range value="1..255"/>
                    </type>
                    <must condition="../../lfa-level2='true' or ../hold-max-cost-level2=15"/>
                    <default value="15"/>
                    <description>
                      <text>Set the value of level2 hold max cost link prefer.</text>
                    </description>
                  </leaf>
                </container>
                <container name="tilfa">
                  <when condition="../../../../../../../ni:name='_public_'"/>
                  <description>
                    <text>Configure TILFA.</text>
                  </description>
                  <leaf name="enable-level1">
                    <type name="boolean"/>
                    <must condition="../../lfa-level1='true' or ../enable-level1='false'"/>
                    <default value="false"/>
                    <description>
                      <text>Enable/disable Level 1 TILFA.</text>
                    </description>
                  </leaf>
                  <leaf name="enable-level2">
                    <type name="boolean"/>
                    <must condition="../../lfa-level2='true' or ../enable-level2='false'"/>
                    <default value="false"/>
                    <description>
                      <text>Enable/disable Level 2 TILFA.</text>
                    </description>
                  </leaf>
                </container>
              </container>
              <container name="filter-import">
                <presence value="filter policy import"/>
                <description>
                  <text>Configure filter incoming routing updates.</text>
                </description>
                <choice name="policy-type">
                  <mandatory value="true"/>
                  <description>
                    <text>Mode of the policy used for filter import.</text>
                  </description>
                  <case name="basic-acl4">
                    <description>
                      <text>Apply basic ACL name or ACL number.</text>
                    </description>
                    <leaf name="name-or-num-v4">
                      <when condition="../../type='afipv4'"/>
                      <type name="leafref">
                        <path value="/acl:acl/acl:groups/acl:group/acl:identity"/>
                      </type>
                      <must condition="/acl:acl/acl:groups/acl:group[acl:identity=current()]/acl:type='basic'"/>
                      <description>
                        <text>Apply basic ACL name or ACL number.</text>
                      </description>
                    </leaf>
                  </case>
                  <case name="basic-acl6">
                    <description>
                      <text>Apply basic ACL6 name or ACL6 number.</text>
                    </description>
                    <leaf name="name-or-num-v6">
                      <when condition="../../type='afipv6'"/>
                      <type name="leafref">
                        <path value="/acl:acl/acl:group6s/acl:group6/acl:identity"/>
                      </type>
                      <must condition="/acl:acl/acl:group6s/acl:group6[acl:identity=current()]/acl:type='basic'"/>
                      <description>
                        <text>Apply basic ACL6 name or ACL6 number.</text>
                      </description>
                    </leaf>
                  </case>
                  <case name="ipv4-prefix">
                    <description>
                      <text>Set the name of an IP prefix list.</text>
                    </description>
                    <leaf name="ipv4-prefix">
                      <when condition="../../type='afipv4'"/>
                      <type name="leafref">
                        <path value="/rtp:routing-policy/rtp:ipv4-prefix-filters/rtp:ipv4-prefix-filter/rtp:name"/>
                      </type>
                      <description>
                        <text>Set the name of an IP prefix list.</text>
                      </description>
                    </leaf>
                  </case>
                  <case name="ipv6-prefix">
                    <description>
                      <text>Set the name of an IPv6 prefix list.</text>
                    </description>
                    <leaf name="ipv6-prefix">
                      <when condition="../../type='afipv6'"/>
                      <type name="leafref">
                        <path value="/rtp:routing-policy/rtp:ipv6-prefix-filters/rtp:ipv6-prefix-filter/rtp:name"/>
                      </type>
                      <description>
                        <text>Set the name of an IPv6 prefix list.</text>
                      </description>
                    </leaf>
                  </case>
                  <case name="route-policy">
                    <description>
                      <text>Set a route policy name.</text>
                    </description>
                    <leaf name="route-policy">
                      <type name="leafref">
                        <path value="/rtp:routing-policy/rtp:policy-definitions/rtp:policy-definition/rtp:name"/>
                      </type>
                      <description>
                        <text>Name of the route filtering policy used during route leaking.</text>
                      </description>
                    </leaf>
                  </case>
                  <case name="route-filter">
                    <description>
                      <text>Set a route filter name.</text>
                    </description>
                    <leaf name="route-filter">
                      <type name="leafref">
                        <path value="/xpl:xpl/xpl:route-filters/xpl:route-filter/xpl:name"/>
                      </type>
                      <description>
                        <text>Set a route filter name.</text>
                      </description>
                    </leaf>
                  </case>
                </choice>
              </container>
              <container name="filter-exports">
                <description>
                  <text>List of filter outgoing routing update.</text>
                </description>
                <list name="filter-export">
                  <key value="protocol protocol-id"/>
                  <description>
                    <text>Configure filter outgoing routing update.</text>
                  </description>
                  <leaf name="protocol">
                    <type name="filter-export-type"/>
                    <must condition="not(../protocol='ospfv3' or ../protocol='ripng') or (../../../type='afipv6' and (../protocol='ospfv3' or ../protocol='ripng'))"/>
                    <description>
                      <text>Protocol that advertises routes.</text>
                    </description>
                  </leaf>
                  <leaf name="protocol-id">
                    <type name="uint32">
                      <range value="0..4294967295"/>
                    </type>
                    <must condition="(../protocol-id=0 and (../protocol='direct' or ../protocol='static' or ../protocol='bgp' or ../protocol='all' or ../protocol='unr')) or (../protocol-id&gt;0 and (../protocol='ospf' or ../protocol='rip' or ../protocol='ospfv3' or ../protocol='ripng' or (../protocol='isis' and ../protocol-id!=../../../../../id)))"/>
                    <description>
                      <text>Process ID when the advertised protocol is RIP, ISIS, or OSPF.</text>
                    </description>
                  </leaf>
                  <choice name="policy-type">
                    <mandatory value="true"/>
                    <description>
                      <text>Mode of the policy used for filter export.</text>
                    </description>
                    <case name="basic-acl4">
                      <description>
                        <text>Apply basic ACL name or ACL number.</text>
                      </description>
                      <leaf name="name-or-num-v4">
                        <when condition="../../../type='afipv4'"/>
                        <type name="leafref">
                          <path value="/acl:acl/acl:groups/acl:group/acl:identity"/>
                        </type>
                        <must condition="/acl:acl/acl:groups/acl:group[acl:identity=current()]/acl:type='basic'"/>
                        <description>
                          <text>Apply basic ACL name or ACL number.</text>
                        </description>
                      </leaf>
                    </case>
                    <case name="basic-acl6">
                      <description>
                        <text>Apply basic ACL6 name or ACL6 number.</text>
                      </description>
                      <leaf name="name-or-num-v6">
                        <when condition="../../../type='afipv6'"/>
                        <type name="leafref">
                          <path value="/acl:acl/acl:group6s/acl:group6/acl:identity"/>
                        </type>
                        <must condition="/acl:acl/acl:group6s/acl:group6[acl:identity=current()]/acl:type='basic'"/>
                        <description>
                          <text>Apply basic ACL6 name or ACL6 number.</text>
                        </description>
                      </leaf>
                    </case>
                    <case name="ipv4-prefix">
                      <description>
                        <text>Set the name of an IP prefix list.</text>
                      </description>
                      <leaf name="ipv4-prefix">
                        <when condition="../../../type='afipv4'"/>
                        <type name="leafref">
                          <path value="/rtp:routing-policy/rtp:ipv4-prefix-filters/rtp:ipv4-prefix-filter/rtp:name"/>
                        </type>
                        <description>
                          <text>Set the name of an IP prefix list.</text>
                        </description>
                      </leaf>
                    </case>
                    <case name="ipv6-prefix">
                      <description>
                        <text>Set the name of an IPv6 prefix list.</text>
                      </description>
                      <leaf name="ipv6-prefix">
                        <when condition="../../../type='afipv6'"/>
                        <type name="leafref">
                          <path value="/rtp:routing-policy/rtp:ipv6-prefix-filters/rtp:ipv6-prefix-filter/rtp:name"/>
                        </type>
                        <description>
                          <text>Set the name of an IPv6 prefix list.</text>
                        </description>
                      </leaf>
                    </case>
                    <case name="route-policy">
                      <description>
                        <text>Set a route policy name.</text>
                      </description>
                      <leaf name="route-policy">
                        <type name="leafref">
                          <path value="/rtp:routing-policy/rtp:policy-definitions/rtp:policy-definition/rtp:name"/>
                        </type>
                        <description>
                          <text>Name of the route filtering policy used during route leaking.</text>
                        </description>
                      </leaf>
                    </case>
                    <case name="route-filter">
                      <description>
                        <text>Set a route filter name.</text>
                      </description>
                      <leaf name="route-filter">
                        <type name="leafref">
                          <path value="/xpl:xpl/xpl:route-filters/xpl:route-filter/xpl:name"/>
                        </type>
                        <description>
                          <text>Set a route filter name.</text>
                        </description>
                      </leaf>
                    </case>
                  </choice>
                </list>
              </container>
              <container name="topologys">
                <when condition="../../../../../../ni:name='_public_'"/>
                <description>
                  <text>List of multi topology for an ISIS process.</text>
                </description>
                <list name="topology">
                  <key value="mt-id"/>
                  <unique tag="topo-name"/>
                  <max-elements value="62"/>
                  <description>
                    <text>Configure multi topology for an ISIS process.</text>
                  </description>
                  <leaf name="mt-id">
                    <type name="uint32">
                      <range value="3..4095"/>
                    </type>
                    <must condition="(../../../type='afipv4' and (../mt-id=3 and ../topo-name='multicast')) or (../../../type='afipv6' and (../mt-id=4 and ../topo-name='multicast')) or (../mt-id&gt;=6 and ../topo-name)"/>
                    <must condition="(../../../../../cost-style='wide' or ../../../../../cost-style='wtransition')"/>
                    <description>
                      <text>ISIS Multi topology ID.Mid values do not include 5.</text>
                    </description>
                  </leaf>
                  <leaf name="topo-name">
                    <ext:operation-exclude value="update"/>
                    <type name="leafref">
                      <path value="/ni:network-instance/ni:instances/ni:instance/l3vpn:afs/l3vpn:af/rt:routing/rt:routing-manage/rt:topologys/rt:topology/rt:name"/>
                    </type>
                    <must condition="((../mt-id=3 or ../mt-id=4) and ../topo-name='multicast') or (../mt-id&gt;=6 and ../topo-name!='base' and ../topo-name!='multicast')"/>
                    <must condition="(../../../type='afipv4' and /ni:network-instance/ni:instances/ni:instance[ni:name='_public_']/l3vpn:afs/l3vpn:af[l3vpn:type='ipv4-unicast']/rt:routing/rt:routing-manage/rt:topologys/rt:topology[rt:name=current()]) or (../../../type='afipv6' and /ni:network-instance/ni:instances/ni:instance[ni:name='_public_']/l3vpn:afs/l3vpn:af[l3vpn:type='ipv6-unicast']/rt:routing/rt:routing-manage/rt:topologys/rt:topology[rt:name=current()])"/>
                    <mandatory value="true"/>
                    <description>
                      <text>Multi topology name.</text>
                    </description>
                  </leaf>
                  <leaf name="max-load-balancing">
                    <type name="uint32">
                      <range value="1..128"/>
                    </type>
                    <description>
                      <text>Set the maximum number of equal cost routes for load balancing.</text>
                    </description>
                    <ext:dynamic-default/>
                  </leaf>
                  <leaf name="auto-cost">
                    <type name="boolean"/>
                    <default value="false"/>
                    <description>
                      <text>Enable/disable automatic cost calculation for the system.</text>
                    </description>
                  </leaf>
                  <leaf name="auto-cost-compatible">
                    <when condition="../auto-cost='true'"/>
                    <type name="boolean"/>
                    <default value="false"/>
                    <description>
                      <text>Enable/disable automatic cost calculation compatibly for the system.</text>
                    </description>
                  </leaf>
                  <leaf name="bandwidth">
                    <type name="uint32">
                      <range value="1..2147483648"/>
                    </type>
                    <default value="100"/>
                    <description>
                      <text>Set the bandwidth reference used for the automatic cost calculation on an interface.</text>
                    </description>
                  </leaf>
                  <leaf name="cost-level1">
                    <type name="uint32">
                      <range value="1..16777215"/>
                    </type>
                    <must condition="(../cost-level1&lt;=63) or (not(../../../../../cost-style!='wide' and ../../../../../cost-style!='wtransition') and ../cost-level1&lt;=16777215)"/>
                    <description>
                      <text>Set the level 1 cost for an interface.</text>
                    </description>
                  </leaf>
                  <leaf name="cost-level2">
                    <type name="uint32">
                      <range value="1..16777215"/>
                    </type>
                    <must condition="(../cost-level2&lt;=63) or (not(../../../../../cost-style!='wide' and ../../../../../cost-style!='wtransition') and ../cost-level2&lt;=16777215)"/>
                    <description>
                      <text>Set the level 2 cost for an interface.</text>
                    </description>
                  </leaf>
                  <container name="attach-bit">
                    <description>
                      <text>Configure LSPs with the ATT bit.</text>
                    </description>
                    <leaf name="adv-control">
                      <type name="att-adv-mode"/>
                      <default value="no-type"/>
                      <description>
                        <text>Control to advertise the ATT bit.</text>
                      </description>
                    </leaf>
                    <leaf name="avoid-learn">
                      <type name="boolean"/>
                      <default value="false"/>
                      <description>
                        <text>Enable/disable prevent the device from learning the LSPs with the ATT bit.</text>
                      </description>
                    </leaf>
                  </container>
                  <container name="preference">
                    <description>
                      <text>Configure a priority for ISIS.</text>
                    </description>
                    <leaf name="value">
                      <type name="uint32">
                        <range value="1..255"/>
                      </type>
                      <default value="15"/>
                      <description>
                        <text>Set the priority for protocols. The smaller the value, the higher the priority.</text>
                      </description>
                    </leaf>
                    <choice name="policy-type">
                      <description>
                        <text>Type of preference rule.</text>
                      </description>
                      <case name="route-policy">
                        <description>
                          <text>Set a route policy name.</text>
                        </description>
                        <leaf name="route-policy">
                          <type name="leafref">
                            <path value="/rtp:routing-policy/rtp:policy-definitions/rtp:policy-definition/rtp:name"/>
                          </type>
                          <description>
                            <text>Set a route policy name.</text>
                          </description>
                        </leaf>
                      </case>
                      <case name="route-filter">
                        <description>
                          <text>Set a route filter name.</text>
                        </description>
                        <leaf name="route-filter">
                          <type name="leafref">
                            <path value="/xpl:xpl/xpl:route-filters/xpl:route-filter/xpl:name"/>
                          </type>
                          <description>
                            <text>Set a route filter name.</text>
                          </description>
                        </leaf>
                      </case>
                    </choice>
                  </container>
                  <container name="default-route">
                    <presence value="Enable default routes."/>
                    <description>
                      <text>Configure the level 1 and level 2 routers to generate default routes.</text>
                    </description>
                    <leaf name="mode">
                      <type name="default-mode"/>
                      <must condition="../mode!='match-any' or (../mode='match-any' and (../route-policy-name or ../route-filter-name))"/>
                      <default value="always"/>
                      <description>
                        <text>Set the default routes mode. When the mode is not match any, route policy name and route filter name cannot be configured.</text>
                      </description>
                    </leaf>
                    <choice name="policy-type">
                      <description>
                        <text>Mode of the policy used for default route.</text>
                      </description>
                      <case name="route-policy-name">
                        <description>
                          <text>Name of a routing policy.</text>
                        </description>
                        <leaf name="route-policy-name">
                          <when condition="../mode='match-any'"/>
                          <type name="leafref">
                            <path value="/rtp:routing-policy/rtp:policy-definitions/rtp:policy-definition/rtp:name"/>
                          </type>
                          <description>
                            <text>Set a route policy name for default routes.</text>
                          </description>
                        </leaf>
                      </case>
                      <case name="route-filter-name">
                        <description>
                          <text>Name of a routing filter.</text>
                        </description>
                        <leaf name="route-filter-name">
                          <when condition="../mode='match-any'"/>
                          <type name="leafref">
                            <path value="/xpl:xpl/xpl:route-filters/xpl:route-filter/xpl:name"/>
                          </type>
                          <description>
                            <text>Set a xpl route policy name entity for default routes.</text>
                          </description>
                        </leaf>
                      </case>
                    </choice>
                    <leaf name="cost">
                      <type name="uint32">
                        <range value="0..4261412864"/>
                      </type>
                      <default value="0"/>
                      <description>
                        <text>Set a cost for default routes. If the ISIS cost type is narrow, narrow-compatible, or compatible, the value ranges from 1 to 63.</text>
                      </description>
                    </leaf>
                    <leaf name="tag">
                      <type name="uint32">
                        <range value="0..4294967295"/>
                      </type>
                      <default value="0"/>
                      <description>
                        <text>Set a tag for default routes.</text>
                      </description>
                    </leaf>
                    <leaf name="level-type">
                      <type name="level-type"/>
                      <default value="level-2"/>
                      <description>
                        <text>Set a level for default routes.</text>
                      </description>
                    </leaf>
                    <leaf name="avoid-learning">
                      <type name="boolean"/>
                      <default value="false"/>
                      <description>
                        <text>Enable/disable to prevent ISIS processes from learning default routes and adding the default routes to the routing table. If the default routes that already exist in the routing table are active, the status of the route is set to inactive.</text>
                      </description>
                    </leaf>
                  </container>
                  <container name="import-routes">
                    <description>
                      <text>List of import route of other routing protocols.</text>
                    </description>
                    <list name="import-route">
                      <key value="protocol process-id"/>
                      <description>
                        <text>Configure import route of other routing protocols.</text>
                      </description>
                      <leaf name="protocol">
                        <type name="import-protocol"/>
                        <must condition="../protocol='direct' or ../protocol='isis' or ../protocol='static'"/>
                        <description>
                          <text>Set the routing protocol from which routes are imported.</text>
                        </description>
                      </leaf>
                      <leaf name="process-id">
                        <type name="uint32">
                          <range value="0..4294967295"/>
                        </type>
                        <must condition="../process-id=0 and (../protocol='direct' or ../protocol='static') or ../process-id&gt;0 and (../protocol='isis')"/>
                        <must condition="not (../process-id=../../../../../../../id and ../protocol='isis')"/>
                        <description>
                          <text>Set the process ID. The process ID is required when the routing protocol is ISIS.</text>
                        </description>
                      </leaf>
                      <leaf name="cost">
                        <when condition="../inherit-cost='false'"/>
                        <type name="uint32">
                          <range value="0..4261412864"/>
                        </type>
                        <default value="0"/>
                        <description>
                          <text>Set a cost for imported routes. If the ISIS cost type is narrow, narrow-compatible, or compatible, the value ranges from 1 to 63.</text>
                        </description>
                      </leaf>
                      <leaf name="tag">
                        <type name="uint32">
                          <range value="0..4294967295"/>
                        </type>
                        <default value="0"/>
                        <description>
                          <text>Set a tag for imported routes. A value of 0 indicates disable.</text>
                        </description>
                      </leaf>
                      <choice name="policy-type">
                        <description>
                          <text>Mode of the policy used for import route.</text>
                        </description>
                        <case name="route-policy-name">
                          <description>
                            <text>Set a route policy name.</text>
                          </description>
                          <leaf name="route-policy-name">
                            <type name="leafref">
                              <path value="/rtp:routing-policy/rtp:policy-definitions/rtp:policy-definition/rtp:name"/>
                            </type>
                            <description>
                              <text>Set a route policy name.</text>
                            </description>
                          </leaf>
                        </case>
                        <case name="route-filter-name">
                          <description>
                            <text>Name of a routing filter.</text>
                          </description>
                          <leaf name="route-filter-name">
                            <type name="leafref">
                              <path value="/xpl:xpl/xpl:route-filters/xpl:route-filter/xpl:name"/>
                            </type>
                            <description>
                              <text>Specify a route filter.</text>
                            </description>
                          </leaf>
                          <leaf name="parameter">
                            <when condition="../route-filter-name"/>
                            <type name="xpl:filter-parameter-type"/>
                            <description>
                              <text>Specify an import route-filter parameter.</text>
                            </description>
                          </leaf>
                        </case>
                      </choice>
                      <leaf name="level-type">
                        <type name="level-type"/>
                        <default value="level-2"/>
                        <description>
                          <text>Import routes to the level 1 or (and) level 2 routing table.</text>
                        </description>
                      </leaf>
                      <leaf name="inherit-cost">
                        <type name="boolean"/>
                        <must condition="../inherit-cost='false' or ../protocol!='static'"/>
                        <default value="false"/>
                        <description>
                          <text>Enable/disable keep the original cost of a route when importing the route to ISIS.</text>
                        </description>
                      </leaf>
                    </list>
                  </container>
                  <container name="leak-route-level2-to-level1">
                    <presence value="Leak route level2 to level1."/>
                    <description>
                      <text>Configure to control ISIS to perform route leaking from level 2 to level 1. This command needs to be set on the level 1 2 router connected to the external areas.</text>
                    </description>
                    <leaf name="tag">
                      <type name="uint32">
                        <range value="0..4294967295"/>
                      </type>
                      <default value="0"/>
                      <description>
                        <text>Management label value assigned for imported routes. The label value is the name of route filtering during route leaking. A value of 0 indicates disable.</text>
                      </description>
                    </leaf>
                    <choice name="policy-type">
                      <description>
                        <text>Mode of the policy used for route leaking.</text>
                      </description>
                      <case name="basic-acl4">
                        <description>
                          <text>Apply basic ACL name or ACL number.</text>
                        </description>
                        <leaf name="name-or-num-v4">
                          <when condition="../../../../type='afipv4'"/>
                          <type name="leafref">
                            <path value="/acl:acl/acl:groups/acl:group/acl:identity"/>
                          </type>
                          <must condition="/acl:acl/acl:groups/acl:group[acl:identity=current()]/acl:type='basic'"/>
                          <description>
                            <text>Apply basic ACL name or ACL number.</text>
                          </description>
                        </leaf>
                      </case>
                      <case name="basic-acl6">
                        <description>
                          <text>Apply basic ACL6 name or ACL6 number.</text>
                        </description>
                        <leaf name="name-or-num-v6">
                          <when condition="../../../../type='afipv6'"/>
                          <type name="leafref">
                            <path value="/acl:acl/acl:group6s/acl:group6/acl:identity"/>
                          </type>
                          <must condition="/acl:acl/acl:group6s/acl:group6[acl:identity=current()]/acl:type='basic'"/>
                          <description>
                            <text>Apply basic ACL6 name or ACL6 number.</text>
                          </description>
                        </leaf>
                      </case>
                      <case name="ipv4-prefix">
                        <description>
                          <text>Set the name of an IP prefix list.</text>
                        </description>
                        <leaf name="ipv4-prefix">
                          <when condition="../../../../type='afipv4'"/>
                          <type name="leafref">
                            <path value="/rtp:routing-policy/rtp:ipv4-prefix-filters/rtp:ipv4-prefix-filter/rtp:name"/>
                          </type>
                          <description>
                            <text>Set the name of an IP prefix list.</text>
                          </description>
                        </leaf>
                      </case>
                      <case name="ipv6-prefix">
                        <description>
                          <text>Set the name of an IPv6 prefix list.</text>
                        </description>
                        <leaf name="ipv6-prefix">
                          <when condition="../../../../type='afipv6'"/>
                          <type name="leafref">
                            <path value="/rtp:routing-policy/rtp:ipv6-prefix-filters/rtp:ipv6-prefix-filter/rtp:name"/>
                          </type>
                          <description>
                            <text>Set the name of an IPv6 prefix list.</text>
                          </description>
                        </leaf>
                      </case>
                      <case name="route-policy-name">
                        <description>
                          <text>Set a route policy name.</text>
                        </description>
                        <leaf name="route-policy-name">
                          <type name="leafref">
                            <path value="/rtp:routing-policy/rtp:policy-definitions/rtp:policy-definition/rtp:name"/>
                          </type>
                          <description>
                            <text>Name of the route filtering policy used during route leaking.</text>
                          </description>
                        </leaf>
                      </case>
                      <case name="route-filter-name">
                        <description>
                          <text>Set a route filter name.</text>
                        </description>
                        <leaf name="route-filter-name">
                          <type name="leafref">
                            <path value="/xpl:xpl/xpl:route-filters/xpl:route-filter/xpl:name"/>
                          </type>
                          <description>
                            <text>Set a route filter name.</text>
                          </description>
                        </leaf>
                      </case>
                    </choice>
                    <leaf name="allow-filter">
                      <type name="boolean"/>
                      <default value="false"/>
                      <description>
                        <text>Enable/disable the name of a route policy which filters routes based on the tag or other protocol parameters.</text>
                      </description>
                    </leaf>
                    <leaf name="allow-updown">
                      <type name="boolean"/>
                      <default value="false"/>
                      <description>
                        <text>Enable/disable the Up/Down bit for routes.</text>
                      </description>
                    </leaf>
                  </container>
                  <container name="leak-route-level1-to-level2">
                    <description>
                      <text>Configure the leaking of the routes from a level 1 area to a level 2 area. This is convenient for route management. This command needs to be configured on the level 1 2 router connected to external areas.</text>
                    </description>
                    <leaf name="enable">
                      <type name="boolean"/>
                      <default value="true"/>
                      <description>
                        <text>Enable/disable level 1 to level 2 route filtering.</text>
                      </description>
                    </leaf>
                    <choice name="policy-type">
                      <description>
                        <text>Type of filter policy rule.</text>
                      </description>
                      <case name="basic-acl4">
                        <description>
                          <text>Apply basic ACL name or ACL number.</text>
                        </description>
                        <leaf name="name-or-num-v4">
                          <when condition="../enable='true' and ../../../../type='afipv4'"/>
                          <type name="leafref">
                            <path value="/acl:acl/acl:groups/acl:group/acl:identity"/>
                          </type>
                          <must condition="/acl:acl/acl:groups/acl:group[acl:identity=current()]/acl:type='basic'"/>
                          <description>
                            <text>Apply basic ACL name or ACL number.</text>
                          </description>
                        </leaf>
                      </case>
                      <case name="basic-acl6">
                        <description>
                          <text>Apply basic ACL6 name or ACL6 number.</text>
                        </description>
                        <leaf name="name-or-num-v6">
                          <when condition="../enable='true' and ../../../../type='afipv6'"/>
                          <type name="leafref">
                            <path value="/acl:acl/acl:group6s/acl:group6/acl:identity"/>
                          </type>
                          <must condition="/acl:acl/acl:group6s/acl:group6[acl:identity=current()]/acl:type='basic'"/>
                          <description>
                            <text>Apply basic ACL6 name or ACL6 number.</text>
                          </description>
                        </leaf>
                      </case>
                      <case name="ipv4-prefix">
                        <description>
                          <text>Set the name of an IP prefix list.</text>
                        </description>
                        <leaf name="ipv4-prefix">
                          <when condition="../enable='true' and ../../../../type='afipv4'"/>
                          <type name="leafref">
                            <path value="/rtp:routing-policy/rtp:ipv4-prefix-filters/rtp:ipv4-prefix-filter/rtp:name"/>
                          </type>
                          <description>
                            <text>Set the name of an IP prefix list.</text>
                          </description>
                        </leaf>
                      </case>
                      <case name="ipv6-prefix">
                        <description>
                          <text>Set the name of an IPv6 prefix list.</text>
                        </description>
                        <leaf name="ipv6-prefix">
                          <when condition="../enable='true' and ../../../../type='afipv6'"/>
                          <type name="leafref">
                            <path value="/rtp:routing-policy/rtp:ipv6-prefix-filters/rtp:ipv6-prefix-filter/rtp:name"/>
                          </type>
                          <description>
                            <text>Set the name of an IPv6 prefix list.</text>
                          </description>
                        </leaf>
                      </case>
                      <case name="route-policy">
                        <description>
                          <text>Set a route policy name.</text>
                        </description>
                        <leaf name="route-policy">
                          <when condition="../enable='true'"/>
                          <type name="leafref">
                            <path value="/rtp:routing-policy/rtp:policy-definitions/rtp:policy-definition/rtp:name"/>
                          </type>
                          <description>
                            <text>Set a route policy name.</text>
                          </description>
                        </leaf>
                      </case>
                      <case name="route-filter">
                        <description>
                          <text>Set a route filter name.</text>
                        </description>
                        <leaf name="route-filter">
                          <when condition="../enable='true'"/>
                          <type name="leafref">
                            <path value="/xpl:xpl/xpl:route-filters/xpl:route-filter/xpl:name"/>
                          </type>
                          <description>
                            <text>Set a route filter name.</text>
                          </description>
                        </leaf>
                      </case>
                    </choice>
                    <leaf name="tag">
                      <when condition="../enable='true'"/>
                      <type name="uint32">
                        <range value="0..4294967295"/>
                      </type>
                      <default value="0"/>
