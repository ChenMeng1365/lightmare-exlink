            </leaf>
            <leaf name="in-broadcast-bytes">
              <type name="uint64"/>
              <units name="Byte"/>
              <description>
                <text>Number of broadcast bytes received by the PW.</text>
              </description>
            </leaf>
            <leaf name="out-unicast-frames">
              <type name="uint64"/>
              <units name="packet"/>
              <description>
