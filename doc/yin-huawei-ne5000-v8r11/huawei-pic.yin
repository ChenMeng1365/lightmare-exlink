    <container name="phy-serial">
      <must condition="(port-mode = 'v-35' and physical-mode = 'sync') or ((port-mode = 'rs-422' or port-mode = 'rs-485' or port-mode = 'rs-232') and physical-mode = 'async') or ((port-mode = 'fxo' or port-mode = 'fxs' or port-mode = 'em' or port-mode = 'c3794' or port-mode = 'codir') and not(physical-mode)) or (port-mode = 'x-21' or port-mode = 'v-24') or (not(port-mode) and not(physical-mode))"/>
      <description>
        <text>Configure physical serial port.</text>
      </description>
      <leaf name="baudrate">
        <ext:dynamic-default/>
        <when condition="../port-mode = 'v-35' or ../port-mode = 'x-21' or ../port-mode = 'rs-485' or ../port-mode = 'rs-422' or ../port-mode = 'v-24' or ../port-mode = 'rs-232'"/>
        <type name="enumeration">
          <enum name="600bit">
            <value value="0"/>
