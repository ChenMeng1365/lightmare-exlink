module of-config1.1.1 {
  namespace urn:onf:of111:config:yang ;
  prefix of11-config ;
  yang-version 1 ;
  import ietf-yang-types {
    prefix yang ;
  }
  import ietf-inet-types {
    prefix inet ;
  }
  import zxr10-netconf-defs {
    prefix ncd ;
  }
  organization 'ONF Config Management Group'
               ;
  contact tbd ;
  description "NETCONF Operational Considerations\nElements that are configurab"
            + "le, optional and have a default\nvalue MAY be reported by replie"
            + "sto NETCONF <get-config>\nrequests. All non-configurable values "
            + "SHOULD be reported by\nreplies to NETCONF <get> requests.\nAttem"
            + "psto modify non-configurable elements with a NETCONF\n<edit-conf"
            + "ig> operation MUST result in an\n'operation-not-supported' error"
            + " with type 'application'.\nWhen validating an <edit??config> ope"
            + "ration the following\nerrors MUST be detected:\n* Delete request"
            + "s fornon-existent data. In this case a\n'data-missing' error is "
            + "returned.\n* Create requests for existent data. In this case a\n"
            + "'data-exists' error is returned.\n* If the NETCONF operation cre"
            + "ates data nodes under a\n'choice', any existing nodes from other"
            + " branches are\ndeleted."
              ;
  revision 2018-09-10 {
    description 'Add case v6-endpoints and leaf local-endpoint-ipv6-adress, lea'
              + 'f remote-endpoint-ipv6-adress.'
                ;
  }
  revision 2017-08-02 {
    description 'Delete leaf-list node and leaf port and leaf number.'
                ;
  }
  revision 2017-07-05 {
    description 'Add leaf-list node and leaf port and leaf number.'
                ;
  }
  revision 2016-09-06 {
    description 'Add leaf node,capable-switch->logical-switches->resources->cer'
              + 'tificate'
                ;
  }
  revision 2016-07-27 {
    description 'Add port features, include current/advertised/supported/advert'
              + 'ised-peer'
                ;
  }
  revision 2016-07-22 {
    description 'Add list owned-certificate leaf resource-id,leaf certificate,c'
              + 'ontainer private-key,choice key-type,case rsa,leaf modulus,lea'
              + 'f exponent; list external-certificate,leaf resource-id,leaf ce'
              + 'rtificate.'
                ;
  }
  revision 2016-03-16 {
    description 'Add list packets-port-statistics and leaf port->number,name'
                ;
  }
  revision 2011-12-07 {
    description 'First Version'
                ;
    reference tbd ;
  }
  typedef OFConfigId {
    type inet:uri ;
    description 'Generic type of an identifier in OF-CONFIG'
                ;
  }
  typedef OFConfigurationPointProtocolType {
    type enumeration {
      enum ssh {
        value 0 ;
      }
    }
    description 'Possible protocols to connect ot an OF
Configuration Point'
                ;
  }
  typedef OFOpenFlowVersionType {
    type enumeration {
      enum not-applicable {
        value 0 ;
      }
      enum 1.0 {
        value 1 ;
      }
      enum 1.0.1 {
        value 2 ;
      }
      enum 1.1 {
        value 3 ;
      }
      enum 1.2 {
        value 4 ;
      }
      enum 1.3 {
        value 5 ;
      }
      enum 1.3.1 {
        value 6 ;
      }
    }
    description 'This enumeration contains the all OpenFlow'+"\n"
              + 'versions released so far.'
                ;
  }
  typedef datapath-id-type {
    type string {
      pattern '[0-9a-fA-F]{2}(:[0-9a-fA-F]{2}){7}'
              ;
    }
    description 'The datapath-id type represents an OpenFlow'+"\n"
              + 'datapath identifier.'
                ;
  }
  typedef OFTenthOfAPercentType {
    type uint16 {
      range 0..1000 ;
    }
    units '1/10 of a percent'
          ;
    description 'This type defines a value in tenth of a percent.'
                ;
  }
  typedef OFUpDownStateType {
    type enumeration {
      enum up {
        value 0 ;
      }
      enum down {
        value 1 ;
      }
    }
    description 'Type to specify state information for a port or a
connection.'
                ;
  }
  typedef OFPortRateType {
    type enumeration {
      enum 10Mb-HD {
        value 0 ;
      }
      enum 10Mb-FD {
        value 1 ;
      }
      enum 100Mb-HD {
        value 2 ;
      }
      enum 100Mb-FD {
        value 3 ;
      }
      enum 1Gb-HD {
        value 4 ;
      }
      enum 1Gb-FD {
        value 5 ;
      }
      enum 10Gb {
        value 6 ;
      }
      enum 40Gb {
        value 7 ;
      }
      enum 100Gb {
        value 8 ;
      }
      enum 1Tb {
        value 9 ;
      }
      enum other {
        value 10 ;
      }
    }
    description 'Type to specify the rate of a port including the'+"\n"
              + 'duplex transmission feature. Possible rates are 10Mb, 100Mb,'+"\n"
              + '1Gb, 10Gb, 40Gb, 100Gb, 1Tb or other. Rates of 10Mb, 100Mb'+"\n"
              + 'and 1 Gb can support half or full duplex transmission.'
                ;
  }
  typedef OFActionType {
    type enumeration {
      enum output {
        value 0 ;
      }
      enum copy-ttl-out {
        value 1 ;
      }
      enum copy-ttl-in {
        value 2 ;
      }
      enum set-mpls-ttl {
        value 3 ;
      }
      enum dec-mpls-ttl {
        value 4 ;
      }
      enum push-vlan {
        value 5 ;
      }
      enum pop-vlan {
        value 6 ;
      }
      enum push-mpls {
        value 7 ;
      }
      enum pop-mpls {
        value 8 ;
      }
      enum set-queue {
        value 9 ;
      }
      enum group {
        value 10 ;
      }
      enum set-nw-ttl {
        value 11 ;
      }
      enum dec-nw-ttl {
        value 12 ;
      }
      enum set-field {
        value 13 ;
      }
      enum push-pbb {
        value 14 ;
      }
      enum pop-pbb {
        value 15 ;
      }
    }
    description 'The types of actions defined in OpenFlow Switch'+"\n"
              + 'Specification versions 1.2, 1.3, and 1.3.1'
                ;
  }
  typedef OFInstructionType {
    type enumeration {
      enum apply-actions {
        value 0 ;
      }
      enum clear-actions {
        value 1 ;
      }
      enum write-actions {
        value 2 ;
      }
      enum write-metadata {
        value 3 ;
      }
      enum goto-table {
        value 4 ;
      }
      enum meter {
        value 5 ;
      }
      enum experimenter {
        value 6 ;
      }
    }
    description 'The types of instructions defined in OpenFlow'+"\n"
              + 'Switch Specification versions 1.2, 1.3, and 1.3.1.'
                ;
  }
  typedef OFMatchFieldType {
    type enumeration {
      enum input-port {
        value 0 ;
      }
      enum physical-input-port {
        value 1 ;
      }
      enum metadata {
        value 2 ;
      }
      enum ethernet-dest {
        value 3 ;
      }
      enum ethernet-src {
        value 4 ;
      }
      enum ethernet-frame-type {
        value 5 ;
      }
      enum vlan-id {
        value 6 ;
      }
      enum vlan-priority {
        value 7 ;
      }
      enum ip-dscp {
        value 8 ;
      }
      enum ip-ecn {
        value 9 ;
      }
      enum ip-protocol {
        value 10 ;
      }
      enum ipv4-src {
        value 11 ;
      }
      enum ipv4-dest {
        value 12 ;
      }
      enum tcp-src {
        value 13 ;
      }
      enum tcp-dest {
        value 14 ;
      }
      enum udp-src {
        value 15 ;
      }
      enum udp-dest {
        value 16 ;
      }
      enum sctp-src {
        value 17 ;
      }
      enum sctp-dest {
        value 18 ;
      }
      enum icmpv4-type {
        value 19 ;
      }
      enum icmpv4-code {
        value 20 ;
      }
      enum arp-op {
        value 21 ;
      }
      enum arp-src-ip-address {
        value 22 ;
      }
      enum arp-target-ip-address {
        value 23 ;
      }
      enum arp-src-hardware-address {
        value 24 ;
      }
      enum arp-target-hardware-address {
        value 25 ;
      }
      enum ipv6-src {
        value 26 ;
      }
      enum ipv6-dest {
        value 27 ;
      }
      enum ipv6-flow-label {
        value 28 ;
      }
      enum icmpv6-type {
        value 29 ;
      }
      enum icmpv6-code {
        value 30 ;
      }
      enum ipv6-nd-target {
        value 31 ;
      }
      enum ipv6-nd-source-link-layer {
        value 32 ;
      }
      enum ipv6-nd-target-link-layer {
        value 33 ;
      }
      enum mpls-label {
        value 34 ;
      }
      enum mpls-tc {
        value 35 ;
      }
      enum mpls-bos {
        value 36 ;
      }
      enum pbb-isid {
        value 37 ;
      }
      enum tunnel-id {
        value 38 ;
      }
      enum ipv6-exthdr {
        value 39 ;
      }
      enum pbb-uca {
        value 41 ;
      }
      enum tcp-flags {
        value 42 ;
      }
      enum actset_output {
        value 43 ;
      }
      enum packet_type {
        value 44 ;
      }
    }
    description 'The types of match field defined in OpenFlow'+"\n"
              + 'Switch Specification versions 1.2, 1.3, and 1.3.1.'
                ;
  }
  typedef hex-binary {
    type binary ;
    description 'hex binary encoded string'
                ;
    reference 'http://www.w3.org/TR/2004/REC-xmlschema-2-'+"\n"
            + '20041028/datatypes.html#hexBinary'
              ;
  }
  container capable-switch {
    description 'The OpenFlow Capable Switch serves as the root'+"\n"
              + 'element for an OpenFlow configuration. It contains logical'+"\n"
              + 'switches and resources that can be assigned to logical'+"\n"
              + 'switches. It may have relations to OpenFlow Configuration'+"\n"
              + 'Points.'
                ;
    presence capable-switch-0-default ;
    leaf id {
      description "A unique but locally arbitrary identifier that
\nuniquely id"
                + "entifies a Capable Switch within the context of
\npotential "
                + "OpenFlow Configuration Points. It MUST be
\npersistent acros"
                + "s reboots of the OpenFlow Capable Switch.
\nThis element MUS"
                + "T be present in the NETCONF data store.
\nIf this element is"
                + " not present in a NETCONF <edit-config>
\noperation 'create'"
                + ", 'merge' or 'replace' and the parent
\nelement does not exi"
                + "st, a 'data-missing'error is
\nreturned."
                  ;
      type inet:uri {
        length 1..63 ;
      }
      mandatory true ;
    }
    leaf config-version {
      config false ;
      description 'The maximum supported OF-CONFIG version that is'+"\n"
                + 'supported by the OpenFlow Capable Switch. For switches'+"\n"
                + 'implementing this version of the OF-CONFIG protocol this'+"\n"
                + 'MUST always be 1.1.1.'+"\n"
                + 'This object can be used to identify the OF-CONFIG version'+"\n"
                + 'a capable switch supports beginning with version 1.1.1 of'+"\n"
                + 'OF-CONFIG. In addtion the supported version can be'+"\n"
                + 'determined by the namespace the OpenFlow Capable Switch'+"\n"
                + 'returns to configuration request of an element (like'+"\n"
                + 'capable-switch) that is present in all OF-CONFIG versions'+"\n"
                + 'specified so far. This is the only possiblity to identify'+"\n"
                + 'OF-CONFIG versions prior to OF-CONFIG 1.1.1.'
                  ;
      type string {
        length 1..63 ;
      }
    }
    container configuration-points {
      description 'The list of all Configuration Points.'
                  ;
      list configuration-point {
        description "The list of all Configuration Points known to
\nthe OpenFl"
                  + "ow Capable Switch that may manage it using
\nOF-CONFIG.
\n"
                  + "The element 'id' of OFConfigurationType MUST be unique
\nw"
                  + "ithin this list."
                    ;
        key id ;
        leaf id {
          description 'A unique but locally arbitrary identifier that'+"\n"
                    + 'identifies a Configuration Point within the context of a'
                    + 'n'+"\n"
                    + 'OpenFlow Capable Switch.'+"\n"
                    + 'This element MUST be present to identify the configurati'
                    + 'on'+"\n"
                    + 'point.'
                      ;
          type OFConfigId {
            length 1..63 ;
          }
          mandatory true ;
        }
        leaf uri {
          description "A locator of the Configuration Point. It
\nidentifies th"
                    + "e location of the Configuration Point as a
\nservice res"
                    + "ource andMUST include all information necessary
\n
\nfor"
                    + " the OpenFlowCapable Switch to connect to the
\nConfigur"
                    + "ation Point or re-connect to it should it become
\ndisco"
                    + "nnected. Such information MAY include, for example,
\npr"
                    + "otocol, fully qualified domain name, IP address, port
\n"
                    + "number, etc.
\nThis element MUSTbe present in the NETCON"
                    + "F data store.
\nIf this element is not present in a NETC"
                    + "ONF <edit-config>
\noperation 'create', 'merge' or 'repl"
                    + "ace' and the parent
\nelement does not exist, a 'data-mi"
                    + "ssing' error is
\nreturned."
                      ;
          type inet:uri {
            length 1..63 ;
          }
          mandatory true ;
        }
        leaf protocol {
          description "The transport protocol that the Configuration
\nPoint us"
                    + "es when communicating via NETCONF with the OpenFlow
\nCa"
                    + "pable Switch.
\nThis element is optional. If it is not p"
                    + "resent its value
\ndefaults to 'ssh'."
                      ;
          reference 'The mappings of NETCONF to different transport'+"\n"
                  + 'protocols are defined in RFC 6242 for SSH, RFC 4743 for'+"\n"
                  + 'SOAP, RFC 4744 for BEEP, and RFC 5539 for TLS'
                    ;
          type OFConfigurationPointProtocolType ;
          default ssh ;
        }
      }
    }
    container resources {
      description 'A lists containing all resources of the OpenFlow'+"\n"
                + 'Capable Switch that can be used by OpenFlow Logical'+"\n"
                + 'Switches. Resources are listed here independent of their'+"\n"
                + 'actual assignment to OpenFlow Logical Switches. They may'+"\n"
                + 'be available to be assigned to an OpenFlow Logical Switch'+"\n"
                + 'or already in use by an OpenFlow Logical Switch.'
                  ;
      list port {
        description "The list contains all port resources of the
\nOpenFlow Cap"
                  + "able Switch.
\nThe element 'resource-id' of OFPortType MUS"
                  + "T be unique
\nwithin this list."
                    ;
        key resource-id ;
        leaf resource-id {
          description 'A unique but locally arbitrary identifier that'+"\n"
                    + 'uniquely identifies an OpenFlow Port within the context'+"\n"
                    + 'of an OpenFlow Logical Switch. It MUST be persistent'+"\n"
                    + 'across reboots of the OpenFlow Capable Switch.'+"\n"
                    + 'This element MUST be present to identify the OpenFlow'+"\n"
                    + ''+"\n"
                    + 'resource.'
                      ;
          type ncd:ifname {
            length 1..31 ;
          }
          mandatory true ;
        }
        leaf number {
          config false ;
          description 'This number identifies the OpenFlow Port to OpenFlow Con'
                    + 'trollers. It is assigned to an OpenFlow Port latest when'
                    + ' the OpenFlow Port is associated with and OpenFlow Logic'
                    + 'al Switch. If the OpenFlow Port is associated with an Op'
                    + 'enFlow Logical Switch, this element MUST be unique withi'
                    + 'n the context of the OpenFlow Logical Switch. OpenFlow C'
                    + 'apable Switch implementations may choose to assign value'
                    + 's to OpenFlow Ports that are unique within the context o'
                    + 'f the OpenFlow Logical Switch. These numbers can be used'
                    + ' independent of assignments to OpenFlow Logical Switches'
                    + '. Other implementations may assign values to this elemen'
                    + 't only if the OpenFlow Port is assigned to an OpenFlow L'
                    + 'ogical Switch. If no value is currently assigned to this'
                    + ' element then this element MUST NOT be included in repli'
                    + 'es to NETCONF <get> requests. Since this element is not '
                    + 'configurable with the NETCONF protocol it MUST NOT be in'
                    + 'cluded in replies to NETCONF <get-config> requests.'
                      ;
          type uint32 ;
          ncd:revise-date 2016-03-16 ;
        }
        leaf name {
          config false ;
          description 'This element assists OpenFlow Controllers in identifying'
                    + ' OpenFlow Ports. This element is not to be set by the OP'
                    + '-CONFIG protocol, but it is set by the switch implementa'
                    + 'tion. It may be set at start-up time of an OpenFlow Capa'
                    + 'ble Switch or when the OpenFlow Port is assigned to an O'
                    + 'penFlow Logical Switch. It MAY also be not set at all. I'
                    + 'f this element is set to a value other than the empty st'
                    + 'ring when being assigned to an OpenFlow Logical Switch, '
                    + 'then the value of this element MUST be unique within the'
                    + ' context of the OpenFlow Logical Switch. If no value or '
                    + 'the empty string is currently assigned to this element t'
                    + 'hen this element MUST not be included in replies to NETC'
                    + 'ONF <get> requests. Since this element is not configurab'
                    + 'le with the NETCONF protocol it MUST NOT be included in '
                    + 'replies to NETCONF <get-config> requests.'
                      ;
          type string {
            length 1..31 ;
          }
          ncd:revise-date 2016-03-16 ;
        }
        choice tunnel-type {
          description "Tunnels are modeled as logical ports.
\nElements in this"
                    + " choice are not configurable and can only
\nbe retrieved"
                    + " by NETCONF <get> operations. Attemps to modify
\nthis e"
                    + "lement and itschildren with a NETCONF <edit-config>
\nop"
                    + "eration MUST result in an 'operation-not-supported' erro"
                    + "r
\nwith type 'application'.
\nOnly elements from one ch"
                    + "oice must exist at a time."
                      ;
          case vxlan-tunnel {
            description 'Properties of a VxLAN tunnel.'
                        ;
            container vxlan-tunnel {
              description 'Properties of a VxLAN tunnel.'
                          ;
              presence vxlan-tunnel ;
              choice endpoints {
                description 'VxLAN tunnel endpoints.'
                            ;
                mandatory true ;
                case v4-endpoints {
                  description 'VxLAN tunnel IPv4 endpoints.'
                              ;
                  leaf local-endpoint-ipv4-adress {
                    description 'The IPv4 address of the local tunnel endpoint.'
                                ;
                    type inet:ipv4-address ;
                  }
                  leaf remote-endpoint-ipv4-adress {
                    description 'The IPv4 address of the remote tunnel endpoint.'
                                ;
                    type inet:ipv4-address ;
                  }
                }
                case v6-endpoints {
                  description 'VxLAN tunnel IPv6 endpoints.'
                              ;
                  leaf local-endpoint-ipv6-adress {
                    description 'The IPv6 address of the local tunnel endpoint.'
                                ;
                    type inet:ipv6-address ;
                  }
                  leaf remote-endpoint-ipv6-adress {
                    description 'The IPv6 address of the remote tunnel endpoint.'
                                ;
                    type inet:ipv6-address ;
                  }
                  ncd:revise-date 2018-09-10 ;
                }
              }
            }
          }
        }
        container features {
          config false ;
          container current {
            description "The features (rates, duplex, etc.) of the port, that a"
                      + "re currently in use. Children of this element are not "
                      + "configurable and can only be retrieved by NETCONF <get"
                      + "> operations. Attemps to modify this element and its c"
                      + "hildren with a NETCONF <edit-config> operation MUST re"
                      + "sult in an 'operation-not-supported' error with type '"
                      + "application'."
                        ;
            leaf rate {
              description 'The transmission rate that is currently used.The val'
                        + 'ue MUST indicate a valid forwarding rate.The current'
                        + ' Port Feature set MUST contain this element exactly '
                        + 'once. The other Port Feature sets MAY contain this e'
                        + 'lement more than once. If this element appears more '
                        + 'than once in a Port Feature set than the value MUST '
                        + 'be unique within the Port Feature set.'
                          ;
              type OFPortRateType ;
            }
            leaf auto-negotiate {
              description 'Specifies the administrative state of the forwarding'
                        + ' rate auto-negotiation protocol at this OpenFlow Por'
                        + 't.'
                          ;
              type boolean ;
              default true ;
            }
            leaf medium {
              description 'This element MUST indicate a valid physical medium u'
                        + 'sed by the OpenFlow Port. The current Port Feature s'
                        + 'et MUST contain this element exactly once. The other'
                        + ' Port Feature sets MAY contain this element more tha'
                        + 'n once. If this element appears more than once in a '
                        + 'Port Feature set than the value MUST be unique withi'
                        + 'n the Port Feature set.'
                          ;
              type enumeration {
                enum copper {
                  value 0 ;
                }
                enum fiber {
                  value 1 ;
                }
              }
            }
            leaf pause {
              description 'Specifies if pausing of transmission is supported at'
                        + ' all and if yes if it is asymmetric or symmetric.'
                          ;
              type enumeration {
                enum unsupported {
                  value 0 ;
                }
                enum symmetric {
                  value 1 ;
                }
                enum asymmetric {
                  value 2 ;
                }
              }
            }
          }
          container advertised {
            description "The features (rates, duplex, etc.) of the port, that a"
                      + "re advertised to the peer port. NETCONF <edit-config> "
                      + "operations MUST be implemented as follows: * The 'reso"
                      + "urce-id' element of OFResoureType MUST be present in t"
                      + "he path or in the filter at all <edit-config> operatio"
                      + "ns to identify the port. * Ifthe operation is 'merge' "
                      + "or 'replace', the element is created if it does not ex"
                      + "ist, and its value is set to the value found in the XM"
                      + "L RPC data. * If the operation is 'create', the elemen"
                      + "t is created if it does not exist. If the element alre"
                      + "ady exists, a 'data exists' error is returned. * If th"
                      + "e operation is 'delete', the element is deleted if it "
                      + "exists. Ifthe element does not exist, a 'data missing'"
                      + " error is returned."
                        ;
            leaf-list rate {
              description "The transmission rate that is supported or advertise"
                        + "d. Multiple transmissions rates are allowed.At least"
                        + " one element MUST be present in the NETCONF data sto"
                        + "re. If none of this elements is are present in a NET"
                        + "CONF <edit-config> operation 'create', 'merge' or 'r"
                        + "eplace' and the parent element does not exist, a 'da"
                        + "ta-missing' error is returned."
                          ;
              type OFPortRateType ;
              min-elements 1 ;
            }
            leaf auto-negotiate {
              description "Specifies if auto-negotiation of transmission parame"
                        + "ters is enabled for the port. This element is option"
                        + "al. If this element is not present it defaults to 't"
                        + "rue'."
                          ;
              type boolean ;
              default true ;
            }
            leaf-list medium {
              description "The transmission medium used by the port. Multiple m"
                        + "edia are allowed. At least one element MUST be prese"
                        + "nt in the NETCONF data store. If none of this elemen"
                        + "ts is are present ina NETCONF <edit-config> operatio"
                        + "n 'create', 'merge' or 'replace' and the parent elem"
                        + "ent does not exist, a 'data-missing' error is return"
                        + "ed."
                          ;
              type enumeration {
                enum copper {
                  value 0 ;
                }
                enum fiber {
                  value 1 ;
                }
              }
              min-elements 1 ;
            }
            leaf pause {
              description "Specifies if pausing of transmission is supported at"
                        + " all and if yes if it is asymmetric or symmetric. Th"
                        + "is element MUST be present in the NETCONF data store"
                        + ". If this element isnot present in a NETCONF <edit-c"
                        + "onfig> operation 'create','merge' or 'replace' and t"
                        + "he parent element does not exist, a 'data-missing' e"
                        + "rror is returned."
                          ;
              type enumeration {
                enum unsupported {
                  value 0 ;
                }
                enum symmetric {
                  value 1 ;
                }
                enum asymmetric {
                  value 2 ;
                }
              }
              mandatory true ;
            }
          }
          container supported {
            description "The features (rates, duplex, etc.) of the port, that a"
                      + "re supported on the port. Children of this element are"
                      + " not configurable and can only be retrieved by NETCONF"
                      + " <get> operations. Attemps to modify this element and "
                      + "its children with a NETCONF <edit-config> operation MU"
                      + "ST result in an 'operation-not-supported' error with t"
                      + "ype 'application'."
                        ;
            leaf-list rate {
              description "The transmission rate that is supported or advertise"
                        + "d. Multiple transmissions rates are allowed.At least"
                        + " one element MUST be present in the NETCONF data sto"
                        + "re. If none of this elements is are present in a NET"
                        + "CONF <edit-config> operation 'create', 'merge' or 'r"
                        + "eplace' and the parent element does not exist, a 'da"
                        + "ta-missing' error is returned."
                          ;
              type OFPortRateType ;
              min-elements 1 ;
            }
            leaf auto-negotiate {
              description "Specifies if auto-negotiation of transmission parame"
                        + "ters is enabled for the port. This element is option"
