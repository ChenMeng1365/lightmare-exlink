        </type>
        <units name="kbit/s"/>
        <mandatory value="true"/>
        <description>
          <text>Configure the rate value of sending packets.</text>
        </description>
      </leaf>
      <leaf name="priority">
        <type name="uint8">
          <range value="0..7"/>
        </type>
        <default value="7"/>
        <description>
          <text>Configure the priority of packets.</text>
        </description>
      </leaf>
      <leaf name="pattern">
        <type name="test-pattern"/>
        <default value="zero-no-crc"/>
        <description>
          <text>Configure the pattern type.</text>
        </description>
      </leaf>
      <leaf name="packet-size">
        <type name="uint16">
          <range value="64..1518"/>
        </type>
        <units name="Byte"/>
        <default value="64"/>
        <description>
          <text>Configure the number of bytes of the sent test packets.</text>
        </description>
      </leaf>
      <leaf name="out-of-service">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Configure the out of service.</text>
        </description>
      </leaf>
      <leaf name="lck-level">
        <type name="uint8">
          <range value="0..7"/>
        </type>
        <description>
          <text>Configure the value of LCK message level.</text>
        </description>
      </leaf>
      <leaf name="timeout">
        <type name="uint16">
          <range value="1..1800"/>
        </type>
        <units name="s"/>
        <default value="300"/>
        <description>
          <text>Configure the value of time out.</text>
        </description>
      </leaf>
    </input>
