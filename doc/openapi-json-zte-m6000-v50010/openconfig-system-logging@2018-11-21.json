                "source-address": {
                    "type": "string",
                    "description": "Source IP address for packets to the log server"
                },
                "remote-port": {
                    "type": "string",
                    "description": "Sets the destination port number for syslog UDP messages to\nthe server.  The default for syslog is 514."
