<?xml version="1.0" encoding="UTF-8"?>
<module name="huawei-sshs"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:sshs="urn:huawei:yang:huawei-sshs"
        xmlns:pub-type="urn:huawei:yang:huawei-pub-type"
