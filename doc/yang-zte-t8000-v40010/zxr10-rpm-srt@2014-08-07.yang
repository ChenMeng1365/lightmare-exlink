    type enumeration {
      enum RIP {
        value 1 ;
        description 'Static route protect RIP route.' ;
      }
      enum OSPF {
        value 2 ;
        description 'Static route protect OSPFv2 route.' ;
      }
      enum BGP {
        value 4 ;
        description 'Static route protect BGP route.' ;
      }
      enum ISIS {
        value 8 ;
        description 'Static route protect ISIS route.' ;
      }
    }
    description 'Define the route type which the static route can protect.' ;
