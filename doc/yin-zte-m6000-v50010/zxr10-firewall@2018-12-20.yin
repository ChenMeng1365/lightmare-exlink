          <leaf name="priority">
            <description>
              <text>Priority of zone security</text>
            </description>
            <type name="uint8">
              <range value="1..254"/>
            </type>
            <mandatory value="true"/>
          </leaf>
          <leaf name="blacklist">
            <description>
              <text>Blacklist enable</text>
