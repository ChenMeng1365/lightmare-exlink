  <container name="ops">
    <description>
      <text>The open programmability system.</text>
    </description>
    <container name="global-switch">
      <description>
        <text>Configure OPS global switch.</text>
      </description>
      <leaf name="enable">
        <type name="boolean"/>
        <default value="true"/>
        <description>
          <text>Enable/disable OPS global switch.</text>
        </description>
      </leaf>
    </container>
    <container name="python-version">
      <config value="false"/>
      <description>
        <text>Operational state of Python version.</text>
      </description>
      <leaf name="version">
        <type name="ops-python-version-type"/>
        <config value="false"/>
        <description>
          <text>Ops python version.</text>
        </description>
      </leaf>
    </container>
    <container name="assistant">
      <description>
        <text>Configure OPS assistant node.</text>
      </description>
      <container name="mtp-status-switch">
        <description>
          <text>Configure assistant maintain probe.</text>
        </description>
        <leaf name="enable">
          <type name="boolean"/>
          <default value="true"/>
          <description>
            <text>Enable/disable assistant maintain probe.</text>
          </description>
        </leaf>
      </container>
      <container name="total-status-switch">
        <description>
          <text>Configure assistant total switch.</text>
        </description>
        <leaf name="suspend">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable assistant total switch.</text>
          </description>
        </leaf>
      </container>
      <container name="script-assistants">
        <description>
          <text>List of script assistant.</text>
        </description>
        <list name="script-assistant">
          <key value="name"/>
          <max-elements value="100"/>
          <description>
            <text>Configure script assistant instance.</text>
          </description>
          <leaf name="name">
            <type name="string">
              <length value="4..64"/>
              <pattern value="([^_].*\.py)"/>
            </type>
            <description>
              <text>Script assistant name.</text>
            </description>
          </leaf>
          <leaf name="shutdown">
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable script assistant.</text>
            </description>
          </leaf>
        </list>
      </container>
    </container>
    <container name="applications">
      <config value="false"/>
      <description>
        <text>List of the OPS application.</text>
      </description>
      <list name="application">
        <key value="file-name"/>
        <config value="false"/>
        <description>
          <text>Operational state of OPS application node.</text>
        </description>
        <leaf name="file-name">
          <type name="string">
            <length value="1..255"/>
          </type>
          <config value="false"/>
          <description>
            <text>OPS application file name.</text>
          </description>
        </leaf>
        <leaf name="size">
          <type name="uint32"/>
          <default value="0"/>
          <config value="false"/>
          <description>
            <text>The file size (unit:byte).</text>
          </description>
        </leaf>
      </list>
    </container>
  </container>
  <rpc name="install-application">
    <description>
      <text>Install OPS application.</text>
    </description>
    <ext:node-ref value="/ops/applications/application"/>
    <input>
      <leaf name="file-name">
        <type name="string">
          <length value="1..127"/>
        </type>
        <mandatory value="true"/>
        <description>
          <text>The install OPS applcation file name.</text>
        </description>
      </leaf>
      <leaf name="destination">
        <type name="string">
          <length value="1..127"/>
        </type>
        <description>
          <text>The install OPS applcation file destionation.</text>
        </description>
      </leaf>
    </input>
  </rpc>
  <rpc name="uninstall-application">
    <description>
      <text>Uninstall OPS application.</text>
    </description>
    <ext:node-ref value="/ops/applications/application"/>
    <input>
      <leaf name="file-name">
        <type name="string">
          <length value="1..127"/>
        </type>
        <mandatory value="true"/>
        <description>
          <text>Uninstall OPS applcation file name.</text>
        </description>
      </leaf>
    </input>
  </rpc>
  <rpc name="run-python-script">
    <description>
      <text>Run python script.</text>
    </description>
    <ext:node-ref value="/ops/applications/application"/>
    <input>
      <leaf name="name">
        <type name="string">
          <length value="1..127"/>
        </type>
        <mandatory value="true"/>
        <description>
          <text>Python script name.</text>
        </description>
      </leaf>
      <leaf name="argument">
        <type name="string">
          <length value="1..383"/>
        </type>
        <description>
          <text>Python script argument.</text>
        </description>
      </leaf>
    </input>
  </rpc>
</module>
