  <container name="bras-basic-access">
    <description>
      <text>Bras basic access.</text>
    </description>
    <container name="user-groups">
      <description>
        <text>List of the user group.</text>
      </description>
      <list name="user-group">
        <key value="name"/>
        <description>
          <text>Configure a user group.</text>
        </description>
        <leaf name="name">
          <type name="string">
            <length value="1..32"/>
            <pattern value="[!-~]{1,32}"/>
            <pattern value="[^&quot;*?']{1,32}"/>
          </type>
          <must condition="../name != '-' and ../name != '--' and ../name != 'a' and ../name != 'A' and ../name != 'an' and ../name != 'aN' and ../name != 'AN' and ../name != 'An' and ../name != 'Any' and ../name != 'AnY' and ../name != 'ANY' and ../name != 'ANy' and ../name != 'aNy' and ../name != 'aNY' and ../name != 'anY' and ../name != 'any'"/>
          <description>
            <text>Specifies the name of a user group. When the node of user-group-case-sensitive is enabled, the user group name is case-sensitive. When the node of user-group-case-sensitive is disabled, the user group name cannot contain uppercase letters.</text>
          </description>
        </leaf>
        <container name="traffic-policy-statistics">
          <config value="false"/>
          <description>
            <text>List of the specified user group or all user groups.</text>
          </description>
          <list name="traffic-policy-statistic">
            <key value="policy-name up-group"/>
            <description>
              <text>Statistics of a specified user group or all user groups.</text>
            </description>
            <leaf name="policy-name">
              <type name="string">
                <length value="1..127"/>
              </type>
              <description>
                <text>Name of a traffic policy.</text>
              </description>
            </leaf>
            <leaf name="up-group">
              <type name="uint32">
                <range value="0..4294967295"/>
              </type>
              <description>
                <text>ID of a up group.</text>
              </description>
            </leaf>
            <leaf name="user-group-type">
              <type name="enumeration">
                <enum name="local">
                  <value value="1"/>
                  <description>
                    <text>The type of the user group is local.</text>
                  </description>
                </enum>
                <enum name="remote">
                  <value value="2"/>
                  <description>
                    <text>The type of the user group is remote.</text>
                  </description>
                </enum>
                <enum name="dynamic">
                  <value value="3"/>
                  <description>
                    <text>The type of the user group is dynamic.</text>
                  </description>
                </enum>
                <enum name="combined">
                  <value value="4"/>
                  <description>
                    <text>The type of the user group is combined.</text>
                  </description>
                </enum>
              </type>
              <description>
                <text>Type of a user group.</text>
              </description>
            </leaf>
            <leaf name="is-mib-locked">
              <type name="boolean"/>
              <default value="false"/>
              <description>
                <text>Whether the MIB is locked.</text>
              </description>
            </leaf>
            <leaf name="is-nat-instance-valid">
              <type name="boolean"/>
              <default value="false"/>
              <description>
                <text>Whether an NAT instance is valid. By default, the value 0 indicates that an NAT instance is valid and the value 1 indicates that an NAT instance is invalid.</text>
              </description>
            </leaf>
            <leaf name="instance">
              <type name="uint8"/>
              <description>
                <text>Instance bound to a domain.</text>
              </description>
            </leaf>
            <leaf name="instance-type">
              <type name="enumeration">
                <enum name="nat">
                  <value value="0"/>
                  <description>
                    <text>NAT instance bound to a domain.</text>
                  </description>
                </enum>
                <enum name="ds-lite">
                  <value value="1"/>
                  <description>
                    <text>DS-Lite instance bound to a domain.</text>
                  </description>
                </enum>
              </type>
              <description>
                <text>Type of the instance bound to a domain.</text>
              </description>
            </leaf>
            <leaf name="bind-domain-number">
              <type name="uint16"/>
              <description>
                <text>Number of times that a user group is bound to a domain.</text>
              </description>
            </leaf>
            <leaf name="bind-daa-number">
              <type name="uint32"/>
              <description>
                <text>Number of times that a user group is bound to a DAA value-added-service policy template.</text>
              </description>
            </leaf>
            <leaf name="used-user-number">
              <type name="uint32"/>
              <description>
                <text>Number of users in a user group.</text>
              </description>
            </leaf>
            <leaf name="reference-cb-count">
              <type name="uint16"/>
              <description>
                <text>Number of times that combined user groups are bound to CB pairs in traffic policies.</text>
              </description>
            </leaf>
            <leaf name="is-wait-delete">
              <type name="boolean"/>
              <default value="false"/>
              <description>
                <text>WaitDelWhether the system waits for deleting a combined user group.</text>
              </description>
            </leaf>
            <leaf name="aging-time">
              <type name="uint8"/>
              <units name="min"/>
              <description>
                <text>Time after which the delete state of the combined user group will age.</text>
              </description>
            </leaf>
          </list>
        </container>
      </list>
    </container>
    <container name="dhcp-multiple-sessions">
