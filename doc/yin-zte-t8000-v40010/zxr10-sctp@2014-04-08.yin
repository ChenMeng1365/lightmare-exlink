  </container>
  <container name="operation">
    <description>
      <text>Command of sctp clear</text>
    </description>
    <config value="false"/>
    <container name="sctp">
      <description>
        <text>Command of sctp clear</text>
      </description>
      <container name="clear">
        <description>
          <text>Closed a sctp connection or clear the statistics of all sctp connections</text>
        </description>
        <container name="connects">
          <description>
            <text>Closed a sctp connection based on the index of association id</text>
          </description>
          <leaf name="association-id">
            <description>
              <text>Closed a sctp connetion based on the index of association id</text>
            </description>
            <type name="uint32">
              <range value="0..4294967295"/>
            </type>
          </leaf>
        </container>
        <container name="statistics">
          <description>
            <text>Clear statistics of SCTP</text>
          </description>
          <leaf name="statistic">
            <description>
              <text>Clear statistics data of sctp</text>
            </description>
            <type name="empty"/>
          </leaf>
        </container>
      </container>
    </container>
  </container>
</module>
