  <augment target-node="/qos:qos/qos:classifier-template/qos:traffic-behaviors/qos:traffic-behavior">
    <description>
      <text>Adds vas configuration to qos traffic-behavior model.</text>
    </description>
    <container name="bas">
      <description>
        <text>Configure traffic behavior.</text>
      </description>
      <container name="vas">
        <must condition="not(./service-class-edsg/keep-queue-level = 'true' and (./daa-accounting/tariff-level or ./daa-accounting/tariff-statistic-enable = 'true' or ./car-for-daa/enable = 'true'))"/>
        <description>
          <text>Configure vas in qos traffic-behavior model.</text>
        </description>
        <container name="daa-accounting">
          <description>
            <text>Configure a tariff level for DAA accounting.</text>
          </description>
          <leaf name="tariff-level">
            <type name="uint8">
              <range value="1..8"/>
            </type>
            <description>
              <text>Configures a tariff level for DAA accounting.</text>
            </description>
          </leaf>
          <leaf name="tariff-statistic-enable">
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable Traffic Statistics Collection.</text>
            </description>
          </leaf>
        </container>
        <container name="service-class-edsg">
          <description>
            <text>Configure the service queue level is not used in the traffic behavior to overwrite the original queue level.</text>
          </description>
          <leaf name="keep-queue-level">
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable the service class for Enhanced dynamic service gateway.</text>
            </description>
          </leaf>
        </container>
        <container name="car-for-daa">
          <description>
            <text>Configure CAR for DAA service traffic.</text>
          </description>
          <leaf name="enable">
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable CAR for DAA service traffic.</text>
            </description>
          </leaf>
        </container>
      </container>
    </container>
  </augment>
  <augment target-node="/radius:radius/radius:radius-server-groups/radius:radius-server-group">
