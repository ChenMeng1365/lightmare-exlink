module: huawei-sm2
  +--rw sm2
     +--rw sm2-peer-keys
     |  +--rw sm2-peer-key* [key-name]
     |     +--rw key-name    string
     |     +--rw key-code    string
     +--rw sm2-key-pairs
        +--rw sm2-key-pair* [key-pair-label]
           +--rw key-pair-label    string
           +--rw key-size?         uint32
           +--ro host-key-code?    string
