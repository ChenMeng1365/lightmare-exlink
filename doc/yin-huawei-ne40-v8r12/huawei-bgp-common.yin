  <grouping name="slow-peer-type">
    <description>
      <text>Peer advertising routes slowly configuration.</text>
    </description>
    <container name="slow-peer">
      <description>
        <text>Configure peer advertising routes slowly.</text>
      </description>
      <leaf name="detection">
        <type name="boolean"/>
        <default value="true"/>
        <description>
          <text>Enable/disable detect slow peers.</text>
        </description>
      </leaf>
      <leaf name="detection-threshold">
        <type name="uint16">
          <range value="120..3600"/>
        </type>
        <units name="s"/>
        <default value="300"/>
        <description>
          <text>Specify the time in seconds lagging behind average when a peer is determined to be a slow peer.</text>
        </description>
      </leaf>
      <leaf name="absolute-detection">
        <type name="boolean"/>
        <default value="true"/>
        <description>
          <text>Enable/disable absolute-detect slow peers.</text>
        </description>
      </leaf>
      <leaf name="absolute-detection-threshold">
        <type name="uint16">
          <range value="3..3600"/>
        </type>
        <units name="s"/>
        <default value="9"/>
        <description>
          <text>Specify the packet send delay time in second when a peer is determined to be a slow peer.</text>
        </description>
      </leaf>
    </container>
  </grouping>
  <grouping name="bestroute-med-plus-igp-type">
    <description>
      <text>Use the sum of MED multiplied by a MED multiplier and IGP cost multiplied by an IGP cost multiplier to select routes.</text>
    </description>
    <leaf name="bestroute-med-plus-igp">
      <type name="boolean"/>
      <must condition="not(../bestroute-igp-metric-ignore='true' and ../bestroute-med-plus-igp='true')"/>
      <default value="false"/>
      <description>
        <text>Enable/disable the function to add the IGP cost to the next-hop destination to the MED before comparing MED values for path selection.</text>
      </description>
    </leaf>
    <leaf name="bestroute-med-multiplier">
      <when condition="../bestroute-med-plus-igp='true'"/>
      <type name="uint16">
        <range value="1..1000"/>
      </type>
      <default value="1"/>
      <description>
        <text>Value of med product factor.</text>
      </description>
    </leaf>
    <leaf name="bestroute-igp-multiplier">
      <when condition="../bestroute-med-plus-igp='true'"/>
      <type name="uint16">
        <range value="1..1000"/>
      </type>
      <default value="1"/>
      <description>
        <text>Value of IGP cost product factor.</text>
      </description>
    </leaf>
    <leaf name="bestroute-igp-metric-ignore">
      <type name="boolean"/>
      <default value="false"/>
      <description>
        <text>Enable/disable BGP to ignore the IGP cost of each BGP route to the next hop in route selection. By default, a BGP route with a smaller IGP cost to the next hop is preferred.</text>
      </description>
    </leaf>
  </grouping>
  <grouping name="bestroute-router-id-prior-clusterlist-type">
