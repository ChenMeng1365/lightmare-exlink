  <ext:task-name value="l2tp"/>
  <typedef name="session-status-type">
    <type name="enumeration">
      <enum name="establishing">
        <value value="0"/>
        <description>
          <text>Session is establishing.</text>
        </description>
      </enum>
      <enum name="up">
        <value value="1"/>
        <description>
          <text>Session establishment up status.</text>
        </description>
      </enum>
      <enum name="down">
        <value value="2"/>
        <description>
          <text>Session establishment down status.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Session establishment status.</text>
    </description>
  </typedef>
  <grouping name="slot-config">
    <description>
      <text>Set slot config.</text>
    </description>
    <container name="icrq-limit">
      <description>
        <text>Configure icrq limit.</text>
      </description>
      <leaf name="set-value">
        <type name="uint32">
          <range value="1..60000"/>
        </type>
        <description>
          <text>Limit value.</text>
        </description>
      </leaf>
    </container>
    <container name="forward-mode">
      <description>
        <text>Configure L2tp forward mode.</text>
      </description>
      <leaf name="mode-value">
        <type name="enumeration">
          <enum name="loopback">
            <value value="0"/>
            <description>
              <text>The software LoopBack enable.</text>
            </description>
          </enum>
          <enum name="through">
            <value value="1"/>
            <description>
              <text>The hardware LoopBack enable.</text>
            </description>
          </enum>
        </type>
        <default value="through"/>
        <description>
          <text>Forward mode.</text>
        </description>
      </leaf>
    </container>
  </grouping>
  <grouping name="l2tp-group">
    <description>
      <text>L2tp-group info.</text>
    </description>
    <leaf name="used-tunnel-number">
      <type name="uint32"/>
      <config value="false"/>
      <description>
        <text>Number of used tunnels.</text>
      </description>
    </leaf>
    <leaf name="used-number">
      <type name="uint16"/>
      <config value="false"/>
      <description>
        <text>Number of times that L2TP tunnels are established for an L2TP group on the backup device.</text>
      </description>
    </leaf>
    <container name="lns-ip-addresss">
      <config value="false"/>
      <description>
        <text>List of informations about LNS IP address. When load balancing based on the master/backup status is configured, only LNS IP addresses are displayed. When weight-based load-balancing is configured, LNS IP addresses and weights are displayed. When priority-based load balancing is configured, LNS IP addresses and priorities are displayed.</text>
      </description>
      <list name="lns-ip-address">
        <key value="lns-index"/>
        <config value="false"/>
        <description>
          <text>Statistics of information about LNS IP address. When load balancing based on the master/backup status is configured, only LNS IP addresses are displayed. When weight-based load-balancing is configured, LNS IP addresses and weights are displayed. When priority-based load balancing is configured, LNS IP addresses and priorities are displayed.</text>
        </description>
        <leaf name="lns-index">
          <type name="uint32"/>
          <config value="false"/>
          <description>
            <text>The index of LNS IP address.</text>
          </description>
        </leaf>
        <leaf name="ip-address">
          <type name="inet:ipv4-address-no-zone"/>
          <config value="false"/>
          <description>
            <text>LNS IP address.</text>
          </description>
        </leaf>
        <leaf name="tunnel-id">
          <type name="uint32">
            <range value="1..65535"/>
          </type>
          <config value="false"/>
          <description>
            <text>The Tunnel ID of LNS IP address.</text>
          </description>
        </leaf>
        <leaf name="expire">
          <type name="uint32"/>
          <config value="false"/>
          <description>
            <text>The expire of LNS IP address.</text>
          </description>
        </leaf>
        <leaf name="state">
          <type name="enumeration">
            <enum name="unused">
              <value value="0"/>
              <description>
                <text>LNS IP address be unused.</text>
              </description>
            </enum>
            <enum name="used">
              <value value="1"/>
              <description>
                <text>LNS IP address be used.</text>
              </description>
            </enum>
          </type>
          <config value="false"/>
          <description>
            <text>The used state of LNS IP address.</text>
          </description>
        </leaf>
      </list>
    </container>
  </grouping>
  <grouping name="lns-group-bind-slots">
    <description>
      <text>Lns-group bind slots.</text>
    </description>
    <container name="bind-slots">
      <description>
        <text>List of LNS group bind the LPU slot.</text>
      </description>
      <list name="bind-slot">
        <key value="slot-id"/>
        <description>
          <text>Configure LNS group bind the LPU slot.</text>
        </description>
        <leaf name="slot-id">
          <type name="string">
            <length value="1..15"/>
            <pattern value="([^A-Z]{1,32})"/>
          </type>
          <description>
            <text>The LPU slot.</text>
          </description>
        </leaf>
        <container name="tunnel-session">
          <config value="false"/>
          <description>
            <text>Statistics of the Tunnel and Session Information on the board in the LNS group.</text>
          </description>
          <leaf name="tunnel-number">
            <type name="uint32">
              <range value="0..65535"/>
            </type>
            <config value="false"/>
            <description>
              <text>The number of tunnels.</text>
            </description>
          </leaf>
          <leaf name="session-number">
            <type name="uint32">
              <range value="0..65535"/>
            </type>
            <config value="false"/>
            <description>
              <text>The number of sessions.</text>
            </description>
          </leaf>
        </container>
      </list>
    </container>
  </grouping>
  <grouping name="lns-group-common">
    <description>
      <text>Lns-group common.</text>
    </description>
    <container name="group-description">
      <description>
        <text>Configure a LNS Group.</text>
      </description>
      <leaf name="description-value">
        <type name="string">
          <length value="1..80"/>
        </type>
        <description>
          <text>Description of a LNS Group.</text>
        </description>
      </leaf>
    </container>
    <container name="load-balance">
      <description>
        <text>Configure tunnel load balance type.</text>
      </description>
      <leaf name="mode">
        <type name="enumeration">
          <enum name="by-tunnel">
            <value value="1"/>
            <description>
              <text>By tunnel.</text>
            </description>
          </enum>
          <enum name="by-session">
            <value value="2"/>
            <description>
              <text>By session.</text>
            </description>
          </enum>
        </type>
        <description>
          <text>Load balance type.</text>
        </description>
      </leaf>
    </container>
    <container name="master-slot">
      <config value="false"/>
      <description>
        <text>Statistics of information about the slot number of the main tunnel board in the LNS group.</text>
      </description>
      <leaf name="slot-id">
        <type name="string">
          <length value="1..15"/>
          <pattern value="([^A-Z]{1,32})"/>
        </type>
        <config value="false"/>
        <description>
          <text>The Master Slot ID.</text>
        </description>
      </leaf>
    </container>
  </grouping>
  <grouping name="lns-statistics-number">
    <description>
      <text>Lns statistics number.</text>
    </description>
    <leaf name="current-up-tunnel-num">
      <type name="uint32"/>
      <config value="false"/>
      <description>
        <text>Displays l2tp statistics lns used tunnel number.</text>
      </description>
    </leaf>
    <leaf name="current-up-session-num">
      <type name="uint32"/>
      <default value="0"/>
      <config value="false"/>
      <description>
        <text>Displays l2tp statistics lns used session number.</text>
      </description>
    </leaf>
    <leaf name="total-established-tunnel-num">
      <type name="uint64"/>
      <default value="0"/>
      <config value="false"/>
      <description>
        <text>Displays l2tp statistics lns established tunnel number.</text>
      </description>
    </leaf>
    <leaf name="total-established-session-num">
      <type name="uint64"/>
      <default value="0"/>
      <config value="false"/>
      <description>
        <text>Displays l2tp statistics lns established session mumber.</text>
      </description>
    </leaf>
    <leaf name="total-down-tunnel-num">
      <type name="uint64"/>
      <default value="0"/>
      <config value="false"/>
      <description>
        <text>Displays l2tp statistics lns down tunnel mumber.</text>
      </description>
    </leaf>
    <leaf name="total-down-session-num">
      <type name="uint64"/>
      <default value="0"/>
      <config value="false"/>
      <description>
        <text>Displays l2tp statistics lns down session mumber.</text>
      </description>
    </leaf>
  </grouping>
  <grouping name="tunnel-statistics-basic">
    <description>
      <text>Tunnel statistics basic info.</text>
    </description>
    <leaf name="local-id">
      <type name="uint32">
        <range value="1..65535"/>
      </type>
      <config value="false"/>
      <description>
        <text>Tunnel local id.</text>
      </description>
    </leaf>
    <leaf name="l2tp-group-name">
      <type name="string">
        <length value="1..32"/>
      </type>
      <config value="false"/>
      <description>
        <text>L2TP group name.</text>
      </description>
    </leaf>
    <leaf name="local-name">
      <type name="string">
        <length value="1..253"/>
      </type>
      <config value="false"/>
      <description>
        <text>Tunnel local name.</text>
      </description>
    </leaf>
    <leaf name="local-ip-address">
      <type name="inet:ipv4-address-no-zone"/>
      <description>
        <text>Source IP address.</text>
      </description>
    </leaf>
    <leaf name="remote-ip-address">
      <type name="inet:ipv4-address-no-zone"/>
      <description>
        <text>Destination IP address.</text>
      </description>
    </leaf>
  </grouping>
  <grouping name="tunnel-statistics-packet">
    <description>
      <text>Tunnel statistics packet info.</text>
    </description>
    <leaf name="rx-packets">
      <type name="uint64"/>
      <config value="false"/>
      <description>
        <text>Receive packets.</text>
      </description>
    </leaf>
    <leaf name="rx-packets-rate">
      <type name="uint32"/>
      <config value="false"/>
      <description>
        <text>Receive packets rate.</text>
      </description>
    </leaf>
    <leaf name="rx-octets">
      <type name="uint64"/>
      <config value="false"/>
      <description>
        <text>Receive octets.</text>
      </description>
    </leaf>
    <leaf name="rx-octets-rate">
      <type name="uint32"/>
      <config value="false"/>
      <description>
        <text>Receive octets rate.</text>
      </description>
    </leaf>
    <leaf name="tx-packets">
      <type name="uint64"/>
      <config value="false"/>
      <description>
        <text>Transmit packets.</text>
      </description>
    </leaf>
    <leaf name="tx-packets-rate">
      <type name="uint32"/>
      <config value="false"/>
      <description>
        <text>Transmit packets rate.</text>
      </description>
    </leaf>
    <leaf name="tx-octets">
      <type name="uint64"/>
      <config value="false"/>
      <description>
        <text>Transmit octets.</text>
      </description>
    </leaf>
    <leaf name="tx-octets-rate">
      <type name="uint32"/>
      <config value="false"/>
      <description>
        <text>Transmit octets rate.</text>
      </description>
    </leaf>
  </grouping>
  <grouping name="user-urpf-ipv4-ipv6">
    <description>
      <text>User urpf IPv4 and IPv6 info.</text>
    </description>
    <leaf name="high">
      <type name="uint32"/>
      <config value="false"/>
      <description>
        <text>High 32-bit count of discard packet.</text>
      </description>
    </leaf>
    <leaf name="low">
      <type name="uint32"/>
      <config value="false"/>
      <description>
        <text>Low 32-bit count of discard packet.</text>
      </description>
    </leaf>
  </grouping>
  <grouping name="session-number">
    <description>
      <text>Session number info.</text>
    </description>
    <leaf name="source-address">
      <type name="inet:ipv4-address-no-zone"/>
      <config value="false"/>
      <description>
        <text>Source IP address.</text>
      </description>
    </leaf>
    <leaf name="vpn-name">
      <type name="string">
        <length value="1..31"/>
      </type>
      <config value="false"/>
      <description>
        <text>Vpn instance name.</text>
      </description>
    </leaf>
    <leaf name="number">
      <type name="uint32">
        <range value="0..4294967295"/>
      </type>
      <config value="false"/>
      <description>
        <text>Session number.</text>
      </description>
    </leaf>
  </grouping>
  <grouping name="l2tp-group-brief">
    <description>
      <text>L2tp-group brief info.</text>
    </description>
    <leaf name="group-description">
      <type name="string">
        <length value="1..80"/>
      </type>
      <config value="false"/>
      <description>
        <text>Group-description.</text>
      </description>
    </leaf>
    <leaf name="group-type">
      <type name="enumeration">
        <enum name="none">
          <value value="0"/>
          <description>
            <text>None.</text>
          </description>
        </enum>
        <enum name="accept-dialin-l2tp">
          <value value="1"/>
          <description>
            <text>Accept dialin.</text>
          </description>
        </enum>
        <enum name="request-dialin-l2tp">
          <value value="2"/>
          <description>
            <text>Request dialin.</text>
          </description>
        </enum>
      </type>
      <default value="none"/>
      <config value="false"/>
      <description>
        <text>Group type.</text>
      </description>
    </leaf>
  </grouping>
  <grouping name="l2tp-group-source">
    <description>
      <text>L2tp-group source info.</text>
    </description>
    <leaf name="interface-name">
      <type name="string">
        <length value="1..49"/>
      </type>
      <config value="false"/>
      <description>
        <text>Source interface name.</text>
      </description>
    </leaf>
    <leaf name="ip-address">
      <type name="inet:ipv4-address-no-zone"/>
      <config value="false"/>
      <description>
        <text>Source IP.</text>
      </description>
    </leaf>
    <leaf name="vpn-route-forward-id">
      <type name="uint32"/>
      <config value="false"/>
      <description>
        <text>VPN Route Forward Id.</text>
      </description>
    </leaf>
  </grouping>
  <grouping name="tunnel-statistics-backup">
    <description>
      <text>Tunnel statistics backup info.</text>
    </description>
    <container name="tunnel">
      <description>
        <text>L2tp tunnel backup statistics.</text>
      </description>
      <leaf name="establish-receive">
        <type name="uint32"/>
        <description>
          <text>The number of tunnel establish Receive.</text>
        </description>
      </leaf>
      <leaf name="teardown-receive">
        <type name="uint32"/>
        <description>
          <text>The number of teardown receive.</text>
        </description>
      </leaf>
      <leaf name="establish-send">
        <type name="uint32"/>
        <description>
          <text>The number of tunnel establish send.</text>
        </description>
      </leaf>
      <leaf name="teardown-send">
        <type name="uint32"/>
        <description>
          <text>The number of tunnel teardown send.</text>
        </description>
      </leaf>
      <leaf name="hello-packet-send">
        <type name="uint32"/>
        <description>
          <text>The number of hello packet send.</text>
        </description>
      </leaf>
      <leaf name="zlb-packet-send">
        <type name="uint32"/>
        <description>
          <text>The number of ZLB packet send.</text>
        </description>
      </leaf>
      <leaf name="hello-packet-receive">
        <type name="uint32"/>
        <description>
          <text>The number of hello packet receive.</text>
        </description>
      </leaf>
      <leaf name="zlb-packet-receive">
        <type name="uint32"/>
        <description>
          <text>The number of ZLB packet receive.</text>
        </description>
      </leaf>
      <leaf name="peer-hello">
        <type name="uint32"/>
        <description>
          <text>The number of peer Hello.</text>
        </description>
      </leaf>
      <leaf name="peer-zlb">
        <type name="uint32"/>
        <description>
          <text>The number of peer ZLB.</text>
        </description>
      </leaf>
    </container>
  </grouping>
  <grouping name="session-statistics-backup">
    <description>
      <text>Session statistics backup info.</text>
    </description>
    <container name="session">
