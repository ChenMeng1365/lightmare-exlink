<?xml version="1.0" encoding="UTF-8"?>
<module name="huawei-clocksync-cesacr"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:clocksync-cesacr="urn:huawei:yang:huawei-clocksync-cesacr"
        xmlns:ext="urn:huawei:yang:huawei-extension"
        xmlns:ifm="urn:huawei:yang:huawei-ifm">
  <namespace uri="urn:huawei:yang:huawei-clocksync-cesacr"/>
  <prefix value="clocksync-cesacr"/>
  <import module="huawei-extension">
    <prefix value="ext"/>
  </import>
  <import module="huawei-ifm">
    <prefix value="ifm"/>
  </import>
  <organization>
    <text>Huawei Technologies Co., Ltd.</text>
  </organization>
  <contact>
    <text>Huawei Industrial Base
Bantian, Longgang
Shenzhen 518129
People's Republic of China
Website: http://www.huawei.com
Email: support@huawei.com</text>
  </contact>
  <description>
    <text>Clock ces-acr data model define.</text>
  </description>
  <revision date="2020-03-04">
    <description>
      <text>Modify the node decsription.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <revision date="2019-11-04">
    <description>
      <text>Initial revision.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <ext:task-name value="clksync"/>
  <typedef name="master-pw-state">
    <type name="enumeration">
      <enum name="none">
        <value value="0"/>
        <description>
          <text>The master PW status is null.</text>
        </description>
      </enum>
      <enum name="wait">
        <value value="1"/>
        <description>
          <text>The master PW is in the wait state.</text>
        </description>
      </enum>
      <enum name="lock">
        <value value="2"/>
        <description>
          <text>The master PW works in lock state.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Master PW status of the CES service.</text>
    </description>
  </typedef>
  <typedef name="recovery-mode">
    <type name="enumeration">
      <enum name="ces-acr">
        <value value="1"/>
        <description>
          <text>The clock recovery mode of the CES service is CES ACR.</text>
        </description>
      </enum>
      <enum name="ces-dcr">
        <value value="2"/>
        <description>
          <text>The clock recovery mode of the CES service is CES DCR.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>CES ACR clock recovery mode.</text>
    </description>
  </typedef>
  <container name="clocksync-cesacr">
    <description>
      <text>CES ACR clock.</text>
    </description>
    <container name="master-domain-infos">
      <config value="false"/>
      <description>
        <text>List of statistics of master domain information.</text>
      </description>
      <list name="master-domain-info">
        <key value="slot-id card-id domain-value"/>
        <description>
          <text>Statistics of master domain information.</text>
        </description>
        <leaf name="slot-id">
          <type name="uint32"/>
          <description>
            <text>Slot ID.</text>
          </description>
        </leaf>
        <leaf name="card-id">
          <type name="uint32"/>
          <description>
            <text>Slot ID of the subcard.</text>
          </description>
        </leaf>
        <leaf name="domain-value">
          <type name="uint32"/>
          <description>
            <text>Master domain.</text>
          </description>
        </leaf>
        <leaf name="e1-port">
          <type name="string">
            <length value="1..50"/>
          </type>
          <description>
            <text>Key master domain port.</text>
          </description>
        </leaf>
        <leaf name="e1-channel-number">
          <type name="uint32"/>
          <description>
            <text>E1 channel number of the master clock domain.</text>
          </description>
        </leaf>
        <container name="e1-master-infos">
          <description>
            <text>List of statistics of information about the E1 interface that joins the master domain.</text>
          </description>
          <list name="e1-master-info">
            <key value="port-name e1-number"/>
            <description>
              <text>Statistics of information about the E1 interface that joins the master domain.</text>
            </description>
            <leaf name="port-name">
              <type name="string">
                <length value="1..50"/>
              </type>
              <description>
                <text>E1 port name.</text>
              </description>
            </leaf>
            <leaf name="e1-number">
              <type name="uint32"/>
              <description>
                <text>E1 port number.</text>
              </description>
            </leaf>
          </list>
        </container>
      </list>
    </container>
    <container name="cesacr-sources">
      <description>
        <text>List of configure CES ACR clock source.</text>
      </description>
      <list name="cesacr-source">
        <key value="slot-id card-id recovery-domain-value"/>
        <description>
          <text>Configure CES ACR clock source.</text>
        </description>
        <leaf name="slot-id">
          <type name="uint32">
            <range value="0..255"/>
          </type>
          <description>
            <text>Slot ID of the interface board.</text>
          </description>
        </leaf>
        <leaf name="card-id">
          <type name="uint32">
            <range value="0..255"/>
          </type>
          <description>
            <text>Slot ID of the subcard.</text>
          </description>
        </leaf>
        <leaf name="recovery-domain-value">
          <type name="uint32">
            <range value="1..8"/>
          </type>
          <description>
            <text>Clock recovery domain.</text>
          </description>
        </leaf>
      </list>
    </container>
    <container name="recovery-domain-infos">
      <config value="false"/>
      <description>
        <text>List of statistics of details about the clock recovery domain.</text>
      </description>
      <list name="recovery-domain-info">
        <key value="slot-id card-id recovery-domain-value"/>
        <description>
          <text>Statistics of details about the clock recovery domain.</text>
        </description>
        <leaf name="slot-id">
          <type name="uint32"/>
          <description>
            <text>Slot ID of the interface board.</text>
          </description>
        </leaf>
        <leaf name="card-id">
          <type name="uint32"/>
          <description>
            <text>Slot ID of the subcard.</text>
          </description>
        </leaf>
        <leaf name="recovery-domain-value">
          <type name="uint32"/>
          <description>
            <text>Clock recovery domain.</text>
          </description>
        </leaf>
        <leaf name="pw-port-name">
          <type name="string">
            <length value="1..51"/>
          </type>
          <description>
            <text>Port name of the master PW.</text>
          </description>
        </leaf>
        <leaf name="pw-channel-id">
          <type name="uint32"/>
          <description>
            <text>Channel ID of the master PW of the CES service.</text>
          </description>
        </leaf>
        <leaf name="pw-status">
          <type name="master-pw-state"/>
          <description>
            <text>Master PW status of the CES service.</text>
          </description>
        </leaf>
        <container name="e1-infos">
          <description>
            <text>List of statistics of E1 port information.</text>
          </description>
          <list name="e1-info">
            <key value="e1-number port-name"/>
            <description>
              <text>Statistics of E1 port information.</text>
            </description>
            <leaf name="e1-number">
              <type name="uint32"/>
              <description>
                <text>E1 port number of the clock domain.</text>
              </description>
            </leaf>
            <leaf name="port-name">
              <type name="string">
                <length value="1..50"/>
              </type>
              <description>
                <text>E1 port name of the clock domain.</text>
              </description>
            </leaf>
            <leaf name="channel-direction">
              <type name="uint32"/>
              <description>
                <text>E1 channel direction, including upstream, downstream, and both upstream and downstream.</text>
              </description>
            </leaf>
            <container name="pw-infos">
              <description>
                <text>List of statistics of PW information of the CES service.</text>
              </description>
              <list name="pw-info">
                <key value="serial-port-name"/>
                <description>
                  <text>Statistics of PW information of the CES service.</text>
                </description>
                <leaf name="serial-port-name">
                  <type name="string">
                    <length value="1..51"/>
                  </type>
                  <description>
                    <text>PW port name of clock domain services.</text>
                  </description>
                </leaf>
                <leaf name="channel-id">
                  <type name="uint32"/>
                  <description>
                    <text>PW channel ID of clock domain services.</text>
                  </description>
                </leaf>
              </list>
            </container>
          </list>
        </container>
      </list>
    </container>
  </container>
  <augment target-node="/ifm:ifm/ifm:interfaces/ifm:interface">
    <description>
      <text>Interface.</text>
    </description>
    <container name="clock-cesacr">
      <description>
        <text>Configure CES ACR clock domain.</text>
      </description>
      <container name="master-domains">
        <when condition="../../ifm:type='Cpos'"/>
        <description>
          <text>List of the CES ACR clock master domain configurations.</text>
        </description>
        <list name="master-domain">
          <ext:generated-by value="system"/>
          <key value="e1-number"/>
          <description>
            <text>Configure CES ACR clock master domain.</text>
          </description>
          <leaf name="e1-number">
            <type name="uint32">
              <range value="1..63"/>
            </type>
            <description>
              <text>E1 port number&lt;1-63&gt;.</text>
            </description>
          </leaf>
          <leaf name="domain-value">
            <type name="uint32">
              <range value="1..32"/>
            </type>
            <description>
              <text>Master domain value, Cpos port &lt;0-3&gt; can config domain &lt;1-16&gt;, Cpos port &lt;4-7&gt; can config domain &lt;17-32&gt;.</text>
            </description>
          </leaf>
        </list>
      </container>
      <container name="recovery-domains">
        <when condition="(../../ifm:type='E1') or (../../ifm:type='Cpos')"/>
        <description>
          <text>List of the CES ACR clock recovery domain configurations.</text>
        </description>
        <list name="recovery-domain">
          <ext:generated-by value="system"/>
          <key value="e1-number"/>
          <description>
            <text>Configure CES ACR clock recovery domain.</text>
          </description>
          <leaf name="e1-number">
            <type name="uint32">
              <range value="0..63"/>
            </type>
            <must condition="((../../../../ifm:type='E1') and (../e1-number='0')) or ((../../../../ifm:type='Cpos') and (../e1-number!='0'))"/>
            <description>
              <text>E1 port number&lt;1-63&gt;.</text>
            </description>
          </leaf>
          <leaf name="domain-value">
            <type name="uint32">
              <range value="1..8"/>
            </type>
            <description>
              <text>Clock recovery domain value of the CES service.</text>
            </description>
          </leaf>
        </list>
      </container>
      <container name="recovery-modes">
        <when condition="(../../ifm:link-protocol='tdm') and (../../ifm:type='Serial')"/>
        <description>
          <text>Configure CES clock recovery mode.</text>
        </description>
        <leaf name="service-mode">
          <type name="recovery-mode"/>
          <description>
            <text>Clock recovery mode of the CES service.</text>
          </description>
        </leaf>
      </container>
    </container>
  </augment>
</module>
