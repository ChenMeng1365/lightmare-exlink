          <must condition="(./cir-value and ./cbs) or not(./cir-value)"/>
          <key value="direction vlan-mode begin-vlan end-vlan"/>
          <description>
            <text>Configure interface applying car.</text>
          </description>
          <leaf name="direction">
            <type name="qos-direction-type"/>
            <description>
              <text>Direction.</text>
            </description>
          </leaf>
          <leaf name="vlan-mode">
            <type name="boolean"/>
            <description>
              <text>Enable/disable VLAN mode.</text>
            </description>
          </leaf>
          <leaf name="begin-vlan">
            <type name="uint16">
              <range value="0..4094"/>
            </type>
            <description>
              <text>Begin VLAN id.</text>
            </description>
          </leaf>
          <leaf name="end-vlan">
            <type name="uint16">
              <range value="0..4094"/>
            </type>
            <description>
              <text>End VLAN id.</text>
            </description>
          </leaf>
          <choice name="cir-parameters">
            <mandatory value="true"/>
            <description>
              <text>Specify cir parameters.</text>
            </description>
            <case name="cir">
              <description>
                <text>Specify cir parameters choice case as cir value.</text>
              </description>
              <leaf name="cir-value">
                <type name="uint32">
                  <range value="0..4294967295"/>
                </type>
                <units name="kbit/s"/>
                <must condition="not(../pir-value) or ../pir-value&gt;=../cir-value"/>
                <mandatory value="true"/>
                <description>
                  <text>Committed information rate.</text>
                </description>
              </leaf>
              <leaf name="pir-value">
                <type name="uint32">
                  <range value="1..4294967295"/>
                </type>
                <units name="kbit/s"/>
                <must condition="../pir-value&gt;=../cir-value"/>
                <description>
                  <text>Peak information rate.</text>
                </description>
              </leaf>
            </case>
            <case name="cir-percentage">
              <description>
                <text>Specify cir parameters choice case as cir percentage value.</text>
              </description>
              <leaf name="cir-percent">
                <when condition="../vlan-mode='false'"/>
                <type name="uint8">
                  <range value="0..100"/>
                </type>
                <units name="%"/>
                <must condition="not(../pir-percent) or ../pir-percent&gt;=../cir-percent"/>
                <mandatory value="true"/>
                <description>
                  <text>Committed information rate percent.</text>
                </description>
              </leaf>
              <leaf name="pir-percent">
                <when condition="../vlan-mode='false'"/>
                <type name="uint8">
                  <range value="0..100"/>
                </type>
                <units name="%"/>
                <must condition="../pir-percent&gt;=../cir-percent"/>
