  <augment target-node="/ifm:ifm/ifm:interfaces/ifm:interface/bras-basic-access:bas/bras-basic-access:access-type/bras-basic-access:layer2-subscriber/bras-basic-access:layer2-subscriber">
    <description>
      <text>PPP configurations under bas interface.</text>
    </description>
    <container name="ppp">
      <description>
        <text>Configure PPP under bas interface.</text>
      </description>
      <container name="roam-domain">
        <description>
          <text>Configure the roaming domain by the roam-domain command.</text>
        </description>
        <leaf name="name">
          <type name="leafref">
            <path value="/aaa:aaa/aaa:domains/aaa:domain/aaa:name"/>
          </type>
          <description>
            <text>Roam domain name.</text>
          </description>
        </leaf>
      </container>
      <container name="keep-alive-slow">
        <description>
          <text>Configure PPP keep alive slow.</text>
        </description>
        <leaf name="enable">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable PPP keep alive slow.</text>
          </description>
        </leaf>
      </container>
      <container name="access-ip-limit">
        <presence value="Create access IP limit."/>
        <description>
          <text>Enable/disable access IP limit.</text>
        </description>
        <leaf name="access-user-limit">
          <type name="uint32">
            <range value="0..2147483647"/>
          </type>
          <mandatory value="true"/>
          <description>
            <text>Access limit user number.</text>
          </description>
        </leaf>
        <leaf name="exclude-enable">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable access limit exclude.</text>
          </description>
        </leaf>
      </container>
      <container name="access-ip">
        <config value="false"/>
        <description>
          <text>Statistics of PPP access IP number information of interface.</text>
        </description>
        <leaf name="access-ip-number">
          <type name="uint32">
            <range value="0..131072"/>
          </type>
          <description>
            <text>Number of accessed users, counted by user IP.</text>
          </description>
        </leaf>
      </container>
      <container name="default-domain">
        <description>
          <text>Configure the default authentication domain for PPP users.</text>
        </description>
        <leaf name="domain-name">
          <type name="leafref">
            <path value="/aaa:aaa/aaa:domains/aaa:domain/aaa:name"/>
          </type>
          <description>
            <text>Specifies the name of the default authentication domain for PPP users.</text>
          </description>
        </leaf>
        <leaf name="force-domain-name">
          <type name="leafref">
            <path value="/aaa:aaa/aaa:domains/aaa:domain/aaa:name"/>
          </type>
          <description>
            <text>Specifies the force name of the default authentication domain for PPP users.</text>
          </description>
        </leaf>
      </container>
    </container>
    <container name="map-interface">
      <config value="false"/>
      <description>
        <text>Statistics of mapping interface name of bas interface.</text>
      </description>
      <leaf name="map-interface-name">
        <type name="string">
          <length value="1..63"/>
        </type>
        <config value="false"/>
        <description>
          <text>Mapping interface name.</text>
        </description>
      </leaf>
    </container>
  </augment>
  <augment target-node="/bras-cu-controller:bras-cu-controller/bras-cu-controller:resource-ups/bras-cu-controller:resource-up">
    <description>
      <text>Add PPPoX configuration to the up.</text>
    </description>
    <container name="pppox">
      <description>
        <text>Configure PPPoX to the up.</text>
      </description>
      <container name="pppoe-session-segment">
        <must condition="./start-session-id &lt;= ./end-session-id"/>
        <presence value="Configure the range of session IDs that a specified vBRAS-UP can assign to PPPoE users in scenarios of one-to-many mapping between one MAC address and multiple sessions."/>
        <description>
          <text>Configure the range of session IDs that a specified vBRAS-UP can assign to PPPoE users in scenarios of one-to-many mapping between one MAC address and multiple sessions.</text>
        </description>
        <leaf name="start-session-id">
          <type name="uint16">
            <range value="1..65534"/>
          </type>
          <mandatory value="true"/>
          <description>
            <text>Start session ID.</text>
          </description>
        </leaf>
        <leaf name="end-session-id">
          <type name="uint16">
            <range value="1..65534"/>
          </type>
          <mandatory value="true"/>
          <description>
            <text>End session ID.</text>
          </description>
        </leaf>
      </container>
    </container>
  </augment>
  <augment target-node="/bras-cu-controller:bras-cu-controller/bras-cu-controller:resource-upboards/bras-cu-controller:resource-upboard">
    <description>
      <text>Add PPPoX statistics to the resource-upboard.</text>
    </description>
    <container name="pppox">
      <config value="false"/>
      <description>
        <text>Statistics of PPPoX added to the resource-upboard.</text>
      </description>
      <uses name="pppox-slot-statistic"/>
    </container>
  </augment>
</module>
