<?xml version="1.0" encoding="UTF-8"?>
<module name="huawei-ipv6-nd"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:ipv6-nd="urn:huawei:yang:huawei-ipv6-nd"
        xmlns:ifm="urn:huawei:yang:huawei-ifm"
