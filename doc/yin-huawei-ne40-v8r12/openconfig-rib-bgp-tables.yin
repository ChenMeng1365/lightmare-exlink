  <grouping name="bgp-adj-rib-common-attr-refs">
    <description>
      <text>Definitions of common references to attribute sets for
multiple AFI-SAFIs for Adj-RIB tables</text>
    </description>
    <leaf name="attr-index">
      <type name="leafref">
        <path value="../../../../../../../../../../attr-sets/attr-set/state/index"/>
      </type>
      <description>
        <text>Reference to the common attribute group for the
route</text>
      </description>
    </leaf>
    <leaf name="community-index">
      <type name="leafref">
        <path value="../../../../../../../../../../communities/community/state/index"/>
      </type>
      <description>
        <text>Reference to the community attribute for the route</text>
      </description>
    </leaf>
    <leaf name="ext-community-index">
      <type name="leafref">
        <path value="../../../../../../../../../../ext-communities/ext-community/state/index"/>
      </type>
      <description>
        <text>Reference to the extended community attribute for the
route</text>
      </description>
    </leaf>
  </grouping>
  <grouping name="bgp-loc-rib-common-attr-refs">
    <description>
      <text>Definitions of common references to attribute sets for
multiple AFI-SAFIs for LOC-RIB tables</text>
    </description>
    <leaf name="attr-index">
      <type name="leafref">
        <path value="../../../../../../../../attr-sets/attr-set/state/index"/>
