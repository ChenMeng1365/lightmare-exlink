  <feature name="mapping-server">
    <description>
      <text>This configuration is used to configure the map of prefix and sid.</text>
    </description>
  </feature>
  <feature name="msd">
    <description>
      <text>This configuration is used to configure maximum SID depth.</text>
    </description>
  </feature>
  <feature name="network-slice"/>
  <typedef name="GLOBAL-STATUS">
    <type name="enumeration">
      <enum name="ENABLE">
        <value value="0"/>
      </enum>
      <enum name="DISABLE">
        <value value="1"/>
      </enum>
    </type>
    <default value="ENABLE"/>
    <description>
      <text>Value of segment routing status.</text>
    </description>
  </typedef>
  <typedef name="LOCAL-PREFIX-PHP-FLAG">
    <type name="enumeration">
      <enum name="EXPLICIT">
        <value value="1"/>
      </enum>
      <enum name="IMPLICIT">
        <value value="2"/>
      </enum>
      <enum name="NULL">
        <value value="0"/>
      </enum>
    </type>
    <default value="NULL"/>
    <description>
      <text>The php-flag of local prefix sid.</text>
    </description>
  </typedef>
  <typedef name="LOCAL-PREFIX-NODE-FLAG">
    <type name="enumeration">
      <enum name="TRUE">
        <value value="1"/>
        <description>
          <text>Node flag.</text>
        </description>
      </enum>
      <enum name="FALSE">
        <value value="0"/>
        <description>
          <text>No node flag.</text>
        </description>
      </enum>
    </type>
    <default value="FALSE"/>
    <description>
      <text>The node-flag of local prefix sid.</text>
    </description>
  </typedef>
  <typedef name="ADJ-TYPE">
    <type name="enumeration">
      <enum name="INVALID">
        <value value="0"/>
      </enum>
      <enum name="LOCAL">
        <value value="1"/>
      </enum>
      <enum name="REMOTE">
        <value value="2"/>
      </enum>
    </type>
    <default value="INVALID"/>
    <description>
      <text>The type of adjacency SID.</text>
    </description>
  </typedef>
  <typedef name="REMOTE-PREFIX-FLAG">
    <type name="enumeration">
      <enum name="NULL">
        <value value="0"/>
      </enum>
      <enum name="IMPLICIT">
        <value value="1"/>
      </enum>
      <enum name="EXPLICIT">
        <value value="2"/>
      </enum>
      <enum name="NODE">
        <value value="3"/>
      </enum>
      <enum name="IMPLICIT-NODE">
        <value value="4"/>
      </enum>
      <enum name="EXPLICIT-NODE">
        <value value="5"/>
      </enum>
    </type>
    <default value="NULL"/>
    <description>
      <text>The sid flag of remote prefix sid.</text>
    </description>
  </typedef>
  <typedef name="SRV6-LOCAL-SID-FUNCTYPE">
    <type name="enumeration">
      <enum name="END">
        <value value="1"/>
      </enum>
      <enum name="END.X">
        <value value="2"/>
      </enum>
      <enum name="END.B6">
        <value value="3"/>
      </enum>
      <enum name="END.BM">
        <value value="4"/>
      </enum>
      <enum name="L3VPN.DX4">
        <value value="5"/>
      </enum>
      <enum name="L3VPN.DX6">
        <value value="6"/>
      </enum>
      <enum name="L3VPN.DT4">
        <value value="7"/>
      </enum>
      <enum name="L3VPN.DT6">
        <value value="8"/>
      </enum>
      <enum name="L3VPN.DT46">
        <value value="9"/>
      </enum>
      <enum name="VPWS.DX2">
        <value value="10"/>
      </enum>
      <enum name="VPWS.DX2V">
        <value value="11"/>
      </enum>
      <enum name="QVPLS.DT2U">
        <value value="12"/>
      </enum>
      <enum name="VPLS.DT2U">
        <value value="13"/>
      </enum>
      <enum name="VPLS.DX2">
        <value value="14"/>
      </enum>
      <enum name="QVPLS.DX2">
        <value value="15"/>
      </enum>
      <enum name="VPLS.DT2M">
        <value value="16"/>
      </enum>
      <enum name="QVPLS.DT2M">
        <value value="17"/>
      </enum>
      <enum name="END.OP">
        <value value="18"/>
      </enum>
      <enum name="END.M">
        <value value="19"/>
      </enum>
    </type>
    <description>
      <text>The function type of srv6 local sid.</text>
    </description>
  </typedef>
  <container name="configuration">
    <description>
      <text>The configuration information processing of segment-routing module.</text>
    </description>
    <container name="ipv4">
      <description>
        <text>The node for IPv4 segment-routing.</text>
      </description>
      <container name="global-status">
        <description>
          <text>The node of global segment-routing status.</text>
        </description>
        <leaf name="status">
          <description>
            <text>Enable or disable the status of global IPv4 segment-routing.</text>
          </description>
          <type name="GLOBAL-STATUS"/>
          <default value="ENABLE"/>
        </leaf>
      </container>
      <container name="local-prefix-sid">
        <description>
          <text>The configuration node of local prefix sid.</text>
        </description>
        <list name="local-prefix-sid">
          <description>
            <text>The configuration node of local prefix sid.</text>
          </description>
          <key value="dest-addr mask-addr"/>
          <leaf name="dest-addr">
            <description>
              <text>The destination address of local prefix sid.</text>
            </description>
            <type name="ncd:ipv4-address"/>
          </leaf>
          <leaf name="mask-addr">
            <description>
              <text>The mask address of local prefix sid.</text>
            </description>
            <type name="ncd:ipv4-address"/>
          </leaf>
          <leaf name="index">
            <description>
              <text>The index value of local prefix sid.</text>
            </description>
            <type name="uint32">
              <range value="0..4294967294"/>
            </type>
            <default value="0"/>
          </leaf>
          <leaf name="node-flag">
            <description>
              <text>The node-flag of local prefix sid.</text>
            </description>
            <type name="LOCAL-PREFIX-NODE-FLAG"/>
            <default value="FALSE"/>
          </leaf>
          <leaf name="php-flag">
            <description>
              <text>The label operation of penultimate hop.</text>
            </description>
            <type name="LOCAL-PREFIX-PHP-FLAG"/>
            <default value="NULL"/>
          </leaf>
        </list>
      </container>
      <container name="adjacencys">
        <description>
          <text>The configuration node of SR adjacency-SID.</text>
        </description>
        <list name="adjacency">
          <description>
            <text>The configuration node of SR adjacency-SID.</text>
          </description>
          <key value="local-ip remote-ip"/>
          <leaf name="local-ip">
            <description>
              <text>The local IP address of SR adjacency-SID.</text>
            </description>
            <type name="ncd:ipv4-address"/>
          </leaf>
          <leaf name="remote-ip">
            <description>
              <text>The remote IP address of SR adjacency-SID.</text>
            </description>
            <type name="ncd:ipv4-address"/>
          </leaf>
          <leaf name="sid">
            <description>
              <text>The SID value of SR adjacency-SID.</text>
            </description>
            <type name="int32"/>
          </leaf>
          <leaf name="protect-sid">
            <description>
              <text>The SID of protect path.</text>
            </description>
            <type name="uint32">
              <range value="16..1048575"/>
            </type>
            <ncd:revise-date date="2019-11-20"/>
          </leaf>
        </list>
        <ncd:revise-date date="2018-08-13"/>
      </container>
      <container name="mapping-server">
        <description>
          <text>The configuration node of prefix-sid-map in ipv4.</text>
        </description>
        <list name="prefix-sid-map">
          <description>
            <text>The configuration node of prefix-sid-map in ipv4.</text>
          </description>
          <key value="dest-addr mask-addr"/>
          <leaf name="dest-addr">
            <description>
              <text>The first prefix mapped with sid.</text>
            </description>
            <type name="ncd:ipv4-address"/>
          </leaf>
          <leaf name="mask-addr">
            <description>
              <text>The mask address of first v4 prefix mapped with sid.</text>
            </description>
            <type name="ncd:ipv4-address"/>
          </leaf>
          <leaf name="first-sid">
            <description>
              <text>The first SID.</text>
            </description>
            <type name="int32">
              <range value="0..1048559"/>
            </type>
            <mandatory value="true"/>
          </leaf>
          <leaf name="range">
            <description>
              <text>The quantity of maps of prefix and sid.</text>
            </description>
            <type name="int32">
              <range value="1..65535"/>
            </type>
            <mandatory value="true"/>
          </leaf>
        </list>
        <ncd:revise-date date="2018-09-19"/>
      </container>
      <container name="adjacency-intfs">
        <description>
          <text>The configuration node of SR adjacency-SID-interface unnumbered.</text>
        </description>
        <list name="adjacency-intf">
          <description>
            <text>The configuration node of SR adjacency-SID-interface unnumbered.</text>
          </description>
          <key value="local-interface remote-router-id"/>
          <leaf name="local-interface">
            <description>
              <text>the local interface of SR adjacency-SID.</text>
            </description>
            <type name="ncd:ifname"/>
          </leaf>
          <leaf name="remote-router-id">
            <description>
              <text>The remote router ID SR adjacency-SID.</text>
            </description>
            <type name="ncd:ipv4-address"/>
          </leaf>
          <leaf name="sid">
            <description>
              <text>The SID value of SR adjacency-SID.</text>
            </description>
            <type name="int32"/>
          </leaf>
          <leaf name="protect-sid">
            <description>
              <text>The SID of protect path.</text>
            </description>
            <type name="uint32">
              <range value="16..1048575"/>
            </type>
            <ncd:revise-date date="2019-11-20"/>
          </leaf>
        </list>
        <ncd:revise-date date="2019-06-10"/>
      </container>
      <container name="adjacency-ip-protect-tunnels">
        <description>
          <text>Set the protect tunnel of adjacency(IP type).</text>
        </description>
        <presence value="primary-tunnel-id"/>
        <list name="adjacency-ip-protect-tunnel">
          <key value="loca-ip remote-ip"/>
          <leaf name="loca-ip">
            <description>
              <text>The local IP address of SR adjacency.</text>
            </description>
            <type name="ncd:ipv4-address-none-zero"/>
          </leaf>
          <leaf name="remote-ip">
            <description>
              <text>The remote IP address of SR adjacency.</text>
            </description>
            <type name="ncd:ipv4-address-none-zero"/>
          </leaf>
          <leaf name="primary-tunnel-id">
            <description>
              <text>The primary protect tunnel ID.</text>
            </description>
            <type name="uint32">
              <range value="1..65535"/>
            </type>
            <mandatory value="true"/>
          </leaf>
          <leaf name="secondary-tunnel-id">
            <description>
              <text>The secondary protect tunnel ID.</text>
            </description>
            <type name="uint32">
              <range value="1..65535"/>
            </type>
          </leaf>
        </list>
        <ncd:revise-date date="2020-03-30"/>
      </container>
      <container name="adjacency-id-protect-tunnels">
        <description>
          <text>Set the protect tunnel of adjacency(ID type).</text>
        </description>
        <presence value="primary-tunnel-id"/>
        <list name="adjacency-id-protect-tunnel">
          <key value="local-interface remote-router-id"/>
          <leaf name="local-interface">
            <description>
              <text>The local interface of SR adjacency.</text>
            </description>
            <type name="ncd:ifname"/>
          </leaf>
          <leaf name="remote-router-id">
            <description>
              <text>The remote router ID SR adjacency.</text>
            </description>
            <type name="ncd:ipv4-address-none-zero"/>
          </leaf>
          <leaf name="primary-tunnel-id">
            <description>
              <text>The primary protect tunnel ID.</text>
            </description>
            <type name="uint32">
              <range value="1..65535"/>
            </type>
            <mandatory value="true"/>
          </leaf>
          <leaf name="secondary-tunnel-id">
            <description>
              <text>The secondary protect tunnel ID.</text>
            </description>
            <type name="uint32">
              <range value="1..65535"/>
            </type>
          </leaf>
        </list>
        <ncd:revise-date date="2020-03-30"/>
      </container>
      <container name="adjacency-protect-ips">
        <status value="obsolete">
          <ncd:revise-date date="2020-03-30"/>
        </status>
        <list name="adjacency-protect-ip">
          <key value="loca-ip remote-ip sr-path-identifier"/>
          <leaf name="loca-ip">
            <description>
