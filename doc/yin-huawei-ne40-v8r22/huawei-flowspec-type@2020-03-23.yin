<?xml version="1.0" encoding="UTF-8"?>
<submodule name="huawei-flowspec-type"
           xmlns="urn:ietf:params:xml:ns:yang:yin:1"
           xmlns:flowspec="urn:huawei:yang:huawei-flowspec">
  <yang-version value="1"/>
  <belongs-to module="huawei-flowspec">
    <prefix value="flowspec"/>
  </belongs-to>
  <organization>
    <text>Huawei Technologies Co., Ltd.</text>
  </organization>
  <contact>
    <text>Huawei Industrial Base
Bantian, Longgang
Shenzhen 518129
People's Republic of China
Website: http://www.huawei.com
Email: support@huawei.com</text>
  </contact>
  <description>
    <text>Flowspec type.</text>
  </description>
  <revision date="2020-03-23">
    <description>
      <text>Add fragment-operator-type, Modify flow-match-operator-type, operator-type and match-port-type.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <revision date="2019-05-08">
    <description>
      <text>Initial revision.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <typedef name="af-type">
    <type name="enumeration">
      <enum name="ipv4flow">
        <value value="7"/>
        <description>
          <text>IPv4flow.</text>
        </description>
      </enum>
      <enum name="ipv6flow">
        <value value="15"/>
        <description>
          <text>IPv6flow.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>The type of flow route address family.</text>
    </description>
  </typedef>
  <typedef name="operator-type">
    <type name="enumeration">
      <enum name="less-than">
        <value value="1"/>
        <description>
          <text>Less than.</text>
        </description>
      </enum>
      <enum name="equal">
        <value value="2"/>
        <description>
          <text>Equal.</text>
        </description>
      </enum>
      <enum name="greater-than">
        <value value="3"/>
        <description>
          <text>Greater than.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Operator.</text>
    </description>
  </typedef>
  <typedef name="match-fragment-type">
    <type name="enumeration">
      <enum name="fragment">
        <value value="1"/>
        <description>
          <text>Check fragments.</text>
        </description>
      </enum>
      <enum name="fragment-spe-first">
        <value value="2"/>
        <description>
          <text>Check the first fragment.</text>
        </description>
      </enum>
      <enum name="non-fragment">
        <value value="3"/>
        <description>
          <text>Check unfragmented packets.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Fragment type name.</text>
    </description>
  </typedef>
  <typedef name="match-port-type">
    <type name="enumeration">
      <enum name="match-dest-port">
        <value value="5"/>
        <description>
          <text>Match the destination port.</text>
        </description>
      </enum>
      <enum name="match-source-port">
        <value value="6"/>
        <description>
          <text>Match the source port.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Port type.</text>
    </description>
  </typedef>
  <typedef name="match-origin-as-type">
    <type name="enumeration">
      <enum name="match-dest-origin-as">
        <value value="201"/>
        <description>
          <text>Specify an origin-AS rule for the destination IP.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Origin-AS type.</text>
    </description>
  </typedef>
  <typedef name="as-number-type">
    <type name="string">
      <length value="1..11"/>
      <pattern value="((([1-9]\d{0,8})|([1-3]\d{9})|(4[0-1]\d{8})|(42[0-8]\d{7})|(429[0-3]\d{6})|(4294[0-8]\d{5})|(42949[0-5]\d{4})|(429496[0-6]\d{3})|(4294967[0-1]\d{2})|(42949672[0-8]\d{1})|(429496729[0-5]))|((([1-9]\d{0,3})|([1-5]\d{4})|(6[0-4]\d{3})|(65[0-4]\d{2})|(655[0-2]\d)|(6553[0-5]))[\.](([0-9]\d{0,3})|([1-5]\d{4})|(6[0-4]\d{3})|(65[0-4]\d{2})|(655[0-2]\d)|(6553[0-5]))))"/>
    </type>
    <description>
      <text>AS number in asplain format or in asdot format.</text>
    </description>
  </typedef>
  <typedef name="flow-match-operator-type">
    <type name="enumeration">
      <enum name="match">
        <value value="1"/>
        <description>
          <text>Match operator.</text>
        </description>
      </enum>
      <enum name="not">
        <value value="2"/>
        <description>
          <text>Not operator.</text>
        </description>
      </enum>
      <enum name="any-match">
        <value value="3"/>
        <description>
          <text>Any-match operator.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Match operator type.</text>
    </description>
  </typedef>
  <typedef name="fragment-operator-type">
    <type name="enumeration">
      <enum name="match">
        <value value="1"/>
        <description>
          <text>Match operator.</text>
        </description>
      </enum>
      <enum name="not">
        <value value="2"/>
        <description>
          <text>Not operator.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Fragment operator type.</text>
    </description>
  </typedef>
</submodule>
