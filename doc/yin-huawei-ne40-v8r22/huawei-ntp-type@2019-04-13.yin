<?xml version="1.0" encoding="UTF-8"?>
<submodule name="huawei-ntp-type"
           xmlns="urn:ietf:params:xml:ns:yang:yin:1"
           xmlns:ntp="urn:huawei:yang:huawei-ntp">
  <belongs-to module="huawei-ntp">
    <prefix value="ntp"/>
  </belongs-to>
  <organization>
    <text>Huawei Technologies Co., Ltd.</text>
  </organization>
  <contact>
    <text>Huawei Industrial Base
Bantian, Longgang
Shenzhen 518129
People's Republic of China
Website: http://www.huawei.com
Email: support@huawei.com</text>
  </contact>
  <description>
    <text>Network Time Protocol.</text>
  </description>
  <revision date="2019-04-13">
    <description>
      <text>Initial revision.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <typedef name="debug-type">
    <type name="string"/>
    <description>
      <text>It is bit flag used for enabling and disabling ntp debug.</text>
    </description>
  </typedef>
  <typedef name="authentication-mode">
    <type name="enumeration">
      <enum name="MD5">
        <value value="1"/>
        <description>
          <text>The NTP authentication mode is MD5.</text>
        </description>
      </enum>
      <enum name="HMAC-SHA256">
        <value value="2"/>
        <description>
          <text>The NTP authentication mode is HMAC-SHA256.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Used to identify which mode of authentication, Md5 or Hmac.</text>
    </description>
  </typedef>
  <typedef name="access-level">
    <type name="enumeration">
      <enum name="Peer">
        <value value="1"/>
        <description>
          <text>Indicates the NTP Access Level is Peer. It sets the fully access authority. Both time request and control query can be performed on the local NTP service, and the local clock can be synchronized to the remote server.</text>
        </description>
      </enum>
      <enum name="Query">
        <value value="2"/>
        <description>
          <text>Indicates the NTP Access Level is Query. It sets the maximum access limitation. Only control query can be performed on the local NTP service.</text>
        </description>
      </enum>
      <enum name="Server">
        <value value="3"/>
        <description>
          <text>Indicates the NTP Access Level is Server. It enables the server access and query. Both time requests and control query can be performed on the local NTP service, but the local clock cannot be synchronized to the remote server.</text>
        </description>
      </enum>
      <enum name="Synchronization">
        <value value="4"/>
        <description>
          <text>Indicates the NTP Access Level is Synchronization. It enables the server to access. Only time request can be performed on the local NTP service.</text>
        </description>
      </enum>
      <enum name="Limited">
        <value value="5"/>
        <description>
          <text>Indicates the NTP Access Level is Limited.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>NTP has 5 ACL level, it identify NTP acl level.</text>
    </description>
  </typedef>
  <typedef name="cast-mode">
    <type name="enumeration">
      <enum name="Client">
        <value value="1"/>
        <description>
          <text>Indicates that the current mode is client.</text>
        </description>
      </enum>
      <enum name="Server">
        <value value="2"/>
        <description>
          <text>Indicates that the current mode is server.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>It defines NTP server client mode.</text>
    </description>
  </typedef>
  <typedef name="port-version">
    <type name="enumeration">
      <enum name="Version1">
        <value value="1"/>
        <description>
          <text>Version1:</text>
        </description>
      </enum>
      <enum name="Version2">
        <value value="2"/>
        <description>
          <text>Version2:</text>
        </description>
      </enum>
      <enum name="Version3">
        <value value="3"/>
        <description>
          <text>Version3:</text>
        </description>
      </enum>
    </type>
    <description>
      <text>It identifies version of NTP.</text>
    </description>
  </typedef>
  <typedef name="peer-mode">
    <type name="enumeration">
      <enum name="Peer">
        <value value="1"/>
        <description>
          <text>Indicates that the system's current configuration type is peer.</text>
        </description>
      </enum>
      <enum name="Server">
        <value value="2"/>
        <description>
          <text>Indicates that the system's current configuration type is server.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>It defines NTP server or peer mode.</text>
    </description>
  </typedef>
  <typedef name="sync-state">
    <type name="enumeration">
      <enum name="clock-is-not-set">
        <value value="0"/>
        <description>
          <text>Indicates that the clock is not set.</text>
        </description>
      </enum>
      <enum name="frequency-is-set">
        <value value="1"/>
        <description>
          <text>Indicates that clock frequency is obtained from the configuration.</text>
        </description>
      </enum>
      <enum name="time-is-set">
        <value value="2"/>
        <description>
          <text>Indicates that the time is set.</text>
        </description>
      </enum>
      <enum name="mode-is-frequency">
        <value value="3"/>
        <description>
          <text>Indicates that the clock is set but the frequency is not determined.</text>
        </description>
      </enum>
      <enum name="clock-is-synchronized">
        <value value="4"/>
        <description>
          <text>Indicates that the clock is synchronized.</text>
        </description>
      </enum>
      <enum name="spike-is-detected.">
        <value value="5"/>
        <description>
          <text>Indicates that the spike is detected.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>It defines NTP clock sync state.</text>
    </description>
  </typedef>
  <typedef name="clock-status">
    <type name="enumeration">
      <enum name="unsynchronized">
        <value value="0"/>
        <description>
          <text>Indicates that the clock is unsynchronized.</text>
        </description>
      </enum>
      <enum name="synchronized">
        <value value="1"/>
        <description>
          <text>Indicates that the clock is synchronized.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>It defines NTP clock sync status.</text>
    </description>
  </typedef>
  <typedef name="is-configured">
    <type name="enumeration">
      <enum name="No">
        <value value="0"/>
        <description>
          <text>NTP is not configured.</text>
        </description>
      </enum>
      <enum name="Yes">
        <value value="1"/>
        <description>
          <text>NTP is configured.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>It tells weather current source is configured source or not.</text>
    </description>
  </typedef>
  <typedef name="addr-family-type">
    <type name="enumeration">
      <enum name="IPv4">
        <value value="1"/>
        <description>
          <text>IPv4 address family.</text>
        </description>
      </enum>
      <enum name="IPv6">
        <value value="2"/>
        <description>
          <text>IPv6 address family.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>It defines address family supported by NTP.</text>
    </description>
  </typedef>
  <typedef name="extend-addr-family-type">
    <type name="enumeration">
      <enum name="IPv4">
        <value value="1"/>
        <description>
          <text>IPv4 address family.</text>
        </description>
      </enum>
      <enum name="IPv6">
        <value value="2"/>
        <description>
          <text>IPv6 address family.</text>
        </description>
      </enum>
      <enum name="IPv4-AND-IPv6">
        <value value="3"/>
        <description>
          <text>IPv4 and IPv6 address family.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>It defines extended address family supported by NTP.</text>
    </description>
  </typedef>
</submodule>
