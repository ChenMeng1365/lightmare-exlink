                <description>
                  <text>Virtual route forwarding name (1-32 characters).</text>
                </description>
                <type name="string">
                  <length value="1..32">
                    <description>
                      <text>The length of vrfname is from 1 to 32 char.</text>
                    </description>
                  </length>
                </type>
                <mandatory value="true"/>
