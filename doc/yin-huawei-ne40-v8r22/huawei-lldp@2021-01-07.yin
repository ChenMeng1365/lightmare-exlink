  <container name="lldp">
    <presence value="Enables LLDP"/>
    <description>
      <text>Link layer discovery protocol.</text>
    </description>
    <container name="global-attribute">
      <description>
        <text>Configure LLDP global attribute.</text>
      </description>
      <leaf name="message-transmission-interval">
        <type name="int32">
          <range value="5..32768"/>
        </type>
        <units name="s"/>
        <must condition="../message-transmission-interval&gt;=(../message-transmission-delay*4)"/>
        <default value="30"/>
        <description>
          <text>Interval at which LLDP messages are transmitted.</text>
        </description>
      </leaf>
      <leaf name="message-transmission-delay">
        <type name="int32">
          <range value="1..8192"/>
        </type>
        <units name="s"/>
        <must condition="../message-transmission-interval&gt;=(../message-transmission-delay*4)"/>
        <default value="2"/>
        <description>
          <text>Time between two successive LLDP messages.</text>
        </description>
      </leaf>
      <leaf name="message-transmission-hold-multiplier">
        <type name="int32">
          <range value="2..10"/>
        </type>
        <default value="4"/>
        <description>
          <text>Multiplier on the message transmission interval that determines the actual TTL value used in an LLDPDU.</text>
        </description>
      </leaf>
      <leaf name="restart-delay">
        <type name="int32">
          <range value="1..10"/>
        </type>
        <units name="s"/>
        <default value="2"/>
        <description>
          <text>Delay from when LLDP interface status becomes disabled until re-initialization will be attempted.</text>
        </description>
      </leaf>
      <leaf name="trap-interval">
        <type name="int32">
          <range value="5..3600"/>
        </type>
        <units name="s"/>
        <default value="5"/>
        <description>
          <text>LLDP trap notification interval.</text>
        </description>
      </leaf>
      <leaf name="fast-count">
        <type name="int32">
          <range value="1..8"/>
        </type>
        <default value="4"/>
        <description>
          <text>Number of rapidly transmitted LLDP messages.</text>
        </description>
      </leaf>
      <choice name="management-address-mode">
        <description>
          <text>Management address configuration mode.</text>
        </description>
        <case name="global-mode">
          <description>
            <text>Global configuration mode of management address configuration.</text>
          </description>
          <leaf name="managment-address">
            <type name="leafref">
              <path value="/ifm:ifm/ifm:interfaces/ifm:interface/ip:ipv4/ip:addresses/ip:address/ip:ip"/>
            </type>
            <description>
              <text>IP address for network management, which must exist in the device.</text>
            </description>
          </leaf>
          <leaf name="management-address-ipv4-bind-if">
            <type name="leafref">
              <path value="/ifm:ifm/ifm:interfaces/ifm:interface/ifm:name"/>
            </type>
            <description>
              <text>To specify the IP address of an interface as the LLDP management address. The device uses the interface's IP address as the management address, if no management address has been configured. The bound interface must be an Ethernet interface, but Eth-Trunk interfaces cannot be bound.</text>
            </description>
          </leaf>
        </case>
        <case name="interface-mode">
          <description>
            <text>Interface configuration mode of management address configuration.</text>
          </description>
          <leaf name="management-address-if-mode">
            <type name="empty"/>
            <description>
              <text>When the node is configured, the LLDP management address TLV carries the IP address of the local interface.</text>
            </description>
          </leaf>
        </case>
      </choice>
      <leaf name="mdn-status">
        <type name="mdn-global-status"/>
        <default value="disabled"/>
        <description>
          <text>MDN status.</text>
        </description>
      </leaf>
      <leaf name="trap-enable">
        <type name="global-status"/>
        <default value="disabled"/>
        <config value="false"/>
        <description>
          <text>Enable the LLDP neighbor change trap function.</text>
        </description>
      </leaf>
    </container>
    <container name="local-info">
      <config value="false"/>
      <description>
        <text>Statistics of information about local LLDP.</text>
      </description>
      <uses name="chassis-id"/>
      <uses name="system-name"/>
      <uses name="system-description"/>
      <leaf name="up-time">
        <type name="yang:date-and-time"/>
        <description>
          <text>Time when LLDP enabled.</text>
        </description>
      </leaf>
      <uses name="local-system-capabilities"/>
      <uses name="management-address"/>
    </container>
  </container>
  <augment target-node="/ifm:ifm/ifm:interfaces/ifm:interface">
    <when condition="/lldp:lldp and ifm:class = 'main-interface'"/>
    <description>
      <text>LLDP interface.</text>
    </description>
    <container name="lldp">
      <description>
        <text>Configure LLDP interfaces.</text>
      </description>
      <container name="session">
        <description>
          <text>Configure LLDP interface.</text>
        </description>
        <leaf name="admin-status">
          <type name="admin-status"/>
          <default value="tx-rx"/>
          <description>
            <text>LLDP enable status.</text>
          </description>
        </leaf>
        <leaf name="port-mode">
          <when condition="not(../admin-status='disabled') and ../../../ifm:type !='Pos' and ../../../ifm:type !='FlexE-50G' and ../../../ifm:type !='FlexE-100G' and ../../../ifm:type !='FlexE-200GE' and ../../../ifm:type !='FlexE-50|100G' and ../../../ifm:type !='FlexE-400G' and ../../../ifm:type !='FlexE-10G'"/>
          <type name="port-mode"/>
          <default value="normal-mode"/>
          <description>
            <text>LLDP port mode.</text>
          </description>
        </leaf>
        <container name="tlv-enable">
          <when condition="not(../admin-status='disabled')"/>
          <description>
            <text>Configure LLDP TLV.</text>
          </description>
          <leaf name="management-address">
            <type name="boolean"/>
            <default value="true"/>
            <description>
              <text>Enable/disable the device to send the management address TLV.</text>
            </description>
          </leaf>
          <leaf name="port-description">
            <type name="boolean"/>
            <default value="true"/>
            <description>
              <text>Enable/disable the device to send the interface description TLV.</text>
            </description>
          </leaf>
          <leaf name="system-capability">
            <type name="boolean"/>
            <default value="true"/>
            <description>
              <text>Enable/disable the device to send the system capability TLV.</text>
            </description>
          </leaf>
          <leaf name="system-description">
            <type name="boolean"/>
            <default value="true"/>
            <description>
              <text>Enable/disable the device to send the system description TLV.</text>
            </description>
          </leaf>
          <leaf name="system-name">
            <type name="boolean"/>
            <default value="true"/>
            <description>
              <text>Enable/disable the device to send the system name TLV.</text>
            </description>
          </leaf>
          <leaf name="port-vlan-id">
            <when condition="(../../../../ifm:type !='Pos') and (../../../../ifm:type !='FlexE-50G') and (../../../../ifm:type !='FlexE-100G') and (../../../../ifm:type !='FlexE-200GE') and (../../../../ifm:type !='FlexE-50|100G') and (../../../../ifm:type !='FlexE-400G') and (../../../../ifm:type !='FlexE-10G')"/>
            <type name="boolean"/>
            <default value="true"/>
            <description>
              <text>Enable/disable the device to send the interface VLAN ID TLV.</text>
            </description>
          </leaf>
          <leaf name="protocol-vlan-id-enable">
            <when condition="(../../../../ifm:type !='Pos') and (../../../../ifm:type !='FlexE-50G') and (../../../../ifm:type !='FlexE-100G') and (../../../../ifm:type !='FlexE-200GE') and (../../../../ifm:type !='FlexE-50|100G') and (../../../../ifm:type !='FlexE-400G') and (../../../../ifm:type !='FlexE-10G')"/>
            <type name="boolean"/>
            <default value="true"/>
            <description>
              <text>Enable/disable the device to send the protocol VLAN ID TLV.</text>
            </description>
          </leaf>
          <leaf name="protocol-vlan-id">
            <when condition="(not(../protocol-vlan-id-enable='false')) and (../../../../ifm:type !='Pos') and (../../../../ifm:type !='FlexE-50G') and (../../../../ifm:type !='FlexE-100G') and (../../../../ifm:type !='FlexE-200GE') and (../../../../ifm:type !='FlexE-50|100G') and (../../../../ifm:type !='FlexE-400G') and (../../../../ifm:type !='FlexE-10G')"/>
            <type name="int32">
              <range value="1..4094"/>
            </type>
            <description>
              <text>The protocol VLAN ID is sent.</text>
            </description>
          </leaf>
          <leaf name="vlan-name-enable">
            <when condition="(../../../../ifm:type !='Pos') and (../../../../ifm:type !='FlexE-50G') and (../../../../ifm:type !='FlexE-100G') and (../../../../ifm:type !='FlexE-200GE') and (../../../../ifm:type !='FlexE-50|100G') and (../../../../ifm:type !='FlexE-400G') and (../../../../ifm:type !='FlexE-10G')"/>
            <type name="boolean"/>
            <default value="true"/>
            <description>
              <text>Enable/disable the device to send the VLAN name TLV.</text>
            </description>
          </leaf>
          <leaf name="vlan-name-id">
            <when condition="(not(../vlan-name-enable='false')) and (../../../../ifm:type !='Pos') and (../../../../ifm:type !='FlexE-50G') and (../../../../ifm:type !='FlexE-100G') and (../../../../ifm:type !='FlexE-200GE') and (../../../../ifm:type !='FlexE-50|100G') and (../../../../ifm:type !='FlexE-400G') and (../../../../ifm:type !='FlexE-10G')"/>
            <type name="int32">
              <range value="1..4094"/>
            </type>
            <description>
              <text>The VLAN ID of the VLAN name TLV is sent.</text>
            </description>
          </leaf>
          <leaf name="protocol-identity">
            <when condition="(../../../../ifm:type !='Pos') and (../../../../ifm:type !='FlexE-50G') and (../../../../ifm:type !='FlexE-100G') and (../../../../ifm:type !='FlexE-200GE') and (../../../../ifm:type !='FlexE-50|100G') and (../../../../ifm:type !='FlexE-400G') and (../../../../ifm:type !='FlexE-10G')"/>
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable the device to send the protocol identity TLV.</text>
            </description>
          </leaf>
          <leaf name="mac-physic">
            <when condition="(../../../../ifm:type !='Pos') and (../../../../ifm:type !='FlexE-50G') and (../../../../ifm:type !='FlexE-100G') and (../../../../ifm:type !='FlexE-200GE') and (../../../../ifm:type !='FlexE-50|100G') and (../../../../ifm:type !='FlexE-400G') and (../../../../ifm:type !='FlexE-10G')"/>
            <type name="boolean"/>
            <default value="true"/>
            <description>
              <text>Enable/disable the device to send the MAC/PHY TLV.</text>
            </description>
          </leaf>
          <leaf name="link-aggregation">
            <when condition="(../../../../ifm:type !='Pos') and (../../../../ifm:type !='FlexE-50G') and (../../../../ifm:type !='FlexE-100G') and (../../../../ifm:type !='FlexE-200GE') and (../../../../ifm:type !='FlexE-50|100G') and (../../../../ifm:type !='FlexE-400G') and (../../../../ifm:type !='FlexE-10G')"/>
            <type name="boolean"/>
            <default value="true"/>
            <description>
              <text>Enable/disable the device to send the link aggregation TLV.</text>
            </description>
          </leaf>
          <leaf name="max-frame-size">
            <when condition="(../../../../ifm:type !='Pos') and (../../../../ifm:type !='FlexE-50G') and (../../../../ifm:type !='FlexE-100G') and (../../../../ifm:type !='FlexE-200GE') and (../../../../ifm:type !='FlexE-50|100G') and (../../../../ifm:type !='FlexE-400G') and (../../../../ifm:type !='FlexE-10G')"/>
            <type name="boolean"/>
            <default value="true"/>
            <description>
              <text>Enable/disable the device to send the maximum frame size TLV.</text>
            </description>
          </leaf>
          <container name="power-tlv">
            <description>
              <text>Configure the LLDP IEEE 802.3 Power Via MDI TLV.</text>
            </description>
            <leaf name="power">
              <type name="boolean"/>
              <default value="true"/>
              <description>
                <text>Enable/disable the device to send the Power Via MDI TLV.</text>
              </description>
            </leaf>
            <leaf name="mode">
              <type name="dot3-power-format-type"/>
              <must condition="(current()!='802.3bt-draft') and (current()!='802.3bt-2018standard')"/>
              <default value="first-802.1ab-then-auto"/>
              <description>
                <text>Mode of IEEE 802.3 Power Via MDI TLV.</text>
              </description>
            </leaf>
          </container>
          <leaf name="ddp">
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable the device to send the DDP TLV.</text>
            </description>
          </leaf>
          <leaf name="identity">
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable the device to send the identity TLV.</text>
            </description>
          </leaf>
          <container name="med-tlv">
            <description>
              <text>Configure LLDP MED TLV.</text>
            </description>
            <leaf name="capability">
              <type name="boolean"/>
              <must condition="(current()='false') or (../../mac-physic = 'true')"/>
              <default value="true"/>
              <description>
                <text>Enable/disable the device to send the MED Capability TLV.</text>
              </description>
            </leaf>
            <leaf name="network-policy">
              <when condition="((../../../../../ethernet:ethernet/ethernet:main-interface/ethernet:l2-attribute) or (/ifm:ifm/ifm:interfaces/ifm:interface[ifm:name = ifm-trunk:trunk/ifm-trunk:members/ifm-trunk:member[ifm-trunk:name=current()/../../../../../ifm:name]/../../../ifm:name]/ethernet:ethernet/ethernet:main-interface/ethernet:l2-attribute))"/>
              <type name="boolean"/>
              <must condition="(current()='false') or (../capability='true')"/>
              <default value="true"/>
              <description>
                <text>Enable/disable the device to send the MED Network Policy TLV.</text>
              </description>
            </leaf>
            <choice name="network-policy-attribute">
              <description>
                <text>Configure MED Network Policy TLV.</text>
              </description>
              <case name="untag">
                <description>
                  <text>Configure the telephone to send untagged voice traffic.</text>
                </description>
                <leaf name="untagged">
                  <when condition="../network-policy='true'"/>
                  <type name="empty"/>
                  <description>
                    <text>Configure the telephone to send untagged voice traffic.</text>
                  </description>
                </leaf>
              </case>
              <case name="voice-vlan">
                <description>
                  <text>Configure the telephone to send voice traffic with VLAN tags.</text>
                </description>
                <leaf name="vlan-id">
                  <when condition="../network-policy='true'"/>
                  <type name="uint16">
                    <range value="1..4094"/>
                  </type>
                  <description>
                    <text>VOICE VLAN ID.</text>
                  </description>
                </leaf>
                <leaf name="cos">
                  <when condition="../vlan-id"/>
                  <type name="uint8">
                    <range value="0..7"/>
                  </type>
                  <description>
                    <text>Layer 2 priority class of service (CoS).</text>
                  </description>
                </leaf>
                <leaf name="dscp">
                  <when condition="../vlan-id"/>
                  <type name="uint8">
                    <range value="0..63"/>
                  </type>
                  <description>
                    <text>The differentiated services code point (DSCP) value.</text>
                  </description>
                </leaf>
              </case>
            </choice>
            <leaf name="power-over-ethernet">
              <type name="boolean"/>
              <must condition="(current()='false') or (../capability = 'true')"/>
              <default value="true"/>
              <description>
                <text>Enable/disable the device to send the MED Extended Power Via MDI TLV.</text>
              </description>
            </leaf>
          </container>
        </container>
        <container name="local-info">
          <config value="false"/>
          <description>
            <text>Statistics of information about local LLDP.</text>
          </description>
          <uses name="port-id"/>
          <uses name="port-description"/>
          <uses name="port-vlan-id"/>
          <uses name="local-protocol-vlan"/>
          <uses name="local-vlan-name"/>
          <uses name="protocol-identity"/>
          <uses name="mac-physic"/>
          <uses name="link-aggregation"/>
          <uses name="maximum-frame-size"/>
          <uses name="management-address"/>
          <uses name="local-power-tlv"/>
          <uses name="identity-tlv"/>
          <container name="med-tlv">
            <description>
              <text>Statistics of information about the local MED TLVs.</text>
            </description>
            <uses name="med-network-policy-tlvs"/>
            <uses name="med-extended-power-tlv"/>
          </container>
        </container>
        <container name="neighbors">
          <config value="false"/>
          <description>
            <text>List of information about LLDP neighbors.</text>
          </description>
          <list name="neighbor">
            <key value="index"/>
            <description>
              <text>Statistics of information about an LLDP neighbor.</text>
            </description>
            <leaf name="index">
              <type name="uint32">
                <range value="0..4294967295"/>
              </type>
              <description>
                <text>Neighbor index.</text>
              </description>
            </leaf>
            <uses name="neighbor-common-info"/>
            <uses name="remote-power-tlv"/>
            <uses name="ddp-tlv"/>
            <uses name="identity-tlv"/>
            <container name="med-tlv">
              <description>
                <text>Statistics of information about the MED TLVs.</text>
              </description>
              <uses name="med-capability-tlv"/>
              <uses name="med-network-policy-tlvs"/>
              <uses name="med-extended-power-tlv"/>
            </container>
            <uses name="legacy-power-capability-tlv"/>
          </list>
        </container>
        <container name="statistics">
          <config value="false"/>
          <description>
            <text>Statistics of information about LLDP interface.</text>
          </description>
          <leaf name="total-neighbors">
            <type name="uint32">
              <range value="0..4294967295"/>
            </type>
            <description>
              <text>Total number of neighbors.</text>
            </description>
          </leaf>
          <leaf name="transmitted-frames">
            <type name="uint32">
              <range value="0..4294967295"/>
            </type>
            <description>
              <text>Total number of transmitted frames.</text>
            </description>
          </leaf>
          <leaf name="received-frames">
            <type name="uint32">
              <range value="0..4294967295"/>
            </type>
            <description>
              <text>Total number of received frames.</text>
            </description>
          </leaf>
          <leaf name="discarded-frames">
            <type name="uint32">
              <range value="0..4294967295"/>
            </type>
            <description>
              <text>Total number of discarded frames.</text>
            </description>
          </leaf>
          <leaf name="error-frames">
            <type name="uint32">
              <range value="0..4294967295"/>
            </type>
            <description>
              <text>Total number of error frames.</text>
            </description>
          </leaf>
          <leaf name="discarded-tlvs">
            <type name="uint32">
              <range value="0..4294967295"/>
            </type>
            <description>
              <text>Total number of discarded TLVs.</text>
            </description>
          </leaf>
          <leaf name="unrecognized-tlvs">
            <type name="uint32">
              <range value="0..4294967295"/>
            </type>
            <description>
              <text>Total number of unrecognized TLVs.</text>
            </description>
          </leaf>
          <leaf name="expired-neighbors">
            <type name="uint32">
              <range value="0..4294967295"/>
            </type>
            <description>
              <text>Total number of expired neighbors.</text>
            </description>
          </leaf>
          <leaf name="transmitted-dcbx-tlvs">
            <type name="uint32">
              <range value="0..4294967295"/>
            </type>
            <description>
              <text>Total number of transmitted DCBX TLVs.</text>
            </description>
          </leaf>
          <leaf name="received-dcbx-tlvs">
            <type name="uint32">
              <range value="0..4294967295"/>
            </type>
            <description>
              <text>Total number of received DCBX TLVs.</text>
            </description>
          </leaf>
          <leaf name="transmitted-med-tlvs">
            <type name="uint32">
              <range value="0..4294967295"/>
            </type>
            <description>
              <text>Total number of transmitted MED TLVs.</text>
            </description>
          </leaf>
          <leaf name="received-med-tlvs">
            <type name="uint32">
              <range value="0..4294967295"/>
            </type>
            <description>
              <text>Total number of received MED TLVs.</text>
            </description>
          </leaf>
          <leaf name="received-network-card-id-tlvs">
            <type name="uint32">
              <range value="0..4294967295"/>
            </type>
            <description>
              <text>Total number of received Network Card ID TLVs.</text>
            </description>
          </leaf>
          <leaf name="last-cleared-time">
            <type name="yang:date-and-time"/>
            <description>
              <text>Time when LLDP statistics were last cleared.</text>
            </description>
          </leaf>
        </container>
      </container>
    </container>
  </augment>
  <augment target-node="/ifm:ifm/ifm:interfaces/ifm:interface">
    <description>
      <text>LLDP sub-interface.</text>
    </description>
    <container name="sub-interface-lldp">
      <when condition="/lldp:lldp and ../ifm:class = 'sub-interface' and ../ethernet:ethernet/ethernet:l3-sub-interface/ethernet:vlan-type-dot1q"/>
      <description>
