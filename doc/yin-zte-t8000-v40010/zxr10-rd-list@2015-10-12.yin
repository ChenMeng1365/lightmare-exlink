<?xml version="1.0" encoding="UTF-8"?>
<module name="zxr10-rd-list"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:rd-list="http://www.zte.com.cn/zxr10/netconf/schema/rosng/rd-list"
        xmlns:ncd="http://www.zte.com.cn/zxr10/netconf/schema/rosng/netconf-defs">
  <namespace uri="http://www.zte.com.cn/zxr10/netconf/schema/rosng/rd-list"/>
  <prefix value="rd-list"/>
  <import module="zxr10-netconf-defs">
    <prefix value="ncd"/>
  </import>
  <organization>
    <text>ZTE Corporation</text>
  </organization>
  <contact>
    <text>ZTE Corporation  
   Nanjing Institute of ZTE Corporation 
   No.68 Zijinghua Rd. Yuhuatai District, Nanjing, China 
   Tel: +86-25-52870000</text>
  </contact>
  <description>
    <text>The netconf module to describe route distinguisher list.</text>
  </description>
  <revision date="2015-10-12">
    <description>
      <text>Initial version.</text>
    </description>
  </revision>
  <container name="configuration">
    <description>
      <text>Route distinguisher list configuration.</text>
    </description>
    <container name="rd-list">
      <description>
        <text>Route distinguisher list module.</text>
      </description>
      <container name="standards">
        <description>
          <text>Standard route distinguisher list.</text>
        </description>
        <list name="standard">
          <description>
            <text>Standard route distinguisher list.</text>
          </description>
          <key value="list-no action value"/>
          <max-elements value="4000"/>
          <leaf name="list-no">
            <description>
              <text>Route distinguisher list number,the range of value is from 1 to 99.</text>
            </description>
            <type name="uint16">
              <range value="1..99">
                <description>
                  <text>The range of value is from 1 to 99.</text>
                </description>
              </range>
            </type>
          </leaf>
          <leaf name="action">
            <description>
              <text>Deny:Specify route distinguisher list to reject.
Permit:Specify route distinguisher list to accept.</text>
            </description>
            <type name="enumeration">
              <enum name="deny">
                <value value="0"/>
                <description>
                  <text>Specify rd list to reject.</text>
                </description>
              </enum>
              <enum name="permit">
                <value value="1"/>
                <description>
                  <text>Specify rd list to accpet.</text>
                </description>
              </enum>
            </type>
          </leaf>
          <leaf name="value">
            <description>
              <text>&lt;0-65535&gt;:&lt;0-4294967295&gt;       Route distinguisher value.
&lt;1-65535&gt;.&lt;0-65535&gt;:&lt;0-65535&gt;  Route distinguisher value.
A.B.C.D:&lt;0-65535&gt;              Route distinguisher value.</text>
            </description>
            <type name="union">
              <type name="ncd:asn-nn"/>
              <type name="ncd:asnd-nn"/>
              <type name="ncd:ipadd-nn"/>
            </type>
          </leaf>
        </list>
      </container>
      <container name="expandeds">
        <description>
          <text>Expanded route distinguisher list.</text>
        </description>
        <list name="expanded">
          <description>
            <text>Expanded route distinguisher list.</text>
          </description>
          <key value="list-no action value"/>
          <max-elements value="4000"/>
          <leaf name="list-no">
            <description>
              <text>Route distinguisher list number,the range of value is from 100 to 500.</text>
            </description>
            <type name="uint16">
              <range value="100..500">
                <description>
                  <text>The range of value is from 100 to 500.</text>
                </description>
              </range>
            </type>
          </leaf>
          <leaf name="action">
            <description>
              <text>Deny:Specify route distinguisher list to reject.
Permit:Specify route distinguisher list to accept.</text>
            </description>
            <type name="enumeration">
              <enum name="deny">
                <value value="0"/>
                <description>
                  <text>Specify rd list to reject.</text>
                </description>
              </enum>
              <enum name="permit">
                <value value="1"/>
                <description>
                  <text>Specify rd list to accpet.</text>
                </description>
              </enum>
            </type>
          </leaf>
          <leaf name="value">
            <description>
              <text>An route distinguisher list as a regular-expression.</text>
            </description>
            <type name="ncd:string-with-blank">
              <length value="1..63">
                <description>
                  <text>The length of this value is from 1 to 63.</text>
                </description>
              </length>
            </type>
          </leaf>
        </list>
      </container>
    </container>
  </container>
</module>
