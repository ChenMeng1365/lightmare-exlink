<?xml version="1.0" encoding="UTF-8"?>
<module name="zxr10-pm-env-satellite"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:pm-env-satellite="http://www.zte.com.cn/zxr10/netconf/schema/rosng/pm-env-satellite"
        xmlns:ncd="http://www.zte.com.cn/zxr10/netconf/schema/rosng/netconf-defs">
  <namespace uri="http://www.zte.com.cn/zxr10/netconf/schema/rosng/pm-env-satellite"/>
  <prefix value="pm-env-satellite"/>
  <yang-version value="1"/>
  <import module="zxr10-netconf-defs">
    <prefix value="ncd"/>
  </import>
  <organization>
    <text>ZTE Corporation</text>
  </organization>
  <contact>
    <text>ZTE Corporation  
   Nanjing Institute of ZTE Corporation 
   No.68 Zijinghua Rd. Yuhuatai District, Nanjing, China 
   Tel: +86-25-52870000</text>
  </contact>
  <revision date="2021-03-03">
    <description>
      <text>add reload system.</text>
    </description>
  </revision>
  <revision date="2021-02-02">
    <description>
      <text>add fan temp sa</text>
    </description>
  </revision>
  <revision date="2021-01-29">
    <description>
      <text>Initialize version.</text>
    </description>
  </revision>
  <container name="state">
    <config value="false"/>
    <container name="env">
      <container name="opticals">
        <list name="optical">
          <key value="peid if-name"/>
          <leaf name="peid">
            <type name="uint32">
              <range value="1..128"/>
            </type>
          </leaf>
          <leaf name="if-name">
            <type name="string"/>
          </leaf>
          <leaf name="optical-status">
            <type name="enumeration">
              <enum name="offline">
                <value value="0"/>
                <description>
                  <text>The optical is offline.</text>
                </description>
              </enum>
              <enum name="online">
                <value value="1"/>
                <description>
                  <text>The optical is online.</text>
                </description>
              </enum>
            </type>
            <default value="offline"/>
          </leaf>
          <leaf name="optical-electric-mode">
            <type name="uint8"/>
          </leaf>
          <leaf name="transceiver-type">
            <type name="ncd:string-all"/>
          </leaf>
          <leaf name="single-fiber-double-orient">
            <type name="uint8"/>
          </leaf>
          <leaf name="media-type">
            <type name="uint8"/>
          </leaf>
          <leaf name="send-wavelength">
            <type name="uint32"/>
          </leaf>
          <leaf name="length-9um-smf">
            <type name="uint32"/>
          </leaf>
          <leaf name="length-50um-om2">
            <type name="uint32"/>
          </leaf>
          <leaf name="length-62um-om1">
            <type name="uint32"/>
          </leaf>
          <leaf name="length-50um-om3">
            <type name="uint32"/>
          </leaf>
          <leaf name="copper-line">
            <type name="uint32"/>
          </leaf>
          <leaf name="sonet-codes">
            <type name="ncd:string-all"/>
          </leaf>
          <leaf name="ethernet-codes">
            <type name="ncd:string-all"/>
          </leaf>
          <leaf name="vendor-name">
            <type name="ncd:string-all"/>
          </leaf>
          <leaf name="vendor-pn">
            <type name="ncd:string-all"/>
          </leaf>
          <leaf name="vendor-sn">
            <type name="ncd:string-all"/>
          </leaf>
          <leaf name="vendor-rev">
            <type name="ncd:string-all"/>
          </leaf>
          <leaf name="send-power">
            <type name="ncd:string-all"/>
          </leaf>
          <leaf name="receive-power">
            <type name="ncd:string-all"/>
          </leaf>
          <leaf name="channel-number">
            <type name="uint8"/>
          </leaf>
          <leaf name="connector-type">
            <type name="ncd:string-all"/>
          </leaf>
        </list>
      </container>
      <container name="temperatures">
        <list name="temperature">
          <key value="peid"/>
          <leaf name="peid">
            <type name="uint32">
              <range value="1..128"/>
            </type>
          </leaf>
          <leaf name="shelf">
            <description>
              <text>Shelf no.</text>
            </description>
            <type name="uint8"/>
          </leaf>
          <leaf name="slot">
            <description>
              <text>Slot no.</text>
            </description>
            <type name="uint8"/>
          </leaf>
          <leaf name="current-temperature">
            <type name="int32"/>
          </leaf>
          <leaf name="fatal-threshold">
            <type name="uint32"/>
          </leaf>
        </list>
        <ncd:revise-date date="2021-02-02"/>
      </container>
      <container name="fans">
        <list name="fan">
          <key value="peid"/>
          <leaf name="peid">
            <type name="uint32">
              <range value="1..128"/>
            </type>
          </leaf>
          <leaf name="shelf">
            <description>
              <text>Shelf no.</text>
            </description>
            <type name="uint8"/>
          </leaf>
          <leaf name="group">
            <description>
              <text>Slot no.</text>
            </description>
            <type name="uint8"/>
          </leaf>
          <leaf name="index">
            <description>
              <text>Shelf no.</text>
            </description>
            <type name="uint8"/>
          </leaf>
          <leaf name="fan-present">
            <type name="uint8"/>
          </leaf>
          <leaf name="work-status">
            <type name="uint8"/>
          </leaf>
        </list>
        <ncd:revise-date date="2021-02-02"/>
      </container>
    </container>
  </container>
  <container name="operation">
    <config value="false"/>
    <container name="env">
      <container name="reload-systems">
        <list name="reload-system">
          <key value="peid"/>
          <leaf name="peid">
            <description>
              <text>Peid.</text>
            </description>
            <type name="uint32">
              <range value="1..128"/>
            </type>
          </leaf>
          <leaf name="reload-mode">
            <description>
              <text>Reload mode.</text>
            </description>
            <type name="enumeration">
              <enum name="force">
                <value value="0"/>
              </enum>
            </type>
          </leaf>
          <ncd:select fields="peid"/>
        </list>
      </container>
    </container>
    <ncd:revise-date date="2021-03-03"/>
  </container>
</module>
