  <container name="macsec">
    <description>
      <text>Containers of MACsec nodes.</text>
    </description>
    <container name="statistics">
      <config value="false"/>
      <description>
        <text>Statistics of MACsec under interface.</text>
      </description>
      <container name="interface-macsecs">
        <description>
          <text>List of MACsec data packet statistics.</text>
        </description>
        <list name="interface-macsec">
          <key value="interface-name"/>
          <description>
            <text>MACsec data packet statistics.</text>
          </description>
          <leaf name="interface-name">
            <type name="leafref">
              <path value="/ifm:ifm/ifm:interfaces/ifm:interface/ifm:name"/>
            </type>
            <description>
              <text>Interface name.</text>
            </description>
          </leaf>
          <leaf name="valid-packets">
            <type name="uint64"/>
            <units name="packet"/>
            <description>
              <text>Number of packets protected by MACsec.</text>
            </description>
          </leaf>
          <leaf name="input-protected-bytes">
            <type name="uint64"/>
            <units name="Byte"/>
            <description>
              <text>Number of bytes for input which only integrity check is implemented but encryption is not.</text>
            </description>
          </leaf>
          <leaf name="decrypted-bytes">
            <type name="uint64"/>
            <units name="Byte"/>
            <description>
              <text>Number of bytes for which both integrity check and encryption are implemented.</text>
            </description>
          </leaf>
          <leaf name="late-packets">
            <type name="uint64"/>
            <units name="packet"/>
            <description>
              <text>Number of packets that fail the replay window size check.</text>
            </description>
          </leaf>
          <leaf name="not-valid-packets">
            <type name="uint64"/>
            <units name="packet"/>
            <description>
              <text>Number of packets that fail the integrity check or encounter decryption errors.</text>
            </description>
          </leaf>
          <leaf name="badtag-packets">
            <type name="uint64"/>
            <units name="packet"/>
            <description>
              <text>Number of packets with incorrect ICV length or invalid sectag header.</text>
            </description>
          </leaf>
          <leaf name="no-using-sa-packets">
            <type name="uint64"/>
            <units name="packet"/>
            <description>
              <text>Number of packets with SAs obtained from received sectag being unavailable.</text>
            </description>
          </leaf>
          <leaf name="protected-packets">
            <type name="uint64"/>
            <units name="packet"/>
            <description>
              <text>Number of packets for which only ICV is encapsulated.</text>
            </description>
          </leaf>
          <leaf name="output-protected-bytes">
            <type name="uint64"/>
            <units name="Byte"/>
            <description>
              <text>Protected bytes for output.</text>
            </description>
          </leaf>
          <leaf name="encrypted-packets">
            <type name="uint64"/>
            <units name="packet"/>
            <description>
              <text>Number of packets for which ICV is encapsulated and encryption is implemented.</text>
            </description>
          </leaf>
          <leaf name="encrypted-bytes">
            <type name="uint64"/>
            <units name="Byte"/>
            <description>
              <text>Number of bytes for which ICV is encapsulated and encryption is implemented.</text>
            </description>
          </leaf>
        </list>
      </container>
