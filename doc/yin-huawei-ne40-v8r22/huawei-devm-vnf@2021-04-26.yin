<?xml version="1.0" encoding="UTF-8"?>
<module name="huawei-devm-vnf"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:devm-vnf="urn:huawei:yang:huawei-devm-vnf"
        xmlns:ext="urn:huawei:yang:huawei-extension"
        xmlns:pub-type="urn:huawei:yang:huawei-pub-type"
        xmlns:inet="urn:ietf:params:xml:ns:yang:ietf-inet-types">
  <namespace uri="urn:huawei:yang:huawei-devm-vnf"/>
  <prefix value="devm-vnf"/>
  <import module="huawei-extension">
    <prefix value="ext"/>
  </import>
  <import module="huawei-pub-type">
    <prefix value="pub-type"/>
  </import>
  <import module="ietf-inet-types">
    <prefix value="inet"/>
  </import>
  <organization>
    <text>Huawei Technologies Co., Ltd.</text>
  </organization>
  <contact>
    <text>Huawei Industrial Base
Bantian, Longgang
Shenzhen 518129
People's Republic of China
Website: http://www.huawei.com
Email: support@huawei.com</text>
  </contact>
  <description>
    <text>Configuration management module.</text>
  </description>
  <revision date="2021-04-26">
    <description>
      <text>Add container vms.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <revision date="2020-04-10">
    <description>
      <text>Modify description warnings.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <revision date="2019-07-01">
    <description>
      <text>Initial revision.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <typedef name="run-state-type">
    <type name="enumeration">
      <enum name="abnormal">
        <value value="0"/>
        <description>
          <text>Abnormal.</text>
        </description>
      </enum>
      <enum name="normal">
        <value value="1"/>
        <description>
          <text>Normal.</text>
        </description>
      </enum>
      <enum name="isolated">
        <value value="2"/>
        <description>
          <text>Isolated.</text>
        </description>
      </enum>
      <enum name="notinstall">
        <value value="3"/>
        <description>
          <text>Not install.</text>
        </description>
      </enum>
      <enum name="wrongtype">
        <value value="4"/>
        <description>
          <text>Wrong type.</text>
        </description>
      </enum>
      <enum name="active">
        <value value="5"/>
        <description>
          <text>Active.</text>
        </description>
      </enum>
      <enum name="inactive">
        <value value="6"/>
        <description>
          <text>Inactive.</text>
        </description>
      </enum>
      <enum name="offline">
        <value value="7"/>
        <description>
          <text>Offline.</text>
        </description>
      </enum>
      <enum name="stop">
        <value value="8"/>
        <description>
          <text>Stop.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Run state.</text>
    </description>
  </typedef>
  <typedef name="resource-type">
    <type name="enumeration">
      <enum name="vm">
        <value value="0"/>
        <description>
          <text>Virtual machine.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Resource source type.</text>
    </description>
  </typedef>
  <typedef name="standby-state-type">
    <type name="enumeration">
      <enum name="not-supported">
        <value value="0"/>
        <description>
          <text>No standby state.</text>
        </description>
      </enum>
      <enum name="master">
        <value value="1"/>
        <description>
          <text>Master board.</text>
        </description>
      </enum>
      <enum name="slave">
        <value value="2"/>
        <description>
          <text>Slave board.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Board standby state.</text>
    </description>
  </typedef>
  <typedef name="board-role-type">
    <type name="enumeration">
      <enum name="unknown">
        <value value="0"/>
        <description>
          <text>Unknown.</text>
        </description>
      </enum>
      <enum name="mmb">
        <value value="1"/>
        <description>
          <text>MMB.</text>
        </description>
      </enum>
      <enum name="mb">
        <value value="4"/>
        <description>
          <text>MB.</text>
        </description>
      </enum>
      <enum name="lc">
        <value value="5"/>
        <description>
          <text>LC.</text>
        </description>
      </enum>
      <enum name="other">
        <value value="200"/>
        <description>
          <text>OTHER.</text>
        </description>
      </enum>
      <enum name="na">
        <value value="255"/>
        <description>
          <text>NA.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Board role.</text>
    </description>
  </typedef>
  <typedef name="network-plane-type">
    <type name="enumeration">
      <enum name="BASE">
        <value value="0"/>
        <description>
          <text>Basic service interface.</text>
        </description>
      </enum>
      <enum name="INNERMNG">
        <value value="1"/>
        <description>
          <text>Internal management interface.</text>
        </description>
      </enum>
      <enum name="OUTERMNG">
        <value value="2"/>
        <description>
          <text>External management interface.</text>
        </description>
      </enum>
      <enum name="OUTERLINK">
        <value value="3"/>
        <description>
          <text>External connection interface.</text>
        </description>
      </enum>
      <enum name="INTRASERV">
        <value value="4"/>
        <description>
          <text>Fabric plane sub-interface.</text>
        </description>
      </enum>
      <enum name="INNERPAE">
        <value value="5"/>
        <description>
          <text>Inter-PAE communication interface.</text>
        </description>
      </enum>
      <enum name="OUTERLINUX">
        <value value="6"/>
        <description>
          <text>External linux interface.</text>
        </description>
      </enum>
      <enum name="NLSOUTERMNG">
        <value value="7"/>
        <description>
          <text>VNFC external management interface.</text>
        </description>
      </enum>
      <enum name="INNERSLE">
        <value value="8"/>
        <description>
          <text>Master/Slave arbitration extension interface.</text>
        </description>
      </enum>
      <enum name="INNERDIRECTLOGIC">
        <value value="9"/>
        <description>
          <text>Forwarding topology logical interface.</text>
        </description>
      </enum>
      <enum name="INNERCHAINLOGIC">
        <value value="10"/>
        <description>
          <text>Forwarding integration logical interface.</text>
        </description>
      </enum>
      <enum name="INNERLOGICSERV">
        <value value="11"/>
        <description>
          <text>Internal logic service interface.</text>
        </description>
      </enum>
      <enum name="EXTBASE">
        <value value="12"/>
        <description>
          <text>Second basic service interface.</text>
        </description>
      </enum>
      <enum name="EXTINNERLOGICSERV">
        <value value="13"/>
        <description>
          <text>Second internal logic service interface.</text>
        </description>
      </enum>
      <enum name="INNERCTRL">
        <value value="14"/>
        <description>
          <text>Internal control interface.</text>
        </description>
      </enum>
      <enum name="TUNNEL">
        <value value="15"/>
        <description>
          <text>Internal data plane socket communication interface.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Network plane type.</text>
    </description>
  </typedef>
  <typedef name="port-state-type">
    <type name="enumeration">
      <enum name="DOWN">
        <value value="0"/>
        <description>
          <text>The virtual interface is Down.</text>
        </description>
      </enum>
      <enum name="UP">
        <value value="1"/>
        <description>
          <text>The virtual interface is Up.</text>
        </description>
      </enum>
      <enum name="ADMINDOWN">
        <value value="2"/>
        <description>
          <text>The virtual interface is administratively Down.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Port state type.</text>
    </description>
  </typedef>
  <ext:task-name value="device-mgr"/>
  <container name="devm-vnf">
    <description>
      <text>Virtualized network function (VNF) infomation.</text>
    </description>
    <container name="vnf-boards">
      <description>
        <text>List of virtualized network function (VNF) board.</text>
      </description>
      <list name="vnf-board">
        <key value="vnfc-name id"/>
        <description>
          <text>Configure virtualized network function (VNF) board.</text>
        </description>
        <leaf name="vnfc-name">
          <type name="string">
            <length value="1..31"/>
          </type>
          <description>
            <text>Virtualized network function component (VNFC) name.</text>
          </description>
        </leaf>
        <leaf name="id">
          <type name="uint32"/>
          <description>
            <text>Board ID.</text>
          </description>
        </leaf>
        <leaf name="type-name">
          <ext:operation-exclude value="update"/>
          <type name="string">
            <length value="1..63"/>
          </type>
          <mandatory value="true"/>
          <description>
            <text>Type name.</text>
          </description>
        </leaf>
        <leaf name="name">
          <type name="string">
            <length value="1..63"/>
          </type>
          <config value="false"/>
          <description>
            <text>Board name.</text>
          </description>
        </leaf>
        <leaf name="run-state">
          <type name="run-state-type"/>
          <config value="false"/>
          <description>
            <text>Running status.</text>
          </description>
        </leaf>
        <leaf name="resource-type">
          <type name="resource-type"/>
          <config value="false"/>
          <description>
            <text>Resource Type.</text>
          </description>
        </leaf>
        <leaf name="resource-name">
          <type name="string">
            <length value="1..63"/>
          </type>
          <config value="false"/>
          <description>
            <text>Name of a resource source.</text>
          </description>
        </leaf>
        <leaf name="standby-state">
          <type name="standby-state-type"/>
          <config value="false"/>
          <description>
            <text>Standby state of a VNF board.</text>
          </description>
        </leaf>
        <leaf name="role">
          <type name="board-role-type"/>
          <config value="false"/>
          <description>
            <text>MPU role.</text>
          </description>
        </leaf>
        <leaf name="up-time">
          <type name="pub-type:time-tick"/>
          <config value="false"/>
          <description>
            <text>Running period.</text>
          </description>
        </leaf>
        <leaf name="physical-index">
          <type name="uint32"/>
          <config value="false"/>
          <description>
            <text>Entity index.</text>
          </description>
        </leaf>
      </list>
    </container>
    <container name="vms">
      <config value="false"/>
      <description>
        <text>List of virtual machines.</text>
      </description>
      <list name="vm">
        <key value="name"/>
        <description>
          <text>Operational state of virtual machine.</text>
        </description>
        <leaf name="name">
          <type name="string">
            <length value="1..63"/>
          </type>
          <description>
            <text>Virtual machine name.</text>
          </description>
        </leaf>
        <leaf name="logic-id">
          <type name="uint32">
            <range value="1..65535"/>
          </type>
          <description>
            <text>Logical virtual machine ID.</text>
          </description>
        </leaf>
        <leaf name="id">
          <type name="string">
            <length value="1..63"/>
          </type>
          <description>
            <text>Virtual machine ID.</text>
          </description>
        </leaf>
        <leaf name="phy-id">
          <type name="string">
            <length value="1..63"/>
          </type>
          <default value="0"/>
          <description>
            <text>Physical virtual machine ID.</text>
          </description>
        </leaf>
        <leaf name="type-name">
          <type name="string">
            <length value="1..63"/>
          </type>
          <description>
            <text>Virtual machine type name.</text>
          </description>
        </leaf>
        <leaf name="run-state">
          <type name="run-state-type"/>
          <description>
            <text>Virtual machine running state.</text>
          </description>
        </leaf>
        <leaf name="memory-size">
          <type name="uint32">
            <range value="0..4294967295"/>
          </type>
          <units name="MB"/>
          <description>
            <text>Memory size.</text>
          </description>
        </leaf>
        <leaf name="storage-size">
          <type name="uint32">
            <range value="0..4294967295"/>
          </type>
          <units name="GB"/>
          <description>
            <text>Storage size.</text>
          </description>
        </leaf>
        <leaf name="cpu-number">
          <type name="uint32">
            <range value="1..31"/>
          </type>
          <default value="1"/>
          <description>
            <text>Virtual machine CPU number.</text>
          </description>
        </leaf>
        <leaf name="memory-usage">
          <type name="uint32">
            <range value="0..100"/>
          </type>
          <units name="%"/>
          <description>
            <text>Memory usage.</text>
          </description>
        </leaf>
        <leaf name="cpu-usage">
          <type name="uint32">
            <range value="0..100"/>
          </type>
          <units name="%"/>
          <default value="1"/>
          <description>
            <text>CPU usage.</text>
          </description>
        </leaf>
        <leaf name="storage-usage">
          <type name="uint32">
            <range value="0..100"/>
          </type>
          <units name="%"/>
          <description>
            <text>Storage usage.</text>
          </description>
        </leaf>
        <leaf name="up-time">
          <type name="uint32">
            <range value="0..4294967295"/>
          </type>
          <units name="s"/>
          <description>
            <text>Virtual machine up time.</text>
          </description>
        </leaf>
        <container name="veth-ports">
          <config value="false"/>
          <description>
            <text>List of virtual ethernet ports.</text>
          </description>
          <list name="veth-port">
            <key value="name"/>
            <description>
              <text>Operational state of virtual ethernet port.</text>
            </description>
            <leaf name="name">
              <type name="string">
                <length value="1..63"/>
              </type>
              <description>
                <text>Virtual port name.</text>
              </description>
            </leaf>
            <leaf name="id">
              <type name="uint32">
                <range value="0..4294967295"/>
              </type>
              <description>
                <text>Virtual port ID.</text>
              </description>
            </leaf>
            <leaf name="is-bind-pae">
              <type name="boolean"/>
              <description>
                <text>Whether the PAE is bound.</text>
              </description>
            </leaf>
            <leaf name="is-external">
              <type name="boolean"/>
              <description>
                <text>Whether the port is an external port.</text>
              </description>
            </leaf>
            <leaf name="network-plane">
              <type name="network-plane-type"/>
              <description>
                <text>Network plane type.</text>
              </description>
            </leaf>
            <leaf name="netmac">
              <type name="pub-type:mac-address"/>
              <default value="0000-0000-0000"/>
              <description>
                <text>MAC address.</text>
              </description>
            </leaf>
            <leaf name="netmask">
              <type name="inet:ipv4-address-no-zone"/>
              <default value="0.0.0.0"/>
              <description>
                <text>Network mask.</text>
              </description>
            </leaf>
            <leaf name="netip">
              <type name="inet:ipv4-address-no-zone"/>
              <description>
                <text>IP address.</text>
              </description>
            </leaf>
            <leaf name="run-state">
              <type name="port-state-type"/>
              <description>
                <text>Port running state.</text>
              </description>
            </leaf>
          </list>
        </container>
      </list>
    </container>
  </container>
</module>
