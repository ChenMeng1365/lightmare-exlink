    "definitions": {
        "grpc_schema": {
            "description": "Google Remote Procedure Call Protocol.",
            "properties": {
                "service": {
                    "description": "Configure the gRPC service.",
                    "properties": {
                        "dscp": {
                            "type": "string",
                            "description": "DSCP marking of packets generated by the telemetry subsystem on the network device."
                        },
                        "server-enable": {
                            "type": "boolean",
                            "description": "Enable/disable the gRPC service."
                        },
                        "source-address": {
                            "type": "string",
                            "description": "IP address listened by the gRPC service. Only the address 0.0.0.0 or valid IP addresses in class A, B, or C are supported."
                        },
                        "vpn-name": {
                            "type": "string",
                            "x-path": "/ni:network-instance/ni:instances/ni:instance/ni:name",
                            "description": "Name of a VPN instance listened by the gRPC service."
                        },
                        "server-port": {
                            "type": "string",
                            "description": "Number of a port listened by the gRPC service."
                        },
                        "idle-timeout": {
                            "type": "string",
                            "description": "Idle (RPC is not invoked) timeout period for gRPC service access. After the timeout period expires, the gRPC service is disconnected."
                        },
                        "acl-number-or-name": {
                            "type": "string",
                            "x-path": "/acl:acl/acl:groups/acl:group/acl:identity",
                            "description": "Specify the name or number of an ACL to configure an access control connection for the gRPC service."
                        },
                        "ssl-policy": {
                            "type": "string",
                            "x-path": "/ssl:ssl/ssl:ssl-policys/ssl:ssl-policy/ssl:policy-name",
                            "description": "Specify the name of an SSL policy to bind an SSL policy to the gRPC server."
                        },
                        "ssl-policy-peer": {
                            "type": "boolean",
                            "description": "Enable/disable SSL two-way authentication."
                        }
                    }
                },
