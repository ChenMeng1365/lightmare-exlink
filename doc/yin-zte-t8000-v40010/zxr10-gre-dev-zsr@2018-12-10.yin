<?xml version="1.0" encoding="UTF-8"?>
<module name="zxr10-gre-dev-zsr"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:gre-dev-zsr="http://www.zte.com.cn/zxr10/netconf/schema/rosng/gre-dev-zsr"
        xmlns:gre="http://www.zte.com.cn/zxr10/netconf/schema/rosng/gre">
  <namespace uri="http://www.zte.com.cn/zxr10/netconf/schema/rosng/gre-dev-zsr"/>
  <prefix value="gre-dev-zsr"/>
  <yang-version value="1"/>
  <import module="zxr10-gre">
    <prefix value="gre"/>
  </import>
  <organization>
    <text>ZTE Corporation</text>
  </organization>
  <contact>
    <text>ZTE Corporation  
   Nanjing Institute of ZTE Corporation 
   No.68 Zijinghua Rd. Yuhuatai District, Nanjing, China 
   Tel: +86-25-52870000</text>
  </contact>
  <description>
    <text>The GRE tunnel netconf module supports route ZSRv2 and PC(MSE).</text>
  </description>
  <revision date="2018-12-10">
    <description>
      <text>Remove "multipoint" from deviation @gre:mode.</text>
    </description>
  </revision>
  <revision date="2015-07-08">
    <description>
      <text>GRE model with zsrv2</text>
    </description>
  </revision>
  <deviation target-node="/gre:configuration/gre:gre/gre:gre-tunnels/gre:gre-tunnel/gre:mode">
    <description>
      <text>GRE tunnel mode support mgre</text>
    </description>
    <deviate value="replace">
      <type name="enumeration">
        <enum name="ip">
          <value value="1"/>
          <description>
            <text>GRE over IP</text>
          </description>
        </enum>
        <enum name="ipv6">
          <value value="6"/>
          <description>
            <text>GRE over IPv6</text>
          </description>
        </enum>
        <enum name="ds-lite-static">
          <value value="9"/>
          <description>
            <text>Static dual stack lite tunnel</text>
          </description>
        </enum>
        <enum name="ds-lite-dynamic">
          <value value="10"/>
          <description>
            <text>Dynamic dual stack lite tunnel</text>
          </description>
        </enum>
      </type>
    </deviate>
  </deviation>
</module>
