                  <value value="6"/>
                  <description>
                    <text>The state of OSPF neighbor is LOADING.</text>
                  </description>
                </enum>
                <enum name="Full">
                  <value value="7"/>
                  <description>
                    <text>The state of OSPF neighbor is FULL.</text>
                  </description>
                </enum>
              </type>
            </leaf>
            <leaf name="priority">
              <description>
                <text>Interface priority of neighbor.</text>
              </description>
              <type name="uint32">
                <range value="0..255"/>
