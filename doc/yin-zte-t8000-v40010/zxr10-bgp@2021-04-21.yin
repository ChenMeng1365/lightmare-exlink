  <container name="configuration">
    <container name="bgp">
      <description>
        <text>BGP configuration.</text>
      </description>
      <container name="bgp-instances">
        <description>
          <text>BGP configuration.</text>
        </description>
        <list name="bgp-inst">
          <description>
            <text>BGP configuration.</text>
          </description>
          <max-elements value="4"/>
          <key value="local-as"/>
          <leaf name="local-as">
            <description>
              <text>the Autonomy system number of BGP in local equipment.</text>
            </description>
            <type name="ncd:asn-dot"/>
          </leaf>
          <container name="bgp">
            <description>
              <text>BGP specific commands.</text>
            </description>
            <leaf name="advertise-by-sequence">
              <description>
                <text>Advertise routes by sequence strictly.</text>
              </description>
              <type name="empty"/>
              <ncd:revise-date date="2016-01-06"/>
            </leaf>
            <leaf name="always-compare-med">
              <description>
                <text>enable route's med to always be compared.</text>
              </description>
              <type name="empty"/>
            </leaf>
            <leaf name="client-to-client-reflection">
              <description>
                <text>Configure client to client route reflection.</text>
              </description>
              <type name="enumeration">
                <enum name="disable">
                  <value value="0"/>
                  <description>
                    <text>Reflection of routes denyed.</text>
                  </description>
                </enum>
                <enum name="enable">
                  <value value="1"/>
                  <description>
                    <text>Reflection of routes allowed.</text>
                  </description>
                </enum>
              </type>
              <default value="enable"/>
            </leaf>
            <leaf name="enforce-first-as">
              <description>
                <text>Enforce the first AS for EBGP routes(default).</text>
              </description>
              <type name="enumeration">
                <enum name="disable">
                  <value value="0"/>
                  <description>
                    <text>Not enforce the first AS for EBGP routes.</text>
                  </description>
                </enum>
                <enum name="enable">
                  <value value="1"/>
                  <description>
                    <text>Enforce the first AS for EBGP routes(default).</text>
                  </description>
                </enum>
              </type>
              <default value="enable"/>
            </leaf>
            <leaf name="confederation-identifier">
              <description>
                <text>AS number of confederation.</text>
              </description>
              <type name="ncd:asn-dot"/>
            </leaf>
            <leaf name="default-local-preference">
              <description>
                <text>Configure default local preference value.</text>
              </description>
              <type name="uint32">
                <range value="0..4294967295">
                  <description>
                    <text>Local preference (higher=more preferred).</text>
                  </description>
                </range>
              </type>
              <default value="100"/>
            </leaf>
            <leaf name="fast-external-fallover">
              <description>
                <text>Immediately reset session if a link to a directly connected external peer goes down.</text>
              </description>
              <type name="enumeration">
                <enum name="disable">
                  <value value="0"/>
                  <description>
                    <text>Not immediately reset session if a link to a directly connected external peer goes down.</text>
                  </description>
                </enum>
                <enum name="enable">
                  <value value="1"/>
                  <description>
                    <text>Immediately reset session if a link to a directly connected external peer goes down.</text>
                  </description>
                </enum>
              </type>
              <default value="enable"/>
            </leaf>
            <leaf name="default-ipv4">
              <description>
                <text>Activate ipv4-unicast for a peer by default.</text>
              </description>
              <type name="enumeration">
                <enum name="disable">
                  <value value="0"/>
                  <description>
                    <text>Not activate ipv4-unicast for a peer by default.</text>
                  </description>
                </enum>
                <enum name="enable">
                  <value value="1"/>
                  <description>
                    <text>Activate ipv4-unicast for a peer by default.</text>
                  </description>
                </enum>
              </type>
              <default value="enable"/>
            </leaf>
            <leaf name="ext-community-type-compatible">
              <description>
                <text>Send extended community compatible use old type.</text>
              </description>
              <type name="empty"/>
            </leaf>
            <leaf name="route-target-filter">
              <description>
                <text>Control behavior based on Route-Target attributes.</text>
              </description>
              <type name="enumeration">
                <enum name="disable">
                  <value value="0"/>
                  <description>
                    <text>Discontrol automatic VPN Route-Target filtering.</text>
                  </description>
                </enum>
                <enum name="enable">
                  <value value="1"/>
                  <description>
                    <text>Control automatic VPN Route-Target filtering.</text>
                  </description>
                </enum>
              </type>
              <default value="enable"/>
            </leaf>
            <leaf name="deterministic-med">
              <description>
                <text>Set BGP deterministic-med parameters.</text>
              </description>
              <type name="empty"/>
            </leaf>
            <leaf name="connect-delay">
              <description>
                <text>Set the connect delay time needed to restart.</text>
              </description>
              <type name="uint32">
                <range value="1..3600">
                  <description>
                    <text>Delay value.</text>
                  </description>
                </range>
              </type>
              <units name="s"/>
            </leaf>
            <leaf name="enhanced-error">
              <description>
                <text>Enable BGP enhanced error handling.</text>
              </description>
              <type name="empty"/>
            </leaf>
            <leaf name="bestpath-as-path-ignore">
              <description>
                <text>Ignore as-path in bestpath selection and maximum-paths.</text>
              </description>
              <type name="empty"/>
            </leaf>
            <leaf name="bestpath-router-id-ignore">
              <description>
                <text>Ignore router-id in bestpath selection.</text>
              </description>
              <type name="empty"/>
            </leaf>
            <leaf name="attribute-download-ipv4">
              <description>
                <text>IPv4 address family.</text>
              </description>
              <type name="empty"/>
            </leaf>
            <leaf name="attribute-download-ipv6">
              <description>
                <text>IPv6 address family.</text>
              </description>
              <type name="empty"/>
            </leaf>
            <leaf name="graceful-restart-time">
              <description>
                <text>Set the max time needed to restart and come back up.</text>
              </description>
              <type name="uint32">
                <range value="1..3600">
                  <description>
                    <text>Delay value.</text>
                  </description>
                </range>
              </type>
              <units name="s"/>
              <default value="120"/>
            </leaf>
            <leaf name="graceful-stalepath-time">
              <description>
                <text>Set the max time to hold on to restarting peer's stale paths.</text>
              </description>
              <type name="uint32">
                <range value="1..604800">
                  <description>
                    <text>Delay value.</text>
                  </description>
                </range>
              </type>
              <units name="s"/>
              <default value="360"/>
            </leaf>
            <leaf name="enable-graceful-restart">
              <description>
                <text>Enable the graceful-restart capability of BGP.</text>
              </description>
              <type name="empty"/>
            </leaf>
            <leaf name="delay-value">
              <description>
                <text>set the update-delay of graceful restart.</text>
              </description>
              <type name="uint32">
                <range value="1..3600">
                  <description>
                    <text>Delay value.</text>
                  </description>
                </range>
              </type>
              <units name="s"/>
              <default value="120"/>
            </leaf>
            <leaf name="delay-always">
              <description>
                <text>The timer overflow must occur.</text>
              </description>
              <type name="empty"/>
            </leaf>
            <container name="cluster-id">
              <description>
                <text>Set Route-Reflector cluster-id type.</text>
              </description>
              <choice name="cluster-id">
                <description>
                  <text>Set Route-Reflector cluster-id type.</text>
                </description>
                <case name="quantity-type">
                  <description>
                    <text>Route-Reflector Cluster-id as 32 bit quantity.</text>
                  </description>
                  <leaf name="cluster-id-quantity">
                    <description>
                      <text>Route-Reflector Cluster-id as 32 bit quantity.</text>
                    </description>
                    <type name="uint32">
                      <range value="1..4294967295">
                        <description>
                          <text>Route-Reflector Cluster-id as 32 bit quantity.</text>
                        </description>
                      </range>
                    </type>
                  </leaf>
                </case>
                <case name="address-type">
                  <description>
                    <text>Route-Reflector Cluster-id in IP address format.</text>
                  </description>
                  <leaf name="cluster-id-addr">
                    <description>
                      <text>Route-Reflector Cluster-id in IP address format.</text>
                    </description>
                    <type name="ncd:ipv4-address">
                      <pattern value="([1-9][0-9]*\.[1-9][0-9]*\.[1-9][0-9]*\.[1-9][0-9]*)|([1-9][0-9]*\.0\.0\.0)|(0\.[1-9][0-9]*\.0\.0)|(0\.0\.[1-9][0-9]*\.0)|(0\.0\.0\.[1-9][0-9]*)|([1-9][0-9]*\.[1-9][0-9]*\.0\.0)|(0\.[1-9][0-9]*\.[1-9][0-9]*\.0)|(0\.0\.[1-9][0-9]*\.[1-9][0-9]*)|([1-9][0-9]*\.0\.0\.[1-9][0-9]*)|(0\.[1-9][0-9]*\.0\.[1-9][0-9]*)|([1-9][0-9]*\.0\.[1-9][0-9]*\.0)|([1-9][0-9]*\.[1-9][0-9]*\.[1-9][0-9]*\.0)|([1-9][0-9]*\.[1-9][0-9]*\.0\.[1-9][0-9]*)|([1-9][0-9]*\.0\.[1-9][0-9]*\.[1-9][0-9]*)|(0\.[1-9][0-9]*\.[1-9][0-9]*\.[1-9][0-9]*)"/>
                    </type>
                  </leaf>
                </case>
              </choice>
            </container>
            <container name="timers">
              <description>
                <text>Adjust routing timers.</text>
              </description>
              <presence value="timers"/>
              <leaf name="keepalive-interval">
                <description>
                  <text>Keepalive interval.</text>
                </description>
                <type name="uint32">
                  <range value="0..65535"/>
                </type>
                <units name="s"/>
                <mandatory value="true"/>
              </leaf>
              <leaf name="holdtime">
                <description>
                  <text>Hold time interval.</text>
                </description>
                <type name="uint32">
                  <range value="0 | 3..65535"/>
                </type>
                <units name="s"/>
                <mandatory value="true"/>
              </leaf>
            </container>
            <container name="neighbor-damp">
              <description>
                <text>Enable neighbor flap dampening.</text>
              </description>
              <presence value="damp-v4-uni"/>
              <choice name="damp">
                <description>
                  <text>Enable neighbor flap dampening.</text>
                </description>
                <case name="route-map">
                  <description>
                    <text>Route-map to specify criteria for dampening.</text>
                  </description>
                  <leaf name="route-map-name">
                    <description>
                      <text>Route-map to specify criteria for dampening.</text>
                    </description>
                    <type name="string">
                      <length value="1..200"/>
                    </type>
                  </leaf>
                  <leaf name="route-policy-name">
                    <description>
                      <text>Route-policy to specify criteria for dampening.</text>
                    </description>
                    <type name="string">
                      <length value="1..200"/>
                    </type>
