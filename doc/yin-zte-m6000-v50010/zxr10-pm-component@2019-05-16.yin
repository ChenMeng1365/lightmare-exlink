<?xml version="1.0" encoding="UTF-8"?>
<module name="zxr10-pm-component"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:pm-component="http://www.zte.com.cn/zxr10/netconf/schema/rosng/pm-component"
        xmlns:ncd="http://www.zte.com.cn/zxr10/netconf/schema/rosng/netconf-defs">
  <namespace uri="http://www.zte.com.cn/zxr10/netconf/schema/rosng/pm-component"/>
  <prefix value="pm-component"/>
  <yang-version value="1"/>
  <import module="zxr10-netconf-defs">
    <prefix value="ncd"/>
  </import>
  <organization>
    <text>ZTE Corporation</text>
  </organization>
  <contact>
    <text>ZTE Corporation  
   Nanjing Institute of ZTE Corporation 
   No.68 Zijinghua Rd. Yuhuatai District, Nanjing, China 
   Tel: +86-25-52870000</text>
  </contact>
  <revision date="2019-05-16">
    <description>
      <text>Add notification node of linecard and cu.</text>
    </description>
  </revision>
  <revision date="2018-08-24">
    <description>
      <text>Initial version.</text>
    </description>
  </revision>
  <container name="configuration">
    <container name="components">
      <description>
        <text>Enclosing container for the components in the system.</text>
      </description>
      <list name="component">
        <description>
          <text>List of components, keyed by component name.</text>
        </description>
        <key value="name"/>
        <leaf name="name">
          <description>
            <text>Device name for the component -- this will not be a configurable parameter on many implementations</text>
          </description>
          <type name="string"/>
        </leaf>
        <leaf name="alias">
          <description>
            <text>alias of the component</text>
          </description>
          <type name="string"/>
        </leaf>
        <leaf name="custom-info">
          <description>
            <text>customer information</text>
          </description>
          <type name="ncd:string-with-blank">
            <length value="1..255"/>
          </type>
        </leaf>
        <leaf name="admin-state">
          <description>
            <text>Set the admin state of the component</text>
          </description>
          <type name="enumeration">
            <enum name="ENABLED">
              <value value="1"/>
              <description>
                <text>Enable admin state</text>
              </description>
            </enum>
            <enum name="DISABLED">
              <value value="2"/>
              <description>
                <text>Disable admin state</text>
              </description>
            </enum>
            <enum name="MAINTENANCE">
              <value value="3"/>
              <description>
                <text>Set admin-state as maintenance</text>
              </description>
            </enum>
          </type>
          <default value="ENABLED"/>
        </leaf>
        <leaf name="vendor-type">
          <description>
            <text>Device model type for the component, defined by the supplier. Where component preconfiguration is supported, for example, the component name may be configurable.</text>
          </description>
          <type name="uint32">
            <range value="1..4294967295"/>
          </type>
        </leaf>
        <leaf name="led">
          <description>
            <text>The alarm led state on the front panel of the card.</text>
          </description>
          <type name="enumeration">
            <enum name="ENABLED">
              <value value="1"/>
              <description>
                <text>The led is remote controlled, and the alarm light is red and flash</text>
              </description>
            </enum>
            <enum name="DISABLED">
              <value value="2"/>
              <description>
                <text>The remote controlled of the led is released, and controlled by device</text>
              </description>
            </enum>
          </type>
          <default value="DISABLED"/>
        </leaf>
      </list>
    </container>
    <container name="linecards">
      <description>
        <text>Configuration data for linecard components.</text>
      </description>
      <list name="linecard">
        <key value="name"/>
        <leaf name="name">
          <description>
            <text>Device name for the component</text>
          </description>
          <type name="string">
            <length value="1..31"/>
          </type>
        </leaf>
        <leaf name="alias">
          <description>
            <text>Device alias for the component</text>
          </description>
          <type name="string"/>
        </leaf>
        <leaf name="card-type-preconf">
          <description>
            <text>The expected card type</text>
          </description>
          <type name="uint32"/>
        </leaf>
      </list>
    </container>
    <container name="cus">
      <description>
        <text>Configuration data for controlcard components.</text>
      </description>
      <list name="cu">
        <key value="name"/>
        <leaf name="name">
          <description>
            <text>Device name for the component</text>
          </description>
          <type name="string"/>
        </leaf>
        <leaf name="alias">
          <description>
            <text>Device alias for the component</text>
          </description>
          <type name="string"/>
        </leaf>
        <leaf name="power-admin-state">
          <description>
            <text>Enable or disable power to the controlcard</text>
          </description>
          <type name="enumeration">
            <enum name="POWER_DISABLED">
              <value value="0"/>
              <description>
                <text>Disable power on the component</text>
              </description>
            </enum>
            <enum name="POWER_ENABLED">
              <value value="1"/>
              <description>
                <text>Enable power on the component</text>
              </description>
            </enum>
            <enum name="MAINTENANCE">
              <value value="2"/>
              <description>
                <text>Maintenance on the component</text>
              </description>
            </enum>
          </type>
          <default value="POWER_ENABLED"/>
        </leaf>
      </list>
    </container>
  </container>
  <container name="state">
    <config value="false"/>
    <container name="components">
      <description>
        <text>Enclosing container for the components in the system.</text>
      </description>
      <list name="component">
        <description>
          <text>List of components, keyed by component name.</text>
        </description>
        <key value="name"/>
        <leaf name="name">
          <description>
            <text>Device name for the component -- this will not be a configurable parameter on many implementations</text>
          </description>
          <type name="string"/>
        </leaf>
        <leaf name="alias">
          <description>
            <text>alias of the component</text>
          </description>
          <type name="string"/>
        </leaf>
        <leaf name="custom-info">
          <description>
            <text>customer information</text>
          </description>
          <type name="ncd:string-with-blank"/>
        </leaf>
        <leaf name="admin-state">
          <description>
            <text>Set the admin state of the component</text>
          </description>
          <type name="enumeration">
            <enum name="ENABLED">
              <value value="1"/>
              <description>
                <text>Enable admin state</text>
              </description>
            </enum>
            <enum name="DISABLED">
              <value value="2"/>
              <description>
                <text>Disable admin state</text>
              </description>
            </enum>
            <enum name="MAINTENANCE">
              <value value="3"/>
              <description>
                <text>Set admin-state as maintenance</text>
              </description>
            </enum>
          </type>
        </leaf>
        <leaf name="vendor-type">
          <description>
            <text>Device model type for the component, defined by the supplier. Where component preconfiguration is supported, for example, the component name may be configurable.</text>
          </description>
          <type name="uint32"/>
        </leaf>
        <leaf name="led">
          <description>
            <text>The alarm led state on the front panel of the card.</text>
          </description>
          <type name="enumeration">
            <enum name="ENABLED">
              <value value="1"/>
              <description>
                <text>The led is remote controlled, and the alarm light is red and flash</text>
              </description>
            </enum>
            <enum name="DISABLED">
              <value value="2"/>
              <description>
                <text>The remote controlled of the led is released, and controlled by device</text>
              </description>
            </enum>
          </type>
        </leaf>
        <leaf name="type">
          <description>
            <text>Type of component as identified by the system</text>
          </description>
          <type name="enumeration">
            <enum name="UNKNOWN">
              <value value="0"/>
            </enum>
            <enum name="CHASSIS">
              <value value="1"/>
            </enum>
            <enum name="BACKPLANE">
              <value value="2"/>
            </enum>
            <enum name="FABRIC">
              <value value="3"/>
            </enum>
            <enum name="POWER_SUPPLY">
              <value value="4"/>
            </enum>
            <enum name="FAN">
              <value value="5"/>
            </enum>
            <enum name="SENSOR">
              <value value="6"/>
            </enum>
            <enum name="FRU">
              <value value="7"/>
            </enum>
            <enum name="LINECARD">
              <value value="8"/>
            </enum>
            <enum name="PORT">
              <value value="9"/>
            </enum>
            <enum name="TRANSCEIVER">
              <value value="10"/>
            </enum>
            <enum name="CPU">
              <value value="11"/>
            </enum>
            <enum name="STORAGE">
              <value value="12"/>
            </enum>
            <enum name="INTEGRATED_CIRCUIT">
              <value value="13"/>
            </enum>
            <enum name="PANEL">
              <value value="14"/>
            </enum>
            <enum name="MUX">
              <value value="15"/>
            </enum>
            <enum name="CONTROLLER_CARD">
              <value value="16"/>
            </enum>
            <enum name="AMPLIFIER">
              <value value="17"/>
            </enum>
            <enum name="OPTICAL_CHANNEL">
              <value value="18"/>
            </enum>
            <enum name="OPERATING_SYSTEM">
              <value value="19"/>
            </enum>
          </type>
        </leaf>
        <leaf name="id">
          <description>
            <text>Unique identifier assigned by the system for the component</text>
          </description>
          <type name="string"/>
        </leaf>
        <leaf name="location">
          <description>
            <text>System-supplied description of the location of the component within the system. This could be a bay position, slot number, socket location, etc. For component types that have an explicit slot-id attribute, such as linecards, the system should populate the more specific slot-id.</text>
          </description>
          <type name="string"/>
        </leaf>
        <leaf name="description">
          <description>
            <text>System-supplied description of the component</text>
          </description>
          <type name="ncd:string-with-blank"/>
        </leaf>
        <leaf name="mfg-name">
          <description>
            <text>System-supplied identifier for the manufacturer of the component. This data is particularly useful when a component manufacturer is different than the overall device vendor.</text>
          </description>
          <type name="string"/>
        </leaf>
        <leaf name="mfg-date">
          <description>
            <text>The manufacturer date of the component.</text>
          </description>
          <type name="string">
            <pattern value="[0-9]{4}\-[0-9]{2}\-[0-9]{2}"/>
          </type>
        </leaf>
        <leaf name="hardware-version">
          <description>
            <text>For hardware components, this is the hardware revision of the component.</text>
          </description>
          <type name="ncd:string-with-blank"/>
        </leaf>
        <leaf name="firmware-version">
          <description>
            <text>For hardware components, this is the version of associated firmware that is running on the component, if applicable.</text>
          </description>
          <type name="ncd:string-with-blank"/>
        </leaf>
        <leaf name="software-version">
          <description>
            <text>For software components such as operating system or other software module, this is the version of the currently running software.</text>
          </description>
          <type name="ncd:string-with-blank"/>
        </leaf>
        <leaf name="serial-no">
          <description>
            <text>System-assigned serial number of the component.</text>
          </description>
          <type name="string"/>
        </leaf>
        <leaf name="part-no">
          <description>
            <text>System-assigned part number for the component.  This should be present in particular if the component is also an FRU (field replacable unit)</text>
          </description>
          <type name="string"/>
        </leaf>
        <leaf name="removable">
          <description>
            <text>If true, this component is removable or is a field replaceable unit</text>
          </description>
          <type name="boolean"/>
        </leaf>
        <leaf name="oper-status">
          <description>
            <text>If applicable, this reports the current operational status of the component.</text>
          </description>
          <type name="enumeration">
            <enum name="ACTIVE">
              <value value="0"/>
              <description>
                <text>Component is enabled and active (i.e., up)</text>
              </description>
            </enum>
            <enum name="INACTIVE">
              <value value="1"/>
              <description>
                <text>Component is enabled but inactive (i.e., down)</text>
              </description>
            </enum>
            <enum name="DISABLED">
              <value value="2"/>
              <description>
                <text>Component is administratively disabled.</text>
              </description>
            </enum>
          </type>
        </leaf>
        <leaf name="led-color">
          <description>
            <text>The LED indicator color.</text>
          </description>
          <type name="enumeration">
            <enum name="OFF">
              <value value="0"/>
              <description>
                <text>The LED indicator is off</text>
              </description>
            </enum>
            <enum name="GREEN">
              <value value="1"/>
              <description>
                <text>The LED indicator is green</text>
              </description>
            </enum>
            <enum name="GREEN_BLINK">
              <value value="2"/>
              <description>
                <text>The LED indicator blinks green</text>
              </description>
            </enum>
            <enum name="AMBER">
              <value value="3"/>
              <description>
                <text>The LED indicator is amber</text>
              </description>
            </enum>
            <enum name="AMBER_BLINK">
              <value value="4"/>
              <description>
                <text>The LED indicator blinks amber</text>
              </description>
            </enum>
            <enum name="RED">
              <value value="5"/>
              <description>
                <text>The LED indicator is red</text>
              </description>
            </enum>
          </type>
          <ncd:revise-date date="2019-05-16"/>
        </leaf>
        <container name="memory">
          <description>
            <text>For components that have associated memory, these values report information about available and utilized memory.</text>
          </description>
          <leaf name="available">
            <description>
              <text>The available memory physically installed, or logically allocated to the component.</text>
            </description>
            <type name="uint64"/>
            <units name="bytes"/>
          </leaf>
          <leaf name="utilized">
            <description>
              <text>The memory currently in use by processes running on the component, not considering reserved memory that is not available for use.</text>
            </description>
            <type name="uint64"/>
            <units name="bytes"/>
          </leaf>
        </container>
        <leaf name="allocated-power">
          <description>
            <text>Power allocated by the system for the component.</text>
          </description>
          <type name="uint32"/>
          <units name="watts"/>
        </leaf>
        <leaf name="used-power">
          <description>
            <text>Actual power used by the component.</text>
          </description>
          <type name="uint32"/>
          <units name="watts"/>
        </leaf>
        <leaf name="parent">
          <description>
            <text>The parent of component.</text>
          </description>
          <type name="string"/>
        </leaf>
        <leaf name="empty">
          <description>
            <text>Mark if the compoent is online.</text>
          </description>
          <type name="boolean"/>
        </leaf>
        <leaf name="equipment-failure">
          <description>
            <text>If true, the hardware indicates that the component's physical equipment has failed.</text>
          </description>
          <type name="boolean"/>
        </leaf>
        <leaf name="equipment-mismatch">
          <description>
            <text>If true, the hardware indicates that the component inserted into the affected component's physical location is of a different type than what is configured.</text>
          </description>
          <type name="boolean"/>
        </leaf>
        <container name="subcomponents">
          <description>
            <text>Enclosing container for subcomponent references.</text>
          </description>
          <list name="subcomponent">
            <description>
              <text>List of subcomponent references.</text>
            </description>
            <key value="name"/>
            <leaf name="name">
              <description>
                <text>Reference to the name of the subcomponent</text>
              </description>
              <type name="string"/>
            </leaf>
            <leaf name="alias">
              <description>
                <text>alias of the subcomponent</text>
              </description>
              <type name="string"/>
            </leaf>
          </list>
        </container>
      </list>
    </container>
    <container name="linecards">
      <description>
        <text>Operational state data for linecard components.</text>
      </description>
      <list name="linecard">
        <key value="name"/>
        <leaf name="name">
          <description>
            <text>Device name for the component</text>
          </description>
          <type name="string">
            <length value="1..31"/>
          </type>
        </leaf>
        <leaf name="alias">
          <description>
            <text>Device alias for the component</text>
          </description>
          <type name="string"/>
        </leaf>
        <leaf name="card-type-preconf">
          <description>
            <text>The expected card type</text>
          </description>
          <type name="uint32"/>
        </leaf>
        <leaf name="power-admin-state">
          <description>
            <text>Enable or disable power to the linecard</text>
          </description>
          <type name="enumeration">
            <enum name="POWER_ENABLED">
              <value value="1"/>
              <description>
                <text>Enable power on the component</text>
              </description>
            </enum>
            <enum name="POWER_DISABLED">
              <value value="0"/>
              <description>
                <text>Disable power on the component</text>
              </description>
            </enum>
            <enum name="MAINTENANCE">
              <value value="2"/>
              <description>
                <text>Disable power on the component</text>
              </description>
            </enum>
          </type>
          <default value="POWER_ENABLED"/>
        </leaf>
        <leaf name="slot-id">
          <description>
            <text>Identifier for the slot or chassis position in which the linecard is installed</text>
          </description>
          <type name="uint8"/>
        </leaf>
        <leaf name="card-type">
          <description>
            <text>The actual card type</text>
          </description>
          <type name="uint32"/>
        </leaf>
        <leaf name="card-class">
          <type name="enumeration">
            <enum name="UNKNOWN">
              <value value="0"/>
            </enum>
            <enum name="T2X2C4">
              <value value="1"/>
            </enum>
            <enum name="T2X4C8">
              <value value="2"/>
            </enum>
            <enum name="MUX96">
              <value value="3"/>
            </enum>
            <enum name="MUX64">
              <value value="4"/>
            </enum>
          </type>
        </leaf>
        <container name="specification">
          <description>
            <text>Operational state data for component.</text>
          </description>
          <leaf name="memory-capacity">
            <description>
              <text>The memory capability of the component.</text>
            </description>
            <type name="uint64"/>
            <units name="bytes"/>
          </leaf>
          <leaf name="rated-power">
            <description>
              <text>Rated power of the Linecard.</text>
            </description>
            <type name="decimal64">
              <fraction-digits value="1"/>
            </type>
            <units name="W"/>
          </leaf>
          <leaf name="min-input-voltage">
            <description>
              <text>The minimum value of the input voltage.</text>
            </description>
            <type name="decimal64">
              <fraction-digits value="1"/>
            </type>
            <units name="V"/>
          </leaf>
          <leaf name="max-input-voltage">
            <description>
              <text>The maximum value of the input voltage.</text>
            </description>
            <type name="decimal64">
              <fraction-digits value="1"/>
            </type>
            <units name="V"/>
          </leaf>
          <leaf name="min-input-current">
            <description>
              <text>The minimum value of the input current.</text>
            </description>
            <type name="decimal64">
              <fraction-digits value="1"/>
            </type>
            <units name="mA"/>
          </leaf>
          <leaf name="max-input-current">
            <description>
              <text>The maximum value of the input current.</text>
            </description>
            <type name="decimal64">
              <fraction-digits value="1"/>
            </type>
            <units name="mA"/>
          </leaf>
          <leaf name="min-temperature">
            <description>
              <text>The minimum value of the temperature.</text>
            </description>
            <type name="decimal64">
              <fraction-digits value="1"/>
            </type>
            <units name="celsius"/>
          </leaf>
          <leaf name="max-temperature">
            <description>
              <text>The maximum value of the temperature.</text>
            </description>
            <type name="decimal64">
              <fraction-digits value="1"/>
            </type>
            <units name="celsius"/>
          </leaf>
          <container name="supported-card-types">
            <description>
              <text>Enclosing container for supported card type.</text>
            </description>
            <list name="card-type">
              <description>
                <text>List for card type.</text>
              </description>
              <key value="card-class"/>
              <leaf name="card-class">
                <description>
                  <text>Linecard name defined by the model.</text>
                </description>
                <type name="enumeration">
                  <enum name="UNKNOWN">
                    <value value="0"/>
                  </enum>
                  <enum name="T2X2C4">
                    <value value="1"/>
                  </enum>
                  <enum name="T2X4C8">
                    <value value="2"/>
                  </enum>
                  <enum name="MUX96">
                    <value value="3"/>
                  </enum>
                  <enum name="MUX64">
                    <value value="4"/>
                  </enum>
                </type>
              </leaf>
              <leaf-list name="supported-card">
                <description>
                  <text>Supported card type for the linecard.</text>
                </description>
                <type name="ncd:string-all"/>
              </leaf-list>
            </list>
          </container>
          <ncd:revise-date date="2019-05-16"/>
        </container>
        <container name="default-configuration">
          <leaf name="power-admin-state">
            <description>
              <text>Set admin state of the linecard.
When the value changes, a notification is required.</text>
            </description>
            <type name="enumeration">
              <enum name="POWER_ENABLED">
                <value value="1"/>
                <description>
                  <text>Enable power on the component</text>
                </description>
              </enum>
              <enum name="POWER_DISABLED">
                <value value="0"/>
                <description>
                  <text>Disable power on the component</text>
                </description>
              </enum>
              <enum name="MAINTENANCE">
                <value value="2"/>
                <description>
                  <text>Disable power on the component</text>
                </description>
              </enum>
            </type>
            <default value="POWER_ENABLED"/>
          </leaf>
          <leaf name="card-type-preconf">
            <description>
              <text>The expected card type.
When the value changes, a notification is required.</text>
            </description>
            <type name="uint32"/>
            <default value="0"/>
          </leaf>
        </container>
      </list>
    </container>
    <container name="cus">
      <list name="cu">
        <key value="name"/>
        <leaf name="name">
          <description>
            <text>Device name for the component</text>
          </description>
          <type name="string"/>
        </leaf>
        <leaf name="alias">
          <description>
            <text>Device alias for the component</text>
          </description>
          <type name="string"/>
        </leaf>
        <leaf name="power-admin-state">
          <description>
            <text>Enable or disable power to the controlcard</text>
          </description>
          <type name="enumeration">
            <enum name="POWER_DISABLED">
              <value value="0"/>
              <description>
                <text>Disable power on the component</text>
              </description>
            </enum>
            <enum name="POWER_ENABLED">
              <value value="1"/>
              <description>
                <text>Enable power on the component</text>
              </description>
            </enum>
            <enum name="MAINTENANCE">
              <value value="2"/>
              <description>
                <text>Maintenance on the component</text>
              </description>
            </enum>
          </type>
        </leaf>
        <leaf name="slot-id">
          <description>
            <text>Identifier for the slot or chassis position in which the controlcard is installed</text>
          </description>
          <type name="uint8"/>
        </leaf>
        <leaf name="redundant-state">
          <description>
            <text>Identifier for redundant state of control card.</text>
          </description>
          <type name="enumeration">
            <enum name="unknown">
              <value value="0"/>
            </enum>
            <enum name="master">
              <value value="1"/>
            </enum>
            <enum name="standby">
              <value value="2"/>
            </enum>
          </type>
          <ncd:revise-date date="2019-05-16"/>
        </leaf>
        <container name="specification">
          <description>
