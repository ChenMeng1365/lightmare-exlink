        "logging-selectors-config": {
            "description": "Configuration data for logging selectors",
            "properties": {
                "facility": {
                    "type": "string",
                    "description": "Specifies the facility, or class of messages to log"
                },
                "severity": {
                    "type": "string",
                    "enum": [
                        "EMERGENCY",
                        "ALERT",
                        "CRITICAL",
                        "ERROR",
                        "WARNING",
                        "NOTICE",
                        "INFORMATIONAL",
                        "DEBUG"
                    ],
                    "description": "Specifies that only messages of the given severity (or\ngreater severity) for the corresonding facility are logged"
                }
            }
        },
        "logging-selectors-state": {
            "description": "Operational state data for logging selectors"
