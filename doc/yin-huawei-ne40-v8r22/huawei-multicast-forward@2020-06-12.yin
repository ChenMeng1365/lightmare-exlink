  <grouping name="switch-para">
    <description>
      <text>The multicast-nat switching para.</text>
    </description>
    <leaf name="switch-mode">
      <type name="switch-mode"/>
      <description>
        <text>Switch mode.</text>
      </description>
    </leaf>
    <leaf name="switch-field-type">
      <when condition="../switch-mode='clean-switch'"/>
      <type name="switch-field-type"/>
      <description>
        <text>Specify a switch field type.</text>
      </description>
    </leaf>
  </grouping>
  <container name="multicast-forward">
    <description>
      <text>Table of multicast-forward.</text>
    </description>
    <container name="global">
      <description>
        <text>Configure multicast forwarding global attributes.</text>
      </description>
      <leaf name="shaping-enable">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable multicast shaping.</text>
        </description>
      </leaf>
      <leaf name="bw-car-enable">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable multicast virtual scheduling information.</text>
        </description>
      </leaf>
      <leaf name="monitor-frr-cycle">
        <type name="cycle"/>
        <units name="ms"/>
        <default value="20"/>
        <description>
          <text>Set multicast frr monitor cycle.</text>
        </description>
      </leaf>
      <leaf name="frr-revert-disable">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable multicast frr revert.</text>
        </description>
      </leaf>
      <leaf name="reliability-enhance-enable">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable multicast reliability enhancement.</text>
        </description>
      </leaf>
      <leaf name="fragment-enable">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable multicast fragmentation. The configuration is performed based on a board.</text>
        </description>
      </leaf>
    </container>
    <container name="nat">
      <presence value="multicast NAT enable"/>
      <description>
        <text>Enable/disable multicast nat configurations.</text>
      </description>
      <leaf name="ttl-decrease-disable">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable the multicast NAT TTL decrement function.</text>
        </description>
      </leaf>
      <container name="inbound-interfaces">
        <description>
          <text>List of interfaces enabled with multicast NAT.</text>
        </description>
        <list name="inbound-interface">
          <key value="interface-name"/>
          <description>
            <text>Configure interface on which multicast NAT is enabled.</text>
          </description>
          <leaf name="interface-name">
            <type name="leafref">
              <path value="/ifm:ifm/ifm:interfaces/ifm:interface/ifm:name"/>
            </type>
            <must condition="(/ifm:ifm/ifm:interfaces/ifm:interface[ifm:name=current()]/ifm:type = 'GigabitEthernet' or /ifm:ifm/ifm:interfaces/ifm:interface[ifm:name=current()]/ifm:type = '4x10GE' or /ifm:ifm/ifm:interfaces/ifm:interface[ifm:name=current()]/ifm:type = '10x10GE' or /ifm:ifm/ifm:interfaces/ifm:interface[ifm:name=current()]/ifm:type = '3x40GE' or /ifm:ifm/ifm:interfaces/ifm:interface[ifm:name=current()]/ifm:type = '4x25GE' or /ifm:ifm/ifm:interfaces/ifm:interface[ifm:name=current()]/ifm:type = 'XGigabitEthernet' or /ifm:ifm/ifm:interfaces/ifm:interface[ifm:name=current()]/ifm:type = '50GE' or /ifm:ifm/ifm:interfaces/ifm:interface[ifm:name=current()]/ifm:type = '40GE' or /ifm:ifm/ifm:interfaces/ifm:interface[ifm:name=current()]/ifm:type = '400GE' or /ifm:ifm/ifm:interfaces/ifm:interface[ifm:name=current()]/ifm:type = '25GE' or /ifm:ifm/ifm:interfaces/ifm:interface[ifm:name=current()]/ifm:type = '10GE' or /ifm:ifm/ifm:interfaces/ifm:interface[ifm:name=current()]/ifm:type = '100GE' or /ifm:ifm/ifm:interfaces/ifm:interface[ifm:name=current()]/ifm:type = '50|100GE')"/>
            <description>
              <text>Name of an interface.</text>
            </description>
          </leaf>
          <leaf name="igmp-enable">
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable multicast NAT IGMP on an interface.</text>
            </description>
          </leaf>
        </list>
      </container>
      <container name="instances">
        <description>
          <text>List of multicast NAT instances.</text>
        </description>
        <list name="instance">
          <key value="instance-id"/>
          <unique tag="instance-name"/>
          <description>
            <text>Configure multicast NAT instance.</text>
          </description>
          <leaf name="instance-id">
            <type name="uint32">
              <range value="1..16000"/>
            </type>
            <description>
              <text>ID of a multicast NAT instance.</text>
            </description>
          </leaf>
          <leaf name="instance-name">
            <type name="string">
              <length value="1..127"/>
            </type>
            <description>
              <text>Name of a multicast NAT instance.</text>
            </description>
          </leaf>
          <leaf name="behavior-name">
            <type name="string">
              <length value="1..127"/>
            </type>
            <config value="false"/>
            <description>
              <text>Name of a traffic behavior to which the multicast NAT instance is bound.</text>
            </description>
          </leaf>
          <container name="outbounds">
            <config value="false"/>
            <description>
              <text>List of output streams.</text>
            </description>
            <list name="outbound">
              <key value="outbound-id"/>
              <description>
                <text>Output stream state.</text>
              </description>
              <leaf name="outbound-id">
                <type name="uint32">
                  <range value="1..16000"/>
                </type>
                <description>
                  <text>ID of an outgoing flow.</text>
                </description>
              </leaf>
              <leaf name="outbound-name">
                <type name="string">
                  <length value="1..127"/>
                </type>
                <description>
                  <text>Name of an outgoing flow.</text>
                </description>
              </leaf>
              <leaf name="src-mac-trans-mode">
                <type name="string">
                  <length value="1..31"/>
                </type>
                <default value="not-translate"/>
                <description>
                  <text>Source MAC Address Modification Mode/Whether the source MAC address is automatically modified or not modified.</text>
                </description>
              </leaf>
              <leaf name="src-address-trans-mode">
                <type name="string">
                  <length value="1..31"/>
                </type>
                <default value="not-translate"/>
                <description>
                  <text>Enable or disable the function of modifying the source IP address.</text>
                </description>
              </leaf>
              <leaf name="source-address">
                <type name="inet:ipv4-address-no-zone"/>
                <description>
                  <text>Multicast source address. If src-address-trans-mode is not-translate, this field is displayed as 0.</text>
                </description>
              </leaf>
              <leaf name="dst-address-trans-mode">
                <type name="string">
                  <length value="1..31"/>
                </type>
                <default value="not-translate"/>
                <description>
                  <text>Enable or disable the function of modifying the destination MAC address.</text>
                </description>
              </leaf>
              <leaf name="destination-address">
                <type name="inet:ipv4-address-no-zone"/>
                <description>
                  <text>Destination IP address of a packet. If dst-address-trans-mode is not-translate, this field is displayed as 0.</text>
                </description>
              </leaf>
              <leaf name="source-udp-trans-mode">
                <type name="string">
                  <length value="1..31"/>
                </type>
                <description>
                  <text>Enable or disable the function of modifying the source UDP port.</text>
                </description>
              </leaf>
              <leaf name="source-udp-port">
                <type name="uint32">
                  <range value="0..65535"/>
                </type>
                <description>
                  <text>Source UDP port number of a packet. If source-udp-trans-mode is not-translate, this field is displayed as 65536.</text>
                </description>
              </leaf>
              <leaf name="dst-udp-trans-mode">
                <type name="string">
                  <length value="1..31"/>
                </type>
                <default value="not-translate"/>
                <description>
                  <text>Enable or disable the function of modifying the destination UDP port.</text>
                </description>
              </leaf>
              <leaf name="dst-udp-port">
                <type name="uint32">
                  <range value="0..65535"/>
                </type>
                <description>
                  <text>Destination UDP port number of a packet. If dst-udp-trans-mode is not-translate, this field is displayed as 65536.</text>
                </description>
              </leaf>
              <leaf name="rtp-ssrc-trans-mode">
                <type name="string">
                  <length value="1..31"/>
                </type>
                <default value="not-translate"/>
                <description>
                  <text>RTP SSRC modification mode.</text>
                </description>
              </leaf>
              <leaf name="rtp-seq-num-trans-mode">
                <type name="string">
                  <length value="1..31"/>
                </type>
                <description>
                  <text>Indicate that the sequence number of multicast streams is automatically translated.</text>
                </description>
              </leaf>
              <leaf name="rtp-ext-seq-num-trans-mode">
                <type name="string">
                  <length value="1..31"/>
                </type>
                <description>
                  <text>Indicate that the extended sequence number of multicast streams is automatically translated.</text>
                </description>
              </leaf>
              <leaf name="interface-name">
                <type name="pub-type:if-name"/>
                <description>
                  <text>Name of an interface.</text>
                </description>
              </leaf>
            </list>
          </container>
        </list>
      </container>
      <container name="outbounds">
        <description>
          <text>List of NAT configurations for multicast streams on outbound interfaces.</text>
        </description>
        <list name="outbound">
          <key value="interface-name outbound-id"/>
          <unique tag="outbound-name"/>
          <unique tag="outbound-id"/>
          <description>
            <text>Configure multicast stream translation on an outbound interface and configures multicast stream translation parameters.</text>
          </description>
          <leaf name="interface-name">
            <type name="leafref">
              <path value="/ifm:ifm/ifm:interfaces/ifm:interface/ifm:name"/>
            </type>
            <must condition="(/ifm:ifm/ifm:interfaces/ifm:interface[ifm:name=current()]/ifm:type = 'GigabitEthernet' or /ifm:ifm/ifm:interfaces/ifm:interface[ifm:name=current()]/ifm:type = '4x10GE' or /ifm:ifm/ifm:interfaces/ifm:interface[ifm:name=current()]/ifm:type = '10x10GE' or /ifm:ifm/ifm:interfaces/ifm:interface[ifm:name=current()]/ifm:type = '3x40GE' or /ifm:ifm/ifm:interfaces/ifm:interface[ifm:name=current()]/ifm:type = '4x25GE' or /ifm:ifm/ifm:interfaces/ifm:interface[ifm:name=current()]/ifm:type = 'XGigabitEthernet' or /ifm:ifm/ifm:interfaces/ifm:interface[ifm:name=current()]/ifm:type = '50GE' or /ifm:ifm/ifm:interfaces/ifm:interface[ifm:name=current()]/ifm:type = '40GE' or /ifm:ifm/ifm:interfaces/ifm:interface[ifm:name=current()]/ifm:type = '400GE' or /ifm:ifm/ifm:interfaces/ifm:interface[ifm:name=current()]/ifm:type = '25GE' or /ifm:ifm/ifm:interfaces/ifm:interface[ifm:name=current()]/ifm:type = '10GE' or /ifm:ifm/ifm:interfaces/ifm:interface[ifm:name=current()]/ifm:type = '100GE' or /ifm:ifm/ifm:interfaces/ifm:interface[ifm:name=current()]/ifm:type = '50|100GE')"/>
            <description>
              <text>Name of an interface.</text>
            </description>
          </leaf>
          <leaf name="outbound-id">
            <type name="uint32">
              <range value="1..16000"/>
            </type>
            <description>
              <text>Specify the ID of an output multicast stream on an outbound interface.</text>
            </description>
          </leaf>
          <leaf name="outbound-name">
            <type name="string">
              <length value="1..127"/>
            </type>
            <description>
              <text>Specify the name of an output multicast stream on an outbound interface.</text>
            </description>
          </leaf>
          <leaf name="source-mac-trans-mode">
            <type name="source-mac-change-type"/>
            <default value="not-translate"/>
            <description>
              <text>Source MAC address modification mode/Whether the source MAC address is automatically modified or not modified.</text>
            </description>
          </leaf>
          <leaf name="source-address-trans-mode">
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable the function of modifying the source address.</text>
            </description>
          </leaf>
          <leaf name="destination-address-trans-mode">
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable the function of modifying the destination MAC address.</text>
            </description>
          </leaf>
          <leaf name="source-udp-trans-mode">
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable the function of modifying the source UDP port.</text>
            </description>
          </leaf>
          <leaf name="destination-udp-trans-mode">
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable the function of modifying the destination UDP port.</text>
            </description>
          </leaf>
          <leaf name="rtp-ssrc-trans-mode">
            <type name="rtp-ssrc-change-type"/>
            <default value="not-translate"/>
            <description>
              <text>RTP SSRC modification mode.</text>
            </description>
          </leaf>
          <leaf name="rtp-seq-num-trans-mode">
            <type name="rtp-seq-num-change-type"/>
            <default value="not-translate"/>
            <description>
              <text>Indicate that the sequence number of multicast streams is automatically translated.</text>
            </description>
          </leaf>
          <leaf name="rtp-ext-seq-num-trans-mode">
            <when condition="../rtp-seq-num-trans-mode='auto-translate'"/>
            <type name="rtp-ext-seq-num-change-type"/>
            <default value="not-translate"/>
            <description>
              <text>RTP extended sequence number modification mode.</text>
            </description>
          </leaf>
          <leaf name="source-address">
            <when condition="../source-address-trans-mode='true'"/>
            <type name="inet:ipv4-address-no-zone">
              <pattern value="(\d|[1-9]\d|1\d{2}|2[0-1]\d|22[0-3])(\.(25[0-5]|2[0-4]\d|1\d\d|[1-9]?\d)){3}"/>
            </type>
            <mandatory value="true"/>
            <description>
              <text>Multicast source address.</text>
            </description>
          </leaf>
          <leaf name="destination-address">
            <when condition="../destination-address-trans-mode='true'"/>
            <type name="inet:ipv4-address-no-zone">
              <pattern value="(22[4-9]|23\d)(\.(25[0-5]|2[0-4]\d|1\d\d|[1-9]?\d)){3}"/>
            </type>
            <mandatory value="true"/>
            <description>
              <text>Destination IP address of a packet.</text>
            </description>
          </leaf>
          <leaf name="source-udp-port">
            <when condition="../source-udp-trans-mode='true'"/>
            <type name="uint32">
              <range value="0..65535"/>
            </type>
            <mandatory value="true"/>
            <description>
              <text>Source UDP port number of a packet.</text>
            </description>
          </leaf>
          <leaf name="destination-udp-port">
            <when condition="../destination-udp-trans-mode='true'"/>
            <type name="uint32">
              <range value="0..65535"/>
            </type>
            <mandatory value="true"/>
            <description>
              <text>Destination UDP port number of a packet.</text>
            </description>
          </leaf>
        </list>
      </container>
      <container name="instance-groups">
        <description>
          <text>List of multicast NAT instance groups.</text>
        </description>
        <list name="instance-group">
          <key value="group-id"/>
          <unique tag="group-name"/>
          <description>
            <text>Configure multicast NAT instance group.</text>
          </description>
          <leaf name="group-id">
            <type name="uint32">
              <range value="1..4000"/>
            </type>
            <description>
              <text>ID of a multicast NAT instance group.</text>
            </description>
          </leaf>
          <leaf name="group-name">
            <type name="string">
              <length value="1..127"/>
            </type>
            <description>
              <text>Name of a multicast NAT instance group.</text>
            </description>
          </leaf>
          <container name="part-instances">
            <description>
              <text>List of multicast NAT part instances.</text>
            </description>
            <list name="part-instance">
              <key value="part-id"/>
              <description>
                <text>Assign an instance id to instance group.</text>
              </description>
              <leaf name="part-id">
                <type name="uint32">
                  <range value="1..16"/>
                </type>
                <description>
                  <text>Location of a multicast NAT instance in an instance group.</text>
                </description>
              </leaf>
              <leaf name="instance-id">
                <type name="leafref">
                  <path value="/multicast-forward/nat/instances/instance/instance-id"/>
                </type>
                <mandatory value="true"/>
                <description>
                  <text>ID of a multicast NAT instance.</text>
                </description>
              </leaf>
              <leaf name="instance-name">
                <type name="leafref">
                  <path value="/multicast-forward/nat/instances/instance[instance-id=current()/../instance-id]/instance-name"/>
                </type>
                <description>
                  <text>Name of a multicast NAT instance.</text>
                </description>
              </leaf>
              <leaf name="stream-type">
                <type name="stream-type"/>
                <must condition="(../stream-type='default-stream-type' and not(/multicast-forward/nat/instance-groups/instance-group/part-instances/part-instance[stream-type!='default-stream-type'])) or (../stream-type !='default-stream-type' and not(/multicast-forward/nat/instance-groups/instance-group/part-instances/part-instance[stream-type='default-stream-type']))"/>
                <default value="default-stream-type"/>
                <description>
                  <text>Stream type. The default value and the specified value (ancillary-data, audio-stream, or video-stream) in the stream-type values of multiple part-instances in the same instance group cannot coexist. That is, all the values must be default or the specified values.</text>
                </description>
              </leaf>
            </list>
          </container>
        </list>
      </container>
      <container name="outbound-groups">
        <description>
          <text>List of output stream groups.</text>
        </description>
        <list name="outbound-group">
          <key value="group-id"/>
          <unique tag="group-name"/>
          <description>
            <text>Configure output stream group.</text>
          </description>
          <leaf name="group-id">
            <type name="uint32">
              <range value="1..4000"/>
            </type>
            <description>
              <text>ID of an output stream group.</text>
            </description>
          </leaf>
          <leaf name="group-name">
            <type name="string">
              <length value="1..127"/>
            </type>
            <description>
              <text>Name of an output stream group.</text>
            </description>
          </leaf>
          <container name="part-outbounds">
            <description>
              <text>List of output part outbounds.</text>
            </description>
            <list name="part-outbound">
              <key value="part-id"/>
              <description>
                <text>Assign outbound id to outbound group.</text>
              </description>
              <leaf name="part-id">
                <type name="uint32">
                  <range value="1..16"/>
                </type>
                <description>
                  <text>Location of an output stream in an output stream group.</text>
                </description>
              </leaf>
              <leaf name="outbound-id">
                <type name="leafref">
                  <path value="/multicast-forward/nat/outbounds/outbound/outbound-id"/>
                </type>
                <must condition="count(/multicast-forward:multicast-forward/multicast-forward:nat/multicast-forward:outbound-groups/multicast-forward:outbound-group/multicast-forward:part-outbounds/multicast-forward:part-outbound[multicast-forward:outbound-id = current()]) = 1"/>
                <mandatory value="true"/>
                <description>
                  <text>ID of an outgoing flow.</text>
                </description>
              </leaf>
              <leaf name="outbound-name">
                <type name="leafref">
                  <path value="/multicast-forward/nat/outbounds/outbound[outbound-id=current()/../outbound-id]/outbound-name"/>
                </type>
                <description>
                  <text>Name of an outgoing flow.</text>
                </description>
              </leaf>
              <leaf name="stream-type">
                <type name="stream-type"/>
                <must condition="(../stream-type='default-stream-type' and not(/multicast-forward/nat/outbound-groups/outbound-group/part-outbounds/part-outbound[stream-type!='default-stream-type'])) or (../stream-type !='default-stream-type' and not(/multicast-forward/nat/outbound-groups/outbound-group/part-outbounds/part-outbound[stream-type='default-stream-type']))"/>
                <default value="default-stream-type"/>
                <description>
                  <text>Stream type. The default value and the specified value (ancillary-data, audio-stream, or video-stream) in the stream-type values of multiple part-outbounds in the same outbound group cannot coexist. That is, all the values must be default or the specified values.</text>
                </description>
              </leaf>
            </list>
          </container>
        </list>
      </container>
      <container name="bind">
        <presence value="multicast nat bind configuration"/>
        <description>
          <text>Enable/disable single or group binding configuration.</text>
        </description>
        <container name="single-binds">
          <description>
            <text>List of bindings between output multicast streams and multicast NAT instances.</text>
          </description>
          <list name="single-bind">
            <key value="outbound-id"/>
            <description>
              <text>Configure binding between output multicast streams and multicast NAT instances.</text>
            </description>
            <leaf name="outbound-id">
              <type name="leafref">
                <path value="/multicast-forward/nat/outbounds/outbound/outbound-id"/>
              </type>
              <description>
                <text>ID of an outgoing flow.</text>
              </description>
            </leaf>
            <leaf name="fast-mode-enable">
              <type name="boolean"/>
              <default value="false"/>
              <description>
                <text>Enable/disable the function of fast switch. Switch parameters (e.g.instance-id/switch-mode/witch-field-type) can not be modified when fast-mode-enable is true.</text>
              </description>
            </leaf>
            <leaf name="outbound-name">
              <type name="leafref">
                <path value="/multicast-forward/nat/outbounds/outbound[outbound-id=current()/../outbound-id]/outbound-name"/>
              </type>
              <description>
                <text>Name of an outgoing flow.</text>
              </description>
            </leaf>
            <leaf name="instance-id">
              <ext:operation-exclude value="update">
                <when condition="../fast-mode-enable = 'true'"/>
                <description>
                  <text>The current node cannot be modified, when fast-mode-enable node is 'true'.</text>
                </description>
              </ext:operation-exclude>
              <type name="leafref">
                <path value="/multicast-forward/nat/instances/instance/instance-id"/>
              </type>
              <mandatory value="true"/>
              <description>
                <text>ID of a multicast NAT instance.</text>
              </description>
            </leaf>
            <leaf name="instance-name">
              <type name="leafref">
                <path value="/multicast-forward/nat/instances/instance[instance-id=current()/../instance-id]/instance-name"/>
              </type>
              <description>
                <text>Name of a multicast NAT instance.</text>
              </description>
            </leaf>
            <leaf name="switch-mode">
              <ext:operation-exclude value="update|delete">
                <when condition="../fast-mode-enable = 'true'"/>
                <description>
                  <text>The current node cannot be modified, when fast-mode-enable node is 'true'.</text>
                </description>
              </ext:operation-exclude>
              <type name="switch-mode"/>
              <default value="default-switch"/>
              <description>
                <text>Switch mode.</text>
              </description>
            </leaf>
            <leaf name="switch-field-type">
              <ext:operation-exclude value="update|delete">
                <when condition="../fast-mode-enable = 'true'"/>
                <description>
                  <text>The current node cannot be modified, when fast-mode-enable node is 'true'.</text>
                </description>
              </ext:operation-exclude>
              <when condition="../switch-mode='clean-switch'"/>
              <type name="switch-field-type"/>
              <default value="default-field"/>
              <description>
                <text>Specify a switch field type.</text>
              </description>
            </leaf>
          </list>
        </container>
        <container name="group-binds">
          <description>
            <text>List of bindings between output stream groups and multicast NAT instance groups.</text>
          </description>
          <list name="group-bind">
            <key value="outbound-group-id"/>
            <description>
              <text>Configure binding between output stream groups and multicast NAT instance groups.
eg. output stream group1: part count (value:outbound-count), part list[1]:outbound-1-part-1,outbound-1,outbound-1-streamtype-1, part list[2]:outbound-2-part-2,outbound-2,outbound-2-streamtype-2...;
 instance group2:part count (value:instance-count), part list[1]:instance-1-part-1,instance-1,instance-1-streamtype-1, part list[2]:instance-2-part-2,instance-2,instance-2-streamtype-2...;
 outbound-count == instance-count, outbound-count ~= 0, instance-count ~= 0,
 outbound-1-part-1 == instance-1-part-1, outbound-1-streamtype-1 == instance-1-streamtype-1;outbound-2-part-2 == instance-2-part-2, outbound-2-streamtype-2 == instance-2-streamtype-2;...;outbound-n-part-n == instance-n-part-n, outbound-n-streamtype-n == instance-n-streamtype-n.</text>
            </description>
            <leaf name="outbound-group-id">
              <type name="leafref">
                <path value="/multicast-forward/nat/outbound-groups/outbound-group/group-id"/>
              </type>
              <description>
                <text>Output stream group ID.</text>
              </description>
            </leaf>
            <leaf name="fast-mode-enable">
              <type name="boolean"/>
              <default value="false"/>
              <description>
                <text>Enable/disable the function of fast switch. Switch parameters (e.g.instance-group-id/switch-mode/switch-field-type) can not be modified when fast-mode-enable is true.</text>
              </description>
            </leaf>
            <leaf name="outbound-group-name">
              <type name="leafref">
                <path value="/multicast-forward/nat/outbound-groups/outbound-group[group-id=current()/../outbound-group-id]/group-name"/>
              </type>
              <description>
                <text>Output stream group name.</text>
              </description>
            </leaf>
            <leaf name="instance-group-id">
              <ext:operation-exclude value="update">
                <when condition="../fast-mode-enable = 'true'"/>
                <description>
                  <text>The current node cannot be modified, when fast-mode-enable node is 'true'.</text>
                </description>
              </ext:operation-exclude>
              <type name="leafref">
                <path value="/multicast-forward/nat/instance-groups/instance-group/group-id"/>
              </type>
              <mandatory value="true"/>
              <description>
                <text>ID of a multicast NAT instance group.</text>
              </description>
            </leaf>
            <leaf name="instance-group-name">
              <type name="leafref">
                <path value="/multicast-forward/nat/instance-groups/instance-group[group-id=current()/../instance-group-id]/group-name"/>
              </type>
              <description>
                <text>Name of a multicast NAT instance group.</text>
              </description>
            </leaf>
            <leaf name="switch-mode">
              <ext:operation-exclude value="update|delete">
                <when condition="../fast-mode-enable = 'true'"/>
                <description>
                  <text>The current node cannot be modified, when fast-mode-enable node is 'true'.</text>
                </description>
              </ext:operation-exclude>
              <type name="switch-mode"/>
              <default value="default-switch"/>
              <description>
                <text>Switch mode.</text>
              </description>
            </leaf>
            <leaf name="switch-field-type">
              <ext:operation-exclude value="update|delete">
                <when condition="../fast-mode-enable = 'true'"/>
                <description>
                  <text>The current node cannot be modified, when fast-mode-enable node is 'true'.</text>
                </description>
              </ext:operation-exclude>
              <when condition="../switch-mode='clean-switch'"/>
              <type name="switch-field-type"/>
              <default value="default-field"/>
              <description>
                <text>Specify a switch field type.</text>
              </description>
            </leaf>
          </list>
        </container>
        <container name="clean-switch-interval">
          <description>
            <text>Configure the interval between two adjacent multicast NAT clean switching commands.</text>
          </description>
          <leaf name="interval">
            <type name="uint32">
              <range value="300..500"/>
            </type>
            <units name="ms"/>
            <default value="500"/>
            <description>
              <text>Specifies the interval between two adjacent multicast NAT clean switching commands.</text>
            </description>
          </leaf>
        </container>
      </container>
    </container>
    <container name="multicast-shaping">
      <presence value="Configuration of multicast shaping."/>
      <description>
        <text>Enable/disable multicast shaping configuration.</text>
      </description>
      <container name="multicast-lists">
        <description>
          <text>List of configuration of the multicast shaping parameters.</text>
        </description>
        <list name="multicast-list">
          <key value="index"/>
          <description>
            <text>Configure the multicast shaping parameters of a specified multicast program list.</text>
          </description>
          <leaf name="index">
            <type name="uint32">
              <range value="0..8191"/>
            </type>
            <description>
              <text>The index of a multicast program. The configured multicast program list index must have been created.</text>
            </description>
          </leaf>
          <leaf name="name">
            <type name="string">
              <length value="1..32"/>
            </type>
            <description>
              <text>The name of a multicast program. The configured multicast program list name must be the same as the name of the program list corresponding to the actual index.</text>
            </description>
          </leaf>
          <leaf name="cir">
            <type name="uint32">
              <range value="8..4294967295"/>
            </type>
            <mandatory value="true"/>
            <description>
              <text>The guaranteed bandwidth for multicast shaping.</text>
            </description>
          </leaf>
          <leaf name="pir">
            <type name="uint32">
              <range value="8..4294967295"/>
            </type>
            <must condition="../pir &gt;= ../cir"/>
            <description>
              <text>The peak bandwidth for multicast shaping.</text>
            </description>
          </leaf>
          <leaf name="queue-length">
            <type name="uint32">
              <range value="2000..128000"/>
            </type>
            <description>
              <text>The length of a multicast shaping packet.</text>
            </description>
          </leaf>
        </list>
      </container>
    </container>
    <container name="egress-forward-mode">
      <description>
        <text>Configure switch downstream multicast traffic replication by NP to replication by TM. The configuration is performed based on a board.</text>
      </description>
      <container name="global">
        <description>
          <text>Configure on all boards.</text>
        </description>
        <leaf name="enable">
          <type name="empty"/>
          <description>
            <text>Globally enable the configuration of switching downstream multicast traffic replication by NP to replication by TM. The configuration is performed based on a board.</text>
          </description>
        </leaf>
      </container>
      <leaf-list name="slot">
        <type name="string">
          <length value="1..16"/>
        </type>
        <must condition="not(../global)"/>
        <description>
          <text>Specify the slot ID of the board where the target port to which downstream multicast traffic is to be replicated on the TM resides.</text>
        </description>
      </leaf-list>
    </container>
    <container name="mcfwd-entrys">
      <config value="false"/>
      <description>
        <text>List of multicast forwarding table.</text>
      </description>
      <list name="mcfwd-entry">
        <key value="vrf-name address-family"/>
        <description>
          <text>Configure multicast forwarding table state.</text>
        </description>
        <leaf name="vrf-name">
          <type name="leafref">
            <path value="/ni:network-instance/ni:instances/ni:instance/ni:name"/>
          </type>
          <description>
            <text>Virtual private network.</text>
          </description>
        </leaf>
        <leaf name="address-family">
          <type name="af-type"/>
          <description>
            <text>Multicast address family type.</text>
          </description>
        </leaf>
        <container name="forward-infors">
          <description>
            <text>List of multicast forwarding information.</text>
          </description>
          <list name="forward-infor">
            <key value="source-address group-address oif-name"/>
            <description>
              <text>Multicast forwarding information state.</text>
            </description>
            <leaf name="source-address">
              <type name="inet:ip-address-no-zone"/>
              <description>
                <text>Multicast source address.</text>
              </description>
            </leaf>
            <leaf name="group-address">
              <type name="inet:ip-address-no-zone"/>
              <description>
                <text>Multicast group address.</text>
              </description>
            </leaf>
            <leaf name="oif-name">
              <type name="string">
                <length value="1..255"/>
              </type>
              <description>
                <text>The name of outgoing interface. If there is no multicast forwarding outbound interface, this field is displayed as --.</text>
              </description>
            </leaf>
            <leaf name="mid">
              <type name="uint32"/>
              <description>
                <text>ID of multicast forwarding.</text>
              </description>
            </leaf>
            <leaf name="flag">
              <type name="string">
                <length value="1..255"/>
              </type>
              <description>
                <text>Flag of multicast forwarding.</text>
              </description>
            </leaf>
            <leaf name="sg-up-time">
              <type name="string">
                <length value="1..255"/>
              </type>
              <description>
                <text>Living time of multicast forwarding table.</text>
              </description>
            </leaf>
            <leaf name="sg-up-exp-time">
              <type name="string">
                <length value="1..255"/>
              </type>
              <description>
                <text>Overtime of multicast forwarding table.</text>
              </description>
            </leaf>
            <leaf name="sg-iif-name">
              <type name="string">
                <length value="1..255"/>
              </type>
              <description>
                <text>The name of incoming interface.</text>
              </description>
            </leaf>
            <leaf name="sg-up-neighbor">
              <type name="inet:ip-address-no-zone"/>
              <description>
                <text>Root ip.</text>
              </description>
            </leaf>
            <leaf name="mtoken">
              <type name="pub-type:hex-binary"/>
              <default value="0x0"/>
              <description>
                <text>Mtoken of the tunnel.</text>
              </description>
            </leaf>
            <leaf name="active-time">
              <type name="string">
                <length value="1..255"/>
              </type>
              <description>
                <text>Active time.</text>
              </description>
