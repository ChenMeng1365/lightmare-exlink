  <container name="configuration">
    <description>
      <text>Configuration model</text>
    </description>
    <container name="firewall">
      <description>
        <text>Configure firewall parameters</text>
      </description>
      <container name="session-timeout">
        <description>
          <text>Configure session timeout parameters</text>
        </description>
        <leaf name="tcp">
          <description>
            <text>TCP session timeout</text>
          </description>
          <type name="int16">
            <range value="1..7200"/>
          </type>
          <units name="s"/>
          <default value="600"/>
        </leaf>
        <leaf name="udp">
          <description>
            <text>UDP session timeout</text>
          </description>
          <type name="int16">
            <range value="1..7200"/>
          </type>
          <units name="s"/>
          <default value="40"/>
        </leaf>
        <leaf name="icmp">
          <description>
            <text>ICMP session timeout</text>
          </description>
          <type name="int16">
            <range value="1..7200"/>
          </type>
          <units name="s"/>
          <default value="20"/>
        </leaf>
        <leaf name="ftp">
          <description>
            <text>FTP session timeout</text>
          </description>
          <type name="int16">
            <range value="1..7200"/>
          </type>
          <units name="s"/>
          <default value="120"/>
        </leaf>
        <leaf name="sip">
          <description>
            <text>SIP session timeout</text>
          </description>
          <type name="int16">
            <range value="1..7200"/>
          </type>
          <units name="s"/>
          <default value="1800"/>
        </leaf>
        <leaf name="http">
          <description>
            <text>HTTP session timeout</text>
          </description>
          <type name="int16">
            <range value="1..7200"/>
          </type>
          <units name="s"/>
          <default value="120"/>
        </leaf>
      </container>
      <container name="ip-tcp-intercept">
        <description>
          <text>Configure IP TCP intercept parameters</text>
        </description>
        <leaf name="finrst-timeout">
          <description>
            <text>IP TCP intercept timeout of FIN and RST</text>
          </description>
          <type name="uint16">
            <range value="1..10"/>
          </type>
          <units name="s"/>
          <default value="5"/>
        </leaf>
        <leaf name="watch-timeout">
          <description>
            <text>IP TCP intercept watch timeout</text>
          </description>
          <type name="uint16">
            <range value="1..60"/>
          </type>
          <units name="s"/>
          <default value="30"/>
        </leaf>
        <leaf name="drop-mode">
          <description>
            <text>IP TCP intercept drop mode</text>
          </description>
          <type name="enumeration">
            <enum name="oldest">
              <value value="0"/>
            </enum>
            <enum name="random">
              <value value="1"/>
            </enum>
          </type>
          <default value="oldest"/>
        </leaf>
        <leaf name="mode">
          <description>
            <text>IP TCP intercept mode</text>
          </description>
          <type name="enumeration">
            <enum name="intercept">
              <value value="0"/>
            </enum>
            <enum name="watch">
              <value value="1"/>
            </enum>
          </type>
          <default value="intercept"/>
        </leaf>
        <container name="max-incomplete">
          <description>
            <text>IP TCP intercept max-incomplete threshold</text>
          </description>
          <leaf name="high">
            <description>
              <text>Max-incomplete high threshold</text>
            </description>
            <type name="uint32">
              <range value="1..500000"/>
            </type>
            <default value="1100"/>
          </leaf>
          <leaf name="low">
            <description>
              <text>Max-incomplete low threshold</text>
            </description>
            <type name="uint32">
              <range value="1..500000"/>
            </type>
            <default value="900"/>
          </leaf>
        </container>
        <container name="one-minute">
          <description>
            <text>IP TCP intercept one-minute threshold</text>
          </description>
          <leaf name="high">
            <description>
              <text>One-minute high threshold</text>
            </description>
            <type name="uint32">
              <range value="1..500000"/>
            </type>
            <default value="1100"/>
          </leaf>
          <leaf name="low">
            <description>
              <text>One-minute low threshold</text>
            </description>
            <type name="uint32">
              <range value="1..500000"/>
            </type>
            <default value="900"/>
          </leaf>
        </container>
      </container>
      <container name="port-timeout-lists">
        <description>
          <text>TCP/UDP port session timeout list</text>
        </description>
        <list name="port-timeout-list">
          <description>
            <text>TCP/UDP port session timeout</text>
          </description>
          <key value="protocol-type port"/>
          <leaf name="protocol-type">
            <description>
              <text>Protocol type</text>
            </description>
            <type name="enumeration">
              <enum name="tcp">
                <value value="1"/>
              </enum>
              <enum name="udp">
                <value value="2"/>
              </enum>
            </type>
          </leaf>
          <leaf name="port">
            <description>
              <text>Port number</text>
            </description>
            <type name="uint16">
              <range value="0..65535"/>
            </type>
          </leaf>
          <leaf name="timeout">
            <description>
              <text>Timeout</text>
            </description>
            <type name="int16">
              <range value="1..7200"/>
            </type>
            <units name="s"/>
            <mandatory value="true"/>
          </leaf>
        </list>
      </container>
      <container name="black-lists">
        <description>
          <text>Configure blacklist list</text>
        </description>
        <container name="black-ip-lists">
          <description>
            <text>Blacklist that key is IP</text>
          </description>
          <list name="black-ip-list">
            <description>
              <text>Blacklist that key is IP</text>
            </description>
            <key value="ip"/>
            <leaf name="ip">
              <description>
                <text>Blacklist IP address</text>
              </description>
              <type name="ncd:ipv4-address"/>
            </leaf>
            <leaf name="time-range">
              <description>
                <text>Timerange name</text>
              </description>
              <type name="string">
                <length value="0..31"/>
              </type>
              <default value=""/>
            </leaf>
          </list>
        </container>
        <container name="black-ip-vrf-lists">
          <description>
            <text>Blacklist that key are IP and VRF</text>
          </description>
          <list name="black-ip-vrf-list">
            <description>
              <text>Blacklist that key are IP and VRF</text>
            </description>
            <key value="vrf ip"/>
            <leaf name="vrf">
              <description>
                <text>VPN routing/forwarding instance name</text>
              </description>
              <type name="string">
                <length value="1..32"/>
              </type>
            </leaf>
            <leaf name="ip">
              <description>
                <text>Blacklist IP address</text>
              </description>
              <type name="ncd:ipv4-address"/>
            </leaf>
            <leaf name="time-range">
              <description>
                <text>Timerange name</text>
              </description>
              <type name="string">
                <length value="0..31"/>
