                          <text>Configure filter-lsa-out.</text>
                        </description>
                        <choice name="type">
                          <mandatory value="true"/>
                          <description>
                            <text>Set filter-lsa-out type.</text>
                          </description>
                          <case name="total">
                            <description>
                              <text>Filter total types of LSAs.</text>
                            </description>
                            <leaf name="all-flag">
                              <type name="empty"/>
                              <description>
                                <text>Filter all types of LSAs.</text>
                              </description>
                            </leaf>
                          </case>
                          <case name="other">
                            <description>
                              <text>Filter other types of LSAs.</text>
                            </description>
                            <leaf name="sum-flag">
                              <type name="empty"/>
                              <description>
                                <text>Filter type-3 Summary LSAs.</text>
                              </description>
                            </leaf>
                            <choice name="sum-filter">
                              <description>
                                <text>Type of a filtering rule.</text>
                              </description>
                              <case name="acl">
                                <description>
                                  <text>ACL-Name or ACL-Number.</text>
                                </description>
                                <leaf name="sum-name">
                                  <when condition="../sum-flag"/>
                                  <type name="leafref">
                                    <path value="/acl:acl/acl:groups/acl:group/acl:identity"/>
                                  </type>
                                  <description>
                                    <text>ACL-Name or ACL-Number.</text>
                                  </description>
                                </leaf>
                              </case>
                            </choice>
                            <leaf name="ase-flag">
                              <type name="empty"/>
                              <description>
                                <text>Filter type-5 ASE LSAs.</text>
                              </description>
                            </leaf>
                            <choice name="ase-filter">
                              <description>
                                <text>Type of a filtering rule.</text>
                              </description>
                              <case name="acl">
                                <description>
                                  <text>ACL-Name or ACL-Number.</text>
                                </description>
                                <leaf name="ase-name">
                                  <when condition="../ase-flag"/>
                                  <type name="leafref">
                                    <path value="/acl:acl/acl:groups/acl:group/acl:identity"/>
                                  </type>
                                  <description>
                                    <text>ACL-Name or ACL-Number.</text>
                                  </description>
                                </leaf>
                              </case>
                            </choice>
                            <leaf name="nssa-flag">
                              <type name="empty"/>
                              <description>
                                <text>Filter type-7 NSSA LSAs.</text>
                              </description>
                            </leaf>
                            <choice name="nssa-filter">
                              <description>
                                <text>Type of a filtering rule.</text>
                              </description>
                              <case name="acl">
                                <description>
                                  <text>ACL-Name or ACL-Number.</text>
