<?xml version="1.0" encoding="UTF-8"?>
<module name="huawei-twamp-responder-notif"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:twamp-responder-notif="urn:huawei:yang:huawei-twamp-responder-notif"
        xmlns:ext="urn:huawei:yang:huawei-extension">
  <yang-version value="1"/>
  <namespace uri="urn:huawei:yang:huawei-twamp-responder-notif"/>
  <prefix value="twamp-responder-notif"/>
  <import module="huawei-extension">
    <prefix value="ext"/>
  </import>
  <organization>
    <text>Huawei Technologies Co., Ltd.</text>
  </organization>
  <contact>
    <text>Huawei Industrial Base
Bantian, Longgang
Shenzhen 518129
People's Republic of China
Website: http://www.huawei.com
Email: support@huawei.com</text>
  </contact>
  <description>
    <text>Notification of twamp responder.</text>
  </description>
  <revision date="2021-02-01">
    <description>
      <text>Add standard TWAMP alarm.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <revision date="2020-10-30">
    <description>
      <text>Initial revision.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <ext:task-name value="twamp"/>
  <notification name="responder-session-num-exceed">
    <description>
      <text>The number of TWAMP responder session is out of device capability.</text>
    </description>
  </notification>
  <notification name="responder-session-num-recovery">
    <description>
      <text>The number of TWAMP responder session is under device capability.</text>
    </description>
  </notification>
  <notification name="control-session-num-exceed">
    <description>
      <text>The number of TWAMP control session is out of device capability.</text>
    </description>
  </notification>
  <notification name="control-session-num-recovery">
    <description>
      <text>The number of TWAMP control session is under device capability.</text>
    </description>
  </notification>
</module>
