            type uint32 {
              range '0 | 8..20000000' ;
            }
            units kbps ;
            mandatory true ;
          }
          leaf pbs {
            description PBS. ;
            type uint32 {
              range 1..5120000 ;
            }
            units KB ;
            mandatory true ;
          }
