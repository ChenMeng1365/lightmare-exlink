              +--ro policy?               enumeration
              +--ro vlan-id?              uint16
              +--ro vpn-instance-type?    enumeration
              +--ro vpn-instance-name?    string
              +--ro loop-flag?            enumeration
              +--ro loop-vlans?           ncd:bitmap
              +--ro loop-internal-vlan?   uint16
              +--ro loop-external-vlan?   uint16
