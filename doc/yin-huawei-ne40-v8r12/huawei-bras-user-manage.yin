  <grouping name="default-password">
    <description>
      <text>The default-password sets the password of an IPoX user. By default, the password of an IPoX user is VLAN.</text>
    </description>
    <leaf name="password-format">
      <type name="enumeration">
        <enum name="password">
          <value value="1"/>
          <description>
            <text>Indicates that the user password is in ciphertext. Specifies a ciphertext password. The password is case sensitive and does not contain command line characters such as blanks and question mark.</text>
          </description>
        </enum>
        <enum name="user-class">
          <value value="2"/>
          <description>
            <text>User's user-class (dhcpv4 option77/dhcpv6 option15).</text>
          </description>
        </enum>
        <enum name="vendor-class">
          <value value="3"/>
          <description>
            <text>User's vendor-class (dhcpv4 option60/dhcpv6 option16).</text>
          </description>
        </enum>
        <enum name="option60">
          <value value="4"/>
          <description>
            <text>User's vendor-class (dhcpv4 option60/dhcpv6 option16).</text>
          </description>
        </enum>
        <enum name="option77">
          <value value="5"/>
          <description>
            <text>User's user-class (dhcpv4 option77/dhcpv6 option15).</text>
          </description>
        </enum>
      </type>
      <mandatory value="true"/>
      <description>
        <text>Indicates the format of user password.</text>
      </description>
    </leaf>
    <leaf name="password-value">
      <when condition="../password-format='password'"/>
      <type name="pub-type:password-extend">
        <length value="1..268"/>
      </type>
      <mandatory value="true"/>
      <description>
        <text>Indicates that the user password is in ciphertext. Specifies a ciphertext password. The password is case sensitive and does not contain command line characters such as blanks and question mark.</text>
      </description>
    </leaf>
    <leaf name="vendor-class-format">
      <when condition="../password-format='vendor-class' or ../password-format='option60'"/>
      <type name="enumeration">
        <enum name="cn">
          <value value="1"/>
          <description>
            <text>Indicates that the system uses the vendor-class attribute of the China Telecom format as the user password.</text>
          </description>
        </enum>
        <enum name="length-defined">
          <value value="2"/>
          <description>
            <text>Configures the field length of the vendor-class attribute as the user password.</text>
          </description>
        </enum>
        <enum name="sub-option">
          <value value="3"/>
          <description>
            <text>Configures the sub-option code of the vendor-class attribute as the user password.</text>
          </description>
        </enum>
      </type>
      <description>
        <text>Configures the format of the vendor-class attribute as the user password.</text>
      </description>
    </leaf>
    <leaf name="vendor-class-offset-value">
      <when condition="../vendor-class-format='length-defined' or ../vendor-class-format='sub-option'"/>
      <type name="uint32">
        <range value="0..255"/>
      </type>
      <description>
        <text>Configures the offset of a vendor-class option as the user password.</text>
      </description>
    </leaf>
    <leaf name="vendor-class-length-value">
      <when condition="../vendor-class-format='length-defined'"/>
      <type name="uint32">
        <range value="0..255"/>
      </type>
      <mandatory value="true"/>
      <description>
        <text>Configures the field length of the vendor-class attribute as the user password.</text>
      </description>
    </leaf>
    <leaf name="vendor-class-sub-option-code">
      <when condition="../vendor-class-format='sub-option'"/>
      <type name="uint32">
        <range value="0..255"/>
      </type>
      <mandatory value="true"/>
      <description>
        <text>Configures the sub-option code of the vendor-class attribute as the user password.</text>
      </description>
    </leaf>
    <leaf name="vendor-class-sub-offset">
      <when condition="../vendor-class-format='sub-option'"/>
      <type name="uint32">
        <range value="0..255"/>
      </type>
      <description>
        <text>Configures the offset of a vendor-class sub-option code as the user password.</text>
      </description>
    </leaf>
    <leaf name="vendor-class-sub-length">
      <when condition="../vendor-class-format='sub-option'"/>
      <type name="uint32">
        <range value="0..255"/>
      </type>
      <description>
        <text>Configures the field length of a vendor-class sub-option as the user password.</text>
      </description>
    </leaf>
    <leaf name="md5-encrypt">
      <when condition="../password-format='vendor-class' or ../password-format='option60'"/>
      <type name="boolean"/>
      <default value="false"/>
      <description>
        <text>Enable/disable the system uses the MD5 algorithm to encrypt the user password.</text>
      </description>
    </leaf>
    <leaf name="support-hex">
      <when condition="../password-format='vendor-class' or ../password-format='option60'"/>
      <type name="boolean"/>
      <default value="false"/>
      <description>
        <text>Enable/disable the user password supports Hex.</text>
      </description>
    </leaf>
  </grouping>
