          </container>
          <choice name="alarm-type">
            <default value="crc-error-threshold"/>
            <description>
              <text>CRC alarm config.</text>
            </description>
            <case name="crc-error-threshold">
              <description>
                <text>Configure set crc error threshold.</text>
              </description>
              <container name="crc-error-threshold">
                <description>
                  <text>Configure set crc error threshold.</text>
                </description>
                <leaf name="crc-error-high">
                  <ext:dynamic-default/>
                  <type name="uint32">
                    <range value="1..4294967295"/>
                  </type>
                  <must condition="(../crc-error-high &gt;= ../crc-error-low) and not(../crc-error-percent-alm-thr)"/>
                  <description>
                    <text>The high threshold of CRC error alarm threshold. The number of error packets counted in the time in interval exceeds this threshold will produce crc-error alarm.</text>
                  </description>
                </leaf>
                <leaf name="crc-error-low">
                  <ext:dynamic-default/>
                  <type name="uint32">
                    <range value="1..4294967295"/>
                  </type>
                  <must condition="(../crc-error-high &gt;= ../crc-error-low) and not(../crc-error-percent-alm-thr)"/>
                  <description>
                    <text>The low threshold of CRC error alarm threshold. The number of error packets counted in the time in interval less than this threshold will recover crc-error alarm.</text>
                  </description>
                </leaf>
                <leaf name="crc-error-interval">
                  <ext:dynamic-default/>
                  <type name="uint32">
                    <range value="1..65535"/>
                  </type>
                  <units name="s"/>
                  <description>
                    <text>Time interval for statistical crc-error alarm.</text>
                  </description>
                </leaf>
                <leaf name="crc-shutdown-enable">
                  <ext:dynamic-default/>
                  <type name="boolean"/>
                  <must condition="not(../crc-error-percent-alm-thr)"/>
                  <description>
                    <text>Enable/disable crc shutdown. The port will shutdown when the number of error packets counted in the time in interval exceeds threshold.</text>
                  </description>
                </leaf>
                <leaf name="crc-error-percent-alm-thr">
                  <ext:dynamic-default/>
                  <type name="uint32">
                    <range value="1..100"/>
                  </type>
                  <units name="%"/>
                  <must condition="not(../crc-error-high) and not(../crc-error-low) and not(../crc-error-interval) and not(../crc-shutdown-enable)"/>
                  <description>
                    <text>Crc-error alarm percent threshold, Higher than this threshold will produce crc-error alarm, lower than this threshold will recover crc-error alarm.</text>
                  </description>
                </leaf>
              </container>
            </case>
            <case name="link-crc">
              <description>
                <text>Enable/disable set link quality bit error ratio threshold and crc packet-error-ratio alarm threshold.</text>
              </description>
              <container name="link-quality">
                <must condition="(link-quality-alm-thr-coefficient &gt; link-quality-rsum-thr-coefficient and link-quality-alm-thr-power = link-quality-rsum-thr-power) or (link-quality-alm-thr-power &lt; link-quality-rsum-thr-power)"/>
                <presence value="Enable link quality threshold."/>
                <description>
                  <text>Enable/disable set link quality bit error ratio threshold.</text>
                </description>
                <leaf name="link-quality-alm-thr-coefficient">
                  <type name="uint32">
                    <range value="1..9"/>
                  </type>
                  <mandatory value="true"/>
                  <description>
                    <text>Link-quality alarm bit error ratio trigger coefficient.</text>
                  </description>
                </leaf>
                <leaf name="link-quality-alm-thr-power">
                  <type name="uint32">
                    <range value="1..7"/>
                  </type>
                  <mandatory value="true"/>
                  <description>
                    <text>Link-quality alarm bit error ratio trigger power.</text>
                  </description>
                </leaf>
                <leaf name="link-quality-rsum-thr-coefficient">
                  <type name="uint32">
                    <range value="1..9"/>
                  </type>
                  <mandatory value="true"/>
                  <description>
                    <text>Link-quality alarm bit error ratio recovery coefficient.</text>
                  </description>
                </leaf>
                <leaf name="link-quality-rsum-thr-power">
                  <type name="uint32">
                    <range value="1..7"/>
                  </type>
                  <mandatory value="true"/>
                  <description>
                    <text>Link-quality alarm bit error ratio recovery power.</text>
                  </description>
                </leaf>
              </container>
              <container name="crc-per-alarm">
                <must condition="((crc-per-alm-thr-coefficient &gt; crc-per-rsum-thr-coefficient and crc-per-alm-thr-power = crc-per-rsum-thr-power) or (crc-per-alm-thr-power &lt; crc-per-rsum-thr-power))"/>
                <must condition="((crc-per-alm-section-enable='false' and crc-per-alm-lsp-enable='true') or (crc-per-alm-section-enable='true' and crc-per-alm-lsp-enable='false') or (crc-per-alm-section-enable='false' and crc-per-alm-lsp-enable='false'))"/>
                <presence value="Enable crc alarm threshold."/>
                <description>
                  <text>Enable/disable set crc packet-error-ratio alarm threshold.</text>
                </description>
                <leaf name="crc-per-alm-thr-coefficient">
                  <type name="uint32">
                    <range value="1..9"/>
                  </type>
                  <mandatory value="true"/>
                  <description>
                    <text>Packet error ratio coefficient (x of xE-y).</text>
                  </description>
                </leaf>
                <leaf name="crc-per-alm-thr-power">
                  <type name="uint32">
                    <range value="1..5"/>
                  </type>
                  <mandatory value="true"/>
                  <description>
                    <text>Packet error ratio exponent (y of xE-y).</text>
                  </description>
                </leaf>
                <leaf name="crc-per-rsum-thr-coefficient">
                  <type name="uint32">
                    <range value="1..9"/>
                  </type>
                  <mandatory value="true"/>
                  <description>
                    <text>For resume-threshold: Packet error ratio coefficient (x of xE-y).</text>
                  </description>
                </leaf>
                <leaf name="crc-per-rsum-thr-power">
                  <type name="uint32">
                    <range value="1..6"/>
                  </type>
                  <mandatory value="true"/>
                  <description>
                    <text>For resume-threshold: Packet error ratio exponent (y of xE-y).</text>
                  </description>
                </leaf>
                <leaf name="crc-per-alm-lsp-enable">
                  <type name="boolean"/>
                  <mandatory value="true"/>
                  <description>
                    <text>Enable/disable configure PER (packet error ratio) of crc-error trigger LSP.</text>
                  </description>
                </leaf>
                <leaf name="crc-per-alm-section-enable">
                  <type name="boolean"/>
                  <mandatory value="true"/>
                  <description>
                    <text>Enable/disable configure PER (packet error ratio) of crc-error trigger section.</text>
                  </description>
                </leaf>
                <container name="crc-sample">
                  <must condition="crc-per-child-win-alarm-num &lt;= crc-per-child-win-max-num and crc-per-child-win-resume-num &lt;= crc-per-child-win-max-num"/>
                  <description>
                    <text>Configure crc error windows.</text>
                  </description>
                  <leaf name="crc-per-samp-win-factor">
                    <ext:dynamic-default/>
                    <type name="uint32">
                      <range value="10..100"/>
                    </type>
                    <description>
                      <text>Sample window factor.</text>
                    </description>
                  </leaf>
                  <leaf name="crc-per-child-win-max-num">
                    <ext:dynamic-default/>
                    <type name="uint32">
                      <range value="10..100"/>
                    </type>
                    <description>
                      <text>Maximum number of child windows.</text>
                    </description>
                  </leaf>
                  <leaf name="crc-per-child-win-alarm-num">
                    <ext:dynamic-default/>
                    <type name="uint32">
                      <range value="1..100"/>
                    </type>
                    <description>
                      <text>Minimal number of child windows for alarm generating.</text>
                    </description>
                  </leaf>
                  <leaf name="crc-per-child-win-resume-num">
                    <ext:dynamic-default/>
                    <type name="uint32">
                      <range value="1..100"/>
                    </type>
                    <description>
                      <text>Minimal number of child windows for alarm resuming.</text>
                    </description>
