  <container name="configuration">
    <description>
      <text>The branch of configuration model.</text>
    </description>
    <container name="p2mp-te">
      <description>
        <text>The configuration of P2MP TE.</text>
      </description>
      <leaf name="te-enable">
        <description>
          <text>The global switch of TE.</text>
        </description>
        <type name="enumeration">
          <enum name="disable">
            <value value="0"/>
          </enum>
          <enum name="enable">
            <value value="1"/>
          </enum>
        </type>
        <default value="disable"/>
      </leaf>
      <leaf name="mvpn-e2e-enable">
        <description>
          <text>The endtoend(E2E) protect global configuration of MVPN mtunnel.</text>
        </description>
        <type name="enumeration">
          <enum name="disable">
            <value value="0"/>
          </enum>
          <enum name="enable">
            <value value="1"/>
          </enum>
        </type>
        <default value="disable"/>
        <ncd:revise-date date="2017-02-21"/>
      </leaf>
      <leaf name="mvpn-frr-block">
        <description>
          <text>The fast-reroute(FRR) block global configuration of MVPN mtunnel.</text>
        </description>
        <type name="enumeration">
          <enum name="disable">
            <value value="0"/>
          </enum>
          <enum name="enable">
            <value value="1"/>
          </enum>
        </type>
        <default value="disable"/>
        <ncd:revise-date date="2019-03-27"/>
      </leaf>
      <container name="mvpn-bfd">
        <description>
          <text>The BFD global configuration of MVPN mtunnel.</text>
        </description>
        <presence value="bfd of mvpn tunnel"/>
        <leaf name="enable">
          <description>
            <text>To enable the bfd of MVPN tunnel when it's true.And to disable the bfd of MVPN tunnel when it's false.</text>
          </description>
          <type name="enumeration">
            <enum name="false">
              <value value="0"/>
            </enum>
            <enum name="true">
              <value value="1"/>
            </enum>
          </type>
          <mandatory value="true"/>
        </leaf>
        <choice name="detect-param">
          <description>
            <text>Detect param.</text>
          </description>
          <mandatory value="true"/>
          <case name="template">
            <description>
              <text>Bfd template name.</text>
            </description>
            <leaf name="template-name">
              <description>
                <text>The tunnel bfd template name.</text>
              </description>
              <type name="string">
                <length value="1..31"/>
              </type>
              <mandatory value="true"/>
            </leaf>
            <ncd:revise-date date="2020-04-18"/>
          </case>
          <case name="detect-params">
            <description>
              <text>Detect params.</text>
            </description>
            <leaf name="mintx-interval">
              <description>
                <text>The interval of sending BFD control packets.</text>
              </description>
              <type name="uint32">
                <range value="3..990"/>
              </type>
              <units name="ms"/>
              <mandatory value="true"/>
            </leaf>
            <leaf name="minrx-interval">
              <description>
                <text>The interval of reciving BFD control packets.</text>
              </description>
              <type name="uint32">
                <range value="3..990"/>
              </type>
              <units name="ms"/>
              <mandatory value="true"/>
            </leaf>
            <leaf name="multiplier">
              <description>
                <text>The multiplier of BFD detecting interval.</text>
              </description>
              <type name="uint16">
                <range value="3..50"/>
              </type>
              <mandatory value="true"/>
            </leaf>
            <ncd:revise-date date="2017-02-21"/>
          </case>
          <ncd:revise-date date="2017-02-21"/>
        </choice>
        <ncd:revise-date date="2017-02-21"/>
      </container>
      <container name="mte-tunnel">
        <description>
          <text>The configration of the P2MP TE tunnel.</text>
        </description>
        <list name="root-tunnel">
          <description>
            <text>The configration of root tunnel.</text>
          </description>
          <key value="tunnel-id tunnel-name"/>
          <leaf name="tunnel-id">
            <description>
              <text>The index of tunnel.</text>
            </description>
            <type name="uint32">
              <range value="1..65535"/>
            </type>
          </leaf>
          <leaf name="tunnel-name">
            <description>
              <text>Tunnel name.</text>
            </description>
            <type name="ncd:ifname">
              <length value="0..31"/>
            </type>
          </leaf>
          <leaf name="integrity">
            <description>
              <text>The integrity of tunnel.</text>
            </description>
            <type name="enumeration">
              <enum name="false">
                <value value="0"/>
              </enum>
              <enum name="true">
                <value value="1"/>
              </enum>
            </type>
            <default value="false"/>
          </leaf>
          <leaf name="record-route">
            <description>
              <text>The record-route of tunnel.</text>
            </description>
            <type name="enumeration">
              <enum name="false">
                <value value="0"/>
              </enum>
              <enum name="true">
                <value value="1"/>
              </enum>
            </type>
            <default value="false"/>
          </leaf>
          <leaf name="resv-style">
            <description>
              <text>The resv-style of tunnel.</text>
            </description>
            <type name="enumeration">
              <enum name="se">
                <value value="1"/>
              </enum>
              <enum name="ff">
                <value value="2"/>
              </enum>
            </type>
            <default value="se"/>
          </leaf>
          <leaf name="crankback">
            <description>
              <text>The crankback mode of tunnel.</text>
            </description>
            <type name="enumeration">
              <enum name="end-to-end">
                <value value="1"/>
                <description>
                  <text>end-to-end</text>
                </description>
              </enum>
              <enum name="disabled">
                <value value="0"/>
              </enum>
            </type>
            <default value="disabled"/>
          </leaf>
          <leaf name="reoptimize">
            <description>
              <text>The reoptimization of tunnel.</text>
            </description>
            <type name="enumeration">
              <enum name="false">
                <value value="0"/>
              </enum>
              <enum name="true">
                <value value="1"/>
              </enum>
            </type>
            <default value="false"/>
          </leaf>
          <leaf name="frr">
            <description>
              <text>The frr of tunnel.</text>
            </description>
            <type name="enumeration">
              <enum name="false">
                <value value="0"/>
              </enum>
              <enum name="true">
                <value value="1"/>
              </enum>
            </type>
            <default value="false"/>
          </leaf>
          <leaf name="escape-path">
            <description>
              <text>The escape-path of tunnel.</text>
            </description>
            <type name="enumeration">
              <enum name="disabled">
                <value value="0"/>
              </enum>
              <enum name="enabled">
                <value value="1"/>
              </enum>
            </type>
            <default value="disabled"/>
          </leaf>
          <leaf name="e2e-protect">
            <description>
              <text>The endtoend(e2e) protection of mtunnel.</text>
            </description>
            <type name="enumeration">
              <enum name="false">
                <value value="0"/>
              </enum>
              <enum name="true">
                <value value="1"/>
              </enum>
            </type>
            <default value="false"/>
            <ncd:revise-date date="2015-12-03"/>
          </leaf>
          <choice name="list-type">
            <description>
              <text>DestList Selection Type.</text>
            </description>
            <case name="name">
              <description>
                <text>Based on the name of the destlist selection.</text>
              </description>
              <leaf name="list-name">
                <description>
                  <text>The related destlist name.</text>
                </description>
                <type name="string">
                  <length value="1..16"/>
                </type>
              </leaf>
            </case>
            <case name="id">
              <description>
                <text>Id-based destlist selection.</text>
              </description>
              <leaf name="list-id">
                <description>
                  <text>The related destlist ID.</text>
                </description>
                <type name="uint16"/>
              </leaf>
            </case>
          </choice>
          <container name="priority">
            <description>
              <text>The priority of tunnel.</text>
            </description>
            <presence value="priority of this tunnel"/>
            <leaf name="setup-priority">
              <description>
                <text>The tunnel setup priority.This attribute can't be higher than the holding-priority.0 is the highest priority and 7 isthe lowest priority.</text>
              </description>
              <type name="uint8">
                <range value="0..7"/>
              </type>
              <mandatory value="true"/>
            </leaf>
            <leaf name="hold-priority">
              <description>
                <text>The tunnel holding priority.The tunnel of lower holding-priority will be preempted by the tunnel of higher setup-priority.0 is the highest priority and 7 is the lowest priority.</text>
              </description>
              <type name="uint8">
                <range value="0..7"/>
              </type>
              <mandatory value="true"/>
            </leaf>
          </container>
          <container name="bfd">
            <description>
              <text>The bfd of tunnel.</text>
            </description>
            <presence value="bfd of this tunnel"/>
            <leaf name="bfd">
              <description>
                <text>To enable the bfd of tunnel when it's true.And to disable the bfd of

tunnel when it's false.</text>
              </description>
              <type name="enumeration">
                <enum name="false">
                  <value value="0"/>
                </enum>
                <enum name="true">
                  <value value="1"/>
                </enum>
              </type>
              <mandatory value="true"/>
            </leaf>
            <choice name="detect-param">
              <description>
                <text>Detect param.</text>
              </description>
              <mandatory value="true"/>
              <case name="template">
                <description>
                  <text>Bfd template name.</text>
                </description>
                <leaf name="template-name">
                  <description>
                    <text>The tunnel bfd template name.</text>
                  </description>
                  <type name="string">
                    <length value="1..31"/>
                  </type>
                  <mandatory value="true"/>
                </leaf>
                <ncd:revise-date date="2020-04-18"/>
              </case>
              <case name="detect-params">
                <description>
                  <text>Detect params.</text>
                </description>
                <leaf name="mintx-interval">
                  <description>
                    <text>The interval of sending BFD control packets.</text>
                  </description>
                  <type name="uint32">
                    <range value="3..990"/>
                  </type>
                  <units name="ms"/>
                  <mandatory value="true"/>
                </leaf>
                <leaf name="minrx-interval">
                  <description>
                    <text>The interval of reciving BFD control packets.</text>
                  </description>
                  <type name="uint32">
                    <range value="3..990"/>
                  </type>
                  <units name="ms"/>
                  <mandatory value="true"/>
                </leaf>
                <leaf name="multiplier">
                  <description>
                    <text>The multiplier of BFD detecting interval.</text>
                  </description>
                  <type name="uint16">
                    <range value="3..50"/>
                  </type>
                  <mandatory value="true"/>
                </leaf>
                <ncd:revise-date date="2015-12-03"/>
              </case>
              <ncd:revise-date date="2015-12-03"/>
            </choice>
            <ncd:revise-date date="2015-12-03"/>
          </container>
          <leaf name="flow-statistics">
            <description>
              <text>The flow statistics of root tunnel.</text>
            </description>
            <type name="enumeration">
              <enum name="false">
                <value value="0"/>
              </enum>
              <enum name="true">
                <value value="1"/>
              </enum>
            </type>
            <default value="false"/>
            <ncd:revise-date date="2016-02-04"/>
          </leaf>
          <ncd:select fields="tunnel-id tunnel-name"/>
        </list>
        <list name="leaf-tunnel">
          <description>
            <text>The configration of leaf tunnel.</text>
          </description>
          <key value="tunnel-id tunnel-name"/>
          <leaf name="tunnel-id">
            <description>
              <text>The index of tunnel.</text>
            </description>
