  <feature name="sr-feature">
    <description>
      <text>Only for the use of 15K ,T8K and PC.</text>
    </description>
  </feature>
  <feature name="pc-feature">
    <description>
      <text>Router and switch PC feature.</text>
    </description>
  </feature>
  <feature name="aii-feature">
    <description>
      <text>Router and switch aii feature.</text>
    </description>
  </feature>
  <feature name="bier-feature">
    <description>
      <text>Router and switch bier feature.</text>
    </description>
  </feature>
  <typedef name="isis-level">
    <type name="enumeration">
      <enum name="level-1">
        <value value="1"/>
        <description>
          <text>Level-1 will be effected on.</text>
        </description>
      </enum>
      <enum name="level-2">
        <value value="2"/>
        <description>
          <text>Level-2 will be effected on.</text>
        </description>
      </enum>
      <enum name="level-1-2">
        <value value="3"/>
        <description>
          <text>Level-1-2 will be effected on.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Level(s) will be effected on.</text>
    </description>
  </typedef>
  <typedef name="isis-state">
    <type name="enumeration">
      <enum name="disable">
        <value value="0"/>
        <description>
          <text>The state is off.</text>
        </description>
      </enum>
      <enum name="enable">
        <value value="1"/>
        <description>
          <text>The state is on.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>The state of some isis attributes.</text>
    </description>
  </typedef>
  <container name="configuration">
    <description>
      <text>Configuration.</text>
    </description>
    <container name="route-isis">
      <description>
        <text>Route ISIS configuration.</text>
      </description>
      <container name="isis-instances">
        <description>
          <text>ISIS instance information.</text>
        </description>
        <list name="isis-instance">
          <description>
            <text>The information of ISIS instance.</text>
          </description>
          <key value="process-id"/>
          <leaf name="process-id">
            <description>
              <text>ISIS process ID</text>
            </description>
            <type name="uint32">
              <range value="0..65535"/>
            </type>
          </leaf>
          <leaf name="vrf-name">
            <description>
              <text>VPN routing/forwarding instance (VRF) name.</text>
            </description>
            <type name="string">
              <length value="1..32"/>
            </type>
          </leaf>
          <container name="system-ids">
            <description>
              <text>System id.</text>
            </description>
            <list name="system-id">
              <description>
                <text>System-ID.</text>
              </description>
              <max-elements value="1"/>
              <key value="isis-system-id"/>
              <leaf name="isis-system-id">
                <description>
                  <text>A system ID for an ISIS instance.</text>
                </description>
                <type name="ncd:mac-address"/>
              </leaf>
              <leaf name="range">
                <description>
                  <text>The range of system-ID.</text>
                </description>
                <type name="uint8">
                  <range value="0..32"/>
                </type>
                <default value="0"/>
              </leaf>
            </list>
          </container>
          <container name="authentications">
            <description>
              <text>Authentication information.</text>
            </description>
            <presence value="encrypted password"/>
            <list name="authentication">
              <description>
                <text>Authentication information.</text>
              </description>
              <key value="level"/>
              <leaf name="level">
                <description>
                  <text>Level(s) will be effected on.</text>
                </description>
                <type name="uint8">
                  <range value="0..2"/>
                </type>
              </leaf>
              <choice name="authen-word">
                <description>
                  <text>Password</text>
                </description>
                <case name="word">
                  <description>
                    <text>Word case.</text>
                  </description>
                  <leaf name="authen-pwd">
                    <description>
                      <text>The clear password.</text>
                    </description>
                    <type name="string">
                      <length value="1..100"/>
                    </type>
                    <mandatory value="true"/>
                  </leaf>
                </case>
                <case name="clear">
                  <description>
                    <text>Authentication clear password case.</text>
                  </description>
                  <leaf name="clear-pwd">
                    <description>
                      <text>Specify the clear password.</text>
                    </description>
                    <type name="string">
                      <length value="1..100"/>
                    </type>
                    <mandatory value="true"/>
                  </leaf>
                </case>
                <case name="encrypted-word">
                  <description>
                    <text>The label of encrypted password</text>
                  </description>
                  <leaf name="encrypted-pwd">
                    <description>
                      <text>Authentication encrypted password</text>
                    </description>
                    <type name="string">
                      <length value="24..192"/>
                    </type>
                    <mandatory value="true"/>
                  </leaf>
                </case>
              </choice>
            </list>
          </container>
          <container name="authentication-types">
            <description>
              <text>Authentication type.</text>
            </description>
            <presence value="authen type"/>
            <list name="authen-type">
              <description>
                <text>Authentication type</text>
              </description>
              <key value="level"/>
              <leaf name="level">
                <description>
                  <text>Level(s) would be effected on.</text>
                </description>
                <type name="uint8">
                  <range value="0..2"/>
                </type>
              </leaf>
              <leaf name="authen-type">
                <description>
                  <text>Authentication type.</text>
                </description>
                <type name="enumeration">
                  <enum name="md5">
                    <value value="1"/>
                  </enum>
                  <enum name="text">
                    <value value="2"/>
                  </enum>
                </type>
                <default value="text"/>
              </leaf>
            </list>
          </container>
          <container name="areas">
            <description>
              <text>Areas.</text>
            </description>
            <list name="area">
              <description>
                <text>Area address information.</text>
              </description>
              <max-elements value="3"/>
              <key value="area-addr"/>
              <leaf name="area-addr">
                <description>
                  <text>ISIS area address</text>
                </description>
                <type name="ncd:area-id"/>
              </leaf>
            </list>
          </container>
          <container name="isis-attr">
            <description>
              <text>ISIS attribution.</text>
            </description>
            <leaf name="isis-protocol-state">
              <description>
                <text>Flag of that system is enable.</text>
              </description>
              <type name="isis-state"/>
              <default value="enable"/>
            </leaf>
            <leaf name="is-type">
              <description>
                <text>Level(s) of the ISIS instance.</text>
              </description>
              <type name="isis-level"/>
              <default value="level-1-2"/>
            </leaf>
            <leaf name="i-spf">
              <description>
                <text>Flag of the i-spf is enable.</text>
              </description>
              <type name="boolean"/>
            </leaf>
            <leaf name="distance">
              <description>
                <text>IPv4 management distance.This leaf is out of use. The value of output is invalid.</text>
              </description>
              <type name="uint8">
                <range value="1..255"/>
              </type>
              <default value="115"/>
            </leaf>
            <leaf name="maximum-paths">
              <description>
                <text>Maximum of paths to one destination.</text>
              </description>
              <type name="uint32">
                <range value="1..128"/>
              </type>
              <default value="1"/>
            </leaf>
            <leaf name="lsp-size-originate">
              <description>
                <text>LSP MTU</text>
              </description>
              <type name="uint16">
                <range value="512..7680"/>
              </type>
              <units name="byte"/>
              <default value="1492"/>
            </leaf>
            <leaf name="lsp-size-recieve">
              <description>
                <text>The size of LSP can be recieved.</text>
              </description>
              <type name="uint16">
                <range value="512..7680"/>
              </type>
              <units name="byte"/>
              <default value="1492"/>
            </leaf>
            <leaf name="snp-authentication-state">
              <description>
                <text>Flag of the ISIS instance whether support SNP authentication.</text>
              </description>
              <type name="enumeration">
                <enum name="disable-snp-authentication">
                  <value value="0"/>
                  <description>
                    <text>Config disable-snp-authentication.</text>
                  </description>
                </enum>
                <enum name="enable-snp-authentication">
                  <value value="1"/>
                  <description>
                    <text>Config enable-snp-authentication.</text>
                  </description>
                </enum>
              </type>
              <default value="enable-snp-authentication"/>
            </leaf>
            <leaf name="ignore-lsp-errors">
              <description>
                <text>Flag of the system whether would ignore LSP checksum error.</text>
              </description>
              <type name="isis-state"/>
              <default value="enable"/>
            </leaf>
            <leaf name="lsp-refresh-time">
              <description>
                <text>LSP refresh interval.</text>
              </description>
              <type name="uint16">
                <range value="1..65535"/>
              </type>
              <units name="second"/>
              <default value="900"/>
            </leaf>
            <leaf name="max-lsp-lifetime">
              <description>
                <text>Max LSP life time.</text>
              </description>
              <type name="uint16">
                <range value="1..65535"/>
              </type>
              <units name="second"/>
              <default value="1200"/>
            </leaf>
            <leaf name="restart-state">
              <description>
                <text>Flag of the system whether enable GR.</text>
              </description>
              <type name="isis-state"/>
            </leaf>
            <leaf name="snmp-context">
              <description>
                <text>ISIS mib context name</text>
              </description>
              <type name="string">
                <length value="1..30"/>
              </type>
            </leaf>
            <leaf name="local-mt-state">
              <description>
                <text>Flag of that IGP-MT is enable.</text>
              </description>
              <type name="isis-state"/>
            </leaf>
            <leaf name="fast-reroute-type">
              <description>
                <text>FRR type</text>
              </description>
              <type name="enumeration">
                <enum name="down-stream-path">
                  <value value="1"/>
                </enum>
              </type>
            </leaf>
            <leaf name="hostname-dynamic">
              <description>
                <text>Dynamic hostname.</text>
              </description>
              <type name="isis-state"/>
              <default value="enable"/>
            </leaf>
            <leaf name="fast-flood-num">
              <description>
                <text>Number of the fast flooding LSPs.</text>
              </description>
              <type name="uint8">
                <range value="1..15"/>
              </type>
            </leaf>
            <leaf name="ipv6-frr-type">
              <description>
                <text>IPv6 fast-reroute type.</text>
              </description>
              <type name="enumeration">
                <enum name="down-stream-path">
                  <value value="1"/>
                  <description>
