    "definitions": {
        "rpki_schema": {
            "description": "Enable/disable RPKI globally.",
            "properties": {
                "region-validation": {
                    "description": "Configure region validation.",
                    "properties": {
                        "regions": {
                            "description": "List of RPKI regions.",
                            "properties": {
                                "region": {
                                    "description": "Configure region information.",
                                    "type": "array",
                                    "items": {
                                        "properties": {
                                            "id": {
                                                "type": "string",
                                                "description": "Region ID."
                                            },
                                            "description": {
                                                "type": "string",
                                                "description": "Description of a region, which is a string of letters and digits, spaces supported. By default, no description is configured for a peer."
                                            },
                                            "as": {
                                                "type": "array",
                                                "items": {
                                                    "type": "string",
                                                    "description": "Configure AS lists contained in the region."
                                                }
                                            }
                                        }
                                    },
                                    "x-key": "id"
                                }
                            }
                        },
                        "confederations": {
                            "description": "List of RPKI region confederations.",
                            "properties": {
