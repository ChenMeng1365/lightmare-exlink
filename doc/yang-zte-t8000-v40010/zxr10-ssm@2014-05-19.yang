                description 'The state of the current out clock is disabled.' ;
              }
              enum unsupported {
                value 3 ;
                description 'The state of the current out clock is unsupported.' ;
              }
            }
          }
          leaf out-mode {
            description 'The mode of the current out clock.' ;
            type enumeration {
              enum priority {
                value 1 ;
                description 'According to priority' ;
              }
              enum quality {
                value 2 ;
                description 'According to quality-level' ;
              }
            }
          }
          leaf sub-if-name {
            description 'The interface name of output clock source.' ;
            type string {
              length 0..31 ;
            }
          }
          leaf sub-clock-type {
            description 'The clock type of output clock source.' ;
            type enumeration {
              enum external-clk {
                value 1 ;
                description 'External clock.' ;
              }
              enum line-clk {
                value 2 ;
                description 'Line clock.' ;
              }
              enum inner-clk {
                value 3 ;
                description 'Internal clock.' ;
