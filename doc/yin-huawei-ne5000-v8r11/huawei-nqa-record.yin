  <grouping name="record-type">
    <description>
      <text>NQA test instance test result.</text>
    </description>
    <container name="results">
      <config value="false"/>
      <description>
        <text>List of NQA test instance test result record table.</text>
      </description>
      <list name="result">
        <key value="admin-name test-name running-index"/>
        <config value="false"/>
        <description>
          <text>Statistics of NQA test instance test result record table.</text>
        </description>
        <leaf name="admin-name">
          <type name="string">
            <length value="1..32"/>
          </type>
          <config value="false"/>
          <description>
            <text>Administrator name.</text>
          </description>
        </leaf>
        <leaf name="test-name">
          <type name="string">
            <length value="1..32"/>
          </type>
          <config value="false"/>
          <description>
            <text>Test instance name.</text>
          </description>
        </leaf>
        <leaf name="running-index">
          <type name="uint32">
            <range value="1..100000"/>
          </type>
          <config value="false"/>
          <description>
            <text>Running index.</text>
          </description>
        </leaf>
        <leaf name="status">
          <type name="test-finsh-status-type"/>
          <config value="false"/>
          <description>
            <text>Test finished status (finished or running).</text>
          </description>
        </leaf>
        <leaf name="completions">
          <type name="result-status-type"/>
          <config value="false"/>
          <description>
            <text>Running results.</text>
          </description>
        </leaf>
        <leaf name="sent-probes">
          <type name="uint32">
            <range value="0..65535"/>
          </type>
          <config value="false"/>
          <description>
            <text>Total number of sent packets.</text>
          </description>
        </leaf>
        <leaf name="response-probes">
          <type name="uint32">
            <range value="0..65535"/>
          </type>
          <config value="false"/>
          <description>
            <text>Total number of received packets.</text>
          </description>
        </leaf>
        <leaf name="rtd-thresholds">
          <type name="uint32">
            <range value="0..65535"/>
          </type>
          <config value="false"/>
          <description>
            <text>Counts of RTD over Threshold.</text>
          </description>
        </leaf>
        <leaf name="test-attempts">
          <type name="uint32">
            <range value="1..100000"/>
          </type>
          <config value="false"/>
          <description>
            <text>Attempts times (the value is 1).</text>
          </description>
        </leaf>
        <leaf name="drops">
          <type name="uint32">
            <range value="0..65535"/>
          </type>
          <config value="false"/>
          <description>
            <text>Number of packets lost.</text>
          </description>
        </leaf>
        <leaf name="disconnects">
          <type name="uint32">
            <range value="0..65535"/>
          </type>
          <config value="false"/>
          <description>
            <text>Number of consecutive failures.</text>
          </description>
        </leaf>
        <leaf name="timeouts">
          <type name="uint32">
            <range value="0..65535"/>
          </type>
          <units name="s"/>
          <config value="false"/>
          <description>
            <text>Timeout period.</text>
          </description>
        </leaf>
        <leaf name="busies">
          <type name="uint32">
            <range value="0..65535"/>
          </type>
          <config value="false"/>
          <description>
            <text>Number of times that system is busy.</text>
          </description>
        </leaf>
        <leaf name="no-connections">
          <type name="uint32">
            <range value="0..65535"/>
          </type>
          <config value="false"/>
          <description>
            <text>Number of connection errors.</text>
          </description>
        </leaf>
        <leaf name="sequence-errors">
          <type name="uint32">
            <range value="0..65535"/>
          </type>
          <config value="false"/>
          <description>
            <text>Number of packets index errors.</text>
          </description>
        </leaf>
        <leaf name="rtt-status-errors">
          <type name="uint32">
            <range value="0..65535"/>
          </type>
          <config value="false"/>
          <description>
            <text>Number of Rtt statistical failures.</text>
          </description>
        </leaf>
        <leaf name="dest-address">
          <type name="inet:ip-address-no-zone"/>
          <config value="false"/>
          <description>
            <text>Destination IP address.</text>
          </description>
        </leaf>
        <leaf name="rtt-min">
          <type name="uint32">
            <range value="0..65535"/>
          </type>
          <config value="false"/>
          <description>
            <text>Minimum RTT of all test packets, expressed in milliseconds.</text>
          </description>
        </leaf>
        <leaf name="rtt-average">
          <type name="uint32">
            <range value="0..65535"/>
          </type>
          <config value="false"/>
          <description>
            <text>Average RTT of all test packets, expressed in milliseconds.</text>
          </description>
        </leaf>
        <leaf name="rtt-max">
          <type name="uint32">
            <range value="0..65535"/>
          </type>
          <config value="false"/>
          <description>
            <text>Maximum RTT of all test packets, expressed in milliseconds.</text>
          </description>
        </leaf>
        <leaf name="rtt-sum">
          <type name="uint32">
            <range value="0..65535"/>
          </type>
          <config value="false"/>
          <description>
            <text>Sum of all test packets'RTT, expressed in milliseconds.</text>
          </description>
        </leaf>
        <leaf name="rtt-sum-square">
          <type name="uint64"/>
          <config value="false"/>
          <description>
            <text>Sum of RTT square.</text>
          </description>
        </leaf>
        <leaf name="packet-loss-ratio">
          <type name="uint8">
            <range value="0..100"/>
          </type>
          <units name="%"/>
          <config value="false"/>
          <description>
            <text>Packet loss ratio.</text>
          </description>
        </leaf>
        <leaf name="last-probe-time">
          <type name="yang:date-and-time"/>
          <config value="false"/>
          <description>
            <text> Last good probe time.</text>
          </description>
        </leaf>
      </list>
    </container>
    <container name="trace-results">
      <description>
        <text>List of NQA test instance test trace result record table.</text>
      </description>
      <list name="trace-result">
        <key value="admin-name test-name running-index"/>
        <description>
          <text>Statistics of NQA test instance test trace result record table.</text>
        </description>
        <leaf name="admin-name">
          <type name="string">
            <length value="1..32"/>
          </type>
          <description>
