  <typedef name="vrrp-type">
    <type name="enumeration">
      <enum name="normal">
        <value value="0"/>
        <description>
          <text>Normal VRRP.</text>
        </description>
      </enum>
      <enum name="admin">
        <value value="1"/>
        <description>
          <text>Admin VRRP.</text>
        </description>
      </enum>
      <enum name="member">
        <value value="2"/>
        <description>
          <text>Member VRRP.</text>
        </description>
      </enum>
      <enum name="lbrg">
        <value value="3"/>
        <description>
          <text>Load-balance redundancy group VRRP.</text>
        </description>
      </enum>
      <enum name="lbrg-mem">
        <value value="4"/>
        <description>
          <text>Load-balance redundancy group member VRRP.</text>
        </description>
      </enum>
      <enum name="mem-lbrg">
        <value value="5"/>
        <description>
          <text>Member of load-balance redundancy group VRRP.</text>
        </description>
      </enum>
      <enum name="mem-lbrg-mem">
        <value value="6"/>
        <description>
          <text>Member of load-balance redundancy group Member VRRP.</text>
        </description>
      </enum>
      <enum name="unicast">
        <value value="7"/>
        <description>
          <text>Unicast VRRP.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>VRRP group type.</text>
    </description>
  </typedef>
  <typedef name="vrrp-bfd-type">
    <type name="enumeration">
      <enum name="normal">
        <value value="0"/>
        <description>
          <text>Normal BFD session.</text>
        </description>
      </enum>
      <enum name="link">
        <value value="1"/>
        <description>
          <text>Link BFD session.</text>
        </description>
      </enum>
      <enum name="peer">
        <value value="2"/>
        <description>
          <text>Peer BFD session.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>The type of VRRP track BFD.</text>
    </description>
  </typedef>
  <typedef name="vrrp-track-mode">
    <type name="enumeration">
      <enum name="increase">
        <value value="0"/>
        <description>
          <text>Increase priority.</text>
        </description>
      </enum>
      <enum name="reduce">
        <value value="1"/>
        <description>
          <text>Reduce priority.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>VRRP track mode is like increase/reduce.</text>
    </description>
  </typedef>
  <typedef name="vrrp-track-bfd-state">
    <type name="enumeration">
      <enum name="down">
        <value value="0"/>
        <description>
          <text>BFD state down.</text>
        </description>
      </enum>
      <enum name="up">
        <value value="1"/>
        <description>
          <text>BFD state up.</text>
        </description>
      </enum>
      <enum name="admin-down">
        <value value="2"/>
        <description>
          <text>BFD state admindown.</text>
        </description>
      </enum>
      <enum name="unknow">
        <value value="3"/>
        <description>
          <text>BFD state unknow.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>VRRP track bfd state is like down/up/admin-down/unknow.</text>
    </description>
  </typedef>
  <typedef name="vrrp-oper-state">
    <type name="enumeration">
      <enum name="initialize">
        <value value="0"/>
        <description>
          <text>Initialize state.</text>
        </description>
      </enum>
      <enum name="backup">
        <value value="1"/>
        <description>
          <text>Backup state.</text>
        </description>
      </enum>
      <enum name="master">
        <value value="2"/>
        <description>
          <text>Master state.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>VRRP operation state is like initialize/backup/master.</text>
    </description>
  </typedef>
  <typedef name="vrrp6cksm-mode">
    <type name="enumeration">
      <enum name="include-pseudo-header">
        <value value="0"/>
        <description>
          <text>Include a pseudo header.</text>
        </description>
      </enum>
      <enum name="exclude-pseudo-header">
        <value value="1"/>
        <description>
          <text>Exclude a pseudo header.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>VRRP6 cksm-mode is like include-pseudo-header/exclude-pseudo-header.</text>
    </description>
  </typedef>
  <typedef name="vrrpauthen-mode">
    <type name="enumeration">
      <enum name="none">
        <value value="0"/>
        <description>
          <text>No authentication mode.</text>
        </description>
      </enum>
      <enum name="simple">
        <value value="1"/>
        <description>
          <text>Simple authentication mode.</text>
        </description>
      </enum>
      <enum name="md5">
        <value value="2"/>
        <description>
          <text>MD5 authentication mode.</text>
        </description>
      </enum>
      <enum name="hmac-sha256">
        <value value="3"/>
        <description>
          <text>SHA256 authentication mode.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>VRRP authen-mode is like none/simple/md5.</text>
    </description>
  </typedef>
  <typedef name="vrrp-v3-sendmode">
    <type name="enumeration">
      <enum name="v3-only">
        <value value="0"/>
        <description>
          <text>Only VRRPv3 advertisement packets are sent.</text>
        </description>
      </enum>
      <enum name="v2v3-both">
        <value value="1"/>
        <description>
          <text>Both VRRPv2 and VRRPv3 advertisement packets are sent.</text>
        </description>
      </enum>
      <enum name="v2-only">
        <value value="2"/>
        <description>
          <text>Only VRRPv2 advertisement packets are sent.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>VRRP V3 send mode is like v2-only/v2v3-both/v3-only.</text>
    </description>
  </typedef>
  <typedef name="vrrp-version">
    <type name="enumeration">
      <enum name="v2">
        <value value="0"/>
        <description>
          <text>VRRPv2 version.</text>
        </description>
      </enum>
      <enum name="v3">
        <value value="1"/>
        <description>
          <text>VRRPv3 version.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>VRRP version is like v2/v3.</text>
    </description>
  </typedef>
  <typedef name="vrrptrack-mode">
    <type name="enumeration">
      <enum name="increase">
        <value value="0"/>
        <description>
          <text>Increase priority.</text>
        </description>
      </enum>
      <enum name="reduce">
        <value value="1"/>
        <description>
          <text>Reduce priority.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>VRRP track mode is like increase/reduce.</text>
    </description>
  </typedef>
  <typedef name="mtgrptrack-mode">
    <type name="enumeration">
      <enum name="link">
        <value value="0"/>
        <description>
          <text>Link mode.</text>
        </description>
      </enum>
      <enum name="reduce">
        <value value="1"/>
        <description>
          <text>Mode in which the priority is reduced.</text>
        </description>
      </enum>
    </type>
    <description>
