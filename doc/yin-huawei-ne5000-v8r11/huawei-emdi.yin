  <rpc name="reset-statistic-history">
    <description>
      <text>Reset channel statistics.</text>
    </description>
    <ext:node-ref value="/emdi/emdi-history-statistic-records/emdi-history-statistic-record"/>
    <input>
      <leaf name="channel-name">
        <type name="string">
          <length value="1..31"/>
        </type>
        <description>
          <text>Channel name.</text>
        </description>
      </leaf>
    </input>
  </rpc>
  <rpc name="reset-outbound-history-statistic">
    <description>
      <text>Reset outbound channel statistics.</text>
    </description>
    <ext:node-ref value="/emdi/emdi-history-statistic-records/emdi-history-statistic-record"/>
    <input>
      <leaf name="slot-id">
        <type name="string">
          <length value="1..15"/>
        </type>
        <mandatory value="true"/>
        <description>
          <text>Slot ID.</text>
        </description>
      </leaf>
      <leaf name="channel-name">
        <type name="string">
          <length value="1..31"/>
        </type>
        <description>
          <text>Channel name.</text>
        </description>
      </leaf>
    </input>
  </rpc>
  <rpc name="filter-history-statistic">
    <description>
      <text>Filter EMDI history statistics.</text>
    </description>
    <ext:node-ref value="/emdi/emdi-history-statistic-records/emdi-history-statistic-record"/>
    <input>
      <leaf name="channel-name">
        <type name="string">
          <length value="1..31"/>
        </type>
        <description>
          <text>Channel name.</text>
        </description>
      </leaf>
      <leaf name="start-index">
        <type name="uint16">
          <range value="1..1000"/>
        </type>
        <description>
          <text>Start index. This parameter is used only as a filter criterion and is not displayed.</text>
        </description>
      </leaf>
      <leaf name="end-index">
        <type name="uint16">
          <range value="1..1000"/>
        </type>
        <description>
          <text>End index. It is only used as a filter criterion and is not displayed.</text>
        </description>
      </leaf>
      <leaf name="last-record-num">
        <type name="uint8">
          <range value="1..5"/>
        </type>
        <description>
          <text>Query the latest specified number of detection records. This parameter is used only as a filter criterion and is not displayed.</text>
        </description>
      </leaf>
      <leaf name="direction">
        <type name="enumeration">
          <enum name="inbound">
            <value value="0"/>
            <description>
              <text>Inbound.</text>
            </description>
          </enum>
          <enum name="outbound">
            <value value="1"/>
            <description>
              <text>Outbound.</text>
            </description>
          </enum>
        </type>
        <description>
          <text>Specify the direction of EMDI.</text>
        </description>
      </leaf>
      <leaf name="slot-id">
        <type name="string">
          <length value="1..15"/>
        </type>
        <description>
          <text>Slot ID.</text>
        </description>
      </leaf>
    </input>
    <output>
      <container name="emdi-history-statistic-records">
        <description>
          <text>List of configure query historical EMDI detection information.</text>
        </description>
        <list name="emdi-history-statistic-record">
          <key value="channel-name record-index direction"/>
          <description>
            <text>Configure query historical EMDI detection information.</text>
          </description>
          <leaf name="channel-name">
            <type name="string">
              <length value="1..31"/>
            </type>
            <description>
              <text>Channel name.</text>
            </description>
          </leaf>
          <leaf name="record-index">
            <type name="uint16">
              <range value="1..1000"/>
            </type>
            <description>
              <text>Index of each detection message.</text>
            </description>
          </leaf>
          <leaf name="direction">
            <type name="enumeration">
              <enum name="inbound">
                <value value="0"/>
                <description>
                  <text>Inbound.</text>
                </description>
              </enum>
              <enum name="outbound">
                <value value="1"/>
                <description>
                  <text>Outbound.</text>
                </description>
              </enum>
            </type>
            <description>
              <text>Specify the direction of EMDI.</text>
            </description>
          </leaf>
          <leaf name="record-time">
            <type name="string">
              <length value="1..19"/>
            </type>
            <description>
              <text>Time when each piece of information is detected.</text>
            </description>
          </leaf>
          <leaf name="monitor-period">
            <type name="enumeration">
              <enum name="second-10s">
                <value value="10"/>
                <description>
                  <text>Monitor period is 10 second.</text>
                </description>
              </enum>
              <enum name="second-30s">
                <value value="30"/>
                <description>
                  <text>Monitor period is 30 second.</text>
                </description>
              </enum>
              <enum name="second-60s">
                <value value="60"/>
                <description>
                  <text>Monitor period is 60 second.</text>
                </description>
              </enum>
            </type>
            <description>
              <text>Detection period of each detection message.</text>
            </description>
          </leaf>
          <leaf name="monitor-status">
            <type name="enumeration">
              <enum name="normal">
                <value value="0"/>
                <description>
                  <text>Status normal.</text>
                </description>
              </enum>
              <enum name="car-drop">
                <value value="1"/>
                <description>
                  <text>Status abnormal, NP drop.</text>
                </description>
              </enum>
              <enum name="rate-only">
                <value value="2"/>
                <description>
                  <text>Status abnormal, Forward Drop.</text>
                </description>
              </enum>
            </type>
            <description>
              <text>Detection status of each detection message.</text>
            </description>
          </leaf>
          <leaf name="receive-packet">
            <type name="uint64"/>
            <description>
              <text>Number of received packets.</text>
            </description>
          </leaf>
          <leaf name="rate-pps">
            <type name="uint64"/>
            <units name="pps"/>
            <description>
              <text>Rate (pps) at which packets are received.</text>
            </description>
          </leaf>
          <leaf name="rate-bps">
            <type name="uint64"/>
            <units name="bit/s"/>
            <description>
              <text>Rate (bit/s) at which packets are received.</text>
            </description>
          </leaf>
          <leaf name="lost-packet">
            <type name="uint64"/>
            <description>
              <text>Number of lost packets on a channel.</text>
            </description>
          </leaf>
          <leaf name="disorder-packet">
            <type name="uint64"/>
            <description>
              <text>Number of disordered channels.</text>
            </description>
          </leaf>
          <leaf name="rtp-lr">
            <type name="uint32"/>
            <units name="0.001 %"/>
            <description>
              <text>Packet loss rate of a channel.</text>
            </description>
          </leaf>
          <leaf name="rtp-ser">
            <type name="uint32"/>
            <units name="0.001 %"/>
            <description>
              <text>Check the channel disorder rate.</text>
            </description>
          </leaf>
          <leaf name="rtp-jitter">
            <type name="string">
              <length value="1..9"/>
            </type>
            <description>
              <text>Check the jitter of the channel.</text>
            </description>
          </leaf>
          <leaf name="slot-id">
            <type name="string">
              <length value="1..15"/>
            </type>
            <description>
              <text>Slot ID.</text>
            </description>
          </leaf>
          <leaf name="interface-name">
            <type name="string">
              <length value="1..63"/>
            </type>
            <description>
              <text>Interface name.</text>
            </description>
          </leaf>
        </list>
      </container>
    </output>
  </rpc>
</module>
