          <ncd:select fields="rmap-name"/>
          <ncd:select fields="rmap-name rseq-no"/>
        </list>
        <container name="trigger-delay">
          <description>
            <text>Configure route-map's trigger delay.</text>
          </description>
          <presence value="trigger-delay"/>
          <leaf name="delay-time">
            <description>
              <text>The delay time value (second) of trigger delay.</text>
            </description>
            <type name="uint32">
              <range value="0..30">
                <description>
                  <text>The range of delay time is from 0 to 30.</text>
                </description>
              </range>
            </type>
          </leaf>
          <ncd:revise-date date="2019-02-27"/>
        </container>
      </container>
    </container>
  </container>
