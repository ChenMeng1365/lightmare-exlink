    <description>
      <text>Static route track NQA.</text>
    </description>
    <case name="nqa">
      <description>
        <text>Track NQA.</text>
      </description>
      <leaf name="admin-name">
        <type name="leafref">
          <path value="/nqa:nqa/nqa:instances/nqa:instance/nqa:admin-name"/>
        </type>
        <must condition="../rt:bfd-enable='false' and ../rt:inherit-cost='false' and ../rt:permanent='false' and ../rt:topology-name='base' and ../test-name"/>
        <description>
          <text>Name of an NQA administer instance.</text>
        </description>
      </leaf>
      <leaf name="test-name">
        <type name="leafref">
          <path value="/nqa:nqa/nqa:instances/nqa:instance/nqa:test-name"/>
        </type>
        <must condition="../rt:bfd-enable='false' and ../rt:inherit-cost='false' and ../rt:permanent='false' and ../rt:topology-name='base' and ../admin-name"/>
        <description>
          <text>Name of an NQA test instance.</text>
        </description>
      </leaf>
    </case>
  </augment>
