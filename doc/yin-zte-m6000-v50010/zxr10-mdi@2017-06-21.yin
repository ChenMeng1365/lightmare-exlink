<?xml version="1.0" encoding="UTF-8"?>
<module name="zxr10-mdi"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:mdi="http://www.zte.com.cn/zxr10/netconf/schema/rosng/mdi"
        xmlns:ncd="http://www.zte.com.cn/zxr10/netconf/schema/rosng/netconf-defs">
  <namespace uri="http://www.zte.com.cn/zxr10/netconf/schema/rosng/mdi"/>
  <prefix value="mdi"/>
  <import module="zxr10-netconf-defs">
    <prefix value="ncd"/>
  </import>
  <organization>
    <text>ZTE Corporation</text>
  </organization>
  <contact>
    <text>ZTE Corporation  
   Nanjing Institute of ZTE Corporation 
   No.68 Zijinghua Rd. Yuhuatai District, Nanjing, China 
   Tel: +86-25-52870000</text>
  </contact>
  <description>
    <text>The netconf module of describing MDI.</text>
  </description>
  <revision date="2017-06-21">
    <description>
      <text>Modify the project label.</text>
    </description>
  </revision>
  <revision date="2017-03-12">
    <description>
      <text>Initial version.</text>
    </description>
  </revision>
  <container name="configuration">
    <description>
      <text>MDI module configuration info.</text>
    </description>
    <container name="mdi">
      <description>
        <text>MDI module configure root node.</text>
      </description>
      <container name="mdi-v4">
        <description>
          <text>IPv4 MDI module configure information.</text>
        </description>
        <container name="mdi-detections">
          <description>
            <text>Public mode of MDI module configure information.</text>
          </description>
          <list name="mdi-detection">
            <description>
              <text>Public mode of MDI module configure information.</text>
            </description>
            <key value="source group"/>
            <leaf name="source">
              <description>
                <text>Source address.</text>
              </description>
              <type name="ncd:ipv4-address"/>
            </leaf>
            <leaf name="group">
              <description>
                <text>Group address.</text>
              </description>
              <type name="ncd:ipv4-address"/>
            </leaf>
            <leaf name="session-id">
              <description>
                <text>Session identifier.</text>
              </description>
              <type name="uint32"/>
            </leaf>
            <leaf name="bit-rate-type">
              <description>
                <text>Bit rate type.</text>
              </description>
              <type name="enumeration">
                <enum name="vbr">
                  <value value="1"/>
                  <description>
                    <text>Variable bit rate.</text>
                  </description>
                </enum>
                <enum name="cbr">
                  <value value="2"/>
                  <description>
                    <text>Constant bit rate.</text>
                  </description>
                </enum>
              </type>
              <default value="vbr"/>
            </leaf>
            <leaf name="code-rate">
              <description>
                <text>Code rate(Kbps).</text>
              </description>
              <type name="uint32">
                <range value="1..1000000">
                  <description>
                    <text>Code rate(kbps).</text>
                  </description>
                </range>
              </type>
            </leaf>
            <leaf name="detection-mode">
              <description>
                <text>Detection mode.</text>
              </description>
              <type name="enumeration">
                <enum name="pre-activated">
                  <value value="1"/>
                  <description>
                    <text>Activation patterns in advance.</text>
                  </description>
                </enum>
                <enum name="as-required">
                  <value value="2"/>
                  <description>
                    <text>According to the need.</text>
                  </description>
                </enum>
              </type>
              <default value="pre-activated"/>
            </leaf>
            <leaf name="detection-duration">
              <description>
                <text>Detection duration(second).</text>
              </description>
              <type name="uint32">
                <range value="10..604800"/>
              </type>
              <default value="900"/>
            </leaf>
            <leaf name="cal-period">
              <description>
                <text>Calculate period(second).</text>
              </description>
              <type name="uint32">
                <range value="1"/>
              </type>
              <default value="1"/>
            </leaf>
            <leaf name="probe-period">
              <description>
                <text>Probe period(second).</text>
              </description>
              <type name="uint32">
                <range value="10..900"/>
              </type>
              <default value="60"/>
            </leaf>
          </list>
        </container>
        <container name="vrf-mdi-detections">
          <description>
            <text>Private mode of MDI module configure information.</text>
          </description>
          <list name="vrf-mdi-detection">
            <description>
              <text>Private mode of MDI module configure information.</text>
            </description>
            <key value="vrf-name source group"/>
            <leaf name="vrf-name">
              <description>
                <text>VPN Routing/Forwarding instance name.</text>
              </description>
              <type name="string">
                <length value="1..32"/>
              </type>
            </leaf>
            <leaf name="source">
              <description>
                <text>Source address.</text>
              </description>
              <type name="ncd:ipv4-address"/>
            </leaf>
            <leaf name="group">
              <description>
                <text>Group address.</text>
              </description>
              <type name="ncd:ipv4-address"/>
            </leaf>
            <leaf name="session-id">
              <description>
                <text>Session identifier.</text>
              </description>
              <type name="uint32"/>
            </leaf>
            <leaf name="bit-rate-type">
              <description>
                <text>Bit rate type.</text>
              </description>
              <type name="enumeration">
                <enum name="vbr">
                  <value value="1"/>
                  <description>
                    <text>Variable bit rate.</text>
                  </description>
                </enum>
                <enum name="cbr">
                  <value value="2"/>
                  <description>
                    <text>Constant bit rate.</text>
                  </description>
                </enum>
              </type>
              <default value="vbr"/>
            </leaf>
            <leaf name="code-rate">
              <description>
                <text>Code rate(Kbps).</text>
              </description>
              <type name="uint32">
                <range value="1..1000000">
                  <description>
                    <text>Code rate(kbps).</text>
                  </description>
                </range>
              </type>
            </leaf>
            <leaf name="detection-mode">
              <description>
                <text>Detection mode.</text>
              </description>
              <type name="enumeration">
                <enum name="pre-activated">
                  <value value="1"/>
                  <description>
                    <text>Activation patterns in advance.</text>
                  </description>
                </enum>
                <enum name="as-required">
                  <value value="2"/>
                  <description>
                    <text>According to the need.</text>
                  </description>
                </enum>
              </type>
              <default value="pre-activated"/>
            </leaf>
            <leaf name="detection-duration">
              <description>
                <text>Detection duration(second).</text>
              </description>
              <type name="uint32">
                <range value="10..604800"/>
              </type>
              <default value="900"/>
            </leaf>
            <leaf name="cal-period">
              <description>
                <text>Calculate period(second).</text>
              </description>
              <type name="uint32">
                <range value="1"/>
              </type>
              <default value="1"/>
            </leaf>
            <leaf name="probe-period">
              <description>
                <text>Probe period(second).</text>
              </description>
              <type name="uint32">
                <range value="10..900"/>
              </type>
              <default value="60"/>
            </leaf>
          </list>
        </container>
      </container>
    </container>
  </container>
  <container name="state">
    <description>
      <text>MDI module state information.</text>
    </description>
    <config value="false"/>
    <container name="mdi">
      <description>
        <text>MDI module state information.</text>
      </description>
      <container name="mdi-v4">
        <description>
          <text>IPv4 MDI module state information.</text>
        </description>
        <container name="mdi-detections">
          <description>
            <text>Public mode of MDI detection information.</text>
          </description>
          <list name="mdi-detection">
            <description>
              <text>Public mode of MDI detection information.</text>
            </description>
            <key value="source group"/>
            <ncd:select fields="source group"/>
            <leaf name="source">
              <description>
                <text>Source address.</text>
              </description>
              <type name="ncd:ipv4-address"/>
            </leaf>
            <leaf name="group">
              <description>
                <text>Group address.</text>
              </description>
              <type name="ncd:ipv4-address"/>
            </leaf>
            <leaf name="session-id">
              <description>
                <text>Session identifier.</text>
              </description>
              <type name="uint32">
                <range value="0..4294967295"/>
              </type>
            </leaf>
            <container name="stats-infos">
              <description>
                <text>Statistics info.</text>
              </description>
              <list name="stats-info">
                <description>
                  <text>Statistics info.</text>
                </description>
                <key value="index"/>
                <leaf name="index">
                  <description>
                    <text>Sequence index.</text>
                  </description>
                  <type name="uint32">
                    <range value="1..4294967295"/>
                  </type>
                </leaf>
                <leaf name="mlt-15m">
                  <description>
                    <text>The media loss counts in the last fifteen minutes.</text>
                  </description>
                  <type name="string">
                    <length value="0..23"/>
                  </type>
                </leaf>
                <leaf name="mlt-24h">
                  <description>
                    <text>The media loss counts in the last twenty-four hours.</text>
                  </description>
                  <type name="string">
                    <length value="0..23"/>
                  </type>
                </leaf>
                <leaf name="df-max">
                  <description>
                    <text>The maximum delay factors(ms).</text>
                  </description>
                  <type name="string">
                    <length value="0..23"/>
                  </type>
                </leaf>
                <leaf name="df-min">
                  <description>
                    <text>The minimum delay factors(ms).</text>
                  </description>
                  <type name="string">
                    <length value="0..23"/>
                  </type>
                </leaf>
                <leaf name="df-mean">
                  <description>
                    <text>The average delay factors(ms).</text>
                  </description>
                  <type name="string">
                    <length value="0..23"/>
                  </type>
                </leaf>
                <leaf name="mlr-max">
                  <description>
                    <text>The maximum media loss rate.</text>
                  </description>
                  <type name="string">
                    <length value="0..23"/>
                  </type>
                </leaf>
                <leaf name="mlr-min">
                  <description>
                    <text>The minimum media loss rate.</text>
                  </description>
                  <type name="string">
                    <length value="0..23"/>
                  </type>
                </leaf>
                <leaf name="mlr-mean">
                  <description>
                    <text>The average media loss rate.</text>
                  </description>
                  <type name="string">
                    <length value="0..23"/>
                  </type>
                </leaf>
              </list>
            </container>
          </list>
        </container>
        <container name="vrf-mdi-detections">
          <description>
            <text>Private mode of MDI detection information.</text>
          </description>
          <list name="vrf-mdi-detection">
            <description>
              <text>Private mode of MDI detection information.</text>
            </description>
            <key value="vrf-name source group"/>
            <ncd:select fields="vrf-name"/>
            <ncd:select fields="vrf-name source group"/>
            <leaf name="vrf-name">
              <description>
                <text>VPN Routing/Forwarding instance name.</text>
              </description>
              <type name="string">
                <length value="1..32"/>
              </type>
            </leaf>
            <leaf name="source">
              <description>
                <text>Source address.</text>
              </description>
              <type name="ncd:ipv4-address"/>
            </leaf>
            <leaf name="group">
              <description>
                <text>Group address.</text>
              </description>
              <type name="ncd:ipv4-address"/>
            </leaf>
            <leaf name="session-id">
              <description>
                <text>Session identifier.</text>
              </description>
              <type name="uint32">
                <range value="0..4294967295"/>
              </type>
            </leaf>
            <container name="stats-infos">
              <description>
                <text>Statistics info.</text>
              </description>
              <list name="stats-info">
                <description>
                  <text>Statistics info.</text>
                </description>
                <key value="index"/>
                <leaf name="index">
                  <description>
                    <text>Sequence index.</text>
                  </description>
                  <type name="uint32">
                    <range value="1..4294967295"/>
                  </type>
                </leaf>
                <leaf name="mlt-15m">
                  <description>
                    <text>The media loss counts in the last fifteen minutes.</text>
                  </description>
                  <type name="string">
                    <length value="0..23"/>
                  </type>
                </leaf>
                <leaf name="mlt-24h">
                  <description>
                    <text>The media loss counts in the last twenty-four hours.</text>
                  </description>
                  <type name="string">
                    <length value="0..23"/>
                  </type>
                </leaf>
                <leaf name="df-max">
                  <description>
                    <text>The maximum delay factors(ms).</text>
                  </description>
                  <type name="string">
                    <length value="0..23"/>
                  </type>
                </leaf>
                <leaf name="df-min">
                  <description>
                    <text>The minimum delay factors(ms).</text>
                  </description>
                  <type name="string">
                    <length value="0..23"/>
                  </type>
                </leaf>
                <leaf name="df-mean">
                  <description>
                    <text>The average delay factors(ms).</text>
                  </description>
                  <type name="string">
                    <length value="0..23"/>
                  </type>
                </leaf>
                <leaf name="mlr-max">
                  <description>
                    <text>The maximum media loss rate.</text>
                  </description>
                  <type name="string">
                    <length value="0..23"/>
                  </type>
                </leaf>
                <leaf name="mlr-min">
                  <description>
                    <text>The minimum media loss rate.</text>
                  </description>
                  <type name="string">
                    <length value="0..23"/>
                  </type>
                </leaf>
                <leaf name="mlr-mean">
                  <description>
                    <text>The average media loss rate.</text>
                  </description>
                  <type name="string">
                    <length value="0..23"/>
                  </type>
                </leaf>
              </list>
            </container>
          </list>
        </container>
      </container>
    </container>
  </container>
</module>
