                <description>
                  <text>mpls   MPLS tunnel;
vxlan  VXLAN tunnel;</text>
                </description>
                <type name="enumeration">
                  <enum name="mpls">
                    <value value="1"/>
                  </enum>
                  <enum name="vxlan">
                    <value value="2"/>
                  </enum>
                </type>
                <mandatory value="true"/>
              </leaf>
              <ncd:revise-date date="2019-05-08"/>
            </container>
            <container name="equivalent-evi-label">
              <description>
                <text>Set equivalent EVI label.</text>
              </description>
              <presence value="equivalent-evi-label"/>
              <leaf name="equivalent-evi-label">
