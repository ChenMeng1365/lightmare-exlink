<?xml version="1.0" encoding="UTF-8"?>
<module name="huawei-software"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:software="urn:huawei:yang:huawei-software"
        xmlns:ext="urn:huawei:yang:huawei-extension"
        xmlns:yang="urn:ietf:params:xml:ns:yang:ietf-yang-types">
  <namespace uri="urn:huawei:yang:huawei-software"/>
  <prefix value="software"/>
  <import module="huawei-extension">
    <prefix value="ext"/>
  </import>
  <import module="ietf-yang-types">
    <prefix value="yang"/>
  </import>
  <organization>
    <text>Huawei Technologies Co., Ltd.</text>
  </organization>
  <contact>
    <text>Huawei Industrial Base
Bantian, Longgang
Shenzhen 518129
People's Republic of China
Website: http://www.huawei.com
Email: support@huawei.com</text>
  </contact>
  <description>
    <text>Specify software package upgrade management.</text>
  </description>
  <revision date="2020-06-30">
    <description>
      <text>Revert modification of software node's config attribute.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <revision date="2020-06-29">
    <description>
      <text>Modify node description.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <revision date="2020-05-27">
    <description>
      <text>Add rpc refresh-startup-secure-version-configuration.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <revision date="2020-03-02">
    <description>
      <text>Add install-feature-software, uninstall-feature-software, upgrade-feature-software.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <revision date="2019-12-02">
    <description>
      <text>Initial revision.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <ext:task-name value="upgrade"/>
  <typedef name="startup-mode">
    <type name="enumeration">
      <enum name="primary">
        <value value="1"/>
        <description>
          <text>Upgrade the software package on the master MPU.</text>
        </description>
      </enum>
      <enum name="slave">
        <value value="2"/>
        <description>
          <text>Upgrade the software package on the slave MPU.</text>
        </description>
      </enum>
      <enum name="all">
        <value value="3"/>
        <description>
          <text>Upgrade the software package on all MPUs.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Startup mode.</text>
    </description>
  </typedef>
  <typedef name="package-type">
    <type name="enumeration">
      <enum name="system">
        <value value="1"/>
        <description>
          <text>The type of a software package on the device is a system software package.</text>
        </description>
      </enum>
      <enum name="feature">
        <value value="2"/>
        <description>
          <text>The type of a software package on the device is a feature software package.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Specify the type of a software package on the device.</text>
    </description>
  </typedef>
  <typedef name="checkpoint-create-type">
    <type name="enumeration">
      <enum name="manual">
        <value value="1"/>
        <description>
          <text>The type of a checkpoint created mode.</text>
        </description>
      </enum>
      <enum name="auto">
        <value value="2"/>
        <description>
          <text>The type of a checkpoint created mode.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Specify the type of a software package on the device.</text>
    </description>
  </typedef>
  <container name="software">
    <description>
      <text>Specify software package upgrade management.</text>
    </description>
    <container name="global">
      <description>
        <text>Configure of auto save checkpoint switch.</text>
      </description>
      <container name="auto-save">
        <description>
          <text>Configure of enable auto save checkpoint.</text>
        </description>
        <leaf name="enable">
          <type name="boolean"/>
          <default value="true"/>
          <description>
            <text>Enable/disable auto save checkpoint when upgrade software package.</text>
          </description>
        </leaf>
      </container>
    </container>
    <container name="versions">
      <config value="false"/>
      <status value="deprecated"/>
      <description>
        <text>List of operational state of software package versions. The node versions is deprecated. You are advised to use the node /system/system-info.</text>
      </description>
      <list name="version">
        <key value="base"/>
        <status value="deprecated"/>
        <description>
          <text>Operational state of a software package version. The node version is deprecated. You are advised to use the node /system/system-info.</text>
        </description>
        <leaf name="base">
          <type name="string">
            <length value="1..63"/>
          </type>
          <status value="deprecated"/>
          <description>
            <text>Current software package version. The node base is deprecated. You are advised to use the node /system/system-info/product-version.</text>
          </description>
        </leaf>
        <leaf name="patch">
          <type name="string">
            <length value="1..63"/>
          </type>
          <status value="deprecated"/>
          <description>
            <text>Current patch package version. The node patch is deprecated. You are advised to use the node /system/system-info/patch-version.</text>
          </description>
        </leaf>
      </list>
    </container>
    <container name="startup-packages">
      <config value="false"/>
      <description>
        <text>List of operational state of startup software packages.</text>
      </description>
      <list name="startup-package">
        <key value="slot-id"/>
        <description>
          <text>Operational state of startup software package.</text>
        </description>
        <leaf name="slot-id">
          <type name="string">
            <length value="1..31"/>
          </type>
          <description>
            <text>Slot ID.</text>
          </description>
        </leaf>
        <leaf name="current-package">
          <type name="string">
            <length value="4..255"/>
          </type>
          <description>
            <text>The name of a software package used at the current startup.</text>
          </description>
        </leaf>
        <leaf name="next-package">
          <type name="string">
            <length value="4..255"/>
          </type>
          <description>
            <text>The name of a software package used at the next startup.</text>
          </description>
        </leaf>
      </list>
    </container>
    <container name="packages">
      <config value="false"/>
      <description>
        <text>List of operational state of software packages on the device.</text>
      </description>
      <list name="package">
        <key value="name"/>
        <description>
          <text>Operational state of a software package on the device.</text>
        </description>
        <leaf name="name">
          <type name="string">
            <length value="4..255"/>
          </type>
          <description>
            <text>The name of a software package on the device. There may exist user-created software packages with names longer than 127 on the device, however, such packages cannot be used for the next startup.</text>
          </description>
        </leaf>
        <leaf name="package-type">
          <type name="package-type"/>
          <description>
            <text>The type of a software package on the device.</text>
          </description>
        </leaf>
      </list>
    </container>
    <container name="upgrade-rollback-timer">
      <config value="false"/>
      <description>
        <text>Operational data of upgrade rollback timer.</text>
      </description>
      <leaf name="timer-enable">
        <type name="boolean"/>
        <description>
          <text>When the rollback timer is closed, the time leaf will not be shown.</text>
        </description>
      </leaf>
      <leaf name="time">
        <when condition="../timer-enable = 'true'"/>
        <type name="uint32">
          <range value="10..360"/>
        </type>
        <units name="min"/>
        <description>
          <text>Rollback time.</text>
        </description>
      </leaf>
    </container>
    <container name="checkpoint-infos">
      <config value="false"/>
      <description>
        <text>List of checkpoint base information.</text>
      </description>
      <list name="checkpoint-info">
        <key value="name"/>
        <description>
          <text>Statistics of a checkpoint info.</text>
        </description>
        <leaf name="name">
          <type name="string">
            <length value="5..32"/>
          </type>
          <description>
            <text>Checkpoint name.</text>
          </description>
        </leaf>
        <leaf name="create-time">
          <type name="yang:date-and-time"/>
          <description>
            <text>Checkpoint create time.</text>
          </description>
        </leaf>
        <leaf name="create-mode">
          <type name="checkpoint-create-type"/>
          <description>
            <text>Checkpoint create mode.</text>
          </description>
        </leaf>
        <leaf name="base-package">
          <type name="string">
            <length value="0..255"/>
          </type>
          <description>
            <text>Checkpoint software package.</text>
          </description>
        </leaf>
        <leaf name="feature-package">
          <type name="string">
            <length value="0..2047"/>
          </type>
          <description>
            <text>Checkpoint feature packages.</text>
          </description>
        </leaf>
        <leaf name="patch-package">
          <type name="string">
            <length value="0..2047"/>
          </type>
          <description>
            <text>Checkpoint patch packages.</text>
          </description>
        </leaf>
        <leaf name="module-package">
          <type name="string">
            <length value="0..2047"/>
          </type>
          <description>
            <text>Checkpoint mod patchs.</text>
          </description>
        </leaf>
      </list>
    </container>
  </container>
  <rpc name="startup-by-mode">
    <description>
      <text>Specify the mode-based upgrade for software packages.</text>
    </description>
    <ext:node-ref value="/software/packages/package"/>
    <input>
      <leaf name="name">
        <type name="string">
          <length value="4..127"/>
        </type>
        <mandatory value="true"/>
        <description>
          <text>Specify the name of a software package that needs to be upgraded.</text>
        </description>
      </leaf>
      <leaf name="mode">
        <type name="startup-mode"/>
        <description>
          <text>Specify a software package upgrade mode.</text>
        </description>
      </leaf>
    </input>
  </rpc>
  <rpc name="startup-by-slot-id">
    <description>
      <text>Specify the slot ID-based upgrade for software packages.</text>
    </description>
    <ext:node-ref value="/software/packages/package"/>
    <input>
      <leaf name="name">
        <type name="string">
          <length value="4..127"/>
        </type>
        <mandatory value="true"/>
        <description>
          <text>Specify the name of a software package that needs to be upgraded.</text>
        </description>
      </leaf>
      <leaf name="slot-id">
        <type name="string">
          <length value="1..31"/>
        </type>
        <description>
          <text>Specify the slot ID of a board on which a software package needs to be upgraded.</text>
        </description>
      </leaf>
    </input>
  </rpc>
  <rpc name="enable-upgrade-rollback">
    <description>
      <text>Specify the rollback time when upgrade software package.</text>
    </description>
    <input>
      <leaf name="rollback-time">
        <type name="uint32">
          <range value="10..360"/>
        </type>
        <units name="min"/>
        <mandatory value="true"/>
        <description>
          <text>Specify the rollback time.</text>
        </description>
      </leaf>
    </input>
  </rpc>
  <rpc name="undo-upgrade-rollback">
    <ext:node-ref value="/software/upgrade-rollback-timer"/>
    <description>
      <text>Delete the rollback time when upgrade software package.</text>
    </description>
  </rpc>
  <rpc name="install-feature-software">
    <description>
      <text>Install feature software.</text>
    </description>
    <input>
      <leaf name="name">
        <type name="string">
          <length value="4..127"/>
        </type>
        <mandatory value="true"/>
        <description>
          <text>Specify the name of a feature software package.</text>
        </description>
      </leaf>
    </input>
  </rpc>
  <rpc name="uninstall-feature-software">
    <description>
      <text>Uninstall feature software.</text>
    </description>
    <input>
      <leaf name="name">
        <type name="string">
          <length value="4..127"/>
        </type>
        <mandatory value="true"/>
        <description>
          <text>Specify the name of a feature software package.</text>
        </description>
      </leaf>
    </input>
  </rpc>
  <rpc name="upgrade-feature-software">
    <description>
      <text>Upgrade feature software on line.</text>
    </description>
    <input>
      <leaf name="name">
        <type name="string">
          <length value="4..127"/>
        </type>
        <mandatory value="true"/>
        <description>
          <text>Specify the name of a feature software package.</text>
        </description>
      </leaf>
    </input>
  </rpc>
  <rpc name="startup-feature-software">
    <description>
      <text>Startup feature software.</text>
    </description>
    <input>
      <leaf name="feature-package-name">
        <type name="string">
          <length value="4..127"/>
        </type>
        <mandatory value="true"/>
        <description>
          <text>Specify the name of a feature software package.</text>
        </description>
      </leaf>
    </input>
  </rpc>
  <rpc name="create-startup-checkpoint">
    <ext:node-ref value="/software/checkpoint-infos/checkpoint-info"/>
    <description>
      <text>Create a specified startup checkpoint.</text>
    </description>
    <input>
      <leaf name="name">
        <type name="leafref">
          <path value="/software/checkpoint-infos/checkpoint-info/name"/>
        </type>
        <mandatory value="true"/>
        <description>
          <text>Specify the name of a checkpoint.</text>
        </description>
      </leaf>
    </input>
  </rpc>
  <rpc name="delete-startup-checkpoint">
    <ext:node-ref value="/software/checkpoint-infos/checkpoint-info"/>
    <description>
      <text>Delete a specified startup checkpoint.</text>
    </description>
    <input>
      <leaf name="name">
        <type name="leafref">
          <path value="/software/checkpoint-infos/checkpoint-info/name"/>
        </type>
        <mandatory value="true"/>
        <description>
          <text>Specify the name of a checkpoint.</text>
        </description>
      </leaf>
    </input>
  </rpc>
  <rpc name="check-startup-checkpoint">
    <ext:node-ref value="/software/checkpoint-infos/checkpoint-info"/>
    <description>
      <text>Check a specified startup checkpoint.</text>
    </description>
    <input>
      <leaf name="name">
        <type name="leafref">
          <path value="/software/checkpoint-infos/checkpoint-info/name"/>
        </type>
        <mandatory value="true"/>
        <description>
          <text>Specify the name of a checkpoint.</text>
        </description>
      </leaf>
    </input>
  </rpc>
  <rpc name="restore-startup-checkpoint">
    <ext:node-ref value="/software/checkpoint-infos/checkpoint-info"/>
    <description>
      <text>Restore a specified checkpoint.</text>
    </description>
    <input>
      <leaf name="name">
        <type name="leafref">
          <path value="/software/checkpoint-infos/checkpoint-info/name"/>
        </type>
        <mandatory value="true"/>
        <description>
          <text>Specify the name of a checkpoint.</text>
        </description>
      </leaf>
    </input>
  </rpc>
  <rpc name="refresh-startup-secure-version-configuration">
    <description>
      <text>Update the eFUSE and version revocation list. If the slot parameter is not specified, information on all boards is updated.</text>
    </description>
    <input>
      <leaf name="slot">
        <type name="string">
          <length value="1..49"/>
        </type>
        <description>
          <text>Specify the slot name.</text>
        </description>
      </leaf>
    </input>
  </rpc>
</module>
