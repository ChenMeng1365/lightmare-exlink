<?xml version="1.0" encoding="UTF-8"?>
<module name="huawei-vcchassis"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:vcchassis="urn:huawei:yang:huawei-vcchassis"
        xmlns:ext="urn:huawei:yang:huawei-extension">
  <yang-version value="1"/>
  <namespace uri="urn:huawei:yang:huawei-vcchassis"/>
  <prefix value="vcchassis"/>
  <import module="huawei-extension">
    <prefix value="ext"/>
  </import>
  <organization>
    <text>Huawei Technologies Co., Ltd.</text>
  </organization>
  <contact>
    <text>Huawei Industrial Base
Bantian, Longgang
Shenzhen 518129
People's Republic of China
Website: http://www.huawei.com
Email: support@huawei.com</text>
  </contact>
  <description>
    <text>Virtual cluter chassis.</text>
  </description>
  <revision date="2019-04-01">
    <description>
      <text>Init revision.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <ext:task-name value="device-mgr"/>
  <typedef name="vcluster-role-type">
    <type name="enumeration">
      <enum name="master">
        <value value="0"/>
        <description>
          <text>Master chassis.</text>
        </description>
      </enum>
      <enum name="slave">
        <value value="1"/>
        <description>
          <text>Slave chassis.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Information of enum type vcluster-role-type.</text>
    </description>
  </typedef>
  <typedef name="vcluster-topo-type">
    <type name="enumeration">
      <enum name="line">
        <value value="0"/>
        <description>
          <text>Line topo.</text>
        </description>
      </enum>
      <enum name="ring">
        <value value="1"/>
        <description>
          <text>Ring topo.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Information of enum type vcluster-topo-type.</text>
    </description>
  </typedef>
  <container name="vcchassis">
    <description>
      <text>Statistics of virtual cluter chassis.</text>
    </description>
    <container name="global-attribute">
      <config value="false"/>
      <description>
        <text>Statistics of global attribute.</text>
      </description>
      <leaf name="cluster-id">
        <type name="uint32">
          <range value="1..255"/>
        </type>
        <description>
          <text>Cluster ID.</text>
        </description>
      </leaf>
      <leaf name="cluster-topo">
        <type name="vcluster-topo-type"/>
        <description>
          <text>Cluster topo.</text>
        </description>
      </leaf>
      <leaf name="cluster-member-number">
        <type name="uint32">
          <range value="1..2"/>
        </type>
        <description>
          <text>Frame number of cluster.</text>
        </description>
      </leaf>
      <leaf name="cluster-manage-sysoid">
        <type name="string">
          <length value="1..50"/>
        </type>
        <description>
          <text>Virtual cluster manage systerm ID.</text>
        </description>
      </leaf>
    </container>
    <container name="vcchassis-members">
      <config value="false"/>
      <description>
        <text>List of virtual cluter members.</text>
      </description>
      <list name="vcchassis-member">
        <key value="chassis-id"/>
        <max-elements value="2"/>
        <description>
          <text>Statistics of virtual cluter member.</text>
        </description>
        <leaf name="chassis-id">
          <type name="uint32">
            <range value="1..2"/>
          </type>
          <description>
            <text>Chassis ID.</text>
          </description>
        </leaf>
        <leaf name="chassis-role">
          <type name="vcluster-role-type"/>
          <description>
            <text>Member chassis role.</text>
          </description>
        </leaf>
        <leaf name="chassis-sysoid">
          <type name="string">
            <length value="1..50"/>
          </type>
          <description>
            <text>Member system ID.</text>
          </description>
        </leaf>
      </list>
    </container>
  </container>
</module>
