        <leaf name="size">
          <type name="uint8">
            <range value="0..5"/>
          </type>
          <units name="kB"/>
          <description>
            <text>The packet block threshold size.</text>
          </description>
        </leaf>
      </container>
      <container name="channel-alarms">
        <description>
          <text>List of channel alarm.</text>
        </description>
        <list name="channel-alarm">
          <key value="alarm-type pool"/>
          <description>
            <text>Configure channel alarm.</text>
          </description>
          <leaf name="alarm-type">
            <type name="channel-alarm-type"/>
            <description>
              <text>Alarm type.</text>
            </description>
          </leaf>
          <leaf name="pool">
            <type name="channel-alarm-pool-type"/>
            <description>
              <text>Pool.</text>
            </description>
          </leaf>
          <leaf name="threshold">
            <type name="uint8">
              <range value="1..100"/>
            </type>
            <units name="%"/>
