<?xml version="1.0" encoding="UTF-8"?>
<submodule name="huawei-dhcp-server"
           xmlns="urn:ietf:params:xml:ns:yang:yin:1"
           xmlns:dhcp="urn:huawei:yang:huawei-dhcp"
           xmlns:ni="urn:huawei:yang:huawei-network-instance"
           xmlns:inet="urn:ietf:params:xml:ns:yang:ietf-inet-types"
           xmlns:l3vpn="urn:huawei:yang:huawei-l3vpn">
  <yang-version value="1"/>
  <belongs-to module="huawei-dhcp">
    <prefix value="dhcp"/>
  </belongs-to>
  <import module="huawei-network-instance">
    <prefix value="ni"/>
  </import>
  <import module="ietf-inet-types">
    <prefix value="inet"/>
  </import>
  <import module="huawei-l3vpn">
    <prefix value="l3vpn"/>
  </import>
  <organization>
    <text>Huawei Technologies Co., Ltd.</text>
  </organization>
  <contact>
    <text>Huawei Industrial Base
Bantian, Longgang
Shenzhen 518129
People's Republic of China
Website: http://www.huawei.com
Email: support@huawei.com</text>
  </contact>
  <description>
    <text>This module contains a collection of YANG definitions for managing DHCP server.</text>
  </description>
  <revision date="2020-03-13">
    <description>
      <text>Modify description.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <revision date="2020-03-03">
    <description>
      <text>Initial revision.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <typedef name="network-bios-type">
    <type name="enumeration">
      <enum name="invalid">
        <value value="0"/>
        <description>
          <text>-.</text>
        </description>
      </enum>
      <enum name="b-node">
        <value value="1"/>
        <description>
          <text>B-node.</text>
        </description>
      </enum>
      <enum name="m-node">
        <value value="2"/>
        <description>
          <text>M-node.</text>
        </description>
      </enum>
      <enum name="p-node">
        <value value="3"/>
        <description>
          <text>P-node.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>The type of network BIOS.</text>
    </description>
  </typedef>
  <typedef name="reply-type">
    <type name="enumeration">
      <enum name="invalid">
        <value value="0"/>
        <description>
          <text>-.</text>
        </description>
      </enum>
      <enum name="broadcast-always">
        <value value="1"/>
        <description>
          <text>Always broadcast the reply.</text>
        </description>
      </enum>
      <enum name="unicast-always">
        <value value="2"/>
        <description>
          <text>Always unicast the reply.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>DHCP reply type.</text>
    </description>
  </typedef>
  <typedef name="sip-mode">
    <type name="enumeration">
      <enum name="ip-mode">
        <value value="1"/>
        <description>
          <text>SIP server IP mode.</text>
        </description>
      </enum>
      <enum name="list-mode">
        <value value="2"/>
        <description>
          <text>SIP server string mode.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>The type of SIP mode.</text>
    </description>
  </typedef>
  <typedef name="address-status">
    <type name="enumeration">
      <enum name="static-bind-used">
        <value value="0"/>
        <description>
          <text>Static bind used status.</text>
        </description>
      </enum>
      <enum name="static-bind">
        <value value="1"/>
        <description>
          <text>Static bind status.</text>
        </description>
      </enum>
      <enum name="used">
        <value value="2"/>
        <description>
          <text>Used status.</text>
        </description>
      </enum>
      <enum name="expired">
        <value value="3"/>
        <description>
          <text>Expired status.</text>
        </description>
      </enum>
      <enum name="conflict">
        <value value="4"/>
        <description>
          <text>Conflict status.</text>
        </description>
      </enum>
      <enum name="disable">
        <value value="5"/>
        <description>
          <text>Disable status.</text>
        </description>
      </enum>
      <enum name="idle">
        <value value="6"/>
        <description>
          <text>Idle status.</text>
        </description>
      </enum>
      <enum name="error">
        <value value="7"/>
        <description>
          <text>Error.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>The type of address status.</text>
    </description>
  </typedef>
  <grouping name="dhcp-server-type">
    <description>
      <text>Configure DHCP server attributes.</text>
    </description>
    <container name="common">
      <description>
        <text>Configure DHCP server common attributes.</text>
      </description>
      <leaf name="ping-packet-nub">
        <type name="uint32">
          <range value="0..10"/>
        </type>
        <units name="pps"/>
        <default value="2"/>
        <description>
          <text>Number of ping packets.</text>
        </description>
      </leaf>
      <leaf name="ping-packet-timeout">
        <type name="uint32">
          <range value="0..10000"/>
        </type>
        <units name="ms"/>
        <default value="500"/>
        <description>
          <text>Timeout interval of ping reply.</text>
        </description>
      </leaf>
      <leaf name="trust-option82">
        <type name="boolean"/>
        <default value="true"/>
        <description>
          <text>Enable/disable the system to trust option82.</text>
        </description>
      </leaf>
      <leaf name="bootp-enable">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable BOOTP.</text>
        </description>
      </leaf>
      <leaf name="bootp-auto-enable">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable BOOTP automatic IP address assignment.</text>
        </description>
      </leaf>
      <leaf name="database-enable">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable database.</text>
        </description>
      </leaf>
      <leaf name="database-recover">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable database recover.</text>
        </description>
      </leaf>
      <leaf name="write-delay-interval">
        <type name="uint32">
          <range value="300..86400"/>
        </type>
        <units name="s"/>
        <default value="300"/>
        <description>
          <text>Write delay interval, in the range of 300 to 86400, in seconds.</text>
        </description>
      </leaf>
      <leaf name="option60-enable">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable to allocate IP address by option60.</text>
        </description>
      </leaf>
      <leaf name="dest-ip-enable">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable to use destionation IP address of the request packet as server identifier option.</text>
        </description>
      </leaf>
      <leaf name="lease-items-path">
        <type name="string">
          <length value="1..64"/>
        </type>
        <config value="false"/>
        <description>
          <text>Lease items path.</text>
        </description>
      </leaf>
      <leaf name="conflict-items-path">
        <type name="string">
          <length value="1..64"/>
        </type>
        <config value="false"/>
        <description>
          <text>Conflict items path.</text>
        </description>
      </leaf>
      <leaf name="link-selection-enable">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable DHCP server to assign IP addresses based on the link-selection value in option82.</text>
        </description>
      </leaf>
      <leaf name="server-id-override-enable">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable DHCP server to encapsulate option54 into packet based on the server-id-override value in option82.</text>
        </description>
      </leaf>
      <leaf name="request-packet-all-interface-enable">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable the function of DHCP server on all interfaces.</text>
        </description>
      </leaf>
    </container>
    <container name="global-ip-pools">
      <description>
        <text>List of global ip pools.</text>
      </description>
      <list name="global-ip-pool">
        <key value="ip-pool-name"/>
        <description>
          <text>Configure a global ip pool.</text>
        </description>
        <leaf name="ip-pool-name">
          <type name="string">
            <length value="1..128"/>
          </type>
          <description>
            <text>Pool name.</text>
          </description>
        </leaf>
        <leaf name="vpn-instance">
          <type name="leafref">
            <path value="/ni:network-instance/ni:instances/ni:instance/ni:name"/>
          </type>
          <must condition="/ni:network-instance/ni:instances/ni:instance[ni:name=current()]/l3vpn:afs/l3vpn:af[l3vpn:type='ipv4-unicast']"/>
          <default value="_public_"/>
          <description>
            <text>VPN instance.</text>
          </description>
        </leaf>
        <leaf name="locked-flag">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable to lock.</text>
          </description>
        </leaf>
        <leaf name="option-router-disable">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable reply router option.</text>
          </description>
        </leaf>
        <leaf name="warning-exhaust-enable">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable the warning-exhaust of the pool.</text>
          </description>
        </leaf>
        <leaf name="warning-threshold">
          <type name="uint8">
            <range value="0..100"/>
          </type>
          <default value="80"/>
          <description>
            <text>The warning threshold of the pool.</text>
          </description>
        </leaf>
        <container name="gateway">
          <must condition="../gateway or (not(../gateway) and not(../sections))"/>
          <description>
            <text>Configure the gateway information of the pool.</text>
          </description>
          <leaf name="ip-address">
            <type name="inet:ipv4-address-no-zone"/>
            <description>
              <text>The gateway address of the pool.</text>
            </description>
          </leaf>
          <leaf name="mask">
            <when condition="../ip-address"/>
            <type name="inet:ipv4-address-no-zone"/>
            <mandatory value="true"/>
            <description>
              <text>The gateway mask of the pool.</text>
            </description>
          </leaf>
        </container>
        <container name="conflict-recycle">
          <description>
            <text>Configure the conflict address auto recycle.</text>
          </description>
          <leaf name="auto-recycle-enable">
            <type name="boolean"/>
            <default value="true"/>
            <description>
              <text>Enable/disable auto recycle of the conflict address.</text>
            </description>
          </leaf>
          <leaf name="conflict-recycle-interval">
            <when condition="../auto-recycle-enable='true'"/>
            <type name="uint16">
              <range value="30..1440"/>
            </type>
            <units name="min"/>
            <default value="30"/>
            <description>
              <text>The interval of the conflict address auto recycle.</text>
            </description>
          </leaf>
        </container>
        <container name="lease-time">
          <must condition="(day=0 and hour=0 and minute=0 and ../rebind-time/day=0 and ../rebind-time/hour=0 and ../rebind-time/minute=0) or (day&gt;../rebind-time/day) or (day=../rebind-time/day and hour&gt;../rebind-time/hour) or (day=../rebind-time/day and hour=../rebind-time/hour and minute&gt;../rebind-time/minute)"/>
          <description>
            <text>Configure lease time.</text>
          </description>
          <leaf name="day">
            <type name="uint16">
              <range value="0..999"/>
            </type>
            <units name="d"/>
            <default value="3"/>
            <description>
              <text>The lease time day.</text>
            </description>
          </leaf>
          <leaf name="hour">
            <type name="uint16">
              <range value="0..23"/>
            </type>
            <units name="h"/>
            <default value="0"/>
            <description>
              <text>The lease time hour.</text>
            </description>
          </leaf>
          <leaf name="minute">
            <type name="uint16">
              <range value="0..59"/>
            </type>
            <units name="min"/>
            <default value="0"/>
            <description>
              <text>The lease time minute.</text>
            </description>
          </leaf>
          <leaf name="unlimited">
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable to unlimit lease time.</text>
            </description>
          </leaf>
        </container>
        <container name="rebind-time">
          <must condition="(day=0 and hour=0 and minute=0 and ../renew-time/day=0 and ../renew-time/hour=0 and ../renew-time/minute=0) or (day&gt;../renew-time/day) or (day=../renew-time/day and hour&gt;../renew-time/hour) or (day=../renew-time/day and hour=../renew-time/hour and minute&gt;../renew-time/minute)"/>
          <description>
            <text>Configure the rebind time of the pool.</text>
          </description>
          <leaf name="day">
            <type name="uint16">
              <range value="0..999"/>
            </type>
            <units name="d"/>
            <default value="0"/>
            <description>
              <text>The rebinding time day.</text>
            </description>
          </leaf>
          <leaf name="hour">
            <type name="uint8">
              <range value="0..23"/>
            </type>
            <units name="h"/>
            <default value="0"/>
            <description>
              <text>The rebinding time hour.</text>
            </description>
          </leaf>
          <leaf name="minute">
            <type name="uint8">
              <range value="0..59"/>
            </type>
            <units name="min"/>
            <default value="0"/>
            <description>
              <text>The rebinding time minute.</text>
            </description>
          </leaf>
        </container>
        <container name="renew-time">
          <must condition="(day = 0 and hour = 0 and minute = 0 and ../rebind-time/day = 0 and ../rebind-time/hour = 0 and ../rebind-time/minute = 0) or&#10;(day &lt; ../rebind-time/day) or&#10;(day = ../rebind-time/day and hour &lt; ../rebind-time/hour) or&#10;(day = ../rebind-time/day and hour = ../rebind-time/hour and minute &lt; ../rebind-time/minute)"/>
          <description>
            <text>Configure the renew time of the pool.</text>
          </description>
          <leaf name="day">
            <type name="uint16">
              <range value="0..999"/>
            </type>
            <units name="d"/>
            <default value="0"/>
            <description>
              <text>The renew time day.</text>
            </description>
          </leaf>
          <leaf name="hour">
            <type name="uint8">
              <range value="0..23"/>
            </type>
            <units name="h"/>
            <default value="0"/>
            <description>
              <text>The renew time hour.</text>
            </description>
          </leaf>
          <leaf name="minute">
            <type name="uint8">
              <range value="0..59"/>
            </type>
            <units name="min"/>
            <default value="0"/>
            <description>
              <text>The renew time minute.</text>
            </description>
          </leaf>
        </container>
        <container name="option125">
          <description>
            <text>Configure an option125 of the pool.</text>
          </description>
          <leaf name="enterprise-code">
            <when condition="../option-string"/>
            <type name="uint32">
              <range value="0..4294967295"/>
            </type>
            <default value="2011"/>
            <description>
              <text>The enterprise information option code.</text>
            </description>
          </leaf>
          <leaf name="option-string">
            <type name="string">
              <length value="1..32"/>
            </type>
            <description>
              <text>The enterprise information option value.</text>
            </description>
          </leaf>
        </container>
        <container name="sip-server">
          <description>
            <text>Configure the SIP server of the pool.</text>
          </description>
          <leaf name="sip-server-mode">
            <type name="sip-mode"/>
            <description>
              <text>The mode of SIP server.</text>
            </description>
          </leaf>
          <choice name="sip-server-format">
            <description>
              <text>The format of SIP server.</text>
            </description>
            <case name="ip-format">
              <description>
                <text>The IP of SIP server.</text>
              </description>
              <leaf name="sip-server-ip1">
                <when condition="../sip-server-mode='ip-mode'"/>
                <type name="inet:ipv4-address-no-zone"/>
                <mandatory value="true"/>
                <description>
                  <text>The first IP of SIP server.</text>
                </description>
              </leaf>
              <leaf name="sip-server-ip2">
                <when condition="../sip-server-mode='ip-mode'"/>
                <type name="inet:ipv4-address-no-zone"/>
                <description>
                  <text>The second IP of SIP server.</text>
                </description>
              </leaf>
            </case>
            <case name="string-format">
              <description>
                <text>The name of SIP server.</text>
              </description>
              <leaf name="sip-server-name1">
                <when condition="../sip-server-mode='list-mode'"/>
                <type name="string">
                  <length value="1..63"/>
                </type>
                <mandatory value="true"/>
                <description>
                  <text>The first name of SIP server.</text>
                </description>
              </leaf>
              <leaf name="sip-server-name2">
                <when condition="../sip-server-mode='list-mode'"/>
                <type name="string">
                  <length value="1..63"/>
                </type>
                <description>
                  <text>The second name of SIP server.</text>
                </description>
              </leaf>
            </case>
          </choice>
        </container>
        <container name="sections">
          <description>
            <text>List of the sections of the pool.</text>
          </description>
          <list name="section">
            <must condition="(../../gateway and ../section) or (not(../section))"/>
            <key value="section-index"/>
            <max-elements value="256"/>
            <description>
              <text>Configure the section of the pool.</text>
            </description>
            <leaf name="section-index">
              <type name="uint32">
                <range value="0..255"/>
              </type>
              <description>
                <text>The section index.</text>
              </description>
            </leaf>
            <leaf name="start-ip">
              <type name="inet:ipv4-address-no-zone"/>
              <mandatory value="true"/>
              <description>
                <text>The start IP address of the section.</text>
              </description>
            </leaf>
            <leaf name="end-ip">
              <type name="inet:ipv4-address-no-zone"/>
              <mandatory value="true"/>
              <description>
                <text>The end IP address of the section.</text>
              </description>
            </leaf>
          </list>
        </container>
        <container name="dns-lists">
          <description>
            <text>List of DNS IPs.</text>
          </description>
          <list name="dns-list">
            <key value="dns-ip"/>
            <max-elements value="8"/>
            <description>
              <text>Configure a DNS IP instance.</text>
            </description>
            <leaf name="dns-ip">
              <type name="inet:ipv4-address-no-zone"/>
              <description>
                <text>DNS IP.</text>
              </description>
            </leaf>
          </list>
        </container>
        <container name="user-options">
          <description>
            <text>List of configuring the user define options of the pool.</text>
          </description>
          <list name="user-option">
            <key value="option-code"/>
            <max-elements value="16"/>
            <description>
              <text>Configure a user define option of the pool.</text>
            </description>
            <leaf name="option-code">
              <type name="uint32">
                <range value="1..254"/>
              </type>
              <description>
                <text>The user define option code.</text>
              </description>
            </leaf>
            <choice name="option-format">
              <mandatory value="true"/>
              <description>
                <text>The format of server option.</text>
              </description>
              <case name="ascii-format">
                <description>
                  <text>ASCII string format.</text>
                </description>
                <leaf name="ascii-string">
                  <type name="string">
                    <length value="1..255"/>
                  </type>
                  <description>
                    <text>The ASCII string of the option.</text>
                  </description>
                </leaf>
              </case>
            </choice>
          </list>
        </container>
        <container name="option-force-replys">
          <description>
            <text>List of the force reply options of the pool.</text>
          </description>
          <list name="option-force-reply">
            <key value="option-code"/>
            <max-elements value="16"/>
            <description>
              <text>Configure the force reply option of the pool.</text>
            </description>
            <leaf name="option-code">
              <type name="uint8">
                <range value="2..254"/>
              </type>
              <description>
                <text>The force reply option code.</text>
              </description>
            </leaf>
          </list>
        </container>
      </list>
    </container>
  </grouping>
</submodule>
