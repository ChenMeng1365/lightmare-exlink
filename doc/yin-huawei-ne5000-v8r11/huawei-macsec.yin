            </description>
          </leaf>
          <leaf name="mka-cipher-suite">
            <type name="string">
              <length value="1..64"/>
            </type>
            <description>
              <text>MKA encryption suite that uniquely identifies the SAK encryption algorithm for the key server.</text>
            </description>
          </leaf>
          <leaf name="macsec-cipher-suite">
            <type name="string">
              <length value="1..64"/>
            </type>
            <description>
              <text>MACsec encryption suite that uniquely identifies the SAK encryption algorithm for the key server.</text>
            </description>
          </leaf>
          <leaf name="key-server-priority">
            <type name="uint8"/>
            <description>
              <text>Key server priority.</text>
            </description>
          </leaf>
          <leaf name="transmit-sci">
            <type name="string">
              <length value="1..64"/>
            </type>
            <description>
              <text>Local SCI value. The SCI uniquely identifies a security channel in the system.</text>
            </description>
          </leaf>
          <leaf name="rx-mka-packets">
            <type name="uint32"/>
            <units name="packet"/>
            <description>
              <text>Number of received MKA packets.</text>
            </description>
          </leaf>
          <leaf name="tx-mka-packets">
            <type name="uint32"/>
            <units name="packet"/>
            <description>
              <text>Number of sent MKA packets.</text>
            </description>
          </leaf>
          <leaf name="drop-mka-packets">
            <type name="uint32"/>
            <units name="packet"/>
            <description>
              <text>Number of discarded MKA packets.</text>
            </description>
          </leaf>
          <leaf name="wrong-ckn-num">
            <type name="uint32"/>
            <units name="packet"/>
            <description>
              <text>Number of packets that fail in CKN verification.</text>
            </description>
          </leaf>
          <leaf name="wrong-icv-num">
            <type name="uint32"/>
            <units name="packet"/>
            <description>
              <text>Number of packets that fail in ICV verification.</text>
            </description>
          </leaf>
          <leaf name="sak-install-times">
            <type name="uint32"/>
            <description>
              <text>Number of times the SAK is installed.</text>
            </description>
          </leaf>
          <leaf name="sak-delete-times">
            <type name="uint32"/>
            <description>
              <text>Number of times the SAK is deleted.</text>
            </description>
          </leaf>
          <leaf name="sak-swap-times">
            <type name="uint32"/>
            <description>
              <text>Number of times the SAK is switched.</text>
            </description>
          </leaf>
          <leaf name="latest-sak-reason">
            <type name="string">
              <length value="1..64"/>
            </type>
            <description>
              <text>Cause of the latest SAK switching.</text>
            </description>
          </leaf>
          <leaf name="protocol">
            <type name="string">
              <length value="1..8"/>
            </type>
            <description>
              <text>Protocol number.</text>
            </description>
          </leaf>
          <leaf name="pe-vlan">
            <type name="string">
              <length value="1..8"/>
            </type>
            <description>
              <text>VLAN ID in the outer VLAN tag.</text>
            </description>
          </leaf>
          <leaf name="ce-vlan">
            <type name="string">
              <length value="1..8"/>
            </type>
            <description>
              <text>VLAN ID in the inner VLAN tag.</text>
            </description>
          </leaf>
          <leaf name="cfi">
            <type name="string">
              <length value="1..4"/>
            </type>
            <description>
              <text>Canonical Format Indicator value.</text>
            </description>
          </leaf>
          <leaf name="priority">
            <type name="string">
              <length value="1..4"/>
            </type>
            <description>
              <text>Priority value.</text>
            </description>
          </leaf>
        </list>
      </container>
    </container>
  </container>
  <rpc name="reset-mka-statistics">
    <description>
      <text>To clear interface mka statistics.</text>
    </description>
    <ext:node-ref value="/macsec:macsec/macsec:statistics/macsec:interface-mkas"/>
    <input>
      <leaf name="interface-name">
        <type name="leafref">
          <path value="/ifm:ifm/ifm:interfaces/ifm:interface/ifm:name"/>
        </type>
        <description>
          <text>Name of an interface.</text>
        </description>
      </leaf>
    </input>
  </rpc>
  <rpc name="reset-macsec-statistics">
    <description>
      <text>To clear interface macsec statistics.</text>
    </description>
    <ext:node-ref value="/macsec:macsec/macsec:statistics/macsec:interface-macsecs"/>
    <input>
      <leaf name="interface-name">
        <type name="leafref">
          <path value="/ifm:ifm/ifm:interfaces/ifm:interface/ifm:name"/>
        </type>
        <description>
          <text>Name of an interface.</text>
        </description>
      </leaf>
    </input>
  </rpc>
  <augment target-node="/ifm:ifm/ifm:interfaces/ifm:interface">
    <description>
      <text>Adds macsec configuration to ifm interface model.</text>
    </description>
    <container name="macsec">
      <when condition="../ifm:type='Ethernet' or&#10;../ifm:type='GigabitEthernet' or&#10;../ifm:type='100GE' or&#10;../ifm:type='10GE' or&#10;../ifm:type='4x10GE' or&#10;../ifm:type='10x10GE' or&#10;../ifm:type='3x40GE' or&#10;../ifm:type='4x25GE' or&#10;../ifm:type='25GE' or&#10;../ifm:type='40GE' or&#10;../ifm:type='50GE' or&#10;../ifm:type='50|100GE' or&#10;../ifm:type='XGigabitEthernet' or&#10;../ifm:type='FlexE' or&#10;../ifm:type='FlexE-50G' or&#10;../ifm:type='FlexE-100G' or&#10;../ifm:type='FlexE-50|100G'"/>
      <description>
        <text>Configure MACsec application. Before applying the macsec configuration, you need to activate the macsec port license and apply for sufficient license resources.</text>
      </description>
      <container name="ckn-caks">
        <description>
          <text>Configure secure connectivity association name and key. The range supported by this node varies according to the interface type.</text>
        </description>
        <container name="ckn-cak">
          <presence value="create ckn and cak"/>
          <description>
            <text>Enable/disable secure connectivity association name and key.</text>
          </description>
          <leaf name="ckn">
            <type name="string">
              <length value="2..64"/>
            </type>
            <mandatory value="true"/>
            <description>
              <text>The value of ckn; CKN is an even number in hexadecimal notation, containing at least one digit and one letter. The letters are case-insensitive.</text>
            </description>
            <ext:case-sensitivity value="upper2lower"/>
          </leaf>
          <leaf name="cak-type">
            <type name="cipher-flag-type"/>
            <mandatory value="true"/>
            <description>
              <text>The type of cak.</text>
            </description>
          </leaf>
          <leaf name="cak">
            <type name="pub-type:password-extend">
              <length value="1..160"/>
            </type>
            <mandatory value="true"/>
            <description>
              <text>The value of cak; CAK is a hexadecimal notation. The letters are case-insensitive. It must be different from CKN. The length of CAK can only be 32 bits, but in the case of encryption mode, the query result will be more than 100 bits, so the length range is defined as 1 to 160.</text>
            </description>
            <ext:case-sensitivity value="upper2lower"/>
          </leaf>
        </container>
      </container>
      <container name="confidentiality-offsets">
        <description>
          <text>Configure confidentiality offset. The range supported by this node varies according to the interface type.</text>
        </description>
        <container name="confidentiality-offset">
          <presence value="create confidentiality offset"/>
          <description>
            <text>Enable/disable confidentiality offset.</text>
          </description>
          <leaf name="offset-value">
            <type name="uint8">
              <range value="0 | 30 | 50"/>
            </type>
            <mandatory value="true"/>
            <description>
              <text>Confidentiality offset value.</text>
            </description>
          </leaf>
        </container>
      </container>
      <container name="replay-windows">
        <description>
          <text>Configure replay protection window size. The range supported by this node varies according to the interface type.</text>
        </description>
        <container name="replay-window">
          <presence value="create replay window"/>
          <description>
            <text>Enable/disable replay protection window size.</text>
          </description>
          <leaf name="window-size">
            <type name="uint32">
              <range value="0..1024"/>
            </type>
            <mandatory value="true"/>
            <description>
              <text>Replay protection window size.</text>
            </description>
          </leaf>
        </container>
      </container>
      <container name="keyserver-prioritys">
        <description>
          <text>Configure key server priority. The range supported by this node varies according to the interface type.</text>
        </description>
        <container name="keyserver-priority">
          <presence value="create keyserver priority"/>
          <description>
            <text>Enable/disable key server priority.</text>
          </description>
          <leaf name="priority">
            <type name="uint8">
              <range value="0..255"/>
            </type>
            <mandatory value="true"/>
            <description>
              <text>Key server priority value.</text>
            </description>
          </leaf>
        </container>
      </container>
      <container name="sak-life-times">
        <description>
          <text>Configure SAK life time. The range supported by this node varies according to the interface type.</text>
        </description>
        <container name="sak-life-time">
          <presence value="create keyserver priority"/>
          <description>
            <text>Enable/disable SAK life time.</text>
          </description>
          <leaf name="life-time">
            <type name="uint32">
              <range value="300..604800"/>
            </type>
            <mandatory value="true"/>
            <description>
              <text>SAK life time value.</text>
            </description>
          </leaf>
        </container>
      </container>
      <container name="encrypt-modes">
        <description>
          <text>Configure encryption mode. The range supported by this node varies according to the interface type.</text>
        </description>
        <container name="encrypt-mode">
          <presence value="create encrypt mode"/>
          <description>
            <text>Enable/disable encryption mode.</text>
          </description>
          <leaf name="mode">
            <type name="encrypt-mode-type"/>
            <mandatory value="true"/>
            <description>
              <text>Encryption mode value.</text>
            </description>
          </leaf>
        </container>
      </container>
      <container name="cipher-algorithm-suites">
        <description>
          <text>Configure cipher algorithm suite. The range supported by this node varies according to the interface type.</text>
        </description>
        <container name="cipher-algorithm-suite">
          <presence value="create cipher algorithm suite"/>
          <description>
            <text>Enable/disable cipher algorithm suite.</text>
          </description>
          <leaf name="suite">
            <type name="cipher-suite-type"/>
            <mandatory value="true"/>
            <description>
              <text>Cipher algorithm suite type.</text>
            </description>
          </leaf>
        </container>
      </container>
      <container name="vlan-in-clears">
        <description>
          <text>Configure VLAN tag to bypass MACsec encryption. The range supported by this node varies according to the interface type.</text>
        </description>
        <container name="vlan-in-clear">
          <when condition="../../ckn-caks/ckn-cak/ckn"/>
          <must condition="../../../ifm:class='sub-interface'"/>
          <presence value="create vlan tag"/>
          <description>
            <text>Enable/disable VLAN tag configured to bypass MACsec encryption.</text>
          </description>
          <leaf name="clear-type">
            <type name="vlan-in-clear-type"/>
            <mandatory value="true"/>
            <description>
              <text>The type of VLAN tag.</text>
            </description>
          </leaf>
        </container>
      </container>
    </container>
  </augment>
  <augment target-node="/lcs:license/lcs:license-items/lcs:license-item">
    <description>
      <text>Add MACsec configuration to license model.</text>
    </description>
    <container name="port-macsecs">
      <when condition="../lcs:name='LCR9S9KMACS00' or ../lcs:name='LCR5MACSEC01' or ../lcs:name='LCR5MACSEC02' or ../lcs:name='LCX6S60NMSEAP' or ../lcs:name='LANKMACSEC011' or ../lcs:name='LANKMACSEC026' or ../lcs:name='LANKMACSEC051' or ../lcs:name='LCR9S9K0XMS80' or ../lcs:name='LCR9S9K0NMS80' or ../lcs:name='LCR9S9K0NMS85' or ../lcs:name='LTNRS00YMSC0P' or ../lcs:name='LTNRS00VMSC0P' or ../lcs:name='LTNRS00NMSC0P' or ../lcs:name='LCR5S40YMSC0P' or ../lcs:name='LCR5S40VMSC0P' or ../lcs:name='LCR5S40NMSC0P' or ../lcs:name='LCX6S00YMSC0P' or ../lcs:name='LCX6S00VMSC0P' or ../lcs:name='LCX6S00NMSC0P' or ../lcs:name='LCR5S40XMSC0P' or ../lcs:name='LCX6S40YMSC0P' or ../lcs:name='LTNRS40YMSC0P' or ../lcs:name='LCX6S00VMSC01' or ../lcs:name='LCX6S00YMSC01' or ../lcs:name='LCR5SMACSEC05' or ../lcs:name='LCR5SMACSEC06' or ../lcs:name='LCR8SMACSECC2' or ../lcs:name='LCR8SMACSECC4' or ../lcs:name='LTNRSMACSECC4'or ../lcs:name='LTNRSMACSECC2' or ../lcs:name='LCR5SMACSECC2' or ../lcs:name='LCR5SMACSECC4' or ../lcs:name='LCX6S00NMSC01' or ../lcs:name='LCX6S00XMSC01' or ../lcs:name='LCX6S00GMSC01' or ../lcs:name='LTNRS00GMSC0P' or ../lcs:name='LCR5S40GMSC0P' or ../lcs:name='LCX6S40GMSC0P' or ../lcs:name='LCR8S8KXMSCCP' or ../lcs:name='LCR8S8KXMSCCR' or ../lcs:name='LANKMACSEC101' or ../lcs:name='LCR8SM6MACSC1' or ../lcs:name='LCR8SM6MACSC0'"/>
      <description>
        <text>List of port MACsec license information.</text>
      </description>
      <list name="port-macsec">
        <key value="position"/>
        <description>
          <text>Configure port MACsec license.</text>
        </description>
        <leaf name="position">
          <type name="string">
            <length value="1..32"/>
          </type>
          <description>
            <text>Port position. The name is expressed in the format of WdmslotID/cardID/portID. For example, Wdm1/0/1. Here Wdm1/0/1 is an example. The value may vary with devices.</text>
          </description>
        </leaf>
        <leaf name="active-status">
          <type name="lcs-active-status-type"/>
          <config value="false"/>
          <description>
            <text>Activated State.</text>
          </description>
        </leaf>
      </list>
    </container>
  </augment>
</module>
