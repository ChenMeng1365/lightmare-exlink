  <feature name="performance-threshold-alarm">
    <description>
      <text>Performance threshold alarm feature.</text>
    </description>
  </feature>
  <feature name="netconf-report">
    <description>
      <text>Netconf report feature.</text>
    </description>
  </feature>
  <feature name="shield-profiles">
    <description>
      <text>Alarm shield profile feature.</text>
    </description>
  </feature>
  <typedef name="alarm-level">
    <type name="enumeration">
      <enum name="emergencies">
        <value value="1"/>
        <description>
          <text>Level of emergency.</text>
        </description>
      </enum>
      <enum name="alerts">
        <value value="2"/>
        <description>
          <text>Level of alert.</text>
        </description>
      </enum>
      <enum name="critical">
        <value value="3"/>
        <description>
          <text>Level of critical.</text>
        </description>
      </enum>
      <enum name="errors">
        <value value="4"/>
        <description>
          <text>Level of error.</text>
        </description>
      </enum>
      <enum name="warnings">
        <value value="5"/>
        <description>
          <text>Level of warning.</text>
        </description>
      </enum>
      <enum name="notifications">
        <value value="6"/>
        <description>
          <text>Level of notification.</text>
        </description>
      </enum>
      <enum name="informational">
        <value value="7"/>
        <description>
          <text>Level of information.</text>
        </description>
      </enum>
      <enum name="debugging">
        <value value="8"/>
        <description>
          <text>Level of debug.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Alarm level.</text>
    </description>
  </typedef>
  <typedef name="alarm-type">
    <type name="enumeration">
      <enum name="communication-alarm">
        <value value="0"/>
      </enum>
      <enum name="qos-alarm">
        <value value="1"/>
      </enum>
      <enum name="device-alarm">
        <value value="2"/>
      </enum>
      <enum name="process-err-alarm">
        <value value="3"/>
      </enum>
      <enum name="environment-alarm">
        <value value="4"/>
      </enum>
    </type>
    <description>
      <text>Alarm type.</text>
    </description>
  </typedef>
  <typedef name="performance-alarm-type">
    <type name="enumeration">
      <enum name="high-threshhold-alarm">
        <value value="1"/>
      </enum>
      <enum name="warn-high-threshhold-alarm">
        <value value="2"/>
      </enum>
      <enum name="warn-low-threshhold-alarm">
        <value value="3"/>
      </enum>
      <enum name="low-threshhold-alarm">
        <value value="4"/>
      </enum>
      <enum name="max-reference-value-alarm">
        <value value="5"/>
      </enum>
      <enum name="min-reference-value-alarm">
        <value value="6"/>
      </enum>
      <enum name="invalid-perf-type">
        <value value="254"/>
      </enum>
    </type>
    <description>
      <text>Performance alarm type.</text>
    </description>
  </typedef>
  <container name="configuration">
    <container name="alarm">
      <description>
        <text>For alarm configuration.</text>
      </description>
      <container name="alarmMasks">
        <description>
          <text>Set some rules based TPID for mask alarm.</text>
        </description>
        <list name="alarmMask">
          <description>
            <text>Set some rules based TPID for mask alarm.</text>
          </description>
          <max-elements value="2048"/>
          <key value="checkPointType checkPointTpid tpidMask code checkPointTpidClass"/>
          <leaf name="checkPointType">
            <description>
              <text>The type of check point. Range 1~0xFFFF, 0xFFFF indicates any type.</text>
            </description>
            <type name="uint16">
              <range value="1..65535"/>
            </type>
          </leaf>
          <leaf name="checkPointTpid">
            <description>
              <text>TPID be masked. Length range is from 62 to 128 bytes.</text>
            </description>
            <type name="binary">
              <length value="62..128"/>
            </type>
          </leaf>
          <leaf name="tpidMask">
            <description>
              <text>Mask bit of attribute 'checkPointTpid'. Length is from 8 to16 bytes. 1 indicates that the corresponding byte is valid.</text>
            </description>
            <type name="binary">
              <length value="8..16"/>
            </type>
          </leaf>
          <leaf name="code">
            <description>
              <text>Alarm code for mask. Range 1~0xFFFFFFFF ,0xFFFFFFFF for any code.</text>
            </description>
            <type name="uint32">
              <range value="1..4294967295"/>
            </type>
          </leaf>
          <leaf name="checkPointTpidClass">
            <description>
              <text>TPID class.</text>
            </description>
            <type name="enumeration">
              <enum name="board">
                <value value="1"/>
              </enum>
              <enum name="CPU">
                <value value="2"/>
              </enum>
              <enum name="system">
                <value value="3"/>
              </enum>
              <enum name="all">
                <value value="255"/>
              </enum>
            </type>
          </leaf>
          <leaf name="checkPointTpidLen">
            <description>
              <text>TPID length.</text>
            </description>
            <type name="uint8">
              <range value="62..128"/>
            </type>
          </leaf>
        </list>
      </container>
      <container name="almSeveritys">
        <list name="almSeverity">
          <description>
            <text>Set the alarm level.</text>
          </description>
          <key value="code tpidType"/>
          <leaf name="code">
            <description>
              <text>Alarm code.Range 1~0xFFFFFFFE.</text>
            </description>
            <type name="uint32">
              <range value="1..4294967294"/>
            </type>
          </leaf>
          <leaf name="tpidType">
            <description>
              <text>The type of check point. Range 1~0xFFFF, 0xFFFF indicates any type.</text>
            </description>
            <type name="uint16">
              <range value="1..65535"/>
            </type>
          </leaf>
          <leaf name="severity">
            <description>
              <text>Severity level.</text>
            </description>
            <type name="enumeration">
              <enum name="emergenciy">
                <value value="1"/>
              </enum>
              <enum name="alert">
                <value value="2"/>
              </enum>
              <enum name="critical">
                <value value="3"/>
              </enum>
              <enum name="error">
                <value value="4"/>
              </enum>
              <enum name="warning">
                <value value="5"/>
              </enum>
              <enum name="notice">
                <value value="6"/>
              </enum>
              <enum name="informational">
                <value value="7"/>
              </enum>
              <enum name="debug">
                <value value="8"/>
              </enum>
            </type>
            <mandatory value="true"/>
          </leaf>
        </list>
      </container>
      <container name="almRelayPara">
        <description>
          <text>Set relay time to alarm occur or disappear. Only support for NE.</text>
        </description>
        <presence value="Alarm acknowledgment time setting."/>
        <leaf name="occurTime">
          <description>
            <text>Confirm time for alarm occur.</text>
          </description>
          <type name="uint8">
            <range value="0..10"/>
          </type>
          <units name="second"/>
          <default value="0"/>
        </leaf>
        <leaf name="clearTime">
          <description>
            <text>Confirm time for alarm disappear.</text>
          </description>
          <type name="uint8">
            <range value="0..10"/>
          </type>
          <units name="second"/>
          <default value="5"/>
        </leaf>
      </container>
      <container name="alarmPreload">
        <description>
          <text>Alarm preload setup.</text>
        </description>
        <list name="preloadBasedIf">
          <description>
            <text>Port-based pre-alarm setup.</text>
          </description>
          <key value="ifName"/>
          <leaf name="ifName">
            <description>
              <text>Interface name.</text>
            </description>
            <type name="ncd:ifname"/>
          </leaf>
          <leaf name="preloadstat">
            <description>
              <text>Alarm preload status.</text>
            </description>
            <type name="enumeration">
              <enum name="auto-recover-report">
                <value value="1"/>
              </enum>
              <enum name="alarm-reverse">
                <value value="2"/>
              </enum>
            </type>
            <mandatory value="true"/>
          </leaf>
        </list>
      </container>
      <container name="performance-threshold-alarm">
        <description>
          <text>Set performance threshold alarm switch.</text>
        </description>
        <leaf name="state">
          <description>
            <text>State of performance threshold alarm switch.</text>
          </description>
          <type name="enumeration">
            <enum name="disable">
              <value value="0"/>
              <description>
                <text>Disable switch.</text>
              </description>
            </enum>
            <enum name="enable">
              <value value="1"/>
              <description>
                <text>Enable switch.</text>
              </description>
            </enum>
          </type>
        </leaf>
      </container>
      <container name="netconf-report">
        <description>
          <text>Set the reporting alarm level.</text>
        </description>
        <leaf name="severity">
          <description>
            <text>Severity level of the alarm code. If the customized alarm level of the product is any level in the 1-8 by default, the customized alarm level or above can be reported through netconf. The get node value is the customized alarm level. If the product has no customized default value, the get node value is off.</text>
          </description>
          <type name="enumeration">
            <enum name="emergencies">
              <value value="1"/>
              <description>
                <text>Level of emergencies.</text>
              </description>
            </enum>
            <enum name="alerts">
              <value value="2"/>
              <description>
                <text>Level of alerts.</text>
              </description>
            </enum>
            <enum name="critical">
              <value value="3"/>
              <description>
                <text>Level of critical.</text>
              </description>
            </enum>
            <enum name="errors">
              <value value="4"/>
              <description>
                <text>Level of errors.</text>
              </description>
            </enum>
            <enum name="warnings">
              <value value="5"/>
              <description>
                <text>Level of warnings.</text>
              </description>
            </enum>
            <enum name="notifications">
              <value value="6"/>
              <description>
                <text>Level of notifications.</text>
              </description>
            </enum>
            <enum name="informational">
              <value value="7"/>
              <description>
                <text>Level of informational.</text>
              </description>
            </enum>
            <enum name="debugging">
              <value value="8"/>
              <description>
                <text>Level of debugging.</text>
              </description>
            </enum>
            <enum name="off">
              <value value="9"/>
              <description>
                <text>Turn off alarm reporting through netconf.</text>
              </description>
            </enum>
          </type>
        </leaf>
      </container>
      <container name="shield-profiles">
        <description>
