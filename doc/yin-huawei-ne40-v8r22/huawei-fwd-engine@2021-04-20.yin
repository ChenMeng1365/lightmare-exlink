<?xml version="1.0" encoding="UTF-8"?>
<module name="huawei-fwd-engine"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:fwd-engine="urn:huawei:yang:huawei-fwd-engine"
        xmlns:ext="urn:huawei:yang:huawei-extension"
        xmlns:devm="urn:huawei:yang:huawei-devm">
  <yang-version value="1"/>
  <namespace uri="urn:huawei:yang:huawei-fwd-engine"/>
  <prefix value="fwd-engine"/>
  <import module="huawei-extension">
    <prefix value="ext"/>
  </import>
  <import module="huawei-devm">
    <prefix value="devm"/>
  </import>
  <organization>
    <text>Huawei Technologies Co., Ltd.</text>
  </organization>
  <contact>
    <text>Huawei Industrial Base
Bantian, Longgang
Shenzhen 518129
People's Republic of China
Website: http://www.huawei.com
Email: support@huawei.com</text>
  </contact>
  <description>
    <text>Global Configuration.</text>
  </description>
  <revision date="2021-04-20">
    <description>
      <text>Added frame-padding-fix.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <revision date="2020-03-09">
    <description>
      <text>Initial revision.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <ext:task-name value="device-mgr"/>
  <augment target-node="/devm:devm">
    <description>
      <text>Forward engine configuration.</text>
    </description>
    <container name="fwd-performance-alarms">
      <description>
        <text>List of forwarding performance alarm.</text>
      </description>
      <list name="fwd-performance-alarm">
        <key value="slot-id"/>
        <description>
          <text>Configure the alarm reporting and alarm clearance thresholds for forwarding performance.</text>
        </description>
        <ext:generated-by value="system"/>
        <ext:operation-exclude value="create|delete"/>
        <leaf name="slot-id">
          <type name="string">
            <length value="1..15"/>
          </type>
          <description>
            <text>Slot ID.</text>
          </description>
        </leaf>
        <leaf name="report-threshold">
          <type name="uint32">
            <range value="0..100"/>
          </type>
          <units name="%"/>
          <must condition="(../report-threshold&gt;../clear-threshold and ../report-threshold!=0 and ../clear-threshold!=0) or (../report-threshold=0 and ../clear-threshold=0)"/>
          <default value="80"/>
          <description>
            <text>Specifies the alarm reporting threshold.</text>
          </description>
        </leaf>
        <leaf name="clear-threshold">
          <type name="uint32">
            <range value="0..100"/>
          </type>
          <units name="%"/>
          <must condition="(../report-threshold&gt;../clear-threshold and ../report-threshold!=0 and ../clear-threshold!=0) or (../report-threshold=0 and ../clear-threshold=0)"/>
          <default value="75"/>
          <description>
            <text>Specifies the alarm clearance threshold.</text>
          </description>
        </leaf>
      </list>
    </container>
    <container name="fwd-services-alarms">
      <description>
        <text>List of value-added services alarm.</text>
      </description>
      <list name="fwd-services-alarm">
        <key value="slot-id"/>
        <description>
          <text>Configure the alarm reporting and alarm clearance thresholds for value-added services.</text>
        </description>
        <ext:generated-by value="system"/>
        <ext:operation-exclude value="create|delete"/>
        <leaf name="slot-id">
          <type name="string">
            <length value="1..15"/>
          </type>
          <description>
            <text>Slot ID.</text>
          </description>
        </leaf>
        <leaf name="report-threshold">
          <type name="uint32">
            <range value="0..100"/>
          </type>
          <units name="%"/>
          <must condition="(../report-threshold&gt;../clear-threshold and ../report-threshold!=0 and ../clear-threshold!=0) or (../report-threshold=0 and ../clear-threshold=0)"/>
          <default value="80"/>
          <description>
            <text>Specifies the alarm reporting threshold.</text>
          </description>
        </leaf>
        <leaf name="clear-threshold">
          <type name="uint32">
            <range value="0..100"/>
          </type>
          <units name="%"/>
          <must condition="(../report-threshold&gt;../clear-threshold and ../report-threshold!=0 and ../clear-threshold!=0) or (../report-threshold=0 and ../clear-threshold=0)"/>
          <default value="75"/>
          <description>
            <text>Specifies the alarm clearance threshold.</text>
          </description>
        </leaf>
      </list>
    </container>
    <container name="fwd-reassembly-alarms">
      <description>
        <text>List of packet reassembly alarm.</text>
      </description>
      <list name="fwd-reassembly-alarm">
        <key value="slot-id"/>
        <description>
          <text>Configure the alarm reporting and alarm clearance thresholds for packet reassembly.</text>
        </description>
        <ext:generated-by value="system"/>
        <ext:operation-exclude value="create|delete"/>
        <leaf name="slot-id">
          <type name="string">
            <length value="1..15"/>
          </type>
          <description>
            <text>Slot ID.</text>
          </description>
        </leaf>
        <leaf name="report-threshold">
          <type name="uint32">
            <range value="0..100"/>
          </type>
          <units name="%"/>
          <must condition="(../report-threshold&gt;../clear-threshold and ../report-threshold!=0 and ../clear-threshold!=0) or (../report-threshold=0 and ../clear-threshold=0)"/>
          <default value="80"/>
          <description>
            <text>Specifies the alarm reporting threshold.</text>
          </description>
        </leaf>
        <leaf name="clear-threshold">
          <type name="uint32">
            <range value="0..100"/>
          </type>
          <units name="%"/>
          <must condition="(../report-threshold&gt;../clear-threshold and ../report-threshold!=0 and ../clear-threshold!=0) or (../report-threshold=0 and ../clear-threshold=0)"/>
          <default value="75"/>
          <description>
            <text>Specifies the alarm clearance threshold.</text>
          </description>
        </leaf>
      </list>
    </container>
    <container name="fwd-board-flow-alarms">
      <description>
        <text>List of traffic alarm on a board.</text>
      </description>
      <list name="fwd-board-flow-alarm">
        <key value="slot-id"/>
        <description>
          <text>Configure the alarm reporting and alarm clearance thresholds for traffic on a board.</text>
        </description>
        <ext:generated-by value="system"/>
        <ext:operation-exclude value="create|delete"/>
        <leaf name="slot-id">
          <type name="string">
            <length value="1..15"/>
          </type>
          <description>
            <text>Slot ID.</text>
          </description>
        </leaf>
        <leaf name="input-report-threshold">
          <type name="uint32">
            <range value="0..100"/>
          </type>
          <units name="%"/>
          <must condition="(../input-report-threshold&gt;../input-clear-threshold and ../input-report-threshold!=0 and ../input-clear-threshold!=0) or (../input-report-threshold=0 and ../input-clear-threshold=0)"/>
          <default value="80"/>
          <description>
            <text>Specifies the input bandwidth alarm reporting threshold.</text>
          </description>
        </leaf>
        <leaf name="input-clear-threshold">
          <type name="uint32">
            <range value="0..100"/>
          </type>
          <units name="%"/>
          <must condition="(../input-report-threshold&gt;../input-clear-threshold and ../input-report-threshold!=0 and ../input-clear-threshold!=0) or (../input-report-threshold=0 and ../input-clear-threshold=0)"/>
          <default value="75"/>
          <description>
            <text>Specifies the input bandwidth alarm clearance threshold.</text>
          </description>
        </leaf>
        <leaf name="output-report-threshold">
          <type name="uint32">
            <range value="0..100"/>
          </type>
          <units name="%"/>
          <must condition="(../output-report-threshold&gt;../output-clear-threshold and ../output-report-threshold!=0 and ../output-clear-threshold!=0) or (../output-report-threshold=0 and ../output-clear-threshold=0)"/>
          <default value="80"/>
          <description>
            <text>Specifies the output bandwidth alarm reporting threshold.</text>
          </description>
        </leaf>
        <leaf name="output-clear-threshold">
          <type name="uint32">
            <range value="0..100"/>
          </type>
          <units name="%"/>
          <must condition="(../output-report-threshold&gt;../output-clear-threshold and ../output-report-threshold!=0 and ../output-clear-threshold!=0) or (../output-report-threshold=0 and ../output-clear-threshold=0)"/>
          <default value="75"/>
          <description>
            <text>Specifies the output bandwidth alarm clearance threshold.</text>
          </description>
        </leaf>
      </list>
    </container>
  </augment>
  <augment target-node="/devm:devm/devm:lpu-boards/devm:lpu-board">
    <description>
      <text>Padding fix configuration.</text>
    </description>
    <container name="frame-padding-fix">
      <presence value="create frame-padding-fix"/>
      <description>
        <text>Enable/disable a board to reserve the padding fields in upstream Ethernet packets.</text>
      </description>
    </container>
  </augment>
  <augment target-node="/devm:devm/devm:mpu-boards/devm:mpu-board">
    <description>
      <text>Padding fix configuration.</text>
    </description>
    <container name="frame-padding-fix">
      <presence value="create frame-padding-fix"/>
      <description>
        <text>Enable/disable a board to reserve the padding fields in upstream Ethernet packets.</text>
      </description>
    </container>
  </augment>
</module>
