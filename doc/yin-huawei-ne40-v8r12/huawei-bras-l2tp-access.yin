        </description>
        <ext:generated-by value="system">
          <ext:filter value="name='default-lac' or name='default-lns'"/>
          <description>
            <text>The instances whose name is 'default-lac' or 'default-lns' are generated by system automatically.</text>
          </description>
        </ext:generated-by>
        <ext:operation-exclude value="create|delete">
          <ext:filter value="name='default-lac' or name='default-lns'"/>
