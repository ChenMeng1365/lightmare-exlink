  <typedef name="active-state">
    <type name="enumeration">
      <enum name="active-or-restore">
        <value value="0"/>
        <description>
          <text>APS active state is active or restore.</text>
        </description>
      </enum>
      <enum name="pause">
        <value value="1"/>
        <description>
          <text>APS active state is pause.</text>
        </description>
      </enum>
    </type>
    <default value="active-or-restore"/>
    <description>
      <text>Active state of APS.</text>
    </description>
  </typedef>
  <typedef name="send-packet-mode">
    <type name="enumeration">
