<?xml version="1.0" encoding="UTF-8"?>
<module name="zxr10-pm-sys-satellite"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:pm-sys-satellite="http://www.zte.com.cn/zxr10/netconf/schema/rosng/pm-sys-satellite"
        xmlns:ncd="http://www.zte.com.cn/zxr10/netconf/schema/rosng/netconf-defs">
  <namespace uri="http://www.zte.com.cn/zxr10/netconf/schema/rosng/pm-sys-satellite"/>
  <prefix value="pm-sys-satellite"/>
  <yang-version value="1"/>
  <import module="zxr10-netconf-defs">
    <prefix value="ncd"/>
  </import>
  <organization>
    <text>ZTE Corporation</text>
  </organization>
  <contact>
    <text>ZTE Corporation  
   Nanjing Institute of ZTE Corporation 
   No.68 Zijinghua Rd. Yuhuatai District, Nanjing, China 
   Tel: +86-25-52870000</text>
  </contact>
  <revision date="2021-01-25">
    <description>
      <text>Initialize version.</text>
    </description>
  </revision>
  <container name="state">
    <config value="false"/>
    <container name="sys">
      <container name="systems">
        <list name="system">
          <key value="peid"/>
          <leaf name="peid">
            <description>
              <text>Satellite ID.</text>
            </description>
            <type name="uint32">
              <range value="1..128">
                <description>
                  <text>peid range.</text>
                </description>
              </range>
            </type>
          </leaf>
          <leaf name="system-version">
            <type name="ncd:string-all"/>
          </leaf>
          <leaf name="boot-time">
            <type name="uint64"/>
          </leaf>
          <leaf name="copyright">
            <description>
              <text>copyRight</text>
            </description>
            <type name="ncd:string-all"/>
          </leaf>
          <leaf name="compile-time">
            <type name="ncd:string-all"/>
          </leaf>
        </list>
      </container>
      <container name="shelfs">
        <list name="shelf">
          <key value="peid"/>
          <leaf name="peid">
            <description>
              <text>Satellite ID.</text>
            </description>
            <type name="uint32">
              <range value="1..128">
                <description>
                  <text>peid range.</text>
                </description>
              </range>
            </type>
          </leaf>
          <leaf name="shelfno">
            <description>
              <text>Shelf number</text>
            </description>
            <type name="uint8">
              <range value="0..255">
                <description>
                  <text>shelf range.</text>
                </description>
              </range>
            </type>
          </leaf>
          <leaf name="shelfsn">
            <description>
              <text>Shelf series number</text>
            </description>
            <type name="ncd:string-all"/>
          </leaf>
        </list>
      </container>
      <container name="processor-usages">
        <list name="processor-usage">
          <key value="peid"/>
          <leaf name="peid">
            <description>
              <text>Satellite ID.</text>
            </description>
            <type name="uint32">
              <range value="1..128">
                <description>
                  <text>peid range.</text>
                </description>
              </range>
            </type>
          </leaf>
          <leaf name="shelf">
            <description>
              <text>Shelf number</text>
            </description>
            <type name="uint8">
              <range value="0..255">
                <description>
                  <text>shelf range.</text>
                </description>
              </range>
            </type>
          </leaf>
          <leaf name="slot">
            <description>
              <text>Slot number</text>
            </description>
            <type name="uint8">
              <range value="0..255">
                <description>
                  <text>slot range.</text>
                </description>
              </range>
            </type>
          </leaf>
          <leaf name="cpu">
            <description>
              <text>CPU number</text>
            </description>
            <type name="uint8"/>
          </leaf>
          <leaf name="cpu-usage-5s">
            <description>
              <text>CPU usage ratio measured in 5 seconds</text>
            </description>
            <type name="ncd:string-all"/>
          </leaf>
          <leaf name="cpu-usage-1m">
            <description>
              <text>CPU usage ratio measured in 1 minute</text>
            </description>
            <type name="ncd:string-all"/>
          </leaf>
          <leaf name="cpu-usage-5m">
            <description>
              <text>CPU usage ratio measured in 5 minute</text>
            </description>
            <type name="ncd:string-all"/>
          </leaf>
          <leaf name="cpu-peak">
            <description>
              <text>CPU peak usage ratio measured in 1 minute</text>
            </description>
            <type name="ncd:string-all"/>
          </leaf>
          <leaf name="mem-usage">
            <description>
              <text>Memory usage ratio</text>
            </description>
            <type name="ncd:string-all"/>
          </leaf>
          <leaf name="mem-physical">
            <description>
              <text>Physical memory</text>
            </description>
            <type name="uint32"/>
          </leaf>
          <leaf name="mem-free">
            <description>
              <text>Free memory</text>
            </description>
            <type name="uint32"/>
          </leaf>
        </list>
      </container>
      <container name="ports">
        <list name="port">
          <key value="peid"/>
          <ncd:select fields="peid"/>
          <leaf name="peid">
            <description>
              <text>Satellite ID.</text>
            </description>
            <type name="uint32">
              <range value="1..128">
                <description>
                  <text>Peid range.</text>
                </description>
              </range>
            </type>
          </leaf>
          <leaf name="shelf">
            <description>
              <text>Shelf number.</text>
            </description>
            <type name="uint8">
              <range value="0..255">
                <description>
                  <text>shelf range.</text>
                </description>
              </range>
            </type>
          </leaf>
          <leaf name="slot">
            <description>
              <text>Slot number.</text>
            </description>
            <type name="uint8">
              <range value="0..255">
                <description>
                  <text>slot range.</text>
                </description>
              </range>
            </type>
          </leaf>
          <leaf name="subslot">
            <description>
              <text>Subslot number.</text>
            </description>
            <type name="uint8"/>
          </leaf>
          <leaf name="portno">
            <description>
              <text>Port number.</text>
            </description>
            <type name="uint32"/>
          </leaf>
          <leaf name="porttype">
            <description>
              <text>Port type.</text>
            </description>
            <type name="uint16"/>
          </leaf>
          <leaf name="portname">
            <description>
              <text>Port name.</text>
            </description>
            <type name="string"/>
          </leaf>
          <leaf name="portphystatus">
            <description>
              <text>Port physical status.</text>
            </description>
            <type name="enumeration">
              <enum name="up">
                <value value="0"/>
                <description>
                  <text>up.</text>
                </description>
              </enum>
              <enum name="down">
                <value value="1"/>
                <description>
                  <text>down.</text>
                </description>
              </enum>
              <enum name="offline">
                <value value="32"/>
                <description>
                  <text>offline.</text>
                </description>
              </enum>
            </type>
          </leaf>
          <leaf name="portpemode">
            <description>
              <text>Port electric optical property.</text>
            </description>
            <type name="enumeration">
              <enum name="media-copper">
                <value value="1"/>
                <description>
                  <text>The PEMode of the port is electric.</text>
                </description>
              </enum>
              <enum name="media-fiber">
                <value value="2"/>
                <description>
                  <text>The PEMode of the port is optical.</text>
                </description>
              </enum>
            </type>
          </leaf>
          <leaf name="portspeed">
            <description>
              <text>Port speed.</text>
            </description>
            <type name="enumeration">
              <enum name="speed-10M">
                <value value="1"/>
                <description>
                  <text>Port speed is 10M.</text>
                </description>
              </enum>
              <enum name="speed-100M">
                <value value="2"/>
                <description>
                  <text>Port speed is 100M.</text>
                </description>
              </enum>
              <enum name="speed-1G">
                <value value="3"/>
                <description>
                  <text>Port speed is 1G.</text>
                </description>
              </enum>
              <enum name="speed-10G">
                <value value="4"/>
                <description>
                  <text>Port speed is 10G.</text>
                </description>
              </enum>
              <enum name="speed-25G">
                <value value="10"/>
                <description>
                  <text>Port speed is 25G.</text>
                </description>
              </enum>
              <enum name="speed-40G">
                <value value="5"/>
                <description>
                  <text>Port speed is 40G.</text>
                </description>
              </enum>
              <enum name="speed-50G">
                <value value="11"/>
                <description>
                  <text>Port speed is 50G.</text>
                </description>
              </enum>
              <enum name="speed-100G">
                <value value="6"/>
                <description>
                  <text>Port speed is 100G.</text>
                </description>
              </enum>
              <enum name="speed-200G">
                <value value="12"/>
                <description>
                  <text>Port speed is 200G.</text>
                </description>
              </enum>
              <enum name="speed-400G">
                <value value="13"/>
                <description>
                  <text>Port speed is 400G.</text>
                </description>
              </enum>
              <enum name="speed-auto">
                <value value="15"/>
                <description>
                  <text>Port speed is configured to auto, the real speed depend on optical module.</text>
                </description>
              </enum>
            </type>
          </leaf>
          <leaf name="portnegotiation">
            <description>
              <text>Port negotiation mode.</text>
            </description>
            <type name="enumeration">
              <enum name="negotiation-force">
                <value value="1"/>
                <description>
                  <text>Force negotiation mode.</text>
                </description>
              </enum>
              <enum name="negotiation-auto">
                <value value="2"/>
                <description>
                  <text>Auto negotiation mode.</text>
                </description>
              </enum>
            </type>
            <default value="negotiation-auto"/>
          </leaf>
          <leaf name="portduplex">
            <description>
              <text>Port duplex mode.</text>
            </description>
            <type name="enumeration">
              <enum name="duplex-full">
                <value value="1"/>
                <description>
                  <text>Full duplex mode.</text>
                </description>
              </enum>
              <enum name="duplex-half">
                <value value="2"/>
                <description>
                  <text>Half duplex mode.</text>
                </description>
              </enum>
            </type>
          </leaf>
        </list>
      </container>
    </container>
  </container>
</module>
