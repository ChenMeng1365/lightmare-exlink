  <container name="state">
    <config value="false"/>
    <container name="temperatures">
      <description>
        <text>Enclosing container for the components in the system.</text>
      </description>
      <list name="temperature">
        <description>
          <text>List of components, keyed by component name.</text>
        </description>
        <key value="name"/>
        <leaf name="name">
          <description>
            <text>Device name for the component -- this will not be a configurable parameter on many implementations</text>
          </description>
          <type name="string"/>
        </leaf>
        <leaf name="alias">
          <type name="string"/>
        </leaf>
        <leaf name="instant">
          <description>
            <text>The instantaneous value of the statistic.</text>
          </description>
          <type name="decimal64">
            <fraction-digits value="1"/>
          </type>
          <units name="celsius"/>
        </leaf>
        <leaf name="avg">
          <description>
            <text>The arithmetic mean value of the statistic over the sampling period.</text>
          </description>
          <type name="decimal64">
            <fraction-digits value="1"/>
          </type>
          <units name="celsius"/>
        </leaf>
        <leaf name="min">
          <description>
            <text>The minimum value of the statistic over the sampling period.</text>
          </description>
          <type name="decimal64">
            <fraction-digits value="1"/>
          </type>
          <units name="celsius"/>
        </leaf>
        <leaf name="max">
          <description>
            <text>The maximum value of the statistic over the sampling period.</text>
          </description>
          <type name="decimal64">
            <fraction-digits value="1"/>
          </type>
          <units name="celsius"/>
        </leaf>
        <leaf name="interval">
          <description>
            <text>The time interval over which the min/max/average statistics are computed by the system.</text>
          </description>
          <type name="uint64"/>
          <units name="nanoseconds"/>
        </leaf>
        <leaf name="min-time">
          <description>
            <text>The absolute time at which the minimum value occurred.</text>
          </description>
          <type name="uint64"/>
          <units name="nanoseconds"/>
          <ncd:revise-date date="2019-07-24"/>
        </leaf>
        <leaf name="max-time">
          <description>
            <text>The absolute time at which the maximum value occurred.</text>
          </description>
          <type name="uint64"/>
          <units name="nanoseconds"/>
          <ncd:revise-date date="2019-07-24"/>
        </leaf>
        <leaf name="alarm-status">
          <description>
            <text>A value of true indicates the alarm has been raised or asserted.  The value should be false when the alarm is cleared.</text>
          </description>
          <type name="boolean"/>
        </leaf>
        <leaf name="alarm-threshold">
          <description>
            <text>The threshold value that was crossed for this alarm.</text>
          </description>
          <type name="uint32"/>
        </leaf>
        <leaf name="alarm-severity">
          <description>
            <text>The severity of the current alarm.</text>
          </description>
          <type name="enumeration">
            <enum name="UNKNOWN">
              <value value="0"/>
              <description>
                <text>Indicates that the severity level could not be determined. This level SHOULD be avoided.</text>
              </description>
            </enum>
            <enum name="MINOR">
              <value value="1"/>
              <description>
                <text>Indicates the existence of a non-service affecting fault condition and that corrective action should be taken in order to prevent a more serious (for example, service affecting) fault. Such a severity can be reported, for example, when the detected alarm condition is not currently degrading the capacity of the resource</text>
              </description>
            </enum>
            <enum name="WARNING">
              <value value="2"/>
              <description>
                <text>Indicates the detection of a potential or impending service affecting fault, before any significant effects have been felt. Action should be taken to further diagnose (if necessary) and correct the problem in order to prevent it from becoming a more serious service affecting fault.</text>
              </description>
            </enum>
            <enum name="MAJOR">
              <value value="3"/>
              <description>
                <text>Indicates that a service affecting condition has developed and an urgent corrective action is required. Such a severity can be reported, for example, when there is a severe degradation in the capability of the resource and its full capability must be restored.</text>
              </description>
            </enum>
            <enum name="CRITICAL">
              <value value="4"/>
              <description>
                <text>Indicates that a service affecting condition has occurred and an immediate corrective action is required. Such a severity can be reported, for example, when a resource becomes totally out of service and its capability must be restored.</text>
              </description>
            </enum>
          </type>
        </leaf>
      </list>
    </container>
