  <container name="configuration">
    <description>
      <text>RADIUS configuration.</text>
    </description>
    <container name="radius">
      <description>
        <text>RADIUS info.</text>
      </description>
      <container name="auth-groups">
        <description>
          <text>RADIUS authentication group info.</text>
        </description>
        <list name="auth-group">
          <description>
            <text>RADIUS auth group.</text>
          </description>
          <key value="group-name"/>
          <leaf name="group-name">
            <description>
              <text>RADIUS auth group name.</text>
            </description>
            <type name="string">
              <length value="1..31">
                <description>
                  <text>RADIUS auth group name.</text>
                </description>
              </length>
            </type>
          </leaf>
          <leaf name="algorithm">
            <description>
              <text>RADIUS auth group algorithm.</text>
            </description>
            <type name="enumeration">
              <enum name="first">
                <value value="0"/>
                <description>
                  <text>First.</text>
                </description>
              </enum>
              <enum name="round-robin">
                <value value="1"/>
                <description>
                  <text>Round-robin.</text>
                </description>
              </enum>
              <enum name="rollover-on-reject">
                <value value="2"/>
                <description>
                  <text>Rollover-on-reject.</text>
                </description>
              </enum>
            </type>
            <default value="first"/>
          </leaf>
          <leaf name="deadtime">
            <description>
              <text>RADIUS auth group deadtime.</text>
            </description>
            <type name="uint32">
              <range value="0..3600">
                <description>
                  <text>RADIUS auth group deadtime.</text>
                </description>
              </range>
            </type>
            <units name="minute"/>
            <default value="5"/>
          </leaf>
          <leaf name="max-retries">
            <description>
              <text>RADIUS auth group max-retries.</text>
            </description>
            <type name="uint8">
              <range value="1..255">
                <description>
                  <text>RADIUS auth group max-retries.</text>
                </description>
              </range>
            </type>
            <default value="3"/>
          </leaf>
          <leaf name="timeout">
            <description>
              <text>RADIUS auth group timeout.</text>
            </description>
            <type name="uint8">
              <range value="1..255">
                <description>
                  <text>RADIUS auth group timeout.</text>
                </description>
              </range>
            </type>
            <units name="second"/>
            <default value="3"/>
          </leaf>
          <leaf name="vendor">
            <description>
              <text>RADIUS auth group vendor.</text>
            </description>
            <type name="enumeration">
              <enum name="disable">
                <value value="0"/>
                <description>
                  <text>Disable.</text>
                </description>
              </enum>
              <enum name="enable">
                <value value="1"/>
                <description>
                  <text>Enable.</text>
                </description>
              </enum>
            </type>
            <default value="enable"/>
          </leaf>
          <leaf name="set-dscp-outer">
            <description>
              <text>RADIUS auth group set-dscp-outer.</text>
            </description>
            <type name="uint8">
              <range value="0..63">
                <description>
                  <text>RADIUS auth group set_dscp_outer.</text>
                </description>
              </range>
            </type>
          </leaf>
          <leaf name="class-as-car">
            <description>
              <text>RADIUS auth group class-as-car.</text>
            </description>
            <type name="enumeration">
              <enum name="disable">
                <value value="0"/>
                <description>
                  <text>Disable.</text>
                </description>
              </enum>
              <enum name="enable">
                <value value="1"/>
                <description>
                  <text>Enable.</text>
                </description>
              </enum>
            </type>
            <default value="disable"/>
          </leaf>
          <leaf name="dsl-vendor">
            <description>
              <text>RADIUS auth group dsl-vendor.</text>
            </description>
            <type name="enumeration">
              <enum name="disable">
                <value value="0"/>
                <description>
                  <text>Disable.</text>
                </description>
              </enum>
              <enum name="enable">
                <value value="1"/>
                <description>
                  <text>Enable.</text>
                </description>
              </enum>
            </type>
            <default value="disable"/>
          </leaf>
          <leaf name="send-rate-limit">
            <description>
              <text>RADIUS auth group send-rate-limit.</text>
            </description>
