          </description>
          <type name="enumeration">
            <enum name="nssa-external-link">
              <value value="0"/>
            </enum>
            <enum name="summary-link">
              <value value="1"/>
            </enum>
          </type>
        </leaf>
        <leaf name="opt-type">
          <description>
            <text>Operation type</text>
          </description>
          <type name="enumeration">
            <enum name="config">
              <value value="0"/>
            </enum>
            <enum name="delete">
              <value value="1"/>
            </enum>
          </type>
        </leaf>
        <leaf name="instance-number">
          <description>
            <text>OSPFv2 instance number</text>
          </description>
          <type name="uint32">
            <range value="0..31"/>
          </type>
          <ncd:revise-date date="2018-06-06"/>
        </leaf>
      </container>
