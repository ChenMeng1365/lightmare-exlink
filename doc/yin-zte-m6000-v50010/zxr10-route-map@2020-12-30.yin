                </type>
                <mandatory value="true"/>
              </leaf>
            </container>
            <container name="vrf">
              <description>
                <text>Set virtual route forwarding name.VRF, global and next-hop(or interface) can't co-exist in one sequence.</text>
              </description>
              <presence value="vrf is not Mandatory"/>
