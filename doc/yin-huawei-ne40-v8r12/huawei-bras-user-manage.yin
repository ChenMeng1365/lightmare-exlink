    <container name="access-backup-check-detect">
      <must condition="detect-count &gt;= fail-count"/>
      <description>
        <text>Configure backup detection interval and number of backup detections on a BAS interface.</text>
      </description>
      <leaf name="detect-interval">
        <type name="uint8">
          <range value="0..24"/>
        </type>
        <units name="h"/>
        <default value="8"/>
        <description>
          <text>Detection interval.</text>
        </description>
      </leaf>
      <leaf name="detect-count">
        <type name="uint8">
          <range value="1..255"/>
        </type>
        <default value="21"/>
        <description>
          <text>Total number of detections in a detection period.</text>
        </description>
      </leaf>
      <leaf name="fail-count">
        <type name="uint8">
          <range value="1..255"/>
        </type>
        <default value="14"/>
        <description>
          <text>Threshold for the number of detection failures over which an alarm is generated.</text>
        </description>
