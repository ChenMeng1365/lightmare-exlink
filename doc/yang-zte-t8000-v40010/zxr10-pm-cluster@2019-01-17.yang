module zxr10-pm-cluster {
  namespace 'http://www.zte.com.cn/zxr10/netconf/schema/rosng/pm-cluster'
            ;
  prefix pm-cluster ;
  yang-version 1 ;
  import zxr10-netconf-defs {
    prefix ncd ;
  }
  organization 'ZTE Corporation'
               ;
  contact 'ZTE Corporation  '+"\n"
        + '   Nanjing Institute of ZTE Corporation '+"\n"
        + '   No.68 Zijinghua Rd. Yuhuatai District, Nanjing, China '+"\n"
        + '   Tel: +86-25-52870000'
          ;
  revision 2019-01-17 {
    description 'shelf range 0-255.slot range 0-255.'
                ;
  }
  revision 2017-07-01 {
    description 'delete the mode below mode of node run mode  '+"\n"
              + '  1+2     LCC shelf number:1-2, CFC shelf number:1, CSFU numbe'
              + 'r:8'+"\n"
              + '  1+4     LCC shelf number:1-4, CFC shelf number:1, CSFU numbe'
              + 'r:16'+"\n"
              + '  2+8     LCC shelf number:1-8, CFC shelf number:2, CSFU numbe'
              + 'r:32'+"\n"
              + '  4+16    LCC shelf number:1-16, CFC shelf number:4, CSFU numb'
              + 'er:64'
                ;
  }
  revision 2017-01-24 {
    description 'delete 2+2 enum of run-mode.'
                ;
  }
  revision 2015-05-20 ;
  container state {
    config false ;
    container cluster {
      container cplink-topologys {
        list cplink-topology {
          key 'source-shelf source-slot source-subslot source-port'
              ;
          leaf source-shelf {
            description 'Source shelf number'
                        ;
            type uint8 {
              range 0..79 ;
            }
          }
          leaf source-slot {
            description 'Source slot number'
                        ;
            type uint8 {
              range 0..27 ;
            }
          }
          leaf source-subslot {
            description 'Source subslot number'
                        ;
            type uint8 ;
          }
          leaf source-port {
            description 'Source port number'
                        ;
            type uint8 ;
          }
          leaf source-restype {
            description 'Source resource type'
                        ;
            type uint16 ;
          }
          leaf source-ifname {
            description 'Source interface name'
                        ;
            type string ;
          }
          leaf dest-shelf {
            description 'Destination shelf number'
                        ;
            type uint8 ;
          }
          leaf dest-slot {
            description 'Destination slot number'
                        ;
            type uint8 ;
          }
          leaf dest-subslot {
            description 'Destination subslot number'
                        ;
            type uint8 ;
          }
          leaf dest-port {
