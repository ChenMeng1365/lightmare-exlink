      <container name="peer-groups">
        <description>
          <text>SIB peer group list.</text>
        </description>
        <list name="peer-group">
          <description>
            <text>SIB peer group configuration.</text>
          </description>
          <key value="peer-group-name"/>
          <leaf name="peer-group-type">
            <description>
              <text>Peer group type.</text>
            </description>
            <type name="enumeration">
              <enum name="global">
                <value value="1"/>
              </enum>
              <enum name="default">
                <value value="3"/>
              </enum>
            </type>
            <default value="default"/>
          </leaf>
          <leaf name="peer-group-name">
            <description>
              <text>Peer group name.</text>
            </description>
            <type name="string">
              <length value="1..31"/>
            </type>
          </leaf>
          <container name="links">
            <description>
              <text>Peer configurations.</text>
            </description>
            <list name="link">
              <description>
                <text>SIB Peer Link configuration.</text>
              </description>
              <key value="peer-type"/>
              <leaf name="peer-type">
                <description>
                  <text>Peer type</text>
                </description>
                <type name="enumeration">
                  <enum name="data">
                    <value value="2"/>
                    <description>
                      <text>peer for user data</text>
                    </description>
                  </enum>
                  <enum name="protocol">
                    <value value="3"/>
                    <description>
                      <text>peer for protocol data</text>
                    </description>
                  </enum>
                  <enum name="one-link">
                    <value value="1"/>
                    <description>
                      <text>nomal peer</text>
                    </description>
                  </enum>
                </type>
              </leaf>
              <leaf name="local-port">
                <description>
                  <text>Peer local port</text>
                </description>
                <type name="uint32">
                  <range value="1025..65535"/>
                </type>
                <mandatory value="true"/>
              </leaf>
              <leaf name="local-ip">
                <description>
                  <text>Peer localIP</text>
                </description>
                <type name="ncd:ipv4-address"/>
                <mandatory value="true"/>
              </leaf>
              <leaf name="remote-port">
                <description>
                  <text>Peer remote port</text>
                </description>
                <type name="uint32">
                  <range value="1025..65535"/>
                </type>
                <mandatory value="true"/>
              </leaf>
              <leaf name="remote-ip">
                <description>
                  <text>Peer remote IP</text>
                </description>
                <type name="ncd:ipv4-address"/>
                <mandatory value="true"/>
              </leaf>
              <leaf name="vrf-name">
                <description>
                  <text>Peer VRF name</text>
                </description>
                <type name="string">
                  <length value="1..32"/>
                </type>
              </leaf>
              <leaf name="tcp-mss">
                <description>
                  <text>Peer tcp mss.</text>
                </description>
                <type name="uint32">
                  <range value="68..10000"/>
                </type>
                <default value="1400"/>
              </leaf>
            </list>
            <container name="default-link">
              <status value="obsolete">
                <ncd:revise-date date="2020-04-26"/>
              </status>
              <description>
                <text>SIB Peer Link configuration.</text>
              </description>
              <presence value="default-link"/>
              <leaf name="local-port">
                <description>
                  <text>Peer local port</text>
                </description>
                <type name="uint32">
                  <range value="1025..65535"/>
                </type>
                <mandatory value="true"/>
              </leaf>
              <leaf name="local-ip">
                <description>
                  <text>Peer localIP</text>
                </description>
                <type name="ncd:ipv4-address"/>
                <mandatory value="true"/>
              </leaf>
              <leaf name="remote-port">
                <description>
                  <text>Peer remote port</text>
                </description>
                <type name="uint32">
                  <range value="1025..65535"/>
                </type>
                <mandatory value="true"/>
              </leaf>
              <leaf name="remote-ip">
                <description>
                  <text>Peer remote IP</text>
                </description>
                <type name="ncd:ipv4-address"/>
                <mandatory value="true"/>
              </leaf>
              <leaf name="vrf-name">
                <description>
                  <text>Peer VRF name</text>
                </description>
                <type name="string">
                  <length value="0..35"/>
                </type>
                <default value=""/>
              </leaf>
              <leaf name="tcp-mss">
                <description>
                  <text>Peer tcp mss.</text>
                </description>
                <type name="uint32">
                  <range value="68..10000"/>
                </type>
                <default value="1400"/>
              </leaf>
            </container>
          </container>
        </list>
        <ncd:revise-date date="2020-03-10"/>
      </container>
      <container name="instances">
        <description>
          <text>SIB instance list.</text>
        </description>
        <list name="instance">
          <description>
            <text>SIB instance common configuration.</text>
          </description>
          <key value="instance-id"/>
          <leaf name="instance-id">
            <description>
              <text>Instance ID.</text>
            </description>
            <type name="uint32">
              <range value="1..255"/>
            </type>
          </leaf>
          <leaf name="role">
            <status value="obsolete">
              <ncd:revise-date date="2020-07-22"/>
            </status>
            <description>
              <text>Role.
0: no role.
1:role master.
2:role slave.</text>
            </description>
            <type name="uint32">
              <range value="0..2"/>
            </type>
            <default value="0"/>
          </leaf>
          <leaf name="cp-control">
            <description>
              <text>Configure whether hot-standby state is determined by control plane.</text>
            </description>
            <type name="enumeration">
              <enum name="enable">
                <value value="1"/>
                <description>
                  <text>enable cp-control</text>
                </description>
              </enum>
              <enum name="disable">
                <value value="0"/>
                <description>
                  <text>disable cp-control</text>
                </description>
              </enum>
            </type>
            <default value="disable"/>
            <ncd:revise-date date="2020-05-18"/>
          </leaf>
          <leaf name="shutdown-flag">
            <description>
              <text>Shutdown flag.
0:no shutdown.
1:shutdown.</text>
            </description>
            <type name="uint8"/>
            <default value="0"/>
          </leaf>
          <leaf name="description">
            <description>
              <text>Description.</text>
            </description>
            <type name="string">
              <length value="1..31"/>
            </type>
          </leaf>
          <container name="loadshare-interfaces">
            <description>
              <text>Load share interface-related configurations.</text>
            </description>
            <list name="interface">
              <description>
                <text>SIB interface common configuration.Dependent on the interface.</text>
              </description>
              <key value="interface-name"/>
              <leaf name="interface-name">
                <description>
                  <text>Interface name.</text>
                </description>
                <type name="ncd:ifname"/>
              </leaf>
              <leaf name="mark-name">
                <description>
                  <text>Mark name.</text>
                </description>
                <type name="string">
                  <length value="1..31"/>
                </type>
              </leaf>
              <leaf name="parity">
                <description>
                  <text>Parity identification</text>
                </description>
                <type name="enumeration">
                  <enum name="no-parity">
                    <value value="0"/>
                    <description>
                      <text>Configure interface without user mac parity</text>
                    </description>
                  </enum>
                  <enum name="odd">
                    <value value="1"/>
                    <description>
                      <text>Configure interface with odd user mac</text>
                    </description>
                  </enum>
                  <enum name="even">
                    <value value="2"/>
                    <description>
                      <text>Configure interface with even user mac</text>
                    </description>
                  </enum>
                </type>
                <default value="no-parity"/>
              </leaf>
            </list>
            <ncd:revise-date date="2020-02-20"/>
          </container>
          <container name="interfaces">
            <description>
              <text>Interface-related configurations.</text>
            </description>
            <list name="interface">
              <description>
                <text>SIB interface common configuration.Dependent on the interface.</text>
              </description>
              <key value="interface-name"/>
              <leaf name="interface-name">
                <description>
                  <text>Interface name.</text>
                </description>
                <type name="ncd:ifname"/>
              </leaf>
              <leaf name="mark-name">
                <description>
                  <text>Mark name.</text>
                </description>
                <type name="string">
                  <length value="1..31"/>
                </type>
              </leaf>
            </list>
          </container>
          <leaf name="ipv6-linklocal">
            <description>
              <text>ipv6 linklocal</text>
            </description>
            <type name="ncd:ipv6-address"/>
            <default value="::"/>
            <ncd:revise-date date="2019-12-04"/>
          </leaf>
          <container name="switch-interfaces">
            <status value="obsolete">
              <ncd:revise-date date="2020-07-22"/>
