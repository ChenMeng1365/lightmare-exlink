                            <text>List of AIS VLAN.</text>
                          </description>
                          <list name="vlan">
                            <key value="pe-vid"/>
                            <description>
                              <text>Configure the instance of AIS VLAN.</text>
                            </description>
                            <leaf name="pe-vid">
                              <type name="uint16">
                                <range value="1..4094"/>
                              </type>
                              <description>
                                <text>Configure the PE VLAN of AIS.</text>
                              </description>
                            </leaf>
                            <leaf name="ce-vid">
                              <type name="pub-type:vlan-range"/>
                              <mandatory value="true"/>
