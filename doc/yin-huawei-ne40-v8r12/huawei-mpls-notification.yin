  <typedef name="mpls-resource-type">
    <type name="enumeration">
      <enum name="auto-bypass-tunnelif">
        <value value="1"/>
        <description>
          <text>Indicates that the resource is auto bypass tunnel interface.</text>
        </description>
      </enum>
      <enum name="p2mp-auto-tunnelif">
        <value value="2"/>
        <description>
