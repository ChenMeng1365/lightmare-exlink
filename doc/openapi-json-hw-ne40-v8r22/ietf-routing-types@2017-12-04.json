    "definitions": {
        "mpls-label-stack": {
            "description": "This grouping specifies an MPLS label stack.  The label\nstack is encoded as a list of label stack entries.  The\nlist key is an identifier that indicates the relative\nordering of each entry, with the lowest-value identifier\ncorresponding to the top of the label stack.",
            "properties": {
                "mpls-label-stack": {
                    "description": "Container for a list of MPLS label stack entries.",
                    "properties": {
                        "entry": {
                            "description": "List of MPLS label stack entries.",
                            "type": "array",
                            "items": {
                                "properties": {
                                    "id": {
                                        "type": "string",
                                        "description": "Identifies the entry in a sequence of MPLS label\nstack entries.  An entry with a smaller identifier\nvalue precedes an entry with a larger identifier\nvalue in the label stack.  The value of this ID has\nno semantic meaning other than relative ordering\nand referencing the entry."
                                    },
                                    "label": {
                                        "type": "string",
                                        "description": "Label value."
                                    },
                                    "ttl": {
                                        "type": "string",
                                        "description": "Time to Live (TTL)."
                                    },
                                    "traffic-class": {
                                        "type": "string",
                                        "description": "Traffic Class (TC)."
                                    }
                                }
                            },
                            "x-key": "id"
                        }
                    }
                }
            }
        },
        "vpn-route-targets": {
            "description": "A grouping that specifies Route Target import-export rules\nused in BGP-enabled VPNs.",
            "properties": {
                "vpn-target": {
                    "description": "List of Route Targets.",
                    "type": "array",
                    "items": {
                        "properties": {
                            "route-target": {
                                "type": "string",
                                "description": "Route Target value."
                            },
                            "route-target-type": {
                                "type": "string",
                                "enum": [
                                    "import",
                                    "export",
                                    "both"
                                ],
                                "description": "Import/export type of the Route Target."
                            }
                        }
                    },
                    "x-key": "route-target"
                }
            }
        }
    }
}