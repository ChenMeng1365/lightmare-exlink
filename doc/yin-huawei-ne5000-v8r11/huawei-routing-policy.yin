<?xml version="1.0" encoding="UTF-8"?>
<module name="huawei-routing-policy"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:rtp="urn:huawei:yang:huawei-routing-policy"
        xmlns:inet="urn:ietf:params:xml:ns:yang:ietf-inet-types"
        xmlns:ext="urn:huawei:yang:huawei-extension"
        xmlns:ifm="urn:huawei:yang:huawei-ifm"
        xmlns:tnlm="urn:huawei:yang:huawei-tunnel-management"
        xmlns:pub-type="urn:huawei:yang:huawei-pub-type">
  <yang-version value="1"/>
  <namespace uri="urn:huawei:yang:huawei-routing-policy"/>
  <prefix value="rtp"/>
  <import module="ietf-inet-types">
    <prefix value="inet"/>
  </import>
  <import module="huawei-extension">
    <prefix value="ext"/>
  </import>
  <import module="huawei-ifm">
    <prefix value="ifm"/>
  </import>
  <import module="huawei-tunnel-management">
    <prefix value="tnlm"/>
  </import>
  <import module="huawei-pub-type">
    <prefix value="pub-type"/>
  </import>
  <include module="huawei-routing-policy-type"/>
  <organization>
    <text>Huawei Technologies Co., Ltd.</text>
  </organization>
  <contact>
    <text>Huawei Industrial Base
Bantian, Longgang
Shenzhen 518129
People's Republic of China
Website: http://www.huawei.com
Email: support@huawei.com</text>
  </contact>
  <description>
    <text>Routing policies are used to filter routes and control the receiving and advertising of routes. By changing the route attributes, such as reachability, you can change the path through which the traffic passes.</text>
  </description>
  <revision date="2019-04-27">
    <description>
      <text>Initial revision.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <ext:task-name value="route-base"/>
  <container name="routing-policy">
    <description>
      <text>Routing policies are used to filter routes and control the receiving and advertising of routes. By changing the route attributes, such as reachability, you can change the path through which the traffic passes.</text>
    </description>
    <container name="ext-community-soo-filters">
      <description>
        <text>List of extended community attribute SoO filters. Routes are filtered based on the SoO extended community attributes of routes.</text>
      </description>
      <list name="ext-community-soo-filter">
        <key value="name"/>
        <max-elements value="65535"/>
        <description>
          <text>Configure an SoO extended community filter, which filters routes based on the SoO extended community attribute.</text>
        </description>
        <leaf name="name">
          <type name="string">
            <length value="1..51"/>
          </type>
          <description>
            <text>Name of an extended community SoO filter. The name is a string and cannot contain spaces.</text>
          </description>
        </leaf>
        <leaf name="filter-type">
          <ext:operation-exclude value="update"/>
          <type name="community-filter-type"/>
          <mandatory value="true"/>
          <description>
            <text>Type of an extended community SoO filter, which can be basic or advanced.</text>
          </description>
        </leaf>
        <container name="basic-nodes">
          <when condition="../filter-type='basic'"/>
          <description>
            <text>List of basic extended community SoO filter nodes. You can use the SoO extended community ID as a matching condition.</text>
          </description>
          <list name="basic-node">
            <key value="sequence"/>
            <min-elements value="1"/>
            <max-elements value="65535"/>
            <description>
              <text>Configure a node for a basic SoO extended community filter, with the SoO extended community attribute specified as a matching rule.</text>
            </description>
            <leaf name="sequence">
              <type name="uint32">
                <range value="1..4294967295"/>
              </type>
              <description>
                <text>Sequence number of a node.</text>
              </description>
            </leaf>
            <leaf name="match-mode">
              <type name="match-mode-type"/>
              <mandatory value="true"/>
              <description>
                <text>Matching mode of nodes.</text>
              </description>
            </leaf>
            <leaf-list name="community-member">
              <type name="ext-community-type"/>
              <min-elements value="1"/>
              <max-elements value="16"/>
              <description>
                <text>Configure a value for the SoO attribute in an SoO extended community filter. The value can be in any of the following formats:
(1) as-number:nn (as-number is an integer ranging from 0 to 65535, and nn is an integer ranging from 0 to 4294967295)
(2) IPv4-address:nn
(3) as-number.as-number:nn
(4) as-number:nn (as-number is an integer ranging from 65536 to 4294967295, and nn is an integer ranging from 0 to 65535.</text>
              </description>
            </leaf-list>
          </list>
        </container>
        <container name="advanced-nodes">
          <when condition="../filter-type='advanced'"/>
          <description>
            <text>List of advanced extended community SoO filter nodes. You can use a regular expression as a matching condition.</text>
          </description>
          <list name="advanced-node">
            <key value="sequence"/>
            <min-elements value="1"/>
            <max-elements value="65535"/>
            <description>
              <text>Configure a node in an advanced extended community SoO filter, with a regular expression specified as a matching rule.</text>
            </description>
            <leaf name="sequence">
              <type name="uint32">
                <range value="1..4294967295"/>
              </type>
              <description>
                <text>Sequence number of a node.</text>
              </description>
            </leaf>
            <leaf name="match-mode">
              <type name="match-mode-type"/>
              <mandatory value="true"/>
              <description>
                <text>Matching mode of nodes.</text>
              </description>
            </leaf>
            <leaf name="regular">
              <type name="string">
                <length value="1..1024"/>
              </type>
              <mandatory value="true"/>
              <description>
                <text>Regular expression that is used for matching.</text>
              </description>
            </leaf>
          </list>
        </container>
      </list>
    </container>
    <container name="community-filters">
      <description>
        <text>List of community attribute filters. Routes are filtered based on the community attributes of routes.</text>
      </description>
      <list name="community-filter">
        <must condition="(string(number(name))='NaN' and filter-type) or (number(name)&gt;=1 and number(name)&lt;=99 and filter-type='basic') or (number(name)&gt;= 100 and number(name)&lt;=199 and filter-type='advanced')"/>
        <key value="name"/>
        <max-elements value="65535"/>
        <description>
          <text>Configure a community attribute filter, which filters routes based on community attributes. Different nodes of a community filter cannot be configured with the same filtering rules.</text>
        </description>
        <leaf name="name">
          <type name="community-filter-name-type"/>
          <description>
            <text>Name of a community filter, which can be a numeral or a string. The ID of a basic community attribute filter is an integer ranging from 1 to 99; the ID of an advanced community attribute filter is an integer ranging from 100 to 199. The name of a community attribute filter is a string of 1 to 51 characters. The string cannot contain only digits.</text>
          </description>
        </leaf>
        <leaf name="filter-type">
          <ext:operation-exclude value="update"/>
          <type name="community-filter-type"/>
          <mandatory value="true"/>
          <description>
            <text>Type of a community filter, which can be basic or advanced.</text>
          </description>
        </leaf>
        <container name="basic-nodes">
          <when condition="../filter-type='basic'"/>
          <description>
            <text>List of basic community filter nodes. You can use the community ID or the attribute of a known community as a matching condition.</text>
          </description>
          <list name="basic-node">
            <key value="sequence"/>
            <min-elements value="1"/>
            <max-elements value="65535"/>
            <description>
              <text>Configure a node in a basic community filter, with a community number or well-known community attribute as a matching rule.</text>
            </description>
            <leaf name="sequence">
              <type name="uint32">
                <range value="1..4294967295"/>
              </type>
              <description>
                <text>Sequence number of a node.</text>
              </description>
            </leaf>
            <leaf name="match-mode">
              <type name="match-mode-type"/>
              <mandatory value="true"/>
              <description>
                <text>Matching mode of nodes.</text>
              </description>
            </leaf>
            <container name="community-members">
              <description>
                <text>List of community attributes.</text>
              </description>
              <list name="community-member">
                <key value="value"/>
                <max-elements value="20"/>
                <description>
                  <text>Configure a community attribute.</text>
                </description>
                <leaf name="value">
                  <type name="community-type"/>
                  <description>
                    <text>Community attribute value.</text>
                  </description>
                </leaf>
                <leaf name="internet-strict-match">
                  <when condition="../value='internet'"/>
                  <type name="boolean"/>
                  <default value="false"/>
                  <description>
                    <text>Enable/disable strict matching for routes with the internet community attribute.</text>
                  </description>
                </leaf>
              </list>
            </container>
          </list>
        </container>
        <container name="advanced-nodes">
          <when condition="../filter-type='advanced'"/>
          <description>
            <text>List of advanced community filter nodes. You can use a regular expression as a matching condition.</text>
          </description>
          <list name="advanced-node">
            <key value="sequence"/>
            <min-elements value="1"/>
            <max-elements value="65535"/>
            <description>
              <text>Configure a node in an advanced community filter, with a regular expression specified as a matching rule.</text>
            </description>
            <leaf name="sequence">
              <type name="uint32">
                <range value="1..4294967295"/>
              </type>
              <description>
                <text>Sequence number of a node.</text>
              </description>
            </leaf>
            <leaf name="match-mode">
              <type name="match-mode-type"/>
              <mandatory value="true"/>
              <description>
                <text>Matching mode of nodes.</text>
              </description>
            </leaf>
            <leaf name="regular">
              <type name="string">
                <length value="1..1024"/>
              </type>
              <mandatory value="true"/>
              <description>
                <text>Regular expression that is used for matching.</text>
              </description>
            </leaf>
          </list>
        </container>
      </list>
    </container>
    <container name="ext-community-filters">
      <description>
        <text>List of extended community filters. You can use the route-target extended community as a matching condition.</text>
      </description>
      <list name="ext-community-filter">
        <must condition="(string(number(name))='NaN' and filter-type) or (number(name)&gt;=1 and number(name)&lt;=199 and filter-type='basic') or (number(name)&gt;= 200 and number(name)&lt;=399 and filter-type='advanced')"/>
        <key value="name"/>
        <max-elements value="65535"/>
        <description>
          <text>Configure an extended community filter, with a VPN-Target extended community specified as a matching rule.</text>
        </description>
        <leaf name="name">
          <type name="ext-community-filter-name-type"/>
          <description>
            <text>Name of an extended community filter, which can be a numeral or a string. The ID of a basic extended community attribute filter is an integer ranging from 1 to 199; the ID of an advanced extended community attribute filter is an integer ranging from 200 to 399. The name of an extended community attribute filter is a string of 1 to 51 characters. The string cannot contain only digits.</text>
          </description>
        </leaf>
        <leaf name="filter-type">
          <ext:operation-exclude value="update"/>
          <type name="community-filter-type"/>
          <mandatory value="true"/>
          <description>
            <text>Type of an extended community filter, which can be basic or advanced.</text>
          </description>
        </leaf>
        <container name="basic-nodes">
          <when condition="../filter-type='basic'"/>
          <description>
            <text>List of extended community filter nodes.</text>
          </description>
          <list name="basic-node">
            <key value="sequence"/>
            <min-elements value="1"/>
            <max-elements value="65535"/>
            <description>
              <text>Configure a node for an extended community filter.</text>
            </description>
            <leaf name="sequence">
              <type name="uint32">
                <range value="1..4294967295"/>
              </type>
              <description>
                <text>Sequence number of a node.</text>
              </description>
            </leaf>
            <leaf name="match-mode">
              <type name="match-mode-type"/>
              <mandatory value="true"/>
              <description>
                <text>Matching mode of nodes.</text>
              </description>
            </leaf>
            <container name="ext-community-members">
              <description>
                <text>List of nodes in an extended community filter.</text>
              </description>
              <list name="ext-community-member">
                <key value="value"/>
                <min-elements value="1"/>
                <max-elements value="16"/>
                <description>
                  <text>Configure a node for an extended community filter.</text>
                </description>
                <leaf name="value">
                  <type name="ext-community-type"/>
                  <description>
                    <text>Extended community attribute value. The format are as follows:
(1)as-number : nn ('as-number' ranges from 0 to 65535 and 'nn' ranges from 0 to 4294967295).
(2)IPv4-address : nn ('IPv4-address' is an IPv4 address and 'nn' ranges from 0 to 65535).
(3)as-number.as-number:nn ('as-number' ranges from 0 to 65535 and 'nn' ranges from 0 to 65535).
(4)as-number : nn ('as-number' ranges from 65536 to 4294967295 and 'nn' ranges from 0 to 65535).</text>
                  </description>
                </leaf>
              </list>
            </container>
          </list>
        </container>
        <container name="advanced-nodes">
          <when condition="../filter-type='advanced'"/>
          <description>
            <text>List of advanced extended community filter nodes. You can use a regular expression as a matching condition.</text>
          </description>
          <list name="advanced-node">
            <key value="sequence"/>
            <min-elements value="1"/>
            <max-elements value="65535"/>
            <description>
              <text>Configure a node in an advanced extended community filter, with a regular expression specified as a matching rule.</text>
            </description>
            <leaf name="sequence">
              <type name="uint32">
                <range value="1..4294967295"/>
              </type>
              <description>
                <text>Sequence number of a node.</text>
              </description>
            </leaf>
            <leaf name="match-mode">
              <type name="match-mode-type"/>
              <mandatory value="true"/>
              <description>
                <text>Matching mode of nodes.</text>
              </description>
            </leaf>
            <leaf name="regular">
              <type name="string">
                <length value="1..1024"/>
              </type>
              <mandatory value="true"/>
              <description>
                <text>Regular expression that is used for matching.</text>
              </description>
            </leaf>
          </list>
        </container>
      </list>
    </container>
    <container name="ipv4-prefix-filters">
      <description>
        <text>List of IPv4 prefix filters.</text>
      </description>
      <list name="ipv4-prefix-filter">
        <key value="name"/>
        <max-elements value="100000"/>
        <description>
          <text>Configure an IPv4 prefix list, which contains a group of route filtering rules. Different nodes of an IPv4 prefix filter cannot be configured with the same filtering rules.</text>
        </description>
        <leaf name="name">
          <type name="string">
            <length value="1..169"/>
          </type>
          <description>
            <text>Name of an IPv4 prefix filter.</text>
          </description>
        </leaf>
        <leaf name="description">
          <type name="string">
            <length value="1..80"/>
          </type>
          <description>
            <text>Description of prefix filter.</text>
          </description>
        </leaf>
        <leaf name="permit-count">
          <type name="uint32">
            <range value="0..65535"/>
          </type>
          <default value="0"/>
          <config value="false"/>
          <description>
            <text>Number of routes that match the route-policy.</text>
          </description>
        </leaf>
        <leaf name="deny-count">
          <type name="uint32">
            <range value="0..65535"/>
          </type>
          <default value="0"/>
          <config value="false"/>
          <description>
            <text>Number of routes that do not match the route-policy.</text>
          </description>
        </leaf>
        <container name="nodes">
          <description>
            <text>List of IPv4 prefix filter nodes.</text>
          </description>
          <list name="node">
            <must condition="(not(masklength-lower) and not(masklength-upper)) or (masklength &lt;= masklength-lower and masklength-lower &lt;= masklength-upper)"/>
            <key value="sequence"/>
            <max-elements value="100000"/>
            <description>
              <text>Configure a node in an IPv4 prefix list. You can specify a prefix range and a mask length range in the node to match the destination network segment addresses or next-hop addresses in routes. There must be at least one node in a filter, min-elements 1. An IPv4 address and its mask must strictly match.</text>
            </description>
            <leaf name="sequence">
              <type name="uint32">
                <range value="1..4294967295"/>
              </type>
              <description>
                <text>Sequence number of a node.</text>
              </description>
            </leaf>
            <leaf name="match-mode">
              <type name="match-mode-type"/>
              <mandatory value="true"/>
              <description>
                <text>Matching mode of nodes.</text>
              </description>
            </leaf>
            <leaf name="ip-address">
              <type name="inet:ipv4-address-no-zone"/>
              <mandatory value="true"/>
              <description>
                <text>IPv4 address.</text>
              </description>
            </leaf>
            <leaf name="masklength">
              <type name="uint8">
                <range value="0..32"/>
              </type>
              <mandatory value="true"/>
              <description>
                <text>Mask length.</text>
              </description>
            </leaf>
            <leaf name="match-network">
              <type name="boolean"/>
              <must condition="../ip-address='0.0.0.0' or ../match-network='false'"/>
              <default value="false"/>
              <description>
                <text>Enable/disable matching of network segment routes with the IPv4 prefix being 0.0.0.0.</text>
              </description>
            </leaf>
            <leaf name="masklength-lower">
              <type name="uint8">
                <range value="0..32"/>
              </type>
              <description>
                <text>If both the IP address and mask length match, this parameter is used to specify the range lower of IP prefixes.</text>
              </description>
            </leaf>
            <leaf name="masklength-upper">
              <type name="uint8">
                <range value="1..32"/>
              </type>
              <description>
                <text>If both the IP address and mask length match, this parameter is used to specify the range upper of IP prefixes.</text>
              </description>
            </leaf>
          </list>
        </container>
      </list>
    </container>
    <container name="ipv6-prefix-filters">
      <description>
        <text>List of IPv6 prefix filters.</text>
      </description>
      <list name="ipv6-prefix-filter">
        <key value="name"/>
        <max-elements value="100000"/>
        <description>
          <text>Configure an IPv6 prefix list, which contains a group of route filtering rules. Different nodes of an IPv6 prefix filter cannot be configured with the same filtering rules.</text>
        </description>
        <leaf name="name">
          <type name="string">
            <length value="1..169"/>
          </type>
          <description>
            <text>Name of an IPv6 prefix filter. The name is a string and cannot contain spaces.</text>
          </description>
        </leaf>
        <leaf name="description">
          <type name="string">
            <length value="1..80"/>
          </type>
          <description>
            <text>Description of prefix filter.</text>
          </description>
        </leaf>
        <leaf name="permit-count">
          <type name="uint32">
            <range value="0..65535"/>
          </type>
          <default value="0"/>
          <config value="false"/>
          <description>
            <text>Number of routes that match the route-policy.</text>
          </description>
        </leaf>
        <leaf name="deny-count">
          <type name="uint32">
            <range value="0..65535"/>
          </type>
          <default value="0"/>
          <config value="false"/>
          <description>
            <text>Number of routes that do not match the route-policy.</text>
          </description>
        </leaf>
        <container name="nodes">
          <description>
            <text>List of IPv6 prefix filter nodes.</text>
          </description>
          <list name="node">
            <must condition="(not(masklength-lower) and not(masklength-upper)) or (masklength &lt;= masklength-lower and masklength-lower &lt;= masklength-upper)"/>
            <key value="sequence"/>
            <max-elements value="100000"/>
            <description>
              <text>Configure a node in an IPv6 prefix list. You can specify a prefix range and a mask length range in the node to match the destination network segment addresses or next-hop addresses in routes. There must be at least one node in a filter, min-elements 1. An IPv6 address and its mask must strictly match.</text>
            </description>
            <leaf name="sequence">
              <type name="uint32">
                <range value="1..4294967295"/>
              </type>
              <description>
                <text>Sequence number of a node.</text>
              </description>
            </leaf>
            <leaf name="match-mode">
              <type name="match-mode-type"/>
              <mandatory value="true"/>
              <description>
                <text>Matching mode of nodes.</text>
              </description>
            </leaf>
            <leaf name="ipv6-address">
              <type name="inet:ipv6-address-no-zone"/>
              <mandatory value="true"/>
              <description>
                <text>IPv6 address.</text>
              </description>
            </leaf>
            <leaf name="masklength">
              <type name="uint8">
                <range value="0..128"/>
              </type>
              <mandatory value="true"/>
              <description>
                <text>Mask length.</text>
              </description>
            </leaf>
            <leaf name="match-network">
              <type name="boolean"/>
              <must condition="../match-network='false' or ../ipv6-address='::'"/>
              <default value="false"/>
              <description>
                <text>Enable/disable matching of network segment routes with the IPv6 prefix being ::.</text>
              </description>
            </leaf>
            <leaf name="masklength-lower">
              <type name="uint8">
                <range value="0..128"/>
              </type>
              <description>
                <text>If both the IP address and mask length match, this parameter is used to specify the range lower of IP prefixes.</text>
              </description>
            </leaf>
            <leaf name="masklength-upper">
              <type name="uint8">
                <range value="1..128"/>
              </type>
              <description>
                <text>If both the IP address and mask length match, this parameter is used to specify the range upper of IP prefixes.</text>
              </description>
            </leaf>
          </list>
        </container>
      </list>
    </container>
    <container name="ext-community-segmented-nexthop-filters">
      <description>
        <text>List of segmented next-hop extended community filters, which are used to filter routes based on segmented-nh attributes.</text>
      </description>
      <list name="ext-community-segmented-nexthop-filter">
        <key value="name"/>
        <max-elements value="65535"/>
        <description>
          <text>Configure a segmented next-hop extended community filter, which is used to filter routes based on the segmented-nh attribute.</text>
        </description>
        <leaf name="name">
          <type name="string">
            <length value="1..51"/>
          </type>
          <description>
            <text>Name of a segmented next-hop extended community filter.</text>
          </description>
        </leaf>
        <leaf name="filter-type">
          <ext:operation-exclude value="update"/>
          <type name="community-filter-type"/>
          <mandatory value="true"/>
          <description>
            <text>Type of a segmented next-hop extended community filter, which can be basic or advanced. Regular expressions can be used to configure matching rules for advanced extended community filters, but not for basic extended community filters.</text>
          </description>
        </leaf>
        <container name="basic-nodes">
          <when condition="../filter-type='basic'"/>
          <description>
            <text>List of nodes in a basic segmented next-hop extended community filter, with segmented-nh attributes used as matching rules.</text>
          </description>
          <list name="basic-node">
            <key value="sequence"/>
            <min-elements value="1"/>
            <max-elements value="65535"/>
            <description>
              <text>Configure a node in a basic segmented next-hop extended community filter, with a segmented-nh attribute used as a matching rule.</text>
            </description>
            <leaf name="sequence">
              <type name="uint32">
                <range value="1..4294967295"/>
              </type>
              <description>
                <text>Sequence number of a node in a basic segmented next-hop extended community filter.</text>
              </description>
            </leaf>
            <leaf name="match-mode">
              <type name="match-mode-type"/>
              <mandatory value="true"/>
              <description>
                <text>Matching mode of a node in a basic segmented next-hop extended community filter.</text>
              </description>
            </leaf>
            <container name="ext-community-segmented-nexthop-members">
              <description>
                <text>List of values of segmented-nh attributes.</text>
              </description>
              <list name="ext-community-segmented-nexthop-member">
                <key value="value"/>
                <min-elements value="1"/>
                <max-elements value="16"/>
                <description>
                  <text>Configure a value for a segmented next-hop extended community filter.</text>
                </description>
                <leaf name="value">
                  <type name="ext-community-type"/>
                  <description>
                    <text>Attribute value of a segmented next-hop extended community filter, which can be in any of the following formats:
(1) as-number:nn
(2) IPv4-address: nn
(3) as-number.as-number:nn
(4) as-number:nn.</text>
                  </description>
                </leaf>
              </list>
            </container>
          </list>
        </container>
        <container name="advanced-nodes">
          <when condition="../filter-type='advanced'"/>
          <description>
            <text>List of nodes of an advanced segmented next-hop extended community filter. Regular expressions are used as matching rules.</text>
          </description>
          <list name="advanced-node">
            <key value="sequence"/>
