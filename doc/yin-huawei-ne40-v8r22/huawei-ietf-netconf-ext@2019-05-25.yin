<?xml version="1.0" encoding="UTF-8"?>
<module name="huawei-ietf-netconf-ext"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:nc-ext="urn:huawei:yang:huawei-ietf-netconf-ext"
        xmlns:nc="urn:ietf:params:xml:ns:netconf:base:1.0"
        xmlns:ext="urn:huawei:yang:huawei-extension">
  <namespace uri="urn:huawei:yang:huawei-ietf-netconf-ext"/>
  <prefix value="nc-ext"/>
  <import module="ietf-netconf">
    <prefix value="nc"/>
  </import>
  <import module="huawei-extension">
    <prefix value="ext"/>
  </import>
  <organization>
    <text>Huawei Technologies Co., Ltd.</text>
  </organization>
  <contact>
    <text>Huawei Industrial Base
Bantian, Longgang
Shenzhen 518129
People's Republic of China
Website: http://www.huawei.com
Email: support@huawei.com</text>
  </contact>
  <description>
    <text>Extension of NETCONF protocol standard operations that were used privately for Huawei device.</text>
  </description>
  <revision date="2019-05-25">
    <description>
      <text>Initial revision.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <ext:task-name value="netconf"/>
  <typedef name="flow-id-type">
    <type name="uint32"/>
    <description>
      <text>This type used for the attribute 'flow-id';
The rpc-reply of the sync-full/edit-config operation will contain the 'flow-id' XML attribute.</text>
    </description>
  </typedef>
  <notification name="netconf-rpc-keepalive">
    <description>
      <text>This notification will be generated every 20 seconds if the operation takes more time,
Client must use filter to subscribe this notification for a specific session,
and it can report error if this notification is not enabled without the filter.</text>
    </description>
    <leaf name="session-id">
      <type name="uint32"/>
      <description>
        <text>Session in which client want to receive this notitification;
Unique identifier for the session. This value is the NETCONF session identifier, as defined in [RFC4741].Refer to RFC 6022 section 2.1.4.</text>
      </description>
    </leaf>
    <leaf name="message-id">
      <type name="string">
        <length value="0..4095"/>
      </type>
      <description>
        <text>Message id of the rpc operation that is taking long time to execute.</text>
      </description>
    </leaf>
  </notification>
  <augment target-node="/nc:commit/nc:input">
    <description>
      <text>Adds the &lt;description&gt; parameter to
the input of the NETCONF &lt;commit&gt; operation.</text>
    </description>
    <leaf name="description">
      <type name="string">
        <length value="1..60"/>
      </type>
      <description>
        <text>This is used for commit comments to identify specific commit operation.</text>
      </description>
    </leaf>
  </augment>
  <augment target-node="/nc:delete-config/nc:input/nc:target/nc:config-target">
    <description>
      <text>Adds the &lt;candidate&gt; target to
the input of the NETCONF &lt;delete-config&gt; operation.</text>
    </description>
    <leaf name="candidate">
      <type name="empty"/>
      <description>
        <text>The candidate configuration is the config target.</text>
      </description>
    </leaf>
  </augment>
</module>
