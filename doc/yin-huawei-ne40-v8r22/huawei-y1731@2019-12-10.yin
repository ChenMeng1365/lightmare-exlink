                            <key value="src-mac port-id"/>
                            <description>
                              <text>Operational state of ETH-BN message.</text>
                            </description>
                            <leaf name="src-mac">
                              <type name="pub-type:mac-address"/>
                              <description>
                                <text>Source MAC in ETH-BN message.</text>
                              </description>
                            </leaf>
                            <leaf name="port-id">
                              <type name="uint32"/>
                              <description>
                                <text>Port ID in ETH-BN message.</text>
                              </description>
                            </leaf>
                            <leaf name="band-width">
                              <type name="uint32"/>
                              <units name="Mbit/s"/>
