  <augment target-node="/devm:devm">
    <description>
      <text>Configure a remote port.</text>
    </description>
    <container name="remote-ports">
      <description>
        <text>List of remote ports.</text>
      </description>
      <list name="remote-port">
        <key value="name"/>
        <description>
          <text>Configure remote ports.</text>
        </description>
        <ext:generated-by value="system"/>
        <ext:operation-exclude value="create"/>
        <leaf name="name">
          <type name="pub-type:if-name"/>
          <description>
            <text>The textual name of the interface. It should be the name of the interface as assigned by the local device. It should be suitable for use in commands which entered at the device's 'console'. This might be a text name, such as 'NULL0', depending on the interface naming syntax of the device.</text>
          </description>
        </leaf>
        <leaf name="loopback-mode">
          <ext:dynamic-default/>
          <type name="remote-port-loopback-state-type"/>
          <description>
            <text>Port loopback mode.</text>
          </description>
        </leaf>
        <leaf name="tx-enable-delay">
          <ext:dynamic-default/>
          <type name="uint32">
            <range value="0..86400000"/>
          </type>
          <units name="ms"/>
          <description>
            <text>Port tx enable delay.</text>
          </description>
        </leaf>
        <container name="ethernet">
          <description>
            <text>Configure Ethernet port.</text>
          </description>
          <leaf name="flow-control">
            <ext:dynamic-default/>
            <type name="enumeration">
              <enum name="all-enable">
                <value value="3"/>
                <description>
                  <text>All enable.</text>
                </description>
              </enum>
              <enum name="receive-enable">
                <value value="1"/>
                <description>
                  <text>Receive enable.</text>
                </description>
              </enum>
              <enum name="send-enable">
                <value value="2"/>
                <description>
                  <text>Send enable.</text>
                </description>
              </enum>
              <enum name="all-disable">
                <value value="0"/>
                <description>
                  <text>All disable.</text>
                </description>
              </enum>
            </type>
            <description>
              <text>Flow control method of an Ethernet port.</text>
            </description>
          </leaf>
          <leaf name="trans-mode">
            <ext:dynamic-default/>
            <type name="enumeration">
              <enum name="lan-mode">
                <value value="0"/>
                <description>
                  <text>LAN Mode.</text>
                </description>
              </enum>
              <enum name="wan-mode">
                <value value="1"/>
                <description>
                  <text>WAN Mode.</text>
                </description>
              </enum>
            </type>
            <description>
              <text>Transmit mode of an Ethernet port.</text>
            </description>
          </leaf>
          <leaf name="down-hold-time">
            <ext:dynamic-default/>
            <when condition="not(../trans-mode='wan-mode')"/>
            <type name="uint32">
              <range value="0..86400000"/>
            </type>
            <units name="ms"/>
            <description>
              <text>Down hold time.</text>
            </description>
          </leaf>
          <leaf name="up-hold-time">
            <ext:dynamic-default/>
            <when condition="not(../trans-mode='wan-mode')"/>
            <type name="uint32">
              <range value="0..86400000"/>
            </type>
            <units name="ms"/>
            <description>
              <text>Up hold time.</text>
            </description>
          </leaf>
          <leaf name="hold-up">
            <ext:dynamic-default/>
            <type name="enumeration">
              <enum name="enable">
                <value value="1"/>
                <description>
                  <text>Enable.</text>
                </description>
              </enum>
              <enum name="disable">
                <value value="0"/>
                <description>
                  <text>Disable.</text>
                </description>
              </enum>
            </type>
            <description>
              <text>Whether the hold-up function is configured on an Ethernet port.</text>
            </description>
          </leaf>
          <leaf name="inter-packet-gap">
            <ext:dynamic-default/>
            <type name="enumeration">
              <enum name="shrink">
                <value value="0"/>
                <description>
                  <text>Shrink.</text>
                </description>
              </enum>
              <enum name="non-shrink">
                <value value="1"/>
                <description>
                  <text>Non-shrink.</text>
                </description>
              </enum>
            </type>
            <description>
              <text>Inter-packet Gap.</text>
            </description>
          </leaf>
          <leaf name="speed">
            <ext:dynamic-default/>
            <when condition="../negotiation!='default'"/>
            <type name="remote-speed-type"/>
            <description>
              <text>Speed of an Ethernet port. This leaf needs to be set at the same time as other leafs.</text>
            </description>
          </leaf>
          <leaf name="duplex">
            <ext:dynamic-default/>
            <when condition="../negotiation!='default'"/>
            <type name="remote-duplex-type"/>
            <description>
              <text>Duplex type of an Ethernet port. This leaf needs to be set at the same time as other leafs.</text>
            </description>
          </leaf>
          <leaf name="negotiation">
            <ext:dynamic-default/>
            <type name="enumeration">
              <enum name="enable">
                <value value="1"/>
                <description>
                  <text>Enable.</text>
                </description>
              </enum>
              <enum name="disable">
                <value value="0"/>
                <description>
                  <text>Disable.</text>
                </description>
              </enum>
              <enum name="default">
                <value value="255"/>
                <description>
                  <text>Auto for GE port, disable for GF port.</text>
                </description>
              </enum>
            </type>
            <description>
              <text>Auto negotiation on an Ethernet port. This leaf needs to be set at the same time as other leafs.</text>
            </description>
          </leaf>
          <leaf name="negotiation-mode">
            <ext:dynamic-default/>
            <type name="remote-port-negotiation-mode-type"/>
            <must condition="(../negotiation-mode = 'slave' and (../speed ='auto' or ../speed='1000M')) or ../negotiation-mode = 'master'"/>
            <description>
              <text>Auto negotiation mode. If the hardware does not support it, returning to the current interface does not support it.</text>
            </description>
          </leaf>
          <leaf name="gf-media-type">
            <ext:dynamic-default/>
            <type name="enumeration">
              <enum name="copper">
                <value value="3"/>
                <description>
                  <text>Copper.</text>
                </description>
              </enum>
              <enum name="fiber100">
                <value value="2"/>
                <description>
                  <text>Fiber 100M.</text>
                </description>
              </enum>
              <enum name="fiber1000">
                <value value="1"/>
                <description>
                  <text>Fiber 1000M.</text>
                </description>
              </enum>
              <enum name="auto">
                <value value="0"/>
                <description>
                  <text>Auto.</text>
                </description>
              </enum>
            </type>
            <description>
              <text>Medium type on an Ethernet GF port. This leaf needs to be set at the same time as other leafs.</text>
            </description>
          </leaf>
          <leaf name="flow-control-state">
            <type name="enumeration">
              <enum name="all-enable">
                <value value="3"/>
                <description>
                  <text>All enable.</text>
                </description>
              </enum>
              <enum name="receive-enable">
                <value value="1"/>
                <description>
                  <text>Receive enable.</text>
                </description>
              </enum>
              <enum name="send-enable">
                <value value="2"/>
                <description>
                  <text>Send enable.</text>
                </description>
              </enum>
              <enum name="all-disable">
                <value value="0"/>
                <description>
                  <text>All disable.</text>
                </description>
              </enum>
            </type>
            <config value="false"/>
            <description>
              <text>Flow control method of an Ethernet port.</text>
            </description>
          </leaf>
          <container name="eth-wan-attr">
            <when condition="../trans-mode='wan-mode'"/>
            <description>
              <text>Configure Ethernet port attributes.</text>
            </description>
            <leaf name="frame-format">
              <ext:dynamic-default/>
              <type name="enumeration">
                <enum name="SDH">
                  <value value="0"/>
                  <description>
                    <text>Synchronous Digital Hierarchy.</text>
                  </description>
                </enum>
                <enum name="SONET">
                  <value value="1"/>
                  <description>
                    <text>Synchronous Optical Network.</text>
                  </description>
                </enum>
              </type>
              <description>
                <text>Frame format of an Ethernet WAN port.</text>
              </description>
            </leaf>
            <leaf name="j0-mode">
              <ext:dynamic-default/>
              <type name="enumeration">
                <enum name="1byte">
                  <value value="1"/>
                  <description>
                    <text>Configure 1 byte mode.</text>
                  </description>
                </enum>
                <enum name="16byte">
                  <value value="16"/>
                  <description>
                    <text>Configure 16 bytes mode.</text>
                  </description>
                </enum>
                <enum name="64byte-or-null-mode">
                  <value value="64"/>
                  <description>
                    <text>Configure 64bytes or null mode.</text>
                  </description>
                </enum>
              </type>
              <description>
                <text>J0 mode on an Ethernet WAN port.</text>
              </description>
            </leaf>
            <leaf name="j0-value">
              <ext:dynamic-default/>
              <when condition="../j0-mode='1byte'"/>
              <type name="uint32">
                <range value="0..255"/>
              </type>
              <description>
                <text>Value of j0-mode on an Ethernet WAN port.</text>
              </description>
            </leaf>
            <leaf name="j0-trace">
              <ext:dynamic-default/>
              <when condition="../j0-mode='16byte' or ../j0-mode='64byte-or-null-mode'"/>
              <type name="string">
                <length value="1..62"/>
              </type>
              <must condition="(string-length(../j0-trace)&lt;16 and ../j0-mode='16byte') or ../j0-mode='64byte-or-null-mode'"/>
              <description>
                <text>Description of the j0 mode on an Ethernet WAN port.</text>
              </description>
            </leaf>
            <leaf name="j1-mode">
              <ext:dynamic-default/>
              <type name="enumeration">
                <enum name="1byte">
                  <value value="1"/>
                  <description>
                    <text>Configure 1 byte mode.</text>
                  </description>
                </enum>
                <enum name="16byte">
                  <value value="16"/>
                  <description>
                    <text>Configure 16 bytes mode.</text>
                  </description>
                </enum>
                <enum name="64byte-or-null-mode">
                  <value value="64"/>
                  <description>
                    <text>Configure 64byte or null mode.</text>
                  </description>
                </enum>
              </type>
              <description>
                <text>J1 mode on an Ethernet WAN port.</text>
              </description>
            </leaf>
            <leaf name="j1-value">
              <ext:dynamic-default/>
              <when condition="../j1-mode='1byte'"/>
              <type name="uint32">
                <range value="0..255"/>
              </type>
              <description>
                <text>Value of j1-mode on an Ethernet WAN port.</text>
              </description>
            </leaf>
            <leaf name="j1-trace">
              <ext:dynamic-default/>
              <when condition="../j1-mode='16byte' or ../j1-mode='64byte-or-null-mode'"/>
              <type name="string">
                <length value="1..62"/>
              </type>
              <must condition="(string-length(../j1-trace)&lt;16 and ../j1-mode='16byte') or ../j1-mode='64byte-or-null-mode'"/>
              <description>
                <text>Description of the j1 mode on an Ethernet WAN port.</text>
              </description>
            </leaf>
            <leaf name="c2">
              <ext:dynamic-default/>
              <type name="uint32">
                <range value="0..255"/>
              </type>
              <description>
                <text>C2 mode on an Ethernet WAN port.</text>
              </description>
            </leaf>
            <leaf name="clock">
              <ext:dynamic-default/>
              <type name="enumeration">
                <enum name="master">
                  <value value="0"/>
                  <description>
                    <text>Master clock.</text>
                  </description>
                </enum>
                <enum name="slave">
                  <value value="1"/>
                  <description>
                    <text>Slave clock.</text>
                  </description>
                </enum>
              </type>
              <description>
                <text>Active/standby clock on an Ethernet WAN port.</text>
              </description>
            </leaf>
            <leaf name="scramble">
              <ext:dynamic-default/>
              <type name="enumeration">
                <enum name="enable">
                  <value value="3"/>
                  <description>
                    <text>Enable.</text>
                  </description>
                </enum>
                <enum name="disable">
                  <value value="0"/>
                  <description>
                    <text>Disable.</text>
                  </description>
                </enum>
              </type>
              <description>
                <text>Whether the scrambling function is configured on an Ethernet WAN port.</text>
              </description>
            </leaf>
          </container>
