  <typedef name="kpi-state">
    <type name="enumeration">
      <enum name="abnormal">
        <value value="0"/>
        <description>
          <text>KPI changes to abnormal state.</text>
        </description>
      </enum>
      <enum name="abnormal-duration">
        <value value="1"/>
        <description>
          <text>KPI remains in abnormal state, reports data periodically.</text>
        </description>
      </enum>
      <enum name="normal">
        <value value="2"/>
        <description>
          <text>KPI changes to normal state.</text>
        </description>
      </enum>
      <enum name="normal-duration">
        <value value="3"/>
        <description>
          <text>KPI remains in normal state, reports data periodically.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>KPI state.</text>
    </description>
  </typedef>
