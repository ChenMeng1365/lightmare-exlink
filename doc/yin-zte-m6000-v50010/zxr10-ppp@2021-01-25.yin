  <container name="state">
    <config value="false"/>
    <description>
      <text>Describe the state of PPP</text>
    </description>
    <container name="ppp">
      <description>
        <text>State data of PPP</text>
      </description>
      <container name="ppp-instance">
        <description>
          <text>PPP correlation state</text>
        </description>
        <list name="pppstate">
          <description>
            <text>PPP correlation state</text>
          </description>
          <key value="ifname"/>
          <leaf name="ifname">
            <description>
              <text>This value identifies the PPP interface name.This attribute is created by Interface Management</text>
            </description>
            <type name="ncd:ifname"/>
          </leaf>
          <leaf name="lcp">
            <description>
              <text>LCP state</text>
            </description>
            <type name="ppp-state-type"/>
          </leaf>
          <leaf name="ipcp">
            <description>
              <text>IPCP state</text>
            </description>
            <type name="ppp-state-type"/>
          </leaf>
          <leaf name="bcp">
            <description>
              <text>BCP state</text>
            </description>
            <type name="ppp-state-type"/>
          </leaf>
          <leaf name="ipv6cp">
            <description>
              <text>IPv6CP state</text>
            </description>
            <type name="ppp-state-type"/>
          </leaf>
          <leaf name="mplscp">
            <description>
              <text>MPLSCP state</text>
            </description>
            <type name="ppp-state-type"/>
          </leaf>
          <leaf name="osinlcp">
            <description>
              <text>OSINLCP state</text>
            </description>
            <type name="ppp-state-type"/>
          </leaf>
          <leaf name="authtype">
            <description>
              <text>authentication type</text>
            </description>
            <type name="enumeration">
              <enum name="none">
                <value value="0"/>
                <description>
                  <text>Authentication type is not configured</text>
                </description>
              </enum>
              <enum name="chap">
                <value value="1"/>
                <description>
                  <text>ppp authentication chap</text>
                </description>
              </enum>
              <enum name="pap">
                <value value="2"/>
                <description>
                  <text>ppp authentication pap</text>
                </description>
              </enum>
              <enum name="chap-pap">
                <value value="3"/>
                <description>
                  <text>CHAP PAP adaptive authentication</text>
                </description>
              </enum>
            </type>
          </leaf>
          <leaf name="authstate">
            <description>
              <text>authentication state</text>
            </description>
            <type name="enumeration">
              <enum name="SUCCESS">
                <value value="0"/>
                <description>
                  <text>PPP_AUTH_SUCCESS</text>
                </description>
              </enum>
              <enum name="FAILURE">
                <value value="1"/>
                <description>
                  <text>PPP_AUTH_FAILURE</text>
                </description>
              </enum>
              <enum name="PROCESS">
                <value value="2"/>
                <description>
                  <text>PPP_AUTH_PROCESS: the flag of waiting for authentication server response</text>
                </description>
              </enum>
              <enum name="INVALID">
                <value value="3"/>
                <description>
                  <text>PPP_AUTH_INVALID: authentication unreachable</text>
                </description>
              </enum>
            </type>
          </leaf>
        </list>
      </container>
    </container>
  </container>
