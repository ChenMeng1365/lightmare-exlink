            <leaf name="jp-policy">
              <when condition="../type='ipv4unicast'"/>
              <type name="leafref">
                <path value="/acl:acl/acl:groups/acl:group/acl:identity"/>
              </type>
              <must condition="/acl:acl/acl:groups/acl:group[acl:identity=current()]/acl:type='advance'"/>
              <description>
                <text>Policy for filtering Join/Prune messages.</text>
              </description>
            </leaf>
            <leaf name="jp-asm-policy">
              <when condition="../type='ipv4unicast'"/>
              <type name="leafref">
                <path value="/acl:acl/acl:groups/acl:group/acl:identity"/>
              </type>
              <must condition="not(../jp-policy) or not(../jp-asm-policy)"/>
              <must condition="/acl:acl/acl:groups/acl:group[acl:identity=current()]/acl:type='basic'"/>
              <description>
                <text>Policy for filtering ASM Join/Prune messages.</text>
              </description>
            </leaf>
            <leaf name="jp-ssm-policy">
              <when condition="../type='ipv4unicast'"/>
              <type name="leafref">
                <path value="/acl:acl/acl:groups/acl:group/acl:identity"/>
              </type>
              <must condition="not(../jp-policy) or not(../jp-ssm-policy)"/>
              <must condition="/acl:acl/acl:groups/acl:group[acl:identity=current()]/acl:type='advance'"/>
              <description>
                <text>Policy for filtering SSM Join/Prune messages.</text>
              </description>
            </leaf>
            <leaf name="nbr-policy">
              <when condition="../type='ipv4unicast'"/>
