    </type>
    <description>
      <text>The definition of ACL port range, support &lt;0-65535&gt;-&lt;0-65535&gt;.</text>
    </description>
  </typedef>
  <typedef name="acl-vlan-range">
    <type name="ncd:single-range">
      <pattern value="((([1-9][0-9]{1,2})|([2-9][0-9]{0,0})|1|([1-3][0-9]{3,3})|(40[0-8][0-9]{1,1})|(409[0-3])|4094)(-(([1-9][0-9]{1,2})|([2-9][0-9]{0,0})|1|([1-3][0-9]{3,3})|(40[0-8][0-9]{1,1})|(409[0-3])|4094)){0,1})"/>
    </type>
    <description>
      <text>The definition of ACL VLAN range, support &lt;1-4094&gt;-&lt;1-4094&gt;.</text>
    </description>
  </typedef>
  <typedef name="acl-ttl-range">
    <type name="ncd:single-range">
      <pattern value="((([1-9][0-9]{1,1})|([2-9][0-9]{0,0})|1|([1-1][0-9]{2,2})|(2[0-4][0-9]{1,1})|(25[0-4])|255)(-(([1-9][0-9]{1,1})|([2-9][0-9]{0,0})|1|([1-1][0-9]{2,2})|(2[0-4][0-9]{1,1})|(25[0-4])|255)){0,1})"/>
    </type>
    <description>
      <text>The definition of ACL TTL range, support &lt;1-255&gt;-&lt;1-255&gt;.</text>
    </description>
  </typedef>
