  <rpc name="start-on-demand-test">
    <description>
      <text>Start twamp-light on demand test.</text>
    </description>
    <input>
      <leaf name="id">
        <type name="leafref">
          <path value="/twamp-controller:twamp-controller/twamp-controller:client/twamp-controller:sessions/twamp-controller:session/twamp-controller:id"/>
        </type>
        <mandatory value="true"/>
        <description>
          <text>The session ID of on demand test session.</text>
        </description>
      </leaf>
      <choice name="packet-sent-type">
        <mandatory value="true"/>
        <description>
          <text>Configure the type how the packet is sent in on demand test session.</text>
        </description>
        <case name="duration-type">
          <description>
            <text>The packet is sent in dedicated duration.</text>
          </description>
          <leaf name="duration-time">
            <type name="uint32">
              <range value="60..300"/>
            </type>
            <units name="s"/>
            <description>
              <text>The duation time of on demand test session.</text>
            </description>
          </leaf>
        </case>
        <case name="count-type">
          <description>
            <text>The packet is sent by total count.</text>
          </description>
          <leaf name="packet-count">
            <type name="uint32">
              <range value="100..3000"/>
            </type>
            <description>
              <text>The packet count of on demand test session.</text>
            </description>
          </leaf>
        </case>
      </choice>
      <leaf name="period">
        <type name="period-type"/>
        <units name="ms"/>
        <default value="100ms"/>
        <description>
          <text>The period of on demand test session.</text>
        </description>
      </leaf>
      <leaf name="timeout">
        <type name="uint16">
          <range value="1..10"/>
        </type>
        <units name="s"/>
        <default value="5"/>
        <description>
          <text>The timeout of on demand test session.</text>
        </description>
      </leaf>
    </input>
  </rpc>
  <rpc name="stop-on-demand-test">
    <description>
      <text>Stop twamp-light on demand test.</text>
    </description>
    <input>
      <leaf name="id">
        <type name="leafref">
          <path value="/twamp-controller:twamp-controller/twamp-controller:client/twamp-controller:sessions/twamp-controller:session/twamp-controller:id"/>
        </type>
        <description>
          <text>The session ID of on demand test session.</text>
        </description>
      </leaf>
    </input>
  </rpc>
  <rpc name="reset-statistics">
    <description>
      <text>Reset the twamp-light statistics.</text>
    </description>
    <input>
      <leaf name="id">
