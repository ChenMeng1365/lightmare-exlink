  <container name="configuration">
    <description>
      <text>Configuration model.</text>
    </description>
    <container name="router-msdp">
      <description>
        <text>MSDP module configure root node.</text>
      </description>
      <container name="msdp">
        <description>
          <text>Multicast Source Discovery Protocol.</text>
        </description>
        <container name="enable-msdp">
          <description>
            <text>Enable Multicast Source Discovery Protocol.</text>
          </description>
          <presence value="enable-msdp"/>
          <leaf name="router-msdp">
            <description>
              <text>Enable msdp.</text>
            </description>
            <type name="empty"/>
            <mandatory value="true"/>
          </leaf>
        </container>
        <container name="global">
          <description>
            <text>Global commands.</text>
          </description>
          <leaf name="sa-advertisement-period">
            <description>
              <text>SA message interval.</text>
            </description>
            <type name="uint32">
              <range value="1..60"/>
            </type>
            <units name="second"/>
            <default value="60"/>
          </leaf>
          <leaf name="sa-cache-holdtime">
            <description>
              <text>SA cache holdtime.</text>
            </description>
            <type name="uint32">
              <range value="150..3600"/>
            </type>
            <units name="second"/>
            <default value="360"/>
          </leaf>
        </container>
        <container name="peers">
          <description>
            <text>MSDP peer.</text>
          </description>
          <list name="peer">
            <description>
              <text>MSDP peer.</text>
            </description>
            <key value="address"/>
            <leaf name="address">
              <description>
                <text>Peer address.</text>
              </description>
              <type name="ncd:ipv4-address"/>
            </leaf>
            <leaf name="mesh-group">
              <description>
                <text>Mesh group name.</text>
              </description>
              <type name="string">
                <length value="1..32"/>
              </type>
            </leaf>
            <leaf name="sa-limit">
              <description>
                <text>The limit of SA cache.</text>
              </description>
              <type name="uint32">
                <range value="1..2147483646"/>
              </type>
            </leaf>
            <leaf name="description">
              <description>
                <text>Description of peer.</text>
              </description>
              <type name="ncd:string-with-blank">
                <length value="1..80"/>
              </type>
            </leaf>
            <leaf name="password">
              <description>
                <text>The password of peer.</text>
              </description>
              <type name="string">
                <length value="1..80"/>
              </type>
            </leaf>
            <leaf name="ttl-threshold">
              <description>
                <text>Source active message encapsulated data TTL threshold.</text>
              </description>
              <type name="uint16">
                <range value="1..255"/>
              </type>
              <default value="1"/>
            </leaf>
            <leaf name="ttl-security-hops">
              <description>
                <text>TTL security hops of peer.</text>
              </description>
              <type name="uint16">
                <range value="1..254"/>
              </type>
            </leaf>
            <leaf name="shutdown">
              <description>
                <text>Shutdown peer.</text>
              </description>
              <type name="empty"/>
            </leaf>
            <leaf name="bfd-enable">
              <description>
                <text>Enable bfd function.</text>
              </description>
              <type name="empty"/>
            </leaf>
            <leaf name="holdtime">
              <description>
                <text>Msdp peer holdtime.</text>
              </description>
              <type name="uint32">
                <range value="1..75"/>
              </type>
              <units name="second"/>
              <default value="75"/>
            </leaf>
            <leaf name="keepalive-period">
              <description>
                <text>Peer keepalive message interval.</text>
              </description>
              <type name="uint32">
                <range value="1..60"/>
              </type>
              <units name="second"/>
              <default value="60"/>
            </leaf>
            <leaf name="reconnect-period">
              <description>
                <text>Peer connect retry interval.</text>
              </description>
              <type name="uint32">
                <range value="1..60"/>
              </type>
              <units name="second"/>
              <default value="30"/>
            </leaf>
            <leaf name="inactive-holdtime">
              <description>
                <text>Peer inactive state holdtime.</text>
              </description>
              <type name="uint32">
                <range value="1..60"/>
              </type>
              <units name="second"/>
              <default value="60"/>
            </leaf>
            <leaf name="keychain">
              <description>
                <text>Keychain name.</text>
              </description>
              <type name="string">
                <length value="1..31"/>
              </type>
              <ncd:revise-date date="2016-01-08"/>
            </leaf>
            <leaf name="password-encrypted">
              <description>
                <text>The encrypted password of peer.</text>
              </description>
              <type name="string">
                <length value="64..172"/>
              </type>
              <ncd:revise-date date="2020-07-10"/>
            </leaf>
            <container name="connect-source">
              <description>
                <text>Configuration connect source interface with peer.</text>
              </description>
              <leaf name="interface">
                <description>
                  <text>Configure interface as tcp connect address peer</text>
                </description>
                <type name="ncd:ifname"/>
              </leaf>
            </container>
            <container name="sa-filter-in">
              <description>
                <text>Enable incoming SA filtering.</text>
              </description>
              <presence value="sa-filter-in"/>
              <leaf name="sa-filter-in">
                <description>
                  <text>Enable incoming SA filtering.</text>
                </description>
                <type name="empty"/>
                <mandatory value="true"/>
              </leaf>
              <leaf name="acl">
                <description>
                  <text>IP extended access list.</text>
                </description>
                <type name="string">
                  <length value="1..31"/>
                </type>
              </leaf>
            </container>
            <container name="sa-filter-out">
              <description>
                <text>Enable outgoing SA filtering.</text>
              </description>
              <presence value="sa-filter-out"/>
              <leaf name="sa-filter-out">
                <description>
                  <text>Enable outgoing SA filtering.</text>
                </description>
                <type name="empty"/>
                <mandatory value="true"/>
              </leaf>
              <leaf name="acl">
                <description>
                  <text>IP extended access list.</text>
                </description>
                <type name="string">
                  <length value="1..31"/>
                </type>
              </leaf>
            </container>
          </list>
        </container>
