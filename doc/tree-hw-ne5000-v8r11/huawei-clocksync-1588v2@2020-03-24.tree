     |  +--ro port-pts-capability* [ptp-src-name]
     |     +--ro ptp-src-name       pub-type:if-name
     |     +--ro is-support-1588?   support-1588-type
     +--ro ptp-common-state
        +--ro max-t2-minus-t1?   string
        +--ro min-t2-minus-t1?   string

  rpcs:
    +---x reset-ptp-pkt
    |  +---w input
    |     +---w port-name?   pub-type:if-name
    +---x recover-ptsf-switch
       +---w input
          +---w recover-ptsf-src-name?   pub-type:if-name
