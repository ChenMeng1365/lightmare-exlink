            </description>
            <type name="uint32"/>
          </leaf>
          <leaf name="pcp-trans-num">
            <description>
              <text>PCP translations number.</text>
            </description>
            <type name="uint32"/>
          </leaf>
          <leaf name="local-ip-num">
            <description>
              <text>Local IP number.</text>
            </description>
            <type name="uint32"/>
          </leaf>
        </list>
