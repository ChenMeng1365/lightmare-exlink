  |     +--rw interfaces
  |        +--rw interface* [if-name]
  |           +--rw if-name          ncd:ifname
  |           +--rw detect-policy
  |              +--rw (policy)?
  |                 +--:(block)
  |                 |  +--rw block-policy         empty
  |                 |  +--rw diagnosis-time?      uint32
  |                 +--:(normal)
  |                 |  +--rw normal-policy?       empty
  |                 +--:(protect) {protect-policy}?
  |                    +--rw protect-policy?      empty
  |                    +--rw protect-diag-time?   uint32
  +--ro operation
  |  +--ro eld
  |     +--ro re-detects
  |        +--ro re-detect* [domain-name]
  |           +--ro domain-name    string
  +--ro state
     +--ro eld
        +--ro domains
           +--ro domain* [name]
              +--ro name                  string
              +--ro entity-name?          ncd:ifname
              +--ro status?               enumeration
