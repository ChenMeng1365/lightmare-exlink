<?xml version="1.0" encoding="UTF-8"?>
<module name="huawei-ippmmgr"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:ippmmgr="urn:huawei:yang:huawei-ippmmgr"
        xmlns:ifm="urn:huawei:yang:huawei-ifm"
        xmlns:ethernet="urn:huawei:yang:huawei-ethernet"
        xmlns:ext="urn:huawei:yang:huawei-extension">
  <namespace uri="urn:huawei:yang:huawei-ippmmgr"/>
  <prefix value="ippmmgr"/>
  <import module="huawei-ifm">
    <prefix value="ifm"/>
  </import>
  <import module="huawei-ethernet">
    <prefix value="ethernet"/>
  </import>
  <import module="huawei-extension">
    <prefix value="ext"/>
  </import>
  <ext:task-name value="forwarding"/>
  <organization>
    <text>Huawei Technologies Co., Ltd.</text>
  </organization>
  <contact>
    <text>Huawei Industrial Base
Bantian, Longgang
Shenzhen 518129
People's Republic of China
Website: http://www.huawei.com
Email: support@huawei.com</text>
  </contact>
  <description>
    <text>IP performance management.</text>
  </description>
  <revision date="2020-03-06">
    <description>
      <text>Change description.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <revision date="2019-05-10">
    <description>
      <text>Init revision.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <container name="ippmmgr">
    <description>
      <text>IP performance management.</text>
    </description>
    <container name="global-enable">
      <description>
        <text>Configure the protocol packets hardware multicast in global.</text>
      </description>
      <leaf name="status">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable the global status.</text>
        </description>
      </leaf>
    </container>
  </container>
  <augment target-node="/ifm:ifm/ifm:interfaces/ifm:interface">
    <description>
      <text>Protocol packets hardware multicast interface configuration.</text>
    </description>
    <container name="hwmc-if-enable">
      <when condition="../ethernet:ethernet/ethernet:l3-sub-interface/ethernet:qinq-termination or ../ethernet:ethernet/ethernet:l3-sub-interface/ethernet:dot1q-termination or ../ifm:type='Vlanif'"/>
      <description>
        <text>Configure the protocol packets hardware multicast in interface.</text>
      </description>
      <leaf name="status">
        <type name="boolean"/>
        <description>
          <text>Enable/disable the interface status.</text>
        </description>
      </leaf>
    </container>
  </augment>
</module>
