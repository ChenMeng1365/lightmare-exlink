      <container name="zone-pair-lists">
        <description>
          <text>Configure zone pair security list</text>
        </description>
        <list name="zone-pair">
          <description>
            <text>Zone pair security</text>
          </description>
          <key value="name"/>
          <leaf name="name">
            <description>
              <text>Zone pair name</text>
            </description>
            <type name="string">
              <length value="1..31"/>
            </type>
          </leaf>
          <choice name="source">
            <description>
              <text>Source zone</text>
            </description>
            <case name="source-type">
              <description>
                <text>Source zone type</text>
              </description>
              <leaf name="source-type">
                <description>
                  <text>Source zone type</text>
                </description>
                <type name="enumeration">
                  <enum name="inside">
                    <value value="0"/>
                  </enum>
                  <enum name="outside">
                    <value value="1"/>
                  </enum>
                </type>
              </leaf>
            </case>
            <case name="source-name">
              <description>
                <text>Source zone name</text>
              </description>
              <leaf name="source-name">
                <description>
                  <text>Source zone name</text>
                </description>
                <type name="string">
                  <length value="1..31"/>
                </type>
              </leaf>
            </case>
          </choice>
          <choice name="destination">
            <description>
              <text>Destination zone</text>
            </description>
            <case name="destination-type">
              <description>
                <text>Destination zone type</text>
              </description>
              <leaf name="destination-type">
                <description>
                  <text>Destination zone type</text>
                </description>
                <type name="enumeration">
                  <enum name="inside">
                    <value value="0"/>
                  </enum>
                  <enum name="outside">
                    <value value="1"/>
                  </enum>
                </type>
              </leaf>
            </case>
            <case name="destination-name">
              <description>
                <text>Destination zone name</text>
              </description>
              <leaf name="destination-name">
                <description>
                  <text>Destination zone name</text>
                </description>
                <type name="string">
                  <length value="1..31"/>
                </type>
              </leaf>
            </case>
          </choice>
          <leaf name="tcp-intercept-acl">
            <description>
              <text>IP TCP intercept ACL for zone pair security</text>
            </description>
            <type name="string">
              <length value="0..31"/>
            </type>
            <default value=""/>
          </leaf>
          <leaf name="ipv4-access-group">
            <description>
              <text>ACL for zone pair security</text>
            </description>
            <type name="string">
              <length value="0..31"/>
            </type>
            <default value=""/>
          </leaf>
          <container name="stateful-inspect">
            <description>
              <text>Stateful inspect</text>
            </description>
            <container name="stock-trading-block">
              <description>
                <text>Stock trading block of stateful inspect</text>
              </description>
              <presence value="stock-trading-block"/>
              <leaf name="stock-trading-block">
                <description>
                  <text>Stock trading block of stateful inspect</text>
                </description>
                <type name="empty"/>
                <mandatory value="true"/>
              </leaf>
              <leaf name="time-range">
                <description>
                  <text>Timerange name</text>
                </description>
                <type name="string">
                  <length value="0..31"/>
                </type>
                <default value=""/>
              </leaf>
            </container>
            <container name="instant-msg">
              <description>
                <text>Instant message of stateful inspect</text>
              </description>
              <container name="qq-block">
                <description>
                  <text>QQ block of stateful inspect</text>
                </description>
                <presence value="qq-block"/>
                <leaf name="qq-block">
                  <description>
                    <text>QQ block of stateful inspect</text>
                  </description>
                  <type name="empty"/>
                  <mandatory value="true"/>
                </leaf>
                <leaf name="time-range">
                  <description>
                    <text>Timerange name</text>
                  </description>
                  <type name="string">
                    <length value="0..31"/>
                  </type>
                  <default value=""/>
