<?xml version="1.0" encoding="UTF-8"?>
<submodule name="huawei-dhcp-client"
           xmlns="urn:ietf:params:xml:ns:yang:yin:1"
           xmlns:dhcp="urn:huawei:yang:huawei-dhcp">
  <yang-version value="1"/>
  <belongs-to module="huawei-dhcp">
    <prefix value="dhcp"/>
  </belongs-to>
  <organization>
    <text>Huawei Technologies Co., Ltd.</text>
  </organization>
  <contact>
    <text>Huawei Industrial Base
Bantian, Longgang
Shenzhen 518129
People's Republic of China
Website: http://www.huawei.com
Email: support@huawei.com</text>
  </contact>
  <description>
    <text>This module contains a collection of YANG definitions for managing DHCP client.</text>
  </description>
  <revision date="2019-12-22">
    <description>
      <text>Initial revision.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <typedef name="client-state">
    <type name="enumeration">
      <enum name="init">
        <value value="0"/>
        <description>
          <text>Init state.</text>
        </description>
      </enum>
      <enum name="selecting">
        <value value="1"/>
        <description>
          <text>Selecting state.</text>
        </description>
      </enum>
      <enum name="requesting">
        <value value="2"/>
        <description>
          <text>Requesting state.</text>
        </description>
      </enum>
      <enum name="bound">
        <value value="3"/>
        <description>
          <text>Bound state.</text>
        </description>
      </enum>
      <enum name="renewing">
        <value value="4"/>
        <description>
          <text>Renewing state.</text>
        </description>
      </enum>
      <enum name="rebinding">
        <value value="5"/>
        <description>
          <text>Rebinding state.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>The type of client-state.</text>
    </description>
  </typedef>
  <grouping name="dhcp-client-type">
    <description>
      <text>Configure DHCP client attributes.</text>
    </description>
    <container name="global">
      <description>
        <text>Configure DHCP client global attributes.</text>
      </description>
      <leaf name="option60">
        <type name="string">
          <length value="1..128"/>
          <pattern value="([^\t]{1,})"/>
        </type>
        <description>
          <text>The value of DHCP client global option60.</text>
        </description>
      </leaf>
      <leaf name="unicast-flag">
        <type name="empty"/>
        <description>
          <text>DHCP client unicast flag.</text>
        </description>
      </leaf>
    </container>
  </grouping>
</submodule>
