  <container name="configuration">
    <description>
      <text>Configuration information</text>
    </description>
    <container name="qos">
      <description>
        <text>Configuration about QoS</text>
      </description>
      <container name="class-map">
        <description>
          <text>Configure H-QoS class map</text>
        </description>
        <container name="normal">
          <description>
            <text>Class map based on normal</text>
          </description>
          <container name="class-maps">
            <description>
              <text>Configure H-QoS class map lists</text>
            </description>
            <list name="class-map">
              <description>
                <text>Configure H-QoS class map list</text>
              </description>
              <key value="cmap-name"/>
              <leaf name="cmap-name">
                <description>
                  <text>Name of the class map, support 1-31 characters</text>
                </description>
                <type name="string">
                  <length value="1..31">
                    <description>
                      <text>Name of the class map, support 1-31 characters</text>
                    </description>
                  </length>
                </type>
              </leaf>
              <leaf name="match-relation">
                <description>
                  <text>The type of packets matching, see bellow:                
  match-all Packets meet all of the match criteria
  match-any Packets meet any of the match criteria</text>
                </description>
                <type name="enumeration">
                  <enum name="match-all">
                    <value value="1"/>
                    <description>
                      <text>Packets meet all of the match criteria</text>
                    </description>
                  </enum>
                  <enum name="match-any">
                    <value value="2"/>
                    <description>
                      <text>Packets meet any of the match criteria</text>
                    </description>
                  </enum>
                </type>
                <mandatory value="true"/>
              </leaf>
              <container name="service-type">
                <description>
                  <text>service-type, the same value represents the same class-map.</text>
                </description>
                <presence value="To set the service type value"/>
                <leaf name="service-type">
                  <description>
                    <text>The same service type value represents the same class-map.</text>
                  </description>
                  <type name="uint32">
                    <range value="1..255">
                      <description>
                        <text>The same service type value represents the same class-map.</text>
                      </description>
                    </range>
                  </type>
                  <mandatory value="true"/>
                </leaf>
                <container name="effect-order">
                  <description>
                    <text>The value specify the class effect order in the policy map.</text>
                  </description>
                  <presence value="To set the effect order value"/>
                  <leaf name="effect-order">
                    <description>
                      <text>The value specify the class effect order in the policy map.</text>
                    </description>
                    <type name="uint32">
                      <range value="1..255">
                        <description>
                          <text>The value specify the class effect order in the policy map.</text>
                        </description>
                      </range>
                    </type>
                    <mandatory value="true"/>
                  </leaf>
                </container>
                <ncd:revise-date date="2017-06-12"/>
              </container>
              <container name="match">
                <description>
                  <text>Configure match criteria for a class map</text>
                </description>
                <container name="dscps">
                  <description>
                    <text>Select DSCP as match criteria</text>
                  </description>
                  <list name="dscp">
                    <description>
                      <text>Set DSCP value as match criteria</text>
                    </description>
                    <key value="ip-type"/>
                    <leaf name="ip-type">
                      <description>
                        <text>Configure DSCP value Based on IP, see below:
  any  Packets based on any IP;
  ipv4 Packets based on IPv4;
  ipv6 Packets based on IPv6;</text>
                      </description>
                      <type name="qos-ip-type"/>
                    </leaf>
                    <leaf name="not">
                      <description>
                        <text>Configure the opposite range flag,
  true  Match opposite range
