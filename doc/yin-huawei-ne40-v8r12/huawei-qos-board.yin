            <range value="1..98"/>
          </type>
          <default value="60"/>
          <description>
            <text>Inbound degrade threshold.</text>
          </description>
        </leaf>
      </container>
      <container name="over-ride-outbound-threshold-adjust">
        <must condition="./upgrade-threshold&gt;./degrade-threshold"/>
