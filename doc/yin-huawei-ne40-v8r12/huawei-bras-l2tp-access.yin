    <container name="l2tp">
      <description>
        <text>Configure l2tp to the up.</text>
      </description>
      <container name="lns-groups">
        <description>
          <text>List of LNS Group for a specified vBRAS-UP.</text>
        </description>
        <list name="lns-group">
          <key value="name"/>
          <max-elements value="16"/>
          <description>
            <text>Configure LNS Group for a specified vBRAS-UP.</text>
          </description>
          <leaf name="name">
            <type name="string">
              <length value="1..30"/>
              <pattern value="([^A-Z]{1,30})"/>
            </type>
            <description>
              <text>Name of a LNS Group.</text>
            </description>
            <ext:case-sensitivity value="lower-only"/>
          </leaf>
          <container name="bind-sources">
            <description>
              <text>List of UP-LNS group bind source interface.</text>
            </description>
            <list name="bind-source">
              <key value="loopback-number"/>
              <max-elements value="256"/>
              <description>
                <text>Configure UP-LNS group bind source interface.</text>
              </description>
              <leaf name="loopback-number">
                <type name="uint32">
                  <range value="0..2147483647"/>
                </type>
                <description>
                  <text>Loopback number.</text>
                </description>
              </leaf>
            </list>
          </container>
          <uses name="lns-group-bind-slots"/>
          <uses name="lns-group-common"/>
        </list>
      </container>
      <container name="session-number">
        <config value="false"/>
        <description>
          <text>Statistics of session number by tunnel source IP address.</text>
        </description>
        <container name="lacs">
          <config value="false"/>
          <description>
            <text>List of session number of LAC by tunnel source IP address.</text>
          </description>
          <list name="lac">
            <key value="source-address vpn-name"/>
            <config value="false"/>
            <description>
              <text>Statistics of session number of LAC by tunnel source IP address.</text>
            </description>
            <uses name="session-number"/>
          </list>
        </container>
        <container name="lnss">
          <config value="false"/>
          <description>
            <text>List of session number of LNS by tunnel source IP address.</text>
          </description>
          <list name="lns">
            <key value="source-address vpn-name"/>
            <config value="false"/>
            <description>
              <text>Statistics of session number of LNS by tunnel source IP address.</text>
            </description>
            <uses name="session-number"/>
          </list>
        </container>
      </container>
      <container name="used-tunnel">
        <config value="false"/>
        <description>
          <text>Statistics of the number of used L2TP tunnel IDs.</text>
        </description>
        <leaf name="number">
          <type name="uint32"/>
          <config value="false"/>
          <description>
            <text>The number of Used Tunnel IDs.</text>
          </description>
        </leaf>
      </container>
    </container>
  </augment>
  <augment target-node="/bras-cu-controller:bras-cu-controller/bras-cu-controller:resource-ups/bras-cu-controller:resource-up/bras-cu-controller:remote-up-boards/bras-cu-controller:remote-up-board">
    <description>
      <text>Adds l2tp configuration to the up-board.</text>
    </description>
    <container name="l2tp">
      <description>
        <text>Configure l2tp to the up-board.</text>
      </description>
      <container name="icrq-limit">
        <description>
          <text>Configure icrq limit.</text>
        </description>
        <leaf name="set-value">
          <type name="uint32">
            <range value="1..60000"/>
          </type>
          <description>
            <text>Limit value.</text>
          </description>
        </leaf>
      </container>
    </container>
  </augment>
  <augment target-node="/devm-vnf:devm-vnf/devm-vnf:vnf-boards/devm-vnf:vnf-board">
    <description>
      <text>Adds l2tp configuration to the vnf-board.</text>
    </description>
    <container name="l2tp">
      <description>
        <text>Configure l2tp to the vnf-board.</text>
      </description>
      <container name="icrq-limit">
        <description>
          <text>Configure icrq limit.</text>
        </description>
        <leaf name="set-value">
          <type name="uint32">
            <range value="1..60000"/>
          </type>
          <description>
            <text>Limit value.</text>
          </description>
        </leaf>
      </container>
    </container>
  </augment>
</module>
