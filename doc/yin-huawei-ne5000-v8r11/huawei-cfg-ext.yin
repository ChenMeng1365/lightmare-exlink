<?xml version="1.0" encoding="UTF-8"?>
<module name="huawei-cfg-ext"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:cfg-ext="urn:huawei:yang:huawei-cfg-ext"
        xmlns:ext="urn:huawei:yang:huawei-extension">
  <yang-version value="1.1"/>
  <namespace uri="urn:huawei:yang:huawei-cfg-ext"/>
  <prefix value="cfg-ext"/>
  <import module="huawei-extension">
    <prefix value="ext"/>
  </import>
  <organization>
    <text>Huawei Technologies Co., Ltd.</text>
  </organization>
  <contact>
    <text>Huawei Industrial Base
Bantian, Longgang
Shenzhen 518129
People's Republic of China
Website: http://www.huawei.com
Email: support@huawei.com</text>
  </contact>
  <description>
    <text>Display the changes information in the candidate datastore.</text>
  </description>
  <revision date="2019-05-21">
    <description>
      <text>Init revision.</text>
    </description>
    <reference>
      <text>RFC 6241, Section 8.3.</text>
    </reference>
  </revision>
  <ext:task-name value="config"/>
  <rpc name="get-candidate-change">
    <description>
      <text>Configuration change information.</text>
    </description>
    <output>
      <anydata name="effective-changes">
        <description>
          <text>Configuration change information recorded in the candidate datastore.</text>
        </description>
      </anydata>
      <anydata name="ineffective-changes">
        <description>
          <text>Changes in configurations that have not taken effect in the candidate datastore.</text>
        </description>
      </anydata>
    </output>
  </rpc>
</module>
