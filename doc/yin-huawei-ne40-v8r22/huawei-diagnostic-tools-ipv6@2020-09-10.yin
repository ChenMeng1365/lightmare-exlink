<?xml version="1.0" encoding="UTF-8"?>
<submodule name="huawei-diagnostic-tools-ipv6"
           xmlns="urn:ietf:params:xml:ns:yang:yin:1"
           xmlns:diagnostic-tools="urn:huawei:yang:huawei-diagnostic-tools"
