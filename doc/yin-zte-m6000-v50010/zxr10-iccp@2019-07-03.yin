  <feature name="label_vxlan">
    <description>
      <text>Feature label of ICCP apply vxlan.</text>
    </description>
  </feature>
  <feature name="label_igmpsnoop">
    <description>
      <text>Feature label of ICCP apply igmpsnoop.</text>
    </description>
  </feature>
  <feature name="label_dhcpsnoop">
    <description>
      <text>Feature label of ICCP apply dhcpsnoop.</text>
    </description>
  </feature>
