            <text>The minimum of BIER label range.</text>
          </description>
          <type name="uint32">
            <range value="0 | 16..1048575"/>
          </type>
          <mandatory value="true"/>
        </leaf>
        <leaf name="max">
          <description>
            <text>The maximum of BIER label range.</text>
          </description>
          <type name="uint32">
            <range value="0 | 16..1048575"/>
