  <container name="configuration">
    <description>
      <text>MONITOR module configuration model</text>
    </description>
    <container name="monitor">
      <description>
        <text>MONITOR module configuration root node</text>
      </description>
      <container name="sessions">
        <description>
          <text>MONITOR sessions</text>
        </description>
        <list name="session">
          <description>
            <text>MONITOR session</text>
          </description>
          <key value="session-id"/>
          <leaf name="session-id">
            <description>
              <text>MONITOR session ID</text>
            </description>
            <type name="uint32">
              <range value="1..64"/>
            </type>
          </leaf>
          <leaf name="description">
            <description>
              <text>Description for use.</text>
            </description>
            <type name="ncd:string-with-blank">
              <length value="1..79"/>
            </type>
            <ncd:revise-date date="2020-06-15"/>
          </leaf>
          <container name="acl-rules">
            <description>
              <text>MONITOR destinations for acl rules</text>
            </description>
            <list name="acl-rule">
              <description>
                <text>MONITOR destination for one acl rule</text>
              </description>
              <key value="acl-type acl-name"/>
              <leaf name="acl-type">
                <description>
                  <text>The ACL type</text>
                </description>
                <type name="enumeration">
                  <enum name="ipv4-acl">
                    <value value="1"/>
                    <description>
                      <text>The type of ACL is IPv4</text>
                    </description>
                  </enum>
                  <enum name="ipv6-acl">
                    <value value="2"/>
                    <description>
                      <text>The type of ACL is IPv6</text>
                    </description>
                  </enum>
                  <enum name="link-acl">
                    <value value="3"/>
                    <description>
                      <text>The type of ACL is LINK</text>
                    </description>
                  </enum>
                  <enum name="ipv4-mixed-acl">
                    <value value="4"/>
                    <description>
                      <text>The type of ACL is IPv4 mixed</text>
                    </description>
                  </enum>
                </type>
              </leaf>
              <leaf name="acl-name">
                <description>
                  <text>The ACL name</text>
                </description>
                <type name="string">
                  <length value="1..31"/>
                </type>
              </leaf>
              <choice name="destination-type">
                <description>
                  <text>The type of MONITOR destination</text>
                </description>
                <mandatory value="true"/>
                <case name="interface">
                  <description>
                    <text>The type of MONITOR destination is interface</text>
                  </description>
                  <leaf name="interface-name">
                    <description>
                      <text>The interface name</text>
                    </description>
                    <type name="ncd:ifname"/>
                  </leaf>
                </case>
                <case name="vpws">
                  <description>
                    <text>The type of MONITOR destination is vpws.</text>
                  </description>
                  <leaf name="vpws-name">
                    <description>
                      <text>The vpws-for-monitor name</text>
                    </description>
                    <type name="string">
                      <length value="1..32"/>
                    </type>
                  </leaf>
                  <ncd:revise-date date="2017-04-14"/>
                </case>
              </choice>
            </list>
          </container>
          <container name="default">
            <description>
              <text>The default MONITOR destination</text>
            </description>
            <presence value="Default destination interface of mirror"/>
            <choice name="destination-type">
              <description>
                <text>The type of MONITOR destination</text>
              </description>
              <mandatory value="true"/>
              <case name="interface">
                <description>
                  <text>The type of MONITOR destination is interface</text>
                </description>
                <leaf name="interface-name">
                  <description>
                    <text>The interface name of MONITOR destination</text>
                  </description>
                  <type name="ncd:ifname"/>
                </leaf>
              </case>
            </choice>
          </container>
          <container name="default-vpwss">
            <description>
              <text>Configuration port mirror of remote mirror by vpws.</text>
            </description>
            <list name="default-vpws">
              <description>
                <text>Configuration list of port mirror of remote mirror by vpws.</text>
              </description>
              <key value="vpws-name"/>
              <leaf name="vpws-name">
                <description>
                  <text>The vpws name</text>
                </description>
                <type name="string">
                  <length value="1..32"/>
                </type>
              </leaf>
            </list>
            <ncd:revise-date date="2017-04-14"/>
          </container>
          <container name="sources">
            <description>
              <text>The MONITOR sources</text>
            </description>
            <list name="interface-source">
              <description>
                <text>The MONITOR source whose type is interface</text>
              </description>
              <key value="interface-name direction"/>
              <leaf name="interface-name">
                <description>
                  <text>The interface name of MONITOR source</text>
                </description>
                <type name="ncd:ifname"/>
              </leaf>
              <leaf name="direction">
                <description>
                  <text>The direction for MONITOR source</text>
                </description>
                <type name="enumeration">
                  <enum name="rx">
                    <value value="2"/>
                    <description>
                      <text>The MONITOR direction is receive</text>
                    </description>
                  </enum>
                  <enum name="tx">
                    <value value="3"/>
                    <description>
                      <text>The MONITOR direction is transmit</text>
                    </description>
                  </enum>
                </type>
              </leaf>
              <leaf name="flow-type">
                <description>
                  <text>flow-type</text>
                </description>
                <type name="enumeration">
                  <enum name="bfd-default">
                    <value value="62"/>
                    <description>
                      <text>Packets from unknown BFD sessions</text>
                    </description>
                  </enum>
                </type>
                <ncd:revise-date date="2017-04-14"/>
              </leaf>
            </list>
            <list name="vpws-source">
              <description>
                <text>The MONITOR source whose type is vpws</text>
              </description>
              <key value="vpws-name direction"/>
              <leaf name="vpws-name">
                <description>
                  <text>The vpws name of MONITOR source</text>
                </description>
                <type name="string">
                  <length value="1..32"/>
                </type>
              </leaf>
              <leaf name="direction">
                <description>
                  <text>The direction for MONITOR source</text>
                </description>
                <type name="enumeration">
                  <enum name="rx">
                    <value value="2"/>
                    <description>
                      <text>The MONITOR direction is receive</text>
                    </description>
                  </enum>
                </type>
              </leaf>
              <ncd:revise-date date="2017-04-14"/>
            </list>
            <container name="switchvlan-sources">
              <description>
                <text>The switchvlan source.</text>
              </description>
              <list name="switchvlan-source">
                <description>
                  <text>The switchvlan source.</text>
                </description>
                <key value="switchvlan-id direction"/>
                <leaf name="switchvlan-id">
                  <description>
                    <text>The vlanid of switchvlan source.</text>
                  </description>
                  <type name="uint16">
                    <range value="1..4094"/>
                  </type>
                </leaf>
                <leaf name="direction">
                  <description>
                    <text>The direction for monitor source.</text>
                  </description>
                  <type name="enumeration">
                    <enum name="rx">
                      <value value="2"/>
                      <description>
                        <text>The monitor source direction is receive</text>
                      </description>
                    </enum>
                    <enum name="tx">
                      <value value="3"/>
                      <description>
                        <text>The monitor source direction is transmit</text>
                      </description>
                    </enum>
                    <enum name="both">
                      <value value="1"/>
                      <description>
                        <text>Set direction to both rx and tx</text>
