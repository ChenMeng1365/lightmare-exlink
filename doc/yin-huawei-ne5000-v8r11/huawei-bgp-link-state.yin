  <augment target-node="/ni:network-instance/ni:instances/ni:instance/bgp:bgp/bgp:base-process/bgp:afs/bgp:af">
    <description>
      <text>BGP link-state address family configuration.</text>
    </description>
    <container name="link-state-unicast">
      <when condition="../bgp:type='ls'"/>
      <description>
        <text>Configure link state unicast address family options.</text>
      </description>
      <container name="common">
        <description>
          <text>Configure link state unicast address family common options.</text>
        </description>
        <leaf name="reflect-between-client">
          <type name="boolean"/>
          <default value="true"/>
          <description>
            <text>Enable/disable route reflection between clients. If clients of a route reflector (RR) are fully meshed, you can disable route reflection among clients to reduce the cost.</text>
          </description>
        </leaf>
        <leaf name="domain-as">
          <type name="bgp:as-number-validate"/>
          <description>
            <text>Domain AS.</text>
          </description>
        </leaf>
        <leaf name="domain-identifier">
          <type name="inet:ip-address-no-zone"/>
          <description>
            <text>Domain ID.</text>
          </description>
        </leaf>
        <uses name="bgp:reflector-cluster-type"/>
      </container>
      <uses name="bgp:slow-peer-type"/>
    </container>
  </augment>
  <augment target-node="/ni:network-instance/ni:instances/ni:instance/bgp:bgp/bgp:base-process/bgp:peers/bgp:peer/bgp:afs/bgp:af">
    <description>
      <text>BGP link state unicast address family configuration.</text>
    </description>
    <container name="link-state-unicast">
      <when condition="../bgp:type='ls'"/>
      <description>
        <text>Configure link state unicast address family options.</text>
      </description>
      <leaf name="group-name">
        <type name="leafref">
          <path value="../../../../../../bgp:peer-groups/bgp:peer-group/bgp:name"/>
        </type>
        <must condition="../../../../../../bgp:peer-groups/bgp:peer-group[bgp:name = current()]/bgp:afs/bgp:af[bgp:type='ls']"/>
        <description>
          <text>Configure a name for a peer group. After a peer is deleted from the peer group, the peer is disabled from the address family. The object cannot be modified.</text>
        </description>
        <ext:operation-exclude value="update">
          <description>
            <text>The node cannot be modified, when the peer already belongs to a peer-group.</text>
          </description>
        </ext:operation-exclude>
      </leaf>
      <leaf name="reflect-client">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable the local device as a route reflector (RR) and a peer or peer group as its client. This configuration is applicable between IBGP peers only. The configurations of RRs and its clients in an address family are valid only in this address family. Therefore, it is recommended to configure RRs and its clients in the specified address family that is required.</text>
        </description>
      </leaf>
      <leaf name="route-update-interval">
        <type name="uint16">
          <range value="0..600"/>
        </type>
        <units name="s"/>
        <description>
          <text>Specify the minimum interval at which Update packets are sent. By default, the interval at which Update packets are sent to IBGP peers is 15s,
and the interval at which Update packets are sent to EBGP peers is 30s. When routes change, a router will send Update packets to notify its peers.
If a route changes frequently, you can set an interval at which Update packets are sent to prevent the router from sending Update packets each time the route changes.
This configuration is valid only to the routes learned from peers.</text>
        </description>
        <ext:dynamic-default/>
      </leaf>
      <choice name="import-policy-type">
        <description>
          <text>Set route policy.</text>
        </description>
        <case name="rtp-ref">
          <description>
            <text>Route policy.</text>
          </description>
          <leaf name="import-policy">
            <type name="leafref">
              <path value="/rtp:routing-policy/rtp:policy-definitions/rtp:policy-definition/rtp:name"/>
            </type>
            <description>
              <text>Specify the filtering policy applied to the routes learned from a peer. By default, no such policy is specified.</text>
            </description>
          </leaf>
        </case>
      </choice>
      <choice name="export-policy-type">
        <description>
          <text>Set route policy.</text>
        </description>
        <case name="rtp-ref">
          <description>
            <text>Route policy.</text>
          </description>
          <leaf name="export-policy">
            <type name="leafref">
              <path value="/rtp:routing-policy/rtp:policy-definitions/rtp:policy-definition/rtp:name"/>
            </type>
            <description>
              <text>Specify the filtering policy applied to the routes to be advertised to a peer. By default, no such policy is specified.</text>
            </description>
          </leaf>
        </case>
      </choice>
      <uses name="bgp:route-limit-type"/>
    </container>
  </augment>
  <augment target-node="/ni:network-instance/ni:instances/ni:instance/bgp:bgp/bgp:base-process/bgp:peer-groups/bgp:peer-group/bgp:afs/bgp:af">
    <description>
      <text>BGP link state unicast address family configuration.</text>
    </description>
    <container name="link-state-unicast">
      <when condition="../bgp:type='ls'"/>
      <description>
        <text>Configure link state unicast address family options.</text>
      </description>
      <leaf name="reflect-client">
        <when condition="../../../../bgp:type='ibgp'"/>
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable the local device as a route reflector (RR) and a peer or peer group as its client. This configuration is applicable between IBGP peers only. The configurations of RRs and its clients in an address family are valid only in this address family. Therefore, it is recommended to configure RRs and its clients in the specified address family that is required.</text>
        </description>
      </leaf>
      <choice name="import-policy-type">
        <description>
          <text>Set route policy.</text>
        </description>
        <case name="rtp-ref">
          <description>
            <text>Route policy.</text>
          </description>
          <leaf name="import-policy">
            <type name="leafref">
              <path value="/rtp:routing-policy/rtp:policy-definitions/rtp:policy-definition/rtp:name"/>
            </type>
            <description>
              <text>Specify the filtering policy applied to the routes learned from a peer. By default, no such policy is specified.</text>
            </description>
          </leaf>
        </case>
      </choice>
      <choice name="export-policy-type">
        <description>
          <text>Set route policy.</text>
        </description>
        <case name="rtp-ref">
          <description>
            <text>Route policy.</text>
          </description>
          <leaf name="export-policy">
            <type name="leafref">
              <path value="/rtp:routing-policy/rtp:policy-definitions/rtp:policy-definition/rtp:name"/>
            </type>
            <description>
              <text>Specify the filtering policy applied to the routes to be advertised to a peer. By default, no such policy is specified.</text>
            </description>
          </leaf>
        </case>
      </choice>
      <uses name="bgp:rt-update-interval-type"/>
      <uses name="bgp:route-limit-type"/>
    </container>
  </augment>
  <augment target-node="/bgp:bgp/bgp:base-process/bgp-rt:bgp-route">
    <description>
      <text>BGP link state unicast address family routing-table.</text>
    </description>
    <container name="link-state-unicast">
      <config value="false"/>
      <description>
        <text>Statistics of link-state-unicast routes.</text>
      </description>
      <container name="prefix-routes">
        <config value="false"/>
        <description>
          <text>List of displayed prefix route information in an address family.</text>
        </description>
        <list name="prefix-route">
          <key value="vpn-name type network from"/>
          <description>
            <text>Statistics of prefix routes.</text>
          </description>
          <uses name="bgp-rt:routing-table-vpn-name"/>
          <uses name="ls-ip-prefix-route-type"/>
        </list>
      </container>
      <container name="link-routes">
        <config value="false"/>
        <description>
          <text>List of displayed link route information in an address family.</text>
        </description>
        <list name="link-route">
          <key value="vpn-name type network from"/>
          <description>
            <text>Statistics of link routes.</text>
          </description>
          <uses name="bgp-rt:routing-table-vpn-name"/>
          <uses name="ls-link-route-type"/>
        </list>
      </container>
      <container name="node-routes">
        <config value="false"/>
        <description>
          <text>List of displayed node route information in an address family.</text>
        </description>
        <list name="node-route">
          <key value="vpn-name type network from"/>
          <description>
            <text>Statistics of node routes.</text>
          </description>
          <uses name="bgp-rt:routing-table-vpn-name"/>
          <uses name="ls-node-route-type"/>
        </list>
      </container>
      <container name="srv6sid-routes">
        <config value="false"/>
        <description>
          <text>List of displayed srv6sid route information in an address family.</text>
        </description>
        <list name="srv6sid-route">
          <key value="vpn-name type network from"/>
          <description>
            <text>Statistics of srv6sid routes.</text>
          </description>
          <uses name="bgp-rt:routing-table-vpn-name"/>
          <uses name="ls-srv6-sid-route-type"/>
        </list>
      </container>
      <container name="peer-advertise-srv6sid-routes">
        <config value="false"/>
        <description>
          <text>List of displayed advertised SRv6-SID route information in an address family.</text>
        </description>
        <list name="peer-advertise-srv6sid-route">
          <key value="vpn-name remote-peer-address type network from"/>
          <description>
            <text>Statistics of advertised SRv6-SID routes.</text>
          </description>
          <uses name="bgp-rt:routing-table-vpn-name"/>
          <leaf name="remote-peer-address">
            <type name="inet:ip-address-no-zone"/>
            <description>
              <text>BGP neighbour address.</text>
            </description>
          </leaf>
          <uses name="ls-srv6-sid-route-type"/>
        </list>
      </container>
      <container name="peer-receive-srv6sid-routes">
        <config value="false"/>
        <description>
          <text>List of displayed received SRv6-SID route information in an address family.</text>
        </description>
        <list name="peer-receive-srv6sid-route">
          <key value="vpn-name remote-peer-address type network from"/>
          <description>
            <text>Statistics of received SRv6-SID routes.</text>
          </description>
          <uses name="bgp-rt:routing-table-vpn-name"/>
          <leaf name="remote-peer-address">
            <type name="inet:ip-address-no-zone"/>
            <description>
              <text>BGP neighbor address.</text>
            </description>
          </leaf>
          <uses name="ls-srv6-sid-route-type"/>
        </list>
      </container>
      <container name="peer-advertise-link-routes">
        <config value="false"/>
        <description>
          <text>List of displayed advertised link route information in an address family.</text>
        </description>
        <list name="peer-advertise-link-route">
          <key value="vpn-name remote-peer-address type network from"/>
          <description>
            <text>Statistics of advertised link routes.</text>
          </description>
          <uses name="bgp-rt:routing-table-vpn-name"/>
          <leaf name="remote-peer-address">
            <type name="inet:ip-address-no-zone"/>
            <description>
              <text>BGP neighbor address.</text>
            </description>
          </leaf>
          <uses name="ls-link-route-type"/>
        </list>
      </container>
      <container name="peer-receive-link-routes">
        <config value="false"/>
        <description>
          <text>List of displayed received link route information in an address family.</text>
        </description>
        <list name="peer-receive-link-route">
          <key value="vpn-name remote-peer-address type network from"/>
          <description>
            <text>Statistics of received link routes.</text>
          </description>
          <uses name="bgp-rt:routing-table-vpn-name"/>
          <leaf name="remote-peer-address">
            <type name="inet:ip-address-no-zone"/>
            <description>
              <text>BGP neighbor address.</text>
            </description>
          </leaf>
          <uses name="ls-link-route-type"/>
        </list>
      </container>
      <container name="statistic">
        <description>
          <text>Statistics of BGP routes information of link-state unicast address family.</text>
        </description>
        <uses name="bgp-rt:route-total-st-type"/>
        <uses name="ls-route-st-type"/>
      </container>
    </container>
  </augment>
</module>
