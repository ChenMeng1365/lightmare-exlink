        <container name="optical-module">
          <description>
            <text>Configure information about optical transceivers.</text>
          </description>
          <leaf name="rx-high-power-warn-enable">
            <ext:dynamic-default/>
            <type name="boolean"/>
            <description>
              <text>Enable/disable receiving optical power high warning.</text>
            </description>
          </leaf>
          <leaf name="rx-low-power-warn-enable">
            <ext:dynamic-default/>
            <type name="boolean"/>
            <description>
              <text>Enable/disable receiving optical power low warning.</text>
            </description>
          </leaf>
          <leaf name="tx-high-power-warn-enable">
            <ext:dynamic-default/>
            <type name="boolean"/>
            <description>
              <text>Enable/disable transmit optical power high warning.</text>
            </description>
          </leaf>
          <leaf name="tx-low-power-warn-enable">
            <ext:dynamic-default/>
            <type name="boolean"/>
            <description>
              <text>Enable/disable transmit optical power low warning.</text>
