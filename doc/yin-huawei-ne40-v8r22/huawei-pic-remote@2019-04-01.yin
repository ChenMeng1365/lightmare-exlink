              <must condition="../sdh-error-high-threshold &gt;= ../sdh-error-low-threshold"/>
              <description>
                <text>The high threshold of SDH error alarm threshold. The number of error packets counted in the time in interval exceeds this threshold will produce sdh-error alarm.</text>
              </description>
            </leaf>
            <leaf name="sdh-error-low-threshold">
              <ext:dynamic-default/>
              <type name="uint32">
