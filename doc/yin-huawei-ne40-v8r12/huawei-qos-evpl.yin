<?xml version="1.0" encoding="UTF-8"?>
<module name="huawei-qos-evpl"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:qos-evpl="urn:huawei:yang:huawei-qos-evpl"
        xmlns:ext="urn:huawei:yang:huawei-extension"
        xmlns:evpl="urn:huawei:yang:huawei-evpl"
        xmlns:qos="urn:huawei:yang:huawei-qos">
  <namespace uri="urn:huawei:yang:huawei-qos-evpl"/>
  <prefix value="qos-evpl"/>
  <import module="huawei-extension">
    <prefix value="ext"/>
  </import>
  <import module="huawei-evpl">
    <prefix value="evpl"/>
  </import>
  <import module="huawei-qos">
    <prefix value="qos"/>
  </import>
  <organization>
    <text>Huawei Technologies Co., Ltd.</text>
  </organization>
  <contact>
    <text>Huawei Industrial Base
Bantian, Longgang
Shenzhen 518129
People's Republic of China
Website: http://www.huawei.com
Email: support@huawei.com</text>
  </contact>
  <description>
    <text>Quality of service for EVPL management.</text>
  </description>
  <revision date="2020-02-27">
    <description>
      <text>Add new nodes.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <revision date="2020-01-20">
    <description>
      <text>Add new nodes.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <revision date="2019-12-02">
    <description>
      <text>Initial revision.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <ext:task-name value="qos"/>
  <augment target-node="/evpl:evpl/evpl:instances/evpl:instance">
    <description>
      <text>Adds QoS configuration to the EVPL.</text>
    </description>
    <container name="vxlan-mode-phb">
      <when condition="../evpl:mode='vxlan-mode'"/>
      <description>
        <text>Configure QoS PHB VXLAN mode.</text>
      </description>
      <leaf name="diffserv-domain-name">
        <type name="leafref">
          <path value="/qos:qos/qos:classifier-template/qos:diffserv-domains/qos:diffserv-domain/qos:name"/>
        </type>
        <description>
          <text>Name of a diffserv-domain.</text>
        </description>
      </leaf>
    </container>
  </augment>
</module>
