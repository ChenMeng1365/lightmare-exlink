<?xml version="1.0" encoding="UTF-8"?>
<module name="huawei-bras-wlan-access"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:bras-wlan-access="urn:huawei:yang:huawei-bras-wlan-access"
        xmlns:ifm="urn:huawei:yang:huawei-ifm"
        xmlns:bras-basic-access="urn:huawei:yang:huawei-bras-basic-access"
        xmlns:ext="urn:huawei:yang:huawei-extension">
  <namespace uri="urn:huawei:yang:huawei-bras-wlan-access"/>
  <prefix value="bras-wlan-access"/>
  <import module="huawei-ifm">
    <prefix value="ifm"/>
  </import>
  <import module="huawei-bras-basic-access">
    <prefix value="bras-basic-access"/>
  </import>
  <import module="huawei-extension">
    <prefix value="ext"/>
  </import>
  <organization>
    <text>Huawei Technologies Co., Ltd.</text>
  </organization>
  <contact>
    <text>Huawei Industrial Base
Bantian, Longgang
Shenzhen 518129
People's Republic of China
Website: http://www.huawei.com
Email: support@huawei.com</text>
  </contact>
  <description>
    <text>Bras-wlan-access.</text>
  </description>
  <revision date="2020-03-18">
    <description>
      <text>Modify description.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <revision date="2019-03-26">
    <description>
      <text>Initial revision.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <ext:task-name value="bras-control"/>
  <container name="bras-wlan-access">
    <description>
      <text>Bras wlan access.</text>
    </description>
    <container name="nas-serial-number">
      <description>
        <text>Configure the AC serial number.</text>
      </description>
      <leaf name="number">
        <type name="string">
          <length value="1..31"/>
          <pattern value="[!-~]{1,31}"/>
          <pattern value="([^&quot;%'*/:&lt;&gt;\?@|]{1,31})"/>
        </type>
        <must condition="../number != '-' and ../number != '--'"/>
        <description>
          <text>Specifies the AC serial number.</text>
        </description>
      </leaf>
    </container>
  </container>
  <augment target-node="/ifm:ifm/ifm:interfaces/ifm:interface/bras-basic-access:bas/bras-basic-access:access-type/bras-basic-access:layer2-subscriber/bras-basic-access:layer2-subscriber">
    <description>
      <text>Wlan access augment layer2 subscriber.</text>
    </description>
    <container name="ssid">
      <description>
        <text>Configure an (service set ID) SSID for WLAN services on a BAS interface. With this SSID, a fat access point (AP) can be connected to the BAS interface. By default, no SSID is configured on a BAS interface.</text>
      </description>
      <leaf name="name">
        <type name="string">
          <length value="1..32"/>
          <pattern value="[!-~]{1,32}"/>
          <pattern value="([^%?]{1,32})"/>
        </type>
        <description>
          <text>Indicates the service set ID.</text>
        </description>
      </leaf>
    </container>
    <container name="wlan-option82-decode-ap-mac">
      <description>
        <text>Configure a BAS interface to parse the AP MAC address in Option 82.</text>
      </description>
      <choice name="mode">
        <description>
          <text>Parse AP MAC address mode.</text>
        </description>
        <case name="huawei">
          <description>
            <text>Parse AP MAC address by huawei mode.</text>
          </description>
          <leaf name="huawei">
            <type name="empty"/>
            <description>
              <text>Indicates the AP MAC address to the huawei mode.</text>
            </description>
          </leaf>
        </case>
        <case name="other">
          <description>
            <text>Parse AP MAC address by other mode.</text>
          </description>
          <leaf name="cisco">
            <type name="empty"/>
            <description>
              <text>Indicates the AP MAC address to the Cisco mode.</text>
            </description>
          </leaf>
          <leaf name="ruskus">
            <type name="empty"/>
            <description>
              <text>Indicates the AP MAC address to the Ruskus mode.</text>
            </description>
          </leaf>
        </case>
      </choice>
    </container>
    <container name="wlan-option82-decode-ssid">
      <description>
        <text>Configure a BAS interface to parse the SSID in Option 82.</text>
      </description>
      <choice name="mode">
        <description>
          <text>Parse SSID mode.</text>
        </description>
        <case name="huawei">
          <description>
            <text>Parse SSID by huawei mode.</text>
          </description>
          <leaf name="huawei">
            <type name="empty"/>
            <description>
              <text>Indicates the ssid to the huawei mode.</text>
            </description>
          </leaf>
        </case>
        <case name="other">
          <description>
            <text>Parse SSID by other mode.</text>
          </description>
          <leaf name="cisco">
            <type name="empty"/>
            <description>
              <text>Indicates the ssid to the Cisco mode.</text>
            </description>
          </leaf>
          <leaf name="ruskus">
            <type name="empty"/>
            <description>
              <text>Indicates the ssid to the Ruskus mode.</text>
            </description>
          </leaf>
        </case>
      </choice>
    </container>
  </augment>
</module>
