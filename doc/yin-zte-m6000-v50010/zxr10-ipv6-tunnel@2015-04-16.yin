<?xml version="1.0" encoding="UTF-8"?>
<module name="zxr10-ipv6-tunnel"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:v6tunnel="http://www.zte.com.cn/zxr10/netconf/schema/rosng/ipv6-tunnel"
        xmlns:ncd="http://www.zte.com.cn/zxr10/netconf/schema/rosng/netconf-defs">
  <namespace uri="http://www.zte.com.cn/zxr10/netconf/schema/rosng/ipv6-tunnel"/>
  <prefix value="v6tunnel"/>
  <import module="zxr10-netconf-defs">
    <prefix value="ncd"/>
  </import>
  <organization>
    <text>ZTE Corporation</text>
  </organization>
  <contact>
    <text>ZTE Corporation

   Nanjing Institute of ZTE Corporation

   No.68 Zijinghua Rd. Yuhuatai District, Nanjing, China

   Tel: +86-25-52870000</text>
  </contact>
  <description>
    <text>The netconf module to describe ipv6-tunnel.This netconf module contains managed object definitions of v6_tunnel.</text>
  </description>
  <revision date="2015-04-16">
    <description>
      <text>Set madatory for nodes:

/v6tunnel:configuration/v6tunnel:ipv6-tunnel/v6tunnel:v6-tunnels/v6tunnel:v6-tunnel/v6tunnel:vrfs/v6tunnel:vrf

/v6tunnel:configuration/v6tunnel:ipv6-tunnel/v6tunnel:v6-tunnels/v6tunnel:v6-tunnel/v6tunnel:dhcp_ifs/v6tunnel:dhcp_if

Set presence for nodes:

/v6tunnel:configuration/v6tunnel:ipv6-tunnel/v6tunnel:v6-tunnels/v6tunnel:v6-tunnel/v6tunnel:vrfs

/v6tunnel:configuration/v6tunnel:ipv6-tunnel/v6tunnel:v6-tunnels/v6tunnel:v6-tunnel/v6tunnel:dhcp_ifs</text>
    </description>
  </revision>
  <revision date="2014-09-03">
    <description>
      <text>Add tunnel dscp node for list v6-tunnel.The full path of this node is /configuration/ipv6-tunnel/v6-tunnels/v6-tunnel.

Modify range from 1-127 to 1-128 for node dns_domain.The full path of this node is /configuration/ipv6-tunnel/v6-tunnels/v6-tunnel/destination/domain/dns/dns_domain.</text>
    </description>
  </revision>
  <revision date="2014-02-17">
    <description>
      <text>Modify the reset value for each node.

Modify the node type of mode.</text>
    </description>
  </revision>
  <revision date="2014-01-20">
    <description>
      <text>Initial version.</text>
    </description>
  </revision>
  <container name="configuration">
    <description>
      <text>Configuration node.</text>
    </description>
    <container name="ipv6-tunnel">
      <description>
        <text>This node is the entry for IPv6 tunnel configuration.</text>
      </description>
      <container name="v6-tunnels">
        <description>
          <text>This node is entry for IPv6 tunnel configuration .the nodes contains in v6-tunnel are tunnel option.Config these nodes to buid a v6_tunnel.</text>
        </description>
        <list name="v6-tunnel">
          <description>
            <text>This node is entry for v6_tunnel configuration .</text>
          </description>
          <key value="v6_ifname"/>
          <leaf name="v6_ifname">
            <description>
              <text>IPv6 tunnel interface name</text>
            </description>
            <type name="ncd:ifname"/>
          </leaf>
          <leaf name="mode">
            <description>
              <text>IPv6 tuunnel mode.</text>
            </description>
            <type name="enumeration">
              <enum name="4in6">
                <value value="3"/>
                <description>
                  <text>IP over IPv6 encapsulation</text>
                </description>
              </enum>
              <enum name="6in4">
                <value value="4"/>
                <description>
                  <text>IPv6 over IP encapsulation</text>
                </description>
              </enum>
              <enum name="6to4">
                <value value="5"/>
                <description>
                  <text>IPv6 automatic tunnelling using 6to4</text>
                </description>
              </enum>
              <enum name="isatap">
                <value value="7"/>
                <description>
                  <text>IPv6 automatic tunnelling using ISATAP</text>
                </description>
              </enum>
              <enum name="ds-lite-static">
                <value value="9"/>
                <description>
                  <text>Static dual stack lite tunnel</text>
                </description>
              </enum>
              <enum name="ds-lite-dynamic">
                <value value="10"/>
                <description>
                  <text>Dynamic dual stack lite tunnel</text>
                </description>
              </enum>
              <enum name="6rd">
                <value value="11"/>
                <description>
                  <text>IPv6 automatic tunnelling using 6RD</text>
                </description>
              </enum>
              <enum name="ds-lite-b4">
                <value value="12"/>
                <description>
                  <text>Dual stack lite B4 tunnel</text>
                </description>
              </enum>
            </type>
          </leaf>
          <choice name="source">
            <description>
              <text>IPv6 tunnel source address</text>
            </description>
            <case name="ipv4">
              <description>
                <text>IPv6 tunnel source IPv4 address type</text>
              </description>
              <container name="ip_src_addr">
                <description>
                  <text>IPv6 tunnel source IPv4 address</text>
                </description>
                <leaf name="ip_source">
                  <description>
                    <text>IPv6 tunnel source IPv4 address value</text>
                  </description>
                  <type name="ncd:ipv4-address"/>
                </leaf>
              </container>
            </case>
            <case name="ipv6">
              <description>
                <text>IPv6 tunnel source IPv6 address type</text>
              </description>
              <container name="ipv6_src_addr">
                <description>
                  <text>IPv6 tunnel source IPv6 address</text>
                </description>
                <leaf name="ipv6_source">
                  <description>
                    <text>IPv6 tunnel source IPv6 address value</text>
                  </description>
                  <type name="ncd:ipv6-address"/>
                </leaf>
              </container>
            </case>
          </choice>
          <choice name="destination">
            <description>
              <text>IPv6 tunnel destination address</text>
            </description>
            <case name="ipv4">
              <description>
                <text>IPv6 tunnel destination ipv4 address type</text>
              </description>
              <container name="ip_dest_addr">
                <description>
                  <text>IPv6 tunnel destination IPv4 address</text>
                </description>
                <leaf name="ip_destination">
                  <description>
                    <text>IPv6 tunnel destination IPv4 address value</text>
                  </description>
                  <type name="ncd:ipv4-address"/>
                </leaf>
              </container>
            </case>
            <case name="ipv6">
              <description>
                <text>IPv6 tunnel destination IPv6 address type</text>
              </description>
              <container name="ipv6_dest_addr">
                <description>
                  <text>IPv6 tunnel destination IPv6 address</text>
                </description>
                <leaf name="ipv6_destination">
                  <description>
                    <text>IPv6 tunnel IPv6 destination value</text>
                  </description>
                  <type name="ncd:ipv6-address"/>
                </leaf>
              </container>
            </case>
            <case name="domain">
              <description>
                <text>Config IPv6 tunnel destination domain name</text>
              </description>
              <container name="dns">
                <description>
                  <text>Config domain name as IPv6 tunnel destination address</text>
                </description>
                <leaf name="dns_domain">
                  <description>
                    <text>DNS domain name</text>
                  </description>
                  <type name="string">
                    <length value="1..128"/>
                  </type>
                </leaf>
              </container>
            </case>
            <case name="dhcp-interface">
              <description>
                <text>Config DS-LITE-B4 tunnel destination interface</text>
              </description>
              <container name="dhcp_ifs">
                <description>
                  <text>Config DS-LITE-B4 tunnel destination interface</text>
                </description>
                <presence value="tunnelv6 dhcp interface"/>
                <leaf name="dhcp_if">
                  <description>
                    <text>IPv6 tunnel destination DHCP interface name</text>
                  </description>
                  <type name="ncd:ifname"/>
                  <mandatory value="true"/>
                </leaf>
              </container>
            </case>
          </choice>
          <container name="vrfs">
            <description>
              <text>Config IPv6 tunnel VRF</text>
            </description>
            <presence value="tunnvlv6 vrf"/>
            <leaf name="vrf">
              <description>
                <text>VRF name</text>
              </description>
              <type name="string">
                <length value="1..32"/>
              </type>
              <mandatory value="true"/>
            </leaf>
          </container>
          <container name="tun_6rds">
            <description>
              <text>Config 6RD prefix/prefix-length</text>
            </description>
            <presence value="tunnel 6rd prefix"/>
            <leaf name="tun_6rdpre">
              <description>
                <text>6RD prefix value</text>
              </description>
              <type name="ncd:ipv6-address"/>
              <mandatory value="true"/>
            </leaf>
            <leaf name="tun_6rdprelen">
              <description>
                <text>6RD prefix-length value</text>
              </description>
              <type name="uint8">
                <range value="0..128"/>
              </type>
              <mandatory value="true"/>
            </leaf>
          </container>
          <leaf name="tun6rdipv4prelen">
            <description>
              <text>6RD ipv4 mask len</text>
            </description>
            <type name="uint8">
              <range value="0..32">
                <description>
                  <text>0-32: valid value</text>
                </description>
              </range>
            </type>
          </leaf>
          <leaf name="dscp">
            <description>
              <text>Config IPv6 tunnel dscp</text>
            </description>
            <type name="uint8">
              <range value="0..63">
                <description>
                  <text>0-63:valid value</text>
                </description>
              </range>
            </type>
          </leaf>
        </list>
      </container>
    </container>
  </container>
</module>
