        <type name="uint32"/>
        <description>
          <text>The number of last session update message.</text>
        </description>
      </leaf>
      <leaf name="session-create">
        <type name="uint32"/>
        <description>
          <text>The number of last session create message.</text>
        </description>
      </leaf>
      <leaf name="session-teardown">
        <type name="uint32"/>
        <description>
          <text>The number of last session teardown message.</text>
        </description>
