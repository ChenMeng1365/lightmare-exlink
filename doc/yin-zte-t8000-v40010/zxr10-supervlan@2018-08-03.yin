<?xml version="1.0" encoding="UTF-8"?>
<module name="zxr10-supervlan"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:supervlan="http://www.zte.com.cn/zxr10/netconf/schema/rosng/supervlan"
        xmlns:ncd="http://www.zte.com.cn/zxr10/netconf/schema/rosng/netconf-defs">
  <namespace uri="http://www.zte.com.cn/zxr10/netconf/schema/rosng/supervlan"/>
  <prefix value="supervlan"/>
  <yang-version value="1"/>
  <import module="zxr10-netconf-defs">
    <prefix value="ncd"/>
  </import>
  <organization>
    <text>ZTE Corporation</text>
  </organization>
  <contact>
    <text>ZTE Corporation  
   Nanjing Institute of ZTE Corporation 
   No.68 Zijinghua Rd. Yuhuatai District, Nanjing, China 
   Tel: +86-25-52870000</text>
  </contact>
  <description>
    <text>The netconf module to describe SuperVLAN.</text>
  </description>
  <revision date="2018-08-03">
    <description>
      <text>1.Change /configuration/supervlan/bind-table/sub-interface-bind/supervlan-id  to mandatory.
2.Change /configuration/supervlan/bind-table/subvlan-bind/supervlan-id to mandatory.</text>
    </description>
  </revision>
  <revision date="2016-04-25">
    <description>
      <text>1.Add a feature gratuitous-arp-broadcast for the switch project.</text>
    </description>
  </revision>
  <revision date="2014-02-10">
    <description>
      <text>Spell check is error, ralate modify relate</text>
    </description>
  </revision>
  <revision date="2014-01-14">
    <description>
      <text>Modify some leaf which relate to performance parameter,include ifattr-flag/ip-pool-filter(default value),sub-interface-bind/supervlan-id(max value and edit type),subvlan-bind/supervlan-id(max value and edit type)</text>
    </description>
  </revision>
  <revision date="2013-11-28">
    <description>
      <text>Initial version.</text>
    </description>
  </revision>
  <feature name="gratuitous-arp-broadcast">
    <description>
      <text>The information of gratuitous arp broadcast.</text>
    </description>
  </feature>
  <container name="configuration">
    <description>
      <text>Configuration model info.</text>
    </description>
    <container name="supervlan">
      <description>
        <text>Configure SuperVLAN parameters.</text>
      </description>
      <container name="supervlan-ifattr">
        <description>
          <text>The interface configuration of a SuperVLAN.</text>
        </description>
        <list name="ifattr-flag">
          <description>
            <text>The interface flag configuration of a SuperVLAN.</text>
          </description>
          <key value="interface-name"/>
          <leaf name="interface-name">
            <description>
              <text>SuperVLAN interface name.</text>
            </description>
            <type name="ncd:ifname">
              <length value="1..31">
                <description>
                  <text>The length of interface name,it must between 1 to 31 BYTE.</text>
                </description>
              </length>
            </type>
          </leaf>
          <leaf name="inter-subvlan-routing">
            <description>
              <text>SuperVLAN inter-subvlan-routing attribute status,1 mean IPv4 enable,2 mean IPv6 enable, 3 mean IPv4 and IPv6 enable, 4 mean disable,default value is 3.</text>
            </description>
            <type name="enumeration">
              <enum name="enable">
                <value value="3"/>
                <description>
                  <text>The flag of inter-subvlan-routing is enable.</text>
                </description>
              </enum>
              <enum name="disable">
                <value value="4"/>
                <description>
                  <text>The flag of inter-subvlan-routing is disable.</text>
                </description>
              </enum>
            </type>
            <default value="enable"/>
          </leaf>
          <leaf name="inter-subvlan-routing-for-v4v6">
            <description>
              <text>SuperVLAN inter-subvlan-routing-for-v4v6 attribute status is used to distinguish between IPv4 and IPv6,1 mean IPv4 enable,2 mean IPv6 enable.If IPv4 and IPv6 are enable or disable at the same time,the attribute value is 5,identified as a invalid value.</text>
            </description>
            <type name="enumeration">
              <enum name="enable-ipv4">
                <value value="1"/>
                <description>
                  <text>The flag of inter-subvlan-routing is enable ipv4.</text>
                </description>
              </enum>
              <enum name="enable-ipv6">
                <value value="2"/>
                <description>
                  <text>The flag of inter-subvlan-routing is enable ipv6.</text>
                </description>
              </enum>
              <enum name="invalid">
                <value value="5"/>
                <description>
                  <text>The flag of inter-subvlan-routing-for-v4v6 is invalid.</text>
                </description>
              </enum>
            </type>
            <default value="invalid"/>
          </leaf>
          <leaf name="ip-pool-filter">
            <description>
              <text>SuperVLAN ip-pool-filter attribute status,1 mean enable,2 mean disable.default value relate to performance parameter.</text>
            </description>
            <type name="enumeration">
              <enum name="enable">
                <value value="1"/>
                <description>
                  <text>The flag of ip-pool-filter is enable.</text>
                </description>
              </enum>
              <enum name="disable">
                <value value="2"/>
                <description>
                  <text>The flag of ip-pool-filter is disable.</text>
                </description>
              </enum>
            </type>
            <default value="enable"/>
          </leaf>
          <leaf name="arp-broadcast">
            <description>
              <text>SuperVLAN arp-broadcast attribute status,1 mean enable,2 mean disable,default value is 2.</text>
            </description>
            <type name="enumeration">
              <enum name="enable">
                <value value="1"/>
                <description>
                  <text>The flag of arp-broadcast is enable.</text>
                </description>
              </enum>
              <enum name="disable">
                <value value="2"/>
                <description>
                  <text>The flag of arp-broadcast is disable.</text>
                </description>
              </enum>
            </type>
            <default value="disable"/>
          </leaf>
          <leaf name="gratuitous-arp-broadcast">
            <description>
              <text>SuperVLAN gratuitous-arp-broadcast attribute status,1 mean enable,2 mean disable,default value is 1.</text>
            </description>
            <type name="enumeration">
              <enum name="enable">
                <value value="1"/>
                <description>
                  <text>The flag of gratuitous-arp-broadcast is enable.</text>
                </description>
              </enum>
              <enum name="disable">
                <value value="2"/>
                <description>
                  <text>The flag of gratuitous-arp-broadcast is disable.</text>
                </description>
              </enum>
            </type>
            <default value="enable"/>
          </leaf>
          <leaf name="nd-broadcast">
            <description>
              <text>SuperVLAN nd-broadcast attribute status for IPv6,1 mean enable,2 mean disable,default value is 2.</text>
            </description>
            <type name="enumeration">
              <enum name="enable">
                <value value="1"/>
                <description>
                  <text>The flag of nd-broadcast is enable.</text>
                </description>
              </enum>
              <enum name="disable">
                <value value="2"/>
                <description>
                  <text>The flag of nd-broadcast is disable.</text>
                </description>
              </enum>
            </type>
            <default value="disable"/>
          </leaf>
        </list>
      </container>
      <container name="bind-table">
        <description>
          <text>The bind relation of SuperVLAN and sub-interface or SubVLAN.</text>
        </description>
        <list name="sub-interface-bind">
          <description>
            <text>The bind relation of SuperVLAN and sub-interface.</text>
          </description>
          <key value="member-name"/>
          <leaf name="member-name">
            <description>
              <text>Sub-interface name.</text>
            </description>
            <type name="ncd:ifname">
              <length value="1..31">
                <description>
                  <text>The length of interface name,it must between 1 to 31 BYTE.</text>
                </description>
              </length>
            </type>
          </leaf>
          <leaf name="supervlan-id">
            <description>
              <text>SuperVLAN ID. Min value is 1, max value relate to performance parameter.</text>
            </description>
            <type name="uint32">
              <range value="1..4096">
                <description>
                  <text>SuperVLAN ID.Min value is 1, max value relate to performance parameter.</text>
                </description>
              </range>
            </type>
            <mandatory value="true"/>
          </leaf>
        </list>
        <list name="subvlan-bind">
          <description>
            <text>The bind relation of SuperVLAN and SubVLAN.</text>
          </description>
          <key value="subvlan-id"/>
          <leaf name="subvlan-id">
            <description>
              <text>SubVLAN ID.</text>
            </description>
            <type name="uint16">
              <range value="1..4094">
                <description>
                  <text>SubVLAN ID.</text>
                </description>
              </range>
            </type>
          </leaf>
          <leaf name="supervlan-id">
            <description>
              <text>SuperVLAN ID. Min value is 1, max value relate to performance parameter.</text>
            </description>
            <type name="uint32">
              <range value="1..4096">
                <description>
                  <text>Min value is 1, max value relate to performance parameter.</text>
                </description>
              </range>
            </type>
            <mandatory value="true"/>
          </leaf>
        </list>
      </container>
      <container name="ippool-table">
        <description>
          <text>The IP-POOL table of SuperVLAN.</text>
        </description>
        <list name="sub-interface-ippool">
          <description>
            <text>The IP-POOL of sub-interface.</text>
          </description>
          <key value="member-name address-begin address-end"/>
          <leaf name="member-name">
            <description>
              <text>Sub-interface name.</text>
            </description>
            <type name="ncd:ifname">
              <length value="1..31">
                <description>
                  <text>The length of interface name,it must between 1 to 31 BYTE.</text>
                </description>
              </length>
            </type>
          </leaf>
          <leaf name="address-begin">
            <description>
              <text>IP-POOL address begin for sub-interface.</text>
            </description>
            <type name="ncd:ipv4-address"/>
          </leaf>
          <leaf name="address-end">
            <description>
              <text>IP-POOL address end for sub-interface.</text>
            </description>
            <type name="ncd:ipv4-address"/>
          </leaf>
        </list>
        <list name="subvlan-ippool">
          <description>
            <text>The IP-POOL of SubVLAN.</text>
          </description>
          <key value="subvlan-id address-begin address-end"/>
          <leaf name="subvlan-id">
            <description>
              <text>SubVLAN ID.</text>
            </description>
            <type name="uint16">
              <range value="1..4094">
                <description>
                  <text>SubVLAN ID.</text>
                </description>
              </range>
            </type>
          </leaf>
          <leaf name="address-begin">
            <description>
              <text>IP-POOL address begin for SubVLAN.</text>
            </description>
            <type name="ncd:ipv4-address"/>
          </leaf>
          <leaf name="address-end">
            <description>
              <text>IP-POOL address end for SubVLAN.</text>
            </description>
            <type name="ncd:ipv4-address"/>
          </leaf>
        </list>
      </container>
      <container name="subvlan-ifattr">
        <description>
          <text>The attribute of current SubVLAN.</text>
        </description>
        <list name="ifattr-flag">
          <description>
            <text>This list contains attribute of SubVLAN.</text>
          </description>
          <key value="subvlan-id"/>
          <leaf name="subvlan-id">
            <description>
              <text>SubVLAN ID.</text>
            </description>
            <type name="uint16">
              <range value="1..4094">
                <description>
                  <text>SubVLAN ID.</text>
                </description>
              </range>
            </type>
          </leaf>
          <leaf name="gratuitous-arp-broadcast">
            <description>
              <text>SubVLAN gratuitous-arp-broadcast attribute status,1 mean enable,2 mean disable,default value is 1.</text>
            </description>
            <type name="enumeration">
              <enum name="enable">
                <value value="1"/>
                <description>
                  <text>The flag of gratuitous-arp-broadcast is enable.</text>
                </description>
              </enum>
              <enum name="disable">
                <value value="2"/>
                <description>
                  <text>The flag of gratuitous-arp-broadcast is disable.</text>
                </description>
              </enum>
            </type>
            <default value="enable"/>
          </leaf>
        </list>
      </container>
    </container>
  </container>
</module>
