        <description>
          <text>Count of sent ICMP timeout error packets.</text>
        </description>
      </leaf>
      <leaf name="in-icmp-sum">
        <type name="uint32"/>
        <description>
          <text>Count of received ICMP packets.</text>
        </description>
      </leaf>
      <leaf name="in-icmp-err">
        <type name="uint32"/>
