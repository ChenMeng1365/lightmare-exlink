  <container name="l2vpn">
    <when condition="/mpls:mpls/mpls:common/mpls:global/mpls:mpls-enable='true'"/>
    <presence value="Enable l2 virtual private network."/>
    <description>
      <text>L2 virtual private network.</text>
    </description>
    <container name="common">
      <description>
        <text>Configure L2VPN common attributes.</text>
      </description>
      <container name="global">
        <description>
          <text>Configure a global L2VPN.</text>
        </description>
        <leaf name="ldp-default-mode">
          <type name="ldp-signal-type"/>
          <default value="pwe3"/>
          <description>
            <text>LDP is used as the signaling protocol. The dynamic VC will then not send a notification during signaling negotiation. The LDP signaling mode is also called the Martini mode.</text>
          </description>
        </leaf>
        <leaf name="vpls-ignore-ac-state">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable the function of the VSI status not to be affected by changes in the AC status.</text>
          </description>
        </leaf>
        <leaf name="vpls-loop-detect-enable">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable MAC withdraw loop detection.</text>
          </description>
        </leaf>
        <leaf name="bfd-cv-type">
          <type name="bfd-vccv-cv-type"/>
          <default value="cv-linkage-bfd"/>
          <description>
            <text>The field of CV will be all filled corresponding BFD CV type after global enable BFDCV.</text>
          </description>
        </leaf>
        <leaf name="ignore-te-oam-state">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable ignoring TE OAM detecting state.</text>
          </description>
        </leaf>
        <leaf name="auto-create-session">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable creating LDP session automatically.</text>
          </description>
        </leaf>
        <leaf name="grp-selective-ldp-enable">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable the selective mLDP P2MP LSP group.</text>
          </description>
        </leaf>
        <leaf name="label-block-size">
          <type name="uint32">
            <range value="2..8"/>
          </type>
          <must condition="(../label-block-size=2 or ../label-block-size=4 or ../label-block-size=8)"/>
          <default value="8"/>
          <description>
            <text>Configure the label block size for BGP L2VPN.</text>
          </description>
        </leaf>
      </container>
      <container name="pw-templates">
        <description>
          <text>List of PW templates.</text>
        </description>
        <list name="pw-template">
          <key value="name"/>
          <max-elements value="2000"/>
          <description>
            <text>Configure PW attributes in a PW template, such as the peer, control word, and tunnel policy.</text>
          </description>
          <leaf name="name">
            <type name="l2vpn-pw-template-name"/>
            <description>
              <text>Name of a PW template.</text>
            </description>
          </leaf>
          <leaf name="peer-address">
            <ext:operation-exclude value="update"/>
            <type name="inet:ipv4-address-no-zone"/>
            <description>
              <text>Peer IP address. The loopback IP address of a peer is usually used as the peer IP address.</text>
            </description>
          </leaf>
          <leaf name="mtu">
            <type name="uint16">
              <range value="46..9600"/>
            </type>
            <description>
              <text>MTU in a PW template.</text>
            </description>
          </leaf>
          <leaf name="control-word">
            <type name="enumeration">
              <enum name="disable">
                <value value="1"/>
                <description>
                  <text>Disable control word.</text>
                </description>
              </enum>
              <enum name="enable">
                <value value="2"/>
                <description>
                  <text>Enable control word.</text>
                </description>
              </enum>
            </type>
            <default value="disable"/>
            <description>
              <text>Enable the control word function.</text>
            </description>
          </leaf>
          <leaf name="tunnel-policy">
            <type name="string">
              <length value="1..39"/>
            </type>
            <description>
              <text>Tunnel policy configured in a PW template.</text>
            </description>
          </leaf>
          <leaf name="vccv-capability">
            <type name="string">
              <length value="0..40"/>
            </type>
            <config value="false"/>
            <description>
              <text>VCCV capability. VCCV supports two modes: control word and label alert.</text>
            </description>
          </leaf>
          <leaf name="total-pw-num">
            <type name="uint32"/>
            <default value="0"/>
            <config value="false"/>
            <description>
              <text>Total number of PWs using a specific PW template, including static PWs and dynamic PWs.</text>
            </description>
          </leaf>
          <leaf name="static-pw-num">
            <type name="uint32"/>
            <default value="0"/>
            <config value="false"/>
            <description>
              <text>Number of static PWs using a specific PW template.</text>
            </description>
          </leaf>
          <leaf name="ldp-pw-num">
            <type name="uint32"/>
            <default value="0"/>
            <config value="false"/>
            <description>
              <text>Number of dynamic PWs using a specific PW template.</text>
            </description>
          </leaf>
          <container name="tdm-parameter">
            <description>
              <text>Configure TDM parameters.</text>
            </description>
            <leaf name="tdm-encapsulate-number">
              <type name="uint32">
                <range value="1..40"/>
              </type>
              <ext:dynamic-default/>
              <description>
                <text>Number of TDM frames encapsulated in a CESoPSN or SAToP packet in the TDMoPSN application.</text>
              </description>
            </leaf>
            <leaf name="jitter-buffer">
              <type name="uint32">
                <range value="1..64000"/>
              </type>
              <units name="us"/>
              <ext:dynamic-default/>
              <description>
                <text>Depth of the jitter buffer in the application of TDMoPSN.</text>
              </description>
            </leaf>
            <leaf name="jitter-buffer-cep">
              <type name="uint32">
                <range value="125..16000"/>
              </type>
              <units name="us"/>
              <default value="1000"/>
              <description>
                <text>Depth of the jitter buffer in the application of CEP.</text>
              </description>
            </leaf>
            <leaf name="rtp-header">
              <type name="boolean"/>
              <default value="false"/>
              <description>
                <text>Enable/disable transparently transmission of TDM frames.</text>
              </description>
            </leaf>
            <leaf name="idle-code">
              <type name="yang:hex-string">
                <length value="1..2"/>
              </type>
              <default value="ff"/>
              <description>
                <text>Idle code that is filled when the jitter buffer underflow occurs.</text>
              </description>
            </leaf>
            <leaf name="payload-compression">
              <type name="l2vpn-payload-comp"/>
              <default value="disable"/>
              <description>
                <text>Payload compression mode.</text>
              </description>
            </leaf>
          </container>
          <container name="atm-parameter">
            <description>
              <text>Configure ATM parameters.</text>
            </description>
            <leaf name="max-atm-cells">
              <type name="uint32">
                <range value="1..28"/>
              </type>
              <ext:dynamic-default/>
              <description>
                <text>Maximum number of ATM cells to be encapsulated into a packet that can be received by a device.</text>
              </description>
            </leaf>
            <leaf name="atm-pack-overtime">
              <type name="uint32">
                <range value="100..10000"/>
              </type>
              <units name="us"/>
              <ext:dynamic-default/>
              <description>
                <text>Delay in encapsulating ATM cells into a packet.</text>
              </description>
            </leaf>
            <leaf name="atm-transmit-cell">
              <type name="uint32">
                <range value="1..28"/>
              </type>
              <description>
                <text>Maximum number of ATM cells to be encapsulated into a packet that can be sent by a device.</text>
              </description>
            </leaf>
            <leaf name="sequence-number">
              <when condition="../../control-word='enable'"/>
              <type name="boolean"/>
              <default value="false"/>
              <description>
                <text>Enable/disable the sequence number function.</text>
              </description>
            </leaf>
          </container>
          <container name="bfd-parameter">
            <when condition="/bfd:bfd"/>
            <must condition="../control-word='enable'"/>
            <presence value="enable bfd-parameter for pw"/>
            <description>
              <text>Enable/disable BFD.</text>
            </description>
            <leaf name="min-rx-interval">
              <type name="uint16">
                <range value="0..65535"/>
              </type>
              <units name="ms"/>
              <default value="10"/>
              <description>
                <text>Minimum receive interval.</text>
              </description>
            </leaf>
            <leaf name="min-tx-interval">
              <type name="uint16">
                <range value="0..65535"/>
              </type>
              <units name="ms"/>
              <default value="10"/>
              <description>
                <text>Minimum transmit interval.</text>
              </description>
            </leaf>
            <leaf name="detect-multiplier">
              <type name="uint16">
                <range value="3..50"/>
              </type>
              <default value="3"/>
              <description>
                <text>Detection multiplier.</text>
              </description>
            </leaf>
          </container>
          <container name="flow-label">
            <presence value="enable flow-label for pw"/>
            <description>
              <text>Enable/disable the flow-label function.</text>
            </description>
            <leaf name="ability">
              <type name="flow-label-ability"/>
              <must condition=".!='default' and .!='disable'"/>
              <mandatory value="true"/>
              <description>
                <text>Specify the flow-label ability.</text>
              </description>
            </leaf>
            <leaf name="static">
              <type name="boolean"/>
              <default value="false"/>
              <description>
                <text>Enable/disable the static flow-label function.</text>
              </description>
            </leaf>
          </container>
        </list>
      </container>
      <container name="pw-apss">
        <description>
          <text>List of APS instances.</text>
        </description>
        <list name="pw-aps">
          <key value="aps-id"/>
          <max-elements value="65536"/>
          <description>
            <text>Configure APS instances.</text>
          </description>
          <leaf name="aps-id">
            <type name="uint32">
              <range value="1..65535"/>
            </type>
            <description>
              <text>ID of a PW APS instance.</text>
            </description>
          </leaf>
          <leaf name="description">
            <type name="string">
              <length value="1..63"/>
            </type>
            <description>
              <text>PW APS description.</text>
            </description>
          </leaf>
          <leaf name="protect-type">
            <ext:operation-exclude value="update|delete"/>
            <type name="l2vpn-pw-aps-type"/>
            <must condition="((../protect-type='pw-aps' or ../protect-type='pw-fps') and ../role='pw-aps') or (../protect-type='mc-pw-aps' and (../role='master' or ../role='slave'))"/>
            <default value="pw-aps"/>
            <description>
              <text>Protection type.</text>
            </description>
          </leaf>
          <leaf name="role">
            <type name="l2vpn-pw-aps-role"/>
            <must condition="((../protect-type='pw-aps' or ../protect-type='pw-fps') and ../role='pw-aps') or (../protect-type='mc-pw-aps' and (../role='master' or ../role='slave'))"/>
            <default value="pw-aps"/>
            <description>
              <text>E-PW APS role.</text>
            </description>
          </leaf>
          <leaf name="remote-aps-id">
            <when condition="not(../role='pw-aps')"/>
            <type name="uint32">
              <range value="1..65535"/>
            </type>
            <description>
              <text>Remote ID of E-PW APS.</text>
            </description>
          </leaf>
          <leaf name="holdoff">
            <type name="uint32">
              <range value="0..100"/>
            </type>
            <units name="ms"/>
            <default value="0"/>
            <description>
              <text>Hold time.</text>
            </description>
          </leaf>
          <leaf name="wtr">
            <when condition="../role!='slave'"/>
            <type name="uint32">
              <range value="0..12"/>
            </type>
            <units name="min"/>
            <default value="5"/>
            <description>
              <text>Wait-to-Restore time.</text>
            </description>
          </leaf>
          <leaf name="operation-type">
            <when condition="../role!='slave'"/>
            <type name="l2vpn-pw-aps-oper-type"/>
            <default value="revertive"/>
            <description>
              <text>Operation type.</text>
            </description>
          </leaf>
          <leaf name="protect-mode">
            <type name="l2vpn-pw-aps-mode"/>
            <default value="1to1"/>
            <config value="false"/>
            <description>
              <text>Protection mode.</text>
            </description>
          </leaf>
          <container name="pw-aps-info">
            <config value="false"/>
            <description>
              <text>Statistics of APS instances.</text>
            </description>
            <leaf name="aps-status">
              <type name="l2vpn-aps-status"/>
              <description>
                <text>PW APS status.</text>
              </description>
            </leaf>
            <leaf name="work-path-status">
              <type name="l2vpn-aps-path-status"/>
              <description>
                <text>Working path status.</text>
              </description>
            </leaf>
            <leaf name="protect-path-status">
              <type name="l2vpn-aps-path-status"/>
              <description>
                <text>Protection path status.</text>
              </description>
            </leaf>
            <leaf name="far-end-status">
              <type name="l2vpn-aps-status"/>
              <description>
                <text>PW APS status on the remote end.</text>
              </description>
            </leaf>
            <leaf name="request-result">
              <type name="l2vpn-aps-result"/>
              <description>
                <text>APS request result.</text>
              </description>
            </leaf>
            <leaf name="alarm">
              <type name="string">
                <length value="1..256"/>
              </type>
              <description>
                <text>Alarm information.</text>
              </description>
            </leaf>
            <leaf name="total-vpn-num">
              <type name="uint32"/>
              <default value="0"/>
              <description>
                <text>Total number of VPN instances (PW protection groups) bound to a PW APS instance.</text>
              </description>
            </leaf>
          </container>
          <container name="aps-vpws-instances">
            <config value="false"/>
            <description>
              <text>List of VPWS PW protection groups.</text>
            </description>
            <list name="aps-vpws-instance">
              <key value="client-interface"/>
              <max-elements value="128"/>
              <description>
                <text>Operational data of a VPWS PW protection group.</text>
              </description>
              <leaf name="client-interface">
                <type name="leafref">
                  <path value="/ifm:ifm/ifm:interfaces/ifm:interface/ifm:name"/>
                </type>
                <description>
                  <text>Interface bound to an L2VPN.</text>
                </description>
              </leaf>
              <leaf name="instance-name">
                <type name="l2vpn-instance-name"/>
                <description>
                  <text>VPWS instance name.</text>
                </description>
              </leaf>
              <leaf name="bind-type">
                <type name="l2vpn-aps-bind-type"/>
                <description>
                  <text>Binding relationship between a PW protection group and a PW APS instance.</text>
                </description>
              </leaf>
              <leaf name="encapsulation-type">
                <ext:support-filter value="true"/>
                <type name="encapsulation-type"/>
                <description>
                  <text>Encapsulation type of a VPWS instance bound to a PW APS instance.</text>
                </description>
              </leaf>
              <leaf name="description">
                <type name="string">
                  <length value="1..128"/>
                </type>
                <description>
                  <text>Description of a VPWS instance. The value is a string of characters.</text>
                </description>
              </leaf>
              <container name="aps-vpws-pws">
                <description>
                  <text>List of protection group PW member.</text>
                </description>
                <list name="aps-vpws-pw">
                  <key value="role"/>
                  <max-elements value="2"/>
                  <description>
                    <text>Operational data of PW members in a protection group.</text>
                  </description>
                  <leaf name="role">
                    <type name="pw-role-type"/>
                    <description>
                      <text>Role of a PW, either primary or secondary.</text>
                    </description>
                  </leaf>
                  <leaf name="pw-id">
                    <type name="uint32"/>
                    <description>
                      <text>PW ID.</text>
                    </description>
                  </leaf>
                  <leaf name="encapsulation-type">
                    <type name="encapsulation-type"/>
                    <description>
                      <text>Encapsulation type of a PW.</text>
                    </description>
                  </leaf>
                  <leaf name="peer-ip">
                    <type name="inet:ipv4-address-no-zone"/>
                    <description>
                      <text>LSR ID of the remote device.</text>
                    </description>
                  </leaf>
                  <leaf name="pw-state">
                    <type name="l2vpn-pw-state"/>
                    <description>
                      <text>Status of a PW.</text>
                    </description>
                  </leaf>
                </list>
              </container>
            </list>
          </container>
        </list>
      </container>
      <container name="service-peers">
        <description>
          <text>List of peers.</text>
