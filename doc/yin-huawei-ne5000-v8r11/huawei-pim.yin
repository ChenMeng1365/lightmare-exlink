                <path value="/acl:acl/acl:groups/acl:group/acl:identity"/>
              </type>
              <must condition="/acl:acl/acl:groups/acl:group[acl:identity=current()]/acl:type='basic'"/>
              <description>
                <text>Policy for filtering PIM neighbors.</text>
              </description>
            </leaf>
            <leaf name="assert-holdtime">
              <type name="uint32">
                <range value="7..65535"/>
              </type>
              <units name="s"/>
              <default value="180"/>
