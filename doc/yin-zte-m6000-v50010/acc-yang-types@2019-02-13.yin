    <text>xxx</text>
  </organization>
  <contact>
    <text>xxx</text>
  </contact>
  <description>
    <text>This module defines access-otn yang types.</text>
  </description>
  <revision date="2019-02-13">
    <description>
      <text>Initial revision.</text>
    </description>
    <reference>
      <text>RFC xxx</text>
    </reference>
  </revision>
  <typedef name="real">
