              <enum name="v1">
                <value value="1"/>
                <description>
                  <text>RIP-1</text>
                </description>
              </enum>
              <enum name="v2-multicast">
                <value value="2"/>
                <description>
                  <text>RIP-2 messages are multicast.</text>
                </description>
              </enum>
              <enum name="v2-broadcast">
                <value value="3"/>
                <description>
                  <text>RIP-2 messages are broadcast.</text>
                </description>
              </enum>
            </type>
          </leaf>
