  <feature name="mvpn-cross-as">
    <description>
      <text>Only for the use of HighEndPacket.15K,HighEndPacket.15K-GM,PC.MSE,6100 and MSE.M6000-S.</text>
    </description>
  </feature>
  <feature name="mvpn-switch-threshold">
    <description>
      <text>Only for the use of HighEndPacket.15K,HighEndPacket.15K-GM,6100 and PC.MSE.</text>
    </description>
  </feature>
  <container name="configuration">
    <description>
      <text>Configuration model.</text>
    </description>
    <container name="ip-multicast">
      <description>
        <text>Ip multicast</text>
      </description>
      <container name="multicast">
        <description>
          <text>Multicast routing</text>
        </description>
        <presence value="multicast"/>
        <container name="global">
          <description>
            <text>Global config</text>
          </description>
          <leaf name="mroute-limit">
            <description>
              <text>The max number of the multicast routes. It's toplimit depends on ability of the device.</text>
            </description>
            <type name="uint32">
              <range value="1..262144">
                <description>
                  <text>The max number of the multicast routes.</text>
                </description>
              </range>
            </type>
          </leaf>
          <leaf name="nsf-time">
            <description>
              <text>NSF lifetime</text>
            </description>
            <type name="int32">
              <range value="30..1800">
                <description>
                  <text>NSF lifetime</text>
                </description>
              </range>
            </type>
            <units name="second"/>
            <default value="60"/>
          </leaf>
          <leaf name="downstream-limit">
            <description>
              <text>The max number of the multicast route forward table downstreams. It's toplimit depends on ability of the device.</text>
            </description>
            <type name="int32">
              <range value="1..1024">
                <description>
                  <text>The max number of the multicast route forward table downstreams.</text>
                </description>
              </range>
            </type>
          </leaf>
          <leaf name="damping-threshold">
            <description>
              <text>Mroute flap damping threshold</text>
            </description>
            <type name="uint32">
              <range value="2..100">
                <description>
                  <text>Mroute flap damping threshold</text>
                </description>
              </range>
            </type>
            <default value="5"/>
          </leaf>
          <leaf name="multipath-sghash">
            <description>
              <text>Multipath</text>
            </description>
            <type name="enumeration">
              <enum name="s-hash-basic">
                <value value="1"/>
                <description>
                  <text>Multicast s-hash-basic</text>
                </description>
              </enum>
              <enum name="sg-hash-basic">
                <value value="2"/>
                <description>
                  <text>Multicast s-g-hash basic</text>
                </description>
              </enum>
              <enum name="sg-hash-nexthop-based">
                <value value="3"/>
                <description>
                  <text>Multicast s-g-hash next-hop-based</text>
                </description>
              </enum>
              <enum name="sg-hash-balance">
                <value value="4"/>
                <description>
                  <text>Multicast s-g-hash balance</text>
                </description>
              </enum>
              <enum name="g-hash-basic">
                <value value="5"/>
                <description>
                  <text>Multicast g-hash-basic</text>
                </description>
              </enum>
            </type>
          </leaf>
          <leaf name="longest-match">
            <description>
              <text>Nexthop longest match</text>
            </description>
            <type name="empty"/>
          </leaf>
          <leaf name="static-first">
            <description>
              <text>Nexthop static first</text>
            </description>
            <type name="empty"/>
          </leaf>
          <leaf name="tos-value">
            <description>
              <text>DSCP value</text>
            </description>
            <type name="uint8">
              <range value="0..63">
                <description>
                  <text>DSCP value</text>
                </description>
              </range>
            </type>
          </leaf>
          <leaf name="damping-enable">
            <description>
              <text>Mroute flap damping enable</text>
            </description>
            <type name="empty"/>
          </leaf>
          <leaf name="rpf-proxy-vector">
            <description>
              <text>RPF proxy vector for MDT.</text>
            </description>
            <type name="empty"/>
          </leaf>
          <leaf name="rpf-proxy-vector-mbgp">
            <description>
              <text>RPF proxy vector ACL for MBGP.</text>
            </description>
            <type name="string">
              <length value="1..31"/>
            </type>
            <ncd:revise-date date="2015-11-20"/>
          </leaf>
          <leaf name="mroute-limit-p-instance">
            <description>
              <text>The max number of the multicast routes attached to the global instance. It's toplimit depends on ability of the device.</text>
            </description>
            <type name="uint32">
              <range value="1..262144">
                <description>
