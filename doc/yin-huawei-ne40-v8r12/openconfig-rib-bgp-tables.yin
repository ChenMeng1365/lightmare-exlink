  <grouping name="ipvX-srte-policy-adjrib-top">
    <description>
      <text>Top-level grouping for the IPv4 and IPv6 AFI, SR-TE Policy SAFI
Adj-RIBs.</text>
    </description>
    <container name="neighbors">
      <description>
        <text>Surrounding container for the list of neighbours that are
enabled for the IPv4 and IPv6 AFI, SR-TE Policy SAFI address
family.</text>
      </description>
      <list name="neighbor">
        <key value="neighbor-address"/>
        <description>
          <text>An individual neighbour that is enabled for the SR-TE
Policy SAFI.</text>
        </description>
        <leaf name="neighbor-address">
          <type name="leafref">
            <path value="../state/neighbor-address"/>
          </type>
          <description>
            <text>Reference to the address of the neighbour for which the
Adj-RIBs specified are maintained.</text>
          </description>
        </leaf>
        <container name="state">
          <description>
            <text>Operational state parameters of the BGP neighbour for
which the SR-TE Policy SAFI is enabled.</text>
          </description>
          <uses name="ipvX-srte-policy-adjrib-neighbor-state"/>
        </container>
        <container name="adj-rib-in-pre">
          <description>
            <text>The Adj-RIB-In for the SR-TE Policy SAFI for the neighbour,
prior to any inbound policy constraints or modifications
having been applied.</text>
          </description>
          <uses name="ipvX-srte-policy-adjrib-common"/>
        </container>
        <container name="adj-rib-in-post">
          <description>
            <text>The Adj-RIB-In for the SR-TE Policy SAFI for the neighbour,
following any inbound policy constraints or modifications
being made.</text>
          </description>
          <uses name="ipvX-srte-policy-adjrib-in-post"/>
        </container>
        <container name="adj-rib-out-pre">
          <description>
            <text>The Adj-RIB-Out for the SR-TE Policy SAFI for the neighbour,
prior to any outgoing policy modifications or constraints
having been applied.</text>
          </description>
          <uses name="ipvX-srte-policy-adjrib-common"/>
        </container>
        <container name="adj-rib-out-post">
          <description>
            <text>The Adj-RIB-Out for the SR-TE Policy SAFI for the neighbour,
follow any outbound policy constraints or modifications being
made.</text>
          </description>
          <uses name="ipvX-srte-policy-adjrib-common"/>
        </container>
      </list>
    </container>
  </grouping>
  <grouping name="ipvX-srte-policy-adjrib-neighbor-state">
    <description>
      <text>Common attributes for each neighbour for which the SR-TE
Policy SAFI RIBs are being maintained.</text>
    </description>
    <leaf name="neighbor-address">
      <description>
        <text>The address of the neighbour for which the SR-TE policy
SAFI has been negotiated.</text>
      </description>
      <type name="oc-inet:ip-address"/>
    </leaf>
  </grouping>
  <grouping name="ipvX-srte-policy-adjrib-common">
    <description>
      <text>Common structure containing the routes that are learnt via
the IPv4 or IPv6 SR-TE Policy SAFI.</text>
    </description>
    <container name="routes">
      <description>
        <text>Surrounding container for the list of routes within the
SR-TE Policy SAFI.</text>
      </description>
      <list name="route">
        <key value="path-id endpoint color"/>
        <description>
          <text>The routes within the SR-TE Policy SAFI Adj-RIB. The
routes are keyed on the path-id - set to a non-zero
value only if ADD-PATHS is being used; the color; and
the endpoint. The colour and endpoint are extracted from
the NLRI.</text>
        </description>
        <uses name="ipvX-srte-policy-common-keys"/>
        <container name="state">
          <description>
            <text>State parameters for entries within the Adj-RIB used
to store SR-TE Policy SAFI routes.</text>
          </description>
          <uses name="ipvX-srte-policy-common-route-state"/>
          <uses name="bgp-adj-rib-common-attr-refs"/>
          <uses name="bgp-common-route-annotations-state"/>
        </container>
        <uses name="bgp-unknown-attr-top"/>
      </list>
    </container>
  </grouping>
  <grouping name="ipvX-srte-policy-common-route-state">
    <description>
      <text>Common attributes used SR-TE Policy SAFI routes.</text>
    </description>
    <leaf name="path-id">
      <type name="uint32"/>
      <default value="0"/>
      <description>
        <text>Identifier for the path when using BGP ADD-PATHS for the SR-TE
policy SAFI.</text>
      </description>
    </leaf>
    <leaf name="endpoint">
      <type name="oc-inet:ip-address"/>
      <description>
        <text>A unique identifier for the remote set of nodes. When the address
family is IPv4, the value is a 4-octet IPv4 address. When the
address family is IPv6, the value is a 16-octet IPv6 address.</text>
      </description>
    </leaf>
    <leaf name="color">
      <type name="uint32"/>
      <description>
        <text>A 4-octet value identifying the policy. Combined with the endpoint
the endpoint and colour represent the unique policy.</text>
      </description>
    </leaf>
  </grouping>
  <grouping name="ipvX-srte-policy-common-keys">
    <description>
      <text>Common grouping of the keys used for lists of SR-TE policy
SAFI routes.</text>
    </description>
    <leaf name="path-id">
      <type name="leafref">
        <path value="../state/path-id"/>
      </type>
      <description>
        <text>Reference to the path identifier for the SR-TE Policy SAFI
route. The value is only non-zero if ADD-PATHS is not being
used.</text>
      </description>
    </leaf>
    <leaf name="endpoint">
      <type name="leafref">
        <path value="../state/endpoint"/>
      </type>
      <description>
        <text>Reference to the endpoint used for the SR-TE Policy SAFI
route within the NLRI.</text>
      </description>
    </leaf>
    <leaf name="color">
      <type name="leafref">
        <path value="../state/color"/>
      </type>
      <description>
        <text>Reference to the colour used for the SR-TE policy SAFI
route within the NLRI.</text>
      </description>
    </leaf>
  </grouping>
  <grouping name="ipvX-srte-policy-adjrib-in-post">
    <description>
      <text>Grouping for the post-policy Adj-RIB-In for SR-TE Policy SAFI routes</text>
    </description>
    <container name="routes">
      <description>
        <text>The set of routes that are within the Adj-RIB-Out for the
neighbour.</text>
      </description>
      <list name="route">
        <key value="path-id endpoint color"/>
        <description>
          <text>The routes that are in the Adj-RIB-In-Post for the specified
BGP neighbour within the SR-TE Policy SAFI for the specified
address family.</text>
        </description>
        <uses name="ipvX-srte-policy-common-keys"/>
        <container name="state">
          <description>
            <text>Operational state attributes related to the route within
the SR-TE Policy SAFI Adj-RIB-In-Post for the specified
neighbour.</text>
          </description>
          <uses name="ipvX-srte-policy-common-route-state"/>
          <uses name="bgp-adj-rib-common-attr-refs"/>
          <uses name="bgp-common-route-annotations-state"/>
          <uses name="bgp-adj-rib-in-post-route-annotations-state"/>
        </container>
        <uses name="bgp-unknown-attr-top"/>
      </list>
    </container>
  </grouping>
  <grouping name="ipvX-srte-policy-locrib-top">
    <description>
      <text>Top-level grouping for the Loc-RIB for IPv4 or IPv6 Adj-RIB
for SR-TE Policy SAFI.</text>
    </description>
    <container name="loc-rib">
      <description>
        <text>The Loc-RIB for the SR-TE Policy SAFI for IPv4 or IPv6 Unicast
AFIs.</text>
      </description>
      <container name="routes">
        <description>
          <text>List of routes within the SR-TE Policy SAFI, for the IPv4 or
IPv6 AFI.</text>
        </description>
        <list name="route">
          <key value="path-id endpoint color"/>
          <description>
            <text>Route within the specified address family for the SR-TE
Policy SAFI.</text>
          </description>
          <uses name="ipvX-srte-policy-common-keys"/>
          <container name="state">
            <description>
              <text>Operational state attributes for each route within the
IPv4 or IPv6 Unicast SR-TE Policy SAFI.</text>
            </description>
            <uses name="ipvX-srte-policy-common-route-state"/>
            <uses name="bgp-loc-rib-common-attr-refs"/>
            <uses name="bgp-common-route-annotations-state"/>
          </container>
          <uses name="bgp-unknown-attr-top"/>
        </list>
      </container>
    </container>
  </grouping>
</submodule>
