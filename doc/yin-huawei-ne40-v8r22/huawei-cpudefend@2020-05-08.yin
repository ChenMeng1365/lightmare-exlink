              <key value="group"/>
              <description>
                <text>Configure the CIR and weight of the packets to be sent to the CPU in specific protocol groups.</text>
              </description>
              <leaf name="group">
                <type name="protocol-group"/>
                <description>
                  <text>Protocol group into which the protocol packets sent to the CPU are classified.</text>
                </description>
              </leaf>
              <leaf name="cir">
                <type name="uint32">
                  <range value="0..51200"/>
                </type>
                <units name="kbit/s"/>
