        <value value="17"/>
        <description>
          <text>Local use.</text>
        </description>
      </enum>
      <enum name="local2">
        <value value="18"/>
        <description>
          <text>Local use.</text>
        </description>
      </enum>
      <enum name="local3">
        <value value="19"/>
        <description>
          <text>Local use.</text>
        </description>
      </enum>
      <enum name="local4">
        <value value="20"/>
        <description>
          <text>Local use.</text>
        </description>
