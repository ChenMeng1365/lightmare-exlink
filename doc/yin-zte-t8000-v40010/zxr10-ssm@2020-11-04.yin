  <feature name="frequency-check">
    <description>
      <text>The frequency check feature.</text>
    </description>
  </feature>
  <container name="configuration">
    <description>
      <text>The configuration of SSM.</text>
    </description>
    <container name="ssm">
      <description>
        <text>The node of SSM.</text>
      </description>
      <container name="global">
        <description>
          <text>The node of SSM global configuration.</text>
        </description>
        <leaf name="usessm">
          <description>
            <text>The switch of SSM algorithm for clock selection,enable/disable.</text>
          </description>
          <type name="enumeration">
            <enum name="enable">
              <value value="1"/>
              <description>
                <text>Enable the SSM algorithm.</text>
              </description>
            </enum>
            <enum name="disable">
              <value value="2"/>
              <description>
                <text>Disable the SSM algorithm.</text>
              </description>
            </enum>
          </type>
          <default value="disable"/>
        </leaf>
        <leaf name="ssm-mode">
          <description>
            <text>The synchronization clock selection algorithm,ssm/ex-ssm.</text>
          </description>
          <type name="enumeration">
            <enum name="ssm">
              <value value="1"/>
              <description>
                <text>Standard SSM algorithm of ITU.</text>
              </description>
            </enum>
            <enum name="expssm">
              <value value="2"/>
              <description>
                <text>Expand SSM algorithm of ZTE.</text>
              </description>
            </enum>
            <enum name="itu-expssm">
              <value value="3"/>
              <description>
                <text>ITU Expand SSM algorithm.</text>
              </description>
            </enum>
          </type>
          <default value="ssm"/>
        </leaf>
        <leaf name="station">
          <description>
            <text>The switch of NE station,yes/no.</text>
          </description>
          <type name="enumeration">
            <enum name="no">
              <value value="0"/>
              <description>
                <text>Set NE station.</text>
              </description>
            </enum>
            <enum name="yes">
              <value value="1"/>
              <description>
                <text>Cancel NE station.</text>
              </description>
            </enum>
          </type>
          <default value="no"/>
        </leaf>
        <leaf name="station-id">
          <description>
            <text>ID of NE station,the range is 1-7.</text>
          </description>
          <type name="uint8">
            <range value="1..7"/>
          </type>
          <default value="7"/>
        </leaf>
        <leaf name="id-protect">
          <description>
            <text>The switch of ID protection,open/close.</text>
          </description>
          <type name="enumeration">
            <enum name="open">
              <value value="1"/>
              <description>
                <text>Open the ID protection.</text>
              </description>
            </enum>
            <enum name="close">
              <value value="2"/>
              <description>
                <text>Close the ID protection.</text>
              </description>
            </enum>
          </type>
          <default value="close"/>
        </leaf>
        <leaf name="quality-unknown">
          <description>
            <text>The switch of QL unknown mode,open/close.</text>
          </description>
          <type name="enumeration">
            <enum name="open">
              <value value="1"/>
              <description>
                <text>Open QL unknown.</text>
              </description>
            </enum>
            <enum name="close">
              <value value="2"/>
              <description>
                <text>Close QL unknown.</text>
              </description>
            </enum>
          </type>
          <default value="close"/>
        </leaf>
        <leaf name="sec-quality">
          <description>
            <text>The QL of inner clock.</text>
          </description>
          <type name="enumeration">
            <enum name="QL-UNK">
              <value value="0"/>
              <description>
                <text>The QL is unknown.</text>
              </description>
            </enum>
            <enum name="QL-PRC">
              <value value="2"/>
              <description>
                <text>The QL is PRC.</text>
              </description>
            </enum>
            <enum name="QL-SSU-A">
              <value value="4"/>
              <description>
                <text>The QL is SSU-A.</text>
              </description>
            </enum>
            <enum name="QL-SSU-B">
              <value value="8"/>
              <description>
                <text>The QL is SSU-B.</text>
              </description>
            </enum>
            <enum name="QL-SEC">
              <value value="11"/>
              <description>
                <text>The QL is SEC.</text>
              </description>
            </enum>
            <enum name="QL-PRTC">
              <value value="5"/>
              <description>
                <text>The QL is PRTC.</text>
              </description>
            </enum>
            <enum name="QL-ePRTC">
              <value value="1"/>
              <description>
                <text>The QL is ePRTC.</text>
              </description>
            </enum>
            <enum name="QL-ePRC">
              <value value="7"/>
              <description>
                <text>The QL is ePRC.</text>
              </description>
            </enum>
            <enum name="QL-eSEC">
              <value value="10"/>
              <description>
                <text>The QL is eSEC.</text>
              </description>
            </enum>
          </type>
          <default value="QL-SEC"/>
        </leaf>
        <leaf name="wtr-time">
          <description>
            <text>The wait time to restore.the range is 0-12 minutes.</text>
          </description>
          <type name="uint8">
            <range value="0..12"/>
          </type>
          <units name="minute"/>
          <default value="5"/>
        </leaf>
        <leaf name="quality-limit">
          <description>
            <text>The switch of local clock QL limit,enable/disable.</text>
          </description>
          <type name="enumeration">
            <enum name="enable">
              <value value="1"/>
              <description>
                <text>Enable the QL limit.</text>
              </description>
            </enum>
            <enum name="disable">
              <value value="2"/>
              <description>
                <text>Disable the QL limit.</text>
              </description>
            </enum>
          </type>
          <default value="disable"/>
        </leaf>
        <leaf name="lock-bits-send-dnu">
          <description>
            <text>The switch of sending DNU when system locks bits clock,enable/disable.</text>
          </description>
          <type name="enumeration">
            <enum name="enable">
              <value value="1"/>
              <description>
                <text>Enable switch.</text>
              </description>
            </enum>
            <enum name="disable">
              <value value="2"/>
              <description>
                <text>Disable switch.</text>
              </description>
            </enum>
          </type>
          <default value="disable"/>
        </leaf>
        <leaf name="holdover-time">
          <description>
            <text>Holdover time(h).</text>
          </description>
          <type name="uint32">
            <range value="0..10000"/>
          </type>
          <units name="hour"/>
          <default value="24"/>
          <ncd:revise-date date="2017-09-21"/>
        </leaf>
      </container>
      <container name="sync-clocks">
        <description>
          <text>The node of synchronization clock configuration.</text>
        </description>
        <list name="sync-clock">
          <description>
            <text>The synchronization clock list.</text>
          </description>
          <key value="if-name clock-type"/>
          <leaf name="if-name">
            <description>
              <text>The interface name of clock,inner-clk or PTP1588-clk is NULL.</text>
            </description>
            <type name="string">
              <length value="0..31"/>
            </type>
          </leaf>
          <leaf name="clock-type">
            <description>
              <text>The clock type.</text>
            </description>
            <type name="enumeration">
              <enum name="external-clk">
                <value value="1"/>
                <description>
                  <text>External clock.</text>
                </description>
              </enum>
              <enum name="line-clk">
                <value value="2"/>
                <description>
                  <text>Line clock.</text>
                </description>
              </enum>
              <enum name="tributeline-clk">
                <value value="5"/>
                <description>
                  <text>Tribute line clock.</text>
                </description>
              </enum>
              <enum name="gps-clk">
                <value value="7"/>
                <description>
                  <text>GPS clock.</text>
                </description>
              </enum>
              <enum name="stm-clk">
                <value value="8"/>
                <description>
                  <text>Stm-n clock.</text>
                </description>
              </enum>
            </type>
          </leaf>
          <leaf name="priority">
            <description>
              <text>The clock priority,the range is 1-255.</text>
            </description>
            <type name="uint8">
              <range value="1..255">
                <description>
                  <text>The value of DIS is 255.</text>
                </description>
              </range>
            </type>
            <default value="255"/>
          </leaf>
          <leaf name="rcv-quality-mode">
            <description>
              <text>The mode of receive clock QL,auto/manual.</text>
            </description>
            <type name="enumeration">
              <enum name="auto">
                <value value="1"/>
                <description>
                  <text>Automatically receive SSM mode.</text>
                </description>
              </enum>
              <enum name="manual">
                <value value="2"/>
                <description>
                  <text>Manually receive SSM mode.</text>
                </description>
              </enum>
            </type>
            <default value="auto"/>
          </leaf>
          <leaf name="manual-rcv-quality">
            <description>
              <text>The value of receive clock QL by manual.</text>
            </description>
            <type name="enumeration">
              <enum name="QL-UNK">
                <value value="0"/>
                <description>
                  <text>The QL is unknown.</text>
                </description>
              </enum>
              <enum name="QL-PRC">
                <value value="2"/>
                <description>
                  <text>The QL is PRC.</text>
                </description>
              </enum>
              <enum name="QL-SSU-A">
                <value value="4"/>
                <description>
                  <text>The QL is SSU-A.</text>
                </description>
              </enum>
              <enum name="QL-SSU-B">
                <value value="8"/>
                <description>
                  <text>The QL is SSU-B.</text>
                </description>
              </enum>
              <enum name="QL-SEC">
                <value value="11"/>
                <description>
                  <text>The QL is SEC.</text>
                </description>
              </enum>
              <enum name="QL-DNU">
                <value value="15"/>
                <description>
                  <text>The QL is DNU.</text>
                </description>
              </enum>
              <enum name="QL-PRTC">
                <value value="5"/>
                <description>
                  <text>The QL is PRTC.</text>
                </description>
              </enum>
              <enum name="QL-ePRTC">
                <value value="1"/>
                <description>
                  <text>The QL is ePRTC.</text>
                </description>
              </enum>
              <enum name="QL-ePRC">
                <value value="7"/>
                <description>
                  <text>The QL is ePRC.</text>
                </description>
              </enum>
              <enum name="QL-eSEC">
                <value value="10"/>
                <description>
                  <text>The QL is eSEC.</text>
                </description>
              </enum>
            </type>
            <default value="QL-DNU"/>
          </leaf>
          <leaf name="frame-mode">
            <description>
              <text>The frame mode for bits interface,unframe/frame.</text>
            </description>
            <type name="enumeration">
              <enum name="frame">
                <value value="1"/>
                <description>
                  <text>Frame mode.</text>
                </description>
              </enum>
              <enum name="unframe">
                <value value="2"/>
                <description>
                  <text>Unframe mode.</text>
                </description>
              </enum>
              <enum name="invaild">
                <value value="254"/>
                <description>
                  <text>Property is invalid.</text>
                </description>
              </enum>
            </type>
            <default value="invaild"/>
          </leaf>
        </list>
        <list name="sync-unif-clock">
          <description>
            <text>The synchronization clock list.</text>
          </description>
          <key value="clock-type"/>
          <leaf name="clock-type">
            <description>
              <text>The clock type.</text>
            </description>
            <type name="enumeration">
              <enum name="inner-clk">
                <value value="3"/>
                <description>
                  <text>Internal clock.</text>
                </description>
              </enum>
              <enum name="PTP1588-clk">
                <value value="6"/>
                <description>
                  <text>PTP clock.</text>
                </description>
              </enum>
            </type>
          </leaf>
          <leaf name="priority">
            <description>
              <text>The clock priority,the range is 1-255.</text>
            </description>
            <type name="uint8">
              <range value="1..255">
                <description>
                  <text>The value of DIS is 255.</text>
                </description>
              </range>
            </type>
            <default value="255"/>
          </leaf>
          <leaf name="rcv-quality-mode">
            <description>
              <text>The mode of receive clock QL,auto/manual.</text>
            </description>
            <type name="enumeration">
              <enum name="auto">
                <value value="1"/>
                <description>
                  <text>Automatically receive SSM mode</text>
                </description>
              </enum>
              <enum name="manual">
                <value value="2"/>
                <description>
                  <text>Manually receive SSM mode</text>
                </description>
              </enum>
            </type>
            <default value="manual"/>
          </leaf>
          <leaf name="manual-rcv-quality">
            <description>
              <text>The value of receive clock QL by manual.</text>
            </description>
            <type name="enumeration">
              <enum name="QL-UNK">
                <value value="0"/>
                <description>
                  <text>The QL is unknown.</text>
                </description>
              </enum>
              <enum name="QL-PRC">
                <value value="2"/>
                <description>
                  <text>The QL is PRC.</text>
                </description>
              </enum>
              <enum name="QL-SSU-A">
                <value value="4"/>
                <description>
                  <text>The QL is SSU-A.</text>
                </description>
              </enum>
              <enum name="QL-SSU-B">
                <value value="8"/>
                <description>
                  <text>The QL is SSU-B.</text>
                </description>
              </enum>
              <enum name="QL-SEC">
                <value value="11"/>
                <description>
                  <text>The QL is SEC.</text>
                </description>
              </enum>
              <enum name="QL-DNU">
                <value value="15"/>
                <description>
                  <text>The QL is DNU.</text>
                </description>
              </enum>
              <enum name="QL-PRTC">
                <value value="5"/>
