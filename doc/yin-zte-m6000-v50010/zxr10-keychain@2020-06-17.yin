      </enum>
      <enum name="tuesday">
        <value value="2"/>
        <description>
          <text>Tuesday</text>
        </description>
      </enum>
      <enum name="wednesday">
        <value value="3"/>
        <description>
          <text>Wednesday</text>
        </description>
      </enum>
      <enum name="thursday">
        <value value="4"/>
        <description>
          <text>Thursday</text>
        </description>
      </enum>
      <enum name="friday">
        <value value="5"/>
        <description>
          <text>Friday</text>
        </description>
      </enum>
      <enum name="saturday">
        <value value="6"/>
        <description>
          <text>Saturday</text>
        </description>
      </enum>
      <enum name="sunday">
