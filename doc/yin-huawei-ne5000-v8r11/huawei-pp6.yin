<?xml version="1.0" encoding="UTF-8"?>
<module name="huawei-pp6"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:pp6="urn:huawei:yang:huawei-pp6"
        xmlns:ifm="urn:huawei:yang:huawei-ifm"
        xmlns:ip="urn:huawei:yang:huawei-ip"
        xmlns:ext="urn:huawei:yang:huawei-extension"
        xmlns:ni="urn:huawei:yang:huawei-network-instance">
  <yang-version value="1"/>
  <namespace uri="urn:huawei:yang:huawei-pp6"/>
  <prefix value="pp6"/>
  <import module="huawei-ifm">
    <prefix value="ifm"/>
  </import>
  <import module="huawei-ip">
    <prefix value="ip"/>
  </import>
  <import module="huawei-extension">
    <prefix value="ext"/>
  </import>
  <import module="huawei-network-instance">
    <prefix value="ni"/>
  </import>
  <organization>
    <text>Huawei Technologies Co., Ltd.</text>
  </organization>
  <contact>
    <text>Huawei Industrial Base
Bantian, Longgang
Shenzhen 518129
People's Republic of China
Website: http://www.huawei.com
Email: support@huawei.com</text>
  </contact>
  <description>
    <text>IPv6 related configurations.</text>
  </description>
  <revision date="2020-03-16">
    <description>
      <text>Modify Icmp6Source.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <revision date="2019-12-30">
    <description>
      <text>Init revision.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <typedef name="extension-header-type">
    <type name="enumeration">
      <enum name="fragment">
        <value value="44"/>
        <description>
          <text>IPv6 fragment extension header.</text>
        </description>
      </enum>
      <enum name="esp">
        <value value="50"/>
        <description>
          <text>IPv6 encapsulating security payload extension header.</text>
        </description>
      </enum>
      <enum name="ah">
        <value value="51"/>
        <description>
          <text>IPv6 authentication extension header.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Description of extension header type.</text>
    </description>
  </typedef>
  <typedef name="filter-rule-type">
    <type name="enumeration">
      <enum name="deny">
        <value value="0"/>
        <description>
          <text>Deny process specified IPv6 extension header packets.</text>
        </description>
      </enum>
      <enum name="permit">
        <value value="1"/>
        <description>
          <text>Permit process specified IPv6 extension header.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Description of filter-rule-type.</text>
    </description>
  </typedef>
  <typedef name="options-all-flag-type">
    <type name="enumeration">
      <enum name="false">
        <value value="0"/>
        <description>
          <text>Not all options of an IPv6 extension header.</text>
        </description>
      </enum>
      <enum name="true">
        <value value="1"/>
        <description>
          <text>All options of an IPv6 extension header.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Description of options all flag type.</text>
    </description>
  </typedef>
  <typedef name="icmp-pkt-type">
    <type name="enumeration">
      <enum name="port-unreachable">
        <value value="1"/>
        <description>
          <text>Indicates that the packet type is port unreachable error packet.</text>
        </description>
      </enum>
      <enum name="hoplimit-exceeded">
        <value value="2"/>
        <description>
          <text>Indicates that the packet type is hoplimit exceeded error packet.</text>
        </description>
      </enum>
      <enum name="all-tracert">
        <value value="3"/>
        <description>
          <text>Indicates that the packet type is hoplimit exceeded and port unreachable error packet.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Description of icmpv6 error packet type.</text>
    </description>
  </typedef>
  <typedef name="fragment-flag-type">
    <type name="enumeration">
      <enum name="no">
        <value value="0"/>
        <description>
          <text>Clear IPv6 Path MTU fragment flag.</text>
        </description>
      </enum>
      <enum name="yes">
        <value value="1"/>
        <description>
          <text>Set IPv6 Path MTU fragment flag.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Description of fragment flag type.</text>
    </description>
  </typedef>
  <typedef name="pmtu-type">
    <type name="enumeration">
      <enum name="static">
        <value value="0"/>
        <description>
          <text>Static IPv6 Path MTU entry.</text>
        </description>
      </enum>
      <enum name="dynamic">
        <value value="1"/>
        <description>
          <text>Dynamic IPv6 Path MTU entry.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Description of IPv6 Path MTU type, static or dynamic.</text>
    </description>
  </typedef>
  <typedef name="switch-type">
    <type name="enumeration">
      <enum name="allow">
        <value value="0"/>
        <description>
          <text>Allow packets to pass.</text>
        </description>
      </enum>
      <enum name="deny">
        <value value="1"/>
        <description>
          <text>Reject packets to pass.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Description of switch type.</text>
    </description>
  </typedef>
  <typedef name="send-or-receive-type">
    <type name="enumeration">
      <enum name="send">
        <value value="0"/>
        <description>
          <text>Packet sending.</text>
        </description>
      </enum>
      <enum name="receive">
        <value value="1"/>
        <description>
          <text>Packet reception.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Description of packet action.</text>
    </description>
  </typedef>
  <typedef name="icmp6-name-type">
    <type name="enumeration">
      <enum name="network-unreachable">
        <value value="1"/>
        <description>
          <text>Type = 1, Code = 0. Not in the range of all-famous-receive-switch.</text>
        </description>
      </enum>
      <enum name="host-unreachable">
        <value value="2"/>
        <description>
          <text>Type = 1, Code = 3. Not in the range of all-famous-receive-switch.</text>
        </description>
      </enum>
      <enum name="port-unreachable">
        <value value="3"/>
        <description>
          <text>Type = 1, Code = 4.</text>
        </description>
      </enum>
      <enum name="packet-too-big">
        <value value="4"/>
        <description>
          <text>Type = 2, Code = 0.</text>
        </description>
      </enum>
      <enum name="hop-limit-exceeded">
        <value value="5"/>
        <description>
          <text>Type = 3, Code = 0.</text>
        </description>
      </enum>
      <enum name="echo">
        <value value="6"/>
        <description>
          <text>Type = 128, Code = 0.</text>
        </description>
      </enum>
      <enum name="echo-reply">
        <value value="7"/>
        <description>
          <text>Type = 129, Code = 0.</text>
        </description>
      </enum>
      <enum name="router-solicitation">
        <value value="8"/>
        <description>
          <text>Type = 133, Code = 0.</text>
        </description>
      </enum>
      <enum name="router-advertisement">
        <value value="9"/>
        <description>
          <text>Type = 134, Code = 0.</text>
        </description>
      </enum>
      <enum name="neighbor-solicitation">
        <value value="10"/>
        <description>
          <text>Type = 135, Code = 0.</text>
        </description>
      </enum>
      <enum name="neighbor-advertisement">
        <value value="11"/>
        <description>
          <text>Type = 136, Code = 0.</text>
        </description>
      </enum>
      <enum name="redirect">
        <value value="12"/>
        <description>
          <text>Type = 137, Code = 0. Not in the range of all-famous-receive-switch.</text>
        </description>
      </enum>
      <enum name="multicast-listener-query">
        <value value="13"/>
        <description>
          <text>Type = 130, code = 0.</text>
        </description>
      </enum>
      <enum name="multicast-listener-report">
        <value value="14"/>
        <description>
          <text>Type = 131, code = 0.</text>
        </description>
      </enum>
      <enum name="multicast-listener-done">
        <value value="15"/>
        <description>
          <text>Type = 132, code = 0.</text>
        </description>
      </enum>
      <enum name="multicast-listener-report-v2">
        <value value="16"/>
        <description>
          <text>Type = 143, code = 0.</text>
        </description>
      </enum>
      <enum name="un-famous">
        <value value="17"/>
        <description>
          <text>The ICMPv6 type that is not in the range of all-famous-send-switch, nor in the range of all-famous-receive-switch.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Description of ICMPv6 name type.</text>
    </description>
  </typedef>
  <container name="pp6">
    <description>
      <text>IPv6 related configurations.</text>
    </description>
    <container name="ipv6-global">
      <description>
        <text>Configure global PP6 attribute configuration.</text>
      </description>
      <leaf name="bucket-size">
        <type name="uint32">
          <range value="1..200"/>
        </type>
        <default value="10"/>
        <description>
          <text>Number of ICMPv6 token buckets.</text>
        </description>
      </leaf>
      <leaf name="interval">
        <type name="uint32">
          <range value="0..2147483647"/>
        </type>
        <units name="ms"/>
        <default value="100"/>
        <description>
          <text>ICMPv6 interval for sending a specified number of ICMPv6 token buckets.</text>
        </description>
      </leaf>
      <leaf name="echo-reply">
        <type name="boolean"/>
        <description>
          <text>Enable/disable sending enabling of echo reply packets.</text>
        </description>
      </leaf>
      <leaf name="host-unreach">
        <type name="boolean"/>
        <description>
          <text>Enable/disable host-unreachable error.</text>
        </description>
      </leaf>
      <leaf name="port-unreach">
        <type name="boolean"/>
        <description>
          <text>Enable/disable port-unreachable error.</text>
        </description>
      </leaf>
      <leaf name="timeout">
        <type name="uint32">
          <range value="10..100"/>
        </type>
        <units name="min"/>
        <default value="10"/>
        <description>
          <text>Path-mtu lifecycle.</text>
        </description>
      </leaf>
      <leaf name="black-list-flag">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable the function enabled IPv6 blacklist packet permit.</text>
        </description>
      </leaf>
      <leaf name="too-big-limit-flag">
        <type name="boolean"/>
        <default value="true"/>
        <description>
          <text>Enable/disable the function enabled toobig packet process rate limit.</text>
        </description>
      </leaf>
      <leaf name="permit-incomplete-fragment">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable global flag of allowing first fragment with incomplete IPv6 header chain.</text>
        </description>
      </leaf>
      <leaf name="reassemble-timeout">
        <type name="uint32">
          <range value="5..120"/>
        </type>
        <units name="ms"/>
        <default value="60"/>
        <description>
          <text>The timeout for the split packets reorganization.</text>
        </description>
      </leaf>
    </container>
    <container name="icmp6-pkt-switch">
      <description>
        <text>Configure the ICMPv6 packet sending and receiving switch. All-famous is enabled by default. When matching, a single type of configuration is preferred, followed by an all-famous configuration.</text>
      </description>
      <leaf name="all-famous-send-switch">
        <type name="switch-type"/>
        <default value="allow"/>
        <description>
          <text>Switches for all famous send types. But, the configuration in node each-type-switch will be used first, if it exists. Refer to the definition in type icmp6-name-type for the scope of all-famous send types.</text>
        </description>
      </leaf>
      <leaf name="all-famous-receive-switch">
        <type name="switch-type"/>
        <default value="allow"/>
        <description>
          <text>Switches for all famous receive types. But, the configuration in node each-type-switch will be used first, if it exists. Refer to the definition in type icmp6-name-type for the scope of all-famous receive types.</text>
        </description>
      </leaf>
      <container name="each-type-switchs">
        <description>
          <text>List of switches of a single ICMPv6 type.</text>
        </description>
        <list name="each-type-switch">
          <key value="type code send-or-receive"/>
          <description>
            <text>Configure switch for single ICMPv6 type list.</text>
          </description>
          <leaf name="type">
            <type name="uint32">
              <range value="0..255"/>
            </type>
            <description>
              <text>The ICMPv6 type.</text>
            </description>
          </leaf>
          <leaf name="code">
            <type name="uint32">
              <range value="0..255"/>
            </type>
            <description>
              <text>The ICMPv6 code.</text>
            </description>
          </leaf>
          <leaf name="send-or-receive">
            <type name="send-or-receive-type"/>
            <description>
              <text>Packet reception or packet sending.</text>
            </description>
          </leaf>
          <leaf name="switch">
            <type name="switch-type"/>
            <mandatory value="true"/>
            <description>
              <text>Switch for this ICMPv6 types.</text>
            </description>
          </leaf>
          <leaf name="name">
            <type name="icmp6-name-type"/>
            <config value="false"/>
            <description>
              <text>The name of ICMPv6 type.</text>
            </description>
          </leaf>
        </list>
      </container>
    </container>
    <container name="extension-header-filter-globals">
      <description>
        <text>Configure global IPv6 extension header filter configurations.</text>
      </description>
      <container name="hop-by-hop-switch">
        <description>
          <text>Configure the IPv6 packet with hop-by-hop options extension header switch.</text>
        </description>
        <leaf name="all-options">
          <type name="filter-rule-type"/>
          <description>
            <text>All options of IPv6 extension header switch.</text>
          </description>
        </leaf>
        <container name="options">
          <description>
            <text>List of some options of IPv6 extension header switch.</text>
          </description>
          <list name="option">
            <key value="option-code"/>
            <description>
              <text>Configure some options of IPv6 extension header switch.</text>
            </description>
            <leaf name="option-code">
              <type name="uint8">
                <range value="0..255"/>
              </type>
              <description>
                <text>Option code of IPv6 extension header.</text>
              </description>
            </leaf>
            <leaf name="filter-rule">
              <type name="filter-rule-type"/>
              <mandatory value="true"/>
              <description>
                <text>Filter policy of IPv6 packet.</text>
              </description>
              <ext:support-filter value="true"/>
            </leaf>
          </list>
        </container>
      </container>
      <container name="routing-switch">
        <description>
          <text>Configure the IPv6 packet with routing options extension header switch.</text>
        </description>
        <leaf name="all-options">
          <type name="filter-rule-type"/>
          <description>
            <text>All options of IPv6 extension header switch.</text>
          </description>
        </leaf>
        <container name="options">
          <description>
            <text>List of some options of IPv6 extension header switch.</text>
          </description>
          <list name="option">
            <key value="option-code"/>
            <description>
              <text>Configure some options of IPv6 extension header switch.</text>
            </description>
            <leaf name="option-code">
              <type name="uint8">
                <range value="0..255"/>
              </type>
              <description>
                <text>Option code of IPv6 extension header.</text>
              </description>
            </leaf>
            <leaf name="filter-rule">
              <type name="filter-rule-type"/>
              <mandatory value="true"/>
              <description>
                <text>Filter policy of IPv6 packet.</text>
              </description>
              <ext:support-filter value="true"/>
            </leaf>
          </list>
        </container>
      </container>
      <container name="destination-switch">
        <description>
          <text>Configure the IPv6 packet with destination options extension header switch.</text>
        </description>
        <leaf name="all-options">
          <type name="filter-rule-type"/>
          <description>
            <text>All options of IPv6 extension header switch.</text>
          </description>
        </leaf>
        <container name="options">
          <description>
            <text>List of some options of IPv6 extension header switch.</text>
          </description>
          <list name="option">
            <key value="option-code"/>
            <description>
              <text>Configure some options of IPv6 extension header switch.</text>
            </description>
            <leaf name="option-code">
              <type name="uint8">
                <range value="0..255"/>
              </type>
              <description>
                <text>Option code of IPv6 extension header.</text>
              </description>
            </leaf>
            <leaf name="filter-rule">
              <type name="filter-rule-type"/>
              <mandatory value="true"/>
              <description>
                <text>Filter policy of IPv6 packet.</text>
              </description>
              <ext:support-filter value="true"/>
            </leaf>
          </list>
        </container>
      </container>
      <container name="no-option-exh-switchs">
        <description>
          <text>List of the IPv6 packet with no options extension header switches.</text>
        </description>
        <list name="no-option-exh-switch">
          <key value="exh-type"/>
          <description>
            <text>Configure the IPv6 packet with no options extension header switches.</text>
          </description>
          <leaf name="exh-type">
            <type name="extension-header-type"/>
            <description>
              <text>IPv6 extension header type which have no option.</text>
            </description>
          </leaf>
          <leaf name="filter-rule">
            <type name="filter-rule-type"/>
            <mandatory value="true"/>
            <description>
              <text>Filter policy of IPv6 packet.</text>
            </description>
            <ext:support-filter value="true"/>
          </leaf>
        </list>
      </container>
    </container>
    <container name="ipv6-stat">
      <config value="false"/>
      <description>
        <text>Statistics of querying for the statistics on IPv6 packets.</text>
      </description>
      <leaf name="total-snd">
        <type name="uint32"/>
        <config value="false"/>
        <description>
          <text>Total count of sent packets.</text>
        </description>
      </leaf>
      <leaf name="local-snd">
        <type name="uint32"/>
        <config value="false"/>
        <description>
          <text>Count of packets that are sent out locally.</text>
        </description>
      </leaf>
      <leaf name="forwd-snd">
        <type name="uint32"/>
        <config value="false"/>
        <description>
          <text>Count of forwarded packets.</text>
        </description>
      </leaf>
      <leaf name="rawpkt-snd">
        <type name="uint32"/>
        <config value="false"/>
        <description>
          <text>Count of sent Raw packets.</text>
        </description>
      </leaf>
      <leaf name="discard-snd">
        <type name="uint32"/>
        <config value="false"/>
        <description>
          <text>Count of discarded packets due to the limit of sending rate.</text>
        </description>
      </leaf>
      <leaf name="fragmented">
        <type name="uint32"/>
        <config value="false"/>
        <description>
          <text>Count of sent fragmented packets.</text>
        </description>
      </leaf>
      <leaf name="fragment-snd">
        <type name="uint32"/>
        <config value="false"/>
        <description>
          <text>Count of sent packet fragments.</text>
        </description>
      </leaf>
      <leaf name="fragment-fail">
        <type name="uint32"/>
        <config value="false"/>
        <description>
          <text>Count of packet fragments that fail to be sent.</text>
        </description>
      </leaf>
      <leaf name="multi-snd">
        <type name="uint32"/>
        <config value="false"/>
        <description>
          <text>Count of sent multicast packets.</text>
        </description>
      </leaf>
      <leaf name="total-rcv">
        <type name="uint32"/>
        <config value="false"/>
        <description>
          <text>Total count of received packets.</text>
        </description>
      </leaf>
      <leaf name="local-rcv">
        <type name="uint32"/>
        <config value="false"/>
        <description>
          <text>Count of packets that are received locally.</text>
        </description>
      </leaf>
      <leaf name="hop-exceed-rcv">
        <type name="uint32"/>
        <config value="false"/>
        <description>
          <text>Count of received packets whose number of hops exceeds the upper limit.</text>
        </description>
      </leaf>
      <leaf name="head-err-rcv">
        <type name="uint32"/>
        <config value="false"/>
        <description>
          <text>Count of packets whose headers are incorrect.</text>
        </description>
      </leaf>
      <leaf name="too-big-rcv">
        <type name="uint32"/>
        <config value="false"/>
        <description>
          <text>Count of received oversized packets.</text>
        </description>
      </leaf>
      <leaf name="routfail-rcv">
        <type name="uint32"/>
        <config value="false"/>
        <description>
          <text>Count of received packets whose routes are unreachable.</text>
        </description>
      </leaf>
      <leaf name="addrerr-rcv">
        <type name="uint32"/>
        <config value="false"/>
        <description>
          <text>Count of received packets with incorrect addresses.</text>
        </description>
      </leaf>
      <leaf name="protoerr-rcv">
        <type name="uint32"/>
        <config value="false"/>
        <description>
          <text>Count of received packets whose protocols are incorrect.</text>
        </description>
      </leaf>
      <leaf name="truncated-rcv">
        <type name="uint32"/>
        <config value="false"/>
        <description>
          <text>Count of received truncated packets.</text>
        </description>
      </leaf>
      <leaf name="opterr-rcv">
        <type name="uint32"/>
        <config value="false"/>
        <description>
          <text>Count of received packets whose options are incorrect.</text>
        </description>
      </leaf>
      <leaf name="frags-rcv">
        <type name="uint32"/>
        <config value="false"/>
        <description>
          <text>Count of received packet fragments.</text>
        </description>
      </leaf>
      <leaf name="reassed-rcv">
        <type name="uint32"/>
        <config value="false"/>
        <description>
          <text>Count of received packets that are reassembled.</text>
        </description>
      </leaf>
      <leaf name="reass-timeout-rcv">
        <type name="uint32"/>
        <config value="false"/>
        <description>
          <text>Count of received packets whose reassembling is timeout.</text>
        </description>
      </leaf>
      <leaf name="multi-rcv">
        <type name="uint32"/>
        <config value="false"/>
        <description>
          <text>Count of received multicast packets.</text>
        </description>
      </leaf>
      <leaf name="hop-by-hop-option">
        <type name="uint32"/>
        <config value="false"/>
        <description>
          <text>Count of received packets whose extension headers are Hop-by-Hop options.</text>
        </description>
      </leaf>
      <leaf name="mobility-option">
        <type name="uint32"/>
        <config value="false"/>
        <description>
          <text>Count of received packets whose extension headers are mobility header.</text>
        </description>
      </leaf>
      <leaf name="dest-option">
        <type name="uint32"/>
        <config value="false"/>
        <description>
          <text>Count of received packets whose extension headers are destination options.</text>
        </description>
      </leaf>
      <leaf name="routing-option">
        <type name="uint32"/>
        <config value="false"/>
        <description>
          <text>Count of received packets whose extension headers are routing header.</text>
        </description>
      </leaf>
      <leaf name="fragment-option">
        <type name="uint32"/>
        <config value="false"/>
        <description>
          <text>Count of received packets whose extension headers are fragment header.</text>
        </description>
      </leaf>
      <leaf name="auth-option">
        <type name="uint32"/>
        <config value="false"/>
        <description>
          <text>Count of received packets whose extension headers are authentication header.</text>
        </description>
      </leaf>
      <leaf name="encap-option">
        <type name="uint32"/>
        <config value="false"/>
        <description>
