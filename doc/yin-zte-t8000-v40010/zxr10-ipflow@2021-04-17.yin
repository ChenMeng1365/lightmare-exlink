      <container name="samplers">
        <description>
          <text>IPFLOW flow sampler information</text>
        </description>
        <list name="sampler">
          <description>
            <text>IPFLOW flow sampler information</text>
          </description>
          <key value="sampler-name"/>
          <leaf name="sampler-name">
            <description>
              <text>IPFLOW sampler name</text>
            </description>
            <type name="string">
              <length value="1..32"/>
            </type>
          </leaf>
          <leaf name="sample-rate">
            <status value="obsolete">
              <ncd:revise-date date="2019-05-14"/>
            </status>
            <description>
              <text>IPFLOW sampling rate</text>
            </description>
            <type name="uint32">
              <range value="0..4294967295"/>
            </type>
            <default value="1000"/>
          </leaf>
          <leaf name="sample-mode">
            <status value="obsolete">
              <ncd:revise-date date="2019-05-14"/>
            </status>
            <description>
              <text>IPFLOW sampling mode</text>
            </description>
            <type name="enumeration">
              <enum name="deterministic">
                <value value="1"/>
              </enum>
            </type>
            <default value="deterministic"/>
          </leaf>
          <choice name="mode">
            <description>
              <text>Specify the type of sampling and the sampling interval.</text>
            </description>
            <default value="deterministic"/>
            <case name="deterministic">
              <description>
                <text>Enable deterministic mode sampling.</text>
              </description>
              <container name="deterministic">
                <description>
                  <text>Enable deterministic mode sampling.</text>
                </description>
                <leaf name="sample-rate">
                  <description>
                    <text>IPFLOW sampling rate.</text>
                  </description>
                  <type name="uint32">
                    <range value="0..4294967295"/>
                  </type>
                  <default value="1000"/>
                </leaf>
              </container>
            </case>
            <case name="time-deterministic">
              <description>
                <text>Enable time-deterministic mode sampling.</text>
              </description>
              <container name="time-deterministic">
                <description>
                  <text>Enable time-deterministic mode sampling.</text>
                </description>
                <presence value="time deterministic"/>
                <leaf name="time-space">
                  <description>
                    <text>Sampling time space.</text>
                  </description>
                  <type name="uint32">
                    <range value="1..30000000"/>
                  </type>
                  <mandatory value="true"/>
                </leaf>
              </container>
            </case>
            <case name="random">
              <description>
                <text>Enable random mode sampling.</text>
              </description>
              <container name="random">
                <description>
                  <text>Enable random mode sampling.</text>
                </description>
                <presence value="random"/>
                <leaf name="random-size">
                  <description>
                    <text>Random sampling size.</text>
                  </description>
                  <type name="uint32">
                    <range value="1..65535"/>
                  </type>
                  <mandatory value="true"/>
                </leaf>
                <leaf name="random-population">
                  <description>
                    <text>Random sampling population.</text>
                  </description>
                  <type name="uint32">
                    <range value="1..65535"/>
                  </type>
                  <mandatory value="true"/>
                </leaf>
              </container>
            </case>
            <ncd:revise-date date="2019-05-14"/>
          </choice>
        </list>
      </container>
      <container name="exporters">
        <description>
          <text>IPFLOW flow exporter</text>
        </description>
        <list name="exporter">
          <description>
            <text>IPFLOW flow exporter</text>
          </description>
          <key value="exporter-name"/>
          <leaf name="exporter-name">
            <description>
              <text>IPFLOW exporter name</text>
            </description>
            <type name="string">
              <length value="1..32"/>
            </type>
          </leaf>
          <leaf name="export-protocol">
            <description>
              <text>IPFLOW exporter format</text>
            </description>
            <type name="enumeration">
              <enum name="netflow-v5">
                <value value="5"/>
              </enum>
              <enum name="netflow-v8">
                <value value="8"/>
              </enum>
              <enum name="netflow-v9">
                <value value="9"/>
              </enum>
              <enum name="ipfix">
                <value value="10"/>
              </enum>
            </type>
            <default value="netflow-v9"/>
          </leaf>
          <leaf name="transport-protocol">
            <description>
              <text>IPFLOW transport protocol</text>
            </description>
            <type name="enumeration">
              <enum name="udp">
                <value value="1"/>
              </enum>
            </type>
            <default value="udp"/>
          </leaf>
          <leaf name="port-num">
            <description>
              <text>IPFLOW transport port number</text>
            </description>
            <type name="uint16">
              <range value="1..65535"/>
            </type>
            <default value="2055"/>
          </leaf>
          <leaf name="template-resend-timeout">
            <description>
              <text>IPFLOW template resend timeout</text>
            </description>
            <type name="uint32">
              <range value="1..86400"/>
            </type>
            <units name="second"/>
            <default value="600"/>
          </leaf>
          <leaf name="template-resend-pkt-num">
            <description>
              <text>IPFLOW template refresh rate</text>
            </description>
            <type name="uint32">
              <range value="1..600"/>
            </type>
            <default value="20"/>
          </leaf>
          <leaf name="dscp">
            <description>
              <text>IPFLOW exporter DSCP value</text>
            </description>
            <type name="uint8">
              <range value="0..63"/>
            </type>
            <default value="0"/>
          </leaf>
          <container name="destination">
            <description>
              <text>IPFLOW destination information</text>
            </description>
            <choice name="ip-type">
              <description>
                <text>IPFLOW destination address type</text>
              </description>
              <case name="ipv4">
                <description>
                  <text>IPFLOW IPv4 type</text>
                </description>
                <leaf name="ipv4-addr">
                  <description>
