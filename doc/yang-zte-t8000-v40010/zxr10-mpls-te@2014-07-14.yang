      container passive-if {
        description 'The configuration of passive interface.Traffic engineering'
                  + ' flood the interface information to IGP-TE for OSPF path c'
                  + 'ompute at inter-area condition.'
                    ;
        presence 'configuration of passive-if' ;
        list cross-domain {
          description 'Passive interface list.' ;
          key 'if-name nbr-route-id' ;
          ncd:select 'if-name nbr-route-id' ;
          max-elements 2550 ;
          leaf if-name {
            description 'Interface name.' ;
            type ncd:ifname ;
          }
          leaf nbr-route-id {
            description 'The neighbor route ID.' ;
            type ncd:ipv4-address ;
          }
          leaf nbr-if-addr {
            description 'Neighbor interface address.' ;
            type ncd:ipv4-address {
              pattern '([1-9]|[1-9][0-9]|1[0-9][0-9]|2[0-4][0-9]|25[0-5])(.([0-'
                    + '9]|[1-9][0-9]|1[0-9][0-9]|2[0-4][0-9]|25[0-5])){3}'
                      {
                description 'To filter the invalid address.' ;
              }
            }
          }
          leaf nbr-ospf-node-id {
            description 'Neighbor OSPF node ID.' ;
            type ncd:ipv4-address {
              pattern '([1-9]|[1-9][0-9]|1[0-9][0-9]|2[0-4][0-9]|25[0-5])(.([0-'
                    + '9]|[1-9][0-9]|1[0-9][0-9]|2[0-4][0-9]|25[0-5])){3}'
                      {
                description 'To filter the invalid address.' ;
              }
            }
          }
          leaf nbr-isis-system-id {
            description 'Neighbor ISIS system ID.' ;
            type ncd:mac-address ;
            default 00:00:00:00:00:00 ;
          }
        }
