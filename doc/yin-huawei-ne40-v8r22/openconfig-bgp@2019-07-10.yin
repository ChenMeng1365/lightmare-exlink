  <grouping name="bgp-top">
    <description>
      <text>Top-level grouping for the BGP model data</text>
    </description>
    <container name="bgp">
      <description>
        <text>Top-level configuration and state for the BGP router</text>
      </description>
      <container name="global">
        <description>
