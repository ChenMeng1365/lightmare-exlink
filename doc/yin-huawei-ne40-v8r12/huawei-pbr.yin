                      <path value="/nqa:nqa/nqa:instances/nqa:instance/nqa:admin-name"/>
                    </type>
                    <must condition="../vrf-name and ../nqa-instance"/>
                    <description>
                      <text>NQA admin name.</text>
                    </description>
                  </leaf>
                  <leaf name="nqa-instance">
                    <type name="leafref">
                      <path value="/nqa:nqa/nqa:instances/nqa:instance[nqa:admin-name=current()/../nqa-admin-name]/nqa:test-name"/>
                    </type>
                    <must condition="../nqa-admin-name"/>
                    <description>
                      <text>NQA instance.</text>
                    </description>
                  </leaf>
                </list>
              </container>
            </container>
          </case>
          <case name="ipv6-multinhp-nqa">
            <description>
              <text>Specify NQA test class.</text>
            </description>
            <container name="ipv6-multinhp-nqa">
              <description>
                <text>Configure NQA test class.</text>
              </description>
              <leaf name="routing-filter">
                <type name="routing-type"/>
                <default value="false"/>
                <description>
                  <text>Routing filter default and blackhole route.</text>
                </description>
              </leaf>
              <leaf name="deny">
                <type name="boolean"/>
                <default value="false"/>
                <description>
                  <text>Enable/disable packets are dropped when routes are invalid.</text>
                </description>
              </leaf>
              <leaf name="common-route-priority">
                <type name="boolean"/>
                <default value="false"/>
                <description>
                  <text>Enable/disable common route priority.</text>
                </description>
              </leaf>
              <container name="multi-nqas">
                <description>
                  <text>List of nexthop list.</text>
                </description>
                <list name="multi-nqa">
                  <key value="nexthop-address nqa-admin-name nqa-instance"/>
                  <unique tag="nexthop-address"/>
                  <min-elements value="2"/>
                  <max-elements value="16"/>
                  <description>
                    <text>Configure nexthop.</text>
                  </description>
                  <leaf name="nexthop-address">
                    <type name="inet:ipv6-address-no-zone"/>
                    <description>
                      <text>Nexthop address.</text>
                    </description>
                  </leaf>
                  <leaf name="nqa-admin-name">
                    <type name="leafref">
                      <path value="/nqa:nqa/nqa:instances/nqa:instance/nqa:admin-name"/>
                    </type>
                    <description>
                      <text>NQA admin name.</text>
                    </description>
                  </leaf>
                  <leaf name="nqa-instance">
                    <type name="leafref">
                      <path value="/nqa:nqa/nqa:instances/nqa:instance[nqa:admin-name=current()/../nqa-admin-name]/nqa:test-name"/>
                    </type>
                    <description>
                      <text>NQA instance.</text>
                    </description>
                  </leaf>
                </list>
              </container>
            </container>
          </case>
        </choice>
      </container>
    </container>
  </augment>
  <augment target-node="/devm-vnf:devm-vnf/devm-vnf:vnf-boards/devm-vnf:vnf-board">
    <description>
      <text>PBR.</text>
    </description>
    <container name="pbr">
      <description>
        <text>Configure To filter default route or black hole route when using redirect.</text>
      </description>
      <container name="routing-filters">
        <description>
          <text>List of to filter default route or black hole route when using redirect.</text>
        </description>
        <list name="routing-filter">
          <key value="type"/>
          <max-elements value="2"/>
          <description>
            <text>Configure action of redirecting to public-network.</text>
          </description>
          <leaf name="type">
            <type name="ip-type"/>
            <description>
              <text>Protocol family.</text>
            </description>
          </leaf>
          <leaf name="routing-type">
            <type name="routing-filter-type"/>
            <mandatory value="true"/>
            <description>
              <text>Routing type.</text>
            </description>
          </leaf>
        </list>
      </container>
    </container>
  </augment>
</module>
