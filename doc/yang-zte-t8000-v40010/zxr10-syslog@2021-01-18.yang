  typedef alarm-level {
    type enumeration {
      enum emergencies {
        value 1 ;
        description 'Level of emergency.'
                    ;
      }
      enum alerts {
        value 2 ;
        description 'Level of alert.'
                    ;
      }
      enum critical {
        value 3 ;
        description 'Level of  critical.'
                    ;
      }
      enum errors {
        value 4 ;
        description 'Level of error.'
                    ;
      }
      enum warnings {
        value 5 ;
        description 'Level of warning.'
                    ;
      }
      enum notifications {
        value 6 ;
        description 'Level of notification.'
                    ;
      }
      enum informational {
        value 7 ;
        description 'Level of information.'
                    ;
      }
      enum debugging {
        value 8 ;
        description 'Level of debug.'
                    ;
      }
    }
    default notifications ;
    description 'Alarm level.'
                ;
  }
  typedef syslog-facility {
    type enumeration {
      enum kern {
        value 0 ;
        description Kernel. ;
      }
      enum user {
        value 1 ;
        description 'User process.'
                    ;
      }
      enum mail {
        value 2 ;
        description 'Mail system.'
                    ;
      }
      enum daemon {
        value 3 ;
        description 'System daemons.'
                    ;
      }
      enum auth1 {
        value 4 ;
        description 'Authorization system.'
                    ;
      }
      enum syslog {
        value 5 ;
        description 'Syslog itself.'
                    ;
      }
      enum lpr {
        value 6 ;
        description 'Line printer system.'
                    ;
      }
      enum news {
        value 7 ;
        description 'USENET news.'
                    ;
      }
      enum uucp {
        value 8 ;
        description 'Unix-to-Unix copy system.'
                    ;
      }
      enum clock1 {
        value 9 ;
        description 'Clock process.'
                    ;
      }
      enum auth2 {
        value 10 ;
        description 'Authorization system.'
                    ;
      }
      enum ftp {
        value 11 ;
        description 'FTP process.'
                    ;
      }
      enum ntp {
        value 12 ;
        description 'NTP process.'
                    ;
      }
      enum log-audit {
        value 13 ;
        description 'Log audit.'
                    ;
      }
      enum log-alert {
        value 14 ;
        description 'Log alert.'
                    ;
      }
      enum clock2 {
        value 15 ;
        description 'Clock daemons.'
                    ;
      }
      enum local0 {
        value 16 ;
        description 'Local use.'
                    ;
      }
      enum local1 {
        value 17 ;
        description 'Local use.'
                    ;
      }
      enum local2 {
        value 18 ;
        description 'Local use.'
                    ;
      }
      enum local3 {
        value 19 ;
        description 'Local use.'
                    ;
      }
      enum local4 {
        value 20 ;
        description 'Local use.'
                    ;
      }
      enum local5 {
        value 21 ;
        description 'Local use.'
                    ;
      }
      enum local6 {
        value 22 ;
        description 'Local use.'
                    ;
      }
      enum local7 {
        value 23 ;
        description 'Local use.'
                    ;
      }
    }
    default local0 ;
    description 'Syslog facility.'
