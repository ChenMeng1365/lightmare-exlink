  <container name="configuration">
    <description>
      <text>The branch of configuration model.</text>
    </description>
    <container name="cac">
      <description>
        <text>The configuration of CAC.</text>
      </description>
      <leaf name="cac-enable">
        <description>
          <text>The global switch of CAC.</text>
        </description>
        <type name="enumeration">
          <enum name="disabled">
            <value value="0"/>
          </enum>
          <enum name="enabled">
            <value value="1"/>
          </enum>
        </type>
        <default value="enabled"/>
      </leaf>
      <ncd:revise-date date="2018-07-14"/>
    </container>
    <container name="mpls-te">
      <description>
        <text>The configuration of MPLS TE.</text>
      </description>
      <leaf name="te-enable">
        <description>
          <text>The global switch of TE.</text>
        </description>
        <type name="enumeration">
          <enum name="disabled">
            <value value="0"/>
          </enum>
          <enum name="enabled">
            <value value="1"/>
          </enum>
        </type>
        <default value="disabled"/>
      </leaf>
      <container name="global-config">
        <description>
          <text>TE global common configuration. Include refresh configuration, retran and srefresh congiguration, explicit none congiguration and resv confirm congiguration.</text>
        </description>
        <presence value="global configuration"/>
        <container name="refresh-config">
          <description>
            <text>The signalling packet containing path and resv refresh configuration.</text>
          </description>
          <leaf name="interval">
            <description>
              <text>Configuration of time interval for sending refresh message, range: from 5000 to 65535000,
units: ms, default value is 30000 ms.</text>
            </description>
            <type name="uint32">
              <range value="5000..65535000"/>
            </type>
            <units name="ms"/>
            <default value="30000"/>
          </leaf>
          <leaf name="misses">
            <description>
              <text>Configuration of miss number for refresh message, range: from 2 to 10, default value is 4.</text>
            </description>
            <type name="uint32">
              <range value="2..10"/>
            </type>
            <default value="4"/>
          </leaf>
        </container>
        <container name="retran-config">
          <description>
            <text>The retransmit configuration.</text>
          </description>
          <leaf name="enable">
            <description>
              <text>The switch of retransmit. Ref-reduction-enable is depend on retran-config enable.</text>
            </description>
            <type name="empty"/>
          </leaf>
          <leaf name="interval">
            <description>
              <text>The interval of retransmit signalling message, range: from 500 to 60000, unit: ms, default value: 1000 ms.</text>
            </description>
            <type name="uint32">
              <range value="500..60000"/>
            </type>
            <units name="ms"/>
            <default value="1000"/>
          </leaf>
          <leaf name="limit">
            <description>
              <text>The limit of signalling message retransmit, range: from 2 to 10, default value: 3.</text>
            </description>
            <type name="uint32">
              <range value="2..10"/>
            </type>
            <default value="3"/>
          </leaf>
          <leaf name="ref-reduction-enable">
            <description>
              <text>Enable summary refresh function, default value is disabled.
Ref-reduction-enable is depend on retran-config enable.</text>
            </description>
            <type name="enumeration">
              <enum name="disabled">
                <value value="0"/>
              </enum>
              <enum name="enabled">
                <value value="1"/>
              </enum>
            </type>
            <default value="disabled"/>
          </leaf>
        </container>
        <container name="explicit-none">
          <description>
            <text>The egress label management. The default label is implicit-null.</text>
          </description>
          <leaf name="explicit-null">
            <description>
              <text>When this attribute is configured, it indicates that the label of egress node is explicit-null.</text>
            </description>
            <type name="boolean"/>
            <default value="false"/>
          </leaf>
          <leaf name="none-null">
            <description>
              <text>When this attribute is configured, it indicates that the label of egress node is none-null.</text>
            </description>
            <type name="boolean"/>
            <default value="false"/>
          </leaf>
        </container>
        <container name="sd-config">
          <description>
            <text>Configuration of SD.</text>
          </description>
          <leaf name="enable">
            <description>
              <text>The switch of sd. default value is disabled.</text>
            </description>
            <type name="enumeration">
              <enum name="disabled">
                <value value="0"/>
              </enum>
              <enum name="enabled">
                <value value="1"/>
              </enum>
            </type>
            <default value="disabled"/>
          </leaf>
          <leaf name="interval">
            <description>
              <text>The interval of sd alarm, range: from 10 to 30, unit: s, default value: 10 s.</text>
            </description>
            <type name="int16">
              <range value="10..30"/>
            </type>
            <units name="second"/>
            <default value="10"/>
          </leaf>
        </container>
        <container name="resv-confirm">
          <description>
            <text>The resv confirm.</text>
          </description>
          <leaf name="enable">
            <description>
              <text>Enable resv confirm function, default value is disabled.
It is configured on egress.</text>
            </description>
            <type name="enumeration">
              <enum name="disabled">
                <value value="0"/>
              </enum>
              <enum name="enabled">
                <value value="1"/>
              </enum>
            </type>
            <default value="disabled"/>
          </leaf>
        </container>
        <container name="router-id">
          <description>
            <text>Configuration of router-id.</text>
          </description>
          <leaf name="route-id">
            <description>
              <text>The TE route ID configuration. Must config router id before any other configuration under dynamic-tunnl node and ensure that router id is reachable in route.</text>
            </description>
            <type name="ncd:ipv4-address">
              <pattern value="([1-9]|[1-9][0-9]|1[0-9][0-9]|2[0-4][0-9]|25[0-5])(.([0-9]|[1-9][0-9]|1[0-9][0-9]|2[0-4][0-9]|25[0-5])){3}"/>
            </type>
          </leaf>
        </container>
        <container name="ipv6-router-id">
          <leaf name="ipv6-router-id">
            <description>
              <text>The TE ipv6 route ID configuration. Must config ipv6 router id before any other configuration about SRv6 tunnel and ensure that ipv6 router id is reachable in route.</text>
            </description>
            <type name="ncd:ipv6-address"/>
          </leaf>
          <ncd:revise-date date="2018-05-14"/>
        </container>
        <container name="tnnl-unact-time">
          <description>
            <text>Tunnel reconstruction interval regularly.</text>
          </description>
          <leaf name="interval">
            <description>
              <text>Tunnel reconstruction interval regularly.</text>
            </description>
            <type name="uint32">
              <range value="30..300"/>
            </type>
            <units name="second"/>
          </leaf>
        </container>
        <container name="delay">
          <description>
            <text>Static TE delay function of management object.</text>
          </description>
          <leaf name="delay_switch_over">
            <description>
              <text>Time delay back cut.</text>
            </description>
            <type name="uint32">
              <range value="0..90"/>
            </type>
            <units name="second"/>
            <default value="0"/>
          </leaf>
          <leaf name="delay_reactive">
            <description>
              <text>Delay reactive time.</text>
            </description>
            <type name="uint32">
              <range value="0..30"/>
            </type>
            <units name="second"/>
            <default value="10"/>
          </leaf>
          <leaf name="reactive-precise">
            <description>
              <text>Delay reactive precise time.</text>
            </description>
            <type name="uint8">
              <range value="1..9">
                <description>
                  <text>reactive precise time(100ms)</text>
                </description>
              </range>
            </type>
            <units name="100ms"/>
            <ncd:revise-date date="2018-01-17"/>
          </leaf>
          <leaf name="delay_linkup">
            <description>
              <text>The linkup of time delay response.</text>
            </description>
            <type name="uint32">
              <range value="0..120"/>
            </type>
            <units name="second"/>
            <default value="0"/>
          </leaf>
          <leaf name="delay-topo-change">
            <description>
              <text>Topology-change delay time(default:5s)</text>
            </description>
            <type name="uint32">
              <range value="0..30"/>
            </type>
            <units name="second"/>
            <default value="5"/>
            <ncd:revise-date date="2017-09-18"/>
          </leaf>
        </container>
        <container name="cspf-preferred-igp">
          <presence value="cspf preferred igp"/>
          <choice name="preferred-igp-type">
            <case name="ospf-preferred">
              <leaf name="preferred-igp-ospf">
                <description>
                  <text>Specify OSPF for CSPF routing to use prior.</text>
                </description>
                <type name="enumeration">
                  <enum name="ospf">
                    <value value="1"/>
                  </enum>
                </type>
                <mandatory value="true"/>
              </leaf>
              <container name="ospf-process">
                <choice name="ospf-process-config">
                  <case name="ospf-process-configed">
                    <leaf name="ospf-process-id">
                      <description>
                        <text>OSPF process ID</text>
                      </description>
                      <type name="uint32">
                        <range value="1..65535"/>
                      </type>
                      <mandatory value="true"/>
                    </leaf>
                    <container name="ospf-area">
                      <choice name="ospf-area-config">
                        <case name="ospf-area-configed">
                          <leaf name="ospf-area-id">
                            <description>
                              <text>OSPF area ID</text>
                            </description>
                            <type name="ncd:ipv4-address"/>
                            <mandatory value="true"/>
                          </leaf>
                        </case>
                      </choice>
                    </container>
                  </case>
                </choice>
              </container>
            </case>
