  revision 2013-09-17 {
    description 'Modify the key of list member in state ,delete the mandatory a'
              + 'nd add length range of leaf name in list VPLS.'
                ;
  }
  revision 2013-08-01 {
    description 'Initial version.'
                ;
  }
  feature vpls {
    description 'IGMP snooping over VPLS.'
                ;
  }
  feature vlan {
    description 'IGMP snooping over VLAN.'
                ;
  }
  feature vxlan {
    description 'IGMP snooping over VXLAN.'
                ;
  }
  feature vbui {
    description 'IGMP snooping over VBUI.'
                ;
  }
  feature vrf {
    description 'IGMP snooping over VRF.'
                ;
  }
  feature ip-global {
    description 'IGMP snooping over IP-global.'
                ;
  }
  feature ring-tree {
    description 'IGMP snooping over Ring Tree.'
                ;
  }
  feature host-aging-closedown {
    description 'Host aging closedown.'
                ;
  }
  feature transmit-to-block-port {
    description 'Transmit packets through block ports.'
                ;
  }
  feature topology-change {
    description 'Response or send packets when the topology changes.'
                ;
  }
  feature max-host-in-group {
    description 'Max access members in the specific group.'
                ;
  }
  feature max-host-in-interface {
    description 'Max access members in the specific interface.'
                ;
  }
  feature max-host-in-vlan {
    description 'Max access members in the specific VLAN.'
                ;
  }
  feature group-specific-query-suppress {
    description 'Group-specific query suppression in transparent mode.'
                ;
  }
  feature supervlan-member {
    description 'Supervlan members.'
                ;
  }
  feature transmit-from-block-port {
    description 'Transmit packets from block ports.'
                ;
  }
  feature group {
    description 'IGMP snooping group configuration.'
                ;
  }
  feature interface {
    description 'IGMP snooping interface configuration.'
                ;
  }
  feature mrouter-version {
    description 'IGMP snooping mrouter verison.'
                ;
  }
  feature redundancy-pw-mrouter-transmitting {
    description 'The mode of transmitting IGMP report/leave packets from mroute'
              + 'r can be configured.'
                ;
  }
  feature vlan-query-interval {
    description 'The query interval configuration in vlan mode.'
                ;
  }
  feature evpn-vpls {
    description 'IGMP snooping over EVPN-VPLS.'
                ;
  }
  feature querier-send-to-mrouter {
    description 'Querier sends query to mrouter.'
                ;
  }
  feature logging {
    description 'Logging igmp packets.'
                ;
  }
  feature mvpls {
    description 'IGMP snooping mvpls configuration.'
                ;
  }
  feature general-state-mrouter {
    description 'IGMP snooping suppoer general state mrouter.'
                ;
  }
  typedef switch-type {
    type enumeration {
      enum disable {
        value 0 ;
        description 'Disable this switch.'
                    ;
      }
      enum enable {
        value 1 ;
        description 'Enable this switch.'
                    ;
      }
    }
    description 'Type of switch status.'
                ;
  }
  typedef packet-dispose-type {
    type enumeration {
      enum accept {
        value 0 ;
        description 'Accept packet mode.'
                    ;
      }
      enum ignore {
        value 1 ;
        description 'Ignore packet mode.'
                    ;
      }
      enum discard {
        value 2 ;
        description 'Discard packet mode.'
                    ;
      }
    }
    description 'Type of packet dispose.'
                ;
  }
  typedef work-mode-type {
    type enumeration {
      enum proxy {
        value 0 ;
        description 'Proxy mode.'
                    ;
      }
      enum route {
        value 1 ;
        description 'Route mode'
                    ;
      }
      enum transparent {
        value 2 ;
        description 'Transparent mode.'
                    ;
      }
    }
    description 'Type of work mode.'
                ;
  }
  typedef version-type {
    type enumeration {
      enum v1 {
        value 1 ;
        description 'IGMP version1.'
                    ;
      }
      enum v2 {
        value 2 ;
        description 'IGMP version2.'
                    ;
      }
      enum v3 {
        value 3 ;
        description 'IGMP version3.'
                    ;
      }
    }
    description 'The version of IGMP.'
                ;
  }
  typedef mode-type {
    type enumeration {
      enum dynamic {
        value 0 ;
        description 'Dynamic mode.'
                    ;
      }
      enum static {
        value 1 ;
        description 'Static mode.'
                    ;
      }
    }
    description 'Type of mode. include dynamic mode or static mode.'
                ;
  }
  typedef mrouter-version-type {
    type enumeration {
      enum v1 {
        value 5 ;
        description 'Mrouter port version 1.'
                    ;
      }
      enum v2 {
        value 7 ;
        description 'Mrouter port version 2.'
                    ;
      }
      enum v3 {
        value 9 ;
        description 'Mrouter port version 3.'
                    ;
      }
    }
    description 'The version of IGMP mrouter port.'
                ;
  }
  typedef redundancy-pw-transmitting-type {
    type enumeration {
      enum double {
        value 2 ;
        description 'If set to double,It means that IGMP packets will be sent t'
                  + 'hrough both working PW and protection PW.'
                    ;
      }
      enum single {
        value 1 ;
        description 'If set to single,It means that IGMP packets will be sent t'
                  + 'hrough only working PW.'
                    ;
      }
    }
    description 'Redundancy PW transmitting mode:single or double.'
                ;
  }
  container configuration {
    description 'Configuration model.'
                ;
    container igmp-snooping {
      description 'Configure IGMP snooping parameters.'
                  ;
      container global {
        description 'IGMP snooping global configure.'
                    ;
        leaf global-enable {
          description 'Enable/disable IGMP snooping in this device.Its default '
                    + 'value depends on the product.'
                      ;
          type switch-type ;
        }
        leaf global-query-enable {
          description 'Enable/disable IGMP queries in this device.'
                      ;
          type switch-type ;
          default disable ;
        }
        leaf query-interval {
          description 'Configure query interval.The query interval is the inter'
                    + 'val between general queries sent by the querier.'
                      ;
          type uint16 {
            range 30..65535 ;
          }
          units second ;
          default 125 ;
        }
        leaf query-rsp-interval {
          description 'Configure query response interval. The response interval'
                    + ' inserted into the periodic general queries.'
                      ;
          type uint8 {
            range 1..255 ;
          }
          units 'tick (1 tick = 100ms)'
                ;
          default 100 ;
        }
        leaf v1-packet-dispose {
          description 'The disposal mode of IGMPv1 Report message.'
                      ;
          type packet-dispose-type ;
          default accept ;
        }
        leaf v2-packet-dispose {
          description 'The disposal mode of IGMPV2 Report/Leave message.'
                      ;
          type packet-dispose-type ;
          default accept ;
        }
        leaf v3-packet-dispose {
          description 'The disposal mode of IGMPV3 Report message.'
                      ;
          type packet-dispose-type ;
          default discard ;
        }
        leaf host-aging-closedown {
          // if-feature host-aging-closedown ;
          description 'Configure host aging closedown.'
                      ;
          type switch-type ;
          default disable ;
          ncd:revise-date 2018-02-12 ;
        }
        container transmit-to-block-port {
          // if-feature transmit-to-block-port ;
          description 'Transmit packets through block ports.'
                      ;
          leaf trans-query-to-block-port {
            description 'Configure query packet through blocked port.'
                        ;
            type switch-type ;
            default disable ;
          }
          leaf trans-report-leave-to-block-port {
            description 'Configure report and leave packet through blocked port.'
                        ;
            type switch-type ;
            default disable ;
          }
          ncd:revise-date 2018-02-12 ;
        }
        container transmit-from-block-port {
          // if-feature transmit-from-block-port ;
          description 'Transmit packets from block ports.'
                      ;
          leaf trans-query-from-block-port {
            description 'Allow to transmit query packet from blocked port.'
                        ;
            type switch-type ;
            default disable ;
          }
          ncd:revise-date 2018-02-12 ;
        }
        leaf querier-send-to-mrouter {
          // if-feature querier-send-to-mrouter ;
          description 'Configure querier sending query to mrouter.'
                      ;
          type enumeration {
            enum enable {
              value 1 ;
              description 'Configure sending query to mrouter enable.'
                          ;
            }
            enum disable {
              value 2 ;
              description 'Configure sending query to mrouter disable.'
                          ;
            }
          }
          default disable ;
          ncd:revise-date 2018-08-07 ;
        }
        container logging {
          description 'Logging the igmp packet.'
                      ;
          leaf packet-report {
            // if-feature logging ;
            description 'Logging the IGMP report.'
                        ;
            type switch-type ;
            default disable ;
          }
          leaf overrun-report {
            // if-feature logging ;
            description 'Logging the IGMP report for group or member number ove'
                      + 'rrun.'
                        ;
            type switch-type ;
            default disable ;
          }
          ncd:revise-date 2019-11-04 ;
        }
      }
      container vpls {
        // if-feature vpls ;
        description "IGMP snooping VPLS configure.Can't config IGMP snooping in"
                  + " VPLS if the VPLS instance not exist.If IGMP snooping conf"
                  + "iguration exist in VPLS,the VPLS instance can't be deleted"
                  + "."
                    ;
        list vpls {
          description 'The config of instance VPLS.'
                      ;
          key name ;
          leaf name {
            description 'Submode instance Name of VPLS.'
                        ;
            type string {
              length 1..32 ;
            }
          }
          leaf snooping-switch {
            description 'Enable/disable IGMP snooping in this instance.'
                        ;
            type switch-type ;
            default disable ;
          }
          leaf member-fast-leave {
            description 'Enable/disable group membership fast leave in this ins'
                      + 'tance.'
                        ;
            type switch-type ;
            default disable ;
          }
          leaf acl-name {
            description 'Configure an ACL rule in this instance.'
                        ;
            type string {
              length 0..31 ;
            }
          }
          leaf member-time-out {
            description 'Member aging time in this instance.'
                        ;
            type uint16 {
              range 30..65535 ;
            }
            units second ;
            default 260 ;
          }
          leaf mrouter-time-out {
            description 'Multicast router port aging time in this instance.'
                        ;
            type uint16 {
              range 30..65535 ;
            }
            units second ;
            default 260 ;
          }
          leaf work-mode {
            description 'Work mode in this instance: proxy, transparent or rout'
                      + 'er.'
                        ;
            type work-mode-type ;
            default proxy ;
          }
          leaf proxy-ip {
            description 'This IP address is used as source IP when sending IGMP'
                      + ' Report and Leave in proxy mode.'
                        ;
            type ncd:ipv4-address ;
          }
          leaf query-ip {
            description 'This IP address is used as source IP when sending IGMP'
                      + ' Query.'
                        ;
            type ncd:ipv4-address ;
          }
          leaf max-group-num {
            description 'Max number of multicast group can access in this insta'
                      + 'nce.Its default value and maximum value depends on the'
                      + ' product.'
                        ;
            type uint32 {
              range 1..4294967295 ;
            }
          }
          leaf ndl-enable {
            description 'Enable/disable all ports are not allowed to learn dyna'
                      + 'mic mrouter port(s).'
                        ;
            type switch-type ;
            default disable ;
          }
          leaf last-mem-qry-interval {
            description 'The last member query interval is the max response tim'
                      + 'e inserted into group-specific queries sent in respons'
                      + 'e to leave group messages, and is also the amount of t'
                      + 'ime between group-specific query messages.'
                        ;
            type uint16 {
              range 1..25 ;
            }
            units second ;
            default 1 ;
          }
          leaf query-version {
            description 'Querier version: V1, V2 or V3.'
                        ;
            type version-type ;
            default v2 ;
          }
          leaf query-enable {
            description 'Enable/disable querier in this instance.'
                        ;
            type switch-type ;
            default disable ;
          }
          leaf redundancy-pw-mrouter-transmitting {
            // if-feature redundancy-pw-mrouter-transmitting ;
            description 'Configure redundancy PW mrouter transmitting mode:sing'
                      + 'le or double.'
                        ;
            type redundancy-pw-transmitting-type ;
            default double ;
            ncd:revise-date 2018-04-03 ;
          }
          leaf querier-send-to-mrouter {
            // if-feature querier-send-to-mrouter ;
            description 'Configure querier sending query to mrouter.'
                        ;
            type enumeration {
              enum unconfigured {
                value 0 ;
                description 'Function of sending query to mrouter is unconfigur'
                          + 'ed.'
                            ;
              }
              enum enable {
                value 1 ;
                description 'Configure sending query to mrouter enable.'
                            ;
              }
              enum disable {
                value 2 ;
                description 'Configure sending query to mrouter disable.'
                            ;
              }
            }
            default unconfigured ;
            ncd:revise-date 2018-08-07 ;
          }
          list drop {
            description 'The config of group with drop in instance VPLS.The max'
                      + '-elements depends on the product.'
                        ;
            key group-ip ;
            leaf group-ip {
              description 'Group IP address.'
                          ;
              type ncd:ipv4-address ;
            }
          }
          list prejoin {
            description 'The config of group with prejoin in instance VPLS.The '
                      + 'max-elements depends on the product.'
                        ;
            key group-ip ;
            leaf group-ip {
              description 'Group IP address.'
                          ;
              type ncd:ipv4-address ;
            }
          }
          list max-member-num {
            description 'The config of group with max-member-num in instance VP'
                      + 'LS.The max-elements depends on the product.'
                        ;
            key group-ip ;
            leaf group-ip {
              description 'Group IP address.'
                          ;
              type ncd:ipv4-address ;
            }
            leaf max-member-num {
              description 'Max number of IGMP member can access to this group.I'
                        + 'ts default value and maximum'+"\n"
                        + 'value depends on the product.'
                          ;
              type uint32 {
                range 1..4294967295 ;
              }
            }
          }
          list member {
            description 'The config of static member in instance VPLS.The max-e'
                      + 'lements depends on the product.'
                        ;
            key 'group-ip interface'
                ;
            leaf group-ip {
              description 'Group IP address.'
                          ;
              type ncd:ipv4-address ;
            }
            leaf interface {
              description 'Interface name.'
                          ;
              type ncd:ifname ;
            }
          }
          list interface {
            description 'The config of interface in instance VPLS.'
                        ;
            key name ;
            leaf name {
              description 'Interface name.'
                          ;
              type ncd:ifname ;
            }
            leaf snooping-switch {
              description 'Enable/Disable IGMP snooping in the interface.'
                          ;
              type switch-type ;
              default enable ;
            }
            leaf mrouter-guard {
              description 'Interface multicast router guard function is enabled.'
                          ;
              type switch-type ;
              default disable ;
            }
            leaf querier-send-to-mrouter {
              // if-feature querier-send-to-mrouter ;
              description 'Configure querier sending query to mrouter.'
                          ;
              type enumeration {
                enum unconfigured {
                  value 0 ;
                  description 'Function of sending query to mrouter is unconfig'
                            + 'ured.'
                              ;
                }
                enum enable {
                  value 1 ;
                  description 'Configure sending query to mrouter enable.'
                              ;
                }
