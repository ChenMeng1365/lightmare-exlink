        <type name="uint32">
          <range value="0..127"/>
        </type>
        <default value="1"/>
        <description>
          <text>Value of j1-mode on a port.</text>
        </description>
      </leaf>
      <leaf name="j1-trace">
        <when condition="../j1-mode='16byte' or ../j1-mode='64byte-or-null-mode'"/>
        <type name="string">
          <length value="1..62"/>
