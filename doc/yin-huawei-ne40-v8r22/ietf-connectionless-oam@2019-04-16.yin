  <container name="cc-session-statistics-data">
    <if-feature name="continuity-check"/>
    <config value="false"/>
    <list name="cc-session-statistics">
      <key value="type"/>
      <leaf name="type">
        <type name="identityref">
          <base name="traffic-type"/>
        </type>
        <description>
          <text>Type of traffic.</text>
        </description>
      </leaf>
      <container name="cc-ipv4-sessions-statistics">
        <when condition="../type = 'cl-oam:ipv4'">
          <description>
            <text>Only applies when traffic type is IPv4.</text>
          </description>
        </when>
        <description>
          <text>CC ipv4 sessions.</text>
        </description>
        <uses name="cc-session-statistics"/>
      </container>
