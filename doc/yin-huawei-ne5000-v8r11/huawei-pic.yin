      </leaf>
      <leaf name="rx-j1-value">
        <type name="uint32">
          <range value="0..127"/>
        </type>
        <config value="false"/>
        <description>
          <text>Value of rxj1mode on a port.</text>
        </description>
      </leaf>
      <leaf name="rx-j1-trace">
        <type name="string">
          <length value="0..64"/>
        </type>
        <config value="false"/>
        <description>
          <text>Rxj1Trace on a port.</text>
        </description>
      </leaf>
      <leaf name="rx-c2">
        <type name="uint32">
          <range value="0..255"/>
        </type>
        <config value="false"/>
        <description>
          <text>RxC2 on a port.</text>
        </description>
      </leaf>
      <container name="e1t1s">
        <when condition="../hp-mapping-mode='VC12'"/>
        <description>
          <text>List of E1T1 port attributes.</text>
        </description>
        <list name="e1t1">
          <ext:generated-by value="system"/>
          <ext:operation-exclude value="create"/>
          <key value="number"/>
          <max-elements value="63"/>
          <description>
            <text>Configure E1T1 port attribute.</text>
          </description>
          <leaf name="number">
            <type name="uint32">
              <range value="1..63"/>
            </type>
            <description>
              <text>E1T1 number.</text>
            </description>
          </leaf>
          <leaf name="work-mode">
            <type name="enumeration">
              <enum name="framed">
                <value value="0"/>
                <description>
                  <text>Framed.</text>
                </description>
              </enum>
              <enum name="unframed">
                <value value="1"/>
                <description>
                  <text>Unframed.</text>
                </description>
              </enum>
            </type>
            <must condition="((not(../channel-sets)) and ((../work-mode='framed') or (../work-mode='unframed'))) or (../work-mode='framed')"/>
            <default value="framed"/>
            <description>
              <text>E1T1 Work mode. Configuration is not allowed to join the APS group.</text>
            </description>
          </leaf>
          <leaf name="clock">
            <type name="enumeration">
              <enum name="master">
                <value value="0"/>
                <description>
                  <text>Master.</text>
                </description>
              </enum>
              <enum name="slave">
                <value value="1"/>
                <description>
                  <text>Slave.</text>
                </description>
              </enum>
              <enum name="domain">
                <value value="2"/>
                <description>
                  <text>Domain.</text>
                </description>
              </enum>
            </type>
            <default value="master"/>
            <description>
              <text>Clock mode.</text>
            </description>
          </leaf>
          <leaf name="clock-recovery-domain">
            <when condition="../clock='domain'"/>
            <type name="uint32">
              <range value="1..8"/>
            </type>
            <description>
              <text>Clock recovery-domain value.</text>
            </description>
          </leaf>
          <leaf name="clock-master-domain">
            <when condition="not(../clock='master')"/>
            <type name="uint32">
              <range value="1..32"/>
            </type>
            <description>
              <text>Clock master-domain value.</text>
            </description>
          </leaf>
          <leaf name="frame-format">
            <when condition="../work-mode='framed'"/>
            <type name="enumeration">
              <enum name="no-crc4">
                <value value="0"/>
                <description>
                  <text>No CRC4.</text>
                </description>
              </enum>
              <enum name="crc4">
                <value value="1"/>
                <description>
                  <text>CRC4.</text>
                </description>
              </enum>
            </type>
            <default value="no-crc4"/>
            <description>
              <text>Frame format.</text>
            </description>
          </leaf>
          <leaf name="loopback">
            <type name="enumeration">
              <enum name="none">
                <value value="0"/>
                <description>
                  <text>None.</text>
                </description>
              </enum>
              <enum name="local">
                <value value="1"/>
                <description>
                  <text>Local.</text>
                </description>
              </enum>
              <enum name="remote">
                <value value="2"/>
                <description>
                  <text>Remote.</text>
                </description>
              </enum>
            </type>
            <default value="none"/>
            <description>
              <text>Loopback mode.</text>
            </description>
          </leaf>
          <leaf name="j2-trace">
            <type name="string">
              <length value="1..15"/>
            </type>
            <default value="NetEngine"/>
            <description>
              <text>J2 trace.</text>
            </description>
          </leaf>
          <leaf name="v5-trace">
            <ext:dynamic-default/>
            <type name="uint32">
              <range value="0..7"/>
            </type>
            <description>
              <text>V5trace.</text>
            </description>
          </leaf>
          <leaf name="auto-clr-period">
            <when condition="not(../loopback='none')"/>
            <type name="uint32">
              <range value="0..43200"/>
            </type>
            <default value="0"/>
            <description>
              <text>Loopback auto-clear period.</text>
            </description>
          </leaf>
          <leaf name="rx-j2-trace">
            <type name="string">
              <length value="1..15"/>
            </type>
            <config value="false"/>
            <description>
              <text>Rx j2Trace.</text>
            </description>
          </leaf>
          <container name="channel-sets">
            <when condition="../work-mode='framed'"/>
            <description>
              <text>List of Channel sets.</text>
            </description>
            <list name="channel-set">
              <key value="number timeslot-list"/>
              <unique tag="number"/>
              <unique tag="timeslot-list"/>
              <max-elements value="30"/>
              <description>
                <text>Configure Channel-sets.</text>
              </description>
              <leaf name="number">
                <type name="uint32">
                  <range value="0..30"/>
                </type>
                <description>
                  <text>Set number.</text>
                </description>
              </leaf>
              <leaf name="timeslot-list">
                <type name="string">
                  <length value="1..83"/>
                </type>
                <description>
                  <text>Timeslot list. Please enter a number from 1 to 83 with ',' or '-' in the middle.</text>
                </description>
              </leaf>
            </list>
          </container>
        </list>
      </container>
    </container>
    <container name="port-alarm">
      <description>
        <text>Configure port alarm information.</text>
      </description>
      <leaf name="crc-enabled-down">
        <ext:dynamic-default/>
        <type name="boolean"/>
        <description>
          <text>Enable/disable CRC error enable down.</text>
        </description>
      </leaf>
      <leaf name="input-enabled-down">
        <ext:dynamic-default/>
        <type name="boolean"/>
        <description>
          <text>Enable/disable input error enable down.</text>
        </description>
      </leaf>
      <leaf name="input-error-high-threshold">
        <ext:dynamic-default/>
        <type name="uint32">
          <range value="1..4294967295"/>
        </type>
        <must condition="../input-error-high-threshold &gt;= ../input-error-low-threshold"/>
        <description>
          <text>The high threshold of input error alarm threshold. The number of error packets counted in the time in interval exceeds this threshold will produce input-error alarm.</text>
        </description>
      </leaf>
      <leaf name="input-error-low-threshold">
        <ext:dynamic-default/>
        <type name="uint32">
          <range value="1..4294967295"/>
        </type>
        <must condition="../input-error-high-threshold &gt;= ../input-error-low-threshold"/>
        <description>
          <text>The low threshold of input error alarm threshold. The number of error packets counted in the time in interval less than this threshold will recover input-error alarm.</text>
        </description>
      </leaf>
      <leaf name="input-error-interval">
        <ext:dynamic-default/>
        <type name="uint32">
          <range value="1..65535"/>
        </type>
        <units name="s"/>
        <description>
          <text>Time interval for statistical input-error alarm.</text>
        </description>
      </leaf>
      <leaf name="output-enabled-down">
        <ext:dynamic-default/>
        <type name="boolean"/>
        <description>
          <text>Enable/disable output error enable down.</text>
        </description>
      </leaf>
      <leaf name="output-error-high-threshold">
        <ext:dynamic-default/>
        <type name="uint32">
          <range value="1..4294967295"/>
        </type>
        <must condition="../output-error-high-threshold &gt;= ../output-error-low-threshold"/>
        <description>
          <text>The high threshold of output error alarm threshold. The number of error packets counted in the time in interval exceeds this threshold will produce output-error alarm.</text>
        </description>
      </leaf>
      <leaf name="output-error-low-threshold">
        <ext:dynamic-default/>
        <type name="uint32">
          <range value="1..4294967295"/>
        </type>
        <must condition="../output-error-high-threshold &gt;= ../output-error-low-threshold"/>
        <description>
          <text>The low threshold of output error alarm threshold. The number of error packets counted in the time in interval less than this threshold will recover output-error alarm.</text>
        </description>
      </leaf>
      <leaf name="output-error-interval">
        <ext:dynamic-default/>
        <type name="uint32">
          <range value="1..65535"/>
        </type>
        <units name="s"/>
        <description>
          <text>Time interval for statistical input-error alarm.</text>
        </description>
      </leaf>
      <leaf name="phy-updown-en">
        <ext:dynamic-default/>
        <type name="boolean"/>
        <description>
          <text>Enable/disable phyUpDownEn.</text>
        </description>
      </leaf>
      <leaf name="crc-per-cur">
        <type name="string">
          <length value="0..8"/>
        </type>
        <config value="false"/>
        <description>
          <text>Current Value of packet error ratio.</text>
        </description>
      </leaf>
      <container name="eth-port-alarm">
        <description>
          <text>Configure ethernet port alarm information.</text>
        </description>
        <leaf name="symbol-enabled-down">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable symbol error enable down.</text>
          </description>
        </leaf>
        <leaf name="symbol-error-high-threshold">
          <type name="uint32">
            <range value="1..4294967295"/>
          </type>
          <must condition="../symbol-error-high-threshold &gt;= ../symbol-error-low-threshold"/>
          <default value="1000"/>
          <description>
            <text>The high threshold of symbol error alarm threshold. The number of error packets counted in the time in interval exceeds this threshold will produce symbol-error alarm.</text>
          </description>
        </leaf>
        <leaf name="symbol-error-low-threshold">
          <type name="uint32">
            <range value="1..4294967295"/>
          </type>
          <must condition="../symbol-error-high-threshold &gt;= ../symbol-error-low-threshold"/>
          <default value="100"/>
          <description>
            <text>The low threshold of symbol error alarm threshold. The number of error packets counted in the time in interval less than this threshold will recover symbol-error alarm.</text>
          </description>
        </leaf>
        <leaf name="symbol-error-interval">
          <type name="uint32">
            <range value="1..65535"/>
          </type>
          <units name="s"/>
          <default value="10"/>
          <description>
            <text>Time interval for statistical symbol-error alarm.</text>
          </description>
        </leaf>
        <leaf name="bip8-sd-enabled-down">
          <ext:dynamic-default/>
          <type name="boolean"/>
          <description>
            <text>Enable/disable BIP8-SD error enable down.</text>
          </description>
        </leaf>
        <leaf name="bip8-sd-error-high-threshold">
          <ext:dynamic-default/>
          <type name="uint32">
            <range value="6..9"/>
          </type>
          <description>
            <text>The high threshold of BIP8-SD error alarm threshold (x of 10Ex). The number of error packets counted in the time in interval exceeds this threshold will produce bip8-sd-error alarm.</text>
          </description>
        </leaf>
        <leaf name="bad-bytes-threshold">
          <type name="uint32">
            <range value="1..65535"/>
