            enum QL-SEC {
              value 11 ;
              description 'The QL is SEC.' ;
            }
            enum QL-DNU {
              value 15 ;
              description 'The QL is DNU.' ;
            }
            enum invalid {
              value 254 ;
            }
          }
        }
      }
      container current-out-clock {
        description 'SSM current out clock information.' ;
        list current-out-clock-list {
          description 'The list of current ssm out clock information.' ;
          key if-name ;
          leaf if-name {
            description 'The interface name of out clock.' ;
            type string {
              length 0..31 ;
            }
          }
          leaf out-state {
            description 'The state of the current out clock.' ;
            type enumeration {
              enum enable {
                value 1 ;
                description 'The state of the current out clock is enabled.' ;
              }
              enum disable {
