<?xml version="1.0" encoding="UTF-8"?>
<module name="huawei-socket"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:socket="urn:huawei:yang:huawei-socket"
        xmlns:ext="urn:huawei:yang:huawei-extension">
  <yang-version value="1"/>
  <namespace uri="urn:huawei:yang:huawei-socket"/>
  <prefix value="socket"/>
  <import module="huawei-extension">
    <prefix value="ext"/>
  </import>
  <organization>
    <text>Huawei Technologies Co., Ltd.</text>
  </organization>
  <contact>
    <text>Huawei Industrial Base
Bantian, Longgang
Shenzhen 518129
People's Republic of China
Website: http://www.huawei.com
Email: support@huawei.com</text>
  </contact>
  <description>
    <text>Socket.</text>
  </description>
  <revision date="2020-03-06">
    <description>
      <text>Initial revision.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <ext:task-name value="ip-stack"/>
  <container name="socket">
    <description>
      <text>Socket.</text>
    </description>
    <container name="tcp-global">
      <description>
        <text>Configure global tcp attribute configuration.</text>
      </description>
      <leaf name="tcp-fin-timeout">
        <type name="uint32">
          <range value="76..3600"/>
        </type>
        <units name="s"/>
        <default value="675"/>
        <description>
          <text>TCP IPv4 fin timer.</text>
        </description>
      </leaf>
      <leaf name="tcp-syn-timeout">
        <type name="uint32">
          <range value="2..600"/>
        </type>
        <units name="s"/>
        <default value="75"/>
        <description>
          <text>TCP IPv4 synchronised timer.</text>
        </description>
      </leaf>
      <leaf name="tcp-window">
        <type name="uint32">
          <range value="1..32"/>
        </type>
        <units name="kB"/>
        <default value="8"/>
        <description>
          <text>TCP IPv4 window.</text>
        </description>
      </leaf>
      <leaf name="tcp6-fin-timeout">
        <type name="uint32">
          <range value="76..3600"/>
        </type>
        <units name="s"/>
        <default value="675"/>
        <description>
          <text>TCP IPv6 fin timer.</text>
        </description>
      </leaf>
      <leaf name="tcp6-syn-timeout">
        <type name="uint32">
          <range value="2..600"/>
        </type>
        <units name="s"/>
        <default value="75"/>
        <description>
          <text>TCP IPv6 synchronised timer.</text>
        </description>
      </leaf>
      <leaf name="tcp6-window">
        <type name="uint32">
          <range value="1..32"/>
        </type>
        <units name="kB"/>
        <default value="8"/>
        <description>
          <text>TCP IPv6 window.</text>
        </description>
      </leaf>
      <leaf name="tcp-max-mss">
        <type name="uint32">
          <range value="32..9600"/>
        </type>
        <units name="Octet"/>
        <must condition="../tcp-min-mss&lt;=../tcp-max-mss"/>
        <description>
          <text>IPv4 TCP max mss.</text>
        </description>
      </leaf>
      <leaf name="tcp6-max-mss">
        <type name="uint32">
          <range value="32..9600"/>
        </type>
        <units name="Octet"/>
        <must condition="../tcp6-min-mss&lt;=../tcp6-max-mss"/>
        <description>
          <text>IPv6 TCP max mss.</text>
        </description>
      </leaf>
      <leaf name="tcp-pathmtu-timeout">
        <type name="uint32">
          <range value="10..100"/>
        </type>
        <units name="min"/>
        <description>
          <text>TCP IPv4 path MTU timer.</text>
        </description>
      </leaf>
      <leaf name="tcp-min-mss">
        <type name="uint32">
          <range value="32..9600"/>
        </type>
        <units name="Octet"/>
        <must condition="(../tcp-min-mss&lt;=../tcp-max-mss) or not(../tcp-max-mss)"/>
        <default value="216"/>
        <description>
          <text>IPv4 TCP min mss.</text>
        </description>
      </leaf>
      <leaf name="tcp6-min-mss">
        <type name="uint32">
          <range value="32..9600"/>
        </type>
        <units name="Octet"/>
        <must condition="(../tcp6-min-mss&lt;=../tcp6-max-mss) or not(../tcp6-max-mss)"/>
        <default value="216"/>
        <description>
          <text>IPv6 TCP min mss.</text>
        </description>
      </leaf>
    </container>
  </container>
</module>
