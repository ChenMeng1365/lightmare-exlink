  <container name="pp4">
    <description>
      <text>IPv4.</text>
    </description>
    <container name="global">
      <description>
        <text>Configure IPv4 global configuration.</text>
      </description>
      <leaf name="reassembling-timeout">
        <type name="uint32">
          <range value="5..120"/>
        </type>
        <units name="s"/>
        <default value="30"/>
        <description>
          <text>Configuration of timeout period for reassembling packets. The value is expressed in seconds.</text>
        </description>
      </leaf>
      <leaf name="blacklist-flag">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable IP blacklist option.</text>
        </description>
      </leaf>
      <leaf name="route-aler-switch">
        <type name="pp4-switch-operation"/>
        <default value="enable"/>
        <description>
          <text>Enabling or disabling of global route-aler IP option packet configuration.</text>
        </description>
      </leaf>
      <leaf name="route-record-switch">
        <type name="pp4-switch-operation"/>
        <default value="enable"/>
        <description>
          <text>Enabling or disabling of global route-record IP option packet configuration.</text>
        </description>
      </leaf>
      <leaf name="source-route-switch">
        <type name="pp4-switch-operation"/>
        <default value="enable"/>
        <description>
          <text>Enabling or disabling of global source-route IP option packet configuration.</text>
        </description>
      </leaf>
      <leaf name="time-stamp-switch">
        <type name="pp4-switch-operation"/>
        <default value="enable"/>
        <description>
          <text>Enabling or disabling of global time-stamp IP option packet configuration.</text>
        </description>
      </leaf>
      <leaf name="te-pkt-ctrl-switch">
        <type name="pp4-switch-operation"/>
        <default value="disable"/>
        <description>
          <text>Enabling or disabling of packet control policy.</text>
        </description>
      </leaf>
    </container>
    <container name="icmp-securitys">
      <description>
        <text>List of global ICMP security configuration.</text>
      </description>
      <list name="icmp-security">
        <must condition="((((icmp-name='echo' and icmp-type=8 and icmp-code=0) or (icmp-name='echo-reply' and icmp-type=0 and icmp-code=0) or (icmp-name='fragmentneed-dfset' and icmp-type=3 and icmp-code=4) or (icmp-name='host-redirect' and icmp-type=5 and icmp-code=1) or (icmp-name='host-tos-redirect' and icmp-type=5 and icmp-code=3) or (icmp-name='host-unreachable' and icmp-type=3 and icmp-code=1) or (icmp-name='information-reply' and icmp-type=16 and icmp-code=0) or (icmp-name='information-request' and icmp-type=15 and icmp-code=0) or (icmp-name='net-redirect' and icmp-type=5 and icmp-code=0) or (icmp-name='net-tos-redirect' and icmp-type=5 and icmp-code=2) or (icmp-name='net-unreachable' and icmp-type=3 and icmp-code=0) or (icmp-name='parameter-problem' and icmp-type=12 and icmp-code=0) or (icmp-name='port-unreachable' and icmp-type=3 and icmp-code=3) or (icmp-name='protocol-unreachable' and icmp-type=3 and icmp-code=2) or (icmp-name='reassembly-timeout' and icmp-type=11 and icmp-code=1) or (icmp-name='source-quench' and icmp-type=4 and icmp-code=0) or (icmp-name='source-route-failed' and icmp-type=3 and icmp-code=5) or (icmp-name='timestamp-reply' and icmp-type=14 and icmp-code=0) or (icmp-name='timestamp-request' and icmp-type=13 and icmp-code=0) or (icmp-name='ttl-exceeded' and icmp-type=11 and icmp-code=0) or icmp-name='user-defined') and action ='receive-pkt') or (((icmp-name='echo' and icmp-type=8 and icmp-code=0) or (icmp-name='echo-reply' and icmp-type=0 and icmp-code=0) or (icmp-name='fragmentneed-dfset' and icmp-type=3 and icmp-code=4) or (icmp-name='net-unreachable' and icmp-type=3 and icmp-code=0) or (icmp-name='parameter-problem' and icmp-type=12 and icmp-code=0) or (icmp-name='port-unreachable' and icmp-type=3 and icmp-code=3) or (icmp-name='reassembly-timeout' and icmp-type=11 and icmp-code=1) or (icmp-name='source-route-failed' and icmp-type=3 and icmp-code=5) or (icmp-name='timestamp-reply' and icmp-type=14 and icmp-code=0) or (icmp-name='timestamp-request' and icmp-type=13 and icmp-code=0) or (icmp-name='ttl-exceeded' and icmp-type=11 and icmp-code=0)) and action ='send-pkt'))"/>
        <key value="action icmp-name icmp-type icmp-code"/>
        <description>
          <text>Configure global ICMP security function.</text>
        </description>
        <leaf name="action">
          <type name="pp4-pkt-action"/>
          <description>
            <text>Packet reception or packet sending.</text>
          </description>
        </leaf>
        <leaf name="icmp-name">
          <type name="pp4-icmpsec-type"/>
          <description>
            <text>ICMP names.</text>
          </description>
        </leaf>
        <leaf name="icmp-type">
          <type name="uint32">
            <range value="0..255"/>
          </type>
          <description>
            <text>ICMP types. For the value of this node, see the description of pp4-icmpsec-type.</text>
          </description>
        </leaf>
        <leaf name="icmp-code">
          <type name="uint32">
            <range value="0..255"/>
          </type>
          <description>
            <text>ICMP code. For the value of this node, see the description of pp4-icmpsec-type.</text>
          </description>
        </leaf>
        <leaf name="switch-operation">
          <type name="pp4-switch-operation"/>
          <mandatory value="true"/>
          <description>
            <text>Enabling or disabling of ICMP packet security configuration.</text>
          </description>
        </leaf>
      </list>
    </container>
    <container name="ip-dscps">
      <description>
        <text>List of DSCP configuration.</text>
      </description>
      <list name="ip-dscp">
        <key value="proto-type"/>
        <max-elements value="2"/>
        <description>
          <text>Specify DSCP value.</text>
        </description>
        <leaf name="proto-type">
          <type name="app-proto-type"/>
          <description>
            <text>Protocol type.</text>
          </description>
        </leaf>
        <leaf name="dscp">
          <type name="uint32">
            <range value="0..63"/>
          </type>
          <default value="0"/>
          <description>
            <text>DSCP priority.</text>
          </description>
        </leaf>
      </list>
    </container>
    <container name="pkt-fast-reply-ctrl">
      <description>
        <text>Configure host packet fast reply control.</text>
      </description>
      <leaf name="tcp-action">
        <type name="pp4-fast-switch"/>
        <default value="enable"/>
        <description>
          <text>Host TCP packet fast reply control.</text>
        </description>
      </leaf>
      <leaf name="udp-action">
        <type name="pp4-fast-switch"/>
        <default value="enable"/>
        <description>
          <text>Host UDP packet fast reply control.</text>
        </description>
      </leaf>
      <leaf name="rawip-action">
        <type name="pp4-fast-switch"/>
        <default value="enable"/>
        <description>
          <text>Host Rawip packet fast reply control.</text>
        </description>
      </leaf>
    </container>
    <container name="pkt-fast-reply-snd">
      <description>
        <text>Configure host packet fast send control.</text>
      </description>
      <leaf name="tcp-action">
        <type name="pp4-fast-switch"/>
        <default value="disable"/>
        <description>
          <text>Host TCP packet fast send control.</text>
        </description>
      </leaf>
      <leaf name="udp-action">
        <type name="pp4-fast-switch"/>
        <default value="disable"/>
        <description>
          <text>Host UDP packet fast send control.</text>
        </description>
      </leaf>
      <leaf name="rawip-action">
        <type name="pp4-fast-switch"/>
        <default value="disable"/>
        <description>
          <text>Host Rawip packet fast send control.</text>
        </description>
      </leaf>
    </container>
    <container name="host-proto-types">
      <config value="false"/>
      <description>
        <text>List of host protocol type.</text>
      </description>
      <list name="host-proto-type">
        <key value="packet-type"/>
        <description>
          <text>Operational data of host protocol type.</text>
        </description>
        <leaf name="packet-type">
          <type name="app-proto-name"/>
          <description>
            <text>Application name.</text>
          </description>
        </leaf>
        <leaf name="protocol-type">
          <type name="app-proto-type"/>
          <description>
            <text>Protocol type.</text>
          </description>
        </leaf>
      </list>
    </container>
    <container name="icmp-stats">
      <config value="false"/>
      <description>
        <text>Statistics of collection of statistics on ICMP packets.</text>
      </description>
      <leaf name="in-bad-fmt">
        <type name="uint32"/>
        <description>
          <text>Count of received ICMP packets whose formats are incorrect.</text>
        </description>
      </leaf>
      <leaf name="in-bad-checksum">
        <type name="uint32"/>
        <description>
          <text>Count of received packets whose checksums are incorrect.</text>
        </description>
      </leaf>
      <leaf name="in-echo">
        <type name="uint32"/>
        <description>
          <text>Count of received ICMP echo request packets.</text>
        </description>
      </leaf>
      <leaf name="in-dst-unreach">
        <type name="uint32"/>
        <description>
          <text>Count of received packets whose destination addresses are unreachable.</text>
        </description>
      </leaf>
      <leaf name="in-src-quench">
        <type name="uint32"/>
        <description>
          <text>Count of received source suppression packets.</text>
        </description>
      </leaf>
      <leaf name="in-redirect">
        <type name="uint32"/>
        <description>
          <text>Count of received redirection packets.</text>
        </description>
      </leaf>
      <leaf name="in-echo-reply">
        <type name="uint32"/>
        <description>
          <text>Count of received echo reply packets.</text>
        </description>
      </leaf>
      <leaf name="in-para-err">
        <type name="uint32"/>
        <description>
          <text>Count of received packets whose parameters are incorrect.</text>
        </description>
      </leaf>
      <leaf name="in-time-stamp">
        <type name="uint32"/>
        <description>
          <text>Count of received timestamp request packets.</text>
        </description>
      </leaf>
      <leaf name="in-info-request">
        <type name="uint32"/>
        <description>
          <text>Count of received information request packets.</text>
        </description>
      </leaf>
      <leaf name="in-mask-request">
        <type name="uint32"/>
        <description>
          <text>Count of received mask request packets.</text>
        </description>
      </leaf>
      <leaf name="in-mask-reply">
        <type name="uint32"/>
        <description>
          <text>Count of received mask request response packets.</text>
        </description>
      </leaf>
      <leaf name="in-time-exceed">
        <type name="uint32"/>
        <description>
          <text>Count of received ICMP timeout error packets.</text>
        </description>
      </leaf>
      <leaf name="out-echo">
        <type name="uint32"/>
        <description>
          <text>Count of sent echo request packets.</text>
        </description>
      </leaf>
      <leaf name="out-dst-unreach">
        <type name="uint32"/>
        <description>
          <text>Count of sent destination-unreachable packets.</text>
        </description>
      </leaf>
      <leaf name="out-src-quench">
        <type name="uint32"/>
        <description>
          <text>Count of sent source suppression packets.</text>
        </description>
      </leaf>
      <leaf name="out-redirect">
        <type name="uint32"/>
        <description>
          <text>Count of sent redirection packets.</text>
        </description>
      </leaf>
      <leaf name="out-echo-reply">
        <type name="uint32"/>
        <description>
          <text>Count of sent echo reply packets.</text>
        </description>
      </leaf>
      <leaf name="out-para-err">
        <type name="uint32"/>
        <description>
          <text>Count of sent packets whose parameters are incorrect.</text>
        </description>
      </leaf>
      <leaf name="out-time-stamp">
        <type name="uint32"/>
        <description>
          <text>Count of sent timestamp request packets.</text>
        </description>
      </leaf>
      <leaf name="out-info-request">
        <type name="uint32"/>
        <description>
          <text>Count of sent information request packets.</text>
        </description>
      </leaf>
      <leaf name="out-mask-request">
        <type name="uint32"/>
        <description>
          <text>Count of sent mask request packets.</text>
        </description>
      </leaf>
      <leaf name="out-mask-reply">
        <type name="uint32"/>
        <description>
          <text>Count of sent mask request response packets.</text>
        </description>
      </leaf>
      <leaf name="out-time-exceed">
