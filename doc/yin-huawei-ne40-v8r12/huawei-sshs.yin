      <description>
        <text>Configure alarm when manager login failed frequently.</text>
      </description>
      <leaf name="upper-limit">
        <type name="uint32">
          <range value="0..100"/>
        </type>
        <must condition="../upper-limit &gt;= ../lower-limit"/>
        <default value="30"/>
        <description>
          <text>Login alarm report times, default is 30 times.</text>
        </description>
      </leaf>
      <leaf name="lower-limit">
        <type name="uint32">
          <range value="0..45"/>
        </type>
        <must condition="../lower-limit &lt;= ../upper-limit"/>
        <default value="20"/>
        <description>
          <text>Login alarm resume times, default is 20 times.</text>
        </description>
      </leaf>
      <leaf name="period">
        <type name="uint32">
          <range value="1..120"/>
        </type>
        <units name="min"/>
        <default value="5"/>
        <description>
          <text>Login alarm period, default is 5 minutes.</text>
        </description>
      </leaf>
    </container>
    <container name="rekey">
      <description>
        <text>Configure rekey.</text>
      </description>
      <leaf name="max-packet">
        <type name="uint32">
          <range value="268435456..2147483648"/>
        </type>
        <default value="2147483648"/>
        <description>
          <text>Rekey maximum packet number.</text>
        </description>
      </leaf>
      <leaf name="time">
        <type name="int32">
          <range value="30..1440"/>
        </type>
        <units name="min"/>
        <default value="60"/>
        <description>
          <text>Rekey Time.</text>
        </description>
      </leaf>
      <leaf name="max-data">
        <type name="int32">
          <range value="100..10000"/>
        </type>
        <units name="MB"/>
        <default value="1000"/>
        <description>
