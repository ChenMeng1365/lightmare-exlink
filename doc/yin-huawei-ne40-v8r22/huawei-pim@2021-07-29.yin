  <augment target-node="/mc:multicast">
    <description>
      <text>Adds PIM configuration to the multicast module.</text>
    </description>
    <container name="rpf-vector-paths">
      <description>
        <text>List of paths configuration to multicast sources.</text>
      </description>
      <list name="rpf-vector-path">
        <key value="path-name path-type"/>
        <unique tag="path-name"/>
        <max-elements value="2048"/>
        <description>
          <text>Configure paths to multicast sources.</text>
        </description>
        <leaf name="path-name">
          <type name="string">
            <length value="1..31"/>
            <pattern value="[^ \?]*"/>
          </type>
          <description>
            <text>Path name.</text>
          </description>
        </leaf>
        <leaf name="path-type">
          <type name="pim-path-type"/>
          <description>
            <text>Path type.</text>
          </description>
        </leaf>
        <container name="hops">
          <description>
            <text>List of IP addresses of hops on paths.</text>
          </description>
          <list name="hop">
            <key value="index"/>
            <unique tag="ip-addr"/>
            <max-elements value="22"/>
            <description>
              <text>Configure IP addresses of hops on paths.</text>
            </description>
            <leaf name="index">
              <type name="uint32">
                <range value="1..1024"/>
              </type>
              <description>
                <text>Address index.</text>
              </description>
            </leaf>
            <leaf name="ip-addr">
              <type name="inet:ip-address-no-zone"/>
              <mandatory value="true"/>
              <description>
                <text>IP address.</text>
              </description>
            </leaf>
          </list>
        </container>
      </list>
    </container>
  </augment>
  <ext:task-name value="pim"/>
</module>
