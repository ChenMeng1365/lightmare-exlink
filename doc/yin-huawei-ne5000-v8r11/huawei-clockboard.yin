<?xml version="1.0" encoding="UTF-8"?>
<module name="huawei-clockboard"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:clockboard="urn:huawei:yang:huawei-clockboard"
        xmlns:ext="urn:huawei:yang:huawei-extension"
        xmlns:pub-type="urn:huawei:yang:huawei-pub-type"
        xmlns:devm="urn:huawei:yang:huawei-devm">
  <namespace uri="urn:huawei:yang:huawei-clockboard"/>
  <prefix value="clockboard"/>
  <import module="huawei-extension">
    <prefix value="ext"/>
  </import>
  <import module="huawei-pub-type">
    <prefix value="pub-type"/>
  </import>
  <import module="huawei-devm">
    <prefix value="devm"/>
  </import>
  <organization>
    <text>Huawei Technologies Co., Ltd.</text>
  </organization>
  <contact>
    <text>Huawei Industrial Base
Bantian, Longgang
Shenzhen 518129
People's Republic of China
Website: http://www.huawei.com
Email: support@huawei.com</text>
  </contact>
  <description>
    <text>System clock synchronization management, including frequency synchronization and time synchronization.</text>
  </description>
  <revision date="2020-03-14">
    <description>
      <text>Modify the node description.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <revision date="2020-03-04">
    <description>
      <text>Modify the node description.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <revision date="2019-05-23">
    <description>
      <text>Initial revision.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <ext:task-name value="clksync"/>
  <typedef name="devm-board-type">
    <type name="enumeration">
      <enum name="clock">
        <value value="1"/>
        <description>
          <text>Clock board.</text>
        </description>
      </enum>
      <enum name="unknow">
        <value value="0"/>
        <description>
          <text>Unknown board type.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Clock board type.</text>
    </description>
  </typedef>
  <typedef name="devm-run-state">
    <type name="enumeration">
      <enum name="normal">
        <value value="1"/>
        <description>
          <text>Normal.</text>
        </description>
      </enum>
      <enum name="abnormal">
        <value value="0"/>
        <description>
          <text>Abnormal.</text>
        </description>
      </enum>
      <enum name="unknown">
        <value value="2"/>
        <description>
          <text>Unknown.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Devm run state.</text>
    </description>
  </typedef>
  <augment target-node="/devm:devm">
    <description>
      <text>Information of clockboard.</text>
    </description>
    <container name="clockboards">
      <config value="false"/>
      <description>
        <text>List of statistics of clock board.</text>
      </description>
      <list name="clockboard">
        <key value="position"/>
        <description>
          <text>Statistics of clock board.</text>
        </description>
        <leaf name="position">
          <type name="string">
            <length value="0..32"/>
          </type>
          <description>
            <text>Clock board position.</text>
          </description>
        </leaf>
        <leaf name="ent-index">
          <type name="uint32"/>
          <description>
            <text>Clock board index.</text>
          </description>
        </leaf>
        <leaf name="board-type">
          <type name="devm-board-type"/>
          <description>
            <text>Type of a clock board.</text>
          </description>
        </leaf>
        <leaf name="up-time">
          <type name="pub-type:time-tick"/>
          <description>
            <text>Up time of a clock board, the unit is timetick.</text>
          </description>
        </leaf>
        <leaf name="sram-state">
          <type name="devm-run-state"/>
          <description>
            <text>SRAM state.</text>
          </description>
        </leaf>
      </list>
    </container>
  </augment>
</module>
