        "isis_schema": {
            "description": "Intermediate System to Intermediate System (ISIS) routes.",
            "properties": {
                "global": {
                    "description": "Configure ISIS global config.",
                    "properties": {
                        "lsp-seq-over-flag": {
                            "type": "boolean",
                            "description": "Enable/disable to recover ISIS system ID automatically when the sequence number of the local lsp overflowed."
                        },
                        "sys-id-conflict-flag": {
                            "type": "boolean",
                            "description": "Enable/disable to recover ISIS system ID automatically when the system ID conflicted."
                        },
                        "purge-lsp-protect-enable": {
                            "type": "boolean",
                            "description": "Enable/disable automatic ISIS purge LSP protection."
                        },
                        "purge-source-trace-enable": {
                            "type": "boolean",
                            "description": "Enable/disable purge LSP source tracing."
                        },
                        "purge-source-trace-port": {
                            "type": "string",
                            "description": "Configure the port of purge LSP source tracing allocated by huawei-pp4.The related CLI is 'display ip port info | inc UDP'."
                        },
                        "suppress-flap-intf-disable": {
                            "type": "boolean",
                            "description": "Enable/disable the ISIS interface flapping suppression function."
                        },
                        "intf-limit-enable": {
                            "type": "boolean",
                            "description": "Enable/disable the limit of the number of ISIS interfaces."
