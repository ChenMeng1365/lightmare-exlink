  <typedef name="sshs-srvc-type">
    <type name="enumeration">
      <enum name="not-set">
        <value value="0"/>
        <description>
          <text>Server service is not set.</text>
        </description>
      </enum>
      <enum name="stelnet">
        <value value="1"/>
        <description>
          <text>STelnet server service.</text>
        </description>
      </enum>
      <enum name="sftp">
        <value value="2"/>
        <description>
          <text>SFTP server service.</text>
        </description>
      </enum>
      <enum name="snetconf">
        <value value="3"/>
        <description>
          <text>SNETCONF server service.</text>
        </description>
      </enum>
      <enum name="all">
        <value value="4"/>
        <description>
          <text>ALL server service.</text>
        </description>
      </enum>
      <enum name="sftp-snetconf">
        <value value="5"/>
        <description>
          <text>SFTP and SNETCONF server service.</text>
        </description>
      </enum>
      <enum name="stelnet-sftp">
        <value value="6"/>
        <description>
          <text>STelnet and SFTP server service.</text>
        </description>
      </enum>
      <enum name="stelnet-snetconf">
        <value value="7"/>
        <description>
          <text>STelnet and SNETCONF server service.</text>
        </description>
      </enum>
      <enum name="sftp-snetconf-stelnet">
        <value value="8"/>
        <description>
          <text>STelnet, SFTP and SNETCONF server service.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>SSH server service type list.</text>
    </description>
  </typedef>
  <typedef name="sshs-enable-flag">
    <type name="enumeration">
      <enum name="enable">
        <value value="1"/>
        <description>
          <text>SSH server/service enable.</text>
        </description>
      </enum>
      <enum name="disable">
        <value value="2"/>
        <description>
          <text>SSH server/service disable.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>SSH server bool switch.</text>
    </description>
  </typedef>
