module: huawei-cusp-agent
  +--rw cusp-agent
     +--ro agents
        +--ro agent* [agent-name]
           +--ro agent-name            string
           +--ro transport-address?    inet:ipv4-address-no-zone
           +--ro transport-vpn-name?   -> /ni:network-instance/instances/instance/name
           +--ro echo-interval?        uint8
           +--ro keychain-name?        -> /keychain:keychain/keychains/keychain/name
           +--ro ssl-policy-name?      -> /ssl:ssl/ssl-policys/ssl-policy/policy-name
           +--ro timeout-count?        uint8
           +--ro congest-duration?     uint8
           +--ro tcp-mss?              uint16
           +--ro connections
              +--ro connection* [controller-ip]
                 +--ro controller-ip         inet:ipv4-address-no-zone
                 +--ro session
                 |  +--ro up-time?   pub-type:time-tick
                 |  +--ro state?     enumeration
                 +--ro message-statistics
                    +--ro message-statistic* [message-type]
                       +--ro message-type         enumeration
                       +--ro received-count?      uint32
                       +--ro sent-count?          uint32
                       +--ro invalid-count?       uint32
                       +--ro send-failed-count?   uint32

  rpcs:
    +---x reset-session
    |  +---w input
    |     +---w peer-address    inet:ipv4-address-no-zone
    +---x reset-statistics
       +---w input
          +---w peer-address    inet:ipv4-address-no-zone
