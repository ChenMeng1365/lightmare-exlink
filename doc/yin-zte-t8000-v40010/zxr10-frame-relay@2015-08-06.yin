<?xml version="1.0" encoding="UTF-8"?>
<module name="zxr10-frame-relay"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:frame-relay="http://www.zte.com.cn/zxr10/netconf/schema/rosng/frame-relay"
        xmlns:ncd="http://www.zte.com.cn/zxr10/netconf/schema/rosng/netconf-defs">
  <namespace uri="http://www.zte.com.cn/zxr10/netconf/schema/rosng/frame-relay"/>
  <prefix value="frame-relay"/>
  <yang-version value="1"/>
  <import module="zxr10-netconf-defs">
    <prefix value="ncd"/>
  </import>
  <organization>
    <text>ZTE Corporation</text>
  </organization>
  <contact>
    <text>ZTE Corporation  
   Nanjing Institute of ZTE Corporation 
   No.68 Zijinghua Rd. Yuhuatai District, Nanjing, China 
   Tel: +86-25-52870000</text>
  </contact>
  <description>
    <text>The netconf module to describe frame relay</text>
  </description>
  <revision date="2015-08-06">
    <description>
      <text>1 Adjust the position of the node /configuration/frame-relay/intf-mode-instance, and make it to be in the front of the node /configuration/frame-relay/ptop-instance.
2 Change the name of node from /configuration/frame-relay/dte-connter-instance to /configuration/frame-relay/dte-counter-instance</text>
    </description>
  </revision>
  <revision date="2013-12-02">
    <description>
      <text>The initial version</text>
    </description>
  </revision>
  <container name="configuration">
    <description>
      <text>Describe the configuration of frame relay</text>
    </description>
    <container name="frame-relay">
      <description>
        <text>Configuration data of frame relay</text>
      </description>
      <container name="base-instance">
        <description>
          <text>Set interface type and LMI type</text>
        </description>
        <list name="base">
          <description>
            <text>Set interface type and LMI type</text>
          </description>
          <key value="ifname"/>
          <leaf name="ifname">
            <description>
              <text>Interface name</text>
            </description>
            <type name="ncd:ifname"/>
          </leaf>
          <leaf name="intf-type">
            <description>
              <text>DTE/DCE interface</text>
            </description>
            <type name="enumeration">
              <enum name="dte">
                <value value="0"/>
                <description>
                  <text>Interface type is DTE</text>
                </description>
              </enum>
              <enum name="dce">
                <value value="1"/>
                <description>
                  <text>Interface type is DCE</text>
                </description>
              </enum>
            </type>
            <default value="dte"/>
          </leaf>
          <leaf name="lmi-type">
            <description>
              <text>LMI type of ANSI/CISCO/Q933A</text>
            </description>
            <type name="enumeration">
              <enum name="q933a">
                <value value="0"/>
                <description>
                  <text>q933a</text>
                </description>
              </enum>
              <enum name="cisco">
                <value value="1"/>
                <description>
                  <text>cisco</text>
                </description>
              </enum>
              <enum name="ansi">
                <value value="2"/>
                <description>
                  <text>ansi</text>
                </description>
              </enum>
            </type>
            <default value="q933a"/>
          </leaf>
        </list>
      </container>
      <container name="intf-mode-instance">
        <description>
          <text>Set interface mode</text>
        </description>
        <list name="intf-mode">
          <description>
            <text>Set interface mode</text>
          </description>
          <key value="ifname"/>
          <leaf name="ifname">
            <description>
              <text>Interface name</text>
            </description>
            <type name="ncd:ifname"/>
          </leaf>
          <leaf name="mode">
            <description>
              <text>Interface mode</text>
            </description>
            <type name="enumeration">
              <enum name="point-to-point">
                <value value="0"/>
                <description>
                  <text>point-to-point</text>
                </description>
              </enum>
              <enum name="point-to-multipoint">
                <value value="1"/>
                <description>
                  <text>point-to-multipoint</text>
                </description>
              </enum>
            </type>
            <default value="point-to-point"/>
          </leaf>
        </list>
      </container>
      <container name="ptop-instance">
        <description>
          <text>Set DLCI parameter</text>
        </description>
        <list name="ptop">
          <description>
            <text>Set DLCI parameter</text>
          </description>
          <key value="ifname"/>
          <leaf name="ifname">
            <description>
              <text>Interface name</text>
            </description>
            <type name="ncd:ifname"/>
          </leaf>
          <leaf name="dlci">
            <description>
              <text>interface DLCI</text>
            </description>
            <type name="uint32">
              <range value="16..1007">
                <description>
                  <text>The range of DLCI should be 16 to 1007</text>
                </description>
              </range>
            </type>
            <mandatory value="true"/>
          </leaf>
          <leaf name="encapsulation">
            <description>
              <text>Packet encapsulation type</text>
            </description>
            <type name="enumeration">
              <enum name="ietf">
                <value value="0"/>
                <description>
                  <text>ietf enacpsulation</text>
                </description>
              </enum>
              <enum name="cisco">
                <value value="1"/>
                <description>
                  <text>cisco enacpsulation</text>
                </description>
              </enum>
            </type>
            <default value="ietf"/>
          </leaf>
          <leaf name="broadcast">
            <description>
              <text>Broadcasts should be forwarded to this address</text>
            </description>
            <type name="enumeration">
              <enum name="nobroadcast">
                <value value="0"/>
              </enum>
              <enum name="broadcast">
                <value value="1"/>
              </enum>
            </type>
            <default value="nobroadcast"/>
          </leaf>
        </list>
      </container>
      <container name="ptomp-instance">
        <description>
          <text>Set map  parameter</text>
        </description>
        <list name="ptomp">
          <description>
            <text>Set map  parameter</text>
          </description>
          <key value="ifname dlci ipv4address"/>
          <leaf name="ifname">
            <description>
              <text>Interface name</text>
            </description>
            <type name="ncd:ifname"/>
          </leaf>
          <leaf name="dlci">
            <description>
              <text>interface DLCI</text>
            </description>
            <type name="uint32">
              <range value="16..1007">
                <description>
                  <text>The range of DLCI should be 16 to 1007</text>
                </description>
              </range>
            </type>
          </leaf>
          <leaf name="ipv4address">
            <description>
              <text>IPv4 address</text>
            </description>
            <type name="ncd:ipv4-address"/>
          </leaf>
          <leaf name="encapsulation">
            <description>
              <text>Packet encapsulation type</text>
            </description>
            <type name="enumeration">
              <enum name="ietf">
                <value value="0"/>
                <description>
                  <text>ietf encapsulation</text>
                </description>
              </enum>
              <enum name="cisco">
                <value value="1"/>
                <description>
                  <text>cisco encapsulation</text>
                </description>
              </enum>
            </type>
            <default value="ietf"/>
          </leaf>
          <leaf name="broadcast">
            <description>
              <text>Broadcasts should be forwarded to this address</text>
            </description>
            <type name="enumeration">
              <enum name="nobroadcast">
                <value value="0"/>
              </enum>
              <enum name="broadcast">
                <value value="1"/>
              </enum>
            </type>
            <default value="nobroadcast"/>
          </leaf>
        </list>
      </container>
      <container name="dte-counter-instance">
        <description>
          <text>Set DTE counter</text>
        </description>
        <list name="dte-counter">
          <description>
            <text>Set DTE counter</text>
          </description>
          <key value="ifname"/>
          <leaf name="ifname">
            <description>
              <text>Interface name</text>
            </description>
            <type name="ncd:ifname"/>
          </leaf>
          <leaf name="n391dte">
            <description>
              <text>Full status polling counter</text>
            </description>
            <type name="uint32">
              <range value="1..255">
                <description>
                  <text>n391dte counter</text>
                </description>
              </range>
            </type>
            <default value="6"/>
          </leaf>
          <leaf name="n392dte">
            <description>
              <text>DTE LMI error threshold</text>
            </description>
            <type name="uint32">
              <range value="1..10">
                <description>
                  <text>n392dte counter</text>
                </description>
              </range>
            </type>
            <default value="3"/>
          </leaf>
          <leaf name="n393dte">
            <description>
              <text>DTE LMI monitored event count</text>
            </description>
            <type name="uint32">
              <range value="1..10">
                <description>
                  <text>n393dte counter</text>
                </description>
              </range>
            </type>
            <default value="5"/>
          </leaf>
        </list>
        <ncd:revise-date date="2015-08-06"/>
      </container>
      <container name="dce-counter-instance">
        <description>
          <text>Set DCE counter</text>
        </description>
        <list name="dce-counter">
          <description>
            <text>Set DCE counter</text>
          </description>
          <key value="ifname"/>
          <leaf name="ifname">
            <description>
              <text>Interface name</text>
            </description>
            <type name="ncd:ifname"/>
          </leaf>
          <leaf name="t392dce">
            <description>
              <text>DCE LMI overtime period</text>
            </description>
            <type name="uint32">
              <range value="5..30">
                <description>
                  <text>t392dce counter</text>
                </description>
              </range>
            </type>
            <units name="second"/>
            <default value="15"/>
          </leaf>
          <leaf name="n392dce">
            <description>
              <text>DCE LMI error threshold</text>
            </description>
            <type name="uint32">
              <range value="1..10">
                <description>
                  <text>n392dce counter</text>
                </description>
              </range>
            </type>
            <default value="3"/>
          </leaf>
          <leaf name="n393dce">
            <description>
              <text>DCE LMI monitored event count</text>
            </description>
            <type name="uint32">
              <range value="1..10">
                <description>
                  <text>n393dce counter</text>
                </description>
              </range>
            </type>
            <default value="5"/>
          </leaf>
        </list>
      </container>
      <container name="keepalive-instance">
        <description>
          <text>Set keepalive time</text>
        </description>
        <list name="keepalive">
          <description>
            <text>Set keepalive time</text>
          </description>
          <key value="ifname"/>
          <leaf name="ifname">
            <description>
              <text>Interface name</text>
            </description>
            <type name="ncd:ifname"/>
          </leaf>
          <leaf name="keepalive">
            <description>
              <text>Keepalive time</text>
            </description>
            <type name="uint32">
              <range value="0..29">
                <description>
                  <text>keepalive time</text>
                </description>
              </range>
            </type>
            <units name="second"/>
            <default value="10"/>
          </leaf>
        </list>
      </container>
    </container>
  </container>
  <container name="state">
    <config value="false"/>
    <description>
      <text>Describe the state of frame relay</text>
    </description>
    <container name="frame-relay">
      <description>
        <text>State data of frame relay</text>
      </description>
      <container name="lmi-info-instance">
        <description>
          <text>Describe the LMI infomation of frame relay</text>
        </description>
        <list name="lmi-info">
          <description>
            <text>Describe the LMI infomation of frame relay</text>
          </description>
          <key value="ifname"/>
          <leaf name="ifname">
            <description>
              <text>Interface name</text>
            </description>
            <type name="ncd:ifname"/>
          </leaf>
          <leaf name="intf-type">
            <description>
              <text>DTE/DCE interface</text>
            </description>
            <type name="enumeration">
              <enum name="dte">
                <value value="0"/>
                <description>
                  <text>Interface is DTE</text>
                </description>
              </enum>
              <enum name="dce">
                <value value="1"/>
                <description>
                  <text>Interface is DCE</text>
                </description>
              </enum>
            </type>
          </leaf>
          <leaf name="lmi-type">
            <description>
              <text>LMI type of ANSI/CISCO/Q933A</text>
            </description>
            <type name="enumeration">
              <enum name="q933a">
                <value value="0"/>
                <description>
                  <text>LMI type is Q933A</text>
                </description>
              </enum>
              <enum name="cisco">
                <value value="1"/>
                <description>
                  <text>LMI type is CISCO</text>
                </description>
              </enum>
              <enum name="ansi">
                <value value="2"/>
                <description>
                  <text>LMI type is ANSI</text>
                </description>
              </enum>
            </type>
          </leaf>
          <leaf name="invalid-unnumbered-info">
            <description>
              <text>Invalid Unnumbered info</text>
            </description>
            <type name="uint32">
              <range value="0..4294967295"/>
            </type>
          </leaf>
          <leaf name="invalid-prot-disc">
            <description>
              <text>Invalid Prot Disc</text>
            </description>
            <type name="uint32">
              <range value="0..4294967295"/>
            </type>
          </leaf>
          <leaf name="invalid-dummy-call-ref">
            <description>
              <text>Invalid dummy Call Ref</text>
            </description>
            <type name="uint32">
              <range value="0..4294967295"/>
            </type>
          </leaf>
          <leaf name="invalid-msg-type">
            <description>
              <text>Invalid Msg Type</text>
            </description>
            <type name="uint32">
              <range value="0..4294967295"/>
            </type>
          </leaf>
          <leaf name="invalid-status-message">
            <description>
              <text>Invalid Status Message</text>
            </description>
            <type name="uint32">
              <range value="0..4294967295"/>
            </type>
          </leaf>
          <leaf name="invalid-lock-shift">
            <description>
              <text>Invalid Lock Shift</text>
            </description>
            <type name="uint32">
              <range value="0..4294967295"/>
            </type>
          </leaf>
          <leaf name="invalid-information-id">
            <description>
              <text>Invalid Information ID</text>
            </description>
            <type name="uint32">
              <range value="0..4294967295"/>
            </type>
          </leaf>
          <leaf name="invalid-report-id-len">
            <description>
              <text>Invalid Report IE Len</text>
            </description>
            <type name="uint32">
              <range value="0..4294967295"/>
            </type>
          </leaf>
          <leaf name="invalid-report-request">
            <description>
              <text>Invalid report request</text>
            </description>
            <type name="uint32">
              <range value="0..4294967295"/>
            </type>
          </leaf>
          <leaf name="invalid-keep-ie-len">
            <description>
              <text>Invalid Keep IE Len</text>
            </description>
            <type name="uint32">
              <range value="0..4294967295"/>
            </type>
          </leaf>
          <leaf name="status-enq-sent">
            <description>
              <text>Num Status Enq. Sent</text>
            </description>
            <type name="uint32">
              <range value="0..4294967295"/>
            </type>
          </leaf>
          <leaf name="status-msgs-rcvd">
            <description>
              <text>Num Status msgs Rcvd</text>
            </description>
            <type name="uint32">
              <range value="0..4294967295"/>
            </type>
          </leaf>
          <leaf name="update-status-rcvd">
            <description>
              <text>Num Update Status Rcvd</text>
            </description>
            <type name="uint32">
              <range value="0..4294967295"/>
            </type>
          </leaf>
          <leaf name="status-timeouts">
            <description>
              <text>Num Status Timeouts</text>
            </description>
            <type name="uint32">
              <range value="0..4294967295"/>
            </type>
          </leaf>
          <leaf name="status-enq-rcvd">
            <description>
              <text>Num Status Enq. Rcvd</text>
            </description>
            <type name="uint32">
              <range value="0..4294967295"/>
            </type>
          </leaf>
          <leaf name="status-msgs-sent">
            <description>
              <text>Num Status msgs Sent</text>
            </description>
            <type name="uint32">
              <range value="0..4294967295"/>
            </type>
          </leaf>
          <leaf name="update-status-sent">
            <description>
              <text>Num Update Status Sent</text>
            </description>
            <type name="uint32">
              <range value="0..4294967295"/>
            </type>
          </leaf>
          <leaf name="stenq-timeouts">
            <description>
              <text>Num St Enq. Timeouts</text>
            </description>
            <type name="uint32">
              <range value="0..4294967295"/>
            </type>
          </leaf>
        </list>
      </container>
      <container name="pvc-status-instance">
        <description>
          <text>Describe PVC status of frame relay</text>
        </description>
        <list name="pvc-status">
          <description>
            <text>Describe PVC status of frame relay</text>
          </description>
          <key value="ifname dlci"/>
          <leaf name="ifname">
            <description>
              <text>Interface name</text>
            </description>
            <type name="ncd:ifname"/>
          </leaf>
          <leaf name="dlci">
            <description>
              <text>interface DLCI</text>
            </description>
            <type name="uint32">
              <range value="16..1007">
                <description>
                  <text>The range of DLCI should be 16 to 1007</text>
                </description>
              </range>
            </type>
          </leaf>
          <leaf name="dlci-usage">
            <description>
              <text>DLCI USAGE</text>
            </description>
            <type name="enumeration">
              <enum name="LOCAL">
                <value value="1"/>
                <description>
                  <text>DLCI which is configed</text>
                </description>
              </enum>
              <enum name="UNUSED">
                <value value="2"/>
                <description>
                  <text>DLCI which is learned</text>
                </description>
              </enum>
            </type>
          </leaf>
          <leaf name="pvc-status">
            <description>
              <text>PVC status</text>
            </description>
            <type name="enumeration">
              <enum name="ACTIVE">
                <value value="1"/>
                <description>
                  <text>ATCIVE</text>
                </description>
              </enum>
              <enum name="INACTIVE">
                <value value="2"/>
                <description>
                  <text>INACTIVE</text>
                </description>
              </enum>
              <enum name="DELETED">
                <value value="3"/>
                <description>
                  <text>DELETED</text>
                </description>
              </enum>
            </type>
          </leaf>
          <leaf name="attached-interface">
            <description>
              <text>Interface which DLCI belongs to</text>
            </description>
            <type name="string">
              <length value="1..31">
                <description>
                  <text>Interface which DLCI belongs to</text>
                </description>
              </length>
            </type>
          </leaf>
          <ncd:select fields="ifname"/>
          <ncd:select fields="dlci"/>
          <ncd:select fields="ifname dlci"/>
        </list>
      </container>
    </container>
  </container>
</module>
