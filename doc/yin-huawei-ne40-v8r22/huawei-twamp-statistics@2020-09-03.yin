<?xml version="1.0" encoding="UTF-8"?>
<module name="huawei-twamp-statistics"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:twamp-statistics="urn:huawei:yang:huawei-twamp-statistics"
        xmlns:twamp-controller="urn:huawei:yang:huawei-twamp-controller"
        xmlns:ext="urn:huawei:yang:huawei-extension"
        xmlns:ifm="urn:huawei:yang:huawei-ifm">
  <namespace uri="urn:huawei:yang:huawei-twamp-statistics"/>
  <prefix value="twamp-statistics"/>
  <import module="huawei-twamp-controller">
    <prefix value="twamp-controller"/>
  </import>
  <import module="huawei-extension">
    <prefix value="ext"/>
  </import>
  <import module="huawei-ifm">
    <prefix value="ifm"/>
  </import>
  <organization>
    <text>Huawei Technologies Co., Ltd.</text>
  </organization>
  <contact>
    <text>Huawei Industrial Base
Bantian, Longgang
Shenzhen 518129
People's Republic of China
Website: http://www.huawei.com
Email: support@huawei.com</text>
  </contact>
  <description>
    <text>TWAMP statistics.</text>
  </description>
  <revision date="2020-09-03">
    <description>
      <text>Add new container: link-bundle-statistics.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <revision date="2020-03-05">
    <description>
      <text>Modify initial revision's description.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <revision date="2019-05-05">
    <description>
      <text>Init revision.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <ext:task-name value="twampclient"/>
  <augment target-node="/twamp-controller:twamp-controller/twamp-controller:client/twamp-controller:sessions/twamp-controller:session">
    <description>
      <text>TWAMP augment.</text>
    </description>
    <container name="statistics">
      <config value="false"/>
      <description>
        <text>TWAMP statistics.</text>
      </description>
      <leaf name="avg-delay">
        <type name="uint32"/>
        <units name="us"/>
        <description>
          <text>The average delay.</text>
        </description>
      </leaf>
      <leaf name="avg-jitter">
        <type name="uint32"/>
        <units name="us"/>
        <description>
          <text>The average jitter.</text>
        </description>
      </leaf>
      <leaf name="loss-count">
        <type name="uint32"/>
        <description>
          <text>The loss count.</text>
        </description>
      </leaf>
      <leaf name="send-count">
        <type name="uint32"/>
        <description>
          <text>The send count.</text>
        </description>
      </leaf>
    </container>
    <container name="link-bundle-statistics">
      <config value="false"/>
      <description>
        <text>List of TWAMP link-bundle statistics.</text>
      </description>
      <list name="link-bundle-statistic">
        <key value="if-name"/>
        <description>
          <text>Statistics of TWAMP link-bundle.</text>
        </description>
        <leaf name="if-name">
          <type name="leafref">
            <path value="/ifm:ifm/ifm:interfaces/ifm:interface/ifm:name"/>
          </type>
          <description>
            <text>Interface Name.</text>
          </description>
        </leaf>
        <leaf name="if-index">
          <type name="uint32"/>
          <description>
            <text>Interface ID.</text>
          </description>
        </leaf>
        <leaf name="loss-count">
          <type name="uint32"/>
          <description>
            <text>The loss count.</text>
          </description>
        </leaf>
        <leaf name="send-count">
          <type name="uint32"/>
          <description>
            <text>The send count.</text>
          </description>
        </leaf>
        <leaf name="avg-delay">
          <type name="uint32"/>
          <units name="us"/>
          <description>
            <text>The average delay.</text>
          </description>
        </leaf>
        <leaf name="avg-jitter">
          <type name="uint32"/>
          <units name="us"/>
          <description>
            <text>The average jitter.</text>
          </description>
        </leaf>
        <leaf name="avg-tx-jitter">
          <type name="uint32"/>
          <units name="us"/>
          <description>
            <text>The average TX jitter.</text>
          </description>
        </leaf>
        <leaf name="avg-rx-jitter">
          <type name="uint32"/>
          <units name="us"/>
          <description>
            <text>The average RX jitter.</text>
          </description>
        </leaf>
        <leaf name="max-delay">
          <type name="uint32"/>
          <units name="us"/>
          <description>
            <text>The maximum delay.</text>
          </description>
        </leaf>
        <leaf name="min-delay">
          <type name="uint32"/>
          <units name="us"/>
          <description>
            <text>The minimum delay.</text>
          </description>
        </leaf>
        <leaf name="max-jitter">
          <type name="uint32"/>
          <units name="us"/>
          <description>
            <text>The maximum jitter.</text>
          </description>
        </leaf>
        <leaf name="min-jitter">
          <type name="uint32"/>
          <units name="us"/>
          <description>
            <text>The minimum jitter.</text>
          </description>
        </leaf>
        <leaf name="max-tx-jitter">
          <type name="uint32"/>
          <units name="us"/>
          <description>
            <text>The maximum TX jitter.</text>
          </description>
        </leaf>
        <leaf name="min-tx-jitter">
          <type name="uint32"/>
          <units name="us"/>
          <description>
            <text>The minimum TX jitter.</text>
          </description>
        </leaf>
        <leaf name="max-rx-jitter">
          <type name="uint32"/>
          <units name="us"/>
          <description>
            <text>The maximum RX jitter.</text>
          </description>
        </leaf>
        <leaf name="min-rx-jitter">
          <type name="uint32"/>
          <units name="us"/>
          <description>
            <text>The minimum RX jitter.</text>
          </description>
        </leaf>
        <leaf name="member-id-error-count">
          <type name="uint32"/>
          <description>
            <text>The member-id error count.</text>
          </description>
        </leaf>
        <leaf name="checksum-error-count">
          <type name="uint32"/>
          <description>
            <text>The checksum error count.</text>
          </description>
        </leaf>
      </list>
    </container>
  </augment>
</module>
