  <typedef name="pm-elaborate-sample-type">
    <type name="enumeration">
      <enum name="default">
        <value value="0"/>
        <description>
          <text>Sets the value to the default one, which is disable.</text>
        </description>
      </enum>
      <enum name="second">
        <value value="1"/>
        <description>
          <text>Sets the time unit to seconds.</text>
        </description>
      </enum>
      <enum name="millisecond">
        <value value="2"/>
        <description>
