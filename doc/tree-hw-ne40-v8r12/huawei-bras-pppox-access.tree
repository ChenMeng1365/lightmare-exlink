module: huawei-bras-pppox-access
  +--rw bras-pppox-access
     +--rw ppp-chastens
     |  +--rw ppp-chasten* [quick-offline]
     |     +--rw quick-offline             boolean
     |     +--rw (chasten-type)?
     |     |  +--:(chasten)
     |     |  |  +--rw chasten?            empty
     |     |  +--:(option105)
     |     |     +--rw option105?          empty
     |     +--rw padi-discard?             boolean
     |     +--rw multi-sessions-per-mac?   boolean
     |     +--rw request-sessions          uint32
     |     +--rw request-period            uint32
     |     +--rw block-period              uint32
     +--rw ppp-echo-slow-acl
     |  +--rw acl-number?   -> /acl:acl/groups/group/identity
     +--rw user-threshold
     |  +--rw slot-warning-value?     uint8
     |  +--rw entire-warning-value?   uint8
     +--rw bgp-over-pppoe-enable
     |  +--rw over-pppoe-enable?   boolean
     +--rw pppoe-user-aaa
     |  +--rw name-check?            boolean
     |  +--rw gateway-loopback?      boolean
     |  +--rw offline-standardize?   boolean
     +--rw pppoe-rui-session-segment
     |  +--rw start-session-id?   uint16
     |  +--rw end-session-id?     uint16
     +--rw ppp-vlan-subinterface
     |  +--rw statistic-enable?   boolean
     +--rw pppoe-option82-sub-option
     |  +--rw option82-sub-option-enable?   boolean
     +--rw pppoe-server
     |  +--rw ncp-limit-enable?                boolean
     |  +--rw same-user-forbid-flag?           boolean
     |  +--rw padt-delay-flag?                 boolean
     |  +--rw remote-mac-max-session-number?   uint32
     |  +--rw max-pay-load-flag?               boolean
     +--ro ppp-user-infos
     |  +--ro ppp-user-info* [user-id]
     |     +--ro user-id                       uint32
     |     +--ro user-name?                    string
     |     +--ro circuit-id?                   string
     |     +--ro remote-id?                    string
     |     +--ro ipv4-address?                 inet:ipv4-address-no-zone
     |     +--ro ipv6-address?                 inet:ipv6-address-no-zone
     |     +--ro vpn-instance-name?            string
     |     +--ro session-id?                   uint32
     |     +--ro mac?                          pub-type:mac-address
     |     +--ro interface-name?               string
     |     +--ro pe-vlan?                      string
     |     +--ro ce-vlan?                      string
     |     +--ro ip?                           string
     |     +--ro gateway?                      string
     |     +--ro user-ip-netmask-client?       string
     |     +--ro user-ip-netmask-radius?       string
     |     +--ro user-ip-netmask-result?       string
     |     +--ro primary-dns?                  string
     |     +--ro second-dns?                   string
     |     +--ro ipv6-interfaceid-type?        string
     |     +--ro ipv6-local-interfaceid?       string
     |     +--ro ipv6-peer-interfaceid?        string
     |     +--ro user-ip-type?                 string
     |     +--ro authentication?               string
     |     +--ro mtu?                          string
     |     +--ro mru?                          string
     |     +--ro magic-number?                 string
     |     +--ro called-number?                string
     |     +--ro calling-number?               string
     |     +--ro keep-alive-time?              string
     |     +--ro retransmit-times?             string
     |     +--ro datacheck?                    string
     |     +--ro negotiation-timer?            string
     |     +--ro request-timeout?              string
     |     +--ro host-name?                    string
     |     +--ro challenge-retry-times?        string
     |     +--ro challenge-timeout?            string
     |     +--ro min-challenge-length?         string
     |     +--ro max-challenge-length?         string
     |     +--ro challenge-retry-times-msv1?   string
     |     +--ro challenge-timeout-msv1?       string
     |     +--ro min-challenge-length-msv1?    string
     |     +--ro max-challenge-length-msv1?    string
     |     +--ro challenge-retry-times-msv2?   string
     |     +--ro challenge-timeout-msv2?       string
     |     +--ro min-challenge-length-msv2?    string
     |     +--ro max-challenge-length-msv2?    string
     |     +--ro lac-peer-ip?                  string
     |     +--ro lac-peer-tunnel-id?           string
     |     +--ro lac-peer-session-id?          string
     |     +--ro lac-local-ip?                 string
     |     +--ro lac-local-tunnel-id?          string
     |     +--ro lac-local-session-id?         string
     |     +--ro lac-source-udp-port?          string
     |     +--ro lac-destination-udp-port?     string
     |     +--ro lns-peer-ip?                  string
     |     +--ro lns-peer-tunnel-id?           string
     |     +--ro lns-peer-session-id?          string
     |     +--ro lns-local-ip?                 string
     |     +--ro lns-local-tunnel-id?          string
     |     +--ro lns-local-session-id?         string
     |     +--ro lns-source-udp-port?          string
     |     +--ro lns-destination-udp-port?     string
