  <ext:task-name value="snmp"/>
  <container name="snmp">
    <description>
      <text>SNMP.</text>
    </description>
    <container name="agent-flag">
      <description>
        <text>Configure SNMP agent parameters.</text>
      </description>
      <leaf name="agent-enable">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable SNMP Agent Switch.</text>
        </description>
      </leaf>
    </container>
    <container name="engine">
      <description>
        <text>Configure SNMP engine parameters.</text>
      </description>
      <leaf name="version">
        <type name="snmp-version"/>
        <default value="v3"/>
        <description>
          <text>Version supported by SNMP engine.</text>
        </description>
      </leaf>
      <leaf name="id">
        <type name="snmp-engine-id"/>
        <description>
          <text>Engine ID of the SNMP, official unique identifier. By default, the VRP uses an internal algorithm to automatically generate an engine ID. The engine ID consists of an enterprise number and device information. When the engine ID changes, all existing SNMP USM users will be deleted.</text>
        </description>
      </leaf>
      <leaf name="max-msg-size">
        <type name="int32">
          <range value="484..17940"/>
        </type>
        <default value="12000"/>
        <description>
          <text>Maximum length of packet supported by SNMP engine.</text>
        </description>
      </leaf>
    </container>
    <container name="system">
      <description>
        <text>Configure SNMP parameters.</text>
      </description>
      <leaf name="trap-source-interface-name">
        <type name="pub-type:if-name"/>
        <description>
          <text>Name of the interface to send the trap message.</text>
        </description>
      </leaf>
      <leaf name="trap-source-ipv4-address">
        <type name="inet:ipv4-address-no-zone"/>
        <description>
          <text>Source IP to send trap/notification message.</text>
        </description>
      </leaf>
      <leaf name="inform-timeout">
        <type name="uint32">
          <range value="1..1800"/>
        </type>
        <units name="s"/>
        <default value="15"/>
        <description>
          <text>Wait for the confirmation of the sent alarm till timeout.</text>
        </description>
      </leaf>
      <leaf name="inform-resend-times">
        <type name="uint32">
          <range value="0..10"/>
        </type>
        <default value="3"/>
        <description>
          <text>Maximum number of times the trap packet should be sent if confirmation of alarm is not received.</text>
        </description>
      </leaf>
      <leaf name="inform-pend-number">
        <type name="uint32">
          <range value="1..2048"/>
        </type>
        <default value="39"/>
        <description>
          <text>Number of alarms sent for which confirmation is still pending.</text>
        </description>
      </leaf>
      <leaf name="packet-priority">
        <type name="uint32">
          <range value="0..7"/>
        </type>
        <default value="6"/>
        <description>
          <text>The priority of SNMP response packet.</text>
        </description>
      </leaf>
      <leaf name="trap-packet-priority">
        <type name="uint32">
          <range value="0..7"/>
        </type>
        <default value="6"/>
        <description>
          <text>The priority of SNMP trap packet.</text>
        </description>
      </leaf>
      <leaf name="listen-port">
        <type name="uint32">
          <range value="161 | 1025..65535"/>
        </type>
        <default value="161"/>
        <description>
          <text>SNMP listen port.</text>
        </description>
      </leaf>
      <leaf name="trap-listen-port">
        <type name="uint32">
          <range value="1025..65535"/>
        </type>
        <description>
          <text>SNMP trap port.</text>
        </description>
      </leaf>
      <leaf name="acl">
        <type name="string">
          <length value="1..32"/>
        </type>
        <description>
          <text>SNMP protocol level ACL.</text>
        </description>
      </leaf>
      <leaf name="community-cmplx-check">
        <type name="boolean"/>
        <default value="true"/>
        <description>
          <text>Enable/disable community complexity check switch.</text>
        </description>
      </leaf>
      <leaf name="usm-user-cmplx-check">
        <type name="boolean"/>
        <default value="true"/>
        <description>
          <text>Enable/disable USM user password complexity check switch.</text>
        </description>
      </leaf>
      <leaf name="ext-err-code-enable">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable extend error code switch.</text>
        </description>
      </leaf>
      <leaf name="set-cache-enable">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable set-cache switch.</text>
        </description>
      </leaf>
      <leaf name="local-user-cmplx-check">
        <type name="boolean"/>
        <default value="true"/>
        <description>
          <text>Enable/disable local user password complexity check switch.</text>
        </description>
      </leaf>
      <leaf name="protocol-source-interface-name">
        <type name="pub-type:if-name"/>
        <status value="obsolete"/>
        <description>
          <text>SNMP protocol source interface. The node /snmp:snmp/snmp:system/snmp:protocol-source-interface-name is not supported currently. You are advised to use the node /snmp:snmp/snmp:source-interfaces/snmp:source-interface/snmp:interface-name.</text>
        </description>
      </leaf>
      <leaf name="password-min-length">
        <type name="uint32">
          <range value="8..16"/>
        </type>
        <default value="8"/>
        <description>
          <text>SNMP password minimum length.</text>
        </description>
      </leaf>
      <leaf name="getbulk-timeout">
        <type name="uint32">
          <range value="0..600"/>
        </type>
        <units name="s"/>
        <default value="2"/>
        <description>
          <text>Timeout period of SNMP get-bulk.</text>
        </description>
      </leaf>
      <leaf name="protocol-source-ipv6-address">
        <type name="inet:ipv6-address-no-zone"/>
        <status value="obsolete"/>
        <description>
          <text>SNMP protocol source IPv6 address. The node /snmp:snmp/snmp:system/snmp:protocol-source-ipv6-address is not supported currently. You are advised to use the node /snmp:snmp/snmp:ipv6-sources/snmp:ipv6-source/snmp:src-ipv6-address.</text>
        </description>
      </leaf>
      <leaf name="vpn-instance-name">
        <type name="string">
          <length value="1..31"/>
        </type>
        <must condition="../vpn-instance-name='_public_' or ../is-public-net='false'"/>
        <description>
          <text>VPN instance Name.</text>
        </description>
      </leaf>
      <leaf name="ipv6-vpn-instance-name">
        <type name="string">
          <length value="1..31"/>
        </type>
        <must condition="../ipv6-vpn-instance-name='_public_' or ../is-ipv6-public-net='false'"/>
        <description>
          <text>VPN instance Name.</text>
        </description>
      </leaf>
      <leaf name="is-public-net">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable receive SNMP IPv4 message from public net.</text>
        </description>
      </leaf>
      <leaf name="is-ipv6-public-net">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable receive SNMP IPv6 message from public net.</text>
        </description>
      </leaf>
      <leaf name="ctx-engineid-check-enable">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable context engine id check switch.</text>
        </description>
      </leaf>
      <leaf name="ipv4-listen-disable">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable IPv4 listen function switch.</text>
        </description>
      </leaf>
      <leaf name="ipv6-listen-disable">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable IPv6 listen function switch.</text>
        </description>
      </leaf>
    </container>
    <container name="source-interfaces">
      <description>
        <text>List of source interfaces.</text>
      </description>
      <list name="source-interface">
        <key value="interface-name"/>
        <max-elements value="20"/>
        <description>
          <text>Configure Source interface entry.</text>
        </description>
        <leaf name="interface-name">
          <type name="pub-type:if-name"/>
          <description>
            <text>Source interface name.</text>
          </description>
        </leaf>
      </list>
    </container>
    <container name="ipv6-sources">
      <description>
        <text>List of source IPv6 address.</text>
      </description>
      <list name="ipv6-source">
        <key value="src-ipv6-address"/>
        <max-elements value="20"/>
        <description>
          <text>Configure source IPv6 address entry.</text>
        </description>
        <leaf name="src-ipv6-address">
          <type name="inet:ipv6-address-no-zone"/>
          <description>
            <text>Source IPv6 address.</text>
          </description>
        </leaf>
        <leaf name="vpn-name">
          <type name="string">
            <length value="1..31"/>
          </type>
          <description>
            <text>Source IPv6 VPN instance name.</text>
          </description>
        </leaf>
      </list>
    </container>
    <container name="all-interface">
      <description>
        <text>Configure all-interface.</text>
      </description>
      <leaf name="all-ipv4-interface">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable bind all IPv4 interface.</text>
        </description>
      </leaf>
      <leaf name="all-ipv6-interface">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable bind all IPv6 interface.</text>
        </description>
      </leaf>
    </container>
    <container name="target-hosts">
      <description>
        <text>List of target host for traps and informs.</text>
      </description>
      <list name="target-host">
        <key value="nms-name"/>
        <max-elements value="20"/>
        <description>
          <text>Configure target host entry.</text>
        </description>
        <leaf name="nms-name">
          <type name="string">
            <length value="1..32"/>
          </type>
          <description>
            <text>Unique name to identify target host entry.</text>
          </description>
        </leaf>
        <leaf name="domain">
          <type name="snmp-domain"/>
          <mandatory value="true"/>
          <description>
            <text>Address domain.</text>
          </description>
        </leaf>
        <leaf name="address">
          <type name="inet:ip-address-no-zone"/>
          <mandatory value="true"/>
          <description>
            <text>Network address.</text>
          </description>
        </leaf>
        <leaf name="target-inform-timeout">
          <when condition="../notify-type = 'inform'"/>
          <type name="int32">
            <range value="1..1800"/>
          </type>
          <units name="s"/>
          <default value="15"/>
          <description>
            <text>Timeout period of SNMP inform packets for the target.</text>
          </description>
        </leaf>
        <leaf name="target-inform-resend-times">
          <when condition="../notify-type = 'inform'"/>
          <type name="int32">
            <range value="0..10"/>
          </type>
          <default value="3"/>
          <description>
            <text>Maximum number of time to re-transmit the SNMP inform packet if acknowledgement is not received from the target.</text>
          </description>
        </leaf>
        <leaf name="notify-type">
          <type name="snmp-notify"/>
          <mandatory value="true"/>
          <description>
            <text>Notify type about alarm message.</text>
          </description>
        </leaf>
        <leaf name="vpn-instance-name">
          <type name="string">
            <length value="1..31"/>
          </type>
          <must condition="../vpn-instance-name='_public_' or ../is-public-net='false'"/>
          <description>
            <text>VPN instance name.</text>
          </description>
        </leaf>
        <leaf name="port-number">
          <type name="uint32">
            <range value="0..65535"/>
          </type>
          <default value="162"/>
          <description>
            <text>UDP port number used by network management to receive alarm messages.</text>
          </description>
        </leaf>
        <choice name="params">
          <mandatory value="true"/>
          <description>
            <text>This choice is augmented with case nodes containing configuration parameters specific to the security model.</text>
          </description>
          <case name="v1">
            <description>
              <text>Security model SNMPv1.</text>
            </description>
            <leaf name="security-name-v1">
              <when condition="../notify-type != 'inform'"/>
              <type name="pub-type:password-extend">
                <length value="1..32 | 48 | 56 | 68..168"/>
              </type>
              <mandatory value="true"/>
              <description>
                <text>SNMPV1 security name.</text>
              </description>
            </leaf>
          </case>
          <case name="v2c">
            <description>
              <text>Security model v2c.</text>
            </description>
            <leaf name="security-name-v2c">
              <type name="pub-type:password-extend">
                <length value="1..32 | 48 | 56 | 68..168"/>
              </type>
              <mandatory value="true"/>
              <description>
                <text>SNMPV2c security name.</text>
              </description>
            </leaf>
          </case>
          <case name="usm">
            <description>
              <text>Security model SNMPv3.</text>
            </description>
            <leaf name="usm-name">
              <type name="string">
                <length value="1..32"/>
              </type>
              <mandatory value="true"/>
              <description>
                <text>Usm name.</text>
              </description>
            </leaf>
            <leaf name="security-level">
              <type name="snmp:snmp-security-level"/>
              <mandatory value="true"/>
              <description>
                <text>Security level indicating whether to use authentication and encryption.</text>
              </description>
            </leaf>
          </case>
        </choice>
        <leaf name="private-netmanager">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable private VB's for target host.</text>
          </description>
        </leaf>
        <leaf name="is-public-net">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable Public Net-manager for target Host.</text>
          </description>
        </leaf>
        <leaf name="ext-vb">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable extended VB's for target host.</text>
          </description>
        </leaf>
        <leaf name="notify-fltr-profile-name">
          <type name="string">
            <length value="1..32"/>
          </type>
          <description>
            <text>To configure notification filter profile for target host.</text>
          </description>
        </leaf>
        <leaf name="retries-num">
          <type name="uint32"/>
          <config value="false"/>
          <description>
            <text>Number of retries made in sending inform.</text>
          </description>
        </leaf>
        <leaf name="pending-num">
          <type name="uint32"/>
          <config value="false"/>
          <description>
            <text>Number of informs which are waiting for acknowledgement.</text>
          </description>
        </leaf>
        <leaf name="sent-num">
          <type name="uint32"/>
          <config value="false"/>
          <description>
            <text>Number of packets sent.</text>
          </description>
        </leaf>
        <leaf name="dropped-num">
          <type name="uint32"/>
          <config value="false"/>
          <description>
            <text>Number of packets dropped while sending.</text>
          </description>
        </leaf>
        <leaf name="failed-num">
          <type name="uint32"/>
          <config value="false"/>
          <description>
            <text>Number of informs failed to receive acknowledgement.</text>
          </description>
        </leaf>
        <leaf name="confirmed-num">
          <type name="uint32"/>
          <config value="false"/>
          <description>
            <text>Number of informs received acknowledgement.</text>
          </description>
        </leaf>
        <leaf name="interface-name">
          <type name="pub-type:if-name"/>
          <description>
            <text>The specified source interface must exist and an IP address must be configured on the interface.</text>
          </description>
        </leaf>
        <leaf name="is-need-heart-beat">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable isNeedHeartBeat.</text>
          </description>
        </leaf>
        <leaf name="heart-beat-time">
          <when condition="../is-need-heart-beat = 'true'"/>
          <type name="uint32">
            <range value="1..48"/>
          </type>
          <units name="h"/>
          <default value="24"/>
          <description>
            <text>Heart beat time.</text>
          </description>
        </leaf>
        <leaf name="status">
          <type name="boolean"/>
          <default value="false"/>
          <config value="false"/>
          <description>
            <text>Is target alive.</text>
          </description>
        </leaf>
      </list>
    </container>
    <container name="mib-views">
      <description>
        <text>List of VACM view to configure MIB subtree.</text>
      </description>
      <list name="mib-view">
        <key value="view-name sub-tree"/>
        <description>
          <text>Configure name of MIB View entry.</text>
        </description>
        <leaf name="view-name">
          <type name="string">
            <length value="1..32"/>
          </type>
          <description>
            <text>Name of subtree's MIB view entry.</text>
          </description>
        </leaf>
        <leaf name="sub-tree">
          <type name="string">
            <length value="1..255"/>
          </type>
          <description>
            <text>MIB subtree.</text>
          </description>
        </leaf>
        <leaf name="type">
          <type name="snmp-filter"/>
          <mandatory value="true"/>
          <description>
            <text>Filter (include | exclude).</text>
          </description>
        </leaf>
      </list>
    </container>
    <container name="v3-groups">
      <description>
        <text>List of SNMPv3 groups.</text>
      </description>
      <list name="v3-group">
        <key value="group-name security-level"/>
        <max-elements value="20"/>
        <description>
          <text>Configure SNMPv3 group entry.</text>
        </description>
        <leaf name="group-name">
          <type name="string">
            <length value="1..32"/>
          </type>
          <description>
            <text>Unique name to identify the SNMPv3 group.</text>
          </description>
        </leaf>
        <leaf name="security-level">
          <type name="snmp-security-level"/>
          <description>
            <text>Security level indicating whether to use authentication and encryption.</text>
          </description>
        </leaf>
        <leaf name="read-view-name">
          <type name="leafref">
            <path value="/snmp:snmp/snmp:mib-views/snmp:mib-view/snmp:view-name"/>
          </type>
          <description>
            <text>MIB view name for read.</text>
          </description>
        </leaf>
        <leaf name="write-view-name">
          <type name="leafref">
            <path value="/snmp:snmp/snmp:mib-views/snmp:mib-view/snmp:view-name"/>
          </type>
          <description>
            <text>MIB view name for write.</text>
          </description>
        </leaf>
        <leaf name="notify-view-name">
          <type name="leafref">
            <path value="/snmp:snmp/snmp:mib-views/snmp:mib-view/snmp:view-name"/>
          </type>
          <description>
            <text>MIB view name for notification.</text>
          </description>
        </leaf>
        <leaf name="acl-number">
          <type name="string">
            <length value="1..32"/>
          </type>
          <description>
            <text>ACL, leafref path /acl/aclGroups/aclGroup/aclNumOrName or /acl/aclGroup6s/aclGroup6/aclNumOrName.</text>
          </description>
        </leaf>
      </list>
    </container>
    <container name="usm-users">
      <description>
        <text>List of USM users for SNMPv3.</text>
      </description>
      <list name="usm-user">
        <key value="user-name"/>
        <max-elements value="20"/>
        <description>
          <text>Configure USM user entry.</text>
        </description>
        <leaf name="user-name">
          <type name="string">
            <length value="1..32"/>
          </type>
          <description>
            <text>Unique name to identify the USM user.</text>
          </description>
        </leaf>
        <leaf name="group-name">
          <type name="string">
            <length value="1..32"/>
          </type>
          <description>
            <text>Name of the group where user belongs to.</text>
          </description>
        </leaf>
        <leaf name="is-auth-localized">
          <type name="boolean"/>
          <default value="false"/>
          <config value="false"/>
          <description>
            <text>The flag of localized authentication-mode.</text>
          </description>
        </leaf>
        <leaf name="auth-protocol">
          <type name="snmp-authenmode"/>
          <must condition="(../auth-key and ../auth-protocol!='noAuth') or ../auth-protocol='noAuth'"/>
          <default value="noAuth"/>
          <description>
            <text>Authentication protocol.</text>
          </description>
        </leaf>
        <leaf name="auth-key">
          <when condition="../auth-protocol != 'noAuth'"/>
          <type name="pub-type:password-extend">
            <length value="1..432"/>
          </type>
          <must condition="string-length(../auth-key)&gt;=../../../system/password-min-length or ../../../system/usm-user-cmplx-check='false'"/>
          <mandatory value="true"/>
          <description>
            <text>The authentication password.</text>
          </description>
        </leaf>
        <leaf name="is-priv-localized">
          <type name="boolean"/>
          <default value="false"/>
          <config value="false"/>
