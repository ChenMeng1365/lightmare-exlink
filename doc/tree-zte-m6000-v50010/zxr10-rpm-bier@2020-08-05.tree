                 |           +--rw encapsulation-type    encapsulation-type
                 |           +--rw bift-coding?          enumeration
                 |           +--rw max-si?               uint16
                 +--rw static-bifts
                    +--rw static-bift* [min-bfrid max-bfrid]
                       +--rw min-bfrid        uint32
                       +--rw max-bfrid        uint32
                       +--rw nexthop
                       |  +--rw (nexthop)?
                       |     +--:(ipv4-nexthop)
                       |     |  +--rw ipv4-nexthop?   ncd:ipv4-address-none-zero
                       |     +--:(ipv6-nexthop)
