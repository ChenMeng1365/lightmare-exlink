            <description>
              <text>Notify default route on the interface.</text>
            </description>
            <type name="enumeration">
              <enum name="disable">
                <value value="0"/>
                <description>
                  <text>Can not notify default route on this interface.</text>
                </description>
              </enum>
              <enum name="enable">
                <value value="1"/>
                <description>
                  <text>Notify default route on this interface.</text>
                </description>
              </enum>
              <enum name="only">
                <value value="2"/>
                <description>
                  <text>Only notify default route on this interface.</text>
                </description>
              </enum>
            </type>
          </leaf>
          <leaf name="originate-default-route">
            <description>
              <text>Originate default route on the interface.</text>
            </description>
            <type name="enumeration">
              <enum name="disable">
                <value value="0"/>
                <description>
                  <text>Can not originate default route on this interface.</text>
                </description>
              </enum>
              <enum name="enable">
                <value value="1"/>
                <description>
                  <text>Originate default route and send it on this interface.</text>
                </description>
              </enum>
              <enum name="only">
                <value value="2"/>
                <description>
                  <text>Originate default route and only send it on this interface.</text>
                </description>
              </enum>
            </type>
          </leaf>
