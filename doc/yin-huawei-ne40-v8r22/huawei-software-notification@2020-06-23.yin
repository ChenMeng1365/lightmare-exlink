<?xml version="1.0" encoding="UTF-8"?>
<module name="huawei-software-notification"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:software-notification="urn:huawei:yang:huawei-software-notification"
        xmlns:ext="urn:huawei:yang:huawei-extension">
  <yang-version value="1"/>
  <namespace uri="urn:huawei:yang:huawei-software-notification"/>
  <prefix value="software-notification"/>
  <import module="huawei-extension">
    <prefix value="ext"/>
  </import>
  <organization>
    <text>Huawei Technologies Co., Ltd.</text>
  </organization>
  <contact>
    <text>Huawei Industrial Base
Bantian, Longgang
Shenzhen 518129
People's Republic of China
Website: http://www.huawei.com
Email: support@huawei.com</text>
  </contact>
  <description>
    <text>Notification of software.</text>
  </description>
  <revision date="2020-06-23">
    <description>
      <text>Add Smart-Upgrade-User-Data-Retention-Period-Expired.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <revision date="2020-06-11">
    <description>
      <text>Add system-rollback.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <revision date="2019-10-16">
    <description>
      <text>Initial revision.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <ext:task-name value="rpm"/>
  <notification name="package-verify-failed">
    <description>
      <text>Package digital signature verification failed to be verified.</text>
    </description>
    <leaf name="physical-name">
      <type name="string">
        <length value="1..31"/>
      </type>
      <description>
        <text>Entity name.</text>
      </description>
    </leaf>
    <leaf name="package-name">
      <type name="string">
        <length value="4..127"/>
      </type>
      <description>
        <text>Software package name.</text>
      </description>
    </leaf>
    <leaf name="fail-reason">
      <type name="string">
        <length value="1..127"/>
      </type>
      <description>
        <text>Error reason.</text>
      </description>
    </leaf>
  </notification>
  <notification name="package-verify-failed-resume">
    <description>
      <text>Package digital signature verification succeeded to be verified.</text>
    </description>
    <leaf name="physical-name">
      <type name="string">
        <length value="1..31"/>
      </type>
      <description>
        <text>Entity name.</text>
      </description>
    </leaf>
    <leaf name="package-name">
      <type name="string">
        <length value="4..127"/>
      </type>
      <description>
        <text>Software package name.</text>
      </description>
    </leaf>
    <leaf name="fail-reason">
      <type name="string">
        <length value="1..127"/>
      </type>
      <description>
        <text>Error reason.</text>
      </description>
    </leaf>
  </notification>
  <notification name="startup-secure-version-configuration-need-refresh">
    <description>
      <text>The secure boot version configuration needs to be updated.</text>
    </description>
    <leaf name="slot-id">
      <type name="string">
        <length value="1..49"/>
      </type>
      <description>
        <text>Slot ID.</text>
      </description>
    </leaf>
  </notification>
  <notification name="startup-secure-version-configuration-need-refresh-resume">
    <description>
      <text>The secure boot version configuration is successfully updated.</text>
    </description>
    <leaf name="slot-id">
      <type name="string">
        <length value="1..49"/>
      </type>
      <description>
        <text>Slot ID.</text>
      </description>
    </leaf>
  </notification>
  <notification name="system-rollback">
    <description>
      <text>Upgraded software fails to start the system, and the software rolls back to the old version.</text>
    </description>
    <leaf name="package-name">
      <type name="string">
        <length value="4..127"/>
      </type>
      <description>
        <text>The software package name before the rollback.</text>
      </description>
    </leaf>
    <leaf name="version">
      <type name="string">
        <length value="1..63"/>
      </type>
      <description>
        <text>The version before the rollback.</text>
      </description>
    </leaf>
    <leaf name="fail-reason">
      <type name="string">
        <length value="1..127"/>
      </type>
      <description>
        <text>Error reason.</text>
      </description>
    </leaf>
  </notification>
  <notification name="smart-upgrade-user-data-retention-period-expired">
    <description>
      <text>The smart-upgrade user data retention period has expired.</text>
    </description>
  </notification>
  <notification name="smart-upgrade-user-data-retention-period-expired-resume">
    <description>
      <text>The smart-upgrade user data is updated.</text>
    </description>
  </notification>
</module>
