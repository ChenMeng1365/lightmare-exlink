<?xml version="1.0" encoding="UTF-8"?>
<module name="zxr10-rip"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:rip="http://www.zte.com.cn/zxr10/netconf/schema/rosng/rip"
        xmlns:ncd="http://www.zte.com.cn/zxr10/netconf/schema/rosng/netconf-defs">
  <namespace uri="http://www.zte.com.cn/zxr10/netconf/schema/rosng/rip"/>
  <prefix value="rip"/>
  <yang-version value="1"/>
  <import module="zxr10-netconf-defs">
    <prefix value="ncd"/>
  </import>
  <organization>
    <text>ZTE Corporation</text>
  </organization>
  <contact>
    <text>ZTE Corporation  
   Nanjing Institute of ZTE Corporation 
   No.68 Zijinghua Rd. Yuhuatai District, Nanjing, China 
   Tel: +86-25-52870000</text>
  </contact>
  <revision date="2020-11-11">
    <description>
      <text>Add leafs to set all interfaces of RIP to be passive.</text>
    </description>
  </revision>
  <revision date="2020-09-21">
    <description>
      <text>Modify the length of route policy name to 200 characters.</text>
    </description>
  </revision>
  <revision date="2020-07-06">
    <description>
      <text>Modify the length of encrypted text.</text>
    </description>
  </revision>
  <revision date="2020-06-09">
    <description>
      <text>Modify the range of route-map, the paths are configuration/rip/public-instance/redistributes and configuration/rip/vrf-redistributes.</text>
    </description>
  </revision>
  <revision date="2017-09-26">
    <description>
      <text>Initial version.</text>
    </description>
  </revision>
  <typedef name="enable-or-disable">
    <type name="enumeration">
      <enum name="disable">
        <value value="0"/>
        <description>
          <text>The function is disabled.</text>
        </description>
      </enum>
      <enum name="enable">
        <value value="1"/>
        <description>
          <text>The function is enabled.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Define the state of function switch.</text>
    </description>
  </typedef>
  <typedef name="select-mode-def">
    <type name="enumeration">
      <enum name="global">
        <value value="1"/>
        <description>
          <text>The public network.</text>
        </description>
      </enum>
      <enum name="vpn">
        <value value="2"/>
        <description>
          <text>The VPN network.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>The type of network. The "global" means the public network, the "vpn" means private network.</text>
    </description>
  </typedef>
  <typedef name="route-status-def">
    <type name="enumeration">
      <enum name="invalid">
        <value value="0"/>
        <description>
          <text>The route is invalid.</text>
        </description>
      </enum>
      <enum name="invalid-and-best">
        <value value="1"/>
        <description>
          <text>The route is invalid but is the best.</text>
        </description>
      </enum>
      <enum name="valid">
        <value value="2"/>
        <description>
          <text>The route is valid but not best.</text>
        </description>
      </enum>
      <enum name="valid-and-best">
        <value value="3"/>
        <description>
          <text>The route is valid and best.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>The definition of route status.</text>
    </description>
  </typedef>
  <typedef name="route-time-status-def">
    <type name="enumeration">
      <enum name="normal">
        <value value="0"/>
        <description>
          <text>The route is normal.</text>
        </description>
      </enum>
      <enum name="holddown">
        <value value="1"/>
        <description>
          <text>The route is in holddown time.</text>
        </description>
      </enum>
      <enum name="flush">
        <value value="2"/>
        <description>
          <text>The route is out holddown time, possibly to be flushed.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>The definition of route time status.</text>
    </description>
  </typedef>
  <typedef name="route-owner-def">
    <type name="enumeration">
      <enum name="unknown">
        <value value="0"/>
        <description>
          <text>The route owner is unknown.</text>
        </description>
      </enum>
      <enum name="connected">
        <value value="1"/>
        <description>
          <text>Connected route.</text>
        </description>
      </enum>
      <enum name="static">
        <value value="2"/>
        <description>
          <text>Static route.</text>
        </description>
      </enum>
      <enum name="ospf-int">
        <value value="3"/>
        <description>
          <text>OSPF-INT route.</text>
        </description>
      </enum>
      <enum name="ospf-ext">
        <value value="4"/>
        <description>
          <text>OSPF-EXT route.</text>
        </description>
      </enum>
      <enum name="isis-l1">
        <value value="5"/>
        <description>
          <text>ISIS-L1 route.</text>
        </description>
      </enum>
      <enum name="isis-l2">
        <value value="6"/>
        <description>
          <text>ISIS-L2 route.</text>
        </description>
      </enum>
      <enum name="bgp-int">
        <value value="7"/>
        <description>
          <text>BGP-INT route.</text>
        </description>
      </enum>
      <enum name="bgp-ext">
        <value value="8"/>
        <description>
          <text>BGP-EXT route.</text>
        </description>
      </enum>
      <enum name="rip-connected">
        <value value="9"/>
        <description>
          <text>RIP connected route.</text>
        </description>
      </enum>
      <enum name="rip-discard">
        <value value="10"/>
        <description>
          <text>RIP discard route.</text>
        </description>
      </enum>
      <enum name="rip">
        <value value="11"/>
        <description>
          <text>RIP route.</text>
        </description>
      </enum>
      <enum name="nat">
        <value value="12"/>
        <description>
          <text>NAT route.</text>
        </description>
      </enum>
      <enum name="pat">
        <value value="13"/>
        <description>
          <text>PAT route.</text>
        </description>
      </enum>
      <enum name="user-ipaddr">
        <value value="14"/>
        <description>
          <text>User IP address route.</text>
        </description>
      </enum>
      <enum name="user-network">
        <value value="15"/>
        <description>
          <text>User network route.</text>
        </description>
      </enum>
      <enum name="user-special">
        <value value="16"/>
        <description>
          <text>User special route.</text>
        </description>
      </enum>
      <enum name="ps-busi-addr">
        <value value="17"/>
        <description>
          <text>PS-BUSI-address route.</text>
        </description>
      </enum>
      <enum name="ps-user-addr">
        <value value="18"/>
        <description>
          <text>PS-user-address route.</text>
        </description>
      </enum>
      <enum name="ipsec">
        <value value="19"/>
        <description>
          <text>IPSec route.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>The definition of route owner.</text>
    </description>
  </typedef>
  <typedef name="rip-version-def">
    <type name="enumeration">
      <enum name="v1">
        <value value="1"/>
        <description>
          <text>RIP version 1.</text>
        </description>
      </enum>
      <enum name="v2">
        <value value="2"/>
        <description>
          <text>RIP version 2.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>The definition of RIP version.</text>
    </description>
  </typedef>
  <container name="configuration">
    <description>
      <text>The configuration information processing of RIP module.</text>
    </description>
    <container name="rip">
      <description>
        <text>The configuration of RIP module.</text>
      </description>
      <container name="public-instance">
        <description>
          <text>The configuration of RIP public instance.</text>
        </description>
        <leaf name="version">
          <description>
            <text>Set routing protocol version.</text>
          </description>
          <type name="rip-version-def"/>
          <default value="v2"/>
        </leaf>
        <leaf name="max-paths">
          <description>
            <text>Maximum number of parallel routes RIP can support.</text>
          </description>
          <type name="uint8">
            <range value="1..32"/>
          </type>
          <default value="1"/>
        </leaf>
        <leaf name="default-metric">
          <description>
            <text>Default metric of redistributed routes.</text>
          </description>
          <type name="uint8">
            <range value="1..16"/>
          </type>
          <default value="1"/>
        </leaf>
        <leaf name="distance">
          <description>
            <text>Define an administrative distance.</text>
          </description>
          <type name="uint8">
            <range value="1..255"/>
          </type>
          <default value="120"/>
        </leaf>
        <leaf name="auto-summary">
          <description>
            <text>Enable automatic network number summarization.</text>
          </description>
          <type name="enable-or-disable"/>
          <default value="enable"/>
        </leaf>
        <leaf name="validate-update-source">
          <description>
            <text>Perform sanity checks against source address of routing updates.</text>
          </description>
          <type name="enable-or-disable"/>
          <default value="enable"/>
        </leaf>
        <leaf name="flash-update-threshold">
          <description>
            <text>Specify flash update threshold in second.</text>
          </description>
          <type name="uint8">
            <range value="1..30"/>
          </type>
          <units name="second"/>
          <default value="5"/>
        </leaf>
        <leaf name="bfd">
          <description>
            <text>Configure all interfaces BFD capability.</text>
          </description>
          <type name="enable-or-disable"/>
          <default value="disable"/>
        </leaf>
        <leaf name="fast-reroute">
          <description>
            <text>Enable RIP fast-reroute function.</text>
          </description>
          <type name="enable-or-disable"/>
          <default value="disable"/>
        </leaf>
        <leaf name="track-name">
          <description>
            <text>Event tracking.</text>
          </description>
          <type name="string">
            <length value="1..31"/>
          </type>
        </leaf>
        <container name="timer-basic">
          <description>
            <text>Adjust routing timers.</text>
          </description>
          <presence value="timer-basic"/>
          <leaf name="update-time">
            <description>
              <text>Interval between updates, default is 30(seconds).</text>
            </description>
            <type name="uint32">
              <range value="1..65535"/>
            </type>
            <units name="second"/>
            <mandatory value="true"/>
          </leaf>
          <leaf name="invalid-time">
            <description>
              <text>Invalid time, default is 180(seconds).</text>
            </description>
            <type name="uint32">
              <range value="1..65535"/>
            </type>
            <units name="second"/>
            <mandatory value="true"/>
          </leaf>
          <leaf name="holddown-time">
            <description>
              <text>Holddown time, default is 180(seconds).</text>
            </description>
            <type name="uint32">
              <range value="0..65535"/>
            </type>
            <units name="second"/>
            <mandatory value="true"/>
          </leaf>
          <leaf name="flush-time">
            <description>
              <text>Flush time, default is 240(seconds).</text>
            </description>
            <type name="uint32">
              <range value="1..65535"/>
            </type>
            <units name="second"/>
            <mandatory value="true"/>
          </leaf>
        </container>
        <container name="output-delay">
          <description>
            <text>Interpacket delay of RIP updates.</text>
          </description>
          <presence value="output-delay"/>
          <leaf name="packets">
            <description>
              <text>Packet number.</text>
            </description>
            <type name="uint32">
              <range value="1..4294967295"/>
            </type>
            <mandatory value="true"/>
          </leaf>
          <leaf name="delay-time">
            <description>
              <text>Delay in milliseconds.</text>
            </description>
            <type name="uint8">
              <range value="1..100"/>
            </type>
            <units name="millisecond"/>
            <mandatory value="true"/>
          </leaf>
        </container>
        <container name="networks">
          <description>
            <text>The configuration of RIP networks in public instance.</text>
          </description>
          <list name="network">
            <description>
              <text>Enable routing on an IP network.</text>
            </description>
            <key value="network-addr network-wildmask"/>
            <leaf name="network-addr">
              <description>
                <text>Network address.</text>
              </description>
              <type name="ncd:ipv4-address"/>
            </leaf>
            <leaf name="network-wildmask">
              <description>
                <text>Network wild mask.</text>
              </description>
              <type name="ncd:ipv4-imask"/>
            </leaf>
          </list>
        </container>
        <container name="neighbors">
          <description>
            <text>The configuration of RIP neighbors in public instance.</text>
          </description>
          <list name="neighbor">
            <description>
              <text>Specify a neighbor router.</text>
            </description>
            <key value="neighbor-addr"/>
            <leaf name="neighbor-addr">
              <description>
                <text>Neighbor IP address.</text>
              </description>
              <type name="ncd:ipv4-address"/>
            </leaf>
          </list>
        </container>
        <container name="redistributes">
          <description>
            <text>Redistribute information from another routing protocol.</text>
          </description>
          <container name="redistribute-isiss">
            <description>
              <text>Redistribute information from ISIS.</text>
            </description>
            <list name="redistribute-isis">
              <description>
                <text>Redistribute information from ISIS.</text>
              </description>
              <key value="protocol process-id"/>
              <leaf name="protocol">
                <description>
                  <text>Routing protocol type.</text>
                </description>
                <type name="enumeration">
                  <enum name="isis-1">
                    <value value="6"/>
                    <description>
                      <text>IS-IS level-1 routes.</text>
                    </description>
                  </enum>
                  <enum name="isis-2">
                    <value value="7"/>
                    <description>
                      <text>IS-IS level-2 routes.</text>
                    </description>
                  </enum>
                </type>
              </leaf>
              <leaf name="process-id">
                <description>
                  <text>Process ID of ISIS.</text>
                </description>
                <type name="uint32">
                  <range value="0..65535"/>
                </type>
              </leaf>
              <leaf name="metric">
                <description>
                  <text>Metric for redistributed routes.</text>
                </description>
                <type name="uint8">
                  <range value="1..16"/>
                </type>
              </leaf>
              <leaf name="route-map">
                <description>
                  <text>Apply route-map to redistribute.</text>
                </description>
                <type name="string">
                  <length value="1..200"/>
                </type>
              </leaf>
              <leaf name="route-policy">
                <description>
                  <text>Apply route-policy to redistribute.</text>
                </description>
                <type name="string">
                  <length value="1..200"/>
                </type>
              </leaf>
            </list>
          </container>
          <container name="redistribute-ospfs">
            <description>
              <text>Redistribute information from OSPF.</text>
            </description>
            <list name="redistribute-ospf">
              <description>
                <text>Redistribute information from OSPF.</text>
              </description>
              <key value="protocol process-id"/>
              <leaf name="protocol">
                <description>
                  <text>Routing protocol type.</text>
                </description>
                <type name="enumeration">
                  <enum name="ospf-int">
                    <value value="2"/>
                    <description>
                      <text>Open shortest path first(OSPF) internal routes.</text>
                    </description>
                  </enum>
                  <enum name="ospf-ext">
                    <value value="3"/>
                    <description>
                      <text>Open shortest path first(OSPF) external routes.</text>
                    </description>
                  </enum>
                </type>
              </leaf>
              <leaf name="process-id">
                <description>
                  <text>Process ID of OSPF.</text>
                </description>
                <type name="uint32">
                  <range value="1..65535"/>
                </type>
              </leaf>
              <leaf name="metric">
                <description>
                  <text>Metric for redistributed routes.</text>
                </description>
                <type name="uint8">
                  <range value="1..16"/>
                </type>
              </leaf>
              <leaf name="route-map">
                <description>
                  <text>Apply route-map to redistribute.</text>
                </description>
                <type name="string">
                  <length value="1..200"/>
                </type>
              </leaf>
              <leaf name="route-policy">
                <description>
                  <text>Apply route-policy to redistribute.</text>
                </description>
                <type name="string">
                  <length value="1..200"/>
                </type>
              </leaf>
            </list>
          </container>
          <container name="redistribute-pss">
            <description>
              <text>Redistribute information from PS.</text>
            </description>
            <list name="redistribute-ps">
              <description>
                <text>Redistribute information from PS.</text>
              </description>
              <key value="protocol"/>
              <leaf name="protocol">
                <description>
                  <text>Routing protocol type.</text>
                </description>
                <type name="enumeration">
                  <enum name="ps-busi-addr">
                    <value value="12"/>
                    <description>
                      <text>Ps-busi-addr routes.</text>
                    </description>
                  </enum>
                  <enum name="ps-user-addr">
                    <value value="13"/>
                    <description>
                      <text>Ps-user-addr routes.</text>
                    </description>
                  </enum>
                </type>
              </leaf>
              <leaf name="metric">
                <description>
                  <text>Configure metric for redistributed routes.</text>
                </description>
                <type name="uint8">
                  <range value="1..16"/>
                </type>
              </leaf>
              <leaf name="with-originate-metric">
                <description>
                  <text>Use original metric of redistributed route.</text>
                </description>
                <type name="empty"/>
              </leaf>
              <leaf name="route-map">
                <description>
                  <text>Apply route-map to redistribute.</text>
                </description>
                <type name="string">
                  <length value="1..200"/>
                </type>
              </leaf>
              <leaf name="route-policy">
                <description>
                  <text>Apply route-policy to redistribute.</text>
                </description>
                <type name="string">
                  <length value="1..200"/>
                </type>
              </leaf>
            </list>
          </container>
          <container name="redistribute-others">
            <description>
              <text>Redistribute information from another routing protocol.</text>
            </description>
            <list name="redistribute-other">
              <description>
                <text>Redistribute information from another routing protocol.</text>
              </description>
              <key value="protocol"/>
              <leaf name="protocol">
                <description>
                  <text>Routing protocol type.</text>
                </description>
                <type name="enumeration">
                  <enum name="connected">
                    <value value="0"/>
                    <description>
                      <text>Connected routes.</text>
                    </description>
                  </enum>
                  <enum name="static">
                    <value value="1"/>
                    <description>
                      <text>Static routes.</text>
                    </description>
                  </enum>
                  <enum name="bgp-ext">
                    <value value="4"/>
                    <description>
                      <text>External border gateway protocol (EBGP) routes.</text>
                    </description>
                  </enum>
                  <enum name="bgp-int">
                    <value value="5"/>
                    <description>
                      <text>Internal border gateway protocol (IBGP) routes.</text>
                    </description>
                  </enum>
                  <enum name="nat">
                    <value value="8"/>
                    <description>
                      <text>NAT routes.</text>
                    </description>
                  </enum>
                  <enum name="natpt">
                    <value value="9"/>
                    <description>
                      <text>Port-address Translation routes.</text>
                    </description>
                  </enum>
                  <enum name="subscriber-host">
                    <value value="10"/>
                    <description>
                      <text>Bras subscriber's host routes.</text>
                    </description>
                  </enum>
                  <enum name="subscriber-aggregation">
                    <value value="11"/>
                    <description>
                      <text>Bras subscriber's aggregation routes.</text>
                    </description>
                  </enum>
                  <enum name="user-special">
                    <value value="14"/>
                    <description>
                      <text>User-special routes.</text>
                    </description>
                  </enum>
                  <enum name="ipsec">
                    <value value="15"/>
                    <description>
                      <text>IPsec routes.</text>
                    </description>
                  </enum>
                </type>
              </leaf>
              <leaf name="metric">
                <description>
                  <text>Metric for redistributed routes.</text>
                </description>
                <type name="uint8">
                  <range value="1..16"/>
                </type>
              </leaf>
              <leaf name="route-map">
                <description>
                  <text>Apply route-map to redistribute.</text>
                </description>
                <type name="string">
                  <length value="1..200"/>
                </type>
              </leaf>
              <leaf name="route-policy">
                <description>
                  <text>Apply route-policy to redistribute.</text>
                </description>
                <type name="string">
                  <length value="1..200"/>
                </type>
              </leaf>
            </list>
          </container>
        </container>
        <container name="offset-lists">
          <description>
            <text>Apply offset lists to RIP.</text>
          </description>
          <container name="for-insts">
            <description>
              <text>Apply offset lists to RIP instance.</text>
            </description>
            <presence value="for-insts"/>
            <list name="for-inst">
              <description>
                <text>Apply offset lists to RIP instance.</text>
              </description>
              <key value="in-or-out"/>
              <leaf name="in-or-out">
                <description>
                  <text>Specify the direction of offset list.</text>
                </description>
                <type name="enumeration">
                  <enum name="in">
                    <value value="1"/>
                    <description>
                      <text>Inbound packets.</text>
                    </description>
                  </enum>
                  <enum name="out">
                    <value value="2"/>
                    <description>
                      <text>Outbound packets.</text>
                    </description>
                  </enum>
                </type>
              </leaf>
              <leaf name="acl-name">
                <description>
                  <text>Access list of networks to apply offset list(1-31 characters).</text>
                </description>
                <type name="string">
                  <length value="1..31"/>
                </type>
                <mandatory value="true"/>
              </leaf>
              <leaf name="metric">
                <description>
                  <text>Offset list metric.</text>
                </description>
                <type name="uint8">
                  <range value="0..16"/>
                </type>
                <mandatory value="true"/>
              </leaf>
            </list>
          </container>
          <container name="for-ifs">
            <description>
              <text>Apply offset lists to RIP interface.</text>
            </description>
            <presence value="for-ifs"/>
            <list name="for-if">
              <description>
                <text>Apply offset lists to RIP interface.</text>
              </description>
              <key value="in-or-out interface"/>
              <leaf name="in-or-out">
                <description>
                  <text>Specify the direction of offset list.</text>
                </description>
                <type name="enumeration">
                  <enum name="in">
                    <value value="1"/>
                    <description>
                      <text>Inbound packets.</text>
                    </description>
                  </enum>
                  <enum name="out">
                    <value value="2"/>
                    <description>
                      <text>Outbound packets.</text>
                    </description>
                  </enum>
                </type>
              </leaf>
              <leaf name="interface">
                <description>
                  <text>Interface name.</text>
                </description>
                <type name="ncd:ifname">
                  <length value="1..31"/>
                </type>
              </leaf>
              <leaf name="acl-name">
                <description>
                  <text>Access list of networks to apply offset (1-31 characters).</text>
                </description>
                <type name="string">
                  <length value="1..31"/>
                </type>
                <mandatory value="true"/>
              </leaf>
              <leaf name="metric">
                <description>
                  <text>Offset list metric.</text>
                </description>
                <type name="uint8">
                  <range value="0..16"/>
                </type>
                <mandatory value="true"/>
              </leaf>
            </list>
          </container>
        </container>
        <container name="interfaces">
          <description>
            <text>Configuration of RIP interfaces.</text>
          </description>
          <list name="interface">
            <description>
              <text>Configuration of RIP interface.</text>
            </description>
            <key value="if-name"/>
