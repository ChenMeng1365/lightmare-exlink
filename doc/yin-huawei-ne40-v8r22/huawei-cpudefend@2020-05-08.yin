  <augment target-node="/ifm:ifm/ifm:interfaces/ifm:interface">
    <description>
      <text>Adds cpudefend configuration to the interface.</text>
    </description>
    <container name="cpudefend">
      <description>
        <text>Configure policies for global management and service plane protection.</text>
      </description>
      <container name="arp-safeguards">
        <description>
          <text>Configure ARP bidirectional isolation.</text>
        </description>
        <leaf name="arp-safeguard">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable ARP bidirectional isolation.</text>
          </description>
        </leaf>
      </container>
      <container name="host-car-qinq-hashs">
        <description>
          <text>Configure the QinQ hash mode for HOST CAR.</text>
        </description>
        <leaf name="host-car-qinq-hash">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable the QinQ hash mode for HOST CAR.</text>
          </description>
        </leaf>
      </container>
      <container name="ipv4-urpf">
        <presence value="config ipv4 urpf"/>
        <description>
          <text>Enable/disable IPv4 URPF check in the interface.</text>
        </description>
        <choice name="method">
          <mandatory value="true"/>
          <description>
            <text>The method of IPv4 URPF.</text>
          </description>
          <case name="general">
            <description>
              <text>IPv4 general URPF on an interface.</text>
            </description>
            <leaf name="mode">
              <type name="urpf-mode"/>
              <mandatory value="true"/>
              <description>
                <text>Indicates URPF check mode, such as strict or loose.</text>
              </description>
            </leaf>
            <leaf name="default-route">
              <type name="boolean"/>
              <default value="false"/>
              <description>
                <text>Enable/disable indicates that URPF is implemented for packets matching the default route.</text>
              </description>
            </leaf>
          </case>
          <case name="peer-based">
            <description>
              <text>IPv4 peer-based URPF on an interface.</text>
            </description>
            <leaf name="peer-id">
              <type name="uint32">
                <range value="1..255"/>
              </type>
              <mandatory value="true"/>
              <description>
                <text>A peer group ID.</text>
              </description>
            </leaf>
          </case>
        </choice>
        <leaf name="statistics-enable">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable indicates the URPF statistics function.</text>
          </description>
        </leaf>
      </container>
      <container name="ipv6-urpf">
        <presence value="config ipv6 urpf"/>
        <description>
          <text>Enable/disable IPv6 URPF check in the interface.</text>
        </description>
        <choice name="method">
          <mandatory value="true"/>
          <description>
            <text>The method of IPv6 URPF.</text>
          </description>
          <case name="general">
            <description>
              <text>IPv6 general URPF on an interface.</text>
            </description>
            <leaf name="mode">
              <type name="urpf-mode"/>
              <mandatory value="true"/>
              <description>
                <text>Indicates URPF check mode, such as strict or loose.</text>
              </description>
            </leaf>
            <leaf name="default-route">
              <type name="boolean"/>
              <default value="false"/>
              <description>
                <text>Enable/disable indicates that URPF is implemented for packets matching the default route.</text>
              </description>
            </leaf>
          </case>
          <case name="peer-based">
            <description>
              <text>IPv6 peer-based URPF on an interface.</text>
            </description>
            <leaf name="peer-id">
              <type name="uint32">
                <range value="1..255"/>
