                          <key value="delay-type"/>
                          <description>
                            <text>Configure the threshold of delay measurement.</text>
                          </description>
                          <leaf name="delay-type">
                            <type name="delay-type"/>
                            <description>
                              <text>Configure the type of delay measurement.</text>
                            </description>
                          </leaf>
                          <leaf name="upper-limit">
                            <type name="uint32">
                              <range value="0..4294967295"/>
                            </type>
                            <mandatory value="true"/>
                            <description>
                              <text>Configure the upper limit of delay threshold.</text>
                            </description>
                          </leaf>
                          <leaf name="lower-limit">
                            <type name="uint32">
                              <range value="0..4294967295"/>
                            </type>
                            <mandatory value="true"/>
                            <description>
                              <text>Configure the lower limit of delay threshold.</text>
                            </description>
                          </leaf>
                        </list>
                      </container>
                      <container name="delay-threshold-prioritys">
                        <description>
                          <text>List of the threshold of delay measurement.</text>
                        </description>
                        <list name="delay-threshold-priority">
                          <must condition="upper-limit&gt;lower-limit"/>
                          <must condition="not(../../../../l2tp-ccc-name) and not(../../../../tnl-name) and not(../../../../no-bind)"/>
                          <key value="delay-type priority"/>
                          <description>
                            <text>Configure the threshold of delay measurement.</text>
                          </description>
                          <leaf name="delay-type">
                            <type name="delay-type"/>
                            <description>
                              <text>Configure the type of delay measurement.</text>
                            </description>
                          </leaf>
                          <leaf name="priority">
                            <type name="uint8">
                              <range value="0..7"/>
                            </type>
                            <description>
                              <text>Configure the priority of delay measurement.</text>
                            </description>
                          </leaf>
                          <leaf name="upper-limit">
                            <type name="uint32">
                              <range value="0..4294967295"/>
                            </type>
                            <mandatory value="true"/>
                            <description>
                              <text>Configure the upper limit of delay threshold.</text>
                            </description>
                          </leaf>
                          <leaf name="lower-limit">
                            <type name="uint32">
                              <range value="0..4294967295"/>
                            </type>
                            <mandatory value="true"/>
                            <description>
                              <text>Configure the lower limit of delay threshold.</text>
                            </description>
                          </leaf>
                        </list>
                      </container>
                      <container name="loss-thresholds">
                        <description>
                          <text>List of the threshold of loss measurement.</text>
                        </description>
                        <list name="loss-threshold">
                          <must condition="not(../../../../l2tp-ccc-name) and not(../../../../tnl-name) and not(../../../../no-bind) and not(../../../../evpn-name) and not(../../../../bd-id)"/>
                          <key value="loss-type"/>
                          <description>
                            <text>Configure the threshold of loss measurement.</text>
                          </description>
                          <leaf name="loss-type">
                            <type name="loss-type"/>
                            <description>
                              <text>Configure the type of loss measurement.</text>
                            </description>
                          </leaf>
                          <leaf name="upper-limit">
                            <type name="string">
                              <length value="1..8"/>
                              <pattern value="[0-1]{0,1}[0-9]{0,1}[0-9]{0,1}(\.[0-9]{1,4})?"/>
                            </type>
                            <mandatory value="true"/>
                            <description>
                              <text>Configure the upper limit of loss threshold.</text>
                            </description>
                          </leaf>
                          <leaf name="lower-limit">
                            <type name="string">
                              <length value="1..8"/>
                              <pattern value="[0-1]{0,1}[0-9]{0,1}[0-9]{0,1}(\.[0-9]{1,4})?"/>
                            </type>
                            <mandatory value="true"/>
                            <description>
                              <text>Configure the lower limit of loss threshold.</text>
                            </description>
                          </leaf>
                        </list>
                      </container>
                      <container name="loss-threshold-prioritys">
                        <description>
                          <text>List of the threshold of loss measurement.</text>
                        </description>
                        <list name="loss-threshold-priority">
                          <must condition="not(../../../../l2tp-ccc-name) and not(../../../../tnl-name) and not(../../../../no-bind) and not(../../../../evpn-name) and not(../../../../bd-id)"/>
                          <key value="loss-type priority"/>
                          <description>
                            <text>Configure the threshold of loss measurement.</text>
                          </description>
                          <leaf name="loss-type">
                            <type name="loss-type-priority"/>
                            <description>
                              <text>Configure the type of loss measurement.</text>
                            </description>
                          </leaf>
                          <leaf name="priority">
                            <type name="uint8">
                              <range value="0..7"/>
                            </type>
                            <description>
                              <text>Configure the priority of loss measurement.</text>
                            </description>
                          </leaf>
                          <leaf name="upper-limit">
                            <type name="string">
                              <length value="1..8"/>
                              <pattern value="[0-1]{0,1}[0-9]{0,1}[0-9]{0,1}(\.[0-9]{1,4})?"/>
                            </type>
                            <mandatory value="true"/>
                            <description>
                              <text>Configure the upper limit of loss threshold.</text>
                            </description>
                          </leaf>
                          <leaf name="lower-limit">
                            <type name="string">
                              <length value="1..8"/>
                              <pattern value="[0-1]{0,1}[0-9]{0,1}[0-9]{0,1}(\.[0-9]{1,4})?"/>
                            </type>
                            <mandatory value="true"/>
                            <description>
                              <text>Configure the lower limit of loss threshold.</text>
                            </description>
                          </leaf>
                        </list>
                      </container>
                      <description>
                        <text>Configure a MEP on an interface.</text>
                      </description>
                    </case>
                    <case name="pw-mep">
                      <when condition="../../vsi-name"/>
                      <leaf name="peer-ip">
                        <ext:operation-exclude value="update"/>
                        <type name="inet:ipv4-address-no-zone"/>
                        <mandatory value="true"/>
                        <description>
                          <text>Configure the IP of peer end.</text>
                        </description>
                      </leaf>
                      <leaf name="vc-id">
                        <ext:operation-exclude value="update|delete"/>
                        <when condition="../peer-ip"/>
                        <type name="uint32">
                          <range value="1..4294967295"/>
                        </type>
                        <description>
                          <text>Configure the ID of VC.</text>
                        </description>
                      </leaf>
                      <leaf name="config-mac">
                        <ext:operation-exclude value="update|delete"/>
                        <type name="pub-type:mac-address"/>
                        <description>
                          <text>MAC address of a MEP.</text>
                        </description>
                      </leaf>
                      <description>
                        <text>Configure a MEP on a PW.</text>
                      </description>
                    </case>
                    <case name="vlan-if-mep">
                      <when condition="../../vlan-id"/>
                      <leaf name="vlan-mep">
                        <type name="empty"/>
                        <description>
                          <text>A VLAN TYPE MEP.</text>
                        </description>
                      </leaf>
                      <description>
                        <text>Configure a MEP on a VLANIF.</text>
                      </description>
                    </case>
                  </choice>
                  <leaf name="direction">
                    <ext:operation-exclude value="update|delete"/>
                    <type name="direction"/>
                    <must condition="(../direction='outward' and count(../../mep)=1) or not(../../../meps/mep[direction!=current()])"/>
                    <description>
                      <text>Configure the direction of a MEP.</text>
                    </description>
                  </leaf>
                  <leaf name="cc-send-enable">
                    <type name="boolean"/>
                    <must condition="../cc-send-enable='false' or (../if-name or ../vlan-mep)"/>
                    <default value="false"/>
                    <description>
                      <text>Enable/disable CCMs which can be sent.</text>
                    </description>
                  </leaf>
                  <leaf name="rdi-alarm-disabled">
                    <type name="boolean"/>
                    <must condition="../rdi-alarm-disabled='false' or (../if-name or ../peer-ip or ../vlan-mep)"/>
                    <default value="false"/>
                    <description>
                      <text>Enable/disable suppressing RDI alarm.</text>
                    </description>
                  </leaf>
                  <leaf name="ais-alarm-disabled">
                    <type name="boolean"/>
                    <must condition="../ais-alarm-disabled='false' or (../if-name or ../peer-ip or ../vlan-mep)"/>
                    <default value="false"/>
                    <description>
                      <text>Enable/disable suppressing AIS alarm.</text>
                    </description>
                  </leaf>
                </list>
              </container>
              <container name="rmeps">
                <description>
                  <text>List of all remote maintenance points.</text>
                </description>
                <list name="rmep">
                  <key value="id"/>
                  <description>
                    <text>Configure a remote maintenance point.</text>
                  </description>
                  <leaf name="id">
                    <type name="uint16">
                      <range value="1..8191"/>
                    </type>
                    <description>
                      <text>Configure the ID of a RMEP.</text>
                    </description>
                  </leaf>
                  <leaf name="cc-recv-enabled">
                    <type name="boolean"/>
                    <default value="false"/>
                    <description>
                      <text>Enable/disable CC receive.</text>
                    </description>
                  </leaf>
                  <leaf name="config-mac">
                    <ext:operation-exclude value="create|update|delete"/>
                    <type name="pub-type:mac-address"/>
                    <description>
                      <text>Configure the MAC of a RMEP, format: H-H-H.</text>
                    </description>
                  </leaf>
                  <leaf name="cfm-status">
                    <type name="cfm-status"/>
                    <config value="false"/>
                    <description>
                      <text>Status of a RMEP.</text>
                    </description>
                  </leaf>
                </list>
              </container>
            </list>
          </container>
        </list>
      </container>
      <container name="test-instances">
        <description>
          <text>List of all test instances.</text>
        </description>
        <list name="test-instance">
          <key value="test-id"/>
          <description>
            <text>Configure a test instance.</text>
          </description>
          <leaf name="test-id">
            <type name="uint32">
              <range value="1..4294967295"/>
            </type>
            <description>
              <text>Configure the ID of a test instance (65536 to 4294967295 is reserved for automatic allocation).</text>
            </description>
          </leaf>
          <leaf name="description">
            <ext:operation-exclude value="update|delete"/>
            <type name="string">
              <length value="1..63"/>
              <pattern value="[^\?]*"/>
            </type>
            <description>
              <text>Configure the description of a test instance.</text>
            </description>
          </leaf>
          <leaf name="md-name">
            <ext:operation-exclude value="update"/>
            <type name="leafref">
              <path value="/y1731/global-enable/mds/md/name"/>
            </type>
            <mandatory value="true"/>
            <description>
              <text>Configure the name of a maintenance domain (MD).</text>
            </description>
          </leaf>
          <leaf name="ma-name">
            <ext:operation-exclude value="update"/>
            <type name="leafref">
              <path value="/y1731/global-enable/mds/md/mas/ma/name"/>
            </type>
            <must condition="(/y1731:y1731/y1731:global-enable/y1731:mds/y1731:md[y1731:name=current()/../md-name]/y1731:mas/y1731:ma[y1731:name=current()])"/>
            <mandatory value="true"/>
            <description>
              <text>Configure the name of a maintenance association (MA).</text>
            </description>
          </leaf>
          <leaf name="mep-id">
            <ext:operation-exclude value="update"/>
            <type name="leafref">
              <path value="/y1731/global-enable/mds/md/mas/ma/meps/mep/id"/>
            </type>
            <must condition="/y1731:y1731/y1731:global-enable/y1731:mds/y1731:md[y1731:name=current()/../md-name]/y1731:mas/y1731:ma[y1731:name=current()/../ma-name]/y1731:meps/y1731:mep[y1731:id=current()]"/>
            <mandatory value="true"/>
            <description>
              <text>Configure a MEP ID.</text>
            </description>
          </leaf>
          <choice name="remote">
            <description>
              <text>Configure the RMEP ID or MAC.</text>
            </description>
            <case name="rmep-id">
              <leaf name="rmep-id">
                <ext:operation-exclude value="create|update|delete"/>
                <type name="leafref">
                  <path value="/y1731/global-enable/mds/md/mas/ma/rmeps/rmep/id"/>
                </type>
                <must condition="/y1731:y1731/y1731:global-enable/y1731:mds/y1731:md[y1731:name=current()/../md-name]/y1731:mas/y1731:ma[y1731:name=current()/../ma-name]/y1731:rmeps/y1731:rmep[y1731:id=current()]"/>
                <description>
                  <text>Configure a RMEP ID.</text>
                </description>
              </leaf>
              <description>
                <text>Configure a RMEP ID.</text>
              </description>
            </case>
            <case name="remote-mac">
              <leaf name="rmep-mac">
                <ext:operation-exclude value="create|update|delete"/>
                <type name="pub-type:mac-address"/>
                <must condition="not(../../../y1731:mds/y1731:md[y1731:name=current()/../md-name]/y1731:mas/y1731:ma[y1731:name=current()/../ma-name]/y1731:meps/y1731:mep[y1731:id=current()/../mep-id]/vlan-mep)"/>
                <description>
                  <text>Configure the MAC of a RMEP, format: H-H-H.</text>
                </description>
              </leaf>
              <description>
                <text>Configure the MAC of a RMEP, format: H-H-H.</text>
              </description>
            </case>
          </choice>
          <leaf name="priority">
            <ext:operation-exclude value="update|delete"/>
            <type name="uint8">
              <range value="0..7"/>
            </type>
            <must condition="(../priority and not(../peer-ip) and not(../vc-id)) or not(../priority)"/>
            <description>
              <text>Configure the priority of packets.</text>
            </description>
          </leaf>
          <leaf name="peer-ip">
            <ext:operation-exclude value="update|delete"/>
            <type name="inet:ipv4-address-no-zone"/>
            <must condition="(../peer-ip and not(../priority)) or not(../peer-ip)"/>
            <description>
              <text>Configure the IP of peer end.</text>
            </description>
          </leaf>
          <leaf name="vc-id">
            <ext:operation-exclude value="update|delete"/>
            <type name="uint32">
              <range value="1..4294967295"/>
            </type>
            <must condition="((../vc-id and ../peer-ip and not(../priority)) or not(../vc-id))"/>
            <description>
              <text>Configure the ID of VC.</text>
            </description>
          </leaf>
          <leaf name="test-file-enable">
            <ext:operation-exclude value="update|delete"/>
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable test ID file.</text>
            </description>
          </leaf>
          <container name="one-way-delay-send">
            <presence value="Enable/disable one-way DM send."/>
            <description>
              <text>Configure an one-way delay measurement send instance.</text>
            </description>
            <leaf name="interval">
              <ext:operation-exclude value="update"/>
              <type name="interval"/>
              <mandatory value="true"/>
              <description>
                <text>Configure the interval of one-way delay measurement send.</text>
              </description>
            </leaf>
          </container>
          <container name="one-way-delay-recv">
            <presence value="Enable/disable one-way DM receive."/>
            <description>
              <text>Configure an one-way delay measurement receive instance.</text>
            </description>
            <leaf name="is-continue">
              <ext:operation-exclude value="update|delete"/>
              <type name="boolean"/>
              <default value="false"/>
              <description>
                <text>Enable/disable one-way delay continue measurement receive.</text>
              </description>
            </leaf>
          </container>
          <container name="two-way-delay-send">
            <presence value="Enable/disable two-way DM send."/>
            <description>
              <text>Configure a two-way delay measurement send instance.</text>
            </description>
            <leaf name="interval">
              <ext:operation-exclude value="update"/>
              <type name="interval"/>
              <mandatory value="true"/>
              <description>
                <text>Configure the interval of two-way delay measurement send.</text>
              </description>
            </leaf>
            <leaf name="period">
              <ext:operation-exclude value="update|delete"/>
              <type name="uint16">
                <range value="1..65535"/>
              </type>
              <units name="s"/>
              <default value="900"/>
              <description>
                <text>The value of measurement duration.</text>
              </description>
            </leaf>
          </container>
          <container name="two-way-delay-recv">
            <description>
              <text>Configure a two-way delay measurement receive instance.</text>
            </description>
            <leaf name="receive-enabled">
              <type name="boolean"/>
              <default value="false"/>
              <description>
                <text>Enable/disable two-way delay measurement receive instance.</text>
              </description>
            </leaf>
          </container>
          <container name="single-loss-send">
            <must condition="not(../dual-loss/enabled='true')"/>
            <presence value="Enable/disable single loss send."/>
            <description>
              <text>Configure a single-ended loss measurement send instance.</text>
            </description>
            <leaf name="interval">
              <ext:operation-exclude value="update"/>
              <type name="interval"/>
              <mandatory value="true"/>
              <description>
                <text>Configure the interval of single-ended loss measurement send.</text>
              </description>
            </leaf>
            <leaf name="period">
              <ext:operation-exclude value="update|delete"/>
              <type name="uint16">
                <range value="1..65535"/>
              </type>
              <units name="s"/>
              <default value="900"/>
              <description>
                <text>The value of measurement duration.</text>
              </description>
            </leaf>
          </container>
          <container name="single-loss-recv">
            <description>
              <text>Configure a single-ended loss measurement receive instance.</text>
            </description>
            <leaf name="enabled">
              <type name="boolean"/>
              <must condition="(not(../../dual-loss/enabled='true') or ((../../dual-loss/enabled='true') and (../enabled='false')))"/>
              <default value="false"/>
              <description>
                <text>Enable/disable single-ended loss measurement receive instance.</text>
              </description>
            </leaf>
          </container>
          <container name="dual-loss">
            <description>
              <text>Configure a dual-ended loss measurement instance.</text>
            </description>
            <leaf name="enabled">
              <type name="boolean"/>
              <must condition="((../enabled='false') or ((../enabled='true') and (not(../../single-loss-send/interval) and ../../single-loss-recv/enabled='false') and (/y1731:y1731/y1731:global-enable/y1731:mds/y1731:md[y1731:name=current()/../../md-name]/y1731:mas/y1731:ma[y1731:name=current()/../../ma-name]/y1731:ccm-interval='100ms' or /y1731:y1731/y1731:global-enable/y1731:mds/y1731:md[y1731:name=current()/../../md-name]/y1731:mas/y1731:ma[y1731:name=current()/../../ma-name]/y1731:ccm-interval='1s' or /y1731:y1731/y1731:global-enable/y1731:mds/y1731:md[y1731:name=current()/../../md-name]/y1731:mas/y1731:ma[y1731:name=current()/../../ma-name]/y1731:ccm-interval='10s')))"/>
              <default value="false"/>
              <description>
                <text>Enable/disable dual-ended loss measurement instance.</text>
              </description>
            </leaf>
          </container>
          <container name="synthetic-loss-send">
            <presence value="Enable/disable single synthetic LM send enable."/>
            <description>
              <text>Configure a single ended synthetic send instance.</text>
            </description>
            <leaf name="interval">
              <ext:operation-exclude value="update"/>
              <type name="sslm-interval"/>
              <mandatory value="true"/>
              <description>
                <text>Configuration interval of single ended synthetic send.</text>
              </description>
            </leaf>
            <leaf name="send-count">
              <ext:operation-exclude value="update|delete"/>
              <type name="uint16">
                <range value="1..1000"/>
              </type>
              <default value="10"/>
              <description>
                <text>Configuration count of single ended synthetic send.</text>
              </description>
            </leaf>
            <leaf name="timeout">
              <ext:operation-exclude value="update|delete"/>
              <type name="uint8">
                <range value="1..10"/>
              </type>
              <default value="5"/>
              <description>
                <text>Configuration time out value of single ended synthetic send.</text>
              </description>
            </leaf>
            <leaf name="period">
              <ext:operation-exclude value="update|delete"/>
              <type name="uint16">
                <range value="1..65535"/>
              </type>
              <default value="900"/>
              <description>
                <text>Configuration period of single ended synthetic loss measurement.</text>
              </description>
