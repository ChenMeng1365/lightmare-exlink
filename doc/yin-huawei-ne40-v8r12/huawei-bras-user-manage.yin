  <grouping name="option60-vendorclass-cfg">
    <description>
      <text>Configures the vendor-class (DHCPv4 option60/DHCPv6 option16).</text>
    </description>
    <leaf name="encrypt">
      <type name="boolean"/>
      <default value="false"/>
      <description>
        <text>Enable/disable the VENDOR-CLASS (DHCPv4 OPTION60/DHCPv6 OPTION16) field value.</text>
      </description>
    </leaf>
    <leaf name="partial-match">
      <type name="enumeration">
        <enum name="domain-included">
          <value value="0"/>
          <description>
            <text>Specifies the customized options of the vendor contains domain name.</text>
          </description>
        </enum>
        <enum name="included-in-domain">
          <value value="1"/>
          <description>
            <text>Specifies the customized options of the vendor contains a part of domain name.</text>
          </description>
        </enum>
      </type>
      <description>
        <text>Sets domainInclude or includeInDomain business.</text>
      </description>
    </leaf>
    <choice name="partial-info">
      <description>
        <text>Configures part of the vendor-class (DHCPv4 option60/DHCPv6 option16) attribute to identify domain names.</text>
      </description>
      <case name="cn">
        <description>
          <text>Indicates that the system uses the CN format to identify domain names.</text>
        </description>
        <leaf name="cn">
          <type name="empty"/>
          <description>
            <text>Indicates that the system uses the CN format to identify domain names.</text>
          </description>
        </leaf>
      </case>
      <case name="not-cn">
        <description>
          <text>Indicates that the system does not use the CN format to identify domain names.</text>
        </description>
        <leaf name="offset-value">
          <when condition="../length or ../sub-option-value"/>
          <type name="uint16">
            <range value="1..254"/>
          </type>
          <description>
            <text>Indicates that the system does not use the CN format to identify domain names.</text>
          </description>
        </leaf>
        <choice name="length-or-sub-option">
          <description>
            <text>Configures the field length or sub-option code of the vendor-class attribute to identify domain names.</text>
          </description>
          <case name="length">
            <description>
              <text>Configures the field length of the vendor-class attribute to identify domain names.</text>
            </description>
            <leaf name="length">
              <type name="uint16">
                <range value="1..254"/>
              </type>
              <description>
                <text>Configures the field length of the vendor-class attribute to identify domain names.</text>
              </description>
            </leaf>
          </case>
          <case name="sub-option">
            <description>
              <text>Configures the sub-option code of the vendor-class attribute to identify domain names.</text>
            </description>
            <leaf name="sub-option-value">
              <type name="uint16">
                <range value="1..255"/>
              </type>
              <description>
                <text>Configures the sub-option code of the vendor-class attribute to identify domain names..</text>
              </description>
            </leaf>
            <leaf name="sub-offset-value">
              <when condition="../sub-option-value"/>
              <type name="uint16">
                <range value="1..254"/>
              </type>
              <description>
                <text>Configures the offset value of a vendor-class sub-option to identify domain names.</text>
              </description>
            </leaf>
            <leaf name="sub-length-value">
              <when condition="../sub-option-value"/>
              <type name="uint16">
                <range value="1..254"/>
              </type>
              <description>
                <text>Configures the field length of a vendor-class sub-option to identify domain names.</text>
              </description>
            </leaf>
          </case>
        </choice>
      </case>
    </choice>
  </grouping>
