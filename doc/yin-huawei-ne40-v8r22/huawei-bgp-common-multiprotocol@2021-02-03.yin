  <grouping name="ipv4-hierarchy-convergence">
    <description>
      <text>Configure unicast route hierarchy convergence.</text>
    </description>
    <container name="common">
      <presence value="Configure hierarchy convergence."/>
      <description>
        <text>Configure hierarchy convergence parameters.</text>
      </description>
      <leaf name="enable">
        <type name="empty"/>
        <mandatory value="true"/>
        <description>
          <text>Enable the hierarchy convergence.</text>
        </description>
      </leaf>
    </container>
    <container name="hierarchy-convergence-routes">
      <description>
        <text>List of hierarchy convergence routes.</text>
      </description>
      <list name="hierarchy-convergence-route">
        <key value="base-route-address base-route-mask-length"/>
        <max-elements value="1"/>
        <description>
          <text>Configure the hierarchy convergence route.</text>
        </description>
        <leaf name="base-route-address">
          <type name="inet:ipv4-address-no-zone"/>
          <description>
            <text>Set the base ip address. Base ip address will be higher convergence when processed.</text>
          </description>
        </leaf>
        <leaf name="base-route-mask-length">
          <type name="uint8">
            <range value="1..32"/>
          </type>
          <description>
            <text>Specify the mask length of a summarized address.</text>
          </description>
        </leaf>
        <choice name="hierarchy-route">
          <mandatory value="true"/>
          <description>
            <text>Set hierarchy route.</text>
          </description>
          <case name="all">
            <description>
              <text>All route without hierarchy attribute will be set to current base route.</text>
            </description>
            <leaf name="all-route">
              <type name="empty"/>
              <description>
                <text>All routes.</text>
              </description>
            </leaf>
          </case>
          <case name="route-policy">
            <description>
              <text>The route without hierarchy attribute and match the policy will be set to current base route.</text>
            </description>
            <leaf name="route-policy">
              <type name="leafref">
                <path value="/rtp:routing-policy/rtp:policy-definitions/rtp:policy-definition/rtp:name"/>
              </type>
              <description>
                <text>Specify an route policy.</text>
              </description>
            </leaf>
          </case>
        </choice>
      </list>
    </container>
  </grouping>
