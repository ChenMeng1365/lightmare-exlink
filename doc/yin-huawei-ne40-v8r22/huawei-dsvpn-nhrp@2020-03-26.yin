<?xml version="1.0" encoding="UTF-8"?>
<module name="huawei-dsvpn-nhrp"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:dsvpn-nhrp="urn:huawei:yang:huawei-dsvpn-nhrp"
        xmlns:ext="urn:huawei:yang:huawei-extension">
  <namespace uri="urn:huawei:yang:huawei-dsvpn-nhrp"/>
  <prefix value="dsvpn-nhrp"/>
  <import module="huawei-extension">
    <prefix value="ext"/>
  </import>
  <organization>
    <text>Huawei Technologies Co., Ltd.</text>
  </organization>
  <contact>
    <text>Huawei Industrial Base
Bantian, Longgang
Shenzhen 518129
People's Republic of China
Website: http://www.huawei.com
Email: support@huawei.com</text>
  </contact>
  <description>
    <text>Dynamic Smart Virtual Private Network (DSVPN) refers to a technology for dynamically
establishing VPN between a branch and a headquarters, a branch, and a branch in the
Hub-Spoke network model.
The Next Hop Resolution Protocol (NHRP) is an extension of the ATM ARP routing mechanism
that is sometimes used to improve the efficiency of routing computer network traffic over
Non-Broadcast, Multiple Access (NBMA) Networks.</text>
  </description>
  <revision date="2020-03-26">
    <description>
      <text>White-box errors are resolved.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <revision date="2019-12-10">
    <description>
      <text>Initial revision.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <typedef name="entry-type">
    <type name="enumeration">
      <enum name="hub">
        <value value="0"/>
        <description>
          <text>Hub.</text>
        </description>
      </enum>
      <enum name="static">
        <value value="1"/>
        <description>
          <text>Static.</text>
        </description>
      </enum>
      <enum name="local">
        <value value="2"/>
        <description>
          <text>Local.</text>
        </description>
      </enum>
      <enum name="registered">
        <value value="3"/>
        <description>
          <text>Registered or spoke.</text>
        </description>
      </enum>
      <enum name="remote">
        <value value="4"/>
        <description>
          <text>Remote.</text>
        </description>
      </enum>
      <enum name="remote-network">
        <value value="5"/>
        <description>
          <text>Remote-network.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>NHRP peer type.</text>
    </description>
  </typedef>
  <typedef name="entry-flag">
    <type name="enumeration">
      <enum name="up">
        <value value="0"/>
        <description>
          <text>Up.</text>
        </description>
      </enum>
      <enum name="down">
        <value value="1"/>
        <description>
          <text>Down.</text>
        </description>
      </enum>
      <enum name="up.unique">
        <value value="2"/>
        <description>
          <text>Up|unique.</text>
        </description>
      </enum>
      <enum name="up.no-unique">
        <value value="3"/>
        <description>
          <text>Up|no-unique.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>NHRP peer flag.</text>
    </description>
  </typedef>
  <typedef name="scenario-type">
    <type name="enumeration">
      <enum name="hub">
        <value value="0"/>
        <description>
          <text>Hub.</text>
        </description>
      </enum>
      <enum name="spoke">
        <value value="1"/>
        <description>
          <text>Spoke.</text>
        </description>
      </enum>
      <enum name="hub-spoke">
        <value value="2"/>
        <description>
          <text>Hub-spoke.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Scenario type.</text>
    </description>
  </typedef>
  <typedef name="authen-mode">
    <type name="enumeration">
      <enum name="sha2-256">
        <value value="2"/>
        <description>
          <text>SHA2-256.</text>
        </description>
      </enum>
      <enum name="sha2-384">
        <value value="3"/>
        <description>
          <text>SHA2-384.</text>
        </description>
      </enum>
      <enum name="sha2-512">
        <value value="4"/>
        <description>
          <text>SHA2-512.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Authentication algorithm.</text>
    </description>
  </typedef>
  <ext:task-name value="nhrp"/>
  <container name="dsvpn-nhrp">
    <description>
      <text>Dynamic Smart Virtual Private Network, Next Hop Resolution Protocol.</text>
    </description>
    <container name="global">
      <description>
        <text>Configure NHRP on DSVPN.</text>
      </description>
      <leaf name="enabled">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable global NHRP on DSVPN.</text>
        </description>
      </leaf>
    </container>
  </container>
</module>
