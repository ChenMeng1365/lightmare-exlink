<?xml version="1.0" encoding="UTF-8"?>
<module name="huawei-pbr-notification"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:pbr-notification="urn:huawei:yang:huawei-pbr-notification"
        xmlns:ext="urn:huawei:yang:huawei-extension">
  <yang-version value="1"/>
  <namespace uri="urn:huawei:yang:huawei-pbr-notification"/>
  <prefix value="pbr-notification"/>
  <import module="huawei-extension">
    <prefix value="ext"/>
  </import>
  <organization>
    <text>Huawei Technologies Co., Ltd.</text>
  </organization>
  <contact>
    <text>Huawei Industrial Base
Bantian, Longgang
Shenzhen 518129
People's Republic of China
Website: http://www.huawei.com
Email: support@huawei.com</text>
  </contact>
  <description>
    <text>Notification of pbr.</text>
  </description>
  <revision date="2020-06-12">
    <description>
      <text>Add new nodes.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <revision date="2019-11-06">
    <description>
      <text>Initial revision.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <ext:task-name value="qos"/>
  <typedef name="redirect-type">
    <type name="enumeration">
      <enum name="ipv4">
        <value value="0"/>
        <description>
          <text>IPv4.</text>
        </description>
      </enum>
      <enum name="ipv6">
        <value value="1"/>
        <description>
          <text>IPv6.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Redirect type.</text>
    </description>
  </typedef>
  <notification name="xqos-redirect-nexthop-invalid-alarm">
    <description>
      <text>The redirect nexthop address did not match the network segment of the outbound interface.</text>
    </description>
    <leaf name="behavior-name">
      <type name="string">
        <length value="1..127"/>
      </type>
      <description>
        <text>Behavior name.</text>
      </description>
    </leaf>
    <leaf name="redirect">
      <type name="redirect-type"/>
      <description>
        <text>Redirect type.</text>
      </description>
    </leaf>
    <leaf name="interface-name">
      <type name="string">
        <length value="1..511"/>
      </type>
      <description>
        <text>Interface name.</text>
      </description>
    </leaf>
  </notification>
  <notification name="xqos-redirect-nexthop-invalid-alarm-resume">
    <description>
      <text>The redirect nexthop address matches the network segment of the outbound interface or the redirect configuration has been cleared.</text>
    </description>
    <leaf name="behavior-name">
      <type name="string">
        <length value="1..127"/>
      </type>
      <description>
        <text>Behavior name.</text>
      </description>
    </leaf>
    <leaf name="redirect">
      <type name="redirect-type"/>
      <description>
        <text>Redirect type.</text>
      </description>
    </leaf>
    <leaf name="interface-name">
      <type name="string">
        <length value="1..511"/>
      </type>
      <description>
        <text>Interface name.</text>
      </description>
    </leaf>
  </notification>
  <notification name="xqos-redirect-nexthop-lack-resource-alarm">
    <description>
      <text>Redirecting traffic to the next hop is invalid due to insufficient NHP resources.</text>
    </description>
    <leaf name="behavior-name">
      <type name="string">
        <length value="1..127"/>
      </type>
      <description>
        <text>Behavior name.</text>
      </description>
    </leaf>
    <leaf name="slot-id">
      <type name="string">
        <length value="1..16"/>
      </type>
      <description>
        <text>Slot ID.</text>
      </description>
    </leaf>
    <leaf name="redirect-type">
      <type name="redirect-type"/>
      <description>
        <text>Redirect type.</text>
      </description>
    </leaf>
  </notification>
  <notification name="xqos-redirect-nexthop-lack-resource-alarm-resume">
    <description>
      <text>Resources are sufficient or the configuration of redirecting traffic to the next hop is deleted.</text>
    </description>
    <leaf name="behavior-name">
      <type name="string">
        <length value="1..127"/>
      </type>
      <description>
        <text>Behavior name.</text>
      </description>
    </leaf>
    <leaf name="slot-id">
      <type name="string">
        <length value="1..16"/>
      </type>
      <description>
        <text>Slot ID.</text>
      </description>
    </leaf>
    <leaf name="redirect-type">
      <type name="redirect-type"/>
      <description>
        <text>Redirect type.</text>
      </description>
    </leaf>
  </notification>
  <notification name="xqos-interface-down-redirect-next-hop-invalid-alarm">
    <description>
      <text>Alarm when the interface goes down and the redirection next hop becomes invalid.</text>
    </description>
    <leaf name="rdr-type">
      <type name="redirect-type"/>
      <description>
        <text>Redirect type.</text>
      </description>
    </leaf>
    <leaf name="slot-id">
      <type name="string">
        <length value="1..128"/>
      </type>
      <description>
        <text>Slot ID.</text>
      </description>
    </leaf>
    <leaf name="interface-name">
      <type name="string">
        <length value="1..511"/>
      </type>
      <description>
        <text>Interface name.</text>
      </description>
    </leaf>
    <leaf name="behavior-name">
      <type name="string">
        <length value="1..128"/>
      </type>
      <description>
        <text>Behavior name.</text>
      </description>
    </leaf>
  </notification>
  <notification name="xqos-interface-down-redirect-next-hop-invalid-alarm-clear">
    <description>
      <text>Alarm clear when the interface goes up or the redirection next hop configuration is deleted.</text>
    </description>
    <leaf name="rdr-type">
      <type name="redirect-type"/>
      <description>
        <text>Redirect type.</text>
      </description>
    </leaf>
    <leaf name="slot-id">
      <type name="string">
        <length value="1..128"/>
      </type>
      <description>
        <text>Slot ID.</text>
      </description>
    </leaf>
    <leaf name="interface-name">
      <type name="string">
        <length value="1..511"/>
      </type>
      <description>
        <text>Interface name.</text>
      </description>
    </leaf>
  </notification>
  <notification name="xqos-redirect-next-hop-event-log">
    <description>
      <text>Print log when route switching in scenarios where a redirection policy with no outbound interface specified is deployed.</text>
    </description>
    <leaf name="slot-id">
      <type name="string">
        <length value="1..128"/>
      </type>
      <description>
        <text>Slot ID.</text>
      </description>
    </leaf>
    <leaf name="rdr-type">
      <type name="redirect-type"/>
      <description>
        <text>Redirect type.</text>
      </description>
    </leaf>
    <leaf name="ip-address">
      <type name="string">
        <length value="1..128"/>
      </type>
      <description>
        <text>Ip address.</text>
      </description>
    </leaf>
    <leaf name="behavior-name">
      <type name="string">
        <length value="1..128"/>
      </type>
      <description>
        <text>Behavior name.</text>
      </description>
    </leaf>
  </notification>
</module>
