<?xml version="1.0" encoding="UTF-8"?>
<module name="huawei-clocksync-cluster"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:clocksync-cluster="urn:huawei:yang:huawei-clocksync-cluster"
        xmlns:ext="urn:huawei:yang:huawei-extension"
        xmlns:ifm="urn:huawei:yang:huawei-ifm">
  <namespace uri="urn:huawei:yang:huawei-clocksync-cluster"/>
  <prefix value="clocksync-cluster"/>
  <import module="huawei-extension">
    <prefix value="ext"/>
  </import>
  <import module="huawei-ifm">
    <prefix value="ifm"/>
  </import>
  <organization>
    <text>Huawei Technologies Co., Ltd.</text>
  </organization>
  <contact>
    <text>Huawei Industrial Base
Bantian, Longgang
Shenzhen 518129
People's Republic of China
Website: http://www.huawei.com
Email: support@huawei.com</text>
  </contact>
  <description>
    <text>Clock cluster management data model define.</text>
  </description>
  <revision date="2020-03-14">
    <description>
      <text>Modify the node description.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <revision date="2020-03-04">
    <description>
      <text>Modify the node description.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <revision date="2019-12-25">
    <description>
      <text>Fixed some problems.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <revision date="2019-05-06">
    <description>
      <text>Initial revision.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <typedef name="cm-pull-in-state">
    <type name="enumeration">
      <enum name="pull-in">
        <value value="1"/>
        <description>
          <text>Pull-in.</text>
        </description>
      </enum>
      <enum name="pull-out">
        <value value="0"/>
        <description>
          <text>Pull-out.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Cluster management pull in state type.</text>
    </description>
  </typedef>
  <typedef name="cm-src-state">
    <type name="enumeration">
      <enum name="normal">
        <value value="1"/>
        <description>
          <text>Normal.</text>
        </description>
      </enum>
      <enum name="abnormal">
        <value value="0"/>
        <description>
          <text>Abnormal.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Cluster management source state type.</text>
    </description>
  </typedef>
  <typedef name="clock-trace-status">
    <type name="enumeration">
      <enum name="freerun">
        <value value="0"/>
        <description>
          <text>Freerun.</text>
        </description>
      </enum>
      <enum name="fastlock">
        <value value="1"/>
        <description>
          <text>Fastlock.</text>
        </description>
      </enum>
      <enum name="trace">
        <value value="2"/>
        <description>
          <text>Trace.</text>
        </description>
      </enum>
      <enum name="hold">
        <value value="3"/>
        <description>
          <text>Hold.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Clock trace status type.</text>
    </description>
  </typedef>
  <ext:task-name value="clksync"/>
  <container name="clocksync-cluster">
    <description>
      <text>Clock cluster management data model.</text>
    </description>
    <container name="inter-links">
      <description>
        <text>List of configure internal communication interface information. Including the source names and internal communication interface.</text>
      </description>
      <list name="inter-link">
        <key value="src-name"/>
        <description>
          <text>Configure internal communication interface information.</text>
        </description>
        <ext:generated-by value="system"/>
        <ext:operation-exclude value="create|delete"/>
        <leaf name="src-name">
          <type name="string">
            <length value="1..50"/>
          </type>
          <description>
            <text>Name of an internal communication interface.</text>
          </description>
        </leaf>
        <leaf name="switch">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable or disable internal communication.</text>
          </description>
        </leaf>
      </list>
    </container>
    <container name="freq-trace-topos">
      <config value="false"/>
      <description>
        <text>List of statistics of information about the frequency syncing topology in the cluster. Including the chassis ID, frequency locking status, syncing source name, syncing source status, and pull-in status.</text>
      </description>
      <list name="freq-trace-topo">
        <key value="chassis-id"/>
        <config value="false"/>
        <description>
          <text>Statistics of information about the frequency syncing topology in the cluster.</text>
        </description>
        <leaf name="chassis-id">
          <type name="string">
            <length value="1..20"/>
          </type>
          <config value="false"/>
          <description>
            <text>Chassis ID of the frequency syncing topology.</text>
          </description>
        </leaf>
        <leaf name="trace-state">
          <type name="clock-trace-status"/>
          <config value="false"/>
          <description>
            <text>Sync status of the frequency topology.</text>
          </description>
        </leaf>
        <leaf name="pullin-state">
          <type name="cm-pull-in-state"/>
          <config value="false"/>
          <description>
            <text>Pull-in status of the frequency syncing topology.</text>
          </description>
        </leaf>
        <leaf name="trace-source">
          <type name="string">
            <length value="1..50"/>
          </type>
          <config value="false"/>
          <description>
            <text>Syncing source name of the frequency topology.</text>
          </description>
        </leaf>
        <leaf name="lock-state">
          <type name="boolean"/>
          <default value="false"/>
          <config value="false"/>
          <description>
            <text>Frequency locking status of the cluster chassis.</text>
          </description>
        </leaf>
      </list>
    </container>
    <container name="time-trace-topos">
      <config value="false"/>
      <description>
        <text>List of statistics of cluster time syncing topology information. Including the chassis ID, time locking status, and PTP device clock IDs.</text>
      </description>
      <list name="time-trace-topo">
        <key value="chassis-id"/>
        <config value="false"/>
        <description>
          <text>Statistics of cluster time syncing topology information.</text>
        </description>
        <leaf name="chassis-id">
          <type name="string">
            <length value="1..20"/>
          </type>
          <config value="false"/>
          <description>
            <text>Chassis ID of time syncing topology.</text>
          </description>
        </leaf>
        <leaf name="trace-source">
          <type name="string">
            <length value="1..50"/>
          </type>
          <config value="false"/>
          <description>
            <text>Source name of the time syncing topology.</text>
          </description>
        </leaf>
        <leaf name="lock-state">
          <type name="boolean"/>
          <default value="false"/>
          <config value="false"/>
          <description>
            <text>Time locking status of time syncing topology.</text>
          </description>
        </leaf>
        <leaf name="ptp-clock-id">
          <type name="string">
            <length value="16"/>
          </type>
          <config value="false"/>
          <description>
            <text>PTP device clock ID of cluster time syncing topology.</text>
          </description>
        </leaf>
      </list>
    </container>
    <container name="freq-link-ports">
      <config value="false"/>
      <description>
        <text>List of statistics of internal communication interface used to perform frequency synchronization between chassis in the cluster.</text>
      </description>
      <list name="freq-link-port">
        <key value="src-name"/>
        <config value="false"/>
        <description>
          <text>Statistics of internal communication interface used to perform frequency synchronization between chassis in the cluster.</text>
        </description>
        <leaf name="src-name">
          <type name="string">
            <length value="1..50"/>
          </type>
          <config value="false"/>
          <description>
            <text>Clock source name of the cluster chassis.</text>
          </description>
        </leaf>
        <leaf name="state">
          <type name="cm-src-state"/>
          <config value="false"/>
          <description>
            <text>Clock source status of the cluster chassis.</text>
          </description>
        </leaf>
        <leaf name="peer-src-name">
          <type name="string">
            <length value="1..50"/>
          </type>
          <config value="false"/>
          <description>
            <text>Name of the peer interface in the cluster chassis.</text>
          </description>
        </leaf>
      </list>
    </container>
    <container name="time-link-ports">
      <config value="false"/>
      <description>
        <text>List of statistics of internal communication interface of the time syncing topology.</text>
      </description>
      <list name="time-link-port">
        <key value="src-name"/>
        <config value="false"/>
        <description>
          <text>Statistics of internal communication interface of the time syncing topology.</text>
        </description>
        <leaf name="src-name">
          <type name="string">
            <length value="1..50"/>
          </type>
          <config value="false"/>
          <description>
            <text>Source name of the time syncing topology.</text>
          </description>
        </leaf>
        <leaf name="state">
          <type name="cm-src-state"/>
          <config value="false"/>
          <description>
            <text>Source status of the time syncing topology.</text>
          </description>
        </leaf>
        <leaf name="peer-src-name">
          <type name="string">
            <length value="1..50"/>
          </type>
          <config value="false"/>
          <description>
            <text>Adjacent interface name of the time syncing topology.</text>
          </description>
        </leaf>
      </list>
    </container>
  </container>
  <augment target-node="/ifm:ifm/ifm:interfaces/ifm:interface">
    <description>
      <text>Interface.</text>
    </description>
    <container name="vinter-links">
      <description>
        <text>Configure internal communication interface in a virtual cluster.</text>
      </description>
      <leaf name="src-name">
        <type name="leafref">
          <path value="/ifm:ifm/ifm:interfaces/ifm:interface/ifm:name"/>
        </type>
        <must condition="/ifm:ifm/ifm:interfaces/ifm:interface[ifm:name=current()]/ifm:type='GigabitEthernet' or /ifm:ifm/ifm:interfaces/ifm:interface[ifm:name=current()]/ifm:type='10GE' or /ifm:ifm/ifm:interfaces/ifm:interface[ifm:name=current()]/ifm:type='40GE' or /ifm:ifm/ifm:interfaces/ifm:interface[ifm:name=current()]/ifm:type='100GE'"/>
        <description>
          <text>Name of an internal communication interface.</text>
        </description>
      </leaf>
    </container>
  </augment>
</module>
