           xmlns:ni="urn:huawei:yang:huawei-network-instance"
           xmlns:ifm="urn:huawei:yang:huawei-ifm"
           xmlns:yang="urn:ietf:params:xml:ns:yang:ietf-yang-types"
           xmlns:inet="urn:ietf:params:xml:ns:yang:ietf-inet-types"
           xmlns:ext="urn:huawei:yang:huawei-extension">
  <yang-version value="1"/>
  <belongs-to module="huawei-diagnostic-tools">
    <prefix value="diagnostic-tools"/>
  </belongs-to>
  <import module="huawei-pub-type">
    <prefix value="pub-type"/>
  </import>
  <import module="huawei-network-instance">
    <prefix value="ni"/>
  </import>
  <import module="huawei-ifm">
    <prefix value="ifm"/>
  </import>
  <import module="ietf-yang-types">
    <prefix value="yang"/>
  </import>
  <import module="ietf-inet-types">
    <prefix value="inet"/>
  </import>
  <import module="huawei-extension">
    <prefix value="ext"/>
  </import>
  <include module="huawei-diagnostic-tools-common"/>
  <include module="huawei-diagnostic-tools-type"/>
  <organization>
    <text>Huawei Technologies Co., Ltd.</text>
  </organization>
  <contact>
    <text>Huawei Industrial Base
Bantian, Longgang
Shenzhen 518129
People's Republic of China
Website: http://www.huawei.com
Email: support@huawei.com</text>
  </contact>
  <description>
    <text>IPv6 diagnosis tool.</text>
  </description>
  <revision date="2020-09-10">
    <description>
      <text>Add fast reply inbound node.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <revision date="2019-04-09">
    <description>
      <text>Initial revision.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <grouping name="ip6-ping-trace-result-type">
    <description>
      <text>IPv6 ping trace test result.</text>
    </description>
    <container name="ping-results">
      <config value="false"/>
      <description>
        <text>List of test result.</text>
      </description>
      <list name="ping-result">
        <key value="test-name"/>
        <config value="false"/>
        <description>
          <text>Statistics of test result of a certain hop in the test path. For a ping-type test, there is only one test result.</text>
        </description>
        <leaf name="test-name">
          <type name="string">
            <length value="1..32"/>
          </type>
          <config value="false"/>
          <description>
            <text>Test name.</text>
          </description>
        </leaf>
        <leaf name="packet-recv">
          <type name="uint32">
            <range value="0..4294967295"/>
          </type>
          <config value="false"/>
          <description>
            <text>Total number of received packets.</text>
          </description>
        </leaf>
        <leaf name="packet-send">
          <type name="uint32">
            <range value="0..4294967295"/>
          </type>
          <config value="false"/>
          <description>
            <text>Total number of sent packets.</text>
          </description>
        </leaf>
        <leaf name="loss-ratio">
          <type name="pub-type:percent">
            <range value="0..100"/>
          </type>
          <units name="%"/>
          <config value="false"/>
          <description>
            <text>Packet loss ratio of a test.</text>
          </description>
        </leaf>
        <leaf name="rtt-min">
          <type name="uint32">
            <range value="0..4294967295"/>
          </type>
          <units name="ms"/>
          <config value="false"/>
          <description>
            <text>Minimum RTT of all test packets.</text>
          </description>
        </leaf>
        <leaf name="rtt-max">
          <type name="uint32">
            <range value="0..4294967295"/>
          </type>
          <units name="ms"/>
          <config value="false"/>
          <description>
            <text>Maximum RTT of all test packets.</text>
          </description>
        </leaf>
        <leaf name="average-rtt">
          <type name="uint32">
            <range value="0..4294967295"/>
          </type>
          <units name="ms"/>
          <config value="false"/>
          <description>
            <text>Average RTT of all test packets.</text>
          </description>
        </leaf>
        <leaf name="status">
          <type name="dgntl-test-status-type"/>
          <config value="false"/>
          <description>
            <text>Test status.</text>
          </description>
        </leaf>
        <leaf name="error-type">
          <type name="dgntl-error-type"/>
          <config value="false"/>
          <status value="deprecated"/>
          <description>
            <text>Test operation status.</text>
          </description>
        </leaf>
        <container name="ping-result-details">
          <config value="false"/>
          <description>
            <text>List of ping results of all packets in a test.</text>
          </description>
          <list name="ping-result-detail">
            <key value="index"/>
            <config value="false"/>
            <description>
              <text>Statistics of probe result.</text>
            </description>
            <leaf name="index">
              <type name="uint32">
                <range value="0..4294967295"/>
              </type>
              <config value="false"/>
              <description>
                <text>Serial number of a test packet received by a certain hop.</text>
              </description>
            </leaf>
            <leaf name="dest-ip-addr">
              <type name="inet:ipv6-address-no-zone"/>
              <config value="false"/>
              <description>
                <text>Destination address of the related hop in a test.</text>
              </description>
            </leaf>
            <leaf name="packet-size">
              <type name="uint32">
                <range value="0..4294967295"/>
              </type>
              <config value="false"/>
              <description>
                <text>Number of bytes in a received response packet.</text>
              </description>
            </leaf>
            <leaf name="ttl">
              <type name="uint8">
                <range value="1..255"/>
              </type>
              <config value="false"/>
              <description>
                <text>Hop limit of received response packets.</text>
              </description>
            </leaf>
            <leaf name="rtt">
              <type name="uint32">
                <range value="0..4294967295"/>
              </type>
              <units name="ms"/>
              <config value="false"/>
              <description>
                <text>RTT of test packets.</text>
              </description>
            </leaf>
            <leaf name="result-type">
              <type name="detail-result-type"/>
              <config value="false"/>
              <description>
                <text>Result of packet sending, which is either success or timeout.</text>
              </description>
            </leaf>
            <leaf name="is-delete">
              <ext:support-filter value="true"/>
              <type name="boolean"/>
              <config value="false"/>
              <description>
                <text>Enable/disable the function of deleting test results.</text>
              </description>
            </leaf>
            <leaf name="system-time">
              <type name="yang:date-and-time"/>
              <config value="false"/>
              <description>
                <text>System time of response packets.</text>
              </description>
            </leaf>
            <leaf name="incoming-if-name">
              <type name="string">
                <length value="1..255"/>
              </type>
              <config value="false"/>
              <description>
                <text>The logical source interface of echo response packet.</text>
              </description>
            </leaf>
            <leaf name="incoming-phy-if-name">
              <type name="string">
                <length value="1..255"/>
              </type>
              <config value="false"/>
              <description>
                <text>The physical source interface of echo response packet.</text>
              </description>
            </leaf>
          </list>
        </container>
      </list>
    </container>
    <container name="trace-results">
      <config value="false"/>
      <description>
        <text>List of test result.</text>
      </description>
      <list name="trace-result">
        <key value="test-name"/>
        <config value="false"/>
        <description>
          <text>Statistics of trace test result.</text>
        </description>
        <leaf name="test-name">
          <type name="string">
            <length value="1..32"/>
          </type>
          <config value="false"/>
          <description>
            <text>Test name.</text>
          </description>
        </leaf>
        <leaf name="status">
          <type name="dgntl-test-status-type"/>
          <config value="false"/>
          <description>
            <text>Test status.</text>
          </description>
        </leaf>
        <leaf name="error-type">
          <type name="dgntl-error-type"/>
          <config value="false"/>
          <status value="deprecated"/>
          <description>
            <text>Test operation status.</text>
          </description>
        </leaf>
        <container name="trace-result-details">
          <config value="false"/>
          <status value="deprecated"/>
          <description>
            <text>List of ping results of all packets in a test.</text>
          </description>
          <list name="trace-result-detail">
            <key value="index"/>
            <config value="false"/>
            <status value="deprecated"/>
            <description>
              <text>Statistics of ping result of a single packet in a test.</text>
            </description>
            <leaf name="index">
              <type name="uint32"/>
              <config value="false"/>
              <status value="deprecated"/>
              <description>
                <text>Serial number of a hop in the test path. For a ping-type test such as the ping test or LSP ping test, there is only one hop in the test path, so the value is 1. For a trace-type test such as the trace test or LSP trace test, the value indicates the exact serial number of a hop in the test path.</text>
              </description>
            </leaf>
            <leaf name="ttl">
              <type name="uint8">
                <range value="1..255"/>
              </type>
              <config value="false"/>
              <status value="deprecated"/>
              <description>
                <text>TTL of received response packets.</text>
              </description>
            </leaf>
            <leaf name="rtt">
              <type name="uint32"/>
              <config value="false"/>
              <status value="deprecated"/>
              <description>
                <text>RTT of test packets.</text>
              </description>
            </leaf>
            <leaf name="dest-ip-addr">
              <type name="inet:ipv6-address-no-zone"/>
              <config value="false"/>
              <status value="deprecated"/>
              <description>
                <text>Destination address of the related hop in a test.</text>
              </description>
            </leaf>
            <leaf name="is-delete">
              <ext:support-filter value="true"/>
              <type name="boolean"/>
              <config value="false"/>
              <status value="deprecated"/>
              <description>
                <text>Enable/disable the function of deleting test results.</text>
              </description>
            </leaf>
          </list>
        </container>
        <container name="hops">
          <config value="false"/>
          <description>
            <text>List of hop results.</text>
          </description>
          <list name="hop">
            <key value="hop-index"/>
            <config value="false"/>
            <description>
              <text>Statistics of hop result.</text>
            </description>
            <leaf name="hop-index">
              <type name="uint32"/>
              <config value="false"/>
              <description>
                <text>The index of the hop.</text>
              </description>
            </leaf>
            <leaf name="is-delete">
              <ext:support-filter value="true"/>
              <type name="boolean"/>
              <config value="false"/>
              <description>
                <text>Enable/disable the function of deleting test results.</text>
              </description>
            </leaf>
            <container name="probes">
              <config value="false"/>
              <description>
                <text>List of probe results.</text>
              </description>
              <list name="probe">
                <key value="probe-index"/>
                <config value="false"/>
                <description>
                  <text>Statistics of probe result.</text>
                </description>
                <leaf name="probe-index">
                  <type name="uint32"/>
                  <config value="false"/>
                  <description>
                    <text>The index of the probe packet.</text>
                  </description>
                </leaf>
                <leaf name="rtt">
                  <type name="uint32"/>
                  <config value="false"/>
                  <description>
                    <text>RTT of test packets, expressed in milliseconds.</text>
                  </description>
                </leaf>
                <leaf name="replier">
                  <type name="inet:ipv6-address-no-zone"/>
                  <config value="false"/>
                  <description>
                    <text>Reply address of the related hop in a test.</text>
                  </description>
                </leaf>
                <leaf name="result-type">
                  <type name="probe-result-type"/>
                  <config value="false"/>
                  <description>
                    <text>Result of packet sending, which is either success or timeout.</text>
                  </description>
                </leaf>
                <leaf name="srh-segment-left">
                  <type name="uint32"/>
                  <config value="false"/>
                  <description>
                    <text>Segments left.</text>
                  </description>
                </leaf>
                <container name="mpls-labels">
                  <config value="false"/>
                  <description>
                    <text>List of MPLS label informations.</text>
                  </description>
                  <list name="mpls-label">
                    <key value="label-index"/>
                    <config value="false"/>
                    <description>
                      <text>Statistics of MPLS label information.</text>
                    </description>
                    <leaf name="label-index">
                      <type name="uint32"/>
                      <config value="false"/>
                      <description>
                        <text>Label index.</text>
                      </description>
                    </leaf>
                    <leaf name="label-value">
                      <type name="uint32"/>
                      <config value="false"/>
                      <description>
                        <text>Show MPLS label value.</text>
                      </description>
                    </leaf>
                    <leaf name="label-exp">
                      <type name="uint32"/>
                      <config value="false"/>
                      <description>
                        <text>Show MPLS label exp.</text>
                      </description>
                    </leaf>
                    <leaf name="label-stack-bottom">
                      <type name="uint32"/>
                      <config value="false"/>
                      <description>
                        <text>Show MPLS label stack bottom flag.</text>
                      </description>
                    </leaf>
                    <leaf name="label-ttl">
                      <type name="uint32"/>
                      <config value="false"/>
                      <description>
                        <text>Show MPLS label TTL.</text>
                      </description>
                    </leaf>
                  </list>
                </container>
                <container name="srh-sids">
                  <config value="false"/>
                  <description>
                    <text>List of SID informations.</text>
                  </description>
                  <list name="srh-sid">
                    <key value="sid-index"/>
                    <config value="false"/>
                    <description>
                      <text>Statistics of SID information.</text>
                    </description>
                    <leaf name="sid-index">
                      <type name="uint32"/>
                      <config value="false"/>
                      <description>
                        <text>SID index.</text>
                      </description>
                    </leaf>
                    <leaf name="sid">
                      <type name="inet:ipv6-address-no-zone"/>
                      <config value="false"/>
                      <description>
                        <text>Segment ID.</text>
                      </description>
                    </leaf>
                  </list>
                </container>
                <leaf name="slice-id">
                  <type name="uint32"/>
                  <config value="false"/>
                  <description>
                    <text>Network slice ID.</text>
                  </description>
                </leaf>
              </list>
            </container>
          </list>
        </container>
      </list>
    </container>
  </grouping>
  <rpc name="ipv6-start-ping">
    <description>
      <text>Start IPv6 ping.</text>
    </description>
    <ext:node-ref value="/diagnostic-tools:diagnostic-tools/diagnostic-tools:ipv6/diagnostic-tools:ping-results"/>
    <input>
      <leaf name="test-name">
        <type name="string">
          <length value="1..32"/>
        </type>
        <mandatory value="true"/>
        <description>
          <text>Test name.</text>
        </description>
      </leaf>
      <leaf name="dest-addr">
        <type name="pub-type:host-name"/>
        <description>
          <text>Destination address of the string type. You can click this node to set different types of addresses and deploy the configurations together with the configuration of the destination address type node. If the destination address type is set to IPv4, this parameter is set to IPv4. If the destination address type is set to IPv6, this parameter is set to IPv6.</text>
        </description>
      </leaf>
      <leaf name="source-address">
        <type name="inet:ipv6-address-no-zone"/>
        <description>
          <text>Source address of the string type. You can click this node to set different types of addresses and deploy the configurations together with the configuration of the source address type node. If the source address type is set to IPv4, this parameter is set to IPv4. If the source address type is set to IPv6, this parameter is set to IPv6.</text>
        </description>
      </leaf>
      <leaf name="vrf-name">
        <type name="leafref">
          <path value="/ni:network-instance/ni:instances/ni:instance/ni:name"/>
        </type>
        <description>
          <text>Name of a VPN instance.</text>
        </description>
      </leaf>
      <leaf name="interval">
        <type name="uint32">
          <range value="1..10000"/>
        </type>
        <units name="ms"/>
        <description>
          <text>Interval for sending test packets.</text>
        </description>
      </leaf>
      <leaf name="packet-size">
        <type name="uint32">
          <range value="20..9600"/>
        </type>
        <description>
          <text>Number of bytes in a sent test packet.</text>
        </description>
      </leaf>
      <leaf name="packet-count">
        <type name="uint32">
          <range value="1..4294967295"/>
        </type>
        <description>
          <text>Number of sent test packets.</text>
        </description>
      </leaf>
      <leaf name="timeout">
        <type name="uint32">
          <range value="0..65535"/>
        </type>
        <units name="ms"/>
        <description>
          <text>Time to wait for a response packet after a test packet is sent. If no response packet is received within the timeout period, the system considers that the test packet times out.</text>
        </description>
      </leaf>
      <leaf name="ttl">
        <type name="uint8">
          <range value="1..255"/>
        </type>
        <description>
          <text>Time to live.</text>
        </description>
      </leaf>
      <leaf name="if-name">
        <type name="leafref">
          <path value="/ifm:ifm/ifm:interfaces/ifm:interface/ifm:name"/>
        </type>
        <description>
          <text>The out interface for sending packets.</text>
        </description>
      </leaf>
      <leaf name="inbound-reply-fast">
        <type name="boolean"/>
        <must condition="../if-name and not(../nexthop-address) and not(../source-if-name)"/>
        <description>
          <text>Enable/disable fast reply from the specified Eth-Trunk member interface.</text>
        </description>
      </leaf>
      <leaf name="source-if-name">
        <type name="leafref">
          <path value="/ifm:ifm/ifm:interfaces/ifm:interface/ifm:name"/>
        </type>
        <description>
          <text>The source interface for echo requests.</text>
        </description>
      </leaf>
      <leaf name="segment-by-segment">
        <type name="boolean"/>
        <description>
          <text>Segment by segment.</text>
        </description>
      </leaf>
      <leaf name="min-packet-size">
        <type name="uint32">
          <range value="20..9600"/>
        </type>
        <description>
          <text>The minimum size of packet to be sent.</text>
        </description>
      </leaf>
      <leaf name="max-packet-size">
        <type name="uint32">
          <range value="20..9600"/>
        </type>
        <description>
          <text>The maximum size of packet to be sent.</text>
        </description>
      </leaf>
      <leaf name="step-size">
        <type name="uint32">
          <range value="1..1000"/>
        </type>
        <description>
          <text>Increasing step size.</text>
        </description>
      </leaf>
      <leaf name="nexthop-address">
        <type name="inet:ipv6-address-no-zone"/>
        <description>
          <text>Next hop IPv6 address.</text>
        </description>
      </leaf>
      <leaf name="ignore-mtu">
        <type name="boolean"/>
        <description>
          <text>Check MTU flag.</text>
        </description>
      </leaf>
      <leaf name="show-system-time">
        <type name="boolean"/>
        <description>
          <text>Show sending echo-request packet time flag.</text>
        </description>
      </leaf>
      <leaf name="show-incoming-if-name">
        <type name="boolean"/>
        <description>
          <text>Interface name of incoming packet.</text>
        </description>
      </leaf>
      <leaf name="ipv6-forwarding">
        <type name="boolean"/>
        <description>
          <text>Request packet forwarded by IPv6.</text>
        </description>
      </leaf>
      <choice name="priority">
        <description>
          <text>Priority type.</text>
        </description>
        <case name="dscp-value">
          <description>
            <text>DSCP value.</text>
          </description>
          <leaf name="dscp">
            <type name="uint32">
              <range value="0..63"/>
            </type>
            <description>
              <text>DSCP value.</text>
            </description>
          </leaf>
        </case>
        <case name="tos-value">
          <description>
            <text>ToS value.</text>
          </description>
          <leaf name="tos">
            <type name="uint8">
              <range value="0..255"/>
            </type>
            <description>
              <text>ToS.</text>
            </description>
          </leaf>
        </case>
      </choice>
      <leaf name="pattern">
        <type name="string">
          <length value="1..8"/>
        </type>
        <description>
          <text>Packet fill.</text>
        </description>
      </leaf>
      <leaf name="sid1">
        <type name="inet:ipv6-address-no-zone"/>
        <description>
          <text>Segment ID.</text>
        </description>
      </leaf>
      <leaf name="sid2">
        <type name="inet:ipv6-address-no-zone"/>
        <description>
          <text>Segment ID.</text>
        </description>
      </leaf>
      <leaf name="sid3">
        <type name="inet:ipv6-address-no-zone"/>
        <description>
          <text>Segment ID.</text>
        </description>
      </leaf>
      <leaf name="sid4">
        <type name="inet:ipv6-address-no-zone"/>
        <description>
          <text>Segment ID.</text>
        </description>
      </leaf>
      <leaf name="sid5">
        <type name="inet:ipv6-address-no-zone"/>
        <description>
          <text>Segment ID.</text>
        </description>
      </leaf>
      <leaf name="sid6">
        <type name="inet:ipv6-address-no-zone"/>
        <description>
          <text>Segment ID.</text>
        </description>
      </leaf>
      <leaf name="sid7">
        <type name="inet:ipv6-address-no-zone"/>
        <description>
          <text>Segment ID.</text>
        </description>
      </leaf>
      <leaf name="sid8">
        <type name="inet:ipv6-address-no-zone"/>
        <description>
          <text>Segment ID.</text>
        </description>
      </leaf>
      <leaf name="sid9">
        <type name="inet:ipv6-address-no-zone"/>
        <description>
          <text>Segment ID.</text>
        </description>
      </leaf>
      <leaf name="sid10">
        <type name="inet:ipv6-address-no-zone"/>
        <description>
          <text>Segment ID.</text>
        </description>
      </leaf>
      <leaf name="sid11">
        <type name="inet:ipv6-address-no-zone"/>
        <description>
          <text>Segment ID.</text>
        </description>
      </leaf>
      <leaf name="network-slice-id">
        <when condition="../sid1"/>
        <type name="uint32">
          <range value="1..4294967295"/>
        </type>
        <description>
          <text>Network slice ID.</text>
        </description>
      </leaf>
      <leaf name="force-match-slice">
        <when condition="../sid1 and ../network-slice-id"/>
        <type name="empty"/>
        <description>
          <text>Mandatory match network slice.</text>
        </description>
      </leaf>
      <leaf name="service-class">
        <type name="diagnostic-tools:service-class-value"/>
        <description>
          <text>Service class.</text>
        </description>
      </leaf>
    </input>
  </rpc>
  <rpc name="ipv6-stop-ping">
    <description>
      <text>Stop IPv6 ping.</text>
    </description>
    <ext:node-ref value="/diagnostic-tools:diagnostic-tools/diagnostic-tools:ipv6/diagnostic-tools:ping-results"/>
    <input>
      <leaf name="test-name">
        <type name="leafref">
          <path value="/diagnostic-tools:diagnostic-tools/diagnostic-tools:ipv6/diagnostic-tools:ping-results/diagnostic-tools:ping-result/diagnostic-tools:test-name"/>
        </type>
        <mandatory value="true"/>
        <description>
          <text>Test name.</text>
        </description>
      </leaf>
    </input>
  </rpc>
  <rpc name="ipv6-delete-ping">
    <description>
      <text>Delete IPv6 ping.</text>
    </description>
    <ext:node-ref value="/diagnostic-tools:diagnostic-tools/diagnostic-tools:ipv6/diagnostic-tools:ping-results"/>
    <input>
      <leaf name="test-name">
        <type name="leafref">
          <path value="/diagnostic-tools:diagnostic-tools/diagnostic-tools:ipv6/diagnostic-tools:ping-results/diagnostic-tools:ping-result/diagnostic-tools:test-name"/>
        </type>
        <mandatory value="true"/>
        <description>
          <text>Test name.</text>
        </description>
      </leaf>
    </input>
  </rpc>
  <rpc name="ipv6-start-trace">
    <description>
      <text>Start IPv6 trace.</text>
    </description>
    <ext:node-ref value="/diagnostic-tools:diagnostic-tools/diagnostic-tools:ipv6/diagnostic-tools:trace-results"/>
    <input>
      <leaf name="test-name">
        <type name="string">
          <length value="1..32"/>
        </type>
        <mandatory value="true"/>
        <description>
          <text>Test name.</text>
        </description>
      </leaf>
      <leaf name="dest-addr">
        <type name="pub-type:host-name"/>
        <description>
          <text>Destination address of the string type. You can click this node to set different types of addresses and deploy the configurations together with the configuration of the destination address type node. If the destination address type is set to IPv4, this parameter is set to IPv4. If the destination address type is set to IPv6, this parameter is set to IPv6.</text>
        </description>
      </leaf>
      <leaf name="source-address">
        <type name="inet:ipv6-address-no-zone"/>
        <description>
          <text>Source address of the string type. You can click this node to set different types of addresses and deploy the configurations together with the configuration of the source address type node. If the source address type is set to IPv4, this parameter is set to IPv4. If the source address type is set to IPv6, this parameter is set to IPv6.</text>
        </description>
      </leaf>
      <leaf name="vrf-name">
        <type name="leafref">
          <path value="/ni:network-instance/ni:instances/ni:instance/ni:name"/>
        </type>
        <description>
          <text>Name of a VPN instance.</text>
        </description>
      </leaf>
      <leaf name="packet-size">
        <type name="uint32">
          <range value="20..9600"/>
        </type>
        <description>
          <text>Number of bytes in a sent test packet.</text>
        </description>
      </leaf>
      <leaf name="count">
        <type name="uint32">
          <range value="1..65535"/>
        </type>
        <description>
          <text>Number of sent test packets.</text>
        </description>
      </leaf>
      <leaf name="timeout">
        <type name="uint32">
          <range value="0..65535"/>
        </type>
        <units name="ms"/>
        <description>
          <text>Time to wait for a response packet after a test packet is sent. If no response packet is received within the timeout period, the system considers that the test packet times out.</text>
        </description>
      </leaf>
      <leaf name="first-ttl">
        <type name="uint8">
          <range value="1..255"/>
        </type>
        <description>
          <text>TTL of the first packet in a test.</text>
        </description>
      </leaf>
      <leaf name="max-ttl">
        <type name="uint8">
          <range value="1..255"/>
        </type>
        <description>
          <text>TTL of test packets.</text>
        </description>
      </leaf>
      <leaf name="udp-port">
        <type name="uint32">
          <range value="0..65535"/>
        </type>
        <description>
          <text>Destination port number.</text>
        </description>
      </leaf>
      <leaf name="show-mpls-label">
        <type name="boolean"/>
        <description>
          <text>Show MPLS label information flag.</text>
        </description>
      </leaf>
      <leaf name="overlay">
        <type name="boolean"/>
        <description>
          <text>Overlay.</text>
        </description>
      </leaf>
      <leaf name="nexthop-address">
        <type name="inet:ipv6-address-no-zone"/>
        <description>
          <text>Next hop IPv6 address.</text>
        </description>
      </leaf>
      <leaf name="if-name">
        <type name="leafref">
          <path value="/ifm:ifm/ifm:interfaces/ifm:interface/ifm:name"/>
        </type>
        <description>
          <text>The out interface for sending packets.</text>
        </description>
      </leaf>
      <choice name="priority">
        <description>
          <text>Priority type.</text>
        </description>
        <case name="dscp-value">
          <description>
            <text>DSCP value.</text>
          </description>
          <leaf name="dscp">
            <type name="uint32">
              <range value="0..63"/>
            </type>
            <default value="0"/>
            <description>
              <text>DSCP value.</text>
            </description>
          </leaf>
        </case>
      </choice>
      <leaf name="pass-route">
        <type name="boolean"/>
        <description>
          <text>If not check pass route.</text>
        </description>
      </leaf>
      <leaf name="sid1">
        <type name="inet:ipv6-address-no-zone"/>
        <description>
          <text>Segment ID.</text>
        </description>
      </leaf>
      <leaf name="sid2">
        <type name="inet:ipv6-address-no-zone"/>
        <description>
          <text>Segment ID.</text>
        </description>
      </leaf>
      <leaf name="sid3">
        <type name="inet:ipv6-address-no-zone"/>
        <description>
          <text>Segment ID.</text>
        </description>
      </leaf>
      <leaf name="sid4">
        <type name="inet:ipv6-address-no-zone"/>
        <description>
          <text>Segment ID.</text>
        </description>
      </leaf>
      <leaf name="sid5">
        <type name="inet:ipv6-address-no-zone"/>
        <description>
          <text>Segment ID.</text>
        </description>
      </leaf>
      <leaf name="sid6">
        <type name="inet:ipv6-address-no-zone"/>
        <description>
          <text>Segment ID.</text>
        </description>
      </leaf>
      <leaf name="sid7">
        <type name="inet:ipv6-address-no-zone"/>
        <description>
          <text>Segment ID.</text>
        </description>
      </leaf>
      <leaf name="sid8">
        <type name="inet:ipv6-address-no-zone"/>
        <description>
          <text>Segment ID.</text>
        </description>
      </leaf>
      <leaf name="sid9">
        <type name="inet:ipv6-address-no-zone"/>
        <description>
          <text>Segment ID.</text>
        </description>
      </leaf>
      <leaf name="sid10">
        <type name="inet:ipv6-address-no-zone"/>
        <description>
          <text>Segment ID.</text>
        </description>
      </leaf>
      <leaf name="sid11">
        <type name="inet:ipv6-address-no-zone"/>
        <description>
          <text>Segment ID.</text>
        </description>
      </leaf>
      <leaf name="network-slice-id">
        <when condition="../sid1"/>
        <type name="uint32">
          <range value="1..4294967295"/>
        </type>
        <description>
          <text>Network slice ID.</text>
        </description>
      </leaf>
      <leaf name="force-match-slice">
        <when condition="../sid1 and ../network-slice-id"/>
        <type name="empty"/>
        <description>
          <text>Mandatory match network slice.</text>
        </description>
      </leaf>
      <leaf name="service-class">
        <type name="diagnostic-tools:service-class-value"/>
        <description>
          <text>Service class.</text>
        </description>
      </leaf>
    </input>
  </rpc>
  <rpc name="ipv6-stop-trace">
    <description>
      <text>Stop IPv6 trace.</text>
    </description>
    <ext:node-ref value="/diagnostic-tools:diagnostic-tools/diagnostic-tools:ipv6/diagnostic-tools:trace-results"/>
    <input>
      <leaf name="test-name">
        <type name="leafref">
          <path value="/diagnostic-tools:diagnostic-tools/diagnostic-tools:ipv6/diagnostic-tools:trace-results/diagnostic-tools:trace-result/diagnostic-tools:test-name"/>
        </type>
        <mandatory value="true"/>
        <description>
          <text>Test name.</text>
        </description>
      </leaf>
    </input>
  </rpc>
  <rpc name="ipv6-delete-trace">
    <description>
      <text>Delete IPv6 trace.</text>
    </description>
    <ext:node-ref value="/diagnostic-tools:diagnostic-tools/diagnostic-tools:ipv6/diagnostic-tools:trace-results"/>
    <input>
      <leaf name="test-name">
        <type name="leafref">
          <path value="/diagnostic-tools:diagnostic-tools/diagnostic-tools:ipv6/diagnostic-tools:trace-results/diagnostic-tools:trace-result/diagnostic-tools:test-name"/>
        </type>
        <mandatory value="true"/>
        <description>
          <text>Test name.</text>
        </description>
      </leaf>
    </input>
  </rpc>
  <augment target-node="/diagnostic-tools:diagnostic-tools">
    <description>
      <text>List of IPv6 tests.</text>
    </description>
    <container name="ipv6">
      <config value="false"/>
      <description>
        <text>Statistics of IPv6 tests.</text>
      </description>
      <uses name="ip6-ping-trace-result-type"/>
    </container>
  </augment>
</submodule>
