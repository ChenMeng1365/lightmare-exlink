<?xml version="1.0" encoding="UTF-8"?>
<module name="huawei-ifm-bundle"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:ifm-bundle="urn:huawei:yang:huawei-ifm-bundle"
        xmlns:ifm="urn:huawei:yang:huawei-ifm"
        xmlns:ext="urn:huawei:yang:huawei-extension">
  <namespace uri="urn:huawei:yang:huawei-ifm-bundle"/>
  <prefix value="ifm-bundle"/>
  <import module="huawei-ifm">
    <prefix value="ifm"/>
  </import>
  <import module="huawei-extension">
    <prefix value="ext"/>
  </import>
  <organization>
    <text>Huawei Technologies Co., Ltd.</text>
  </organization>
  <contact>
    <text>Huawei Industrial Base
Bantian, Longgang
Shenzhen 518129
People's Republic of China
Website: http://www.huawei.com
Email: support@huawei.com</text>
  </contact>
  <description>
    <text>Define the interface that bundles multiple ATM sub-interfaces into a logical interface.</text>
  </description>
  <revision date="2020-02-13">
    <description>
      <text>Initial revision.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <ext:task-name value="trunk"/>
  <augment target-node="/ifm:ifm/ifm:interfaces/ifm:interface">
    <when condition="ifm:type='Atm-Bundle'"/>
    <description>
      <text>Active when the interface is set to type Atm-Bundle.</text>
    </description>
    <container name="bundle">
      <description>
        <text>Configure the interface that bundles multiple ATM sub-interfaces into a logical interface.</text>
      </description>
      <leaf name="up-member-num">
        <type name="uint32"/>
        <config value="false"/>
        <description>
          <text>Display the number of ATM-Bundle member interfaces in the Up state.</text>
        </description>
      </leaf>
      <container name="members">
        <description>
          <text>List of ATM-Bundle member interfaces.</text>
        </description>
        <list name="member">
          <key value="name"/>
          <description>
            <text>Configure ATM-Bundle member interfaces.</text>
          </description>
          <leaf name="name">
            <type name="leafref">
              <path value="/ifm:ifm/ifm:interfaces/ifm:interface/ifm:name"/>
            </type>
            <must condition="/ifm:ifm/ifm:interfaces/ifm:interface[ifm:name=current()]/ifm:class='sub-interface' and (/ifm:ifm/ifm:interfaces/ifm:interface[ifm:name=current()]/ifm:type='Serial' or /ifm:ifm/ifm:interfaces/ifm:interface[ifm:name=current()]/ifm:type='Ima-group')"/>
            <description>
              <text>Name of an ATM-Bundle member interface.</text>
            </description>
          </leaf>
          <leaf name="status">
            <type name="ifm:port-status"/>
            <config value="false"/>
            <description>
              <text>Display the status of an ATM-Bundle member interface.</text>
            </description>
          </leaf>
        </list>
      </container>
    </container>
  </augment>
</module>
