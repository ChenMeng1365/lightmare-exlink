<?xml version="1.0" encoding="UTF-8"?>
<module name="huawei-pcep"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:pcep="urn:huawei:yang:huawei-pcep"
        xmlns:ext="urn:huawei:yang:huawei-extension">
  <namespace uri="urn:huawei:yang:huawei-pcep"/>
  <prefix value="pcep"/>
  <import module="huawei-extension">
    <prefix value="ext"/>
  </import>
  <organization>
    <text>Huawei Technologies Co., Ltd.</text>
  </organization>
  <contact>
    <text>Huawei Industrial Base
Bantian, Longgang
Shenzhen 518129
People's Republic of China
Website: http://www.huawei.com
Email: support@huawei.com</text>
  </contact>
  <description>
    <text>Path Computation Element Communication (PCEP).</text>
  </description>
  <revision date="2020-07-10">
    <description>
      <text>Initial revision.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <ext:task-name value="pce"/>
  <container name="pcep">
    <description>
      <text>PCEP configuration.</text>
    </description>
    <container name="session-car">
      <description>
        <text>Configure session CAR.</text>
      </description>
      <container name="ipv4">
        <must condition="cir&lt;=pir"/>
        <must condition="cbs&lt;=pbs"/>
        <description>
          <text>Configure PCEP IPv4 session CAR.</text>
        </description>
        <leaf name="enable">
          <type name="boolean"/>
          <default value="true"/>
          <description>
            <text>Enable/disable PCEP IPv4 session CAR.</text>
          </description>
        </leaf>
        <leaf name="cir">
          <type name="uint32">
            <range value="1..1000000"/>
          </type>
          <units name="kbit/s"/>
          <default value="9"/>
          <description>
            <text>Session CAR CIR value.</text>
          </description>
        </leaf>
        <leaf name="cbs">
          <type name="uint32">
            <range value="1..9000000"/>
          </type>
          <units name="Byte"/>
          <default value="1758"/>
          <description>
            <text>Session CAR CBS value.</text>
          </description>
        </leaf>
        <leaf name="pir">
          <type name="uint32">
            <range value="1..1000000"/>
          </type>
          <units name="kbit/s"/>
          <default value="2400"/>
          <description>
            <text>Session CAR PIR value.</text>
          </description>
        </leaf>
        <leaf name="pbs">
          <type name="uint32">
            <range value="1..9000000"/>
          </type>
          <units name="Byte"/>
          <default value="450000"/>
          <description>
            <text>Session CAR PBS value.</text>
          </description>
        </leaf>
      </container>
    </container>
  </container>
</module>
