<?xml version="1.0" encoding="UTF-8"?>
<module name="huawei-fei"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:fei="urn:huawei:yang:huawei-fei"
        xmlns:ext="urn:huawei:yang:huawei-extension">
  <yang-version value="1"/>
  <namespace uri="urn:huawei:yang:huawei-fei"/>
  <prefix value="fei"/>
  <import module="huawei-extension">
    <prefix value="ext"/>
  </import>
  <organization>
    <text>Huawei Technologies Co., Ltd.</text>
  </organization>
  <contact>
    <text>Huawei Industrial Base
Bantian, Longgang
Shenzhen 518129
People's Republic of China
Website: http://www.huawei.com
Email: support@huawei.com
</text>
  </contact>
  <description>
    <text>System hardware difference specifications.</text>
  </description>
  <revision date="2020-02-28">
    <description>
      <text>Init revision.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <ext:task-name value="forward-system"/>
  <container name="fei">
    <config value="false"/>
    <description>
      <text>Global configuration.</text>
    </description>
    <container name="hard-diffs">
      <config value="false"/>
      <description>
        <text>List of hard rule tables.</text>
      </description>
      <list name="hard-diff">
        <key value="op-mode entity-para parent-para schema-spec element-xpath bind-type bind-node-xpath bind-parent-xpath board-type board-version subcard-type interface-type if-main-sub-type"/>
        <description>
          <text>Statistics of hard rule table.</text>
        </description>
        <leaf name="op-mode">
          <type name="uint32"/>
          <description>
            <text>Operate mode.</text>
          </description>
        </leaf>
        <leaf name="entity-para">
          <type name="string">
            <length value="1..256"/>
          </type>
          <description>
            <text>Entity parameter.</text>
          </description>
        </leaf>
        <leaf name="parent-para">
          <type name="string">
            <length value="1..256"/>
          </type>
          <description>
            <text>Entity parent parameter.</text>
          </description>
        </leaf>
        <leaf name="schema-spec">
          <type name="string">
            <length value="1..256"/>
          </type>
          <description>
            <text>Schema name.</text>
          </description>
        </leaf>
        <leaf name="element-xpath">
          <type name="string">
            <length value="1..256"/>
          </type>
          <description>
            <text>The xpath of element.</text>
          </description>
        </leaf>
        <leaf name="bind-type">
          <type name="uint32"/>
          <description>
            <text>Bind type.</text>
          </description>
        </leaf>
        <leaf name="bind-node-xpath">
          <type name="string">
            <length value="1..256"/>
          </type>
          <description>
            <text>The xpath of bind node.</text>
          </description>
        </leaf>
        <leaf name="bind-parent-xpath">
          <type name="string">
            <length value="1..256"/>
          </type>
          <description>
            <text>The xpath of bind parent node.</text>
          </description>
        </leaf>
        <leaf name="board-type">
          <type name="uint32"/>
          <description>
            <text>Board type.</text>
          </description>
        </leaf>
        <leaf name="board-version">
          <type name="uint32"/>
          <description>
            <text>Board version.</text>
          </description>
        </leaf>
        <leaf name="subcard-type">
          <type name="uint32"/>
          <description>
            <text>Subcard type.</text>
          </description>
        </leaf>
        <leaf name="interface-type">
          <type name="uint32"/>
          <description>
            <text>Interface type.</text>
          </description>
        </leaf>
        <leaf name="if-main-sub-type">
          <type name="uint32"/>
          <description>
            <text>Main or sub type.</text>
          </description>
        </leaf>
        <leaf name="is-support">
          <type name="uint32"/>
          <description>
            <text>Support or not.</text>
          </description>
        </leaf>
        <leaf name="range">
          <type name="string">
            <length value="1..1024"/>
          </type>
          <description>
            <text>Range of record.</text>
          </description>
        </leaf>
        <leaf name="default">
          <type name="string">
            <length value="1..256"/>
          </type>
          <description>
            <text>Default vaule of record.</text>
          </description>
        </leaf>
      </list>
    </container>
  </container>
</module>
