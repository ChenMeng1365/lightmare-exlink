module: huawei-bras-layer3-access
  +--rw bras-layer3-access
     +--rw global
     |  +--rw user-switch
     |  |  +--rw switch-policy?   enumeration
     |  +--rw any-ip
     |     +--rw domain-name?   -> /aaa:aaa/domains/domain/name
     +--rw subscriber-ips
     |  +--rw subscriber-ip* [start-ip-address vpn-name]
     |     +--rw start-ip-address    inet:ipv4-address-no-zone
     |     +--rw vpn-name            -> /network-instance:network-instance/instances/instance/name
     |     +--rw domain-name         -> /aaa:aaa/domains/domain/name
     |     +--rw end-ip-address      inet:ipv4-address-no-zone
     +--rw subscriber-ipv6s
     |  +--rw subscriber-ipv6* [address-type start-ipv6-address vpn-name]
