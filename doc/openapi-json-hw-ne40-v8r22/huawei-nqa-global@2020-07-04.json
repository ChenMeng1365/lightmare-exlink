{
    "swagger": "2.0",
    "info": {
        "description": "huawei-nqa-global API generated from huawei-nqa-global@2020-07-04.yang",
        "version": "1.0.0",
        "title": "huawei-nqa-global API"
    },
    "host": "localhost:8080",
    "basePath": "/restconf",
    "schemes": [
        "http"
    ],
    "paths": {},
    "definitions": {
        "global-type": {
            "description": "Global configuration.",
            "properties": {
                "fec-validation-ldp-enable": {
                    "type": "boolean",
                    "description": "Enable/disable LDP FEC validation."
                },
                "fec-validation-sr-prefix-enable": {
                    "type": "boolean",
                    "description": "Enable/disable sr-prefix FEC validation."
                },
                "lspv-cpu-limit": {
                    "type": "string",
                    "description": "Lspv cpu limit."
                },
                "jitter-tag-version": {
                    "type": "string",
                    "description": "Version number of jitter."
                },
                "lbm-receive-enable": {
                    "type": "boolean",
                    "description": "Enable/disable a device from receiving LBM packets."
                },
                "ltm-receive-enable": {
                    "type": "boolean",
                    "description": "Enable/disable a device from receiving LTM packets."
                },
                "gmac-ltm-receive-enable": {
                    "type": "boolean",
                    "description": "Enable/disable a device from receiving GMAC-LTM packets."
                },
                "lspv-enable": {
                    "type": "boolean",
                    "description": "Enable/disable a device to respond to MPLS echo request packets."
                },
                "lspv-ipv6-enable": {
                    "type": "boolean",
                    "description": "Enable/disable a device to respond to MPLS echo request IPv6 packets."
                },
                "lspv-fec-compitable-enable": {
                    "type": "boolean",
                    "description": "Enable/disable the LSPV's compatibility function, and reply response packets don't carry FEC information."
                },
                "gmac-ping-enable": {
                    "type": "boolean",
                    "description": "Enable/disable general GMAC ping function."
                },
                "gmac-trace-enable": {
                    "type": "boolean",
                    "description": "Enable/disable general GMAC trace function."
                },
                "test-max-flow-id": {
                    "type": "string",
                    "description": "Maximum NQA test flow ID."
                },
                "reflector-max-id": {
                    "type": "string",
                    "description": "Maximum NQA reflector ID."
                },
                "vxlan-echo-v4": {
                    "description": "Enable/disable NQA VXLANECHO.",
                    "properties": {
                        "port": {
                            "type": "string",
                            "description": "Port of NQA VXLANECHO server."
                        },
                        "source-ip-if-name": {
                            "type": "string",
                            "x-path": "/ifm:ifm/ifm:interfaces/ifm:interface/ifm:name",
                            "description": "Source IP interface of VXLANECHO."
                        }
                    }
                },
                "bier6-oam": {
                    "description": "Configure BIER IPv6 OAM.",
                    "properties": {
                        "dest-port": {
                            "type": "string",
                            "description": "Destination UDP port of BIER IPv6 echo packet."
                        },
                        "echo-reply-disable": {
                            "type": "boolean",
                            "description": "Enable/disable BIER IPv6 reply function."
                        },
                        "rate-limit": {
                            "type": "string",
                            "description": "BIER IPv6 receive rate limit."
                        }
                    }
                }
            }
        }
    }
}