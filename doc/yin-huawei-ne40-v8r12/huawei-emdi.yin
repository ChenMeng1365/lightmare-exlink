  <rpc name="filter-history-statistic">
    <description>
      <text>Filter EMDI history statistics.</text>
    </description>
    <ext:node-ref value="/emdi/emdi-history-statistic-records/emdi-history-statistic-record"/>
    <input>
      <leaf name="channel-name">
        <type name="string">
          <length value="1..31"/>
        </type>
        <description>
          <text>Channel name.</text>
        </description>
      </leaf>
      <leaf name="start-index">
        <type name="uint16">
          <range value="1..1000"/>
        </type>
        <description>
          <text>Start index. This parameter is used only as a filter criterion and is not displayed.</text>
        </description>
      </leaf>
      <leaf name="end-index">
        <type name="uint16">
          <range value="1..1000"/>
        </type>
        <description>
          <text>End index. It is only used as a filter criterion and is not displayed.</text>
        </description>
      </leaf>
      <leaf name="last-record-num">
        <type name="uint8">
          <range value="1..5"/>
        </type>
        <description>
          <text>Query the latest specified number of detection records. This parameter is used only as a filter criterion and is not displayed.</text>
        </description>
      </leaf>
      <leaf name="direction">
        <type name="enumeration">
          <enum name="inbound">
            <value value="0"/>
            <description>
              <text>Inbound.</text>
            </description>
          </enum>
