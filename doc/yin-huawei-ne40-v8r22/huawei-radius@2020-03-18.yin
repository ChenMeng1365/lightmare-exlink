            <case name="vendor">
              <description>
                <text>Vendor.</text>
              </description>
              <leaf name="vendor-id">
                <type name="uint32">
                  <range value="2352|2636"/>
                </type>
                <mandatory value="true"/>
                <description>
                  <text>Vendor ID.</text>
                </description>
              </leaf>
              <leaf name="include-option82">
                <when condition="../vendor-id=2352"/>
                <type name="empty"/>
                <description>
                  <text>Enable/disable user's access-line-id (dhcpv4 option82/pppoe+/dhcpv6 option18+37+17).</text>
                </description>
              </leaf>
              <leaf name="version1">
                <when condition="../vendor-id=2352"/>
                <type name="empty"/>
                <description>
                  <text>Enable/disable using when calling-station-id is configed as redback format.</text>
                </description>
              </leaf>
            </case>
            <case name="user-defined">
              <description>
                <text>Customized version of calling-station-id.</text>
              </description>
              <leaf name="version3">
                <type name="empty"/>
                <description>
                  <text>Configure the Calling-Station-Id attribute to be encapsulated in the version3 format.</text>
                </description>
              </leaf>
            </case>
          </choice>
