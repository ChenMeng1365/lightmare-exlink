<?xml version="1.0" encoding="UTF-8"?>
<module name="huawei-bras-python-script"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:bras-python-script="urn:huawei:yang:huawei-bras-python-script"
        xmlns:ext="urn:huawei:yang:huawei-extension"
        xmlns:yang="urn:ietf:params:xml:ns:yang:ietf-yang-types">
  <yang-version value="1"/>
  <namespace uri="urn:huawei:yang:huawei-bras-python-script"/>
  <prefix value="bras-python-script"/>
  <import module="huawei-extension">
    <prefix value="ext"/>
  </import>
  <import module="ietf-yang-types">
    <prefix value="yang"/>
  </import>
  <organization>
    <text>Huawei Technologies Co., Ltd.</text>
  </organization>
  <contact>
    <text>Huawei Industrial Base
Bantian, Longgang
Shenzhen 518129
People's Republic of China
Website: http://www.huawei.com
Email: support@huawei.com</text>
  </contact>
  <description>
    <text>Python script refers to the function of an automatically loading of version file (including system software, configuration files and patch files) used for powering on a new factory device or an empty configuration device.</text>
  </description>
  <revision date="2020-12-14">
    <description>
      <text>Initial revision.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <ext:task-name value="bras-control"/>
  <grouping name="python-script-statistic">
    <description>
      <text>Statistics for the loaded Python script package grouping.</text>
    </description>
    <container name="python-script-statistic">
      <config value="false"/>
      <description>
        <text>Operational data of statistic related to the loaded Python script package.</text>
      </description>
      <leaf name="package-version">
        <type name="string">
          <length value="1..32"/>
        </type>
        <description>
          <text>Version number of the Python script package.</text>
        </description>
      </leaf>
      <leaf name="package-run-time">
        <type name="yang:date-and-time"/>
        <description>
          <text>Running time of the Python script package on the device.</text>
        </description>
      </leaf>
      <leaf name="compatible-api-versions">
        <type name="string">
          <length value="1..128"/>
        </type>
        <description>
          <text>Compatible API version.</text>
        </description>
      </leaf>
      <leaf name="package-api-version">
        <type name="string">
          <length value="1..7"/>
        </type>
        <description>
          <text>API version of the Python script package.</text>
        </description>
      </leaf>
      <container name="script-states">
        <description>
          <text>List of script state in the Python script package.</text>
        </description>
        <list name="script-state">
          <key value="script-name"/>
          <description>
            <text>Operational data of script state in the Python script package.</text>
          </description>
          <leaf name="script-name">
            <type name="string">
              <length value="1..32"/>
            </type>
            <description>
              <text>Script name.</text>
            </description>
          </leaf>
          <leaf name="run-state">
            <type name="enumeration">
              <enum name="running">
                <value value="1"/>
                <description>
                  <text>Running.</text>
                </description>
              </enum>
            </type>
            <description>
              <text>Running status of the script.</text>
            </description>
          </leaf>
        </list>
      </container>
    </container>
  </grouping>
  <container name="bras-python-script">
    <description>
      <text>Python script.</text>
    </description>
    <uses name="python-script-statistic"/>
    <container name="cu-python-script-statistics">
      <config value="false"/>
      <description>
        <text>List of statistic related to the loaded Python script package. Only support CU mode.</text>
      </description>
      <list name="cu-python-script-statistic">
        <key value="upmng-instance"/>
        <description>
          <text>Operational data of statistic related to the loaded Python script package. Only support CU mode.</text>
        </description>
        <leaf name="upmng-instance">
          <type name="uint32"/>
          <description>
            <text>UPMNG instance ID.</text>
          </description>
        </leaf>
        <uses name="python-script-statistic"/>
      </list>
    </container>
  </container>
</module>
