      </leaf>
      <leaf name="physical-down-count">
        <type name="uint32"/>
        <description>
          <text>Number of IPv4 interfaces that are in the physical down state.</text>
        </description>
      </leaf>
      <leaf name="protocol-up-count">
        <type name="uint32"/>
        <description>
          <text>Number of IPv4 interfaces that are in the protocol up state.</text>
        </description>
      </leaf>
      <leaf name="protocol-down-count">
        <type name="uint32"/>
        <description>
          <text>Number of IPv4 interfaces that are in the protocol down state.</text>
        </description>
      </leaf>
    </container>
    <container name="remote-interfaces">
      <config value="false"/>
      <description>
        <text>List of statistics of remote interfaces.</text>
      </description>
      <list name="remote-interface">
        <key value="name"/>
        <description>
          <text>Statistics of remote interfaces.</text>
        </description>
        <leaf name="name">
          <type name="pub-type:if-name"/>
          <description>
            <text>Name of a remote interface.</text>
          </description>
        </leaf>
        <leaf name="index">
          <type name="uint32"/>
          <description>
            <text>Index of a remote interface.</text>
          </description>
        </leaf>
        <container name="remote-statistics-state">
          <config value="false"/>
          <description>
            <text>Statistics of remote interfaces.</text>
          </description>
          <leaf name="last-cleared-time">
            <type name="pub-type:time"/>
            <description>
              <text>Last time when statistics about remote interfaces were cleared.</text>
            </description>
          </leaf>
          <leaf name="in-bit-rate">
            <type name="uint64"/>
            <units name="bit/s"/>
            <description>
              <text>Rate at which bits are received.</text>
            </description>
          </leaf>
          <leaf name="in-packet-rate">
            <type name="uint64"/>
            <units name="pps"/>
            <description>
              <text>Rate at which packets are received.</text>
            </description>
          </leaf>
          <leaf name="in-peak-bit-rate">
            <type name="uint64"/>
            <units name="bit/s"/>
            <description>
              <text>Peak rate at which bits are received.</text>
            </description>
          </leaf>
          <leaf name="in-peak-bit-rate-record-time">
            <type name="pub-type:time"/>
            <description>
              <text>Time when the peak bit receive rate is reached.</text>
            </description>
          </leaf>
          <leaf name="out-bit-rate">
            <type name="uint64"/>
            <units name="bit/s"/>
            <description>
              <text>Rate at which bits are sent.</text>
            </description>
          </leaf>
          <leaf name="out-packet-rate">
            <type name="uint64"/>
            <units name="pps"/>
            <description>
              <text>Rate at which packets are sent.</text>
            </description>
          </leaf>
          <leaf name="out-peak-bit-rate">
            <type name="uint64"/>
            <units name="bit/s"/>
            <description>
              <text>Peak rate at which bits are sent.</text>
            </description>
          </leaf>
          <leaf name="out-peak-bit-rate-record-time">
            <type name="pub-type:time"/>
            <description>
              <text>Time when the peak bit send rate is reached.</text>
            </description>
          </leaf>
          <leaf name="in-use-rate">
            <type name="string">
              <length value="1..50"/>
            </type>
            <description>
              <text>Received bandwidth usage.</text>
            </description>
          </leaf>
          <leaf name="out-use-rate">
            <type name="string">
              <length value="1..50"/>
            </type>
            <description>
              <text>Sent bandwidth usage.</text>
            </description>
          </leaf>
        </container>
      </list>
    </container>
  </container>
