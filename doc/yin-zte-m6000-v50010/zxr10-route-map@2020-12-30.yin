                <description>
                  <text>Set the label value.</text>
                </description>
                <type name="uint32">
                  <range value="16..1048575">
                    <description>
                      <text>The range of evi label is from 16 to 1048575.</text>
                    </description>
                  </range>
                </type>
                <mandatory value="true"/>
              </leaf>
              <ncd:revise-date date="2019-09-09"/>
            </container>
            <container name="gw-ip">
              <description>
                <text>Gateway IP address.</text>
              </description>
              <presence value="gw-ip is not Mandatory"/>
              <leaf name="ipv4-addr">
                <description>
                  <text>Set IPv4 address.</text>
                </description>
                <type name="ncd:ipv4-address"/>
                <mandatory value="true"/>
              </leaf>
              <ncd:revise-date date="2019-09-26"/>
            </container>
            <container name="l2-srv6-function-id">
              <description>
                <text>Set l2-srv6-function-id.</text>
              </description>
              <presence value="l2-srv6-function-id"/>
              <container name="dt2m">
                <description>
                  <text>DT2M function code.</text>
                </description>
                <presence value="dt2m"/>
