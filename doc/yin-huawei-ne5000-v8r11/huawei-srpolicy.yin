              <type name="uint32"/>
              <config value="false"/>
              <description>
                <text>Candidate path template Id.</text>
              </description>
            </leaf>
            <leaf name="segment-list-count">
              <type name="uint32"/>
              <config value="false"/>
              <description>
                <text>Candidate path segment list count.</text>
              </description>
            </leaf>
            <container name="segment-lists">
              <when condition="../../../endpoint and ../../../color and ../protocol-origin and ../asn and ../node-address and ../discriminator and ../preference"/>
              <config value="false"/>
              <description>
                <text>List of segment list information in a candidate path.</text>
              </description>
              <list name="segment-list">
                <key value="id"/>
                <config value="false"/>
                <max-elements value="64"/>
                <description>
                  <text>Operational state of a segment list in a candidate path.</text>
                </description>
                <leaf name="id">
                  <type name="uint32"/>
                  <config value="false"/>
                  <description>
                    <text>Segment list ID.</text>
                  </description>
                </leaf>
                <leaf name="name">
                  <type name="string">
                    <length value="1..32"/>
                  </type>
                  <config value="false"/>
                  <description>
                    <text>Segment list name.</text>
                  </description>
                </leaf>
                <leaf name="xc-index">
                  <type name="uint32"/>
                  <config value="false"/>
                  <description>
                    <text>XC index.</text>
                  </description>
                </leaf>
                <leaf name="delay-time-remain">
                  <type name="uint32"/>
                  <units name="ms"/>
                  <config value="false"/>
                  <description>
                    <text>Delay delete timer remain.</text>
                  </description>
                </leaf>
                <leaf name="state">
                  <type name="string">
                    <length value="1..32"/>
                  </type>
                  <config value="false"/>
                  <description>
                    <text>Segment list state.</text>
                  </description>
                </leaf>
                <leaf name="bfd-state">
                  <type name="string">
                    <length value="1..32"/>
                  </type>
                  <config value="false"/>
                  <description>
                    <text>Segment list BFD state.</text>
                  </description>
                </leaf>
                <leaf name="exp-value">
                  <type name="uint32"/>
                  <config value="false"/>
                  <description>
                    <text>EXP value.</text>
                  </description>
                </leaf>
                <leaf name="ttl-value">
                  <type name="uint32"/>
                  <config value="false"/>
                  <description>
                    <text>TTL value.</text>
                  </description>
                </leaf>
                <container name="sids">
                  <when condition="../../../../../endpoint and ../../../../../color and ../../../protocol-origin and ../../../asn and ../../../node-address and ../../../discriminator and ../../../preference and ../id"/>
                  <config value="false"/>
                  <description>
                    <text>List of SIDs.</text>
                  </description>
                  <list name="sid">
                    <key value="index"/>
                    <config value="false"/>
                    <max-elements value="10"/>
                    <description>
                      <text>Operational data of SID.</text>
                    </description>
                    <leaf name="index">
                      <type name="uint32">
                        <range value="0..4294967295"/>
                      </type>
                      <config value="false"/>
                      <description>
                        <text>Index.</text>
                      </description>
                    </leaf>
                    <leaf name="value">
                      <type name="uint32">
                        <range value="16..1048575"/>
                      </type>
                      <config value="false"/>
                      <description>
                        <text>Value.</text>
                      </description>
                    </leaf>
                  </list>
                </container>
              </list>
            </container>
          </list>
        </container>
      </list>
    </container>
