                ;
  }
  container configuration {
    description 'The branch of configuration model.'
                ;
    container p2mp-te {
      description 'The configuration of P2MP TE.'
                  ;
      leaf te-enable {
        description 'The global switch of TE.'
                    ;
        type enumeration {
          enum disable {
            value 0 ;
          }
          enum enable {
            value 1 ;
          }
        }
        default disable ;
      }
      leaf mvpn-e2e-enable {
        description 'The endtoend(E2E) protect global configuration of MVPN mtu'
                  + 'nnel.'
                    ;
        type enumeration {
          enum disable {
            value 0 ;
          }
          enum enable {
            value 1 ;
          }
        }
        default disable ;
        ncd:revise-date 2017-02-21 ;
      }
      leaf mvpn-frr-block {
        description 'The fast-reroute(FRR) block global configuration of MVPN m'
                  + 'tunnel.'
                    ;
        type enumeration {
          enum disable {
            value 0 ;
          }
          enum enable {
            value 1 ;
          }
        }
        default disable ;
        ncd:revise-date 2019-03-27 ;
      }
      container mvpn-bfd {
        description 'The BFD global configuration of MVPN mtunnel.'
                    ;
        presence 'bfd of mvpn tunnel'
                 ;
        leaf enable {
          description "To enable the bfd of MVPN tunnel when it's true.And to d"
                    + "isable the bfd of MVPN tunnel when it's false."
                      ;
          type enumeration {
            enum false {
              value 0 ;
            }
            enum true {
              value 1 ;
            }
          }
          mandatory true ;
        }
        choice detect-param {
          description 'Detect param.'
                      ;
          mandatory true ;
          case template {
            description 'Bfd template name.'
                        ;
            leaf template-name {
              description 'The tunnel bfd template name.'
                          ;
              type string {
                length 1..31 ;
              }
              mandatory true ;
            }
            ncd:revise-date 2020-04-18 ;
          }
          case detect-params {
            description 'Detect params.'
                        ;
            leaf mintx-interval {
              description 'The interval of sending BFD control packets.'
                          ;
              type uint32 {
                range 3..990 ;
              }
              units ms ;
              mandatory true ;
            }
            leaf minrx-interval {
              description 'The interval of reciving BFD control packets.'
                          ;
              type uint32 {
                range 3..990 ;
              }
              units ms ;
              mandatory true ;
            }
            leaf multiplier {
              description 'The multiplier of BFD detecting interval.'
                          ;
              type uint16 {
                range 3..50 ;
              }
              mandatory true ;
            }
            ncd:revise-date 2017-02-21 ;
          }
          ncd:revise-date 2017-02-21 ;
        }
        ncd:revise-date 2017-02-21 ;
      }
      container mte-tunnel {
        description 'The configration of the P2MP TE tunnel.'
                    ;
        list root-tunnel {
          description 'The configration of root tunnel.'
                      ;
          key 'tunnel-id tunnel-name'
              ;
          leaf tunnel-id {
            description 'The index of tunnel.'
                        ;
            type uint32 {
              range 1..65535 ;
            }
          }
          leaf tunnel-name {
            description 'Tunnel name.'
                        ;
            type ncd:ifname {
              length 0..31 ;
            }
          }
          leaf integrity {
            description 'The integrity of tunnel.'
                        ;
            type enumeration {
              enum false {
                value 0 ;
              }
              enum true {
                value 1 ;
              }
            }
            default false ;
          }
          leaf record-route {
            description 'The record-route of tunnel.'
                        ;
            type enumeration {
              enum false {
                value 0 ;
              }
              enum true {
                value 1 ;
              }
            }
            default false ;
          }
          leaf resv-style {
            description 'The resv-style of tunnel.'
                        ;
            type enumeration {
              enum se {
                value 1 ;
              }
              enum ff {
                value 2 ;
              }
            }
            default se ;
          }
          leaf crankback {
            description 'The crankback mode of tunnel.'
                        ;
            type enumeration {
              enum end-to-end {
                value 1 ;
                description end-to-end ;
              }
              enum disabled {
                value 0 ;
              }
            }
            default disabled ;
          }
          leaf reoptimize {
            description 'The reoptimization of tunnel.'
                        ;
            type enumeration {
              enum false {
                value 0 ;
              }
              enum true {
                value 1 ;
              }
            }
            default false ;
          }
          leaf frr {
            description 'The frr of tunnel.'
                        ;
            type enumeration {
              enum false {
                value 0 ;
              }
              enum true {
                value 1 ;
              }
            }
            default false ;
          }
          leaf escape-path {
            description 'The escape-path of tunnel.'
                        ;
            type enumeration {
              enum disabled {
                value 0 ;
              }
              enum enabled {
                value 1 ;
              }
            }
            default disabled ;
          }
          leaf e2e-protect {
            description 'The endtoend(e2e) protection of mtunnel.'
                        ;
            type enumeration {
              enum false {
                value 0 ;
              }
              enum true {
                value 1 ;
              }
            }
            default false ;
            ncd:revise-date 2015-12-03 ;
          }
          choice list-type {
            description 'DestList Selection Type.'
                        ;
            case name {
              description 'Based on the name of the destlist selection.'
                          ;
              leaf list-name {
                description 'The related destlist name.'
                            ;
                type string {
                  length 1..16 ;
                }
              }
            }
            case id {
              description 'Id-based destlist selection.'
                          ;
              leaf list-id {
                description 'The related destlist ID.'
                            ;
                type uint16 ;
              }
            }
          }
          container priority {
            description 'The priority of tunnel.'
                        ;
            presence 'priority of this tunnel'
                     ;
            leaf setup-priority {
              description "The tunnel setup priority.This attribute can't be hi"
                        + "gher than the holding-priority.0 is the highest prio"
                        + "rity and 7 isthe lowest priority."
                          ;
              type uint8 {
                range 0..7 ;
              }
              mandatory true ;
            }
            leaf hold-priority {
              description 'The tunnel holding priority.The tunnel of lower hold'
                        + 'ing-priority will be preempted by the tunnel of high'
                        + 'er setup-priority.0 is the highest priority and 7 is'
                        + ' the lowest priority.'
                          ;
              type uint8 {
                range 0..7 ;
              }
              mandatory true ;
            }
          }
          container bfd {
            description 'The bfd of tunnel.'
                        ;
            presence 'bfd of this tunnel'
                     ;
            leaf bfd {
              description "To enable the bfd of tunnel when it's true.And to di"
                        + "sable the bfd of
\ntunnel when it's false."
                          ;
              type enumeration {
                enum false {
                  value 0 ;
                }
                enum true {
                  value 1 ;
                }
              }
              mandatory true ;
            }
            choice detect-param {
              description 'Detect param.'
                          ;
              mandatory true ;
              case template {
                description 'Bfd template name.'
                            ;
                leaf template-name {
                  description 'The tunnel bfd template name.'
                              ;
                  type string {
                    length 1..31 ;
                  }
                  mandatory true ;
                }
                ncd:revise-date 2020-04-18 ;
              }
              case detect-params {
                description 'Detect params.'
                            ;
                leaf mintx-interval {
                  description 'The interval of sending BFD control packets.'
                              ;
                  type uint32 {
                    range 3..990 ;
                  }
                  units ms ;
                  mandatory true ;
                }
                leaf minrx-interval {
                  description 'The interval of reciving BFD control packets.'
                              ;
                  type uint32 {
                    range 3..990 ;
                  }
                  units ms ;
                  mandatory true ;
                }
                leaf multiplier {
                  description 'The multiplier of BFD detecting interval.'
                              ;
                  type uint16 {
                    range 3..50 ;
                  }
                  mandatory true ;
                }
                ncd:revise-date 2015-12-03 ;
              }
              ncd:revise-date 2015-12-03 ;
            }
            ncd:revise-date 2015-12-03 ;
          }
          leaf flow-statistics {
            description 'The flow statistics of root tunnel.'
                        ;
            type enumeration {
              enum false {
                value 0 ;
              }
              enum true {
                value 1 ;
              }
            }
            default false ;
            ncd:revise-date 2016-02-04 ;
          }
          ncd:select 'tunnel-id tunnel-name'
                     ;
        }
        list leaf-tunnel {
          description 'The configration of leaf tunnel.'
                      ;
          key 'tunnel-id tunnel-name'
              ;
          leaf tunnel-id {
            description 'The index of tunnel.'
                        ;
            type uint32 {
              range 1..65535 ;
            }
          }
          leaf tunnel-name {
            description 'Tunnel name.'
                        ;
            type ncd:ifname {
              length 0..31 ;
            }
          }
          container list-type {
            description 'The container which have three leaf ,there need to con'
                      + 'fig at the sametimes.'
                        ;
            presence necessary ;
            leaf root-lsr-ip-addr {
              description 'The ip address which is bind the root tunnel.'
                          ;
              type ncd:ipv4-address {
                pattern '([1-9]|[1-9][0-9]|1[0-9][0-9]|2[0-4][0-9]|25[0-5])(.(['
                      + '0-9]|[1-9][0-9]|1[0-9][0-9]|2[0-4][0-9]|25[0-5])){3}'
                        {
                  description 'To filter the invalid address.'
                              ;
                }
              }
              mandatory true ;
            }
            leaf root-lsr-tnnl-id {
              description 'The tunnel id which is bind with root tunnel.'
                          ;
              type uint16 {
                range 1..65535 ;
              }
              mandatory true ;
            }
            leaf root-lsr-p2mp-id {
              description 'The p2mp id which is bind with root tunnel.'
                          ;
              type uint32 {
                range 1..4294967295 ;
              }
              mandatory true ;
            }
          }
          leaf flow-statistics {
            description 'The flow statistics of leaf tunnel.'
                        ;
            type enumeration {
              enum false {
                value 0 ;
              }
              enum true {
                value 1 ;
              }
            }
            default false ;
            ncd:revise-date 2016-02-04 ;
          }
          ncd:select 'tunnel-id tunnel-name'
                     ;
        }
      }
      container destlist {
        description 'The list of destination.'
                    ;
        list name-based {
          description 'Based on the name of destlist.'
                      ;
          key name ;
          leaf name {
            description 'The name of destlist.'
                        ;
            type string {
              length 1..16 ;
            }
          }
          ncd:select name ;
        }
        list id-based {
          description 'Based on the id of destlist.'
                      ;
          key id ;
          leaf id {
            description 'The id of destlist.'
                        ;
            type uint16 {
              range 1..65535 ;
            }
          }
          ncd:select id ;
        }
      }
      container destip {
        description 'Dest ip.'
                    ;
        list name-based {
          description 'Based on the name of destip.'
                      ;
          key 'name ip-addr'
              ;
          leaf name {
            description 'The name of destlist.'
                        ;
            type string {
              length 1..16 ;
            }
          }
          leaf ip-addr {
            description 'The ip address of destip.'
                        ;
            type ncd:ipv4-address ;
          }
          leaf path-option {
            description 'The attribute is the path-option.'
                        ;
            type uint16 {
              range 1..16 ;
            }
            mandatory true ;
          }
          choice path-type {
            description 'Path Selection Type.'
                        ;
            mandatory true ;
            case path-name {
              description 'Based on the name of the explicit path selection.'
                          ;
              leaf explicit-path-name {
                description 'The related explict path name.'
                            ;
                type string {
                  length 1..64 ;
                }
                mandatory true ;
              }
            }
            case path-id {
              description 'Id-based explicit path selection.'
                          ;
              leaf explicit-path-id {
                description 'The related explicit path ID.'
                            ;
                type uint16 {
                  range 1..65535 ;
                }
                mandatory true ;
              }
            }
            case dynamic {
              description 'Dynamic Path Selection.'
                          ;
