              </description>
            </leaf>
            <leaf name="binding-sid">
              <type name="uint32"/>
              <config value="false"/>
              <description>
                <text>Candidate path binding SID.</text>
              </description>
            </leaf>
            <leaf name="group-id">
              <type name="uint32"/>
              <config value="false"/>
              <description>
                <text>Candidate path groupId.</text>
              </description>
            </leaf>
