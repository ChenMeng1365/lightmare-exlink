  <typedef name="create-type">
    <type name="enumeration">
      <enum name="session-static">
        <value value="0"/>
        <description>
          <text>A BFD session is created in static mode.</text>
        </description>
      </enum>
      <enum name="session-auto">
        <value value="1"/>
        <description>
