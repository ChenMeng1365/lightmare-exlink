  <typedef name="threshold-type">
    <type name="enumeration">
      <enum name="all-threshold">
        <value value="0"/>
        <description>
          <text>All threshold.</text>
        </description>
      </enum>
      <enum name="upper-threshold">
        <value value="1"/>
        <description>
          <text>Upper threshold.</text>
        </description>
      </enum>
      <enum name="lower-threshold">
        <value value="2"/>
        <description>
