                </description>
              </leaf>
              <choice name="level">
                <default value="auto"/>
                <description>
                  <text>Interface physical status damping level.</text>
                </description>
                <case name="auto">
                  <description>
                    <text>Auto damping level.</text>
                  </description>
                  <container name="auto">
                    <description>
                      <text>Configure an auto suppression level.</text>
                    </description>
                    <leaf name="level">
                      <type name="damp-level-type"/>
                      <default value="light"/>
                      <description>
                        <text>Auto damping level.</text>
                      </description>
                    </leaf>
                  </container>
                </case>
                <case name="manual">
                  <description>
                    <text>Manual damping level.</text>
                  </description>
                  <container name="manual">
                    <description>
                      <text>Configure a manual suppression level.</text>
                    </description>
                    <leaf name="suppress">
                      <type name="uint32">
                        <range value="1..20000"/>
                      </type>
                      <must condition="(../suppress&gt;../reuse)"/>
                      <mandatory value="true"/>
                      <description>
                        <text>1000 times of suppress threshold.</text>
                      </description>
                    </leaf>
                    <leaf name="reuse">
                      <type name="uint32">
                        <range value="1..20000"/>
                      </type>
                      <must condition="(../reuse&lt;../suppress)"/>
                      <mandatory value="true"/>
                      <description>
                        <text>1000 times of reuse threshold.</text>
                      </description>
                    </leaf>
                    <leaf name="max-suppress-time">
                      <type name="uint16">
                        <range value="1..255"/>
                      </type>
                      <units name="s"/>
                      <mandatory value="true"/>
                      <description>
                        <text>Max suppress time.</text>
                      </description>
                    </leaf>
                    <leaf name="half-life-period">
                      <type name="uint16">
                        <range value="1..60"/>
                      </type>
                      <units name="s"/>
                      <mandatory value="true"/>
                      <description>
                        <text>Half life time.</text>
                      </description>
                    </leaf>
                  </container>
                </case>
              </choice>
            </container>
          </case>
        </choice>
