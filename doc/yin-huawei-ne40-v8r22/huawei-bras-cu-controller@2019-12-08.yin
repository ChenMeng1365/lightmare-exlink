  <grouping name="backup-profile-basic">
    <description>
      <text>CU backup profile basic attributes.</text>
    </description>
    <leaf name="ipv4-assigned-mode">
      <type name="ip-assigned-mode-type"/>
      <default value="by-backup-profile"/>
      <description>
        <text>IPv4 address allocation mode.</text>
      </description>
    </leaf>
    <leaf name="ipv6-assigned-mode">
      <type name="ip-assigned-mode-type"/>
      <default value="by-backup-profile"/>
      <description>
        <text>IPv6 address allocation mode.</text>
      </description>
    </leaf>
    <leaf name="nas-logic-port">
      <type name="pub-type:if-name"/>
      <description>
        <text>Name of an NAS logical interface. This interface can only be delivered as a GE or Eth-Trunk main interface.</text>
      </description>
    </leaf>
  </grouping>
  <grouping name="hot-up-status">
    <description>
      <text>Information about vBRAS-UP status in UP backup mode.</text>
    </description>
    <leaf name="up-id">
      <type name="uint32"/>
      <description>
        <text>UP ID.</text>
      </description>
    </leaf>
    <leaf name="state">
