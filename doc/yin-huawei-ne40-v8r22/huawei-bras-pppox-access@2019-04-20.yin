  <augment target-node="/aaa:aaa/aaa:domains/aaa:domain/bras-basic-access:bas">
    <description>
      <text>PPPoX configurations under domain.</text>
    </description>
    <container name="bras-pppox">
      <description>
        <text>Configure PPPoX under domain.</text>
      </description>
      <container name="address-release-separate">
        <description>
          <text>Configure PPPoE dual-stack users to go offline from a single stack.</text>
        </description>
        <leaf name="enable">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable PPPoE dual-stack users to go offline from a single stack.</text>
          </description>
        </leaf>
      </container>
      <container name="force-authen-type">
        <description>
          <text>Configure indicates the mode of PPP authentication for a PPPoE user in a domain.</text>
        </description>
        <leaf name="force-authentication-type">
          <type name="authentication-type"/>
          <default value="none"/>
          <description>
            <text>Indicates the mode of PPP authentication for a PPPoE user in a domain.</text>
          </description>
        </leaf>
      </container>
      <container name="force-assign-interface-id">
        <description>
          <text>Configure the router to allocate interface IDs and reject that are not 0 and do not conflict with one another carried in Configure-Request messages sent from users during IPv6CP negotiation.</text>
        </description>
        <leaf name="assign-interface-id">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable allocates interface IDs during IPv6CP negotiation.</text>
          </description>
        </leaf>
      </container>
      <container name="dhcpv6-wait-delay">
        <description>
          <text>Configure a timeout period for waiting for a DHCPv6 connection request from a PPPoE user on the.</text>
        </description>
        <choice name="wait-time-set-type">
          <default value="limited"/>
          <description>
            <text>Limited type or infinity type.</text>
          </description>
          <case name="limited">
            <description>
              <text>Limited type.</text>
            </description>
            <leaf name="wait-dhcpv6-time">
              <type name="uint8">
                <range value="30..120"/>
              </type>
              <units name="s"/>
              <default value="60"/>
              <description>
                <text>Specifys the timeout period for waiting for a DHCPv6 connection request.</text>
              </description>
            </leaf>
          </case>
          <case name="infinity">
            <description>
              <text>Infinity type.</text>
            </description>
            <leaf name="infinity">
              <type name="empty"/>
              <description>
