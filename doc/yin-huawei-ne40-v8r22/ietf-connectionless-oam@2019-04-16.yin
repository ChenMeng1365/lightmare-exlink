        </description>
      </when>
      <container name="test-point-ipv6-location-list">
        <list name="test-point-locations">
          <key value="ipv6-location ni"/>
          <leaf name="ipv6-location">
            <type name="inet:ipv6-address"/>
            <description>
              <text>IPv6 address.</text>
            </description>
          </leaf>
          <leaf name="ni">
            <type name="routing-instance-ref"/>
            <description>
              <text>The ni is used to describe the
corresponding network instance.</text>
            </description>
          </leaf>
          <uses name="test-point-location-info"/>
          <description>
            <text>List of test point locations.</text>
          </description>
        </list>
        <description>
          <text>Serves as top-level container
for test point location list.</text>
        </description>
      </container>
      <description>
        <text>ipv6 location type container.</text>
      </description>
    </container>
    <container name="mac-location-type">
      <when condition="derived-from-or-self(../tp-location-type,'cl-oam:mac-address-type')">
        <description>
          <text>When test point location type is equal to MAC address.</text>
        </description>
      </when>
      <container name="test-point-mac-address-location-list">
        <list name="test-point-locations">
          <key value="mac-address-location"/>
          <leaf name="mac-address-location">
            <type name="yang:mac-address"/>
            <description>
              <text>MAC address.</text>
            </description>
          </leaf>
          <uses name="test-point-location-info"/>
          <description>
            <text>List of test point locations.</text>
          </description>
        </list>
        <description>
          <text>Serves as top-level container
for test point location list.</text>
        </description>
      </container>
      <description>
        <text>Container for MAC address location types.</text>
      </description>
    </container>
    <container name="group-as-number-location-type">
      <when condition="derived-from-or-self(../tp-location-type,'cl-oam:as-number-address-type')">
        <description>
          <text>When test point location type is equal to AS number.</text>
        </description>
      </when>
      <container name="test-point-as-number-location-list">
        <list name="test-point-locations">
          <key value="as-number-location"/>
          <leaf name="as-number-location">
            <type name="inet:as-number"/>
            <description>
              <text>AS number for point-to-multipoint OAM.</text>
            </description>
          </leaf>
          <leaf name="ni">
            <type name="routing-instance-ref"/>
            <description>
              <text>The ni is used to describe the
corresponding network instance.</text>
            </description>
          </leaf>
          <uses name="test-point-location-info"/>
          <description>
            <text>List of test point locations.</text>
          </description>
        </list>
        <description>
          <text>Serves as top-level container
for test point location list.</text>
        </description>
      </container>
      <description>
        <text>Container for AS number location types.</text>
      </description>
    </container>
    <container name="group-router-id-location-type">
      <when condition="derived-from-or-self(../tp-location-type,'cl-oam:router-id-address-type')">
        <description>
