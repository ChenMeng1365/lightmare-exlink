  namespace 'http://www.zte.com.cn/zxr10/netconf/schema/rosng/vxlan'
            ;
  prefix vxlan ;
  yang-version 1 ;
  import zxr10-netconf-defs {
    prefix ncd ;
  }
  organization 'ZTE Corporation'
               ;
  contact 'ZTE Corporation  '+"\n"
        + '   Nanjing Institute of ZTE Corporation '+"\n"
        + '   No.68 Zijinghua Rd. Yuhuatai District, Nanjing, China '+"\n"
        + '   Tel: +86-25-52870000'
          ;
  description 'The netconf module to describe VXLAN.'
              ;
  revision 2020-04-09 {
    description '1.Add a new select evpn-tunnel-id on zxr10-vxlan/state/vxlan/v'
              + 'xlan-tunnels;'
                ;
  }
