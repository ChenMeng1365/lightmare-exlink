{
    "swagger": "2.0",
    "info": {
        "description": "huawei-dhcp-snooping API generated from huawei-dhcp-snooping@2020-02-25.yang",
        "version": "1.0.0",
        "title": "huawei-dhcp-snooping API"
    },
    "host": "localhost:8080",
    "basePath": "/restconf",
    "schemes": [
        "http"
    ],
    "paths": {},
    "definitions": {
        "dhcp-snooping-type": {
            "description": "Configure DHCP snooping attributes.",
            "properties": {
                "global": {
                    "description": "Configure global attributes of DHCP snooping.",
                    "properties": {
                        "enable": {
                            "type": "boolean",
                            "description": "Enable/disable DHCP snooping globally."
                        },
                        "server-detect-enable": {
                            "type": "boolean",
                            "description": "Enable/disable the check on bogus DHCP servers."
                        },
                        "user-bind-auto-save-enable": {
                            "type": "boolean",
                            "description": "Enable/disable automatic saving of DHCP snooping binding entries."
                        },
                        "user-bind-file-name": {
                            "type": "string",
                            "description": "File name of saving the snooping binding table."
                        },
                        "alarm-threshold": {
                            "type": "string",
                            "description": "Global alarm threshold for DHCP packets."
                        },
                        "arp-detect-enable": {
                            "type": "boolean",
                            "description": "Enable/disable ARP detection for DHCP users based on DHCP snooping binding entries."
                        },
                        "mac-strict-check-enable": {
                            "type": "boolean",
                            "description": "Enable/disable MAC strict check."
                        },
                        "unicast-cpu-forward-disable": {
                            "type": "boolean",
                            "description": "Enable/disable CPU forwarding for DHCP snooping unicast packets."
                        },
                        "file-authentication-mode": {
                            "type": "string",
                            "enum": [
                                "check",
                                "no-check",
                                "force-check"
                            ],
                            "description": "Set file authentication mode."
                        }
                    }
                },
                "white-lists": {
                    "description": "List of whitelists.",
                    "properties": {
                        "white-list": {
                            "description": "Configure a whitelist.",
                            "type": "array",
                            "items": {
                                "properties": {
                                    "white-list-name": {
                                        "type": "string",
                                        "description": "Whitelist name."
                                    },
                                    "apply-flag": {
                                        "type": "boolean",
                                        "description": "Enable/disable to determine whether to use whitelists."
                                    },
                                    "white-rules": {
                                        "description": "List of whitelist rules.",
                                        "properties": {
                                            "white-rule": {
                                                "description": "Configure a whitelist rule.",
                                                "type": "array",
                                                "items": {
                                                    "properties": {
                                                        "rule-id": {
                                                            "type": "string",
                                                            "description": "Whitelist rule ID."
                                                        },
                                                        "source-ip": {
                                                            "type": "string",
                                                            "description": "Source address."
                                                        },
                                                        "source-mask": {
                                                            "type": "string",
                                                            "description": "Source address mask."
                                                        },
                                                        "destination-address": {
                                                            "type": "string",
                                                            "description": "Destination address."
                                                        },
                                                        "destination-mask": {
                                                            "type": "string",
                                                            "description": "Destination address mask."
                                                        },
                                                        "source-port": {
                                                            "type": "string",
                                                            "enum": [
                                                                "bootps",
                                                                "bootpc"
                                                            ],
                                                            "description": "Source port."
                                                        },
                                                        "destination-port": {
                                                            "type": "string",
                                                            "enum": [
                                                                "bootps",
                                                                "bootpc"
                                                            ],
                                                            "description": "Destination port."
                                                        }
                                                    }
                                                },
                                                "x-key": "rule-id"
                                            }
                                        }
                                    }
                                }
                            },
                            "x-key": "white-list-name"
                        }
                    }
                }
            }
        }
    }
}