          <description>
            <text>The low threshold of tx-pause error alarm threshold. The number of error packets counted in the time in interval less than this threshold will recover tx-pause-error alarm.</text>
          </description>
        </leaf>
        <leaf name="tx-pause-error-interval">
          <type name="uint32">
            <range value="1..65535"/>
          </type>
          <units name="s"/>
          <default value="10"/>
          <description>
            <text>Time interval for statistical tx-pause-error alarm.</text>
          </description>
        </leaf>
        <leaf name="rx-pause-error-high-threshold">
          <type name="uint32">
            <range value="1..4294967295"/>
          </type>
          <must condition="../rx-pause-error-high-threshold &gt;= ../rx-pause-error-low-threshold"/>
          <default value="500"/>
          <description>
            <text>The high threshold of rx-pause error alarm threshold. The number of error packets counted in the time in interval exceeds this threshold will produce rx-pause-error alarm.</text>
          </description>
        </leaf>
        <leaf name="rx-pause-error-low-threshold">
          <type name="uint32">
            <range value="1..4294967295"/>
          </type>
          <must condition="../rx-pause-error-high-threshold &gt;= ../rx-pause-error-low-threshold"/>
          <default value="100"/>
          <description>
            <text>The low threshold of rx-pause error alarm threshold. The number of error packets counted in the time in interval less than this threshold will recover rx-pause-error alarm.</text>
          </description>
        </leaf>
        <leaf name="rx-pause-error-interval">
          <type name="uint32">
            <range value="1..65535"/>
          </type>
          <units name="s"/>
          <default value="10"/>
          <description>
            <text>Time interval for statistical rx-pause-error alarm.</text>
          </description>
        </leaf>
      </container>
      <container name="sdh-port-alarm">
        <description>
          <text>Configure SDH port alarm information.</text>
        </description>
        <leaf name="sdh-enabled-down">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable SDH error enable down.</text>
          </description>
        </leaf>
        <leaf name="sdh-error-high-threshold">
          <type name="uint32">
            <range value="1..4294967295"/>
          </type>
          <must condition="../sdh-error-high-threshold &gt;= ../sdh-error-low-threshold"/>
          <default value="1000"/>
          <description>
            <text>The high threshold of SDH error alarm threshold. The number of error packets counted in the time in interval exceeds this threshold will produce sdh-error alarm.</text>
          </description>
        </leaf>
        <leaf name="sdh-error-low-threshold">
          <type name="uint32">
            <range value="1..4294967295"/>
          </type>
          <must condition="../sdh-error-high-threshold &gt;= ../sdh-error-low-threshold"/>
          <default value="100"/>
          <description>
            <text>The low threshold of SDH error alarm threshold. The number of error packets counted in the time in interval less than this threshold will recover sdh-error alarm.</text>
          </description>
        </leaf>
        <leaf name="sdh-error-interval">
          <type name="uint32">
            <range value="1..65535"/>
          </type>
          <units name="s"/>
          <default value="10"/>
          <description>
            <text>Time interval for statistical sdh-error alarm.</text>
          </description>
        </leaf>
        <leaf name="sdh-b1-err-threshold">
          <type name="uint32">
            <range value="1..65535"/>
          </type>
          <units name="s"/>
          <default value="1000"/>
          <description>
            <text>B1 Error Threshold.</text>
          </description>
        </leaf>
        <leaf name="sdh-b2-err-threshold">
          <type name="uint32">
            <range value="1..65535"/>
          </type>
          <units name="s"/>
          <default value="1000"/>
          <description>
            <text>B2 Error Threshold.</text>
          </description>
        </leaf>
        <leaf name="sdh-b1-err-interval">
          <type name="uint32">
            <range value="1..65535"/>
          </type>
          <units name="s"/>
          <default value="10"/>
          <description>
            <text>B1 error interval.</text>
          </description>
        </leaf>
        <leaf name="sdh-b2-err-interval">
          <type name="uint32">
            <range value="1..65535"/>
          </type>
          <units name="s"/>
          <default value="10"/>
          <description>
            <text>B2 error interval.</text>
          </description>
        </leaf>
      </container>
      <container name="pdh-port-alarm">
        <must condition="trigger-thres &gt; resume-thres or (not(trigger-thres) and not(resume-thres))"/>
        <description>
          <text>Configure PDH port alarm information.</text>
        </description>
        <leaf name="trigger-thres">
          <type name="uint32">
            <range value="1..900"/>
          </type>
          <default value="65"/>
          <description>
            <text>The value of pdh-port-alarm trigger-threshold.</text>
          </description>
        </leaf>
        <leaf name="resume-thres">
          <type name="uint32">
            <range value="1..899"/>
          </type>
          <default value="30"/>
          <description>
            <text>The value of pdh-port-alarm resume-threshold.</text>
          </description>
        </leaf>
      </container>
      <container name="crc-mfe-alarm">
        <when condition="../crc-mfs-alarm"/>
        <must condition="(crc-mfe-alm-thr-coefficient &gt; crc-mfe-rsum-thr-coefficient and crc-mfe-alm-thr-power = crc-mfe-rsum-thr-power) or (crc-mfe-alm-thr-power &lt; crc-mfe-rsum-thr-power)"/>
        <presence value="Enable mfe alarm threshold."/>
        <description>
          <text>Enable/disable CRC MFE ALARM.</text>
        </description>
        <leaf name="crc-mfe-alm-thr-coefficient">
          <ext:dynamic-default/>
          <type name="uint32">
            <range value="1..9"/>
          </type>
          <mandatory value="true"/>
          <description>
            <text>Mac fcs exc coefficient (x of xE-y). The range is 1 to 9.</text>
          </description>
        </leaf>
        <leaf name="crc-mfe-alm-thr-power">
          <ext:dynamic-default/>
          <type name="uint32">
            <range value="2..5"/>
          </type>
          <mandatory value="true"/>
          <description>
            <text>Mac fcs exc exponent (y of xE-y). The range is 2 to 5.</text>
          </description>
        </leaf>
        <leaf name="crc-mfe-rsum-thr-coefficient">
          <ext:dynamic-default/>
          <type name="uint32">
            <range value="1..9"/>
          </type>
          <mandatory value="true"/>
          <description>
            <text>Mac fcs exc coefficient (x of xE-y). The range is 1 to 9.</text>
          </description>
        </leaf>
        <leaf name="crc-mfe-rsum-thr-power">
          <ext:dynamic-default/>
          <type name="uint32">
            <range value="2..6"/>
          </type>
          <mandatory value="true"/>
          <description>
            <text>For resume-threshold: mac fcs exc exponent (y of xE-y). The range is 2 to 6.</text>
          </description>
        </leaf>
        <leaf name="crc-mfe-alm-lsp-en">
          <ext:dynamic-default/>
          <type name="boolean"/>
          <mandatory value="true"/>
          <description>
            <text>Enable/disable configure MFE (mac fcs exc) of crc-error trigger LSP.</text>
          </description>
        </leaf>
      </container>
      <container name="crc-sample">
        <when condition="../crc-per-alarm"/>
        <must condition="crc-per-child-win-alarm-num &lt;= crc-per-child-win-max-num and crc-per-child-win-resume-num &lt;= crc-per-child-win-max-num"/>
        <description>
          <text>Configure crc error windows.</text>
        </description>
        <leaf name="crc-per-samp-win-factor">
          <ext:dynamic-default/>
          <type name="uint32">
            <range value="10..100"/>
          </type>
          <description>
            <text>Sample window factor.</text>
          </description>
        </leaf>
        <leaf name="crc-per-child-win-max-num">
          <ext:dynamic-default/>
          <type name="uint32">
            <range value="10..100"/>
          </type>
          <description>
            <text>Maximum number of child windows.</text>
          </description>
        </leaf>
        <leaf name="crc-per-child-win-alarm-num">
          <ext:dynamic-default/>
          <type name="uint32">
            <range value="1..100"/>
          </type>
          <description>
            <text>Minimal number of child windows for alarm generating.</text>
          </description>
        </leaf>
        <leaf name="crc-per-child-win-resume-num">
          <ext:dynamic-default/>
          <type name="uint32">
            <range value="1..100"/>
          </type>
          <description>
            <text>Minimal number of child windows for alarm resuming.</text>
          </description>
        </leaf>
        <leaf name="crc-alg-realtime-factor">
          <ext:dynamic-default/>
          <type name="uint32">
            <range value="0..10"/>
          </type>
          <description>
            <text>Realtime factor template for crc-error algorithm.</text>
          </description>
        </leaf>
      </container>
      <choice name="alarm-type">
        <default value="crc-error-threshold"/>
        <description>
          <text>CRC alarm config.</text>
        </description>
        <case name="crc-error-threshold">
          <description>
            <text>Configure set crc error threshold.</text>
          </description>
          <container name="crc-error-threshold">
            <description>
              <text>Configure set crc error threshold.</text>
            </description>
            <leaf name="crc-error-high">
              <type name="uint32">
                <range value="1..4294967295"/>
              </type>
              <must condition="(../crc-error-high &gt;= ../crc-error-low) and not(../crc-error-percent-alm-thr)"/>
              <description>
                <text>The high threshold of CRC error alarm threshold. The number of error packets counted in the time in interval exceeds this threshold will produce crc-error alarm.</text>
              </description>
            </leaf>
            <leaf name="crc-error-low">
              <type name="uint32">
                <range value="1..4294967295"/>
              </type>
              <must condition="(../crc-error-high &gt;= ../crc-error-low) and not(../crc-error-percent-alm-thr)"/>
              <description>
                <text>The low threshold of CRC error alarm threshold. The number of error packets counted in the time in interval less than this threshold will recover crc-error alarm.</text>
              </description>
            </leaf>
            <leaf name="crc-error">
              <type name="uint32">
                <range value="1..65535"/>
              </type>
              <description>
                <text>CRC error threshold.</text>
              </description>
            </leaf>
            <leaf name="crc-error-interval">
              <type name="uint32">
                <range value="1..65535"/>
              </type>
              <units name="s"/>
              <description>
                <text>Time interval for statistical crc-error alarm.</text>
              </description>
            </leaf>
            <leaf name="crc-shutdown-en">
              <type name="boolean"/>
              <must condition="not(../crc-error-percent-alm-thr)"/>
              <description>
                <text>Enable/disable crc shutdown. The port will shutdown when the number of error packets counted in the time in interval exceeds threshold.</text>
              </description>
            </leaf>
            <leaf name="crc-error-percent-alm-thr">
              <type name="uint32">
                <range value="1..100"/>
              </type>
              <units name="%"/>
              <must condition="not(../crc-error-high) and not(../crc-error-low) and not(../crc-error-interval) and not(../crc-shutdown-en)"/>
              <description>
                <text>Crc-error alarm percent threshold, Higher than this threshold will produce crc-error alarm, lower than this threshold will recover crc-error alarm.</text>
              </description>
            </leaf>
          </container>
        </case>
        <case name="link-quality">
          <description>
            <text>Enable/disable set link quality threshold.</text>
          </description>
          <container name="link-quality">
            <must condition="(link-quality-alm-thr-coefficient &gt; link-quality-rsum-thr-coefficient and link-quality-alm-thr-power = link-quality-rsum-thr-power) or (link-quality-alm-thr-power &lt; link-quality-rsum-thr-power)"/>
            <presence value="Enable link quality threshold."/>
            <description>
              <text>Enable/disable set link quality threshold.</text>
            </description>
            <leaf name="link-quality-alm-thr-coefficient">
              <ext:dynamic-default/>
              <type name="uint32">
                <range value="1..9"/>
              </type>
              <mandatory value="true"/>
              <description>
                <text>Link-quality alarm Trigger coefficient.</text>
              </description>
            </leaf>
            <leaf name="link-quality-alm-thr-power">
              <ext:dynamic-default/>
              <type name="uint32">
                <range value="1..7"/>
              </type>
              <mandatory value="true"/>
              <description>
                <text>Link-quality alarm Trigger power.</text>
              </description>
            </leaf>
            <leaf name="link-quality-rsum-thr-coefficient">
              <ext:dynamic-default/>
              <type name="uint32">
                <range value="1..9"/>
              </type>
              <mandatory value="true"/>
              <description>
                <text>Link-quality alarm Recovery coefficient.</text>
              </description>
            </leaf>
            <leaf name="link-quality-rsum-thr-power">
              <ext:dynamic-default/>
              <type name="uint32">
                <range value="1..7"/>
              </type>
              <mandatory value="true"/>
              <description>
                <text>Link-quality alarm Recovery power.</text>
              </description>
            </leaf>
          </container>
        </case>
        <case name="crc-per-alarm">
          <description>
            <text>Enable/disable set crc packet-error-ratio alarm threshold.</text>
          </description>
          <container name="crc-per-alarm">
            <must condition="((crc-per-alm-thr-coefficient &gt; crc-per-rsum-thr-coefficient and crc-per-alm-thr-power = crc-per-rsum-thr-power) or (crc-per-alm-thr-power &lt; crc-per-rsum-thr-power))"/>
            <must condition="((crc-per-alm-sec-en='false' and crc-per-alm-lsp-en='true') or (crc-per-alm-sec-en='true' and crc-per-alm-lsp-en='false') or (crc-per-alm-sec-en='false' and crc-per-alm-lsp-en='false'))"/>
            <presence value="Enable crc alarm threshold."/>
            <description>
              <text>Enable/disable set crc packet-error-ratio alarm threshold.</text>
            </description>
            <leaf name="crc-per-alm-thr-coefficient">
              <type name="uint32">
                <range value="1..9"/>
              </type>
              <mandatory value="true"/>
              <description>
                <text>Packet error ratio coefficient (x of xE-y). The range is 1 to 9.</text>
              </description>
            </leaf>
            <leaf name="crc-per-alm-thr-power">
              <type name="uint32">
                <range value="1..7"/>
              </type>
              <mandatory value="true"/>
              <description>
                <text>Packet error ratio exponent (y of xE-y). The range is 1 to 7.</text>
              </description>
            </leaf>
            <leaf name="crc-per-rsum-thr-coefficient">
              <type name="uint32">
                <range value="1..9"/>
              </type>
              <mandatory value="true"/>
              <description>
                <text>Packet error ratio coefficient (x of xE-y). The range is 1 to 9.</text>
              </description>
            </leaf>
            <leaf name="crc-per-rsum-thr-power">
              <type name="uint32">
                <range value="1..8"/>
              </type>
              <mandatory value="true"/>
              <description>
                <text>For resume-threshold: Packet error ratio exponent (y of xE-y). The range is 1 to 8.</text>
              </description>
            </leaf>
            <leaf name="crc-per-alm-lsp-en">
              <type name="boolean"/>
              <mandatory value="true"/>
              <description>
                <text>Enable/disable configure PER (packet error ratio) of crc-error trigger LSP.</text>
              </description>
            </leaf>
            <leaf name="crc-per-alm-sec-en">
              <type name="boolean"/>
              <mandatory value="true"/>
              <description>
                <text>Enable/disable configure PER (packet error ratio) of crc-error trigger section.</text>
              </description>
            </leaf>
          </container>
          <container name="crc-per-warn">
            <must condition="((crc-per-war-thr-coefficient &gt; crc-per-war-rsum-thr-coefficient and crc-per-war-thr-power = crc-per-war-rsum-thr-power) or crc-per-war-thr-power &lt; crc-per-war-rsum-thr-power)"/>
            <must condition="((crc-per-war-thr-coefficient &gt; ../crc-per-alarm/crc-per-alm-thr-coefficient and crc-per-war-thr-power = ../crc-per-alarm/crc-per-alm-thr-power) or crc-per-war-thr-power &gt; ../crc-per-alarm/crc-per-alm-thr-power)"/>
            <must condition="((crc-per-war-rsum-thr-coefficient &lt; ../crc-per-alarm/crc-per-rsum-thr-coefficient and crc-per-war-rsum-thr-power = ../crc-per-alarm/crc-per-rsum-thr-power) or crc-per-war-rsum-thr-power &gt; ../crc-per-alarm/crc-per-rsum-thr-power)"/>
            <presence value="Enable crc warn threshold."/>
            <description>
              <text>Enable/disable set crc packet-error-ratio warn threshold.</text>
            </description>
            <leaf name="crc-per-war-thr-coefficient">
              <ext:dynamic-default/>
              <type name="uint32">
                <range value="1..9"/>
              </type>
              <mandatory value="true"/>
              <description>
                <text>Packet error ratio coefficient (x of xE-y). The range is 1 to 9.</text>
              </description>
            </leaf>
            <leaf name="crc-per-war-thr-power">
              <ext:dynamic-default/>
              <type name="uint32">
                <range value="1..7"/>
              </type>
              <mandatory value="true"/>
              <description>
                <text>Packet error ratio exponent (y of xE-y). The range is 1 to 7.</text>
              </description>
            </leaf>
            <leaf name="crc-per-war-rsum-thr-coefficient">
              <ext:dynamic-default/>
              <type name="uint32">
                <range value="1..9"/>
              </type>
              <mandatory value="true"/>
              <description>
                <text>Packet error ratio coefficient (x of xE-y). The range is 1 to 9.</text>
              </description>
            </leaf>
            <leaf name="crc-per-war-rsum-thr-power">
              <ext:dynamic-default/>
              <type name="uint32">
                <range value="1..8"/>
              </type>
              <mandatory value="true"/>
              <description>
                <text>For resume-threshold: Packet error ratio exponent (y of xE-y). The range is 1 to 8.</text>
              </description>
            </leaf>
          </container>
        </case>
        <case name="crc-mfs-alarm">
          <description>
            <text>Enable/disable CRC MFS ALARM.</text>
          </description>
          <container name="crc-mfs-alarm">
            <must condition="(crc-mfs-alm-thr-coefficient &gt; crc-mfs-rsum-thr-coefficient and crc-mfs-alm-thr-power = crc-mfs-rsum-thr-power) or (crc-mfs-alm-thr-power &lt; crc-mfs-rsum-thr-power)"/>
            <presence value="Enable mfs alarm threshold."/>
            <description>
              <text>Enable/disable CRC MFS ALARM.</text>
            </description>
            <leaf name="crc-mfs-alm-thr-coefficient">
              <type name="uint32">
                <range value="1..9"/>
              </type>
              <mandatory value="true"/>
              <description>
                <text>Mac fcs sd coefficient (x of xE-y). The range is 1 to 9.</text>
              </description>
            </leaf>
            <leaf name="crc-mfs-alm-thr-power">
              <type name="uint32">
                <range value="5..9"/>
              </type>
              <mandatory value="true"/>
              <description>
                <text>Mac fcs sd exponent (y of xE-y). The range is 5 to 9.</text>
              </description>
            </leaf>
            <leaf name="crc-mfs-rsum-thr-coefficient">
              <type name="uint32">
                <range value="1..9"/>
              </type>
              <mandatory value="true"/>
              <description>
                <text>Mac fcs sd coefficient (x of xE-y). The range is 1 to 9.</text>
              </description>
            </leaf>
            <leaf name="crc-mfs-rsum-thr-power">
              <type name="uint32">
                <range value="5..10"/>
              </type>
              <mandatory value="true"/>
              <description>
                <text>For resume-threshold: mac fcs sd exponent (y of xE-y). The range is 5 to 10.</text>
              </description>
            </leaf>
            <leaf name="crc-mfs-alm-lsp-en">
              <type name="boolean"/>
              <mandatory value="true"/>
              <description>
                <text>Enable/disable configure MFS (mac fcs sd) of crc-error trigger LSP.</text>
              </description>
            </leaf>
          </container>
        </case>
      </choice>
    </container>
    <container name="adm-port">
      <description>
        <text>Configure ADM Port.</text>
      </description>
      <leaf name="rx-power">
        <type name="string">
          <length value="0..64"/>
        </type>
        <config value="false"/>
        <description>
          <text>Receive power.</text>
        </description>
      </leaf>
      <leaf name="tx-power">
        <type name="string">
          <length value="0..64"/>
        </type>
        <config value="false"/>
        <description>
          <text>Transmit power.</text>
        </description>
      </leaf>
      <leaf name="descriton">
        <type name="string">
          <length value="1..242"/>
        </type>
        <description>
          <text>Adm port description.</text>
        </description>
      </leaf>
      <leaf name="wavelength">
        <type name="string">
          <length value="0..8"/>
        </type>
        <config value="false"/>
        <description>
          <text>Length of wave.</text>
        </description>
      </leaf>
      <leaf name="conect-relation">
        <type name="leafref">
          <path value="/ifm:ifm/ifm:interfaces/ifm:interface/ifm:name"/>
        </type>
        <description>
          <text>Connection relation.</text>
        </description>
      </leaf>
      <leaf name="direction">
        <type name="string">
          <length value="0..8"/>
        </type>
        <config value="false"/>
        <description>
          <text>Direction.</text>
        </description>
      </leaf>
      <leaf name="wave-band">
        <type name="string">
          <length value="0..8"/>
        </type>
        <config value="false"/>
        <description>
          <text>Wave band.</text>
        </description>
      </leaf>
      <leaf name="wavelength-num">
        <type name="string">
          <length value="0..8"/>
        </type>
        <config value="false"/>
        <description>
          <text>Wavelength number.</text>
        </description>
      </leaf>
      <leaf name="tx-wavelength">
        <type name="string">
          <length value="0..8"/>
        </type>
        <config value="false"/>
        <description>
          <text>Length of wave-TX.</text>
        </description>
      </leaf>
      <leaf name="rx-wavelength">
        <type name="string">
          <length value="0..8"/>
        </type>
        <config value="false"/>
        <description>
          <text>Length of wave-RX.</text>
        </description>
      </leaf>
      <leaf name="tx-wavelength-num">
        <type name="string">
          <length value="0..8"/>
        </type>
        <config value="false"/>
        <description>
          <text>Wave length number tx.</text>
        </description>
      </leaf>
      <leaf name="rx-wavelength-num">
        <type name="string">
          <length value="0..8"/>
        </type>
        <config value="false"/>
        <description>
          <text>Wave length number rx.</text>
        </description>
      </leaf>
