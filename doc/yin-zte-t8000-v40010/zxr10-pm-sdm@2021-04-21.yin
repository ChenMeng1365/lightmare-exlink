  <container name="state">
    <config value="false"/>
    <leaf name="current-software">
      <description>
        <text>Current software filename.</text>
      </description>
      <type name="string">
        <length value="1..160"/>
      </type>
    </leaf>
    <container name="upgrade">
      <container name="state">
        <leaf name="upgrade-type">
          <type name="enumeration">
            <enum name="ACTIVATE">
              <value value="2"/>
            </enum>
            <enum name="ROLLBACK">
              <value value="3"/>
            </enum>
            <enum name="NULL">
              <value value="0"/>
            </enum>
            <enum name="COMMIT">
              <value value="4"/>
            </enum>
          </type>
        </leaf>
        <leaf name="software-name">
          <description>
            <text>Software filename rollback to.</text>
          </description>
          <type name="string">
            <length value="1..160"/>
          </type>
        </leaf>
        <leaf name="software-version">
          <type name="string"/>
        </leaf>
        <leaf name="upgrade-state">
          <description>
            <text>Upgrade state.</text>
          </description>
          <type name="enumeration">
            <enum name="COMMIT">
              <value value="1"/>
            </enum>
            <enum name="ROLLBACK">
              <value value="2"/>
            </enum>
            <enum name="ACTIVATE">
              <value value="3"/>
            </enum>
            <enum name="COMPLETE">
              <value value="4"/>
            </enum>
            <enum name="FAIL">
              <value value="5"/>
            </enum>
            <enum name="ACTIVATE_COMPLETE">
              <value value="6"/>
            </enum>
            <enum name="ACTIVATE_FAIL">
              <value value="7"/>
            </enum>
            <enum name="ABORT">
              <value value="8"/>
            </enum>
            <enum name="DOWNLOAD_FAIL">
              <value value="9"/>
            </enum>
            <enum name="COMMIT_FAIL">
              <value value="10"/>
            </enum>
            <enum name="ROLLBACK_FAIL">
              <value value="11"/>
            </enum>
            <enum name="NULL">
              <value value="255"/>
            </enum>
          </type>
        </leaf>
        <leaf name="upgrade-time">
          <description>
            <text>Upgrade finish time.</text>
          </description>
          <type name="ncd:string-with-blank">
            <length value="1..32"/>
            <pattern value="[0-9]{4}\-[0-9]{2}\-[0-9]{2}T[0-9]{2}:[0-9]{2}:[0-9]{2}(\.[0-9]+)?Z[+-][0-9]{2}:[0-9]{2}"/>
          </type>
        </leaf>
        <leaf name="fail-reason">
          <type name="ncd:string-all"/>
        </leaf>
      </container>
    </container>
    <container name="download">
      <container name="state">
        <leaf name="software-name">
          <description>
            <text>Download software filename.</text>
          </description>
          <type name="string">
            <length value="1..160"/>
          </type>
        </leaf>
        <leaf name="software-version">
          <type name="string"/>
        </leaf>
        <leaf name="expected-duration">
          <description>
            <text>Expected duration.</text>
          </description>
          <type name="string">
            <length value="1..32"/>
          </type>
        </leaf>
        <leaf name="download-state">
          <description>
            <text>Download state.</text>
          </description>
          <type name="enumeration">
            <enum name="DOWNLOADING">
              <value value="0"/>
            </enum>
            <enum name="COMMIT">
              <value value="1"/>
            </enum>
            <enum name="ROLLBACK">
              <value value="2"/>
            </enum>
            <enum name="ACTIVATE">
              <value value="3"/>
            </enum>
            <enum name="COMPLETE">
              <value value="4"/>
            </enum>
            <enum name="FAIL">
              <value value="5"/>
            </enum>
            <enum name="ACTIVATE_COMPLETE">
              <value value="6"/>
            </enum>
            <enum name="ACTIVATE_FAIL">
              <value value="7"/>
            </enum>
            <enum name="ABORT">
              <value value="8"/>
            </enum>
            <enum name="DOWNLOAD_FAIL">
              <value value="9"/>
            </enum>
            <enum name="COMMIT_FAIL">
              <value value="10"/>
            </enum>
            <enum name="ROLLBACK_FAIL">
              <value value="11"/>
            </enum>
            <enum name="NULL">
              <value value="255"/>
            </enum>
          </type>
        </leaf>
        <leaf name="download-time">
          <description>
            <text>Software download finish time.</text>
          </description>
          <type name="ncd:string-with-blank">
            <length value="1..32"/>
            <pattern value="[0-9]{4}\-[0-9]{2}\-[0-9]{2}T[0-9]{2}:[0-9]{2}:[0-9]{2}(\.[0-9]+)?Z[+-][0-9]{2}:[0-9]{2}"/>
          </type>
        </leaf>
        <leaf name="fail-reason">
          <type name="ncd:string-all"/>
        </leaf>
      </container>
    </container>
    <container name="firmware">
      <container name="boot-versions">
        <list name="boot-version">
          <description>
            <text>show boot-firmware list</text>
          </description>
          <key value="shelf slot cpu"/>
          <ncd:select fields="shelf slot cpu"/>
          <leaf name="shelf">
            <description>
              <text>shelf number</text>
            </description>
            <type name="uint8"/>
          </leaf>
          <leaf name="slot">
            <description>
              <text>slot number</text>
            </description>
            <type name="uint8"/>
          </leaf>
          <leaf name="cpu">
            <description>
              <text>cpu number</text>
            </description>
            <type name="uint8"/>
          </leaf>
          <leaf name="board-type">
            <description>
              <text>board type</text>
            </description>
            <type name="string"/>
          </leaf>
          <leaf name="board-id">
            <description>
              <text>board id</text>
            </description>
            <type name="uint32"/>
          </leaf>
          <leaf name="firmware-type">
            <description>
              <text>firmware type</text>
            </description>
            <type name="string"/>
          </leaf>
          <leaf name="master-version">
            <description>
              <text>master boot version number</text>
            </description>
            <type name="string"/>
          </leaf>
          <leaf name="slave-version">
            <description>
              <text>slave boot version number</text>
            </description>
            <type name="string"/>
          </leaf>
        </list>
      </container>
    </container>
    <container name="software-storage">
      <container name="state">
        <leaf name="software-number">
