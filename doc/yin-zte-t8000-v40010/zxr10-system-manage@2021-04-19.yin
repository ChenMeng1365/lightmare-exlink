  <feature name="auto-write"/>
  <feature name="startrun-download-dcn"/>
  <feature name="dcn-surpport-product"/>
  <container name="configuration">
    <container name="auto-save">
      <description>
        <text>Automatic saving configuration.</text>
      </description>
      <choice name="policy">
        <description>
          <text>Automatic saving configuration policy:delay-policy and everyday-policy.</text>
        </description>
        <case name="delay-policy">
          <description>
            <text>Delay-policy of automatic saving configuration.</text>
          </description>
          <leaf name="delay">
            <description>
              <text>The hold off time interval of automatic saving configuration when configuration data is changed. Uint:minutes.Range:1-60.</text>
            </description>
            <type name="uint32">
              <range value="1..60"/>
            </type>
          </leaf>
        </case>
        <case name="everyday-policy">
          <description>
            <text>Everyday-policy of automatic saving configuration.</text>
          </description>
          <leaf name="everyday">
            <description>
              <text>The time of automatic saving configuration time everyday. For example: HH:MM:SS.(02:22:33).</text>
            </description>
            <type name="ncd:time"/>
          </leaf>
        </case>
      </choice>
      <container name="auto-save-perf-para">
        <description>
          <text>Automatic saving configuration will take effective interval xx minutes when cpu utilization reaches to upper limit.</text>
        </description>
        <presence value="auto-save-perf-para"/>
        <leaf name="cpu-limit">
          <description>
            <text>The CPU usage threshold of automatic saving configuration.Range:1-90.</text>
          </description>
          <type name="uint32">
            <range value="1..90"/>
          </type>
          <mandatory value="true"/>
        </leaf>
        <leaf name="interval">
          <description>
            <text>The time interval of automatic saving configuration when cpu utilization reaches to upper limit.Uint:minutes.Range:1-60.</text>
          </description>
          <type name="uint32">
            <range value="1..60"/>
          </type>
        </leaf>
      </container>
    </container>
    <container name="startrun-backup-to-ipv4-servers">
      <description>
        <text>Backup startrun to file servers with IPv4.</text>
      </description>
      <list name="startrun-backup-to-ipv4-server">
        <description>
          <text>Backup startrun to file server with IPv4.</text>
        </description>
        <key value="ipv4-address"/>
        <leaf name="ipv4-address">
          <description>
            <text>IPv4 address for server.</text>
          </description>
          <type name="ncd:ipv4-address-none-zero"/>
        </leaf>
        <leaf name="port">
          <description>
            <text>Listen port.</text>
          </description>
          <type name="uint16">
            <range value="1..65535"/>
          </type>
        </leaf>
        <choice name="vrf-name">
          <description>
            <text>VPN name.</text>
          </description>
          <case name="normal-vrf">
            <description>
              <text>VPN name without DCN.</text>
            </description>
            <leaf name="vpn-instance-name">
              <description>
                <text>VPN instance name.</text>
              </description>
              <type name="string">
                <length value="1..32"/>
              </type>
            </leaf>
          </case>
          <case name="dcn">
            <description>
              <text>Whether DCN VRF or not.</text>
            </description>
            <leaf name="dcn">
              <description>
                <text>Whether DCN VRF or not.</text>
              </description>
              <type name="empty"/>
            </leaf>
            <ncd:revise-date date="2019-09-24"/>
          </case>
        </choice>
        <choice name="transport-type">
          <description>
            <text>Transport type.</text>
          </description>
          <mandatory value="true"/>
          <case name="ftp">
            <description>
              <text>FTP server.</text>
            </description>
            <container name="ftp">
              <description>
                <text>FTP server.</text>
              </description>
              <leaf name="user-name">
                <description>
                  <text>FTP server's username.</text>
                </description>
                <type name="string">
                  <length value="1..65"/>
                </type>
                <mandatory value="true"/>
              </leaf>
              <choice name="key">
                <description>
                  <text>FTP server key.</text>
                </description>
                <mandatory value="true"/>
                <case name="password">
                  <description>
                    <text>Clear password, which display in encrypted password.</text>
                  </description>
                  <leaf name="password">
                    <description>
                      <text>FTP server's password, which display in encrypted password.</text>
                    </description>
                    <type name="string">
                      <length value="1..128"/>
                    </type>
                    <mandatory value="true"/>
                  </leaf>
                </case>
                <case name="encrypted-password">
                  <description>
                    <text>Encrypted password.</text>
                  </description>
                  <leaf name="encrypted-password">
                    <description>
                      <text>Encrypted password.</text>
                    </description>
                    <type name="string">
                      <length value="64..240"/>
                    </type>
                    <mandatory value="true"/>
                  </leaf>
                </case>
                <ncd:revise-date date="2019-02-27"/>
              </choice>
            </container>
          </case>
          <case name="sftp">
            <description>
              <text>SFTP server.</text>
            </description>
            <container name="sftp">
              <description>
                <text>SFTP server.</text>
              </description>
              <leaf name="user-name">
                <description>
                  <text>SFTP server's username.</text>
                </description>
                <type name="string">
                  <length value="1..65"/>
                </type>
                <mandatory value="true"/>
              </leaf>
              <choice name="key">
                <description>
                  <text>SFTP server key.</text>
                </description>
                <mandatory value="true"/>
                <case name="password">
                  <description>
                    <text>SFTP server's password, which display in encrypted-password.</text>
                  </description>
                  <leaf name="password">
                    <description>
                      <text>SFTP server's password, which display in encrypted-password.</text>
                    </description>
                    <type name="string">
                      <length value="1..128"/>
                    </type>
                    <mandatory value="true"/>
                  </leaf>
                </case>
                <case name="encrypted-password">
                  <description>
                    <text>Encrypted password.</text>
                  </description>
                  <leaf name="encrypted-password">
                    <description>
                      <text>Encrypted password.</text>
                    </description>
                    <type name="string">
                      <length value="64..240"/>
                    </type>
                    <mandatory value="true"/>
                  </leaf>
                </case>
                <ncd:revise-date date="2019-02-27"/>
              </choice>
            </container>
          </case>
          <case name="tftp">
            <description>
              <text>TFTP server.</text>
            </description>
            <leaf name="tftp">
              <description>
                <text>TFTP server.</text>
              </description>
              <type name="empty"/>
              <mandatory value="true"/>
            </leaf>
          </case>
        </choice>
        <leaf name="path">
          <description>
            <text>File path in server.</text>
          </description>
          <type name="string">
            <length value="1..64"/>
          </type>
        </leaf>
        <leaf name="zip">
          <description>
            <text>Compress file before backuping it to server.</text>
          </description>
          <type name="empty"/>
          <ncd:revise-date date="2019-02-27"/>
        </leaf>
      </list>
    </container>
