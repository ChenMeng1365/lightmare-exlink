            <key value="ipv4-dest-address ipv4-masklen out-interface-name next-hop lsr-type"/>
            <description>
              <text>Operational data of the traffic statistical table of SRBE.</text>
            </description>
            <leaf name="ipv4-dest-address">
              <type name="inet:ipv4-address-no-zone"/>
              <description>
                <text>Prefix.</text>
              </description>
            </leaf>
            <leaf name="ipv4-masklen">
              <type name="uint32">
                <range value="0..32"/>
              </type>
              <description>
                <text>Length of mask.</text>
              </description>
            </leaf>
            <leaf name="out-interface-name">
              <type name="string">
                <length value="1..49"/>
              </type>
              <description>
                <text>The name of interface.</text>
              </description>
            </leaf>
            <leaf name="next-hop">
              <type name="inet:ipv4-address-no-zone"/>
              <description>
                <text>The ip address of next hop.</text>
              </description>
            </leaf>
            <leaf name="lsr-type">
              <type name="ldp-lsp-type"/>
              <description>
                <text>Tunnel role.</text>
              </description>
            </leaf>
            <leaf name="in-label">
              <type name="uint32">
                <range value="0..4294967295"/>
              </type>
              <description>
                <text>Inlabel.</text>
              </description>
            </leaf>
            <leaf name="out-label">
              <type name="uint32">
                <range value="0..4294967295"/>
              </type>
              <description>
                <text>OutLabel.</text>
              </description>
            </leaf>
            <leaf name="flow-period">
              <type name="uint32">
                <range value="0..4294967295"/>
              </type>
              <description>
                <text>Out period of the last period.</text>
              </description>
            </leaf>
            <leaf name="last-period-byte-rate">
              <type name="uint64">
                <range value="0..18446744073709551615"/>
              </type>
              <description>
                <text>Out bites rate of the last period.</text>
              </description>
            </leaf>
            <leaf name="last-period-packet-rate">
              <type name="uint64">
                <range value="0..18446744073709551615"/>
              </type>
              <description>
                <text>Out packet rate of the last period.</text>
              </description>
            </leaf>
            <leaf name="display-interval">
              <type name="uint64">
                <range value="0..18446744073709551615"/>
              </type>
              <description>
                <text>Query interval.</text>
              </description>
            </leaf>
            <leaf name="interval-byte-rate">
              <type name="uint64">
                <range value="0..18446744073709551615"/>
              </type>
              <description>
                <text>Rate at which bytes are sent within a specified interval.</text>
              </description>
            </leaf>
            <leaf name="interval-packet-rate">
              <type name="uint64">
                <range value="0..18446744073709551615"/>
              </type>
              <description>
                <text>Rate at which packets are sent within a specified interval.</text>
              </description>
            </leaf>
            <leaf name="total-byte">
              <type name="uint64">
                <range value="0..18446744073709551615"/>
              </type>
              <description>
                <text>Counting bytes.</text>
              </description>
            </leaf>
            <leaf name="total-packet">
              <type name="uint64">
                <range value="0..18446744073709551615"/>
              </type>
              <description>
                <text>The number of packets.</text>
              </description>
            </leaf>
          </list>
        </container>
        <container name="auto-config-interfaces">
          <when condition="../../../../ni:name='_public_'"/>
          <config value="false"/>
          <description>
            <text>List of auto config LDP interfaces.</text>
          </description>
          <list name="auto-config-interface">
            <key value="name"/>
            <description>
              <text>Operational state of an auto config LDP interface. LDP instance that is automatically created using an IGP.</text>
            </description>
            <leaf name="name">
              <type name="pub-type:if-name"/>
              <description>
                <text>Interface name. The name is case-insensitive.</text>
              </description>
            </leaf>
            <leaf name="state">
              <type name="ldp-state"/>
              <config value="false"/>
              <description>
                <text>Interface status.</text>
              </description>
            </leaf>
            <leaf name="negotiated-hello-hold-time">
              <type name="uint16">
                <range value="0..65535"/>
              </type>
              <units name="s"/>
              <config value="false"/>
              <description>
                <text>Specifies a negotiated value of the Hello hold timer used by an interface.in seconds.</text>
              </description>
            </leaf>
            <leaf name="auto-trigger-type">
              <type name="ldp-if-auto-config-type"/>
              <config value="false"/>
              <description>
                <text>Specifies auto config flag of LDP interface by IGP. For example, ISIS, OSPF, ISIS&amp;OSPF trigger config. By default, there is no auto config.</text>
              </description>
            </leaf>
          </list>
        </container>
        <container name="auto-config-remote-peers">
          <when condition="../../../../ni:name='_public_'"/>
          <config value="false"/>
          <description>
            <text>List of auto config remote LDP neighbor.</text>
          </description>
          <list name="auto-config-remote-peer">
            <key value="remote-ip"/>
            <config value="false"/>
            <description>
              <text>Operational state of an auto config remote LDP neighbor.</text>
            </description>
            <leaf name="remote-ip">
              <type name="inet:ipv4-address-no-zone"/>
              <config value="false"/>
              <description>
                <text>Specifies the IPv4 address of a remote neighbor.</text>
              </description>
            </leaf>
            <leaf name="remote-peer-name">
              <type name="string">
                <length value="1..32"/>
              </type>
              <config value="false"/>
              <description>
                <text>Specifies the name of a remote neighbor.</text>
              </description>
            </leaf>
            <leaf name="peer-state">
              <type name="ldp-state"/>
              <config value="false"/>
              <description>
                <text>Specifies the value status of a remote neighbor.</text>
              </description>
            </leaf>
            <leaf name="negotiated-hello-hold-time">
              <type name="uint16">
                <range value="0..65535"/>
              </type>
              <units name="s"/>
              <config value="false"/>
              <description>
                <text>Specifies the length of a negotiated hello holding time on an interface.</text>
              </description>
            </leaf>
            <container name="auto-config-type">
              <config value="false"/>
              <description>
                <text>Operational state of auto config type.</text>
              </description>
              <leaf name="l2vpn">
                <type name="remote-peer-auto-config-l2"/>
                <config value="false"/>
                <description>
                  <text>Remote peer that is automatically created using an L2VPN.</text>
                </description>
              </leaf>
              <leaf name="session-protection">
                <type name="remote-peer-auto-config-session-protection"/>
                <config value="false"/>
                <description>
                  <text>Remote peer that is automatically created using session protection.</text>
                </description>
              </leaf>
              <leaf name="rlfa">
                <type name="remote-peer-auto-config-rlfa"/>
                <config value="false"/>
                <description>
                  <text>Remote peer that is automatically created using RLFA.</text>
                </description>
              </leaf>
              <leaf name="auto-accept-function">
                <type name="remote-peer-auto-config-accept-function"/>
                <config value="false"/>
                <description>
                  <text>Remote peer that is automatically created using the automatic accept function.</text>
                </description>
              </leaf>
            </container>
          </list>
        </container>
        <container name="instance-status">
          <config value="false"/>
          <description>
            <text>Statistics of the status information about the LDP VPN instance.</text>
          </description>
          <leaf name="label-dist-mode">
            <type name="ldp-label-dist-mode-all"/>
            <config value="false"/>
            <description>
              <text>Specifies a label distribution control mode.</text>
            </description>
          </leaf>
          <leaf name="label-retention-mode">
            <type name="ldp-label-ret-mode-all"/>
            <config value="false"/>
            <description>
              <text>Specifies a label retention mode.</text>
            </description>
          </leaf>
          <leaf name="lsp-number">
            <type name="uint32"/>
            <config value="false"/>
            <description>
              <text>Specifies the number of LSPs.</text>
            </description>
          </leaf>
          <leaf name="session-number">
            <type name="uint32"/>
            <config value="false"/>
            <description>
              <text>Number of sessions.</text>
            </description>
          </leaf>
          <leaf name="adjacency-number">
            <type name="uint32"/>
            <config value="false"/>
            <description>
              <text>Number of adjacencies.</text>
            </description>
          </leaf>
          <leaf name="interface-number">
            <type name="uint32"/>
            <config value="false"/>
            <description>
              <text>Number of interfaces.</text>
            </description>
          </leaf>
          <leaf name="fec-number">
            <type name="uint32"/>
            <config value="false"/>
            <description>
              <text>Specifies the number of FECs.</text>
            </description>
          </leaf>
        </container>
      </container>
    </container>
  </augment>
  <augment target-node="/mpls:mpls/mpls:common/mpls:session-car">
    <description>
      <text>MPLS LDP Session CAR Configuration information augment to module MPLS.</text>
    </description>
    <container name="ldp">
      <description>
        <text>Configure LDP session CAR.</text>
      </description>
      <leaf name="enable">
        <type name="boolean"/>
        <default value="true"/>
        <description>
          <text>Enable/disable session CAR.</text>
        </description>
      </leaf>
      <container name="tcp">
        <must condition="cir&lt;=pir and cbs&lt;=pbs"/>
        <description>
          <text>Configure LDP session CAR to limit TCP packets receive from peer.</text>
        </description>
        <uses name="session-car-grp">
          <refine target-node="cir">
            <default value="1"/>
          </refine>
          <refine target-node="cbs">
            <default value="188"/>
          </refine>
          <refine target-node="pir">
            <default value="4000"/>
          </refine>
          <refine target-node="pbs">
            <default value="1600000"/>
          </refine>
        </uses>
      </container>
      <container name="udp-local">
        <must condition="cir&lt;=pir and cbs&lt;=pbs"/>
        <description>
          <text>Configure LDP session CAR to limit UDP packets receive from local adjacency.</text>
        </description>
        <uses name="session-car-grp">
          <refine target-node="cir">
            <default value="1"/>
          </refine>
          <refine target-node="cbs">
            <default value="188"/>
          </refine>
          <refine target-node="pir">
            <default value="4"/>
          </refine>
          <refine target-node="pbs">
            <default value="750"/>
          </refine>
        </uses>
      </container>
      <container name="udp-remote">
        <must condition="cir&lt;=pir and cbs&lt;=pbs"/>
        <description>
          <text>Configure LDP session CAR to limit UDP packets receive from targeted adjacency.</text>
        </description>
        <uses name="session-car-grp">
          <refine target-node="cir">
            <default value="1"/>
          </refine>
          <refine target-node="cbs">
            <default value="188"/>
          </refine>
          <refine target-node="pir">
            <default value="4"/>
          </refine>
          <refine target-node="pbs">
            <default value="750"/>
          </refine>
        </uses>
      </container>
    </container>
  </augment>
</module>
