          </leaf>
          <leaf name="shaping-value">
            <type name="uint32">
              <range value="1..400000"/>
            </type>
            <units name="Mbit/s"/>
            <mandatory value="true"/>
            <description>
              <text>Shaping value.</text>
            </description>
          </leaf>
        </list>
