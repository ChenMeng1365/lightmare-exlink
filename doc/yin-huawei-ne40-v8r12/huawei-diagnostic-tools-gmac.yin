<?xml version="1.0" encoding="UTF-8"?>
<submodule name="huawei-diagnostic-tools-gmac"
           xmlns="urn:ietf:params:xml:ns:yang:yin:1"
           xmlns:diagnostic-tools="urn:huawei:yang:huawei-diagnostic-tools"
           xmlns:pub-type="urn:huawei:yang:huawei-pub-type"
           xmlns:ifm="urn:huawei:yang:huawei-ifm"
           xmlns:ext="urn:huawei:yang:huawei-extension">
  <yang-version value="1"/>
  <belongs-to module="huawei-diagnostic-tools">
    <prefix value="diagnostic-tools"/>
  </belongs-to>
  <import module="huawei-pub-type">
    <prefix value="pub-type"/>
  </import>
  <import module="huawei-ifm">
    <prefix value="ifm"/>
  </import>
  <import module="huawei-extension">
    <prefix value="ext"/>
  </import>
  <include module="huawei-diagnostic-tools-common"/>
  <include module="huawei-diagnostic-tools-type"/>
  <organization>
    <text>Huawei Technologies Co., Ltd.</text>
  </organization>
  <contact>
    <text>Huawei Industrial Base
Bantian, Longgang
Shenzhen 518129
People's Republic of China
Website: http://www.huawei.com
Email: support@huawei.com</text>
  </contact>
  <description>
    <text>MAC diagnosis tool.</text>
  </description>
  <revision date="2020-01-14">
    <description>
      <text>Add container for list.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <revision date="2019-04-13">
    <description>
      <text>Init revision.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <grouping name="gmac-ping-trace-result-type">
    <description>
      <text>Gmac test result.</text>
    </description>
    <container name="ping-results">
      <config value="false"/>
      <description>
        <text>List of ping test results.</text>
      </description>
      <list name="ping-result">
        <key value="test-name"/>
        <config value="false"/>
        <description>
          <text>Statistics of ping test result.</text>
        </description>
        <leaf name="test-name">
          <type name="string">
            <length value="1..32"/>
          </type>
          <config value="false"/>
          <description>
            <text>Test name.</text>
          </description>
        </leaf>
        <leaf name="packet-send">
          <type name="uint32"/>
          <config value="false"/>
          <description>
            <text>Total number of sent packets.</text>
          </description>
        </leaf>
        <leaf name="packet-receive">
          <type name="uint32"/>
          <config value="false"/>
          <description>
            <text>Total number of received packets.</text>
          </description>
        </leaf>
        <leaf name="loss-ratio">
          <type name="pub-type:percent">
            <range value="0..100"/>
          </type>
          <units name="%"/>
          <config value="false"/>
          <description>
            <text>Packet loss ratio of a test.</text>
          </description>
        </leaf>
        <leaf name="rtt-min">
          <type name="uint32">
            <range value="0..4294967295"/>
          </type>
          <units name="ms"/>
          <config value="false"/>
          <description>
            <text>Minimum RTT of all test packets.</text>
          </description>
        </leaf>
        <leaf name="rtt-max">
          <type name="uint32">
            <range value="0..4294967295"/>
          </type>
          <units name="ms"/>
          <config value="false"/>
          <description>
            <text>Maximum RTT of all test packets.</text>
          </description>
        </leaf>
        <leaf name="rtt-average">
          <type name="uint32">
            <range value="0..4294967295"/>
          </type>
          <units name="ms"/>
          <config value="false"/>
          <description>
            <text>Average RTT of all test packets.</text>
          </description>
        </leaf>
        <leaf name="status">
          <type name="dgntl-test-status-type"/>
          <config value="false"/>
          <description>
            <text>Test status.</text>
          </description>
        </leaf>
        <container name="probes">
          <config value="false"/>
          <description>
            <text>List of probe test results.</text>
          </description>
          <list name="probe">
            <key value="sequence"/>
            <config value="false"/>
            <description>
              <text>Statistics of probe result.</text>
            </description>
            <leaf name="sequence">
              <type name="uint32"/>
              <config value="false"/>
              <description>
                <text>Serial number of a test packet received by a certain hop.</text>
              </description>
            </leaf>
            <leaf name="dest-mac">
              <type name="pub-type:mac-address"/>
              <config value="false"/>
              <description>
                <text>Destination mac address of the related hop in a test.</text>
              </description>
            </leaf>
            <leaf name="data-bytes">
              <type name="uint32"/>
              <config value="false"/>
              <description>
                <text>Number of bytes in a received response packet.</text>
              </description>
            </leaf>
            <leaf name="rtt">
              <type name="uint32">
                <range value="0..4294967295"/>
              </type>
              <units name="ms"/>
              <config value="false"/>
              <description>
                <text>The round-trip time of the test message, in milliseconds.</text>
              </description>
            </leaf>
            <leaf name="result-type">
              <type name="probe-result-type"/>
              <config value="false"/>
              <description>
                <text>Result of packet sending, which is either success or timeout.</text>
              </description>
            </leaf>
            <leaf name="is-delete">
              <type name="boolean"/>
              <config value="false"/>
              <description>
                <text>Enable/disable the function of deleting test results.</text>
              </description>
            </leaf>
          </list>
        </container>
      </list>
    </container>
  </grouping>
  <rpc name="gmac-start-ping">
    <description>
      <text>Start gmac ping.</text>
    </description>
    <ext:node-ref value="/diagnostic-tools:diagnostic-tools/diagnostic-tools:gmac/diagnostic-tools:ping-results"/>
    <input>
      <leaf name="test-name">
        <type name="string">
          <length value="1..32"/>
        </type>
        <mandatory value="true"/>
        <description>
          <text>Test name.</text>
        </description>
      </leaf>
      <leaf name="dest-mac">
        <type name="pub-type:mac-address"/>
        <mandatory value="true"/>
        <description>
          <text>Destination mac address of the related hop in a test.</text>
        </description>
      </leaf>
      <choice name="l2-protocol">
        <description>
          <text>L2 protocol.</text>
        </description>
        <case name="vsi">
          <description>
            <text>VSI.</text>
          </description>
          <leaf name="vsi-name">
            <type name="string">
              <length value="1..31"/>
            </type>
            <description>
              <text>VSI name.</text>
            </description>
          </leaf>
        </case>
      </choice>
      <leaf name="if-name">
        <type name="leafref">
          <path value="/ifm:ifm/ifm:interfaces/ifm:interface/ifm:name"/>
        </type>
        <description>
          <text>Interface name.</text>
        </description>
      </leaf>
      <choice name="vlan-type">
        <description>
          <text>VLAN type.</text>
        </description>
        <case name="qinq">
          <description>
            <text>QINQ VLAN.</text>
          </description>
          <leaf name="pe-vid">
            <type name="uint16">
              <range value="1..4094"/>
            </type>
            <description>
              <text>PE VLAN ID.</text>
            </description>
          </leaf>
          <leaf name="ce-vid">
            <type name="uint16">
              <range value="1..4094"/>
            </type>
            <description>
              <text>CE VLAN ID.</text>
            </description>
          </leaf>
        </case>
        <case name="dot1q">
          <description>
            <text>DOT1Q VLAN.</text>
          </description>
          <leaf name="dot1q-vlan">
            <type name="uint16">
              <range value="1..4094"/>
            </type>
            <description>
              <text>Config DOT1Q VLAN ID.</text>
            </description>
          </leaf>
        </case>
      </choice>
      <leaf name="count">
        <type name="uint32">
          <range value="1..4294967295"/>
        </type>
        <default value="5"/>
        <description>
          <text>Specify the times of ping action (default is 5).</text>
        </description>
      </leaf>
      <leaf name="packet-size">
        <type name="uint32">
          <range value="95..9000"/>
        </type>
        <units name="Byte"/>
        <default value="95"/>
        <description>
          <text>Number of data bytes to be sent.</text>
        </description>
      </leaf>
      <leaf name="timeout">
        <type name="uint32">
          <range value="1..65535"/>
        </type>
        <units name="ms"/>
        <default value="2000"/>
        <description>
          <text>Specifies the time-out period.</text>
        </description>
      </leaf>
      <leaf name="priority">
        <type name="uint32">
          <range value="0..7"/>
        </type>
        <default value="7"/>
        <description>
          <text>Message priority, the default value is 7.</text>
        </description>
      </leaf>
    </input>
  </rpc>
  <rpc name="gmac-stop-ping">
    <description>
      <text>Stop gmac ping.</text>
    </description>
    <ext:node-ref value="/diagnostic-tools:diagnostic-tools/diagnostic-tools:gmac/diagnostic-tools:ping-results"/>
    <input>
      <leaf name="test-name">
        <type name="leafref">
          <path value="/diagnostic-tools:diagnostic-tools/diagnostic-tools:gmac/diagnostic-tools:ping-results/diagnostic-tools:ping-result/diagnostic-tools:test-name"/>
        </type>
        <mandatory value="true"/>
        <description>
          <text>Test name.</text>
        </description>
      </leaf>
    </input>
  </rpc>
  <rpc name="gmac-delete-ping">
    <description>
      <text>Delete gmac ping.</text>
    </description>
    <ext:node-ref value="/diagnostic-tools:diagnostic-tools/diagnostic-tools:gmac/diagnostic-tools:ping-results"/>
    <input>
      <leaf name="test-name">
        <type name="leafref">
          <path value="/diagnostic-tools:diagnostic-tools/diagnostic-tools:gmac/diagnostic-tools:ping-results/diagnostic-tools:ping-result/diagnostic-tools:test-name"/>
        </type>
        <mandatory value="true"/>
        <description>
          <text>Test name.</text>
        </description>
      </leaf>
    </input>
  </rpc>
  <augment target-node="/diagnostic-tools:diagnostic-tools">
    <description>
      <text>List of gmac Ping &amp; Trace tests.</text>
    </description>
    <container name="gmac">
      <config value="false"/>
      <description>
        <text>Statistics of gmac Ping &amp; Trace.</text>
      </description>
      <uses name="diagnostic-tools:gmac-ping-trace-result-type"/>
    </container>
  </augment>
</submodule>
