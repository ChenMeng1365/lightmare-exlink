      </description>
    </leaf>
    <leaf name="if-index">
      <type name="uint32"/>
      <description>
        <text>Indicates the index of the interface that discovers the neighbor.</text>
      </description>
    </leaf>
    <leaf name="ssn-state">
      <type name="uint32"/>
      <description>
        <text>Session status.</text>
      </description>
    </leaf>
    <leaf name="ssn-disco-time">
      <type name="uint32"/>
      <units name="s"/>
      <description>
        <text>Session keepalive time.</text>
      </description>
    </leaf>
    <leaf name="ssn-rec-unknown-msg-errs">
      <type name="uint32"/>
      <description>
        <text>Number of unknown types of messages received by the session.</text>
      </description>
    </leaf>
    <leaf name="ssn-rec-unknown-tlv-errs">
      <type name="uint32"/>
      <description>
        <text>Number of unknown TLVs received by the session.</text>
      </description>
    </leaf>
    <leaf name="local-lsr-id">
      <type name="string">
        <length value="1..31"/>
      </type>
      <description>
        <text>Local LSR ID.</text>
      </description>
    </leaf>
    <leaf name="ldp-entity-index">
      <type name="uint32"/>
      <description>
        <text>The index for the LDP entry.</text>
      </description>
    </leaf>
    <leaf name="if-name">
      <type name="pub-type:if-name"/>
      <description>
        <text>Name of an LDP peer interface.</text>
      </description>
