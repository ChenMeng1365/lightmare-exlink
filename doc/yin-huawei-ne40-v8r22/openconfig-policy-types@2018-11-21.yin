<?xml version="1.0" encoding="UTF-8"?>
<module name="openconfig-policy-types"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:oc-pol-types="http://openconfig.net/yang/policy-types"
        xmlns:yang="urn:ietf:params:xml:ns:yang:ietf-yang-types"
        xmlns:oc-ext="http://openconfig.net/yang/openconfig-ext">
  <yang-version value="1"/>
  <namespace uri="http://openconfig.net/yang/policy-types"/>
  <prefix value="oc-pol-types"/>
  <import module="ietf-yang-types">
    <prefix value="yang"/>
  </import>
  <import module="openconfig-extensions">
    <prefix value="oc-ext"/>
  </import>
  <organization>
    <text>OpenConfig working group</text>
  </organization>
  <contact>
    <text>OpenConfig working group
netopenconfig@googlegroups.com</text>
  </contact>
  <description>
    <text>This module contains general data definitions for use in routing
policy.  It can be imported by modules that contain protocol-
specific policy conditions and actions.</text>
  </description>
  <oc-ext:openconfig-version semver="3.1.1"/>
  <revision date="2018-11-21">
    <description>
      <text>Add OpenConfig module metadata extensions.</text>
    </description>
    <reference>
      <text>3.1.1</text>
    </reference>
  </revision>
  <revision date="2018-06-05">
    <description>
      <text>Add PIM, IGMP to INSTALL_PROTOCOL_TYPES identity</text>
    </description>
    <reference>
      <text>3.1.0</text>
    </reference>
  </revision>
  <revision date="2017-07-14">
    <description>
      <text>Replace policy choice node/type with policy-result
enumeration;simplified defined set naming;removed generic
IGP actions; migrate to OpenConfig types; added mode for
prefix sets</text>
    </description>
    <reference>
      <text>3.0.0</text>
    </reference>
  </revision>
  <revision date="2016-05-12">
    <description>
      <text>OpenConfig public release</text>
    </description>
    <reference>
      <text>2.0.1</text>
    </reference>
  </revision>
  <oc-ext:regexp-posix/>
  <oc-ext:catalog-organization org="openconfig"/>
  <oc-ext:origin origin="openconfig"/>
  <identity name="ATTRIBUTE_COMPARISON">
    <description>
      <text>base type for supported comparison operators on route
attributes</text>
    </description>
  </identity>
  <identity name="ATTRIBUTE_EQ">
    <base name="ATTRIBUTE_COMPARISON"/>
    <description>
      <text>== comparison</text>
    </description>
  </identity>
  <identity name="ATTRIBUTE_GE">
    <base name="ATTRIBUTE_COMPARISON"/>
    <description>
      <text>&gt;= comparison</text>
    </description>
  </identity>
  <identity name="ATTRIBUTE_LE">
    <base name="ATTRIBUTE_COMPARISON"/>
    <description>
      <text>&lt;= comparison</text>
    </description>
  </identity>
  <typedef name="match-set-options-type">
    <type name="enumeration">
      <enum name="ANY">
        <description>
          <text>match is true if given value matches any member
of the defined set</text>
        </description>
      </enum>
      <enum name="ALL">
        <description>
          <text>match is true if given value matches all
members of the defined set</text>
        </description>
      </enum>
      <enum name="INVERT">
        <description>
          <text>match is true if given value does not match any
member of the defined set</text>
        </description>
      </enum>
    </type>
    <default value="ANY"/>
    <description>
      <text>Options that govern the behavior of a match statement.  The
default behavior is ANY, i.e., the given value matches any
of the members of the defined set</text>
    </description>
  </typedef>
  <typedef name="match-set-options-restricted-type">
    <type name="enumeration">
      <enum name="ANY">
        <description>
          <text>match is true if given value matches any member
of the defined set</text>
        </description>
      </enum>
      <enum name="INVERT">
        <description>
          <text>match is true if given value does not match any
member of the defined set</text>
        </description>
      </enum>
    </type>
    <default value="ANY"/>
    <description>
      <text>Options that govern the behavior of a match statement.  The
default behavior is ANY, i.e., the given value matches any
of the members of the defined set.  Note this type is a
restricted version of the match-set-options-type.</text>
    </description>
  </typedef>
  <grouping name="attribute-compare-operators">
    <description>
      <text>common definitions for comparison operations in
condition statements</text>
    </description>
    <leaf name="operator">
      <type name="identityref">
        <base name="ATTRIBUTE_COMPARISON"/>
      </type>
      <description>
        <text>type of comparison to be performed</text>
      </description>
    </leaf>
    <leaf name="value">
      <type name="uint32"/>
      <description>
        <text>value to compare with the community count</text>
      </description>
    </leaf>
  </grouping>
  <typedef name="tag-type">
    <type name="union">
      <type name="uint32"/>
      <type name="yang:hex-string"/>
    </type>
    <description>
      <text>type for expressing route tags on a local system,
including IS-IS and OSPF; may be expressed as either decimal or
hexidecimal integer</text>
    </description>
    <reference>
      <text>RFC 2178 OSPF Version 2
RFC 5130 A Policy Control Mechanism in IS-IS Using
Administrative Tags</text>
    </reference>
  </typedef>
  <identity name="INSTALL_PROTOCOL_TYPE">
    <description>
      <text>Base type for routing protocols, including those which may
install prefixes into the RIB</text>
    </description>
  </identity>
  <identity name="BGP">
    <base name="INSTALL_PROTOCOL_TYPE"/>
    <description>
      <text>BGP</text>
    </description>
    <reference>
      <text>RFC 4271</text>
    </reference>
  </identity>
  <identity name="ISIS">
    <base name="INSTALL_PROTOCOL_TYPE"/>
    <description>
      <text>IS-IS</text>
    </description>
    <reference>
      <text>ISO/IEC 10589</text>
    </reference>
  </identity>
  <identity name="OSPF">
    <base name="INSTALL_PROTOCOL_TYPE"/>
    <description>
      <text>OSPFv2</text>
    </description>
    <reference>
      <text>RFC 2328</text>
    </reference>
  </identity>
  <identity name="OSPF3">
    <base name="INSTALL_PROTOCOL_TYPE"/>
    <description>
      <text>OSPFv3</text>
    </description>
    <reference>
      <text>RFC 5340</text>
    </reference>
  </identity>
  <identity name="STATIC">
    <base name="INSTALL_PROTOCOL_TYPE"/>
    <description>
      <text>Locally-installed static route</text>
    </description>
  </identity>
  <identity name="DIRECTLY_CONNECTED">
    <base name="INSTALL_PROTOCOL_TYPE"/>
    <description>
      <text>A directly connected route</text>
    </description>
  </identity>
  <identity name="LOCAL_AGGREGATE">
    <base name="INSTALL_PROTOCOL_TYPE"/>
    <description>
      <text>Locally defined aggregate route</text>
    </description>
  </identity>
  <identity name="PIM">
    <base name="INSTALL_PROTOCOL_TYPE"/>
    <description>
      <text>Protocol Independent Multicast</text>
    </description>
    <reference>
      <text>RFC 7761</text>
    </reference>
  </identity>
  <identity name="IGMP">
    <base name="INSTALL_PROTOCOL_TYPE"/>
    <description>
      <text>Internet Group Management Protocol</text>
    </description>
    <reference>
      <text>RFC 3376</text>
    </reference>
  </identity>
</module>
