<?xml version="1.0" encoding="UTF-8"?>
<module name="zxr10-lpp-l2"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:lpp-l2="http://www.zte.com.cn/zxr10/netconf/schema/rosng/lpp-l2"
        xmlns:ncd="http://www.zte.com.cn/zxr10/netconf/schema/rosng/netconf-defs">
  <namespace uri="http://www.zte.com.cn/zxr10/netconf/schema/rosng/lpp-l2"/>
  <prefix value="lpp-l2"/>
  <import module="zxr10-netconf-defs">
    <prefix value="ncd"/>
  </import>
  <organization>
    <text>ZTE Corporation</text>
  </organization>
  <contact>
    <text>ZTE Corporation  
   Nanjing Institute of ZTE Corporation 
   No.68 Zijinghua Rd. Yuhuatai District, Nanjing, China 
   Tel: +86-25-52870000</text>
  </contact>
  <description>
    <text>The netconf module to describe the L2 processing of local packet.</text>
  </description>
  <revision date="2020-08-15">
    <description>
      <text>Initial version.</text>
    </description>
  </revision>
  <feature name="data-link-layer-traffic">
    <description>
      <text>A feature about show data-link-layer traffic and clear data-link-layer traffic.</text>
    </description>
  </feature>
  <container name="state">
    <description>
      <text>State model info.</text>
    </description>
    <config value="false"/>
    <container name="data-link-layer">
      <description>
        <text>Data link layer state.</text>
      </description>
      <container name="traffic-statistics">
        <description>
          <text>The traffic statistics of data link layer.</text>
        </description>
        <leaf name="vlan-layer-num-error">
          <description>
            <text>Vlan layers errors.</text>
          </description>
          <type name="uint32"/>
        </leaf>
        <leaf name="vlan-mismatch">
          <description>
            <text>Vlan mismatch.</text>
          </description>
          <type name="uint32"/>
        </leaf>
        <leaf name="unknown-protocol">
          <description>
            <text>Unkonwn protocol.</text>
          </description>
          <type name="uint32"/>
        </leaf>
        <leaf name="invalid-destination-mac">
          <description>
            <text>The destination mac is invalid.</text>
          </description>
          <type name="uint32"/>
        </leaf>
      </container>
    </container>
  </container>
  <container name="operation">
    <description>
      <text>Operation model info.</text>
    </description>
    <config value="false"/>
    <container name="data-link-layer">
      <description>
        <text>Data link layer operation.</text>
      </description>
      <container name="traffic-statistics">
        <description>
          <text>Operate the traffic statistics of data link layer.</text>
        </description>
        <leaf name="clear-traffic-statistics">
          <description>
            <text>Clear the traffic statistics of data link layer.</text>
          </description>
          <type name="empty"/>
        </leaf>
      </container>
    </container>
  </container>
</module>
