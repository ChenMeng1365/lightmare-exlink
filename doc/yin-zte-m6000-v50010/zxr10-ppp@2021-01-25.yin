  <revision date="2014-05-07">
    <description>
      <text>Change value of chap(configuration/ppp/authen/authenchap/authtype) from 0 to 1
Change value of pap(configuration/ppp/authen/authenchap/authtype) from 1 to 2</text>
    </description>
  </revision>
  <revision date="2014-02-12">
    <description>
      <text>The initial version</text>
    </description>
  </revision>
  <feature name="ppp-lifetime">
    <description>
      <text>Set PPP lifetime</text>
    </description>
  </feature>
  <feature name="ppp-domain">
    <description>
      <text>Set PPP domain</text>
    </description>
  </feature>
  <feature name="ppp-aim-pool">
    <description>
      <text>Set PPP AIM pool</text>
    </description>
  </feature>
  <feature name="ppp-rasend">
    <description>
      <text>Set PPP rasend</text>
    </description>
  </feature>
  <typedef name="ppp-state-type">
    <type name="enumeration">
      <enum name="INITIAL">
        <value value="0"/>
        <description>
          <text>INITIAL state</text>
        </description>
      </enum>
      <enum name="STARTING">
        <value value="1"/>
        <description>
          <text>STARTING state</text>
        </description>
      </enum>
      <enum name="CLOSED">
        <value value="2"/>
        <description>
          <text>CLOSED state</text>
        </description>
      </enum>
      <enum name="STOPPED">
        <value value="3"/>
        <description>
          <text>STOPPED state</text>
        </description>
      </enum>
      <enum name="CLOSING">
        <value value="4"/>
        <description>
          <text>CLOSING state</text>
        </description>
      </enum>
      <enum name="STOPPING">
        <value value="5"/>
        <description>
          <text>STOPPING state</text>
        </description>
      </enum>
      <enum name="REQ_SENT">
        <value value="6"/>
        <description>
          <text>REQ_SENT state</text>
        </description>
      </enum>
      <enum name="ACK_RCVD">
        <value value="7"/>
        <description>
          <text>ACK_RCVD state</text>
        </description>
      </enum>
      <enum name="ACK_SENT">
        <value value="8"/>
        <description>
          <text>ACK_SENT state</text>
        </description>
      </enum>
      <enum name="OPENED">
        <value value="9"/>
        <description>
          <text>OPENED state</text>
        </description>
      </enum>
    </type>
    <description>
      <text>PPP correlation state type</text>
    </description>
  </typedef>
  <container name="configuration">
    <description>
      <text>Describe the configuration of PPP</text>
    </description>
    <container name="ppp">
      <description>
        <text>Configuration data of PPP</text>
      </description>
      <container name="lcp">
        <description>
          <text>LCP correlation configuration</text>
        </description>
        <list name="lcpattr">
          <description>
            <text>Configure LCP parameters</text>
          </description>
          <key value="ifname"/>
          <leaf name="ifname">
            <description>
              <text>This value identifies the PPP interface name.This attribute is created by Interface Management</text>
            </description>
            <type name="ncd:ifname"/>
          </leaf>
          <leaf name="maxecho">
            <description>
              <text>Set PPP keepalive granularity</text>
            </description>
            <type name="uint32">
              <range value="1..10"/>
            </type>
          </leaf>
          <leaf name="authtimeout">
            <description>
              <text>Maximum time to wait for an authentication response</text>
            </description>
            <type name="uint32">
              <range value="1..30"/>
            </type>
            <units name="seconds"/>
            <default value="5"/>
          </leaf>
          <leaf name="negotiationtimeout">
            <description>
              <text>Maximum time to wait for response during PPP negotiation</text>
            </description>
            <type name="uint32">
              <range value="1..30"/>
            </type>
            <units name="seconds"/>
            <default value="5"/>
          </leaf>
        </list>
      </container>
      <container name="authen">
        <description>
          <text>authentation correlation configuration</text>
        </description>
        <list name="authenchap">
          <description>
            <text>Configure CHAP authentication parameters</text>
          </description>
          <key value="ifname"/>
          <leaf name="ifname">
            <description>
              <text>This value identifies the PPP interface name.This attribute is created by Interface Management</text>
            </description>
            <type name="ncd:ifname"/>
          </leaf>
          <leaf name="authtype">
            <description>
              <text>Configure authentication type</text>
            </description>
            <type name="enumeration">
              <enum name="chap">
                <value value="1"/>
                <description>
                  <text>Challenge handshake authentication protocol(CHAP)</text>
                </description>
              </enum>
              <enum name="pap">
                <value value="2"/>
                <description>
                  <text>Password authentication protocol(PAP)</text>
                </description>
              </enum>
              <enum name="chap-pap">
                <value value="3"/>
                <description>
                  <text>CHAP PAP adaptive authentication</text>
                </description>
              </enum>
            </type>
          </leaf>
          <leaf name="chapusername">
            <description>
              <text>Configure CHAP username</text>
            </description>
            <type name="string">
              <length value="1..159"/>
            </type>
          </leaf>
          <choice name="chappassword-choice">
            <description>
              <text>Configure CHAP password</text>
            </description>
            <case name="unencrypted-case">
              <description>
                <text>Specify the unencrypted password</text>
              </description>
              <leaf name="unencrypted-password">
                <description>
                  <text>The unencryted password</text>
                </description>
                <type name="string">
                  <length value="1..64"/>
                </type>
              </leaf>
            </case>
            <case name="encrypted-case">
              <description>
                <text>Specify the encrypted password</text>
              </description>
              <leaf name="encrypted-password">
                <description>
                  <text>The encryted password</text>
                </description>
                <type name="string">
                  <length value="1..200"/>
                </type>
              </leaf>
            </case>
          </choice>
        </list>
      </container>
      <container name="ipcp">
        <description>
          <text>IPCP correlation configuration</text>
        </description>
        <list name="ipcpattr">
          <description>
            <text>Configure IPCP parameters</text>
          </description>
          <key value="ifname"/>
          <leaf name="ifname">
            <description>
              <text>This value identifies the PPP interface name.This attribute is created by Interface Management</text>
            </description>
            <type name="ncd:ifname"/>
          </leaf>
          <leaf name="enable">
            <description>
              <text>ppp ipcp enable</text>
            </description>
            <type name="enumeration">
              <enum name="disable">
                <value value="0"/>
                <description>
                  <text>Disable IPCP</text>
                </description>
              </enum>
              <enum name="enable">
                <value value="1"/>
                <description>
                  <text>Enable IPCP</text>
                </description>
              </enum>
            </type>
            <default value="enable"/>
          </leaf>
        </list>
      </container>
      <container name="bcp">
        <description>
          <text>BCP correlation configuration</text>
        </description>
        <list name="bcpattr">
          <description>
            <text>Configure BCP parameters</text>
          </description>
          <key value="ifname"/>
          <leaf name="ifname">
            <description>
              <text>This value identifies the PPP interface name.This attribute is created by Interface Management</text>
            </description>
            <type name="ncd:ifname"/>
          </leaf>
          <leaf name="managementinline">
            <description>
              <text>Enable management inline</text>
            </description>
            <type name="uint8">
              <range value="0..1"/>
            </type>
            <default value="0"/>
          </leaf>
          <leaf name="packetindicator">
            <description>
              <text>Enable bridge control packet indicator</text>
            </description>
            <type name="uint8">
              <range value="0..1"/>
            </type>
            <default value="0"/>
          </leaf>
          <leaf name="taggedframe">
            <description>
              <text>Enable IEEE 802 Tagged Frame</text>
            </description>
            <type name="uint8">
              <range value="0..1"/>
            </type>
            <default value="0"/>
          </leaf>
        </list>
        <list name="bcpenable">
          <description>
            <text>Configure BCP enable</text>
          </description>
          <key value="ifname"/>
          <leaf name="ifname">
            <description>
              <text>This value identifies the PPP interface name.This attribute is created by Interface Management</text>
            </description>
            <type name="ncd:ifname"/>
          </leaf>
          <leaf name="enable">
            <description>
              <text>ppp bcp enable</text>
            </description>
            <type name="enumeration">
              <enum name="disable">
                <value value="0"/>
                <description>
                  <text>Disable BCP</text>
                </description>
              </enum>
              <enum name="enable">
                <value value="1"/>
                <description>
                  <text>Enable BCP</text>
                </description>
              </enum>
            </type>
            <default value="disable"/>
          </leaf>
        </list>
      </container>
      <container name="epd">
        <description>
          <text>EPD(End Point Discriminator) correlation configuration</text>
        </description>
        <list name="epd">
          <description>
            <text>Configure the local endpoint discriminator</text>
          </description>
          <key value="ifname"/>
          <leaf name="ifname">
            <description>
              <text>This value identifies the PPP interface name.This attribute is created by Interface Management</text>
            </description>
            <type name="ncd:ifname"/>
          </leaf>
          <leaf name="epd">
            <description>
              <text>Configure the local endpoint discriminator</text>
            </description>
            <type name="string">
              <length value="1..32"/>
            </type>
            <mandatory value="true"/>
          </leaf>
        </list>
      </container>
      <container name="ippool">
        <description>
          <text>IP pool correlation configuration</text>
        </description>
        <list name="ipattr">
          <description>
            <text>Configure IP address parameters</text>
          </description>
          <key value="ifname"/>
          <leaf name="ifname">
            <description>
              <text>This value identifies the PPP interface name.This attribute is created by Interface Management</text>
            </description>
            <type name="ncd:ifname"/>
          </leaf>
          <leaf name="bindipv4pool">
            <description>
              <text>Bind IPv4 pool</text>
            </description>
            <type name="string">
              <length value="1..64"/>
            </type>
          </leaf>
          <leaf name="bindipv6pool">
            <description>
              <text>Bind IPv6 pool</text>
            </description>
            <type name="string">
              <length value="1..64"/>
            </type>
          </leaf>
          <leaf name="bindauthtemp">
            <description>
              <text>Bind AAA authentication template</text>
            </description>
            <type name="uint16">
              <range value="1..2128"/>
            </type>
          </leaf>
          <leaf name="ipaccesstype">
            <description>
              <text>Configure IP access type</text>
            </description>
            <type name="enumeration">
              <enum name="dual">
                <value value="0"/>
                <description>
                  <text>IP dual stack access</text>
                </description>
              </enum>
              <enum name="ipv4">
                <value value="1"/>
                <description>
                  <text>IPv4 access</text>
                </description>
              </enum>
              <enum name="ipv6">
                <value value="2"/>
                <description>
                  <text>IPv6 access</text>
                </description>
              </enum>
            </type>
            <default value="dual"/>
          </leaf>
        </list>
      </container>
      <container name="ipcpdns">
        <description>
          <text>IPCP DNS correlation configuration</text>
        </description>
        <list name="dnsconfig">
          <description>
            <text>Configure PPP IPCP DNS IP address</text>
          </description>
          <key value="ifname"/>
          <leaf name="ifname">
            <description>
              <text>This value identifies the PPP interface name.This attribute is created by Interface Management</text>
            </description>
            <type name="ncd:ifname"/>
          </leaf>
          <leaf name="primaryaddr">
            <description>
              <text>Configure primary DNS IP address. In particular, 0.0.0.0 cannot be configured, but 0.0.0.0 may be obtained.</text>
            </description>
            <type name="ncd:ipv4-address"/>
            <mandatory value="true"/>
          </leaf>
          <leaf name="secondaryaddr">
            <description>
              <text>Configure secondary DNS IP address. In particular, 0.0.0.0 cannot be configured, but 0.0.0.0 may be obtained.</text>
            </description>
            <type name="ncd:ipv4-address"/>
          </leaf>
        </list>
      </container>
      <container name="ipcpwins">
        <description>
          <text>IPCP WINS correlation configuration</text>
        </description>
        <list name="winsconfig">
          <description>
            <text>Set PPP IPCP WINS IP adddress</text>
          </description>
          <key value="ifname"/>
          <leaf name="ifname">
            <description>
              <text>This value identifies the PPP interface name.This attribute is created by Interface Management</text>
            </description>
            <type name="ncd:ifname"/>
          </leaf>
          <leaf name="primaryaddr">
            <description>
              <text>Configure primary WINS IP address. In particular, 0.0.0.0 cannot be configured, but 0.0.0.0 may be obtained.</text>
            </description>
            <type name="ncd:ipv4-address"/>
            <mandatory value="true"/>
          </leaf>
          <leaf name="secondaryaddr">
            <description>
              <text>Configure secondary WINS IP address. In particular, 0.0.0.0 cannot be configured, but 0.0.0.0 may be obtained.</text>
            </description>
            <type name="ncd:ipv4-address"/>
          </leaf>
        </list>
      </container>
      <container name="ipcppara">
        <description>
          <text>IPCP correlation configuration</text>
        </description>
        <list name="ipcppara">
          <description>
            <text>Configure IPCP parameters</text>
          </description>
          <key value="ifname"/>
          <leaf name="ifname">
            <description>
              <text>This value identifies the PPP interface name.This attribute is created by Interface Management</text>
            </description>
            <type name="ncd:ifname"/>
          </leaf>
          <leaf name="dnsaccept">
            <description>
              <text>Accept peer PPP IPCP DNS request</text>
            </description>
            <type name="uint8">
              <range value="0..1"/>
            </type>
            <default value="0"/>
          </leaf>
          <leaf name="dnssend">
            <description>
              <text>Send PPP IPCP DNS request</text>
            </description>
            <type name="uint8">
              <range value="0..1"/>
            </type>
            <default value="0"/>
          </leaf>
          <leaf name="winsaccept">
            <description>
              <text>Accept peer PPP IPCP WINS request</text>
            </description>
            <type name="uint8">
              <range value="0..1"/>
            </type>
            <default value="0"/>
          </leaf>
          <leaf name="winssend">
            <description>
              <text>Send PPP IPCP WINS request</text>
            </description>
            <type name="uint8">
              <range value="0..1"/>
            </type>
            <default value="0"/>
          </leaf>
        </list>
      </container>
      <container name="mppp">
        <description>
          <text>MPPP bind member correlation configuration</text>
        </description>
        <list name="bindmember">
          <description>
            <text>MPPP bind member</text>
          </description>
          <key value="ifname"/>
          <leaf name="ifname">
            <description>
              <text>This value identifies the PPP interface name.This attribute is created by Interface Management</text>
            </description>
            <type name="ncd:ifname"/>
          </leaf>
          <leaf name="multilink">
            <description>
              <text>This value identifies the multilink interface name.</text>
            </description>
            <type name="ncd:ifname"/>
            <mandatory value="true"/>
          </leaf>
        </list>
      </container>
      <container name="pap">
        <description>
          <text>PAP correlation configuration</text>
        </description>
        <list name="papconfig">
          <description>
            <text>Configure PAP authentication parameters</text>
          </description>
          <key value="ifname"/>
          <leaf name="ifname">
            <description>
              <text>This value identifies the PPP interface name.This attribute is created by Interface Management</text>
            </description>
            <type name="ncd:ifname"/>
          </leaf>
          <leaf name="username">
            <description>
              <text>Configure PAP username</text>
            </description>
            <type name="string">
              <length value="1..159"/>
            </type>
            <mandatory value="true"/>
          </leaf>
          <choice name="password-choice">
            <description>
              <text>Configure PAP password</text>
            </description>
            <mandatory value="true"/>
            <case name="unencrypted-case">
              <description>
                <text>Specify the unencrypted password</text>
              </description>
              <leaf name="unencrypted-password">
                <description>
                  <text>The unencryted password</text>
                </description>
                <type name="string">
                  <length value="1..64"/>
                </type>
              </leaf>
            </case>
            <case name="encrypted-case">
              <description>
                <text>Specify the encrypted password</text>
              </description>
              <leaf name="encrypted-password">
                <description>
                  <text>The encryted password</text>
                </description>
                <type name="string">
                  <length value="1..200"/>
                </type>
              </leaf>
            </case>
          </choice>
        </list>
      </container>
      <container name="ipv6dns">
        <description>
          <text>IPv6 DNS correlation configuration</text>
        </description>
        <list name="ipv6dns">
          <description>
            <text>Configure IPv6 DNS server address</text>
          </description>
          <key value="ifname serverflag"/>
          <leaf name="ifname">
            <description>
              <text>This value identifies the PPP interface name.This attribute is created by Interface Management</text>
            </description>
            <type name="ncd:ifname"/>
          </leaf>
          <leaf name="serverflag">
            <description>
              <text>IPv6 secondary DNS server address</text>
            </description>
            <type name="uint8">
              <range value="0..1">
                <description>
                  <text>0:primary server
1:secondary server</text>
                </description>
              </range>
            </type>
          </leaf>
          <leaf name="serveraddr">
            <description>
              <text>IPv6 DNS server addr</text>
            </description>
            <type name="ncd:ipv6-address"/>
            <mandatory value="true"/>
          </leaf>
        </list>
      </container>
      <container name="nasporttype">
        <description>
          <text>Nas-Port-Type correlation configuration</text>
        </description>
        <list name="nasporttype">
          <description>
            <text>Configure NAS port type</text>
          </description>
          <key value="ifname"/>
