      <description>
        <text>LSP ID of the primary tunnel.</text>
      </description>
    </leaf>
    <leaf name="ingress-lsr-id">
      <type name="inet:ipv4-address-no-zone"/>
      <description>
        <text>Ingress LSR ID of the primary tunnel.</text>
      </description>
    </leaf>
    <leaf name="egress-lsr-id">
      <type name="inet:ipv4-address-no-zone"/>
      <description>
        <text>Egress LSR ID of the primary tunnel.</text>
      </description>
    </leaf>
    <leaf name="tunnel-admin-status">
      <type name="interface-status"/>
      <description>
        <text>Indicates the management status of the tunnel.</text>
      </description>
    </leaf>
    <leaf name="tunnel-oper-status">
      <type name="interface-status"/>
      <description>
        <text>Indicates the operating status of the tunnel.</text>
      </description>
    </leaf>
  </notification>
  <notification name="ldp-session-down">
