                  <leaf name="cir-schedule-mode">
                    <type name="queue-cir-schedule-mode"/>
                    <mandatory value="true"/>
                    <description>
                      <text>Scheduling strategy of flow queue.</text>
                    </description>
                  </leaf>
                  <leaf name="pir-schedule-mode">
                    <type name="queue-schedule-mode"/>
                    <mandatory value="true"/>
                    <description>
                      <text>Scheduling strategy of flow queue.</text>
                    </description>
                  </leaf>
                  <leaf name="pir-weight">
