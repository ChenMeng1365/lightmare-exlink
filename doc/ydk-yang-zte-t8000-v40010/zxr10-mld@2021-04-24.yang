  yang-version 1 ;
  import zxr10-netconf-defs {
    prefix ncd ;
  }
  organization 'ZTE Corporation'
               ;
  contact 'ZTE Corporation  '+"\n"
        + '   Nanjing Institute of ZTE Corporation '+"\n"
        + '   No.68 Zijinghua Rd. Yuhuatai District, Nanjing, China '+"\n"
        + '   Tel: +86-25-52870000'
          ;
  description 'The netconf module to describe MLD.'
              ;
  revision 2021-04-24 {
    description '1.Add sibtp-service leaf nodes to /configuration/router-mld/ml'
              + 'ds/interfaces/interface and /configuration/router-mld/mlds-vrf'
              + '/mld-vrf/interfaces/interface.'+"\n"
              + '2.Modify the description of the leaves: /state/mld/public/grou'
              + 'p-details/group/group-flags and /state/mld/private/group-detai'
              + 'ls/group/group-flags,/state/mld/public/group-details/group/sou'
              + 'rces/source/source-flag and /state/mld/private/group-details/g'
              + 'roup/sources/source/source-flag.'
                ;
  }
  revision 2021-01-25 {
    description 'Add ip-source-unspecified leaf nodes to /configuration/router-'
              + 'mld/mlds/global and /configuration/router-mld/mlds-vrf/mld-vrf'
              + '.'
                ;
  }
  revision 2019-07-15 {
    description 'Add maximum-joins,specific-query-ignore,access-ip-source,start'
              + 'up-query-interval and startup-query-count leaf nodes to /confi'
              + 'guration/router-mld/mlds/interfaces/interface and /configurati'
              + 'on/router-mld/mlds-vrf/mld-vrf/interfaces/interface.'
