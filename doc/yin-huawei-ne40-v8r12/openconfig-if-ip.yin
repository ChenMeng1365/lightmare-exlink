  </augment>
  <augment target-node="/oc-if:interfaces/oc-if:interface/oc-vlan:routed-vlan">
    <description>
      <text>IPv6 address family configuration for
interfaces</text>
    </description>
    <uses name="ipv6-top"/>
  </augment>
  <augment target-node="/oc-if:interfaces/oc-if:interface/oc-vlan:routed-vlan/oc-ip:ipv4/oc-ip:addresses/oc-ip:address">
    <description>
      <text>Additional IP addr family configuration for
interfaces</text>
    </description>
    <uses name="ip-vrrp-top"/>
  </augment>
  <augment target-node="/oc-if:interfaces/oc-if:interface/oc-vlan:routed-vlan/oc-ip:ipv6/oc-ip:addresses/oc-ip:address">
    <description>
      <text>Additional IP addr family configuration for
