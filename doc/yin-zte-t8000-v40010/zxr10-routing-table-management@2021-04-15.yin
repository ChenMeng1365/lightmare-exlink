      <leaf name="mask">
        <description>
          <text>Mask of an IP address.</text>
        </description>
        <type name="ncd:ipv4-mask"/>
      </leaf>
      <leaf name="changeType">
        <description>
          <text>The type of current change.</text>
        </description>
        <type name="uint8">
          <range value="1..3"/>
        </type>
      </leaf>
    </list>
    <ncd:revise-date date="2019-01-23"/>
