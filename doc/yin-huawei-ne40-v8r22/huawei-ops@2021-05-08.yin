  <container name="ops">
    <description>
      <text>The open programmability system.</text>
    </description>
    <container name="global-switch">
      <description>
        <text>Configure OPS global switch.</text>
      </description>
      <leaf name="enable">
        <type name="boolean"/>
        <default value="true"/>
        <description>
          <text>Enable/disable OPS global switch.</text>
        </description>
      </leaf>
    </container>
    <container name="python-version">
      <config value="false"/>
      <description>
        <text>Operational state of Python version.</text>
      </description>
      <leaf name="version">
        <type name="ops-python-version-type"/>
        <config value="false"/>
        <description>
          <text>Ops python version.</text>
        </description>
      </leaf>
    </container>
    <container name="assistant">
      <description>
        <text>Configure OPS assistant node.</text>
      </description>
      <container name="mtp-status-switch">
        <description>
          <text>Configure assistant maintain probe.</text>
        </description>
        <leaf name="enable">
          <type name="boolean"/>
          <default value="true"/>
          <description>
            <text>Enable/disable assistant maintain probe.</text>
          </description>
        </leaf>
      </container>
      <container name="total-status-switch">
        <description>
          <text>Configure assistant total switch.</text>
        </description>
        <leaf name="suspend">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable assistant total switch.</text>
          </description>
        </leaf>
      </container>
      <container name="script-assistants">
        <description>
          <text>List of script assistant.</text>
        </description>
        <list name="script-assistant">
          <key value="name"/>
          <max-elements value="100"/>
          <description>
            <text>Configure script assistant instance.</text>
          </description>
          <leaf name="name">
            <type name="string">
              <length value="4..64"/>
              <pattern value="([^_].*\.py)"/>
            </type>
            <description>
              <text>Script assistant name.</text>
            </description>
          </leaf>
          <leaf name="shutdown">
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable script assistant.</text>
            </description>
          </leaf>
        </list>
      </container>
      <container name="cli-assistants">
        <description>
          <text>List of cli assistant.</text>
        </description>
        <list name="cli-assistant">
          <key value="name"/>
          <max-elements value="100"/>
          <description>
            <text>Configure assistant instance.</text>
          </description>
          <leaf name="name">
            <type name="string">
              <length value="1..15"/>
              <pattern value="([A-Za-z][A-Za-z0-9_]*)"/>
            </type>
            <description>
              <text>Name of assistant.</text>
            </description>
          </leaf>
          <leaf name="shutdown">
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable cli assistant.</text>
            </description>
          </leaf>
          <leaf name="condition-type">
            <type name="ops-assistant-cond-type"/>
            <description>
              <text>Condition type.</text>
            </description>
          </leaf>
          <container name="condition">
            <description>
              <text>Configure condition of assistant task.</text>
            </description>
            <container name="alarm">
              <when condition="../../condition-type='alarm'"/>
              <description>
                <text>Configure the conditions of match criteria.</text>
              </description>
              <leaf name="feature-name">
                <type name="string">
                  <length value="1..31"/>
                </type>
                <description>
                  <text>Feature name.</text>
                </description>
              </leaf>
              <leaf name="alarm-name">
                <type name="string">
                  <length value="1..63"/>
                </type>
                <description>
                  <text>Alarm name.</text>
                </description>
              </leaf>
              <leaf name="alarm-state">
                <type name="alarm-state-type"/>
                <description>
                  <text>The state of alarm, start or end.</text>
                </description>
              </leaf>
              <leaf name="occur-number">
                <type name="uint32">
                  <range value="1..32"/>
                </type>
                <description>
                  <text>The number of matching occurrences before the assistant is triggered.</text>
                </description>
              </leaf>
              <leaf name="period">
                <type name="uint32">
                  <range value="1..4294967295"/>
                </type>
                <description>
                  <text>The period of check matching occurrences.</text>
                </description>
              </leaf>
              <container name="parameters">
                <description>
                  <text>List of cli assistant's parameters.</text>
                </description>
                <list name="parameter">
                  <key value="name"/>
                  <max-elements value="4"/>
                  <description>
                    <text>Configure assistant parameters instance.</text>
                  </description>
                  <leaf name="name">
                    <type name="string">
                      <length value="1..32"/>
                    </type>
                    <description>
                      <text>Parameter ID.</text>
                    </description>
                  </leaf>
                  <leaf name="operator">
                    <type name="ops-operation-type"/>
                    <description>
                      <text>The operation type.</text>
                    </description>
                  </leaf>
                  <leaf name="value">
                    <type name="string">
                      <length value="1..63"/>
                    </type>
                    <description>
                      <text>Operation value.</text>
                    </description>
                  </leaf>
                </list>
              </container>
            </container>
            <container name="event">
              <when condition="../../condition-type='event'"/>
              <description>
                <text>Configure the conditions of match criteria.</text>
              </description>
              <leaf name="feature-name">
                <type name="string">
                  <length value="1..31"/>
                </type>
                <description>
                  <text>Feature name.</text>
                </description>
              </leaf>
              <leaf name="log-name">
                <type name="string">
                  <length value="1..63"/>
                </type>
                <description>
                  <text>Log name.</text>
                </description>
              </leaf>
              <leaf name="occur-number">
                <type name="uint32">
                  <range value="1..32"/>
                </type>
                <description>
                  <text>The number of matching occurrences before the assistant is triggered.</text>
                </description>
              </leaf>
              <leaf name="period">
                <type name="uint32">
                  <range value="1..4294967295"/>
                </type>
                <description>
                  <text>The period of check matching occurrences.</text>
                </description>
              </leaf>
              <container name="parameters">
                <description>
                  <text>List of cli assistant's parameters.</text>
                </description>
                <list name="parameter">
                  <key value="name"/>
                  <max-elements value="4"/>
                  <description>
                    <text>Configure assistant parameters instance.</text>
                  </description>
                  <leaf name="name">
                    <type name="string">
                      <length value="1..32"/>
                    </type>
                    <description>
                      <text>Parameter ID.</text>
                    </description>
                  </leaf>
                  <leaf name="operator">
                    <type name="ops-operation-type"/>
                    <description>
                      <text>The operation type.</text>
                    </description>
                  </leaf>
                  <leaf name="value">
                    <type name="string">
                      <length value="1..63"/>
                    </type>
                    <description>
                      <text>Operation value.</text>
                    </description>
                  </leaf>
                </list>
              </container>
            </container>
            <container name="snmp-notification">
              <when condition="../../condition-type='snmp-notification'"/>
              <description>
                <text>Configure the conditions of match criteria.</text>
              </description>
              <leaf name="oid">
                <type name="string">
                  <length value="1..127"/>
                </type>
                <description>
                  <text>The oid string. For exapmle: 1.3.6.1.4.1.2011.5.25.177.1.3.1.</text>
                </description>
              </leaf>
              <leaf name="operator">
                <type name="ops-operation-type"/>
                <description>
                  <text>The operation type.</text>
                </description>
              </leaf>
              <leaf name="oid-value">
                <type name="int32"/>
                <description>
                  <text>The field value of oid string.</text>
                </description>
              </leaf>
            </container>
            <container name="syslog">
              <when condition="../../condition-type='syslog'"/>
              <description>
                <text>Configure the conditions of match criteria.</text>
              </description>
              <leaf name="pattern">
                <type name="string">
                  <length value="1..255"/>
                </type>
                <description>
                  <text>The regular expression of syslog.</text>
                </description>
              </leaf>
              <leaf name="occur-number">
                <type name="uint32">
                  <range value="1..32"/>
                </type>
                <description>
                  <text>The number of matching occurrences before the assistant is triggered.</text>
                </description>
              </leaf>
              <leaf name="period">
                <type name="uint32">
                  <range value="1..4294967295"/>
                </type>
                <description>
                  <text>The period of check matching occurrences.</text>
                </description>
              </leaf>
            </container>
            <container name="timer">
              <when condition="../../condition-type='timer'"/>
              <description>
                <text>Configure the conditions of match criteria.</text>
              </description>
              <leaf name="minute">
                <type name="string">
                  <length value="1..63"/>
                </type>
                <description>
                  <text>Minute vaule (0-59) in crontab expression.</text>
                </description>
              </leaf>
              <leaf name="hour">
                <type name="string">
                  <length value="1..63"/>
                </type>
                <description>
                  <text>Hour vaule (0-23) in crontab expression.</text>
                </description>
              </leaf>
              <leaf name="day-of-month">
                <type name="string">
                  <length value="1..63"/>
                </type>
                <description>
                  <text>Day-of-month vaule (0-31) in crontab expression.</text>
                </description>
              </leaf>
              <leaf name="month">
                <type name="string">
                  <length value="1..63"/>
                </type>
                <description>
                  <text>Month vaule (0-31) in crontab expression.</text>
                </description>
              </leaf>
              <leaf name="day-of-week">
                <type name="string">
                  <length value="1..63"/>
                </type>
                <description>
                  <text>Day-of-week vaule (0-7) in crontab expression (0 to 6 are Sunday to Saturday, 7 is Sunday,the same as 0).</text>
                </description>
              </leaf>
              <leaf name="year">
                <type name="string">
                  <length value="1..4"/>
                </type>
                <description>
                  <text>Year vaule (2000-2099) in crontab expression.</text>
                </description>
              </leaf>
            </container>
          </container>
          <container name="executions">
            <description>
              <text>List of assistant execution node.</text>
            </description>
            <list name="execution">
              <key value="priority"/>
              <max-elements value="10"/>
              <description>
                <text>Configure assistant execution instance.</text>
              </description>
              <leaf name="priority">
                <type name="string">
                  <length value="1..7"/>
                  <pattern value="(\d+(\.\d+)?)"/>
                </type>
                <description>
                  <text>The action sequence number, such as 1, 1.1, 2, 3.</text>
                </description>
              </leaf>
              <leaf name="execution-type">
                <type name="ops-execution-type"/>
                <description>
                  <text>The execution type,now support command,python,batch-file.</text>
                </description>
              </leaf>
              <leaf name="command">
                <when condition="../execution-type='command'"/>
                <type name="string">
                  <length value="1..511"/>
                </type>
                <description>
                  <text>The action content, command string.</text>
                </description>
              </leaf>
              <leaf name="file-name">
                <when condition="../execution-type='python' or ../execution-type='batch-file'"/>
                <type name="string">
                  <length value="1..127"/>
                  <pattern value="(.+\.(py|bat))"/>
                </type>
                <description>
                  <text>The file name.</text>
                </description>
              </leaf>
              <leaf name="argument">
                <when condition="../execution-type='python'"/>
                <type name="string">
                  <length value="1..383"/>
                </type>
                <description>
                  <text>The python script arguments.</text>
                </description>
              </leaf>
            </list>
          </container>
        </list>
      </container>
    </container>
    <container name="environments">
      <description>
        <text>List of OPS environment node.</text>
      </description>
      <list name="environment">
        <key value="name"/>
        <max-elements value="100"/>
        <description>
          <text>Configure script environment instance.</text>
        </description>
        <leaf name="name">
          <type name="string">
            <length value="1..31"/>
          </type>
          <description>
            <text>The environment variable name.</text>
          </description>
        </leaf>
        <leaf name="value">
          <type name="string">
            <length value="1..256"/>
          </type>
          <description>
            <text>The environment variable value.</text>
          </description>
        </leaf>
      </list>
    </container>
    <container name="applications">
      <config value="false"/>
      <description>
        <text>List of the OPS application.</text>
      </description>
      <list name="application">
        <key value="file-name"/>
        <config value="false"/>
        <description>
          <text>Operational state of OPS application node.</text>
        </description>
        <leaf name="file-name">
          <type name="string">
            <length value="1..255"/>
          </type>
          <config value="false"/>
          <description>
            <text>OPS application file name.</text>
          </description>
        </leaf>
        <leaf name="size">
          <type name="uint32"/>
          <config value="false"/>
          <description>
            <text>The file size (unit:byte).</text>
          </description>
        </leaf>
      </list>
    </container>
    <container name="contexts">
      <config value="false"/>
      <description>
        <text>List of the OPS context.</text>
      </description>
      <list name="context">
        <key value="variable"/>
        <config value="false"/>
        <description>
          <text>Operational data of OPS context node.</text>
        </description>
        <leaf name="variable">
          <type name="string">
            <length value="1..17"/>
          </type>
          <description>
            <text>Name of variable.</text>
          </description>
        </leaf>
        <leaf name="creator">
          <type name="string">
            <length value="1..128"/>
          </type>
          <description>
            <text>Script name of access.</text>
          </description>
        </leaf>
        <leaf name="value">
          <type name="string">
            <length value="1..65"/>
          </type>
          <description>
            <text>Value of varible.</text>
          </description>
        </leaf>
        <leaf name="access-type">
          <type name="ops-access-type"/>
          <description>
            <text>Assistant type.</text>
          </description>
        </leaf>
        <leaf name="access-time">
          <type name="yang:date-and-time"/>
          <description>
            <text>When access.</text>
          </description>
        </leaf>
      </list>
    </container>
    <container name="process-current-states">
      <config value="false"/>
      <description>
        <text>List of the current states of OPS processes.</text>
      </description>
      <list name="process-current-state">
        <key value="process-id"/>
        <config value="false"/>
        <description>
          <text>Operational state of OPS process node.</text>
        </description>
        <leaf name="process-id">
          <type name="uint32"/>
          <description>
            <text>Process ID.</text>
          </description>
        </leaf>
        <leaf name="command">
          <type name="string">
            <length value="1..256"/>
          </type>
          <description>
            <text>Start command.</text>
          </description>
        </leaf>
        <leaf name="process-name">
          <type name="string">
            <length value="0..256"/>
          </type>
          <description>
            <text>Process Name.</text>
          </description>
        </leaf>
        <leaf name="state">
          <type name="ops-process-state"/>
          <description>
            <text>The state, such as running, suspend, waiting.</text>
          </description>
        </leaf>
        <leaf name="owner-type">
          <type name="ops-owner-type"/>
          <description>
            <text>Type of owner.</text>
          </description>
        </leaf>
        <leaf name="owner-name">
          <type name="string">
            <length value="0..64"/>
          </type>
          <description>
            <text>Name of owner.</text>
          </description>
        </leaf>
        <leaf name="is-background">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Run in background flag.</text>
          </description>
        </leaf>
        <leaf name="trigger-time">
          <type name="string">
            <length value="1..20"/>
          </type>
          <description>
            <text>Trigger time.</text>
          </description>
        </leaf>
        <leaf name="start-time">
          <type name="string">
            <length value="1..20"/>
          </type>
          <description>
            <text>Start time.</text>
          </description>
        </leaf>
      </list>
    </container>
    <container name="process-history-states">
      <config value="false"/>
      <description>
        <text>List of the history states of OPS processes.</text>
      </description>
      <list name="process-history-state">
        <key value="process-id"/>
        <config value="false"/>
        <description>
          <text>Operational state of OPS process node.</text>
        </description>
        <leaf name="process-id">
          <type name="uint32">
            <range value="0..4294967295"/>
          </type>
          <description>
            <text>Process ID.</text>
          </description>
        </leaf>
        <leaf name="command">
          <type name="string">
            <length value="1..256"/>
          </type>
          <description>
            <text>Start command.</text>
          </description>
        </leaf>
        <leaf name="owner-type">
          <type name="ops-owner-type"/>
          <description>
            <text>Type of owner.</text>
          </description>
        </leaf>
        <leaf name="owner-name">
          <type name="string">
            <length value="0..64"/>
          </type>
          <description>
            <text>Name of owner.</text>
          </description>
        </leaf>
        <leaf name="is-background">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Run in background flag.</text>
          </description>
        </leaf>
        <leaf name="result">
          <type name="ops-process-state-return-type"/>
          <description>
            <text>The result of script.</text>
          </description>
        </leaf>
        <leaf name="trigger-time">
          <type name="yang:date-and-time"/>
          <description>
            <text>Trigger time.</text>
          </description>
        </leaf>
        <leaf name="start-time">
          <type name="yang:date-and-time"/>
          <description>
            <text>Start time.</text>
          </description>
        </leaf>
        <leaf name="end-time">
          <type name="yang:date-and-time"/>
          <description>
            <text>Start time.</text>
          </description>
        </leaf>
      </list>
    </container>
  </container>
  <rpc name="install-application">
    <description>
      <text>Install OPS application.</text>
    </description>
    <ext:node-ref value="/ops/applications/application"/>
    <input>
      <leaf name="file-name">
        <type name="string">
          <length value="1..127"/>
        </type>
        <mandatory value="true"/>
        <description>
          <text>The install OPS applcation file name.</text>
        </description>
      </leaf>
      <leaf name="destination">
        <type name="string">
          <length value="1..127"/>
        </type>
        <description>
          <text>The install OPS applcation file destination.</text>
        </description>
      </leaf>
    </input>
  </rpc>
  <rpc name="uninstall-application">
    <description>
      <text>Uninstall OPS application.</text>
    </description>
    <ext:node-ref value="/ops/applications/application"/>
    <input>
      <leaf name="file-name">
        <type name="string">
          <length value="1..127"/>
        </type>
        <mandatory value="true"/>
        <description>
          <text>Uninstall OPS applcation file name.</text>
        </description>
      </leaf>
    </input>
  </rpc>
  <rpc name="run-python-script">
    <description>
      <text>Run python script.</text>
    </description>
    <ext:node-ref value="/ops/applications/application"/>
    <input>
      <leaf name="name">
        <type name="string">
          <length value="1..127"/>
        </type>
        <mandatory value="true"/>
        <description>
          <text>Python script name.</text>
        </description>
      </leaf>
      <leaf name="argument">
        <type name="string">
          <length value="1..383"/>
        </type>
        <description>
          <text>Python script argument.</text>
        </description>
      </leaf>
    </input>
  </rpc>
</module>
