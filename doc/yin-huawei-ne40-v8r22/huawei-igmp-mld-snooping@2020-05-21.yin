  <typedef name="l2mc-esi">
    <type name="string">
      <length value="0..24"/>
      <pattern value="(^ *(00([0-9a-fA-F]){2}\.(([0-9a-fA-F]){4}\.){3}(([0-9a-fA-F]){4}))? *$)"/>
    </type>
    <description>
      <text>ESI is begin with a-f or A-f or 0-9.</text>
    </description>
  </typedef>
