<?xml version="1.0" encoding="UTF-8"?>
<module name="zxr10-pss-rtm"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:pss-rtm="http://www.zte.com.cn/zxr10/netconf/schema/rosng/pss-rtm"
        xmlns:ncd="http://www.zte.com.cn/zxr10/netconf/schema/rosng/netconf-defs">
  <namespace uri="http://www.zte.com.cn/zxr10/netconf/schema/rosng/pss-rtm"/>
  <prefix value="pss-rtm"/>
  <yang-version value="1"/>
  <import module="zxr10-netconf-defs">
    <prefix value="ncd"/>
  </import>
  <organization>
    <text>ZTE Corporation</text>
  </organization>
  <contact>
    <text>ZTE Corporation  
   Nanjing Institute of ZTE Corporation 
   No.68 Zijinghua Rd. Yuhuatai District, Nanjing, China 
   Tel: +86-25-52870000</text>
  </contact>
  <description>
    <text>The netconf module to describe the routing table management(rtm).</text>
  </description>
  <revision date="2019-12-11">
    <description>
      <text>Add new project label of 6100/6700/65V5 to support fib-high-threshold.</text>
    </description>
  </revision>
  <revision date="2019-06-28">
    <description>
      <text>This Netconf module provides commonly used textual conventions for RTM.
It is initial version.</text>
    </description>
  </revision>
  <feature name="fib-high-threshold"/>
  <container name="configuration">
    <description>
      <text>This module config rtm configuration.</text>
    </description>
    <container name="alarm-config">
      <description>
        <text>This module config rtm alarm configuration.</text>
      </description>
      <container name="ipv4-fib-high-threshold">
        <description>
          <text>IPv4 FIB alarm threshold configuration.</text>
        </description>
        <presence value="ipv4-fib-high-threshold"/>
        <leaf name="warning-threshold">
          <description>
            <text>Maximum IPv4 FIB threshold to set.</text>
          </description>
          <type name="uint8">
            <range value="1..100"/>
          </type>
          <mandatory value="true"/>
        </leaf>
      </container>
      <container name="ipv6-fib-high-threshold">
        <description>
          <text>IPv6 FIB alarm threshold configuration.</text>
        </description>
        <presence value="ipv6-fib-high-threshold"/>
        <leaf name="warning-threshold">
          <description>
            <text>Maximum IPv6 FIB threshold to set.</text>
          </description>
          <type name="uint8">
            <range value="1..100"/>
          </type>
          <mandatory value="true"/>
        </leaf>
      </container>
    </container>
  </container>
</module>
