                  <description>
                    <text>Static VXLAN Tunnel.</text>
                  </description>
                  <container name="statics">
                    <description>
                      <text>Static VXLAN Tunnel.</text>
                    </description>
                    <leaf name="vxlan-interface">
                      <description>
                        <text>The VXLAN interface of sending packets.</text>
                      </description>
                      <type name="ncd:ifname">
                        <length value="1..31">
                          <description>
                            <text>The VXLAN interface of sending packets.</text>
                          </description>
                        </length>
                      </type>
                      <mandatory value="true"/>
                    </leaf>
                  </container>
                </case>
              </choice>
            </container>
            <container name="common-opt">
              <description>
                <text>Option parameters for IPv4 TRACE.</text>
              </description>
              <leaf name="max-ttl">
                <description>
                  <text>The maximum of time to live.</text>
                </description>
                <type name="uint8">
                  <range value="1..255">
                    <description>
                      <text>The maximum of time to live.</text>
                    </description>
                  </range>
                </type>
                <default value="30"/>
              </leaf>
              <leaf name="time-out">
                <description>
                  <text>Timeout.</text>
                </description>
                <type name="uint8">
                  <range value="1..20">
                    <description>
                      <text>Timeout.</text>
                    </description>
                  </range>
                </type>
                <default value="3"/>
              </leaf>
              <leaf name="size">
                <description>
                  <text>The packet size.</text>
                </description>
                <type name="int16">
                  <range value="36..8192">
                    <description>
                      <text>The packet size, include IP header.</text>
                    </description>
                  </range>
                </type>
                <default value="100"/>
              </leaf>
              <leaf name="pattern">
                <description>
                  <text>The pad bytes.</text>
                </description>
                <type name="string">
                  <length value="4">
                    <description>
                      <text>The pattern string.</text>
                    </description>
                  </length>
                </type>
              </leaf>
            </container>
          </case>
          <case name="disable">
            <description>
              <text>Stop IPv4 TRACE function.</text>
            </description>
            <leaf name="trace-disable">
              <description>
                <text>Stop IPv4 TRACE function.</text>
              </description>
              <type name="empty"/>
              <mandatory value="true"/>
            </leaf>
          </case>
        </choice>
      </list>
      <ncd:revise-date date="2020-04-07"/>
    </container>
