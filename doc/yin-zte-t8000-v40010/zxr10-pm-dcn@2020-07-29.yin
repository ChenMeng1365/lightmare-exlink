        </leaf>
        <ncd:revise-date date="2019-09-02"/>
      </container>
      <container name="dcn-ospf-area">
        <description>
          <text>Configure OSPFv2 area route aggregation</text>
        </description>
        <choice name="area-id">
          <description>
            <text>OSPFv2 area</text>
          </description>
          <case name="number-format">
            <description>
              <text>OSPFv2 area in number format</text>
            </description>
            <leaf name="number">
              <description>
                <text>OSPFv2 area in number format</text>
              </description>
              <type name="uint32">
                <range value="0..4294967295"/>
              </type>
            </leaf>
          </case>
          <case name="ip-format">
            <description>
              <text>OSPFv2 area in IPv4 address format</text>
            </description>
            <leaf name="ip">
              <description>
                <text>OSPFv2 area in IPv4 address format</text>
              </description>
              <type name="ncd:ipv4-address"/>
            </leaf>
          </case>
        </choice>
        <leaf name="routeaggr-ip-address">
          <description>
