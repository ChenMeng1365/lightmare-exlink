            <description>
              <text>Configure shaping of user queue template.</text>
            </description>
            <choice name="parameter">
              <mandatory value="true"/>
              <description>
                <text>Specify parameter.</text>
              </description>
              <case name="value-mode">
                <description>
                  <text>Specify shaping parameter choice case as value mode.</text>
                </description>
                <leaf name="pir-value">
                  <type name="uint32">
                    <range value="0 | 16..4294967294"/>
                  </type>
                  <units name="kbit/s"/>
                  <must condition="../pir-value&gt;=../cir-value or not(../cir-value)"/>
                  <mandatory value="true"/>
                  <description>
                    <text>Peak information rate.</text>
                  </description>
                </leaf>
                <leaf name="cir-value">
                  <type name="uint32">
                    <range value="0 | 16..4294967294"/>
                  </type>
                  <units name="kbit/s"/>
                  <must condition="../pir-value&gt;=../cir-value"/>
                  <description>
                    <text>Committed information rate.</text>
                  </description>
                </leaf>
              </case>
              <case name="percent-mode">
                <description>
                  <text>Specify shaping parameter choice case as percent mode.</text>
                </description>
                <leaf name="pir-percent">
                  <type name="uint8">
                    <range value="0..100"/>
                  </type>
                  <units name="%"/>
                  <must condition="../pir-percent&gt;=../cir-percent or not(../cir-percent)"/>
                  <mandatory value="true"/>
                  <description>
                    <text>Peak information rate percent.</text>
                  </description>
                </leaf>
                <leaf name="cir-percent">
                  <type name="uint8">
                    <range value="0..100"/>
                  </type>
                  <units name="%"/>
                  <must condition="../pir-percent&gt;=../cir-percent"/>
                  <description>
                    <text>Committed information rate percent.</text>
                  </description>
                </leaf>
              </case>
            </choice>
