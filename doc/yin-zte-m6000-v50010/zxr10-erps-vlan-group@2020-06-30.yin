<?xml version="1.0" encoding="UTF-8"?>
<module name="zxr10-erps-vlan-group"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:erps-vlan-group="http://www.zte.com.cn/zxr10/netconf/schema/rosng/erps-vlan-group"
        xmlns:ncd="http://www.zte.com.cn/zxr10/netconf/schema/rosng/netconf-defs">
  <namespace uri="http://www.zte.com.cn/zxr10/netconf/schema/rosng/erps-vlan-group"/>
  <prefix value="erps-vlan-group"/>
  <import module="zxr10-netconf-defs">
    <prefix value="ncd"/>
  </import>
  <organization>
    <text>ZTE Corporation</text>
  </organization>
  <contact>
    <text>ZTE Corporation  
   Nanjing Institute of ZTE Corporation 
   No.68 Zijinghua Rd. Yuhuatai District, Nanjing, China 
   Tel: +86-25-52870000</text>
  </contact>
  <description>
    <text>The netconf module to describe the ERPS(Ethernet Ring Protection Switching) group.</text>
  </description>
  <revision date="2020-06-30">
    <description>
      <text>1.Create the ERPS(Ethernet Ring Protection Switching)VLAN group netconf model.</text>
    </description>
  </revision>
  <typedef name="erps-group-port-role">
    <type name="enumeration">
      <enum name="rpl-normal">
        <value value="0"/>
        <description>
          <text>RPL normal port.</text>
        </description>
      </enum>
      <enum name="rpl-owner">
        <value value="1"/>
        <description>
          <text>RPL owner port.</text>
        </description>
      </enum>
      <enum name="rpl-neighbour">
        <value value="2"/>
        <description>
          <text>RPL neighbour port.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Port role of ERPS VLAN group.</text>
    </description>
  </typedef>
  <container name="configuration">
    <description>
      <text>Configuration model information of ERPS VLAN group.</text>
    </description>
    <container name="erps">
      <description>
        <text>The configuration information of ERPS VLAN group.</text>
      </description>
      <container name="VLAN-groups">
        <description>
          <text>Configure ERPS VLAN groups.</text>
        </description>
        <list name="VLAN-group-instance">
          <description>
            <text>ERPS VLAN group instance.</text>
          </description>
          <key value="group-id"/>
          <leaf name="group-id">
            <description>
              <text>Group ID of ERPS VLAN group.</text>
            </description>
            <type name="uint32">
              <range value="1..239">
                <description>
                  <text>The range of ERPS VLAN group id.</text>
                </description>
              </range>
            </type>
          </leaf>
          <container name="control-vlan">
            <description>
              <text>Control VLAN of ERPS VLAN group.</text>
            </description>
            <presence value="control vlan"/>
            <leaf name="vlan-id">
              <description>
                <text>Control protect VLAN of ERPS VLAN group.</text>
              </description>
              <type name="uint16">
                <range value="1..4094">
                  <description>
                    <text>The range of VLAN id.</text>
                  </description>
                </range>
              </type>
              <mandatory value="true"/>
            </leaf>
          </container>
          <container name="east-port">
            <description>
              <text>East port of ERPS VLAN group.</text>
            </description>
            <presence value="east-port"/>
            <leaf name="port-role">
              <description>
                <text>Configure east port role of ERPS VLAN group.</text>
              </description>
              <type name="erps-group-port-role"/>
              <default value="rpl-normal"/>
            </leaf>
            <leaf name="if-name">
              <description>
                <text>Configure east port of ERPS VLAN group.</text>
              </description>
              <type name="ncd:ifname"/>
              <mandatory value="true"/>
            </leaf>
          </container>
          <container name="west-port">
            <description>
              <text>West port of ERPS VLAN group.</text>
            </description>
            <presence value="west-port"/>
            <leaf name="port-role">
              <description>
                <text>Configure west port role of ERPS VLAN group.</text>
              </description>
              <type name="erps-group-port-role"/>
              <default value="rpl-normal"/>
            </leaf>
            <leaf name="if-name">
              <description>
                <text>Configure east port of ERPS VLAN group.</text>
              </description>
              <type name="ncd:ifname"/>
              <mandatory value="true"/>
            </leaf>
          </container>
          <container name="protect-strategy">
            <description>
              <text>Protection strategy of ERPS VLAN group.</text>
            </description>
            <presence value="protection-strategy"/>
            <leaf name="switch-strategy">
              <description>
                <text>Protection strategy type.</text>
              </description>
              <type name="enumeration">
                <enum name="aps">
                  <value value="1"/>
                  <description>
                    <text>Protection strategy type is aps.</text>
                  </description>
                </enum>
              </type>
              <mandatory value="true"/>
            </leaf>
          </container>
          <container name="protect-vlan">
            <description>
              <text>Protection VLAN of ERPS VLAN group.</text>
            </description>
            <presence value="protection-instance"/>
            <list name="protect-vlan">
              <description>
                <text>Protection VLAN of ERPS VLAN group.</text>
              </description>
              <key value="vlan-id"/>
              <leaf name="vlan-id">
                <description>
                  <text>Control protect VLAN of ERPS VLAN group.</text>
                </description>
                <type name="uint16">
                  <range value="1..4094">
                    <description>
                      <text>The range of VLAN id.</text>
                    </description>
                  </range>
                </type>
                <mandatory value="true"/>
              </leaf>
            </list>
          </container>
        </list>
      </container>
    </container>
  </container>
</module>
