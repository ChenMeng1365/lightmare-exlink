      <enum name="te-bfd">
        <value value="3"/>
        <description>
          <text>Indicates that the resource is dynamic BFD for TE LSP.</text>
        </description>
      </enum>
      <enum name="ldp-bfd">
        <value value="4"/>
        <description>
          <text>Indicates that the resource is dynamic BFD for LDP LSP.</text>
        </description>
      </enum>
      <enum name="mldp-total-tree">
        <value value="5"/>
        <description>
          <text>Indicates that the resource is the total number of MLDP trees.</text>
        </description>
      </enum>
      <enum name="mldp-total-branch">
        <value value="6"/>
        <description>
          <text>Indicates that the resource is the total number of MLDP branches.</text>
        </description>
      </enum>
      <enum name="ldp-total-remote-adjacency">
        <value value="7"/>
        <description>
          <text>Indicates that the resource is LDP total remote adjacency.</text>
        </description>
      </enum>
      <enum name="outsegment">
        <value value="8"/>
        <description>
          <text>Indicates that the resource is LDP outSegment.</text>
        </description>
      </enum>
      <enum name="auto-primary-tunnelif">
        <value value="9"/>
        <description>
          <text>Indicates that the resource is auto primary tunnel interface.</text>
        </description>
      </enum>
      <enum name="ldp-total-local-adjacency">
        <value value="10"/>
        <description>
          <text>Indicates that the resource is LDP total local adjacency.</text>
        </description>
      </enum>
      <enum name="cspf-node">
        <value value="11"/>
        <description>
          <text>Indicates that the resource is cspf node.</text>
        </description>
      </enum>
      <enum name="cspf-link">
        <value value="12"/>
        <description>
          <text>Indicates that the resource is cspf link.</text>
        </description>
      </enum>
      <enum name="cspf-nlsa">
        <value value="13"/>
        <description>
          <text>Indicates that the resource is cspf nlsa.</text>
        </description>
      </enum>
      <enum name="cspf-srlg">
        <value value="14"/>
        <description>
          <text>Indicates that the resource is cspf srlg.</text>
        </description>
      </enum>
      <enum name="rsvp-lsp">
        <value value="15"/>
        <description>
          <text>Indicates that the resource is rsvp lsp.</text>
