  </import>
  <include module="huawei-bgp-type"/>
  <organization>
    <text>Huawei Technologies Co., Ltd.</text>
  </organization>
  <contact>
    <text>Huawei Industrial Base
Bantian, Longgang
Shenzhen 518129
People's Republic of China
Website: http://www.huawei.com
Email: support@huawei.com</text>
  </contact>
  <description>
    <text>Inter-AS routing protocol that exchanges Network Layer Reachability Information (NLRI) with other BGP systems.</text>
  </description>
  <revision date="2021-02-03">
    <description>
      <text>Initial revision.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <grouping name="advertise-dependent-filter-type">
    <description>
      <text>The advertise-dependent-filter-type configuration.</text>
    </description>
    <container name="advertise-dependent-filter">
      <presence value="Enable peer advertising dependent filter."/>
      <description>
        <text>Configure peer advertising dependent filter.</text>
      </description>
      <leaf name="dependent-filter-switch">
        <type name="bgp:peer-attr-enable-type"/>
        <mandatory value="true"/>
        <description>
          <text>Specify a switch type.</text>
        </description>
      </leaf>
      <leaf name="dependent-filter">
        <when condition="../dependent-filter-switch='enable'"/>
        <type name="leafref">
          <path value="/rtp:routing-policy/rtp:ipv4-prefix-lists/rtp:ipv4-prefix-list/rtp:name"/>
        </type>
        <mandatory value="true"/>
        <description>
          <text>Specify a dependent route-filter name.</text>
        </description>
      </leaf>
      <leaf name="dependent-filter-type">
        <when condition="../dependent-filter-switch='enable'"/>
        <type name="bgp:dependent-prefix-filter-type"/>
        <mandatory value="true"/>
        <description>
          <text>Specify a dependent route-filter mode.</text>
        </description>
      </leaf>
      <leaf name="condition-filter">
        <when condition="../dependent-filter-switch='enable'"/>
        <type name="leafref">
          <path value="/rtp:routing-policy/rtp:ipv4-prefix-lists/rtp:ipv4-prefix-list/rtp:name"/>
        </type>
        <description>
          <text>Specify a condition route-filter name.</text>
        </description>
      </leaf>
    </container>
    <container name="advertise-dependent-filter-state">
      <config value="false"/>
      <description>
        <text>Peer advertising dependent filter state.</text>
      </description>
      <leaf name="dependent-filter">
        <type name="leafref">
          <path value="/rtp:routing-policy/rtp:ipv4-prefix-lists/rtp:ipv4-prefix-list/rtp:name"/>
        </type>
        <description>
          <text>Specify a dependent route-filter name.</text>
        </description>
      </leaf>
      <leaf name="dependent-filter-type">
        <type name="bgp:dependent-prefix-filter-type"/>
        <description>
          <text>Specify a dependent route-filter mode.</text>
        </description>
      </leaf>
      <leaf name="condition-filter">
        <type name="leafref">
          <path value="/rtp:routing-policy/rtp:ipv4-prefix-lists/rtp:ipv4-prefix-list/rtp:name"/>
        </type>
        <description>
          <text>Specify a condition route-filter name.</text>
        </description>
      </leaf>
    </container>
  </grouping>
