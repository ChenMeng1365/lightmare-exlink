          <max-elements value="20"/>
          <status value="deprecated"/>
          <description>
            <text>Configure community entry.</text>
          </description>
          <leaf name="name">
            <type name="pub-type:password-extend">
              <length value="1..32 | 88..168"/>
            </type>
            <must condition="string-length(../name)&gt;=../../../../system/password-min-length or ../../../../system/community-cmplx-check='false'"/>
            <status value="deprecated"/>
            <description>
              <text>Unique name to identify the community.</text>
            </description>
          </leaf>
          <leaf name="engine-id">
            <ext:support-filter value="true"/>
            <type name="snmp-engine-id"/>
            <mandatory value="true"/>
            <status value="deprecated"/>
            <description>
              <text>Remote engine ID of the SNMP community.</text>
            </description>
          </leaf>
          <leaf name="acl">
            <type name="string">
              <length value="1..32"/>
            </type>
            <status value="deprecated"/>
            <description>
              <text>ACL, leafref path /acl/aclGroups/aclGroup/aclNumOrName or /acl/aclGroup6s/aclGroup6/aclNumOrName.</text>
            </description>
          </leaf>
          <leaf name="alias-name">
            <type name="string">
              <length value="1..32"/>
            </type>
            <must condition="(../alias-name!=../name)"/>
            <status value="deprecated"/>
            <description>
              <text>Unique alias name to identify community.</text>
            </description>
          </leaf>
        </list>
      </container>
      <container name="standard-communitys">
        <description>
          <text>List of SNMP proxy communtiy for SNMPv1 and SNMPv2.</text>
        </description>
        <list name="standard-community">
          <key value="alias-name"/>
          <unique tag="name"/>
          <max-elements value="20"/>
          <description>
            <text>Configure community entry.</text>
          </description>
          <leaf name="alias-name">
            <type name="string">
              <length value="1..32"/>
            </type>
            <must condition="(../alias-name!=../name)"/>
            <description>
              <text>Unique alias name to identify community.</text>
            </description>
          </leaf>
          <leaf name="name">
            <type name="pub-type:password-extend">
              <length value="1..32 | 88..168"/>
            </type>
            <must condition="string-length(../name)&gt;=../../../../system/password-min-length or ../../../../system/community-cmplx-check='false'"/>
            <mandatory value="true"/>
            <description>
              <text>Unique name to identify the community.</text>
            </description>
          </leaf>
          <leaf name="engine-id">
            <ext:support-filter value="true"/>
            <type name="snmp-engine-id"/>
            <mandatory value="true"/>
            <description>
              <text>Remote engine ID of the SNMP community.</text>
            </description>
          </leaf>
          <leaf name="acl">
            <type name="string">
              <length value="1..32"/>
            </type>
            <description>
              <text>ACL, leafref path /acl:acl/acl:groups/acl:group/acl:identity or /acl:acl/acl:group6s/acl:group6/acl:identity.</text>
            </description>
          </leaf>
        </list>
      </container>
      <container name="target-hosts">
        <description>
          <text>List of alarm hosts.</text>
        </description>
        <list name="target-host">
          <key value="name"/>
          <max-elements value="20"/>
          <description>
            <text>Configure address and related params of target host entry which sends alarm.</text>
          </description>
          <leaf name="name">
            <type name="string">
              <length value="1..32"/>
            </type>
            <must condition="count(/snmp/target-hosts/target-host[nms-name = current()]) = 0"/>
            <description>
              <text>Unique name to identify proxy target host entry.</text>
            </description>
          </leaf>
          <leaf name="domain">
            <type name="snmp-domain"/>
            <mandatory value="true"/>
            <description>
              <text>Address domain of target host.</text>
            </description>
          </leaf>
          <leaf name="address">
            <type name="inet:ip-address-no-zone"/>
            <mandatory value="true"/>
            <description>
              <text>Network address of target host.</text>
            </description>
          </leaf>
          <leaf name="timeout">
            <type name="int32">
              <range value="1..1800"/>
            </type>
            <units name="s"/>
            <default value="15"/>
            <description>
              <text>Timeout period of SNMP packets (in seconds) for the target.</text>
            </description>
          </leaf>
          <leaf name="vpn">
            <type name="string">
              <length value="1..31"/>
            </type>
            <must condition="not(../public-net='true') or (../public-net='true' and ../vpn='_public_')"/>
            <description>
              <text>VPN instance name.</text>
            </description>
          </leaf>
          <leaf name="port-num">
            <type name="uint32">
              <range value="1..65535"/>
            </type>
            <mandatory value="true"/>
            <description>
              <text>UDP port number used by network management to receive forwarded packets.</text>
            </description>
          </leaf>
          <choice name="params">
            <mandatory value="true"/>
            <description>
              <text>This choice is augmented with case nodes containing configuration parameters specific to the security model.</text>
            </description>
            <case name="v1">
              <description>
                <text>Security model SNMPv1.</text>
              </description>
              <leaf name="security-name-v1">
                <type name="pub-type:password-extend">
                  <length value="1..32 | 48 | 56 | 68..168"/>
                </type>
                <mandatory value="true"/>
                <description>
                  <text>SNMPV1 security name.</text>
                </description>
              </leaf>
            </case>
            <case name="v2c">
              <description>
                <text>Security model v2c.</text>
              </description>
              <leaf name="security-name-v2c">
                <type name="pub-type:password-extend">
                  <length value="1..32 | 48 | 56 | 68..168"/>
                </type>
                <mandatory value="true"/>
                <description>
                  <text>SNMPV2c security name.</text>
                </description>
              </leaf>
            </case>
            <case name="usm">
              <description>
                <text>Security model SNMPv3.</text>
              </description>
              <leaf name="usm-name">
                <type name="string">
                  <length value="1..32"/>
                </type>
                <mandatory value="true"/>
                <description>
                  <text>Usm name.</text>
                </description>
              </leaf>
              <leaf name="security-level">
                <type name="snmp:snmp-security-level"/>
                <mandatory value="true"/>
                <description>
                  <text>Security level indicating whether to use authentication and encryption.</text>
                </description>
              </leaf>
            </case>
          </choice>
          <leaf name="interface-name">
            <type name="pub-type:if-name"/>
            <description>
              <text>Name of the interface to send the forwarded packets.</text>
            </description>
          </leaf>
          <leaf name="public-net">
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable public net-manager for target host.</text>
            </description>
          </leaf>
        </list>
      </container>
      <container name="rules">
        <description>
          <text>List of proxy rules.</text>
        </description>
        <list name="rule">
          <key value="name"/>
          <max-elements value="20"/>
          <description>
            <text>Configure proxy rule entry.</text>
          </description>
          <leaf name="name">
            <type name="string">
              <length value="1..32"/>
            </type>
            <description>
              <text>Unique name to identify proxy rule entry.</text>
            </description>
          </leaf>
          <leaf name="type">
            <type name="snmp-proxy-type"/>
            <mandatory value="true"/>
            <description>
              <text>Proxy rule for message type (read | write | trap | inform).</text>
            </description>
          </leaf>
          <leaf name="engine-id">
            <type name="snmp-engine-id"/>
            <mandatory value="true"/>
            <description>
              <text>Remote engine ID for the proxy rule.</text>
            </description>
          </leaf>
          <leaf name="host-name">
            <type name="string">
              <length value="1..32"/>
            </type>
            <mandatory value="true"/>
            <description>
              <text>Proxy target host name to forward the packet, leafref path /snmp:snmp/snmp:proxy/snmp:target-hosts/snmp:target-host/snmp:name.</text>
            </description>
          </leaf>
          <leaf name="security-model">
            <type name="snmp-target-host-version"/>
            <must condition="(../type='inform' and (../security-model='v2c' or ../security-model='v3') ) or ((not(../type='inform') and (../security-model='v1' or ../security-model='v2c' or ../security-model='v3') ))"/>
            <mandatory value="true"/>
            <description>
              <text>Security model.</text>
            </description>
          </leaf>
          <leaf name="security-name">
            <when condition="../security-model != 'v3'"/>
            <type name="pub-type:password-extend">
              <length value="1..32 | 48 | 56 | 68..168"/>
            </type>
            <mandatory value="true"/>
            <description>
              <text>Security name.</text>
            </description>
          </leaf>
          <leaf name="security-v3-name">
            <when condition="../security-model='v3'"/>
            <type name="string">
              <length value="1..32"/>
            </type>
            <mandatory value="true"/>
            <description>
              <text>Security name v3.</text>
            </description>
          </leaf>
          <leaf name="security-level">
            <type name="snmp-security-level"/>
            <must condition="(../security-model!='v3' and ../security-level='noAuthNoPriv') or (../security-model='v3')"/>
            <default value="noAuthNoPriv"/>
            <description>
              <text>Security level indicating whether to use authentication and encryption.</text>
            </description>
          </leaf>
        </list>
      </container>
      <list name="usm-users">
        <key value="engine-id"/>
        <description>
          <text>Configure SNMP remote usm user list.</text>
        </description>
        <leaf name="engine-id">
          <type name="snmp-engine-id"/>
          <description>
            <text>Remote engine ID of the USM user.</text>
          </description>
        </leaf>
        <list name="usm-user">
          <key value="user-name"/>
          <max-elements value="20"/>
          <description>
            <text>Configure SNMPv3 USM user entry.</text>
          </description>
          <leaf name="user-name">
            <type name="string">
              <length value="1..32"/>
            </type>
            <description>
              <text>Unique name to identify the SNMPv3 USM user.</text>
            </description>
          </leaf>
          <leaf name="group-name">
            <type name="string">
              <length value="1..32"/>
            </type>
            <description>
              <text>Name of the group where user belongs to SNMPv3.</text>
            </description>
          </leaf>
          <leaf name="is-auth-localized">
            <type name="boolean"/>
            <default value="false"/>
            <config value="false"/>
            <description>
              <text>The flag of localized authentication-mode.</text>
            </description>
          </leaf>
          <leaf name="auth-protocol">
            <type name="snmp-authenmode"/>
            <must condition="(../auth-key and ../auth-protocol!='noAuth') or ../auth-protocol='noAuth'"/>
            <default value="noAuth"/>
            <description>
              <text>Authentication protocol of SNMPv3 user.</text>
            </description>
          </leaf>
          <leaf name="auth-key">
            <when condition="../auth-protocol != 'noAuth'"/>
            <type name="pub-type:password-extend">
              <length value="1..432"/>
            </type>
            <must condition="string-length(../auth-key)&gt;=../../../../system/password-min-length or ../../../../system/usm-user-cmplx-check='false'"/>
            <mandatory value="true"/>
            <description>
              <text>The authentication password.</text>
            </description>
          </leaf>
          <leaf name="is-priv-localized">
            <type name="boolean"/>
            <default value="false"/>
            <config value="false"/>
            <description>
              <text>The flag of localized privacy-mode.</text>
            </description>
          </leaf>
          <leaf name="priv-protocol">
            <type name="snmp-privacy"/>
            <must condition="(not(../priv-protocol='noPriv') and not(../auth-protocol='noAuth') and ../priv-key) or (../priv-protocol='noPriv')"/>
            <default value="noPriv"/>
            <description>
              <text>Encryption protocol of SNMPv3 user.</text>
            </description>
          </leaf>
          <leaf name="priv-key">
            <when condition="../auth-protocol!='noAuth' and ../priv-protocol!='noPriv'"/>
            <type name="pub-type:password-extend">
              <length value="1..432"/>
            </type>
            <must condition="string-length(../priv-key)&gt;=../../../../system/password-min-length or ../../../../system/usm-user-cmplx-check='false'"/>
            <mandatory value="true"/>
            <description>
              <text>The encryption password of SNMPv3 user.</text>
            </description>
          </leaf>
          <leaf name="acl-number">
            <type name="string">
              <length value="1..32"/>
            </type>
            <description>
              <text>ACL used to limit the host which can use SNMP to access network elements.</text>
            </description>
          </leaf>
          <leaf name="active-status">
            <type name="string">
              <length value="1..9"/>
            </type>
            <config value="false"/>
            <description>
              <text>SNMPv3 user block state.</text>
            </description>
          </leaf>
          <leaf name="left-lock-time">
            <type name="uint32">
              <range value="0..4294967295"/>
            </type>
            <units name="s"/>
            <config value="false"/>
            <description>
              <text>Left lock time of locked user.</text>
            </description>
          </leaf>
        </list>
      </list>
      <container name="source-interfaces">
        <description>
          <text>List of source interfaces.</text>
        </description>
        <list name="source-interface">
          <key value="interface-name"/>
          <max-elements value="20"/>
          <description>
            <text>Configure source interface entry.</text>
          </description>
          <leaf name="interface-name">
            <type name="pub-type:if-name"/>
            <description>
              <text>Specify the source interface name.</text>
            </description>
          </leaf>
        </list>
      </container>
      <container name="ipv6-sources">
        <description>
          <text>List of configure source IPv6 address.</text>
        </description>
        <list name="ipv6-source">
          <key value="src-ipv6-address"/>
          <max-elements value="20"/>
          <description>
            <text>Configure source IPv6 address entry.</text>
          </description>
          <leaf name="src-ipv6-address">
            <type name="inet:ipv6-address-no-zone"/>
            <description>
              <text>Source IPv6 address.</text>
            </description>
          </leaf>
          <leaf name="vpn-name">
            <type name="string">
              <length value="1..31"/>
            </type>
            <description>
              <text>Source IPv6 VPN instance name.</text>
            </description>
          </leaf>
        </list>
      </container>
      <container name="all-interface">
        <description>
          <text>Configure bind all interface.</text>
        </description>
        <leaf name="all-ipv4-interface">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable bind all IPv4 interface.</text>
          </description>
        </leaf>
        <leaf name="all-ipv6-interface">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable bind all IPv6 interface.</text>
          </description>
        </leaf>
      </container>
    </container>
  </container>
  <rpc name="activate-users">
    <description>
      <text>Activate the users blocked by authentication failure.</text>
    </description>
    <input>
      <container name="activate-users">
        <description>
          <text>List of users blocked by authentication failure.</text>
        </description>
        <list name="activate-user">
          <key value="name"/>
          <description>
            <text>Activate the user blocked by authentication failure.</text>
          </description>
          <leaf name="name">
            <type name="string">
              <length value="1..32"/>
            </type>
            <description>
              <text>Unique name to identify the USM user.</text>
            </description>
          </leaf>
          <leaf name="engine-id">
            <ext:support-filter value="true"/>
            <type name="string">
              <length value="10..64"/>
              <pattern value="(([0-9a-fA-F])*)"/>
            </type>
            <description>
              <text>Remote engine ID.</text>
            </description>
          </leaf>
        </list>
      </container>
    </input>
  </rpc>
</module>
