  <grouping name="specified-up">
    <description>
      <text>Configure an IPv6 prefix pool to be exclusively used by a vBRAS-UP or UP backup policy profile in CU separation multi-instance scenarios.</text>
    </description>
    <container name="occupied-policy">
      <description>
        <text>Configure a specific policy to specify the IPv6 prefix pool to be occupied.</text>
      </description>
      <choice name="occupied-policy">
        <description>
          <text>Select a specific policy to specify the IPv6 prefix pool to be occupied.</text>
        </description>
        <case name="up-id">
          <description>
            <text>The specified IPv6 prefix pool is exclusive to be used by vBRAS-UP UP ID.</text>
          </description>
          <leaf name="up-id">
            <type name="uint16">
              <range value="1024..2023 | 2048..3047 | 3072..4071 | 4096..5095"/>
            </type>
            <must condition="(../up-id&lt;=(/bras-cu-controller:bras-cu-controller/bras-cu-controller:global-attribute/bras-cu-controller:up-base-id + 999) and ../up-id&gt;=/bras-cu-controller:bras-cu-controller/bras-cu-controller:global-attribute/bras-cu-controller:up-base-id)"/>
            <description>
              <text>Specify the ID of a vBRAS-UP.</text>
            </description>
          </leaf>
        </case>
        <case name="up-backup-profile">
          <description>
            <text>The specified IPv6 prefix pool is exclusive to be used by vBRAS-UP backup policy profile.</text>
          </description>
          <leaf name="up-backup-profile">
            <type name="leafref">
              <path value="/bras-cu-controller:bras-cu-controller/bras-cu-controller:hot-profiles/bras-cu-controller:hot-profile/bras-cu-controller:name"/>
            </type>
            <must condition="/bras-cu-controller:bras-cu-controller/bras-cu-controller:hot-profiles/bras-cu-controller:hot-profile[bras-cu-controller:name=current()]/bras-cu-controller:backup-groups/bras-cu-controller:backup-group/bras-cu-controller:master-interface"/>
            <description>
              <text>Specify the name of a vBRAS-UP backup policy profile.</text>
            </description>
          </leaf>
        </case>
        <case name="hot-standby-up-group">
          <description>
            <text>The specified IPv6 prefix pool is exclusive to be used by vBRAS-UP hot backup groups.</text>
          </description>
          <leaf name="master-up">
            <type name="uint16">
              <range value="1024..2023 | 2048..3047 | 3072..4071 | 4096..5095"/>
            </type>
            <must condition="../master-up!=../slave-up"/>
            <must condition="(../master-up&lt;=(/bras-cu-controller:bras-cu-controller/bras-cu-controller:global-attribute/bras-cu-controller:up-base-id + 999) and ../master-up&gt;=/bras-cu-controller:bras-cu-controller/bras-cu-controller:global-attribute/bras-cu-controller:up-base-id)"/>
            <mandatory value="true"/>
            <description>
              <text>Specify the ID of the vBRAS-UP where the master interface in a vBRAS-UP hot backup group resides.</text>
            </description>
          </leaf>
          <leaf name="slave-up">
            <type name="uint16">
              <range value="1024..2023 | 2048..3047 | 3072..4071 | 4096..5095"/>
            </type>
            <must condition="(../slave-up&lt;=(/bras-cu-controller:bras-cu-controller/bras-cu-controller:global-attribute/bras-cu-controller:up-base-id + 999) and ../slave-up&gt;=/bras-cu-controller:bras-cu-controller/bras-cu-controller:global-attribute/bras-cu-controller:up-base-id)"/>
            <mandatory value="true"/>
            <description>
              <text>Specify the ID of the vBRAS-UP where the slave interface in a vBRAS-UP hot backup group resides.</text>
            </description>
          </leaf>
        </case>
      </choice>
    </container>
  </grouping>
  <grouping name="vpn-instance">
    <description>
      <text>Specify the VPN instance name of a IPv6 prefix pool.</text>
    </description>
    <container name="vpn-instance">
      <description>
        <text>Configure the VPN instance name of a IPv6 prefix pool.</text>
      </description>
      <leaf name="name">
        <type name="leafref">
          <path value="/ni:network-instance/ni:instances/ni:instance/ni:name"/>
        </type>
        <must condition="/ni:network-instance/ni:instances/ni:instance[ni:name=current()]/l3vpn:afs/l3vpn:af[l3vpn:type='ipv6-unicast'] and ../name != '_public_'"/>
        <description>
          <text>Specify the VPN instance name of a IPv6 prefix pool.</text>
        </description>
      </leaf>
    </container>
  </grouping>
  <grouping name="slaac-unshare-only">
    <description>
      <text>Configure the prefix pool to allocate IP addresses by using only the unshared-mode stateless address autoconfiguration (SLAAC) protocol.</text>
    </description>
    <container name="slaac-unshare-only">
      <description>
        <text>Configure the prefix pool to allocate IP addresses by using only the unshared-mode stateless address autoconfiguration (SLAAC) protocol.</text>
      </description>
      <leaf name="enable">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable the prefix pool to allocate IP addresses by using only the unshared-mode stateless address autoconfiguration (SLAAC) protocol.</text>
        </description>
      </leaf>
    </container>
  </grouping>
  <grouping name="pd-unshare-only">
    <description>
      <text>Configure the prefix pool to allocate IP addresses by using only the unshared-mode stateless address autoconfiguration (DHCPv6 IA_PD) protocol.</text>
    </description>
    <container name="pd-unshare-only">
      <description>
        <text>Configure the prefix pool to allocate IP addresses by using only the unshared-mode stateless address autoconfiguration (DHCPv6 IA_PD) protocol.</text>
      </description>
      <leaf name="enable">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable the prefix pool to allocate IP addresses by using only the unshared-mode stateless address autoconfiguration (DHCPv6 IA_PD) protocol.</text>
        </description>
      </leaf>
    </container>
  </grouping>
  <grouping name="dhcpv6-unshare-only">
    <description>
      <text>Configure an IPv6 prefix pool to assign only IPv6 addresses not prefixes to users.</text>
    </description>
    <leaf name="enable">
      <type name="empty"/>
      <description>
        <text>Configure an IPv6 prefix pool to assign only IPv6 addresses not prefixes to users.</text>
      </description>
    </leaf>
  </grouping>
  <grouping name="lifetime-config">
    <description>
      <text>Configure the preferred lifetime anf valid lifetime of an IPv6 prefix.</text>
    </description>
    <container name="lifetime">
      <must condition="not(./preferred-infinite) or ./valid-infinite"/>
      <description>
        <text>Configure the preferred lifetime anf valid lifetime of an IPv6 prefix.
The valid lifetime should not be smaller than the preferred lifetime.</text>
      </description>
      <choice name="preferred-lifetime">
        <default value="preferred-time"/>
        <description>
          <text>Configure the preferred lifetime.</text>
        </description>
        <case name="preferred-infinite">
          <description>
            <text>Specify the preferred lifetime of IPv6 prefix is infinite.</text>
          </description>
          <leaf name="preferred-infinite">
            <type name="empty"/>
            <description>
              <text>Specify the preferred lifetime of IPv6 prefix is infinite.</text>
            </description>
          </leaf>
        </case>
        <case name="preferred-time">
          <description>
            <text>Configure the preferred lifetime that is not infinite.</text>
          </description>
          <leaf name="preferred-day">
            <type name="uint16">
              <range value="0..999"/>
            </type>
            <units name="d"/>
            <must condition="../preferred-day != 0 or ../preferred-hour != 0 or ../preferred-minute != 0"/>
            <default value="2"/>
            <description>
              <text>Prefer day.</text>
            </description>
          </leaf>
          <leaf name="preferred-hour">
            <type name="uint8">
              <range value="0..23"/>
            </type>
            <units name="h"/>
            <default value="0"/>
            <description>
              <text>Prefer hour.</text>
            </description>
          </leaf>
          <leaf name="preferred-minute">
            <type name="uint8">
              <range value="0..59"/>
            </type>
            <units name="min"/>
            <default value="0"/>
            <description>
              <text>Prefer minute.</text>
            </description>
          </leaf>
        </case>
      </choice>
      <choice name="valid-lifetime">
        <default value="valid-time"/>
        <description>
          <text>Configure the valid lifetime.</text>
        </description>
        <case name="valid-infinite">
          <description>
            <text>Configure the valid lifetime of IPv6 prefix is infinite.</text>
          </description>
          <leaf name="valid-infinite">
            <type name="empty"/>
            <description>
              <text>Specify the valid lifetime of IPv6 prefix is infinite.</text>
            </description>
          </leaf>
        </case>
        <case name="valid-time">
          <description>
            <text>Configure the valid lifetime that is not infinite.</text>
          </description>
          <leaf name="valid-day">
            <type name="uint16">
              <range value="0..999"/>
            </type>
            <units name="d"/>
            <must condition="../valid-day != 0 or ../valid-hour != 0 or ../valid-minute != 0"/>
            <default value="3"/>
            <description>
              <text>Valid day.</text>
            </description>
          </leaf>
          <leaf name="valid-hour">
            <type name="uint8">
              <range value="0..23"/>
            </type>
            <units name="h"/>
            <default value="0"/>
            <description>
              <text>Valid hour.</text>
            </description>
          </leaf>
          <leaf name="valid-minute">
            <type name="uint8">
              <range value="0..59"/>
            </type>
            <units name="min"/>
            <default value="0"/>
            <description>
              <text>Valid minute.</text>
            </description>
          </leaf>
        </case>
      </choice>
    </container>
  </grouping>
  <grouping name="rapid-commit-config">
    <description>
      <text>Configure the DHCPv6 server to support a rapid two-message exchange (solicit, reply).</text>
    </description>
    <container name="rapid-commit">
      <description>
        <text>Configure the DHCPv6 server to support a rapid two-message exchange (solicit, reply).</text>
      </description>
      <leaf name="enable">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable rapid commit.</text>
        </description>
      </leaf>
    </container>
  </grouping>
  <grouping name="remote-not-support">
    <description>
      <text>Configure the common information for IPv6 pool which is not remote type.</text>
    </description>
    <container name="dns-server">
      <must condition="not(./ipv6-address1) or not(./ipv6-address2) or ./ipv6-address1!=./ipv6-address2"/>
      <description>
        <text>Configure an IPv6 address for binding a DNS server to an IPv6 address pool.</text>
      </description>
      <leaf name="ipv6-address1">
        <type name="inet:ipv6-address-no-zone"/>
        <description>
          <text>Specify the first IPv6 address of a DNS server.</text>
        </description>
      </leaf>
      <leaf name="ipv6-address2">
        <type name="inet:ipv6-address-no-zone"/>
        <description>
          <text>Specify the second IPv6 address of a DNS server.</text>
        </description>
      </leaf>
    </container>
    <container name="dns-search-list">
      <description>
        <text>Configure a domain name suffix.</text>
      </description>
      <leaf name="name">
        <type name="string">
          <length value="1..32"/>
          <pattern value="[a-zA-Z0-9._-]{1,32}"/>
        </type>
        <must condition="../name != '-' and ../name != '--'"/>
        <description>
          <text>Specify the domain name suffix.</text>
        </description>
      </leaf>
    </container>
    <container name="renew-rebind-time">
      <must condition="./rebind-time-percent &gt;= ./renew-time-percent + 10"/>
      <description>
        <text>Configure the renewal time of an IPv6 address pool. renew-time-percent is the percentage of the renewal time to the preferred lifetime.</text>
      </description>
      <leaf name="renew-time-percent">
        <type name="uint8">
          <range value="10..99"/>
        </type>
        <units name="%"/>
        <default value="50"/>
        <description>
          <text>Set the renewal time and rebinding time of an IPv6 address pool.</text>
        </description>
      </leaf>
      <leaf name="rebind-time-percent">
        <type name="uint8">
          <range value="10..99"/>
        </type>
        <units name="%"/>
        <default value="80"/>
        <description>
          <text>Specify the rebinding time of an IPv6 address pool. rebind-time-percent is the percentage of the rebinding time to the preferred lifetime.</text>
        </description>
      </leaf>
    </container>
  </grouping>
  <grouping name="lock">
    <description>
      <text>Lock an IPv6 prefix pool.</text>
    </description>
    <container name="lock">
      <description>
        <text>Configure a locked IPv6 prefix pool.</text>
      </description>
      <leaf name="enable">
        <type name="empty"/>
        <description>
          <text>Lock an IPv6 prefix pool.</text>
        </description>
      </leaf>
    </container>
  </grouping>
  <grouping name="reserved-ipv6">
    <description>
      <text>Configure a reservation type for the prefix pool.</text>
    </description>
    <container name="reserved-ipv6">
      <description>
        <text>Configure a reservation type for the prefix pool.</text>
      </description>
      <leaf name="reserved-type">
        <type name="reserved-type"/>
        <description>
          <text>Reservation type for the IPv6 prefix or address, after the reserved type of an IPv6 prefix or address is set to mac, one-to-many mapping between one MAC address and multiple sessions cannot be enabled.</text>
        </description>
      </leaf>
      <leaf name="lease">
        <when condition="../reserved-type"/>
        <type name="empty"/>
        <description>
          <text>Reserve the IPv6 prefixes or address during a lease.</text>
        </description>
      </leaf>
    </container>
  </grouping>
  <grouping name="frame-ipv6-lease">
    <description>
      <text>Configure lease for an IPv6 prefix pool.</text>
    </description>
    <container name="frame-ipv6-lease">
      <description>
        <text>Configure manage the leases of RADIUS-delivered IPv6 addresses that are in the supported address pools.</text>
      </description>
      <leaf name="enable">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable manage the leases of RADIUS-delivered IPv6 addresses that are in the supported address pools.</text>
        </description>
      </leaf>
    </container>
  </grouping>
  <grouping name="exclude-ipv6-address">
    <description>
      <text>List of specified IPv6 addresses to prevent it from being allocated to users. The start IPv6 address must be smaller than or equal to the end IPv6 address.</text>
    </description>
    <list name="exclude-ipv6-address">
      <key value="start-ipv6-address end-ipv6-address"/>
      <description>
        <text>Configure a specified IPv6 address to prevent it from being allocated to users. The start IPv6 address must be smaller than or equal to the end IPv6 address.</text>
      </description>
      <leaf name="start-ipv6-address">
        <type name="inet:ipv6-address-no-zone"/>
        <description>
          <text>Specify the start IPv6 address.</text>
        </description>
      </leaf>
      <leaf name="end-ipv6-address">
        <type name="inet:ipv6-address-no-zone"/>
        <description>
          <text>Specify the end IPv6 address.</text>
        </description>
      </leaf>
    </list>
  </grouping>
  <grouping name="ipv6-constant-index">
    <description>
      <text>Configure a constant index for an IPv6 address or prefix pool.</text>
    </description>
    <container name="constant-index">
      <config value="false"/>
      <description>
        <text>Statistics of a constant index for an IPv6 address or prefix pool.</text>
      </description>
      <leaf name="value">
        <type name="uint16">
          <range value="0..16383"/>
        </type>
        <description>
          <text>Specify an index for an IPv6 address or prefix pool.</text>
        </description>
      </leaf>
    </container>
  </grouping>
  <grouping name="ipv6-pool-comm">
    <description>
      <text>Set the preference value of an IPv6 address pool.</text>
    </description>
    <container name="preference">
      <description>
        <text>Configure the preference value of an IPv6 address pool.</text>
      </description>
      <leaf name="value">
        <type name="uint8">
          <range value="0..255"/>
        </type>
        <default value="255"/>
        <description>
          <text>Specify the preference value of an IPv6 address pool.</text>
        </description>
      </leaf>
    </container>
    <container name="lock">
      <presence value="Create lock pool"/>
      <description>
        <text>Enable/disable an IPv6 address pool.</text>
      </description>
      <leaf name="drain">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable an IPv6 address pool. If it is disable, the address pool cannot be used to assign IPv6 addresses to new users and DHCP renew or rebind.</text>
        </description>
      </leaf>
    </container>
    <container name="wait-request-time">
      <description>
        <text>Configure the timeout period for a router to wait for a request message from a client in response to an advertise message sent to the client.</text>
      </description>
      <leaf name="value">
        <type name="uint8">
          <range value="5..60"/>
        </type>
        <units name="s"/>
        <description>
          <text>Specify the timeout period for a router to wait for a request message from a client.</text>
        </description>
      </leaf>
    </container>
    <container name="warning">
      <description>
        <text>Configure warning of IPv6 address usage.</text>
      </description>
      <leaf name="threshold-value">
        <type name="uint8">
          <range value="0..100"/>
        </type>
        <default value="80"/>
        <description>
          <text>The alarm threshold for the address usage of an IPv6 address pool is a percentage.</text>
        </description>
      </leaf>
      <leaf name="exhaust">
        <type name="empty"/>
        <description>
          <text>Enable the address exhaustion alarm function for an IPv6 address pool.</text>
        </description>
      </leaf>
    </container>
    <container name="unr-tag">
      <description>
        <text>Configure a route tag for an IPv6 address pool.</text>
      </description>
      <leaf name="value">
        <type name="uint32">
          <range value="0..4294967295"/>
        </type>
        <description>
          <text>Specify the route tag for the IPv6 address pool.</text>
        </description>
      </leaf>
    </container>
  </grouping>
  <grouping name="weight">
    <description>
      <text>Configure a weight for an IPv6 address pool.</text>
    </description>
    <leaf name="value">
      <type name="uint8">
        <range value="0..100"/>
      </type>
      <default value="5"/>
      <description>
        <text>Specify a weight for an IPv6 address pool.</text>
      </description>
    </leaf>
  </grouping>
  <grouping name="aftr-config">
