<?xml version="1.0" encoding="UTF-8"?>
<module name="huawei-warranty-notification"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:warranty-notification="urn:huawei:yang:huawei-warranty-notification"
        xmlns:ext="urn:huawei:yang:huawei-extension">
  <namespace uri="urn:huawei:yang:huawei-warranty-notification"/>
  <prefix value="warranty-notification"/>
  <import module="huawei-extension">
    <prefix value="ext"/>
  </import>
  <organization>
    <text>Huawei Technologies Co., Ltd.</text>
  </organization>
  <contact>
    <text>Huawei Industrial Base
Bantian, Longgang
Shenzhen 518129
People's Republic of China
Website: http://www.huawei.com
Email: support@huawei.com</text>
  </contact>
  <description>
    <text>Notification of warranty.</text>
  </description>
  <revision date="2020-05-14">
    <description>
      <text>Initial revision.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <ext:task-name value="warranty"/>
  <notification name="warranty-to-be-expired">
    <description>
      <text>The electronic warranty of the device is expiring.</text>
    </description>
    <leaf name="type">
      <type name="string">
        <length value="1..10"/>
      </type>
      <description>
        <text>Specify the unit of the electronic warranty.</text>
      </description>
    </leaf>
    <leaf name="slot">
      <type name="string">
        <length value="1..32"/>
      </type>
      <description>
        <text>Specify the slot path.</text>
      </description>
    </leaf>
    <leaf name="serial-number">
      <type name="string">
        <length value="1..100"/>
      </type>
      <description>
        <text>Serial number of the electronic warranty.</text>
      </description>
    </leaf>
    <leaf name="remained-days">
      <type name="uint32"/>
      <description>
        <text>Remained days of the electronic warranty.</text>
      </description>
    </leaf>
  </notification>
  <notification name="warranty-to-be-expired-clear">
    <description>
      <text>The ararm indicating that the electronic warranty of the device is expiring was cleared.</text>
    </description>
    <leaf name="type">
      <type name="string">
        <length value="1..10"/>
      </type>
      <description>
        <text>Specify the unit of the electronic warranty.</text>
      </description>
    </leaf>
    <leaf name="slot">
      <type name="string">
        <length value="1..32"/>
      </type>
      <description>
        <text>Specify the slot path.</text>
      </description>
    </leaf>
    <leaf name="serial-number">
      <type name="string">
        <length value="1..100"/>
      </type>
      <description>
        <text>Serial number of the electronic warranty.</text>
      </description>
    </leaf>
  </notification>
  <notification name="warranty-expired">
    <description>
      <text>The electronic warranty of the device is expired.</text>
    </description>
    <leaf name="type">
      <type name="string">
        <length value="1..10"/>
      </type>
      <description>
        <text>Specify the unit of the electronic warranty.</text>
      </description>
    </leaf>
    <leaf name="slot">
      <type name="string">
        <length value="1..32"/>
      </type>
      <description>
        <text>Specify the slot path.</text>
      </description>
    </leaf>
    <leaf name="serial-number">
      <type name="string">
        <length value="1..100"/>
      </type>
      <description>
        <text>Serial number of the electronic warranty.</text>
      </description>
    </leaf>
  </notification>
  <notification name="warranty-expired-clear">
    <description>
      <text>The alarm indicating that the electronic warranty of the device is expired cleared.</text>
    </description>
    <leaf name="type">
      <type name="string">
        <length value="1..10"/>
      </type>
      <description>
        <text>Specify the unit of the electronic warranty.</text>
      </description>
    </leaf>
    <leaf name="slot">
      <type name="string">
        <length value="1..32"/>
      </type>
      <description>
        <text>Specify the slot path.</text>
      </description>
    </leaf>
    <leaf name="serial-number">
      <type name="string">
        <length value="1..100"/>
      </type>
      <description>
        <text>Serial number of the electronic warranty.</text>
      </description>
    </leaf>
  </notification>
  <notification name="warranty-missing-session">
    <description>
      <text>The electronic warranty's session of the device is missing.</text>
    </description>
    <leaf name="type">
      <type name="string">
        <length value="1..10"/>
      </type>
      <description>
        <text>Specify the unit of the electronic warranty.</text>
      </description>
    </leaf>
    <leaf name="slot">
      <type name="string">
        <length value="1..32"/>
      </type>
      <description>
        <text>Specify the slot path.</text>
      </description>
    </leaf>
    <leaf name="serial-number">
      <type name="string">
        <length value="1..100"/>
      </type>
      <description>
        <text>Serial number of the electronic warranty.</text>
      </description>
    </leaf>
  </notification>
  <notification name="warranty-missing-session-clear">
    <description>
      <text>The alarm indicationg that the electronic warranty's session of the device is missing was cleared.</text>
    </description>
    <leaf name="type">
      <type name="string">
        <length value="1..10"/>
      </type>
      <description>
        <text>Specify the unit of the electronic warranty.</text>
      </description>
    </leaf>
    <leaf name="slot">
      <type name="string">
        <length value="1..32"/>
      </type>
      <description>
        <text>Specify the slot path.</text>
      </description>
    </leaf>
    <leaf name="serial-number">
      <type name="string">
        <length value="1..100"/>
      </type>
      <description>
        <text>Serial number of the electronic warranty.</text>
      </description>
    </leaf>
  </notification>
</module>
