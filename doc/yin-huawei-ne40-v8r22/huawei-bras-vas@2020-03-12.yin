  <augment target-node="/aaa:aaa/aaa:domains/aaa:domain/bras-basic-access:bas">
    <description>
      <text>Adds vas configuration to aaa domain model.</text>
    </description>
    <container name="vas">
      <must condition="not(./service-policy-group/name and ./vas-policy/name)"/>
      <description>
        <text>Configure vas in aaa domain model.</text>
      </description>
      <container name="service-rate-limit-mode">
        <description>
          <text>Configure a rate limit mode for EDSG service traffic.</text>
        </description>
        <leaf name="inbound">
          <type name="bras-vas-rate-limit-mode"/>
          <default value="car"/>
          <description>
            <text>Configuring the upstream rate limit mode.</text>
          </description>
        </leaf>
        <leaf name="outbound">
          <type name="bras-vas-rate-limit-mode"/>
          <default value="car"/>
          <description>
            <text>Configuring the downstream rate limit mode.</text>
          </description>
        </leaf>
      </container>
      <container name="edsg-traffic-mode">
        <description>
          <text>Configure EDSG service rate limiting and traffic statistics policies. By default, the rate of service traffic is separately limited and is not affected by user bandwidth. Only user traffic is counted as user traffic.</text>
        </description>
        <leaf name="rate-limit-and-traffic-static">
          <type name="enumeration">
            <enum name="default-mode">
              <value value="0"/>
              <description>
                <text>By default, the rate of service traffic is separately limited and is not affected by user bandwidth. Only user traffic is counted as user traffic.</text>
              </description>
            </enum>
            <enum name="separate">
              <value value="1"/>
              <description>
                <text>Performs separate rate limiting on service traffic.</text>
              </description>
            </enum>
            <enum name="together">
              <value value="2"/>
              <description>
                <text>Performs rate limiting on service traffic and then user traffic.</text>
              </description>
            </enum>
          </type>
          <default value="default-mode"/>
          <description>
            <text>Configures EDSG service rate limiting mode.</text>
          </description>
        </leaf>
      </container>
      <container name="billing-server-type">
        <description>
          <text>Configure the accounting server type. By default, Router considers all accounting servers supporting the dynamic policy.</text>
        </description>
        <leaf name="type">
          <type name="enumeration">
            <enum name="dynamic">
              <value value="1"/>
              <description>
                <text>Specifies the dynamic policy switch mode.</text>
              </description>
            </enum>
            <enum name="normal">
              <value value="2"/>
              <description>
                <text>Specifies the normal policy switch mode.</text>
              </description>
            </enum>
          </type>
          <default value="dynamic"/>
          <description>
            <text>Configures the accounting server type.</text>
          </description>
        </leaf>
      </container>
      <container name="daa-coexist-qos-profile">
        <description>
          <text>Configure the QoS profile attribute to take effect when DAA services are dynamically modified.</text>
        </description>
        <leaf name="enable">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable the QoS profile attribute to take effect when DAA services are dynamically modified.</text>
          </description>
        </leaf>
      </container>
      <container name="daa-match-direction">
        <description>
          <text>Configure the upstream or downstream traffic of users that go online through an AAA domain against DAA services.</text>
        </description>
        <leaf name="inbound">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable the DAA service matching function of upstream traffic of users that go online through domain.</text>
          </description>
        </leaf>
        <leaf name="outbound">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable the DAA service matching function of downstream traffic of users that go online through domain.</text>
          </description>
        </leaf>
      </container>
      <container name="family-schedule">
        <description>
          <text>Configure EDSG services to support HQoS scheduling for home users.</text>
        </description>
        <leaf name="inbound">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable upstream traffic for home users.</text>
          </description>
        </leaf>
        <leaf name="outbound">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable downstream traffic for home users.</text>
          </description>
        </leaf>
      </container>
      <container name="traffic-match-user-group">
        <description>
          <text>Configure EDSG service traffic to match a user group.</text>
        </description>
        <leaf name="direction">
          <type name="enumeration">
            <enum name="both">
              <value value="0"/>
              <description>
                <text>Indicates both upstream and downstream EDSG service traffic.</text>
              </description>
            </enum>
            <enum name="inbound">
              <value value="1"/>
              <description>
                <text>Indicates upstream EDSG service traffic.</text>
              </description>
            </enum>
            <enum name="outbound">
              <value value="2"/>
              <description>
                <text>Indicates downstream EDSG service traffic.</text>
              </description>
            </enum>
          </type>
          <description>
            <text>Indicates upstream or downstream EDSG service traffic.</text>
          </description>
        </leaf>
      </container>
      <container name="account-type">
        <must condition="../account-type/radius-server-group-name or not(../account-type/type='radius')"/>
        <description>
          <text>Configure a value-added service accounting mode. default accounting, none accounting, and RADIUS accounting can be adopted for the value-added services. If no accounting mode is specified, the default accounting mode is adopted.</text>
        </description>
        <leaf name="type">
          <type name="enumeration">
            <enum name="default">
              <value value="1"/>
              <description>
                <text>Default accounting.</text>
              </description>
            </enum>
            <enum name="none">
              <value value="2"/>
              <description>
                <text>None accounting.</text>
              </description>
            </enum>
            <enum name="radius">
              <value value="3"/>
              <description>
                <text>RADIUS accounting.</text>
              </description>
            </enum>
          </type>
          <default value="default"/>
          <description>
            <text>Configures a value-added service accounting mode. default accounting, none accounting, and RADIUS accounting can be adopted for the value-added services.</text>
          </description>
        </leaf>
        <leaf name="radius-server-group-name">
          <when condition="../type='radius'"/>
          <type name="leafref">
            <path value="/radius:radius/radius:radius-server-groups/radius:radius-server-group/radius:name"/>
          </type>
          <description>
            <text>Specifies the name of the RADIUS server group. The server group must be a configured RADIUS server group.</text>
          </description>
        </leaf>
      </container>
      <container name="vas-policy">
        <description>
          <text>Configure the value-added service policy used by the domain.</text>
        </description>
        <leaf name="name">
          <type name="leafref">
            <path value="/bras-vas:bras-vas/bras-vas:vas-policys/bras-vas:vas-policy/bras-vas:name"/>
          </type>
          <must condition="/bras-vas:bras-vas/bras-vas:vas-policys/bras-vas:vas-policy[bras-vas:name=current()]/bras-vas:type='daa'"/>
          <description>
            <text>Specifies the name of the service policy used by the domain. The policy must be a configured service policy.</text>
          </description>
        </leaf>
      </container>
      <container name="service-policy">
        <description>
          <text>Configure a service policy to a domain.</text>
        </description>
        <leaf name="name">
          <type name="leafref">
            <path value="/bras-vas:bras-vas/bras-vas:service-policys/bras-vas:service-policy/bras-vas:name"/>
          </type>
          <must condition="/bras-vas:bras-vas/bras-vas:service-policys/bras-vas:service-policy[name=current()]/mode='portal'"/>
          <description>
            <text>Specifies a service policy name.</text>
          </description>
        </leaf>
      </container>
      <container name="edsg-accounting-copy-server">
        <description>
          <text>Configure EDSG service accounting copy and configures a RADIUS copy server group.</text>
        </description>
        <leaf name="radius-server-group-name">
          <type name="leafref">
            <path value="/radius:radius/radius:radius-server-groups/radius:radius-server-group/radius:name"/>
          </type>
          <description>
            <text>Specifies the name of a RADIUS copy server group.</text>
          </description>
        </leaf>
      </container>
      <container name="service-policy-group">
        <description>
          <text>Configure the service policy group used by the domain.</text>
        </description>
        <leaf name="name">
          <type name="leafref">
            <path value="/bras-vas:bras-vas/bras-vas:service-policy-groups/bras-vas:service-policy-group/bras-vas:name"/>
          </type>
          <description>
            <text>Specifies the name of the service policy group used by the domain. The policy group must be a configured service policy group.</text>
          </description>
        </leaf>
      </container>
      <container name="daa-accounting-merge">
        <description>
          <text>Configure daa accounting packet merging for value added services.</text>
        </description>
        <leaf name="start-packet">
