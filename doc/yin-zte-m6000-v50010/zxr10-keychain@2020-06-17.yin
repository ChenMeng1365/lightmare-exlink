  <container name="configuration">
    <description>
      <text>configuration</text>
    </description>
    <container name="keychain">
      <description>
        <text>Keychain config</text>
      </description>
      <container name="keychain-lists">
        <description>
          <text>Set keychain</text>
        </description>
        <list name="keychain-list">
          <description>
            <text>Set keychain</text>
          </description>
          <key value="keychain-name"/>
          <leaf name="keychain-name">
            <description>
              <text>keychain name</text>
            </description>
            <type name="string">
              <length value="1..31"/>
            </type>
          </leaf>
          <leaf name="default-key">
            <description>
              <text>Configure default key</text>
            </description>
            <type name="uint64">
              <range value="0..281474976710655">
                <description>
                  <text>Key ID</text>
                </description>
              </range>
            </type>
          </leaf>
          <container name="tcp-algo-id">
            <description>
              <text>Configure TCP algorithm ID</text>
            </description>
            <presence value="tcp-algorithm-id"/>
            <leaf name="hmac-md5">
              <description>
                <text>Configure HMAC-MD5 ID</text>
              </description>
              <type name="uint8">
                <range value="1..63"/>
              </type>
              <default value="5"/>
            </leaf>
            <leaf name="hmac-sha1-12">
              <description>
                <text>Configure HMAC-SHA1-12 ID</text>
              </description>
              <type name="uint8">
                <range value="1..63"/>
              </type>
              <default value="2"/>
            </leaf>
            <leaf name="hmac-sha1-20">
              <description>
                <text>Configure HMAC-SHA1-20 ID</text>
              </description>
              <type name="uint8">
                <range value="1..63"/>
              </type>
              <default value="6"/>
            </leaf>
            <leaf name="hmac-sha256">
              <description>
                <text>Configure HMAC-SHA256 ID</text>
              </description>
              <type name="uint8">
                <range value="1..63"/>
              </type>
              <default value="7"/>
            </leaf>
            <leaf name="hmac-sm3">
              <description>
                <text>Configure HMAC-SM3 ID</text>
              </description>
              <type name="uint8">
                <range value="1..63"/>
              </type>
              <default value="9"/>
            </leaf>
            <leaf name="md5">
              <description>
                <text>Configure MD5 ID</text>
              </description>
              <type name="uint8">
                <range value="1..63"/>
              </type>
              <default value="3"/>
            </leaf>
            <leaf name="sha1">
              <description>
                <text>Configure SHA1 ID</text>
              </description>
              <type name="uint8">
                <range value="1..63"/>
              </type>
              <default value="4"/>
            </leaf>
            <leaf name="sha256">
              <description>
                <text>Configure SHA256 ID</text>
              </description>
              <type name="uint8">
                <range value="1..63"/>
              </type>
              <default value="8"/>
            </leaf>
            <leaf name="sm3">
              <description>
                <text>Configure SM3 ID</text>
              </description>
              <type name="uint8">
                <range value="1..63"/>
              </type>
              <default value="10"/>
            </leaf>
          </container>
          <container name="lifetime-mode">
            <description>
              <text>Configure lifetime mode</text>
            </description>
            <presence value="lifetime-mode"/>
            <choice name="mode-type">
              <description>
                <text>Configure lifetime mode</text>
              </description>
              <default value="absolute"/>
              <case name="periodic">
                <description>
                  <text>Periodic mode</text>
                </description>
                <leaf name="periodic">
                  <description>
                    <text>Periodic mode</text>
                  </description>
                  <type name="enumeration">
                    <enum name="periodic">
                      <value value="2"/>
                    </enum>
                  </type>
                  <mandatory value="true"/>
                </leaf>
                <leaf name="value">
                  <description>
                    <text>Periodic type value</text>
                  </description>
                  <type name="periodicType"/>
                  <mandatory value="true"/>
                </leaf>
              </case>
              <case name="absolute">
                <description>
                  <text>Absolute mode</text>
                </description>
                <leaf name="absolute">
                  <description>
                    <text>Absolute mode</text>
                  </description>
                  <type name="enumeration">
                    <enum name="absolute">
                      <value value="1"/>
                    </enum>
                  </type>
                  <default value="absolute"/>
                </leaf>
              </case>
            </choice>
          </container>
          <leaf name="tcp-kind">
            <description>
              <text>TCP kind option</text>
            </description>
            <type name="uint32">
              <range value="29..255">
                <description>
                  <text>TCP kind value,default value is 254</text>
                </description>
              </range>
            </type>
            <default value="254"/>
          </leaf>
          <container name="rece-tolerance">
            <description>
              <text>Configure receive tolerance</text>
            </description>
            <presence value="receive-tolerance"/>
            <choice name="receive-type">
              <description>
                <text>Configure receive type</text>
              </description>
              <case name="time">
                <description>
                  <text>Receive tolerance value in seconds</text>
                </description>
                <leaf name="time">
                  <description>
                    <text>Receive tolerance value in seconds</text>
                  </description>
                  <type name="enumeration">
                    <enum name="time">
                      <value value="1"/>
                    </enum>
                  </type>
                  <mandatory value="true"/>
                </leaf>
                <leaf name="value">
                  <description>
                    <text>Receive tolerance value in seconds</text>
                  </description>
                  <type name="uint32">
                    <range value="1..864000"/>
                  </type>
                  <mandatory value="true"/>
                </leaf>
              </case>
              <case name="infinite">
                <description>
                  <text>Receive tolerance infinite</text>
                </description>
                <leaf name="infinite">
                  <description>
                    <text>Receive tolerance infinite</text>
                  </description>
                  <type name="enumeration">
                    <enum name="infinite">
                      <value value="2"/>
                    </enum>
                  </type>
                  <mandatory value="true"/>
                </leaf>
              </case>
            </choice>
          </container>
          <container name="keys">
            <description>
              <text>Set key of keychain</text>
            </description>
            <presence value="keys"/>
            <list name="key">
              <description>
                <text>Set key of keychain</text>
              </description>
              <key value="key-id"/>
              <leaf name="key-id">
                <description>
                  <text>Key ID</text>
                </description>
                <type name="uint64">
                  <range value="0..281474976710655"/>
                </type>
              </leaf>
              <leaf name="cryptographic-algorithm">
                <description>
                  <text>Configure cryptographic algorithm</text>
                </description>
                <type name="algorithmType"/>
                <default value="hmac-md5"/>
              </leaf>
              <container name="send-lifetime">
                <description>
                  <text>Configure send lifetime</text>
                </description>
                <presence value="send-lifetime"/>
                <container name="weekly">
                  <description>
                    <text>Configure lifetime weekly</text>
                  </description>
                  <presence value="weekly"/>
                  <leaf name="start-day">
                    <description>
                      <text>Set start day value</text>
                    </description>
                    <type name="weekType"/>
                    <mandatory value="true"/>
                  </leaf>
                  <leaf name="end-day">
                    <description>
                      <text>Set end day value</text>
                    </description>
                    <type name="weekType"/>
                    <mandatory value="true"/>
                  </leaf>
                </container>
                <container name="daily">
                  <description>
                    <text>Configure lifetime daily</text>
                  </description>
                  <presence value="daily"/>
                  <leaf name="start-time">
                    <description>
                      <text>Start time</text>
                    </description>
                    <type name="ncd:time"/>
                    <mandatory value="true"/>
                  </leaf>
                  <leaf name="end-time">
                    <description>
                      <text>End time</text>
                    </description>
                    <type name="ncd:time"/>
                    <mandatory value="true"/>
                  </leaf>
                </container>
                <container name="yearly">
                  <description>
                    <text>Configure lifetime yearly</text>
                  </description>
                  <presence value="yearly"/>
                  <leaf name="start-month">
                    <description>
                      <text>Set start month value</text>
                    </description>
                    <type name="uint16">
                      <range value="1..12">
                        <description>
                          <text>Set start month value</text>
                        </description>
                      </range>
                    </type>
                    <mandatory value="true"/>
                  </leaf>
                  <leaf name="end-month">
                    <description>
                      <text>Set end month value</text>
                    </description>
                    <type name="uint16">
                      <range value="1..12">
                        <description>
                          <text>Set end month value</text>
                        </description>
                      </range>
                    </type>
                    <mandatory value="true"/>
                  </leaf>
                </container>
                <container name="monthly">
                  <description>
                    <text>Configure lifetime monthly</text>
                  </description>
                  <presence value="monthly"/>
                  <leaf name="start-date">
                    <description>
                      <text>Set start date</text>
                    </description>
                    <type name="uint16">
                      <range value="1..31">
                        <description>
                          <text>Set start date value</text>
                        </description>
                      </range>
                    </type>
                    <mandatory value="true"/>
                  </leaf>
                  <leaf name="end-date">
                    <description>
                      <text>Set end date</text>
                    </description>
                    <type name="uint16">
                      <range value="1..31">
                        <description>
                          <text>Set start date value</text>
                        </description>
                      </range>
                    </type>
                    <mandatory value="true"/>
                  </leaf>
                </container>
                <container name="absolute">
                  <description>
                    <text>Configure send lifetime absolute</text>
                  </description>
                  <presence value="absolute"/>
                  <leaf name="start-time">
                    <description>
                      <text>Start time</text>
                    </description>
                    <type name="ncd:time"/>
                    <mandatory value="true"/>
                  </leaf>
                  <leaf name="start-date">
                    <description>
                      <text>Start date</text>
                    </description>
                    <type name="ncd:date"/>
                    <mandatory value="true"/>
                  </leaf>
                  <choice name="time-type">
                    <description>
                      <text>Send time mode</text>
                    </description>
                    <mandatory value="true"/>
                    <case name="duration">
                      <description>
                        <text>Set duration time</text>
                      </description>
                      <leaf name="duration">
                        <description>
                          <text>Set duration time</text>
                        </description>
                        <type name="enumeration">
                          <enum name="duration">
                            <value value="1"/>
                            <description>
                              <text>Set duration time</text>
                            </description>
                          </enum>
                        </type>
                        <mandatory value="true"/>
                      </leaf>
                      <leaf name="seconds">
                        <description>
                          <text>Duration time in seconds</text>
                        </description>
                        <type name="uint32">
                          <range value="1..31536000"/>
                        </type>
                        <mandatory value="true"/>
                      </leaf>
                    </case>
                    <case name="infinite">
                      <description>
                        <text>Infinite active</text>
                      </description>
                      <leaf name="infinite">
                        <description>
                          <text>Set infinite time</text>
                        </description>
                        <type name="enumeration">
                          <enum name="infinite">
                            <value value="2"/>
                            <description>
                              <text>Infinite active</text>
                            </description>
                          </enum>
                        </type>
                        <mandatory value="true"/>
                      </leaf>
                    </case>
                    <case name="end">
                      <description>
                        <text>Set end time</text>
                      </description>
                      <leaf name="end">
                        <description>
                          <text>Set end time</text>
                        </description>
                        <type name="enumeration">
                          <enum name="end">
                            <value value="3"/>
                            <description>
                              <text>Set end time</text>
                            </description>
                          </enum>
                        </type>
                        <mandatory value="true"/>
                      </leaf>
                      <leaf name="end-time">
                        <description>
                          <text>End time</text>
                        </description>
                        <type name="ncd:time"/>
                        <mandatory value="true"/>
                      </leaf>
                      <leaf name="end-date">
                        <description>
                          <text>Set end date</text>
                        </description>
                        <type name="ncd:date"/>
                        <mandatory value="true"/>
                      </leaf>
                    </case>
                  </choice>
                </container>
              </container>
              <container name="rece-lifetime">
                <description>
                  <text>Configure receive lifetime</text>
                </description>
                <presence value="receive-lifetime"/>
                <container name="weekly">
                  <description>
                    <text>Configure lifetime weekly</text>
                  </description>
                  <presence value="weekly"/>
                  <leaf name="start-day">
                    <description>
                      <text>Set start day value</text>
                    </description>
                    <type name="weekType"/>
                    <mandatory value="true"/>
                  </leaf>
                  <leaf name="end-day">
                    <description>
                      <text>Set end day value</text>
                    </description>
                    <type name="weekType"/>
                    <mandatory value="true"/>
                  </leaf>
                </container>
                <container name="daily">
                  <description>
                    <text>Configure lifetime daily</text>
                  </description>
                  <presence value="daily"/>
                  <leaf name="start-time">
                    <description>
                      <text>Start time</text>
                    </description>
                    <type name="ncd:time"/>
                    <mandatory value="true"/>
                  </leaf>
                  <leaf name="end-time">
                    <description>
                      <text>End time</text>
                    </description>
                    <type name="ncd:time"/>
                    <mandatory value="true"/>
                  </leaf>
                </container>
                <container name="yearly">
                  <description>
                    <text>Configure lifetime yearly</text>
                  </description>
                  <presence value="yearly"/>
                  <leaf name="start-month">
                    <description>
                      <text>Set start month value</text>
                    </description>
                    <type name="uint16">
                      <range value="1..12">
                        <description>
                          <text>Set start month value</text>
                        </description>
                      </range>
                    </type>
                    <mandatory value="true"/>
                  </leaf>
                  <leaf name="end-month">
                    <description>
                      <text>Set end month value</text>
                    </description>
                    <type name="uint16">
                      <range value="1..12">
                        <description>
                          <text>Set end month value</text>
                        </description>
                      </range>
                    </type>
                    <mandatory value="true"/>
                  </leaf>
                </container>
                <container name="monthly">
                  <description>
                    <text>Configure lifetime monthly</text>
                  </description>
                  <presence value="monthly"/>
                  <leaf name="start-date">
                    <description>
                      <text>Set start date</text>
                    </description>
                    <type name="uint16">
                      <range value="1..31">
                        <description>
                          <text>Set start date value</text>
                        </description>
                      </range>
                    </type>
                    <mandatory value="true"/>
                  </leaf>
                  <leaf name="end-date">
                    <description>
                      <text>Set end date</text>
                    </description>
                    <type name="uint16">
                      <range value="1..31">
                        <description>
                          <text>Set start date value</text>
                        </description>
                      </range>
                    </type>
                    <mandatory value="true"/>
                  </leaf>
                </container>
                <container name="absolute">
                  <description>
                    <text>Configure receive lifetime absolute</text>
                  </description>
                  <presence value="absolute"/>
                  <leaf name="start-time">
                    <description>
                      <text>Start time</text>
                    </description>
                    <type name="ncd:time"/>
                    <mandatory value="true"/>
                  </leaf>
                  <leaf name="start-date">
                    <description>
                      <text>Start date</text>
                    </description>
                    <type name="ncd:date"/>
                    <mandatory value="true"/>
                  </leaf>
                  <choice name="time-type">
                    <description>
                      <text>Receive time mode</text>
                    </description>
                    <mandatory value="true"/>
                    <case name="duration">
                      <description>
                        <text>Set duration time</text>
                      </description>
                      <leaf name="duration">
                        <description>
                          <text>Set duration time</text>
                        </description>
                        <type name="enumeration">
                          <enum name="duration">
                            <value value="1"/>
                            <description>
                              <text>Set duration time</text>
                            </description>
                          </enum>
                        </type>
                        <mandatory value="true"/>
                      </leaf>
                      <leaf name="seconds">
                        <description>
                          <text>Duration time in seconds</text>
                        </description>
                        <type name="uint32">
                          <range value="1..31536000"/>
                        </type>
                        <mandatory value="true"/>
                      </leaf>
                    </case>
                    <case name="infinite">
                      <description>
                        <text>Infinite active</text>
                      </description>
                      <leaf name="infinite">
                        <description>
