      <enum name="kern">
        <value value="0"/>
        <description>
          <text>Kernel.</text>
        </description>
      </enum>
      <enum name="user">
        <value value="1"/>
        <description>
          <text>User process.</text>
        </description>
      </enum>
      <enum name="mail">
        <value value="2"/>
        <description>
          <text>Mail system.</text>
        </description>
      </enum>
      <enum name="daemon">
        <value value="3"/>
        <description>
          <text>System daemons.</text>
        </description>
      </enum>
      <enum name="auth1">
        <value value="4"/>
        <description>
          <text>Authorization system.</text>
        </description>
      </enum>
      <enum name="syslog">
        <value value="5"/>
        <description>
          <text>Syslog itself.</text>
        </description>
      </enum>
      <enum name="lpr">
        <value value="6"/>
