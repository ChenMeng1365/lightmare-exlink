<?xml version="1.0" encoding="UTF-8"?>
<module name="zxr10-pm-license"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:pm-license="http://www.zte.com.cn/zxr10/netconf/schema/rosng/pm-license"
        xmlns:ncd="http://www.zte.com.cn/zxr10/netconf/schema/rosng/netconf-defs">
  <namespace uri="http://www.zte.com.cn/zxr10/netconf/schema/rosng/pm-license"/>
  <prefix value="pm-license"/>
  <import module="zxr10-netconf-defs">
    <prefix value="ncd"/>
  </import>
  <organization>
    <text>ZTE Corporation</text>
  </organization>
  <contact>
    <text>ZTE Corporation
   Nanjing Institute of ZTE Corporation
   No.68 Zijinghua Rd. Yuhuatai District, Nanjing, China
   Tel: +86-25-52870000</text>
  </contact>
  <revision date="2021-01-28">
    <description>
      <text>Initial version.</text>
    </description>
  </revision>
  <container name="configuration">
    <container name="ports">
      <container name="active-ports">
        <list name="active-port">
          <key value="shelf slot subslot portno restype itemname"/>
          <leaf name="shelf">
            <type name="uint8"/>
          </leaf>
          <leaf name="slot">
            <type name="uint8"/>
          </leaf>
          <leaf name="subslot">
            <type name="uint8"/>
          </leaf>
          <leaf name="portno">
            <type name="uint32"/>
          </leaf>
          <leaf name="restype">
            <type name="uint16"/>
          </leaf>
          <leaf name="itemname">
            <type name="string">
              <length value="1..64"/>
            </type>
          </leaf>
          <leaf name="activeflag">
            <type name="enumeration">
              <enum name="active">
                <value value="1"/>
              </enum>
              <enum name="inactive">
                <value value="0"/>
              </enum>
            </type>
            <default value="inactive"/>
          </leaf>
        </list>
      </container>
    </container>
  </container>
</module>
