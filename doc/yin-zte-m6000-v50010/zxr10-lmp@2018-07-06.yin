<?xml version="1.0" encoding="UTF-8"?>
<module name="zxr10-lmp"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:lmp="http://www.zte.com.cn/zxr10/netconf/schema/rosng/lmp"
        xmlns:ncd="http://www.zte.com.cn/zxr10/netconf/schema/rosng/netconf-defs">
  <namespace uri="http://www.zte.com.cn/zxr10/netconf/schema/rosng/lmp"/>
  <prefix value="lmp"/>
  <yang-version value="1"/>
  <import module="zxr10-netconf-defs">
    <prefix value="ncd"/>
  </import>
  <organization>
    <text>ZTE Corporation</text>
  </organization>
  <contact>
    <text>ZTE Corporation  
   Nanjing Institute of ZTE Corporation 
   No.68 Zijinghua Rd. Yuhuatai District, Nanjing, China 
   Tel: +86-25-52870000</text>
  </contact>
  <description>
    <text>The netconf model for lmp.</text>
  </description>
  <revision date="2018-07-06">
    <description>
      <text>Obsolete notification node lmp,and add  notification node lmp-notify.</text>
    </description>
  </revision>
  <revision date="2016-09-18">
    <description>
      <text>1.Add presence and RESET2DELETE in lmp container
2.Add configuration/lmp/lmp-enable in lmp container
3.Add range in configuration/lmp/neighbors/neighbor/te-links/te-link/te-link-id
4.Add range in lmp/te-link/te-link-id
5.Add units in lmp/te-link/speed
6.Add range in state/te-link/te-link-id
7.Add units in state/te-link/speed
8.Add DELETE2RESET in configuration/lmp/neighbors/neighbor/neighbor/ipccs
9.Modify the maxElements of configuration/lmp/neighbors/neighbor/neighbor</text>
    </description>
  </revision>
  <revision date="2016-08-10"/>
  <container name="configuration">
    <description>
      <text>The branch of configuration model.</text>
    </description>
    <container name="lmp">
      <description>
        <text>The configuration of LMP.</text>
      </description>
      <presence value="lmp"/>
      <leaf name="lmp-enable">
        <description>
          <text>The global switch of LMP.</text>
        </description>
        <type name="empty"/>
        <mandatory value="true"/>
        <ncd:revise-date date="2016-09-18"/>
      </leaf>
      <leaf name="router-id">
        <description>
          <text>The LMP router ID configuration. Must config router id before any other configuration under neighbor node and ensure that router id is reachable in route.</text>
        </description>
        <type name="ncd:ipv4-address">
          <pattern value="([1-9]|[1-9][0-9]|1[0-9][0-9]|2[0-4][0-9]|25[0-5])(.([0-9]|[1-9][0-9]|1[0-9][0-9]|2[0-4][0-9]|25[0-5])){3}"/>
        </type>
      </leaf>
      <container name="neighbors">
        <description>
          <text>The configuration of neighbors.</text>
        </description>
        <list name="neighbor">
          <description>
            <text>The neighbor configuration list.</text>
          </description>
          <max-elements value="100"/>
          <key value="neighbor-name"/>
          <leaf name="neighbor-name">
            <description>
              <text>The LMP neighbor name.</text>
            </description>
            <type name="string">
              <length value="1..31"/>
            </type>
          </leaf>
          <leaf name="remote-router-id">
            <description>
              <text>The remote router id of the LMP neighbor.A LMP neighbor can only own a remote router id which is different with the local router id.</text>
            </description>
            <type name="ncd:ipv4-address">
              <pattern value="([1-9]|[1-9][0-9]|1[0-9][0-9]|2[0-4][0-9]|25[0-5])(.([0-9]|[1-9][0-9]|1[0-9][0-9]|2[0-4][0-9]|25[0-5])){3}"/>
            </type>
          </leaf>
          <container name="ipccs">
            <description>
              <text>The configuration of IP control channels.</text>
            </description>
            <leaf name="static">
              <description>
                <text>Static route.</text>
              </description>
              <type name="empty"/>
            </leaf>
          </container>
          <container name="te-links">
            <description>
              <text>The configuration of TE links.</text>
            </description>
            <list name="te-link">
              <description>
                <text>The TE link configuration list.</text>
              </description>
              <max-elements value="255"/>
              <key value="te-link-id"/>
              <leaf name="te-link-id">
                <description>
                  <text>The identification of TE link.</text>
                </description>
                <type name="uint32">
                  <range value="1..4294967295"/>
                </type>
              </leaf>
              <leaf name="local-te-link-ipaddr">
                <description>
                  <text>The identification of local TE link.</text>
                </description>
                <type name="ncd:ipv4-address">
                  <pattern value="([1-9]|[1-9][0-9]|1[0-9][0-9]|2[0-4][0-9]|25[0-5])(.([0-9]|[1-9][0-9]|1[0-9][0-9]|2[0-4][0-9]|25[0-5])){3}"/>
                </type>
              </leaf>
              <leaf name="remote-te-link-ipaddr">
                <description>
                  <text>The identification of remote TE link.</text>
                </description>
                <type name="ncd:ipv4-address">
                  <pattern value="([1-9]|[1-9][0-9]|1[0-9][0-9]|2[0-4][0-9]|25[0-5])(.([0-9]|[1-9][0-9]|1[0-9][0-9]|2[0-4][0-9]|25[0-5])){3}"/>
                </type>
              </leaf>
              <container name="data-links">
                <description>
                  <text>The configuration of data links.</text>
                </description>
                <list name="data-link">
                  <description>
                    <text>The data link configuration list.</text>
                  </description>
                  <max-elements value="1"/>
                  <key value="if-name"/>
                  <leaf name="if-name">
                    <description>
                      <text>Interface name.</text>
                    </description>
                    <type name="ncd:ifname"/>
                  </leaf>
                </list>
              </container>
            </list>
          </container>
        </list>
      </container>
    </container>
  </container>
  <container name="state">
    <config value="false"/>
    <description>
      <text>The state of LMP.</text>
    </description>
    <list name="te-link">
      <description>
        <text>The list of TE links state.</text>
      </description>
      <key value="te-link-id"/>
      <leaf name="te-link-id">
        <description>
          <text>The identification of TE link.</text>
        </description>
        <type name="uint32">
          <range value="1..4294967295"/>
        </type>
      </leaf>
      <leaf name="speed">
        <description>
          <text>The speed of data link.</text>
        </description>
        <type name="uint32"/>
        <units name="kbps"/>
      </leaf>
      <leaf name="control-state">
        <description>
          <text>The control state of TE link.</text>
        </description>
        <type name="enumeration">
          <enum name="UP">
            <value value="1"/>
          </enum>
          <enum name="DOWN">
            <value value="0"/>
          </enum>
        </type>
      </leaf>
      <leaf name="data-state">
        <description>
          <text>The data state of TE link.</text>
        </description>
        <type name="enumeration">
          <enum name="UP">
            <value value="1"/>
          </enum>
          <enum name="DOWN">
            <value value="0"/>
          </enum>
        </type>
      </leaf>
    </list>
  </container>
  <notification name="lmp">
    <description>
      <text>The notification of LMP.</text>
    </description>
    <status value="obsolete">
      <ncd:revise-date date="2018-07-06"/>
    </status>
    <list name="te-link">
      <description>
        <text>The TE link notification list.</text>
      </description>
      <key value="te-link-id"/>
      <leaf name="te-link-id">
        <description>
          <text>The identification of TE link.</text>
        </description>
        <type name="uint32">
          <range value="1..4294967295"/>
        </type>
      </leaf>
      <leaf name="local-router-id">
        <description>
          <text>The local router id of TE link.</text>
        </description>
        <type name="ncd:ipv4-address"/>
      </leaf>
      <leaf name="local-if-id">
        <description>
          <text>The identification of local TE link.</text>
        </description>
        <type name="ncd:ipv4-address"/>
      </leaf>
      <leaf name="remote-router-id">
        <description>
          <text>The router id of remote TE link.</text>
        </description>
        <type name="ncd:ipv4-address"/>
      </leaf>
      <leaf name="remote-if-id">
        <description>
          <text>The identification of remote TE link.</text>
        </description>
        <type name="ncd:ipv4-address"/>
      </leaf>
      <leaf name="speed">
        <description>
          <text>The speed of data link.</text>
        </description>
        <type name="uint32"/>
        <units name="kbps"/>
      </leaf>
      <leaf name="control-state">
        <description>
          <text>The control state of TE link.</text>
        </description>
        <type name="enumeration">
          <enum name="UP">
            <value value="1"/>
          </enum>
          <enum name="DOWN">
            <value value="0"/>
          </enum>
        </type>
      </leaf>
      <leaf name="data-state">
        <description>
          <text>The data state of TE link.</text>
        </description>
        <type name="enumeration">
          <enum name="UP">
            <value value="1"/>
          </enum>
          <enum name="DOWN">
            <value value="0"/>
          </enum>
        </type>
      </leaf>
      <list name="data-link">
        <description>
          <text>The data link cnotification list.</text>
        </description>
        <key value="if-name"/>
        <leaf name="if-name">
          <description>
            <text>Interface name.</text>
          </description>
          <type name="ncd:ifname"/>
        </leaf>
      </list>
    </list>
  </notification>
  <notification name="lmp-notify">
    <description>
      <text>The notification of LMP.</text>
    </description>
    <list name="te-link">
      <description>
        <text>The TE link notification list.</text>
      </description>
      <key value="te-link-id"/>
      <leaf name="te-link-id">
        <description>
          <text>The identification of TE link.</text>
        </description>
        <type name="uint32">
          <range value="1..4294967295"/>
        </type>
      </leaf>
      <leaf name="local-router-id">
        <description>
          <text>The local router id of TE link.</text>
        </description>
        <type name="ncd:ipv4-address"/>
      </leaf>
      <leaf name="local-if-id">
        <description>
          <text>The identification of local TE link.</text>
        </description>
        <type name="ncd:ipv4-address"/>
      </leaf>
      <leaf name="remote-router-id">
        <description>
          <text>The router id of remote TE link.</text>
        </description>
        <type name="ncd:ipv4-address"/>
      </leaf>
      <leaf name="remote-if-id">
        <description>
          <text>The identification of remote TE link.</text>
        </description>
        <type name="ncd:ipv4-address"/>
      </leaf>
      <leaf name="speed">
        <description>
          <text>The speed of data link.</text>
        </description>
        <type name="uint32"/>
        <units name="kbps"/>
      </leaf>
      <leaf name="control-state">
        <description>
          <text>The control state of TE link.</text>
        </description>
        <type name="enumeration">
          <enum name="UP">
            <value value="1"/>
          </enum>
          <enum name="DOWN">
            <value value="0"/>
          </enum>
        </type>
      </leaf>
      <leaf name="data-state">
        <description>
          <text>The data state of TE link.</text>
        </description>
        <type name="enumeration">
          <enum name="UP">
            <value value="1"/>
          </enum>
          <enum name="DOWN">
            <value value="0"/>
          </enum>
        </type>
      </leaf>
      <list name="data-link">
        <description>
          <text>The data link cnotification list.</text>
        </description>
        <key value="if-name"/>
        <leaf name="if-name">
          <description>
            <text>Interface name.</text>
          </description>
          <type name="ncd:ifname"/>
        </leaf>
      </list>
    </list>
    <ncd:revise-date date="2018-07-06"/>
  </notification>
</module>
