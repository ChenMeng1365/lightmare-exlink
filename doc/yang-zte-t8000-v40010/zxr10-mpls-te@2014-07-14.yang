                    description 'The bandwidth of class type.' ;
                    type uint32 {
                      range 1..4294967295 ;
                    }
                    units kbps ;
                    mandatory true ;
                  }
                }
              }
              container reverse-ct-bandwidths {
                description 'The reverse ct-bandwidth of static tunnel which is'
                          + ' exclusive with reverse out-bandwidth and tunnel-r'
                          + 'ate-limit.'
                            ;
                list reverse-ct-bandwidth {
                  description 'The positive ct-bandwidth list.' ;
                  key class-type ;
                  max-elements 131070 ;
                  leaf class-type {
                    description 'The configuration of class type.' ;
                    type uint8 {
                      range '0..7 | 255' ;
                    }
                  }
                  leaf bandwidth {
                    description 'The bandwidth of class type.' ;
                    type uint32 {
                      range 1..4294967295 ;
                    }
                    units kbps ;
                    mandatory true ;
                  }
                }
              }
            }
          }
        }
      }
      container te-ecmp-groups {
        description 'The configuration of ECMP-GROUP.' ;
        presence 'configuration of ECMP-GROUP.' ;
        list te-ecmp-group {
          description 'The ECMP-GROUP list.' ;
          key group-id ;
          max-elements 128 ;
          min-elements 1 ;
          leaf group-id {
            description 'ECMP-GROUP id.' ;
            type uint16 {
              range 1..65535 ;
            }
          }
          leaf load-share-policy {
            description 'Load-sharing policy of ECMP-GROUP.' ;
            type enumeration {
              enum per-stream {
                value 1 ;
              }
              enum per-packet {
                value 2 ;
              }
            }
            default per-stream ;
          }
          container te-ecmp-tunnels {
            description 'The configuration of ECMP-GROUP tunnels.' ;
            list te-ecmp-tunnel {
              description 'The ECMP-GROUP tunnel list.' ;
              key tunnel-id ;
              leaf tunnel-id {
                description 'Configure ECMP-GROUP tunnel.' ;
                type uint32 {
                  range 1..131072 ;
                }
              }
            }
          }
        }
      }
      container affinity {
        description 'Configuration of affinity.' ;
        presence 'configuration of affinity' ;
        container tunnels {
          description 'The tunnel configuration of affinity function.' ;
          list tunnel {
            description 'The tunnel list configuration of affinity function.' ;
            key 'name affinity-type bit-position' ;
            max-elements 131070 ;
            leaf name {
              description 'Tunnel name.' ;
              type ncd:ifname ;
            }
            leaf affinity-type {
              description 'The tunnel configuration of affinity type,exclude-an'
                        + 'y(1)/include-any(2)/include-all(3).'
                          ;
              type enumeration {
                enum exclude-any {
                  value 1 ;
                  description 'Configure the exclude-any affinity' ;
                }
                enum include-any {
                  value 2 ;
                  description 'Configure the include-any affinity' ;
                }
                enum include-all {
                  value 3 ;
                  description 'Configure the include-all affinity' ;
                }
              }
            }
            leaf bit-position {
              description 'The tunnel configuration of affinity bit position.' ;
              type uint8 {
                range 0..31 ;
              }
            }
          }
        }
        container interfaces {
          description 'The TE interface configuration of affinity function.' ;
          list interface {
            description 'The TE interface list configuration of affinity functi'
                      + 'on.'
                        ;
            key 'name bit-position' ;
            max-elements 8160 ;
            leaf name {
              description 'Interface name.' ;
              type ncd:ifname ;
            }
            leaf bit-position {
              description 'The TE interface configuration of affinity bit posit'
                        + 'ion.'
                          ;
              type uint8 {
                range 0..31 ;
              }
            }
          }
        }
      }
      container te-trunk {
        description 'The configuration of te-trunk.' ;
        presence 'configuration of te-trunk' ;
        list interface {
          description 'Interface list.' ;
          key name ;
          max-elements 255 ;
          leaf name {
            description 'The name of this interface.' ;
            type ncd:ifname ;
          }
        }
      }
    }
  }
  container state {
    description 'The branch of dynamic data model.' ;
    config false ;
    container mpls-te {
      description 'The state of MPLS-TE.' ;
      container dynamic-tunnel {
        description 'The dynamic tunnel information.' ;
        list tunnel {
          description 'The dynamic tunnel list.' ;
          key tunnel-id ;
          leaf tunnel-id {
            description 'The tunnel ID.' ;
            type uint16 ;
          }
          leaf in-use-pathoption {
            description 'The in use path option.' ;
            type uint32 ;
          }
          leaf oper-status {
            description 'The operation status of tunnel.' ;
            type enumeration {
              enum UNKNOWN {
                value 0 ;
              }
              enum UP {
                value 1 ;
              }
              enum DOWN {
                value 2 ;
              }
              enum WTR {
                value 4 ;
              }
            }
          }
          leaf inuselsp-chop-table-index {
            description 'This attribute is used to record chop table index of t'
                      + 'his tunnel.'
                        ;
            type uint32 ;
          }
          leaf inuselsp-arhop-table-index {
            description 'This attribute is used to record arhop table index of '
                      + 'this tunnel.'
                        ;
            type uint32 ;
          }
          leaf hotlsp-chop-table-index {
            description 'This attribute is used to record chop table index of t'
                      + 'his tunnel.'
                        ;
            type uint32 ;
          }
          leaf hotlsp-arhop-table-index {
            description 'This attribute is used to record arhop table index of '
                      + 'this tunnel.'
                        ;
            type uint32 ;
          }
          leaf wtrlsp-chop-table-index {
            description 'This attribute is used to record chop table index of t'
                      + 'his tunnel.'
                        ;
            type uint32 ;
          }
          leaf wtrlsp-arhop-table-index {
            description 'This attribute is used to record arhop table index of '
                      + 'this tunnel.'
                        ;
            type uint32 ;
          }
          leaf hsb-status {
            description 'The hsb status of tunnel.' ;
            type enumeration {
              enum UNKNOWN {
                value 0 ;
              }
              enum READY {
                value 1 ;
              }
              enum ACTIVE {
                value 2 ;
              }
            }
          }
          leaf inuse-lsp-id {
            description 'The lsp identifier inuse.' ;
            type uint16 ;
          }
          leaf hsb-lsp-id {
            description 'The lsp identifier of hsb.' ;
            type uint16 ;
          }
          leaf wtr-lsp-id {
            description 'The lsp identifier of wtr.' ;
            type uint16 ;
          }
        }
        container tunnel-chop {
          description 'The tunnel ERO,it is the path-compute result of ROUTE.' ;
          list ero {
            description 'The tunnel ERO list.' ;
            key 'hop-table-index hop-index' ;
            ncd:select 'hop-table-index hop-index' ;
            leaf hop-table-index {
              description 'The table index of chop.' ;
              type uint32 ;
            }
            leaf hop-index {
              description 'The index of ERO hop.' ;
              type uint32 ;
            }
            leaf addr-type {
              description 'The hop address type.' ;
              type enumeration {
                enum IPV4 {
                  value 1 ;
                }
                enum IPV6 {
                  value 2 ;
                }
                enum AS_NUM {
                  value 3 ;
                }
                enum UNNUM_IF {
                  value 4 ;
                }
                enum LSP_ID {
                  value 5 ;
                }
                enum UNNUM_V6 {
                  value 6 ;
                }
              }
            }
            leaf ip-addr {
              description 'The ip address of hop.' ;
              type ncd:ipv4-address ;
              default 0.0.0.0 ;
            }
            leaf prefix-len {
              description 'The prefix length of ip address.' ;
              type uint8 {
                range 0..64 ;
              }
              default 0 ;
            }
            leaf as-number {
              description 'AS number.' ;
              type uint16 ;
              default 0 ;
            }
            leaf addr-unnum {
              description 'The unnumbered ID.' ;
              type uint32 ;
              default 0 ;
            }
            leaf hop-type {
              description 'The hop type.' ;
              type enumeration {
                enum strictly {
                  value 1 ;
                }
                enum loose {
                  value 2 ;
                }
              }
            }
          }
        }
        container tnnl-ar-hop {
          description 'The tunnel RRO hop,It records the bypassed path informat'
                    + 'ion of tunnel.'
                      ;
          list rro {
            description 'The tunnel RRO hop list.' ;
            key 'hop-table-index hop-index' ;
            ncd:select 'hop-table-index hop-index' ;
            leaf hop-table-index {
              description 'The table index of arhop.' ;
              type uint32 ;
            }
            leaf hop-index {
              description 'The hop index of RRO.' ;
              type uint32 ;
            }
            leaf lsr-id {
              description 'The router id.' ;
              type ncd:ipv4-address ;
            }
            leaf if-ipaddr {
              description 'The in interface address of tunnel.' ;
              type ncd:ipv4-address ;
            }
            leaf if-ipmask {
              description 'The in interface mask of tunnel.' ;
              type uint32 ;
            }
            leaf path-inlabel {
              description 'The in label of the tunnel path.' ;
              type uint32 ;
            }
            leaf path-outlabel {
              description 'The out label of the tunnel path.' ;
              type uint32 ;
            }
            leaf resv-inlabel {
              description 'The in label of the tunnel resv.' ;
              type uint32 ;
            }
            leaf resv-outlabel {
              description 'The out label of the tunnel resv.' ;
              type uint32 ;
            }
          }
        }
      }
      container remote-tunnel {
        description 'The remote tunnel,it is related with ingress tunnel.' ;
        list tunnel {
          description 'The remote tunnel list.' ;
          key 'local-tunnel-id lsp-id ingress-id egress-id' ;
          ncd:select 'local-tunnel-id lsp-id ingress-id egress-id' ;
          leaf local-tunnel-id {
            description 'The local tunnel ID of remote tunnel.' ;
            type uint16 ;
          }
          leaf lsp-id {
            description 'The lsp id of remote tunnel.' ;
            type uint16 ;
          }
          leaf ingress-id {
            description 'The ingress LSR ID' ;
            type ncd:ipv4-address ;
          }
          leaf egress-id {
            description 'The egress LSR ID.' ;
            type ncd:ipv4-address ;
          }
          leaf in-interface {
            description 'The in interface of remote tunnel.' ;
            type string {
              length 1..31 ;
            }
            default unknown ;
          }
          leaf in-label {
            description 'The in label of remote tunnel.' ;
            type uint32 ;
            default 4294967295 ;
          }
          leaf out-interface {
            description 'The out interface of remote tunnel.' ;
            type string {
              length 1..31 ;
            }
            default unknown ;
          }
          leaf out-label {
            description 'The out label of remote tunnel.' ;
            type uint32 {
              range 0..4294967295 ;
            }
            default 4294967295 ;
          }
          leaf oper-status {
            description 'The operation status of remote tunnel.' ;
            type enumeration {
              enum down {
                value 0 ;
              }
              enum up {
                value 1 ;
              }
            }
            default down ;
          }
        }
      }
      container static-tunnel {
        description 'The state data of static tunnel.' ;
        list tunnel {
          description 'The static tunnel list.' ;
          key tunnel-id ;
          leaf tunnel-id {
            description 'The tunnel ID.' ;
            type uint32 ;
          }
          leaf oper-status {
            description 'The operation status of tunnel.' ;
            type enumeration {
              enum down {
                value 0 ;
              }
              enum up {
                value 1 ;
              }
            }
            default down ;
          }
          leaf admin-status {
            description 'The admin status of tunnel.' ;
            type enumeration {
              enum down {
                value 0 ;
              }
              enum up {
                value 1 ;
              }
            }
            default down ;
          }
          leaf associated-status {
            description 'The associated status of static tunnel.' ;
            type enumeration {
              enum disabled {
                value 1 ;
              }
              enum unbond {
                value 2 ;
              }
              enum bond {
                value 3 ;
              }
            }
            default disabled ;
          }
          leaf sample-bandwidth {
            description 'The sampled bandwidth of static tunnel.' ;
            type uint32 ;
            units kbps ;
            default 0 ;
          }
        }
      }
      container egress-tunnel {
        description 'The egress tunnel information.' ;
        list tunnel {
          description 'The egress tunnel list.' ;
          key 'tunnel-id ingress-id egress-id lsp-id' ;
          leaf tunnel-id {
            description 'The tunnel ID of egress tunnel.' ;
            type uint16 ;
          }
          leaf ingress-id {
            description 'The ingress LSR ID' ;
            type ncd:ipv4-address ;
          }
          leaf egress-id {
            description 'The egress LSR ID.' ;
            type ncd:ipv4-address ;
          }
          leaf lsp-id {
            description 'The lsp id of remote tunnel.' ;
            type uint16 ;
          }
          leaf lsp-arhop-index {
            description 'The arhop index of arhop.' ;
            type uint32 ;
          }
        }
      }
      container pathoption-addition {
        description 'The additional information of pathoption.' ;
        list pathoption {
          description 'The list of pathoption.' ;
          key 'tunnel-id option-id' ;
          ncd:select 'tunnel-id option-id' ;
          leaf tunnel-id {
            description 'The identifier of tunnel.' ;
            type uint16 ;
          }
          leaf option-id {
            description 'The option identifier of tunnel.' ;
            type uint8 ;
          }
          leaf last-time-rst {
            description 'The result of creat the lsp.' ;
            type uint8 ;
          }
          leaf lsp-id {
            description 'The lsp id of successed creat.' ;
            type uint16 ;
          }
          leaf errcode {
            description 'The type of wrong.' ;
            type uint16 ;
          }
          leaf errvalue {
            description 'The value of last failed creat.' ;
            type uint16 ;
          }
          leaf last-try-time {
            description 'The time of creat the lsp.' ;
            type ncd:datetime ;
          }
          leaf fail-node-ipaddr {
            description 'The wrong value of the failed creat.' ;
            type ncd:ipv4-address ;
          }
        }
      }
    }
  }
}
