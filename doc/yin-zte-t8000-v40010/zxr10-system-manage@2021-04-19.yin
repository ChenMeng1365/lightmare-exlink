    <leaf name="timestamp">
      <description>
        <text>After the timestamp function is enabled, the system time is printed first when the 'show' command is executed.</text>
      </description>
      <type name="enumeration">
        <enum name="enable">
          <value value="1"/>
          <description>
            <text>Enable the system's timestamp function</text>
          </description>
        </enum>
        <enum name="disable">
          <value value="2"/>
          <description>
            <text>Disable the system's timestamp function</text>
          </description>
        </enum>
      </type>
      <default value="disable"/>
    </leaf>
