  <container name="configuration">
    <description>
      <text>CFM configuration information.</text>
    </description>
    <container name="cfm">
      <description>
        <text>CFM configuration information.</text>
      </description>
      <container name="global">
        <description>
          <text>CFM global information.</text>
        </description>
        <leaf name="admin-state">
          <description>
            <text>CFM global state.</text>
          </description>
          <type name="enumeration">
            <enum name="enable">
              <value value="1"/>
              <description>
                <text>Enable CFM global status</text>
              </description>
            </enum>
            <enum name="disable">
              <value value="2"/>
              <description>
                <text>Disable CFM global status</text>
              </description>
            </enum>
          </type>
          <default value="disable"/>
        </leaf>
      </container>
      <container name="mds">
        <description>
          <text>CFM MD information.</text>
        </description>
        <list name="md">
          <description>
            <text>MD list.</text>
          </description>
          <key value="index"/>
          <leaf name="index">
            <description>
              <text>MD index. Its maximum value depends on the performance parameters.</text>
            </description>
            <type name="uint16">
              <range value="1..65535"/>
            </type>
          </leaf>
          <leaf name="level">
            <description>
              <text>MD level.</text>
            </description>
            <type name="uint8">
              <range value="0..7"/>
            </type>
            <mandatory value="true"/>
          </leaf>
          <choice name="name-format">
            <description>
              <text>Name format.</text>
            </description>
            <mandatory value="true"/>
            <case name="name-no-present">
              <description>
                <text>No name present.</text>
              </description>
              <leaf name="name-no-present">
                <description>
                  <text>No name present.</text>
                </description>
                <type name="empty"/>
              </leaf>
            </case>
            <case name="name-domain-name-string">
              <description>
                <text>Domain Name based string.</text>
              </description>
              <leaf name="name-domain-name-string">
                <description>
                  <text>Domain Name based string.</text>
                </description>
                <type name="string">
                  <length value="1..43"/>
                </type>
              </leaf>
            </case>
            <case name="name-2-octets-integer">
              <description>
                <text>2-octet integer.</text>
              </description>
              <leaf name="name-2-octets-integer">
                <description>
                  <text>2-octet integer.</text>
                </description>
                <type name="uint16">
                  <range value="0..65535"/>
                </type>
              </leaf>
            </case>
            <case name="name-ietf-rfc2579-string">
              <description>
                <text>Based IETF RFC2579 string.</text>
              </description>
              <leaf name="name-ietf-rfc2579-string">
                <description>
                  <text>Based IETF RFC2579 string.</text>
                </description>
                <type name="string">
                  <length value="1..43"/>
                </type>
              </leaf>
            </case>
          </choice>
        </list>
      </container>
      <container name="mas">
        <description>
          <text>CFM MA information.</text>
        </description>
        <list name="ma">
          <description>
            <text>MA list.</text>
          </description>
          <key value="md-index ma-index"/>
          <leaf name="md-index">
            <description>
              <text>MD index. Its maximum value depends on the performance parameters.</text>
            </description>
            <type name="uint16">
              <range value="1..65535"/>
            </type>
          </leaf>
          <leaf name="ma-index">
            <description>
              <text>MA index. Its maximum value depends on the performance parameters.</text>
            </description>
            <type name="uint16">
              <range value="1..65535"/>
            </type>
          </leaf>
          <leaf name="pvid">
            <status value="obsolete">
              <ncd:revise-date date="2018-05-18"/>
            </status>
            <description>
              <text>MA Primary VID.</text>
            </description>
            <type name="uint16">
              <range value="1..4094"/>
            </type>
          </leaf>
          <leaf name="ccm-interval">
            <description>
              <text>CCM transmission period indication. 1-7 respectively, corresponding to the period of 3.3ms, 10ms, 100ms, 1s, 10s, 60s and 600s.</text>
            </description>
            <type name="uint16">
              <range value="1..7"/>
            </type>
            <default value="4"/>
          </leaf>
          <choice name="name-format">
            <description>
              <text>Name format.</text>
            </description>
            <mandatory value="true"/>
            <case name="name-primary-vid">
              <description>
                <text>Primary VID.</text>
              </description>
              <leaf name="name-primary-vid">
                <description>
                  <text>Primary VID.</text>
                </description>
                <type name="uint32">
                  <range value="0..4094"/>
                </type>
              </leaf>
            </case>
            <case name="name-ietf-rfc2579-string">
              <description>
                <text>Based IETF RFC2579 string.</text>
              </description>
              <leaf name="name-ietf-rfc2579-string">
                <description>
                  <text>Based IETF RFC2579 string.</text>
                </description>
                <type name="string">
                  <length value="1..45"/>
                </type>
              </leaf>
            </case>
            <case name="name-2-octets-integer">
              <description>
                <text>2-octet integer.</text>
              </description>
              <leaf name="name-2-octets-integer">
                <description>
                  <text>2-octet integer.</text>
                </description>
                <type name="uint32">
                  <range value="0..65535"/>
                </type>
              </leaf>
            </case>
            <case name="name-integer-4-octets">
              <description>
                <text>4-octet integer.</text>
              </description>
              <leaf name="name-integer-4-octets">
                <description>
                  <text>4-octet integer.</text>
                </description>
                <type name="uint32">
                  <range value="1..4294967295"/>
                </type>
              </leaf>
            </case>
            <case name="name-icc-string">
              <description>
                <text>ICC-based Format.</text>
              </description>
              <leaf name="name-icc-string">
                <description>
                  <text>MA name. It consists of two subfields: the ITU carrier code (ICC) followed by a unique MAID code (UMC). The ITU Carrier Code consists of 1-6 left-justified characters, alphabetic, or leading alphabetic with trailing numeric. The UMC code immediately follows the ICC and shall consist of 7-12 characters, with trailing NULLs, completing the 13-character MAID Value.</text>
                </description>
                <type name="string">
                  <length value="1..13"/>
                </type>
              </leaf>
            </case>
          </choice>
          <leaf name="addition-detect-vlans">
            <status value="obsolete">
              <ncd:revise-date date="2017-12-05"/>
            </status>
            <description>
              <text>Additional detect VLAN list, VLAN range from [1..4094].</text>
            </description>
            <type name="ncd:bitmap">
              <length value="0..13312">
                <description>
                  <text>VLAN list string length, VLAN range from [1..4094].</text>
                </description>
              </length>
            </type>
            <ncd:revise-date date="2015-05-06"/>
          </leaf>
          <choice name="primary-vid">
            <description>
              <text>MA Primary VID.</text>
            </description>
            <case name="dot1q">
              <description>
                <text>Dot1q VLAN.</text>
              </description>
              <leaf name="vid">
                <description>
                  <text>MA Primary VID.</text>
                </description>
                <type name="uint16">
                  <range value="1..4094"/>
                </type>
              </leaf>
            </case>
            <case name="qinq">
              <description>
                <text>QinQ VLAN.</text>
              </description>
              <leaf name="internal-vid">
                <description>
                  <text>MA internal Primary VID.</text>
                </description>
                <type name="uint16">
                  <range value="1..4094"/>
                </type>
                <mandatory value="true"/>
              </leaf>
              <leaf name="external-vid">
                <description>
                  <text>MA external Primary VID.</text>
                </description>
                <type name="uint16">
                  <range value="1..4094"/>
                </type>
                <mandatory value="true"/>
              </leaf>
            </case>
            <ncd:revise-date date="2018-05-18"/>
          </choice>
        </list>
      </container>
      <container name="lmeps">
        <description>
          <text>CFM local MEP information.</text>
        </description>
        <list name="lmep">
          <description>
            <text>Local MEP list.</text>
          </description>
