  <feature name="nil-fec"/>
  <feature name="evpn-vpws"/>
  <feature name="evpn-vpls"/>
  <feature name="sr-mpls-policy"/>
  <typedef name="path-role">
    <type name="enumeration">
      <enum name="master">
        <value value="1"/>
        <description>
          <text>Master</text>
        </description>
      </enum>
      <enum name="slave">
        <value value="2"/>
        <description>
          <text>Slave</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Path role</text>
    </description>
  </typedef>
  <container name="operation">
    <config value="false"/>
    <description>
      <text>LSPDF operations model info.</text>
    </description>
    <container name="lspdf">
      <description>
        <text>LSPDF operation parameters.</text>
      </description>
      <container name="lsp-pings">
        <description>
          <text>Lsp ping operation parameters.</text>
        </description>
        <list name="lsp-ping">
          <description>
            <text>Lsp ping operation parameters.</text>
          </description>
          <key value="serial-num"/>
          <leaf name="serial-num">
            <description>
              <text>Lsp ping serial number.</text>
            </description>
            <type name="uint64"/>
          </leaf>
          <choice name="status">
            <description>
              <text>The switch for turning on/off lsp ping.</text>
            </description>
            <mandatory value="true"/>
            <case name="enable">
              <description>
                <text>Start lsp ping function.</text>
              </description>
              <leaf name="ping-enable">
                <description>
                  <text>Start lsp ping function.</text>
                </description>
                <type name="empty"/>
                <mandatory value="true"/>
              </leaf>
              <container name="lsp-ping-type">
                <description>
                  <text>Detect type parameters for lsp ping.</text>
                </description>
                <choice name="lsp-type">
                  <description>
                    <text>Detect type for lsp ping.</text>
                  </description>
                  <mandatory value="true"/>
                  <case name="ipv4">
                    <description>
                      <text>LDP IPv4 detect type.</text>
                    </description>
                    <container name="ipv4">
                      <description>
                        <text>LDP IPv4 detect type.</text>
                      </description>
                      <leaf name="fec-address">
                        <description>
                          <text>FEC address for IPv4 lsp ping.</text>
                        </description>
                        <type name="ncd:ipv4-address"/>
                        <mandatory value="true"/>
                      </leaf>
                      <leaf name="mask-length">
                        <description>
                          <text>FEC mask length for IPv4 lsp ping.</text>
                        </description>
                        <type name="uint16">
                          <range value="0..32">
                            <description>
                              <text>FEC mask length for IPv4 lsp ping.</text>
                            </description>
                          </range>
                        </type>
                        <mandatory value="true"/>
                      </leaf>
                      <leaf name="out-if">
                        <description>
                          <text>Output interface for IPv4 lsp ping.</text>
                        </description>
                        <type name="ncd:ifname"/>
                      </leaf>
                      <leaf name="source">
                        <description>
                          <text>Source IPv4 address.</text>
                        </description>
                        <type name="ncd:ipv4-address"/>
                      </leaf>
                    </container>
                  </case>
                  <case name="ipv6">
                    <description>
                      <text>LDP IPv6 detect type.</text>
                    </description>
                    <container name="ipv6">
                      <description>
                        <text>LDP IPv6 detect type.</text>
                      </description>
                      <leaf name="ipv6-fec-address">
                        <description>
                          <text>FEC address for IPv6 lsp ping.</text>
                        </description>
                        <type name="ncd:ipv6-address"/>
                        <mandatory value="true"/>
                      </leaf>
                      <leaf name="ipv6-mask-len">
                        <description>
                          <text>FEC mask length for IPv6 lsp ping.</text>
                        </description>
                        <type name="uint16">
                          <range value="0..128">
                            <description>
                              <text>FEC mask length for IPv6 lsp ping.</text>
                            </description>
                          </range>
                        </type>
                        <mandatory value="true"/>
                      </leaf>
                      <leaf name="source">
                        <description>
                          <text>Source IPv6 address.</text>
                        </description>
                        <type name="ncd:ipv6-address"/>
                      </leaf>
                    </container>
                  </case>
                  <case name="traffic-eng">
                    <description>
                      <text>TE tunnel detect type.</text>
                    </description>
                    <container name="traffic-eng">
                      <description>
                        <text>TE tunnel detect type.</text>
                      </description>
                      <leaf name="tunnel-name">
                        <description>
                          <text>TE tunnel name.</text>
                        </description>
                        <type name="ncd:ifname"/>
                        <mandatory value="true"/>
                      </leaf>
                      <leaf name="lsp-role">
                        <description>
                          <text>Lsp role for TE lsp ping, include master and slave.</text>
                        </description>
                        <type name="enumeration">
                          <enum name="master">
                            <value value="1"/>
                            <description>
                              <text>The master lsp of TUNNEL.</text>
                            </description>
                          </enum>
                          <enum name="slave">
                            <value value="2"/>
                            <description>
                              <text>The slave lsp of TUNNEL.</text>
                            </description>
                          </enum>
                        </type>
                      </leaf>
                      <leaf name="source">
                        <description>
                          <text>Source IPv4 address.</text>
                        </description>
                        <type name="ncd:ipv4-address"/>
                      </leaf>
                      <leaf name="ipv6-source">
                        <description>
                          <text>Source IPv6 address.</text>
                        </description>
                        <type name="ncd:ipv6-address"/>
                        <ncd:revise-date date="2019-09-24"/>
                      </leaf>
                      <leaf name="without-explicit-null-label">
                        <description>
                          <text>No extra explicit null label included, 0: include, 1: exclude. Default value of this parameter depends on the device.</text>
                        </description>
                        <type name="uint8">
                          <range value="0..1">
                            <description>
                              <text>No extra explicit null label included, 0: include, 1: exclude.</text>
                            </description>
                          </range>
                        </type>
                        <ncd:revise-date date="2019-08-19"/>
                      </leaf>
                    </container>
                  </case>
                  <case name="pseudowire">
                    <description>
                      <text>PW detect type.</text>
                    </description>
                    <container name="pseudowire">
                      <description>
                        <text>PW detect type.</text>
                      </description>
                      <leaf name="mspw-flag">
                        <description>
                          <text>The flag which identify the PW is MSPW or not.</text>
                        </description>
                        <type name="enumeration">
                          <enum name="sspw">
                            <value value="0"/>
                          </enum>
                          <enum name="mspw">
                            <value value="1"/>
                          </enum>
                        </type>
                        <default value="sspw"/>
                      </leaf>
                      <leaf name="pw-name">
                        <description>
                          <text>PW name for lsp ping.</text>
                        </description>
                        <type name="string"/>
                        <mandatory value="true"/>
                      </leaf>
                      <leaf name="remote-address">
                        <description>
                          <text>Remote address of PW.</text>
                        </description>
                        <type name="ncd:ipv4-address"/>
                      </leaf>
                      <leaf name="vc-id">
                        <description>
                          <text>Remote vc-id of PW.</text>
                        </description>
                        <type name="uint32">
                          <range value="1..4294967295">
                            <description>
                              <text>Remote vc-id of PW.</text>
                            </description>
                          </range>
                        </type>
                      </leaf>
                      <leaf name="source">
                        <description>
                          <text>Source IPv4 address.</text>
                        </description>
                        <type name="ncd:ipv4-address"/>
                      </leaf>
                    </container>
                  </case>
                  <case name="igp">
                    <description>
                      <text>SR BE detect type.</text>
                    </description>
                    <container name="igp">
                      <description>
                        <text>SR BE detect type.</text>
                      </description>
                      <leaf name="fec-address">
                        <description>
                          <text>FEC address for SR BE lsp ping.</text>
                        </description>
                        <type name="ncd:ipv4-address"/>
                        <mandatory value="true"/>
                      </leaf>
                      <leaf name="mask-length">
                        <description>
                          <text>FEC mask length for SR BE lsp ping.</text>
                        </description>
                        <type name="uint16">
                          <range value="0..32">
                            <description>
                              <text>FEC mask length for IPv4 lsp ping.</text>
                            </description>
                          </range>
                        </type>
                        <mandatory value="true"/>
                      </leaf>
                      <leaf name="source">
                        <description>
                          <text>Source IPv4 address.</text>
                        </description>
                        <type name="ncd:ipv4-address"/>
                      </leaf>
                    </container>
                    <ncd:revise-date date="2019-05-14"/>
                  </case>
                  <case name="igpv6">
                    <description>
                      <text>SR-BE IPv6 detect type.</text>
                    </description>
                    <container name="igpv6">
                      <description>
                        <text>SR-BE IPv6 detect type.</text>
                      </description>
                      <leaf name="ipv6-fec-address">
                        <description>
                          <text>FEC address for SR-BE IPv6 LSP ping.</text>
                        </description>
                        <type name="ncd:ipv6-address"/>
                        <mandatory value="true"/>
                      </leaf>
                      <leaf name="ipv6-mask-len">
                        <description>
                          <text>FEC mask length for SR-BE IPv6 LSP ping.</text>
                        </description>
                        <type name="uint16">
                          <range value="0..128">
                            <description>
                              <text>FEC mask length for SR-BE IPv6 LSP ping.</text>
                            </description>
                          </range>
                        </type>
                        <mandatory value="true"/>
                      </leaf>
                      <leaf name="source">
                        <description>
                          <text>IPv6 Source address.</text>
                        </description>
                        <type name="ncd:ipv6-address"/>
                      </leaf>
                    </container>
                    <ncd:revise-date date="2019-07-02"/>
                  </case>
                  <case name="nil-fec">
                    <description>
                      <text>NIL FEC detect type.</text>
                    </description>
                    <container name="nil-fec">
                      <description>
                        <text>NIL FEC detect type.</text>
                      </description>
                      <leaf-list name="labels">
                        <description>
                          <text>List of nil-fec labels</text>
                        </description>
                        <type name="uint32">
                          <range value="16..1048575"/>
                        </type>
                        <min-elements value="1"/>
                        <ncd:revise-date date="2020-10-16"/>
                      </leaf-list>
                      <leaf name="out-if">
                        <description>
                          <text>Outgoing interface for nil fec lsp ping.</text>
                        </description>
                        <type name="ncd:ifname"/>
                      </leaf>
                      <choice name="address-paras">
                        <description>
                          <text>Address paras</text>
                        </description>
                        <case name="ipv6">
                          <container name="ipv6">
                            <leaf name="nexthop">
                              <description>
                                <text>nexthop address</text>
                              </description>
                              <type name="ncd:ipv6-address-none-zero"/>
                            </leaf>
                            <leaf name="source">
                              <description>
                                <text>source address</text>
                              </description>
                              <type name="ncd:ipv6-address-none-zero"/>
                            </leaf>
                          </container>
                        </case>
                        <case name="ipv4">
                          <container name="ipv4">
                            <leaf name="nexthop">
                              <description>
                                <text>nexthop address</text>
                              </description>
                              <type name="ncd:ipv4-address-none-zero"/>
                            </leaf>
                            <leaf name="source">
                              <description>
                                <text>source address</text>
                              </description>
                              <type name="ncd:ipv4-address-none-zero"/>
                            </leaf>
                          </container>
                        </case>
                      </choice>
                    </container>
                    <ncd:revise-date date="2019-08-28"/>
                  </case>
                  <case name="evpn-vpws">
                    <description>
                      <text>EVPN VPWS detect type.</text>
                    </description>
                    <container name="evpn-vpws">
                      <description>
                        <text>EVPN VPWS detect type.</text>
                      </description>
                      <leaf name="vpws-name">
                        <description>
                          <text>EVPN VPWS instance name.</text>
                        </description>
                        <type name="string">
                          <length value="1..32">
                            <description>
                              <text>VPWS instance name</text>
                            </description>
                          </length>
                        </type>
                        <mandatory value="true"/>
                      </leaf>
                      <choice name="vpws-key">
                        <description>
                          <text>Access point or EVPN VPWS ID.</text>
                        </description>
                        <mandatory value="true"/>
                        <case name="interface">
                          <description>
                            <text>Access point.</text>
                          </description>
                          <leaf name="interface">
                            <description>
                              <text>Access point.</text>
                            </description>
                            <type name="ncd:ifname"/>
                            <mandatory value="true"/>
                          </leaf>
                        </case>
                        <case name="vpws-id">
                          <description>
                            <text>EVPN VPWS ID.</text>
                          </description>
                          <leaf name="local-vpws-id">
                            <description>
                              <text>Local EVPN VPWS ID.</text>
                            </description>
                            <type name="uint32">
                              <range value="1..4294967294"/>
                            </type>
                            <mandatory value="true"/>
                          </leaf>
                          <leaf name="remote-vpws-id">
                            <description>
                              <text>Remote EVPN VPWS ID.</text>
                            </description>
                            <type name="uint32">
                              <range value="1..4294967294"/>
                            </type>
                            <mandatory value="true"/>
                          </leaf>
                          <ncd:revise-date date="2020-09-03"/>
                        </case>
                      </choice>
                      <choice name="source-address">
                        <description>
                          <text>Source address.</text>
                        </description>
                        <case name="source">
                          <description>
                            <text>IPv4 source address.</text>
                          </description>
                          <leaf name="source">
                            <description>
                              <text>IPv4 source address.</text>
                            </description>
                            <type name="ncd:ipv4-address-none-zero"/>
                          </leaf>
                        </case>
                        <case name="ipv6-source">
                          <description>
                            <text>IPv6 source address.</text>
                          </description>
                          <leaf name="ipv6-source">
                            <description>
                              <text>IPv6 source address.</text>
                            </description>
                            <type name="ncd:ipv6-address-none-zero"/>
                          </leaf>
                          <ncd:revise-date date="2020-09-03"/>
                        </case>
                      </choice>
                      <choice name="ecmp-frr">
                        <description>
                          <text>ECMP or FRR.</text>
                        </description>
                        <case name="ecmp">
                          <description>
                            <text>If detect all active pathes.</text>
                          </description>
                          <leaf name="ecmp">
                            <description>
                              <text>If detect all active pathes.</text>
                            </description>
                            <type name="enumeration">
                              <enum name="disable">
                                <value value="0"/>
                                <description>
                                  <text>Detect single path in used.</text>
                                </description>
                              </enum>
                              <enum name="enable">
                                <value value="1"/>
                                <description>
                                  <text>Detect all pathes in used.</text>
                                </description>
                              </enum>
                            </type>
                          </leaf>
                        </case>
                        <case name="frr">
                          <description>
                            <text>Detect master or slave path.</text>
                          </description>
                          <leaf name="path-role">
                            <description>
                              <text>Path role</text>
                            </description>
                            <type name="path-role"/>
                          </leaf>
                        </case>
                        <ncd:revise-date date="2020-07-01"/>
                      </choice>
                    </container>
                    <ncd:revise-date date="2020-03-10"/>
                  </case>
                  <case name="evpn-vpls">
                    <description>
                      <text>EVPN VPLS detect type.</text>
                    </description>
                    <container name="evpn-vpls">
                      <description>
                        <text>EVPN VPLS detect type.</text>
                      </description>
                      <leaf name="vpls-name">
                        <description>
                          <text>EVPN VPLS instance name.</text>
                        </description>
                        <type name="string">
                          <length value="1..32">
                            <description>
                              <text>VPLS instance name</text>
                            </description>
                          </length>
                        </type>
                        <mandatory value="true"/>
                      </leaf>
                      <choice name="peer-address">
                        <description>
                          <text>Peer original address.</text>
                        </description>
                        <mandatory value="true"/>
                        <case name="peer">
                          <description>
                            <text>IPv4 Peer original address.</text>
                          </description>
                          <leaf name="peer">
                            <description>
                              <text>IPv4 peer address.</text>
                            </description>
                            <type name="ncd:ipv4-address-none-zero"/>
                            <mandatory value="true"/>
                          </leaf>
                        </case>
                        <case name="ipv6-peer">
                          <description>
                            <text>IPv6 Peer original address.</text>
                          </description>
                          <leaf name="ipv6-peer">
                            <description>
                              <text>IPv6 peer address.</text>
