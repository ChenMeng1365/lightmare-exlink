          <text>Internal route of BGP.</text>
        </description>
      </enum>
      <enum name="ebgp">
        <value value="11"/>
        <description>
          <text>Eternal route of BGP.</text>
        </description>
      </enum>
      <enum name="mvpn1">
        <value value="16"/>
        <description>
          <text>MVPN1 type of route.</text>
        </description>
      </enum>
      <enum name="mvpn2">
        <value value="17"/>
        <description>
          <text>MVPN2 type of route.</text>
        </description>
      </enum>
      <enum name="mvpn3">
        <value value="18"/>
        <description>
          <text>MVPN3 type of route.</text>
        </description>
      </enum>
      <enum name="mvpn4">
        <value value="19"/>
        <description>
          <text>MVPN4 type of route.</text>
        </description>
      </enum>
      <enum name="mvpn5">
        <value value="20"/>
        <description>
          <text>MVPN5 type of route.</text>
        </description>
      </enum>
      <enum name="mvpn6">
        <value value="21"/>
        <description>
          <text>MVPN6 type of route.</text>
        </description>
      </enum>
      <enum name="mvpn7">
        <value value="22"/>
        <description>
          <text>MVPN7 type of route.</text>
        </description>
      </enum>
      <enum name="evpn-ad">
        <value value="23"/>
        <description>
          <text>Ethernet Auto-Discovery (A-D) Route.</text>
        </description>
      </enum>
      <enum name="evpn-es">
        <value value="24"/>
        <description>
          <text>Ethernet Segment Route.</text>
        </description>
      </enum>
      <enum name="evpn-inclusive">
        <value value="25"/>
        <description>
          <text>Inclusive Multicast Route.</text>
        </description>
      </enum>
      <enum name="evpn-join">
        <value value="26"/>
        <description>
          <text>IGMP Join Synch Route.</text>
