  <typedef name="vgmp-type">
    <type name="enumeration">
      <enum name="active">
        <value value="1"/>
        <description>
          <text>Status of active.</text>
        </description>
      </enum>
      <enum name="standby">
        <value value="2"/>
        <description>
          <text>Status of standby.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>The configuration of VRRP.</text>
    </description>
