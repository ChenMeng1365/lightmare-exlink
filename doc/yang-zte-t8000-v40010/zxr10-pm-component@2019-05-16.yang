                            ;
              }
              enum DISABLED {
                value 2 ;
                description 'The remote controlled of the led is released, and '
                          + 'controlled by device'
                            ;
              }
            }
          }
          leaf type {
            description 'Type of component as identified by the system'
                        ;
            type enumeration {
              enum UNKNOWN {
                value 0 ;
              }
              enum CHASSIS {
                value 1 ;
              }
              enum BACKPLANE {
                value 2 ;
              }
              enum FABRIC {
                value 3 ;
              }
              enum POWER_SUPPLY {
                value 4 ;
              }
              enum FAN {
                value 5 ;
              }
              enum SENSOR {
                value 6 ;
              }
              enum FRU {
                value 7 ;
              }
              enum LINECARD {
                value 8 ;
              }
              enum PORT {
                value 9 ;
              }
              enum TRANSCEIVER {
                value 10 ;
