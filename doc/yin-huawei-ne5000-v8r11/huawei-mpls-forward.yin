  <augment target-node="/loadbalance:loadbalance">
    <description>
      <text>MPLS forward configuration to the load balance module.</text>
    </description>
    <container name="mpls-hashpacket">
      <description>
        <text>Configure load balance based on packet.</text>
      </description>
      <leaf name="hash-type">
        <type name="hash-type"/>
        <default value="flow"/>
        <description>
          <text>Load balance mode.</text>
        </description>
      </leaf>
    </container>
    <container name="mpls-intfmtu">
      <description>
        <text>Configure interface MTU check mode.</text>
      </description>
      <leaf name="chkmode">
        <type name="check-mode"/>
        <default value="ip"/>
        <description>
          <text>Check mode configuration.</text>
        </description>
      </leaf>
    </container>
    <container name="mpls-loadblc-elawords">
      <description>
        <text>Configure MPLS entropy label load balance control word enable.</text>
      </description>
      <leaf name="elawords-enable">
        <type name="enable-flag"/>
        <default value="disable"/>
        <description>
          <text>MPLS entropy label load balance control word enable value.</text>
        </description>
      </leaf>
    </container>
    <container name="p2mp-load-balance-num">
      <description>
        <text>Configure P2MP trunk load balance.</text>
      </description>
      <leaf name="number">
        <type name="uint32">
          <range value="2..64"/>
        </type>
        <default value="16"/>
        <description>
          <text>P2MP trunk load balance number.</text>
        </description>
      </leaf>
    </container>
  </augment>
  <augment target-node="/ni:network-instance/ni:instances/ni:instance/l3vpn:afs/l3vpn:af">
    <description>
      <text>MPLS forward configuration to the L3VPN module.</text>
    </description>
    <container name="vrfpipe">
      <description>
        <text>Configure L3VPN pipe.</text>
      </description>
      <leaf name="split-mode">
        <type name="boolean"/>
        <must condition="((../split-mode='false' and ../pipe-mode and not(../ingress-pipe-mode) and not(../egress-pipe-mode)) or (../split-mode='true' and not(../pipe-mode) and ../ingress-pipe-mode and ../egress-pipe-mode))"/>
        <default value="false"/>
        <description>
          <text>Enable/disable pipe mode.</text>
        </description>
      </leaf>
      <leaf name="pipe-mode">
        <when condition="../split-mode='false'"/>
        <type name="l3vpn-pipe-mode"/>
        <default value="uniform"/>
        <description>
          <text>Pipe mode.</text>
        </description>
      </leaf>
      <leaf name="ingress-pipe-mode">
        <when condition="../split-mode='true'"/>
        <type name="l3vpn-ingpipe-mode"/>
        <default value="uniform"/>
        <description>
          <text>Ingress pipe mode.</text>
        </description>
      </leaf>
      <leaf name="service-class">
        <when condition="(../pipe-mode='pipe' or ../pipe-mode='shortpipe' or ../ingress-pipe-mode='pipe' or ../ingress-pipe-mode='shortpipe')"/>
        <type name="l3vpn-service-class"/>
        <default value="be"/>
        <description>
          <text>Service class.</text>
        </description>
      </leaf>
      <leaf name="color">
        <when condition="(../pipe-mode='pipe' or ../pipe-mode='shortpipe'  or ../ingress-pipe-mode='pipe' or ../ingress-pipe-mode='shortpipe')"/>
        <type name="l3vpn-color"/>
        <default value="green"/>
        <description>
          <text>Color.</text>
        </description>
      </leaf>
      <leaf name="egress-pipe-mode">
        <when condition="../split-mode='true'"/>
        <type name="l3vpn-egpipe-mode"/>
        <default value="uniform"/>
        <description>
          <text>Egress pipe mode.</text>
        </description>
      </leaf>
      <leaf name="domain-name">
        <when condition="(../pipe-mode='shortpipe'  or ../egress-pipe-mode='shortpipe')"/>
        <type name="leafref">
          <path value="/qos:qos/qos:classifier-template/qos:diffserv-domains/qos:diffserv-domain/qos:name"/>
        </type>
        <default value="default"/>
        <description>
          <text>DS domain name.</text>
        </description>
      </leaf>
    </container>
    <container name="vpn-ttlmode">
      <description>
        <text>Configure TTL mode.</text>
      </description>
      <leaf name="ttlmode">
        <type name="l3vpn-ttl-mode"/>
        <default value="pipe"/>
        <description>
          <text>TTL mode value.</text>
        </description>
      </leaf>
    </container>
  </augment>
  <augment target-node="/ifm:ifm/ifm:interfaces/ifm:interface">
    <description>
      <text>MPLS forward configuration to the ifm module.</text>
    </description>
    <container name="deephashcfg">
      <when condition="../ifm:type='Ethernet' or ../ifm:type='FlexE' or ../ifm:type='GigabitEthernet' or ../ifm:type='100GE' or ../ifm:type='10GE' or ../ifm:type='4x10GE' or ../ifm:type='10x10GE' or ../ifm:type='3x40GE' or ../ifm:type='4x25GE' or ../ifm:type='25GE' or ../ifm:type='50GE' or ../ifm:type='50|100GE' or ../ifm:type='XGigabitEthernet' or ../ifm:type='Pos' or ../ifm:type='40GE' or ../ifm:type='MEth' or ../ifm:type='400GE' or ../ifm:type='GEBrief' or ../ifm:type='200GE'"/>
      <description>
        <text>Configure MPLS deep hash mode.</text>
      </description>
      <leaf name="fragment-field">
        <type name="deep-hash-fragment"/>
        <default value="disable"/>
        <description>
          <text>MPLS deep hash fields.</text>
        </description>
      </leaf>
      <leaf name="deepmplshash">
        <type name="deep-hash-flag"/>
        <default value="l3"/>
        <description>
          <text>MPLS deep hash value.</text>
        </description>
      </leaf>
    </container>
    <container name="if-poison-reverse">
      <when condition="../ifm:type='Ethernet' or ../ifm:type='FlexE' or ../ifm:type='GigabitEthernet' or ../ifm:type='100GE' or ../ifm:type='10GE' or ../ifm:type='4x10GE' or ../ifm:type='10x10GE' or ../ifm:type='3x40GE' or ../ifm:type='4x25GE' or ../ifm:type='25GE' or ../ifm:type='XGigabitEthernet' or ../ifm:type='Eth-Trunk' or ../ifm:type='Ip-Trunk' or ../ifm:type='400GE' or ../ifm:type='Pos' or ../ifm:type='40GE' or ../ifm:type='MEth' or ../ifm:type='GEBrief' or ../ifm:type='200GE' or ../ifm:type='50|100GE' or ../ifm:type='50GE'"/>
      <presence value="Enable poison reverse on an interface"/>
      <description>
        <text>Configure MPLS poison reverse on the interface.</text>
      </description>
    </container>
    <container name="if-p2mp-load-balance">
      <when condition="../ifm:type='Eth-Trunk' or ../ifm:type='Ip-Trunk'"/>
      <description>
        <text>Configure Trunk-enable.</text>
      </description>
      <leaf name="loadbanlance-enable">
        <type name="enable-flag"/>
        <default value="disable"/>
        <description>
          <text>Trunk enable flag.</text>
        </description>
      </leaf>
      <leaf name="root-loadbanlance-enable">
        <type name="enable-flag"/>
        <default value="disable"/>
        <description>
          <text>Root node trunk enable flag.</text>
        </description>
      </leaf>
      <leaf name="root-loadbanlance-spmsidis">
        <when condition="../root-loadbanlance-enable='enable' or /mpls:mpls/mpls-forward:p2mp-forceload/mpls-forward:forceload-enable='enable'"/>
        <type name="enable-flag"/>
        <default value="disable"/>
        <description>
          <text>Root node S-PMSI trunk disable flag.</text>
        </description>
      </leaf>
    </container>
    <container name="bgp-localifnet-traffic">
      <when condition="../ifm:type='GigabitEthernet' or ../ifm:type='XGigabitEthernet' or ../ifm:type='10GE' or ../ifm:type='25GE' or ../ifm:type='40GE' or ../ifm:type='50GE' or ../ifm:type='100GE' or ../ifm:type='200GE' or ../ifm:type = 'FlexE' or ../ifm:type='400GE' or ../ifm:type='50|100GE' or ../ifm:type='Ethernet' or ../ifm:type='Eth-Trunk' or ../ifm:type='Ip-Trunk' or ../ifm:type='Pos' or ../ifm:type='4x10GE' or ../ifm:type='10x10GE' or ../ifm:type='3x40GE' or ../ifm:type='4x25GE' or ../ifm:type='Mp-group' or ../ ifm:type='Serial' or ../ifm:class='sub-interface'"/>
      <description>
        <text>Configure the HQoS function for the traffic carried by a BGP local IFNET tunnel on an interface.</text>
      </description>
      <leaf name="localifnet-enable">
        <type name="enable-flag"/>
        <default value="disable"/>
        <description>
          <text>Traffic enable flag.</text>
        </description>
      </leaf>
    </container>
  </augment>
  <augment target-node="/ifm:ifm/ifm:interfaces/ifm:interface/tnlm:tunnel-protocol/tnlm:type/mpls-te:mpls-te/mpls-te:te-tunnel">
    <description>
      <text>MPLS forward configuration to the MPLS TE tunnel module.</text>
    </description>
    <container name="mplstunnelpipe">
      <description>
        <text>Configure TE pipe mode.</text>
      </description>
      <leaf name="pipe-mode">
        <type name="te-pipe-mode"/>
        <default value="uniform"/>
        <description>
          <text>Specifies the pipe mode of TE tunnel.</text>
        </description>
      </leaf>
      <leaf name="service-class">
        <when condition="not(../pipe-mode='uniform')"/>
        <type name="te-pipe-service-class"/>
        <default value="be"/>
        <description>
          <text>PHB of ingress packet.</text>
        </description>
      </leaf>
      <leaf name="color">
        <when condition="not(../pipe-mode='uniform')"/>
        <type name="te-pipe-color"/>
        <default value="green"/>
        <description>
          <text>Color of ingress packet.</text>
        </description>
      </leaf>
    </container>
    <container name="mpls-tunnel-service-class">
      <description>
        <text>Configure specifies the service class of te tunnel.</text>
      </description>
      <leaf name="default-enable">
        <type name="boolean"/>
        <must condition="(../default-enable='false') or not(../af1-enable='true' or ../af2-enable='true' or ../af3-enable='true' or ../af4-enable='true' or ../be-enable='true' or ../cs6-enable='true' or ../cs7-enable='true' or ../ef-enable='true')"/>
        <default value="false"/>
        <description>
          <text>Enable/disable default service class.</text>
        </description>
      </leaf>
      <leaf name="be-enable">
        <type name="boolean"/>
        <must condition="../be-enable='false' or ../default-enable='false'"/>
        <default value="false"/>
        <description>
          <text>Enable/disable BE service class.</text>
        </description>
      </leaf>
      <leaf name="af1-enable">
        <type name="boolean"/>
        <must condition="../af1-enable='false' or ../default-enable='false'"/>
        <default value="false"/>
        <description>
          <text>Enable/disable AF1 service class.</text>
        </description>
      </leaf>
      <leaf name="af2-enable">
        <type name="boolean"/>
        <must condition="../af2-enable='false' or ../default-enable='false'"/>
        <default value="false"/>
        <description>
          <text>Enable/disable AF2 service class.</text>
        </description>
      </leaf>
      <leaf name="af3-enable">
        <type name="boolean"/>
        <must condition="../af3-enable='false' or ../default-enable='false'"/>
        <default value="false"/>
        <description>
          <text>Enable/disable AF3 service class.</text>
        </description>
      </leaf>
      <leaf name="af4-enable">
        <type name="boolean"/>
        <must condition="../af4-enable='false' or ../default-enable='false'"/>
        <default value="false"/>
        <description>
          <text>Enable/disable AF4 service class.</text>
        </description>
      </leaf>
      <leaf name="ef-enable">
        <type name="boolean"/>
        <must condition="../ef-enable='false' or ../default-enable='false'"/>
        <default value="false"/>
        <description>
          <text>Enable/disable EF service class.</text>
        </description>
      </leaf>
      <leaf name="cs6-enable">
        <type name="boolean"/>
        <must condition="../cs6-enable='false' or ../default-enable='false'"/>
        <default value="false"/>
        <description>
          <text>Enable/disable CS6 service class.</text>
        </description>
      </leaf>
      <leaf name="cs7-enable">
        <type name="boolean"/>
        <must condition="../cs7-enable='false' or ../default-enable='false'"/>
        <default value="false"/>
        <description>
          <text>Enable/disable CS7 service class.</text>
        </description>
      </leaf>
    </container>
    <container name="mpls-tunnel-dscp">
      <when condition="../mpls-te:common-attributes/mpls-te:signal-protocol='segment-routing'"/>
      <description>
        <text>Configure DSCP value.</text>
      </description>
      <leaf name="ipv4-defalut">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable default of IPv4.</text>
        </description>
      </leaf>
      <leaf name="ipv6-defalut">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable default of IPv6.</text>
        </description>
      </leaf>
      <leaf name="ipv4-dscp-value">
        <type name="string">
          <length value="1..512"/>
        </type>
        <must condition="../ipv4-dscp-value='*' or ../ipv4-defalut='false'"/>
        <default value="*"/>
        <description>
          <text>IPv4 DSCP, in the format of 0,1,2,3-10,63. A DSCP value is an integer ranging from 0 to 63.Entering an asterisk (*) deletes DSCP configuration.</text>
        </description>
      </leaf>
      <leaf name="ipv6-dscp-value">
        <type name="string">
          <length value="1..512"/>
        </type>
        <must condition="../ipv6-dscp-value='*' or ../ipv6-defalut='false'"/>
        <default value="*"/>
        <description>
          <text>IPv6 DSCP, in the format of 0,1,2,3-10,63. A DSCP value is an integer ranging from 0 to 63.Entering an asterisk (*) deletes DSCP configuration.</text>
        </description>
      </leaf>
    </container>
  </augment>
  <augment target-node="/mpls:mpls">
    <description>
      <text>MPLS forward configuration to the MPLS module.</text>
    </description>
    <container name="ds-enhance">
      <description>
        <text>Configure global shortpipe.</text>
      </description>
      <leaf name="enhance-enable">
        <type name="enable-flag"/>
        <default value="disable"/>
        <description>
          <text>Shortpipe global enhance enable.</text>
        </description>
      </leaf>
    </container>
    <container name="p2mp-fastswitch">
      <description>
        <text>Configure P2MP fast switch.</text>
      </description>
      <leaf name="fastswitch-enable">
        <type name="enable-flag"/>
        <default value="disable"/>
        <description>
          <text>P2MP global fast switch.</text>
        </description>
      </leaf>
    </container>
    <container name="p2mp-forceload">
      <description>
        <text>Configure P2MP forceload.</text>
      </description>
      <leaf name="forceload-enable">
        <type name="enable-flag"/>
        <default value="disable"/>
        <description>
          <text>P2MP force loadbalance.</text>
        </description>
      </leaf>
    </container>
    <container name="p2mp-frrwtrtime">
      <description>
        <text>Configure P2MP FRR WTR for forwarding plane.</text>
      </description>
      <leaf name="frrwtrtime-forpdt">
        <type name="uint32">
          <range value="5..600"/>
        </type>
        <units name="s"/>
        <default value="5"/>
        <description>
          <text>P2MP FRR WTR for forwarding plane.</text>
        </description>
      </leaf>
    </container>
    <container name="te-reverse-path-forwarding">
      <description>
        <text>Configure MPLS TE global RPF capability.</text>
      </description>
      <leaf name="te-rpf-enable">
        <type name="enable-flag"/>
        <default value="enable"/>
        <description>
          <text>MPLS TE RPF capability enable.</text>
        </description>
      </leaf>
    </container>
    <container name="bgp-separate">
      <description>
        <text>Configure MPLS multiple hop disjoint.</text>
      </description>
      <leaf name="bgpsepen">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable MPLS multi-tunnel encapsulation decoupling.</text>
        </description>
      </leaf>
    </container>
    <container name="ldp-separate">
      <description>
        <text>Configure MPLS global convergence enhancement.</text>
      </description>
      <leaf name="ldpsepen">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable LDP-SRBE convergence enhancement.</text>
        </description>
      </leaf>
    </container>
    <container name="te-multiprot">
      <description>
        <text>Configure MPLS TE global multi-protect fast switch.</text>
      </description>
      <leaf name="multi-protect-enable">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable MPLS TE multi-protect fast switch.</text>
        </description>
      </leaf>
    </container>
    <container name="active-tnl">
      <config value="false"/>
      <description>
        <text>Statistics of MPLS TE LSP state INFOs.</text>
      </description>
      <list name="tnlactinfo">
        <key value="interfacename"/>
        <config value="false"/>
        <description>
          <text>Statistics of MPLS TE LSP state INFOs.</text>
        </description>
        <leaf name="interfacename">
          <type name="pub-type:if-name"/>
          <config value="false"/>
          <description>
            <text>Specifies an MPLS TE interface name.</text>
          </description>
        </leaf>
        <leaf name="activepath">
          <type name="string">
            <length value="1..30"/>
          </type>
          <config value="false"/>
          <description>
            <text>MPLS TE LSP forward type.</text>
          </description>
        </leaf>
        <leaf name="tunnelid">
          <type name="pub-type:hex-binary"/>
          <config value="false"/>
          <description>
            <text>Tunnel ID.</text>
          </description>
        </leaf>
        <leaf name="outinterface">
          <type name="string">
            <length value="1..30"/>
          </type>
          <config value="false"/>
          <description>
            <text>The tunnel out interface.</text>
          </description>
        </leaf>
      </list>
    </container>
    <container name="disp-convergence-mode">
      <config value="false"/>
      <description>
        <text>Operational state of convergence mode state info.</text>
      </description>
      <leaf name="is-enable">
        <type name="boolean"/>
        <default value="false"/>
        <config value="false"/>
        <description>
          <text>Convergence mode enable check.</text>
        </description>
      </leaf>
    </container>
  </augment>
  <augment target-node="/mpls:mpls/mpls:common/mpls:global">
    <when condition="/mpls:mpls/mpls:common/mpls:global/mpls:mpls-enable='true'"/>
    <description>
      <text>MPLS forward configuration to the MPLS module.</text>
    </description>
    <container name="exp-ttl-mode">
      <description>
        <text>Configure pipe mode for explicit null label.</text>
      </description>
      <leaf name="exppipemode">
        <type name="enl-exp-mode"/>
        <default value="uniform"/>
        <description>
          <text>Pipe mode of EXP for explicit null label.</text>
        </description>
      </leaf>
      <leaf name="ttlpipemode">
        <type name="enl-ttl-mode"/>
        <default value="uniform"/>
        <description>
          <text>Pipe mode of TTL for explicit null label.</text>
        </description>
      </leaf>
    </container>
    <container name="entropy-label-mode">
      <description>
        <text>Configure pipe mode for entropy label.</text>
      </description>
      <leaf name="exp-pipemode">
        <type name="ela-exp-mode"/>
        <default value="uniform"/>
        <description>
          <text>Pipe mode of EXP for entropy label.</text>
        </description>
      </leaf>
      <leaf name="ttl-pipemode">
        <type name="ela-ttl-mode"/>
        <default value="uniform"/>
        <description>
          <text>Pipe mode of TTL for entropy label.</text>
        </description>
      </leaf>
    </container>
  </augment>
  <augment target-node="/mpls:mpls/mpls:common">
    <description>
      <text>MPLS forward configuration to the MPLS module.</text>
    </description>
    <container name="mpls-forward">
      <description>
        <text>Configure MPLS forward configuration to the MPLS module.</text>
      </description>
      <container name="static-label-only">
        <description>
          <text>Configure the parameters related to the exclusive static label range. The function takes effect after the device is restarted.</text>
        </description>
        <leaf name="max-label">
          <type name="uint32">
            <range value="2048..15999"/>
          </type>
          <description>
            <text>The static label in the exclusive range ranges from 16 to the configured value. The labels that exceed the range can be used by both static and dynamic services.</text>
          </description>
        </leaf>
      </container>
    </container>
  </augment>
  <ext:task-name value="forwarding"/>
</module>
