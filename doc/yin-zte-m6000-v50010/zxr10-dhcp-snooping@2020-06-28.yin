  <feature name="vxlan">
    <description>
      <text>Support VXLAN instance.</text>
    </description>
  </feature>
  <feature name="vpls">
    <description>
      <text>Support VPLS instance.</text>
    </description>
  </feature>
  <feature name="ip-global">
    <description>
      <text>Support ip-global instance.</text>
    </description>
  </feature>
  <feature name="vlan">
    <description>
      <text>Support VLAN instance.</text>
    </description>
  </feature>
  <feature name="database">
    <description>
      <text>Support to get database state.</text>
    </description>
  </feature>
  <feature name="user-configuration">
    <description>
      <text>Support user-configuration of option82</text>
    </description>
  </feature>
  <feature name="option82-information">
    <description>
      <text>Support relay agent information option.</text>
    </description>
  </feature>
  <feature name="option82-policy">
    <description>
      <text>Support Option82 policy.</text>
    </description>
  </feature>
  <feature name="evpn-vpls">
    <description>
      <text>Support EVPN-VPLS instance.</text>
    </description>
  </feature>
  <feature name="auto-discovered-vxlan-tunnel">
    <description>
      <text>Support to auto-discover vxlan-tunnels.</text>
    </description>
  </feature>
  <feature name="multi-ip-binding">
    <description>
      <text>Support multicast IP bindings.</text>
    </description>
  </feature>
  <typedef name="option-82">
    <type name="enumeration">
      <enum name="china-tel">
        <value value="1"/>
        <description>
          <text>China telcom</text>
        </description>
      </enum>
      <enum name="dsl-forum">
        <value value="2"/>
        <description>
          <text>DSL forum</text>
        </description>
      </enum>
      <enum name="telenor">
        <value value="3"/>
        <description>
          <text>Telenor</text>
        </description>
      </enum>
      <enum name="user-configuration">
        <value value="4"/>
        <description>
          <text>User configuration</text>
        </description>
      </enum>
      <enum name="cisco">
        <value value="5"/>
        <description>
          <text>Cisco</text>
        </description>
      </enum>
      <enum name="china-uni">
        <value value="6"/>
        <description>
          <text>China UNI</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Option82 format.1:china-tel,2:dsl-forum,3:telenor,4:user-configuration,5:cisco,6:china-uni.</text>
    </description>
  </typedef>
  <typedef name="option-policy">
    <type name="enumeration">
      <enum name="keep">
        <value value="2"/>
        <description>
          <text>keep original 82 option</text>
        </description>
      </enum>
      <enum name="replace">
        <value value="3"/>
        <description>
          <text>replace 82 option</text>
        </description>
      </enum>
      <enum name="stripping">
        <value value="4"/>
        <description>
          <text>strip off 82 option</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Option policy. 2:keep, 3:replace,4:stripping.</text>
    </description>
  </typedef>
  <typedef name="switch">
    <type name="enumeration">
      <enum name="off">
        <value value="0"/>
        <description>
          <text>log switch off</text>
        </description>
      </enum>
      <enum name="on">
        <value value="1"/>
        <description>
          <text>log switch on</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Log switch.0:off,1:on.</text>
    </description>
  </typedef>
  <typedef name="boot-type">
    <type name="enumeration">
      <enum name="none">
        <value value="0"/>
        <description>
          <text>no recover</text>
        </description>
      </enum>
      <enum name="localload">
        <value value="1"/>
        <description>
          <text>recover from local</text>
        </description>
      </enum>
      <enum name="download">
        <value value="2"/>
        <description>
          <text>recover from remote server</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Boot type.0:none,1:localload,2:download.</text>
    </description>
  </typedef>
  <typedef name="server-type">
    <type name="enumeration">
      <enum name="tftp">
        <value value="2"/>
        <description>
          <text>trivial file transfer protocal</text>
        </description>
      </enum>
      <enum name="ftp">
        <value value="3"/>
        <description>
          <text>file transfer protocal</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Server name.2:tftp,3:ftp.</text>
    </description>
  </typedef>
  <typedef name="database-type">
    <type name="enumeration">
      <enum name="dynamic">
        <value value="0"/>
      </enum>
      <enum name="static">
        <value value="1"/>
      </enum>
    </type>
    <description>
      <text>database client type.0:dynamic,1:static.</text>
    </description>
  </typedef>
  <typedef name="switch-type">
    <type name="enumeration">
      <enum name="disable">
        <value value="0"/>
      </enum>
      <enum name="enable">
        <value value="1"/>
      </enum>
    </type>
    <description>
      <text>Switch type.0:disable,1:enable.</text>
    </description>
  </typedef>
  <typedef name="auto-circuit-id-type">
    <type name="enumeration">
      <enum name="port">
        <value value="1"/>
        <description>
          <text>Generate port string automatically.</text>
        </description>
      </enum>
      <enum name="port-and-vlan">
        <value value="2"/>
        <description>
          <text>Generate port and VLAN string automatically.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Generate circuit ID automatically,1:port, 2:port and VLAN</text>
    </description>
  </typedef>
  <typedef name="user-configuration-policy">
    <type name="enumeration">
      <enum name="interface">
        <value value="1"/>
        <description>
          <text>Use user-configuration of the interface.</text>
        </description>
      </enum>
      <enum name="global">
        <value value="2"/>
        <description>
          <text>Use user-configuration of global.</text>
        </description>
      </enum>
      <enum name="vlan">
        <value value="3"/>
        <description>
          <text>Use user-configuration of the VLAN.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Policy of user configuration.1:interface; 2:global; 3:vlan.</text>
    </description>
  </typedef>
  <container name="configuration">
    <description>
      <text>Configuration model.</text>
    </description>
    <container name="dhcp-snooping">
      <description>
        <text>Configure dhcp snooping parameters.</text>
      </description>
      <container name="global">
        <description>
          <text>Dhcp snooping global configure.</text>
        </description>
        <leaf name="enable">
          <description>
            <text>Enable/disable DHCP snooping globally.</text>
          </description>
          <type name="switch-type"/>
          <default value="disable"/>
        </leaf>
        <leaf name="max-user">
          <description>
            <text>Configure max user number.</text>
          </description>
          <type name="uint32">
            <range value="1..256000"/>
          </type>
          <default value="2048"/>
        </leaf>
        <leaf name="ramble">
          <description>
            <text>Enable/disable DHCP snooping user ramble.</text>
          </description>
          <type name="switch-type"/>
          <default value="disable"/>
        </leaf>
        <leaf name="verify-mac">
          <description>
            <text>Enable DHCP snooping mac-verifying.</text>
          </description>
          <type name="switch-type"/>
          <default value="disable"/>
        </leaf>
        <container name="information">
          <description>
            <text>Configure relay agent information option.</text>
          </description>
          <leaf name="option">
            <description>
              <text>Insert relay information in BOOTREQUEST.</text>
            </description>
            <type name="switch-type"/>
            <default value="disable"/>
          </leaf>
          <leaf name="format">
            <description>
              <text>Relay information format style.</text>
            </description>
            <type name="option-82"/>
            <default value="china-tel"/>
          </leaf>
          <leaf name="policy">
            <description>
              <text>Define reforwarding policy.</text>
            </description>
            <type name="option-policy"/>
            <default value="keep"/>
          </leaf>
          <container name="user-configuration">
            <description>
              <text>Configure global relay agent information through user-configuration.</text>
            </description>
            <leaf name="remote-id">
              <description>
                <text>Configure remote ID.</text>
              </description>
              <type name="string"/>
            </leaf>
            <leaf name="policy">
              <description>
                <text>Configure user-configuration policy,base on interface,global or VLAN.</text>
              </description>
              <type name="user-configuration-policy"/>
              <default value="interface"/>
            </leaf>
            <container name="circuit-id-configuration">
              <description>
                <text>Configure circuit ID suboption.</text>
              </description>
              <choice name="circuit-id-suboption">
                <description>
                  <text>Configure circuit ID suboption.</text>
                </description>
                <case name="auto-circuit-id">
                  <description>
                    <text>Generate circuit-id automatically.</text>
                  </description>
                  <leaf name="auto-circuit-id">
                    <description>
                      <text>Generate circuit-id automatically.</text>
                    </description>
                    <type name="auto-circuit-id-type"/>
                  </leaf>
                </case>
                <case name="circuit-id">
                  <description>
                    <text>Configure circuit ID.</text>
                  </description>
                  <leaf name="circuit-id">
                    <description>
                      <text>Configure circuit ID.</text>
                    </description>
                    <type name="string"/>
                  </leaf>
                </case>
              </choice>
              <ncd:revise-date date="2017-12-26"/>
            </container>
            <ncd:revise-date date="2017-10-26"/>
          </container>
        </container>
        <leaf name="logging">
          <description>
            <text>Modify message logging facilities.</text>
          </description>
          <type name="switch"/>
          <default value="off"/>
        </leaf>
        <container name="file">
          <description>
            <text>Configure DHCP snooping file.</text>
          </description>
          <leaf name="timeout">
            <description>
              <text>The DHCP snooping database timeout.</text>
            </description>
            <type name="uint32">
              <range value="0..2147483647"/>
            </type>
            <units name="second"/>
            <default value="0"/>
          </leaf>
          <leaf name="upload-time">
            <description>
              <text>The DHCP snooping database upload time (minutes).</text>
            </description>
            <type name="uint32">
              <range value="0..2147483647"/>
            </type>
            <units name="minute"/>
            <default value="0"/>
          </leaf>
          <leaf name="localdir">
            <description>
              <text>The DHCP snooping database local directory.</text>
            </description>
            <type name="string">
              <length value="1..32"/>
            </type>
          </leaf>
          <container name="server">
            <description>
              <text>Configure DHCP Snooping database server address.</text>
            </description>
            <presence value="server"/>
            <leaf name="server-type">
              <description>
                <text>tftp or ftp.</text>
              </description>
              <type name="server-type"/>
              <mandatory value="true"/>
            </leaf>
            <leaf name="ipv6">
              <description>
                <text>Copy file using ipv6.</text>
              </description>
              <type name="uint32">
                <range value="0..1"/>
              </type>
              <default value="0"/>
            </leaf>
            <choice name="vrf">
              <description>
                <text>VPN routing/forwarding instance (VRF) name.</text>
              </description>
              <default value="mng"/>
              <case name="mng">
                <description>
                  <text>Management interface routes.</text>
                </description>
                <leaf name="mng">
                  <description>
                    <text>Management interface routes.</text>
                  </description>
                  <type name="uint32">
                    <range value="0..1"/>
                  </type>
                  <default value="0"/>
                </leaf>
              </case>
              <case name="vrf-name">
                <description>
                  <text>Vrf name.</text>
                </description>
                <leaf name="vrf-name">
                  <description>
                    <text>Vrf name.</text>
                  </description>
                  <type name="string">
                    <length value="1..35"/>
                  </type>
                </leaf>
              </case>
            </choice>
            <choice name="address">
              <description>
                <text>Server address.</text>
              </description>
              <mandatory value="true"/>
              <case name="url-type">
                <description>
                  <text>File server address.</text>
                </description>
                <leaf name="url">
                  <description>
                    <text>The DHCP snooping database server address://HOST/filepath[@username:password].</text>
                  </description>
                  <type name="string">
                    <length value="1..145"/>
                  </type>
                  <mandatory value="true"/>
                </leaf>
              </case>
              <case name="path-type">
                <description>
                  <text>File server address.</text>
                </description>
                <container name="path-account">
                  <description>
                    <text>Configure path and account.</text>
                  </description>
                  <leaf name="path">
                    <description>
                      <text>The DHCP snooping database server address://HOST/filepath.</text>
                    </description>
                    <type name="string">
                      <length value="1..80"/>
                    </type>
                    <mandatory value="true"/>
                  </leaf>
                  <container name="account">
                    <description>
                      <text>Account.</text>
                    </description>
                    <presence value="account"/>
                    <leaf name="user-name">
                      <description>
                        <text>User name.</text>
                      </description>
                      <type name="string">
                        <length value="1..65"/>
                      </type>
                      <mandatory value="true"/>
                    </leaf>
                    <choice name="password">
                      <description>
                        <text>Password.</text>
                      </description>
                      <mandatory value="true"/>
                      <case name="clear">
                        <description>
                          <text>Clear password.</text>
                        </description>
                        <leaf name="clear-password">
                          <description>
                            <text>Clear password.</text>
                          </description>
                          <type name="string">
                            <length value="1..64"/>
                          </type>
                          <mandatory value="true"/>
                        </leaf>
                      </case>
                      <case name="encrypted">
                        <description>
                          <text>Encrypted password.</text>
                        </description>
                        <leaf name="encrypted-password">
                          <description>
                            <text>Encrypted password.</text>
                          </description>
                          <type name="string">
                            <length value="64 | 128 | 148"/>
                          </type>
                          <mandatory value="true"/>
                        </leaf>
                      </case>
                    </choice>
                  </container>
                </container>
                <ncd:revise-date date="2019-05-07"/>
              </case>
            </choice>
            <leaf name="listen-port">
              <description>
                <text>Listen port.</text>
              </description>
              <type name="uint32">
                <range value="1..65535"/>
              </type>
            </leaf>
            <leaf name="if-name">
              <description>
                <text>Interface name.</text>
              </description>
              <type name="ncd:ifname"/>
            </leaf>
          </container>
          <container name="onboot">
            <description>
              <text>The DHCP snooping database recover after device booting.</text>
            </description>
            <presence value="onboot"/>
            <leaf name="onboot-type">
              <description>
                <text>Configure DHCP snooping on-boot recovery mode.</text>
              </description>
              <type name="boot-type"/>
              <mandatory value="true"/>
            </leaf>
            <leaf name="retry-times">
              <description>
                <text>Retry times of recovering database when failed.</text>
              </description>
              <type name="uint32">
                <range value="0..10"/>
              </type>
              <default value="0"/>
            </leaf>
            <leaf name="retry-interval">
              <description>
                <text>Retry interval of recovering database when failed.</text>
              </description>
              <type name="uint32">
                <range value="60..600"/>
              </type>
              <units name="second"/>
              <default value="60"/>
            </leaf>
          </container>
        </container>
      </container>
      <container name="vplses">
        <description>
          <text>DHCP snooping vpls configure.Can't config DHCP snooping in vpls if the vpls instance not exist.If DHCP snooping configuration exist in vpls,the vpls instance can't be deleted.</text>
        </description>
        <list name="vpls">
          <description>
            <text>The config of instance VPLS.</text>
          </description>
          <key value="vpls-name"/>
          <leaf name="vpls-name">
            <description>
              <text>Submode instance Name of VPLS.</text>
            </description>
            <type name="string">
              <length value="1..32"/>
            </type>
          </leaf>
          <leaf name="enable">
            <description>
              <text>Enable/disable DHCP snooping in this instance.</text>
            </description>
            <type name="switch-type"/>
            <default value="disable"/>
          </leaf>
          <container name="bindings">
            <description>
              <text>Add a configurable binding to the DHCP snooping database.</text>
            </description>
            <list name="binding">
              <description>
                <text>Binding item.</text>
              </description>
              <key value="mac-address vlanid"/>
              <leaf name="mac-address">
                <description>
