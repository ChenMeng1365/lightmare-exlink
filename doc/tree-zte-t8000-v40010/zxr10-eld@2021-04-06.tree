  |     |     |     +--:(protect) {protect-policy}?
  |     |     |        +--rw protect-policy?      empty
  |     |     |        +--rw protect-diag-time?   uint32
  |     |     +--rw manual-specify-port?   boolean {detect-interface}?
  |     |     +--rw detect-interfaces {detect-interface}?
  |     |        +--rw detect-interface* [if-name]
