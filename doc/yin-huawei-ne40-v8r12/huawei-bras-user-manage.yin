        <must condition="minor-trap-usernum &lt;= major-trap-usernum"/>
        <key value="interface-type"/>
        <description>
          <text>Configure minor and major alarm thresholds for the number of users configured on an interface during backup detections.</text>
        </description>
        <leaf name="interface-type">
          <type name="enumeration">
            <enum name="eth-trunk">
              <value value="1"/>
              <description>
                <text>Eth-Trunk interface type.</text>
              </description>
            </enum>
            <enum name="gigabit-ethernet">
              <value value="2"/>
              <description>
                <text>GigabitEthernet interface type.</text>
              </description>
            </enum>
            <enum name="gigabit-ethernet-10-ge">
              <value value="3"/>
              <description>
                <text>GigabitEthernet10GE interface type.</text>
              </description>
            </enum>
            <enum name="gigabit-ethernet-100-ge">
              <value value="4"/>
              <description>
                <text>GigabitEthernet100GE interface type.</text>
              </description>
            </enum>
            <enum name="gigabit-ethernet-25-ge">
              <value value="5"/>
              <description>
                <text>GigabitEthernet25GE interface type.</text>
              </description>
            </enum>
            <enum name="gigabit-ethernet-50-ge">
              <value value="6"/>
              <description>
                <text>GigabitEthernet50GE interface type.</text>
              </description>
            </enum>
            <enum name="gigabit-ethernet-50-or-100-ge">
              <value value="7"/>
              <description>
