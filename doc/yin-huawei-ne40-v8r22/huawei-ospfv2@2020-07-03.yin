  <ext:task-name value="ospf"/>
  <grouping name="policy-type">
    <description>
      <text>Type of a filtering rule. The available options are ACL Name or ACL Number or IP Prefix List or Route policy.</text>
    </description>
    <choice name="policy-type">
      <mandatory value="true"/>
      <description>
        <text>Type of a filtering rule.</text>
      </description>
      <case name="basic-acl">
        <description>
          <text>Apply basic ACL Name or ACL Number.</text>
        </description>
        <leaf name="name-or-num">
          <type name="leafref">
            <path value="/acl:acl/acl:groups/acl:group/acl:identity"/>
          </type>
          <must condition="/acl:acl/acl:groups/acl:group[acl:identity=current()]/acl:type='basic'"/>
          <description>
            <text>Apply basic ACL Name or ACL Number.</text>
          </description>
        </leaf>
      </case>
      <case name="ip-prefix">
        <description>
          <text>Set the name of an IP prefix list.</text>
        </description>
        <leaf name="ip-prefix">
          <type name="leafref">
            <path value="/rtp:routing-policy/rtp:ipv4-prefix-filters/rtp:ipv4-prefix-filter/rtp:name"/>
          </type>
          <description>
            <text>Set the name of an IP prefix list.</text>
          </description>
        </leaf>
      </case>
      <case name="route-policy">
        <description>
          <text>Set a route policy name.</text>
        </description>
        <leaf name="route-policy">
          <type name="leafref">
            <path value="/rtp:routing-policy/rtp:policy-definitions/rtp:policy-definition/rtp:name"/>
          </type>
          <description>
            <text>Set a route policy name.</text>
          </description>
        </leaf>
      </case>
      <case name="route-filter">
        <description>
          <text>Set a route filter name.</text>
        </description>
        <leaf name="route-filter">
          <type name="leafref">
            <path value="/xpl:xpl/xpl:route-filters/xpl:route-filter/xpl:name"/>
          </type>
          <description>
            <text>Set a route filter name.</text>
          </description>
        </leaf>
      </case>
    </choice>
  </grouping>
  <container name="ospfv2">
    <description>
      <text>Configure OSPFv2 config.</text>
    </description>
    <container name="check-rt-id">
      <description>
        <text>Configure automatic modification of conflicted router IDs. If enable, check and resolve OSPF router ID conflict.</text>
      </description>
      <leaf name="conflict-flag">
        <type name="boolean"/>
        <default value="true"/>
        <description>
          <text>Enable/disable automatic modification of conflicted router IDs. If enable, check and resolve OSPF router ID conflict.</text>
        </description>
      </leaf>
    </container>
    <container name="advertise-max-cost">
      <description>
        <text>Configure OSPF LSA advertise max cost globally.</text>
      </description>
      <leaf name="enable">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable OSPF LSAs to advertise max cost globally.</text>
        </description>
      </leaf>
    </container>
    <container name="session-car">
      <must condition="pir-interval&gt;=cir-interval"/>
      <must condition="pbs-interval&gt;=cbs-interval"/>
      <description>
        <text>Configure OSPF Session CAR.</text>
      </description>
      <leaf name="enable">
        <type name="boolean"/>
        <default value="true"/>
        <description>
          <text>Enable/disable OSPF Session CAR.</text>
        </description>
      </leaf>
      <leaf name="cir-interval">
        <type name="uint32">
          <range value="0..1000000"/>
        </type>
        <units name="kbit/s"/>
        <default value="8"/>
        <description>
          <text>Specify the Session CAR CIR interval.</text>
        </description>
      </leaf>
      <leaf name="cbs-interval">
        <type name="uint32">
          <range value="0..1000000"/>
        </type>
        <units name="Byte"/>
        <default value="100000"/>
        <description>
          <text>Specify the Session CAR CBS interval.</text>
        </description>
      </leaf>
      <leaf name="pir-interval">
        <type name="uint32">
          <range value="0..1000000"/>
        </type>
        <units name="kbit/s"/>
        <default value="16000"/>
        <description>
          <text>Specify the Session CAR PIR interval.</text>
        </description>
      </leaf>
      <leaf name="pbs-interval">
        <type name="uint32">
          <range value="0..10000000"/>
        </type>
        <units name="Byte"/>
        <default value="9000000"/>
        <description>
          <text>Specify the Session CAR PBS interval.</text>
        </description>
      </leaf>
    </container>
    <container name="micro-isolation-car">
      <must condition="pir&gt;=cir"/>
      <must condition="pbs&gt;=cbs"/>
      <description>
        <text>Configure OSPF Micro Isolation CAR.</text>
      </description>
      <leaf name="enable">
        <type name="boolean"/>
        <default value="true"/>
        <description>
          <text>Enable/disable OSPF Micro Isolation CAR.</text>
        </description>
      </leaf>
      <leaf name="cir">
        <type name="uint32">
          <range value="0..1000000"/>
        </type>
        <units name="kbit/s"/>
        <default value="8"/>
        <description>
          <text>Specify the Micro Isolation CAR CIR interval.</text>
        </description>
      </leaf>
      <leaf name="cbs">
        <type name="uint32">
          <range value="0..1000000"/>
        </type>
        <units name="Byte"/>
        <default value="100000"/>
        <description>
          <text>Specify the Micro Isolation CAR CBS interval.</text>
        </description>
      </leaf>
      <leaf name="pir">
        <type name="uint32">
          <range value="0..1000000"/>
        </type>
        <units name="kbit/s"/>
        <default value="16000"/>
        <description>
          <text>Specify the Micro Isolation CAR PIR interval.</text>
        </description>
      </leaf>
      <leaf name="pbs">
        <type name="uint32">
          <range value="0..10000000"/>
        </type>
        <units name="Byte"/>
        <default value="9000000"/>
        <description>
          <text>Specify the Micro Isolation CAR PBS interval.</text>
        </description>
      </leaf>
    </container>
    <container name="maxage-lsa-protect">
      <description>
        <text>Configure auto protect of purge LSA error for OSPF.</text>
      </description>
      <leaf name="disable">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable auto protect of purge LSA error for OSPF.</text>
        </description>
      </leaf>
    </container>
    <container name="suppress-flap-intf">
      <description>
        <text>Configure OSPF suppress flapping interface.</text>
      </description>
      <leaf name="disable">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable cancel the current setting or default setting.</text>
        </description>
      </leaf>
    </container>
    <container name="mib-binding">
      <description>
        <text>Configure MIB binding an OSPF process.</text>
      </description>
      <leaf name="process-id">
        <type name="leafref">
          <path value="/ni:network-instance/ni:instances/ni:instance/ospfv2:ospfv2/ospfv2:sites/ospfv2:site/ospfv2:process-id"/>
        </type>
        <description>
          <text>Process ID.</text>
        </description>
      </leaf>
    </container>
    <container name="flush-source-trace">
      <description>
        <text>Configure OSPF flush source trace globally.</text>
      </description>
      <leaf name="enable">
        <type name="boolean"/>
        <default value="true"/>
        <description>
          <text>Enable/disable OSPF flush source trace globally.</text>
        </description>
      </leaf>
      <leaf name="vlink-enable">
        <type name="boolean"/>
        <default value="true"/>
        <description>
          <text>Enable/disable OSPF vlink flush source trace globally.</text>
        </description>
      </leaf>
      <leaf name="port">
        <type name="uint16">
          <range value="1025..65535"/>
        </type>
        <default value="50122"/>
        <description>
          <text>Set OSPF flush source trace port allocated by huawei-pp4.The related CLI is 'display ip port info | inc UDP'.</text>
        </description>
      </leaf>
      <leaf name="vlink-port">
        <type name="uint16">
          <range value="1025..65535"/>
        </type>
        <default value="50123"/>
        <description>
          <text>Set OSPF vlink flush source trace port allocated by huawei-pp4.The related CLI is 'display ip port info | inc UDP'.</text>
        </description>
      </leaf>
    </container>
    <container name="system-control">
      <description>
        <text>Configure memory overload control and CPU overload control.</text>
      </description>
      <leaf name="cpu-overload-ctrl-enable">
        <type name="boolean"/>
        <default value="true"/>
        <description>
          <text>Enable/disable CPU overload control.</text>
        </description>
      </leaf>
      <leaf name="mem-overload-ctrl-enable">
        <type name="boolean"/>
        <default value="true"/>
        <description>
          <text>Enable/disable memory overload control.</text>
        </description>
      </leaf>
    </container>
    <container name="route-loop-detect">
      <description>
        <text>Configure OSPF import route loop detect.</text>
      </description>
      <leaf name="enable">
        <type name="boolean"/>
        <default value="true"/>
        <description>
          <text>Enable/disable OSPF import route loop detect.</text>
        </description>
      </leaf>
    </container>
    <container name="global-state">
      <config value="false"/>
      <description>
        <text>Statistics of OSPFv2 global status.</text>
      </description>
      <leaf name="dcn-link-cnt">
        <type name="uint32">
          <range value="1..4294967295"/>
        </type>
        <description>
          <text>Number of dcn links.</text>
        </description>
      </leaf>
    </container>
  </container>
  <rpc name="reset-redistribution">
    <description>
      <text>To re-import routes.</text>
    </description>
    <input>
      <leaf name="process-id">
        <type name="uint32">
          <range value="1..4294967295"/>
        </type>
        <description>
          <text>Process ID.</text>
        </description>
      </leaf>
    </input>
  </rpc>
  <rpc name="reset-counter">
    <description>
      <text>To clear the counter.</text>
    </description>
    <input>
      <leaf name="process-id">
        <type name="uint32">
          <range value="1..4294967295"/>
        </type>
        <description>
          <text>Process ID.</text>
        </description>
      </leaf>
      <leaf name="router-id">
        <type name="inet:ipv4-address-no-zone"/>
        <description>
          <text>Neighbor router ID.</text>
        </description>
      </leaf>
      <leaf name="if-name">
        <type name="leafref">
          <path value="/ifm:ifm/ifm:interfaces/ifm:interface/ifm:name"/>
        </type>
        <description>
          <text>Interface name.</text>
        </description>
      </leaf>
    </input>
  </rpc>
  <rpc name="reset-process">
    <description>
      <text>To restart an OSPF process.</text>
    </description>
    <input>
      <leaf name="process-id">
        <type name="uint32">
          <range value="1..4294967295"/>
        </type>
        <description>
          <text>Process ID.</text>
        </description>
      </leaf>
    </input>
  </rpc>
  <rpc name="reset-peer">
    <description>
      <text>To re-establish a neighbor.</text>
    </description>
    <input>
      <leaf name="process-id">
        <type name="uint32">
          <range value="1..4294967295"/>
        </type>
        <description>
          <text>Process ID.</text>
        </description>
      </leaf>
      <leaf name="router-id">
        <type name="inet:ipv4-address-no-zone"/>
        <description>
          <text>Neighbor router ID.</text>
        </description>
      </leaf>
      <leaf name="if-name">
        <type name="leafref">
          <path value="/ifm:ifm/ifm:interfaces/ifm:interface/ifm:name"/>
        </type>
        <description>
          <text>Interface name.</text>
        </description>
      </leaf>
    </input>
  </rpc>
  <rpc name="reset-spf">
    <description>
      <text>To restart SPF.</text>
    </description>
    <input>
      <leaf name="process-id">
        <type name="uint32">
          <range value="1..4294967295"/>
        </type>
        <description>
          <text>Process ID.</text>
        </description>
      </leaf>
    </input>
  </rpc>
  <rpc name="reset-frr">
    <description>
      <text>Restart FRR.</text>
    </description>
    <input>
      <leaf name="process-id">
        <type name="uint32">
          <range value="1..4294967295"/>
        </type>
        <description>
          <text>Process ID.</text>
        </description>
      </leaf>
    </input>
  </rpc>
  <rpc name="reset-suppress-peer">
    <description>
      <text>Restart OSPF peer flapping suppress.</text>
    </description>
    <input>
      <leaf name="process-id">
        <type name="uint32">
          <range value="1..4294967295"/>
        </type>
        <description>
          <text>Process ID.</text>
        </description>
      </leaf>
      <leaf name="if-name">
        <type name="leafref">
          <path value="/ifm:ifm/ifm:interfaces/ifm:interface/ifm:name"/>
        </type>
        <description>
          <text>Interface name.</text>
        </description>
      </leaf>
      <leaf name="notify-peer-flag">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable notification neighbor quit peer flapping suppress flag.</text>
        </description>
      </leaf>
    </input>
  </rpc>
  <rpc name="clear-loop-detect">
    <description>
      <text>Clear OSPF import route loop detect status and alarm.</text>
    </description>
  </rpc>
  <augment target-node="/ni:network-instance/ni:instances/ni:instance/l3vpn:afs/l3vpn:af">
    <description>
      <text>OSPF augment to L3VPN.</text>
    </description>
    <container name="ospf-import-rts">
      <description>
        <text>List of import route information from another routing protocol.</text>
      </description>
      <list name="ospf-import-rt">
        <key value="src-vrf process-id"/>
        <description>
          <text>Configure import route information from another routing protocol.</text>
        </description>
        <leaf name="src-vrf">
          <type name="leafref">
            <path value="/ni:network-instance/ni:instances/ni:instance/ni:name"/>
          </type>
          <must condition="../src-vrf!=../../../../../ni:name"/>
          <description>
            <text>VPN instance route information.</text>
          </description>
        </leaf>
        <leaf name="process-id">
          <type name="uint32">
            <range value="1..4294967295"/>
          </type>
          <description>
            <text>Process ID.</text>
          </description>
        </leaf>
        <leaf name="valid-route">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable the valid routes.</text>
          </description>
        </leaf>
        <choice name="policy-type">
          <description>
            <text>Type of import route rule.</text>
          </description>
          <case name="route-policy">
            <description>
              <text>Set a route policy name.</text>
            </description>
            <leaf name="route-policy">
              <type name="leafref">
                <path value="/rtp:routing-policy/rtp:policy-definitions/rtp:policy-definition/rtp:name"/>
              </type>
              <description>
                <text>Set a route policy name.</text>
              </description>
            </leaf>
          </case>
          <case name="route-filter">
            <description>
              <text>Set a route filter name.</text>
            </description>
            <leaf name="route-filter">
              <type name="leafref">
                <path value="/xpl:xpl/xpl:route-filters/xpl:route-filter/xpl:name"/>
              </type>
              <description>
                <text>Set a route filter name.</text>
              </description>
            </leaf>
          </case>
        </choice>
      </list>
    </container>
  </augment>
  <augment target-node="/ni:network-instance/ni:instances/ni:instance">
    <description>
      <text>OSPFv2 augment to L3VPN.</text>
    </description>
    <container name="ospfv2">
      <description>
        <text>Configure OSPFv2.</text>
      </description>
      <container name="gtsm">
        <when condition="../../l3vpn:afs/l3vpn:af[l3vpn:type='ipv4-unicast']"/>
        <presence value="Enable gtsm."/>
        <description>
          <text>Configure the OSPF GTSM.</text>
        </description>
        <leaf name="hops">
          <type name="uint8">
            <range value="1..255"/>
          </type>
          <mandatory value="true"/>
          <description>
            <text>Specifies the number of TTL hops to be detected.</text>
          </description>
        </leaf>
        <leaf name="nonstandard-multicast">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable sending Multicast packet with TTL as 255 and receive multicast packet TTL should be 1 or 255 to ((255 - configured hop) + 1).</text>
          </description>
        </leaf>
      </container>
      <container name="sites">
        <when condition="../../l3vpn:afs/l3vpn:af[l3vpn:type='ipv4-unicast']"/>
        <description>
          <text>List of OSPF instance.</text>
        </description>
        <list name="site">
          <key value="process-id"/>
          <description>
            <text>Configure OSPF instance.</text>
          </description>
          <leaf name="process-id">
            <type name="uint32">
              <range value="1..4294967295"/>
            </type>
            <description>
              <text>Process ID.</text>
            </description>
          </leaf>
          <leaf name="description">
            <type name="string">
              <length value="1..80"/>
              <pattern value="([^?]*)"/>
            </type>
            <description>
              <text>Description of an OSPF process.</text>
            </description>
          </leaf>
          <leaf name="router-id">
            <type name="inet:ipv4-address-no-zone"/>
            <description>
              <text>Router ID. It is used to set the OSPF private ID.</text>
            </description>
          </leaf>
          <leaf name="opaque">
            <type name="boolean"/>
            <default value="true"/>
            <description>
              <text>Enable/disable the opaque LSA capability. After it is enabled, an OSPF process can generate the opaque LSA and receive the opaque LSA from the neighbor device.</text>
            </description>
          </leaf>
          <leaf name="rfc1583-compatible">
            <type name="boolean"/>
            <default value="true"/>
            <description>
              <text>Enable/disable the route selection rule that is compatible with RFC 1583.</text>
            </description>
          </leaf>
          <leaf name="rfc1583-prefer">
            <type name="asbr-prefer-rule"/>
            <must condition="../rfc1583-prefer='none' or ../rfc1583-prefer='lower-cost' and ../rfc1583-compatible='true' or ../rfc1583-prefer='intra-area' and ../rfc1583-compatible='false'"/>
            <default value="none"/>
            <description>
              <text>ASBR source prefer rules for compatible with RFC 1583.</text>
            </description>
          </leaf>
          <leaf name="silent-all-interface">
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable to suppress all interfaces in an OSPF process from sending or receiving OSPF packets.</text>
            </description>
          </leaf>
          <leaf name="retransmission-limit">
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable maximum retransmission.</text>
            </description>
          </leaf>
          <leaf name="retransmission-limit-value">
            <when condition="../retransmission-limit='true'"/>
            <type name="uint32">
              <range value="2..255"/>
            </type>
            <default value="30"/>
            <description>
              <text>Maximum retransmission times.</text>
            </description>
          </leaf>
          <leaf name="safe-sync">
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable safe synchronization.</text>
            </description>
          </leaf>
          <leaf name="traffic-adj">
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable traffic adjust info.</text>
            </description>
          </leaf>
          <leaf name="traffic-adv">
            <when condition="../traffic-adj='true'"/>
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable traffic advertise info.</text>
            </description>
          </leaf>
          <leaf name="local-mt">
            <when condition="../../../../ni:name='_public_'"/>
            <type name="boolean"/>
            <must condition="../local-mt='false' or ../traffic-adj='true'"/>
            <default value="false"/>
            <description>
              <text>Enable/disable local multicast topology.</text>
            </description>
          </leaf>
          <leaf name="advertise-mpls-lsr-id">
            <type name="boolean"/>
            <must condition="../advertise-mpls-lsr-id='false' or ../../../../ni:name='_public_'"/>
            <default value="false"/>
            <description>
              <text>Enable/disable advertise MPLS LSR ID into multiple areas as an intra area route.</text>
            </description>
          </leaf>
          <leaf name="advertise-mpls-lsr-id-cost">
            <when condition="../advertise-mpls-lsr-id='true'"/>
            <type name="uint32">
              <range value="0..65535"/>
            </type>
            <default value="0"/>
            <description>
              <text>MPLS LSR ID link cost.</text>
            </description>
          </leaf>
          <leaf name="suppress-reachability">
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable suppress reachability.</text>
            </description>
          </leaf>
          <leaf name="lsa-refresh">
            <type name="boolean"/>
            <default value="true"/>
            <description>
              <text>Enable/disable refresh the age of LSA with greater lsa age.</text>
            </description>
          </leaf>
          <leaf name="mpls-ldp-auto-flag">
            <type name="proc-auto-cfg-state"/>
            <default value="none"/>
            <description>
              <text>Enable LDP automatically on OSPF enabled interfaces, but need to set MPLS first.</text>
            </description>
          </leaf>
          <leaf name="lsdb-overflow-limit">
            <type name="uint32">
              <range value="1..1000000"/>
            </type>
            <description>
              <text>Maximum number of AS external LSAs allowed in the LSDB.</text>
            </description>
          </leaf>
          <leaf name="peer-flapping-suppress">
            <type name="boolean"/>
            <default value="true"/>
            <description>
              <text>Enable/disable peer flapping suppress feature.</text>
            </description>
          </leaf>
          <leaf name="shutdown">
            <type name="boolean"/>
            <must condition="../shutdown='false' or ../../../../ni:name='__dcn_vpn__'"/>
            <default value="false"/>
            <description>
              <text>Enable/disable shutdown OSPFv2 service.</text>
            </description>
          </leaf>
          <leaf name="sham-hello">
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable peers keep alive by receiving any valid OSPF packet.</text>
            </description>
          </leaf>
          <leaf name="max-link-cost">
            <type name="uint16">
              <range value="1..65534"/>
            </type>
            <description>
              <text>Set the max value of link cost.</text>
            </description>
          </leaf>
          <leaf name="maxage-lsa">
            <type name="uint32">
              <range value="0..65535"/>
            </type>
            <units name="s"/>
            <default value="20"/>
            <description>
              <text>Calculation of route delay when the OSPF LSA frequently flaps.</text>
            </description>
          </leaf>
          <container name="private-net">
            <when condition="../../../../ni:name!='_public_'"/>
            <description>
              <text>Configure private network configuration of OSPF.</text>
            </description>
            <leaf name="eca-route">
              <type name="boolean"/>
              <default value="false"/>
              <description>
                <text>Enable/disable route type of the extended community attribute for VPN.</text>
              </description>
            </leaf>
            <choice name="route-tag">
              <description>
                <text>To set route tag value.</text>
              </description>
              <case name="disable">
                <description>
                  <text>To disable the using of the tag for loop detection.</text>
                </description>
                <leaf name="disable">
                  <type name="empty"/>
                  <description>
                    <text>To disable the using of the tag for loop detection.</text>
                  </description>
                </leaf>
              </case>
              <case name="value">
                <description>
                  <text>Tag of an imported VPN route.</text>
                </description>
                <leaf name="value">
                  <type name="uint32">
                    <range value="0..4294967295"/>
                  </type>
                  <description>
                    <text>Tag of an imported VPN route. By default, the first two bytes of the tag value are fixed to be 0xD000, and the last two bytes are the local AS number of the local BGP. For example, if the AS number of BGP is 100, the default tag value in decimal notation is 3489661028.If not configed, the default is 0.</text>
                  </description>
                </leaf>
              </case>
            </choice>
            <leaf name="vpn-capability">
              <type name="boolean"/>
              <default value="false"/>
              <description>
                <text>Enable/disable route loop detection and directly calculate routes. When VPN multi instance is supported on the multi VPN instance CE (MCE) router, loop detection must be cancelled. It is applicable to only OSPF VPN instances.</text>
              </description>
            </leaf>
            <container name="domain-id">
              <presence value="ID of an OSPF domain."/>
              <description>
                <text>Configure ID of an OSPF domain.</text>
              </description>
              <leaf name="domain-id">
                <type name="inet:ipv4-address-no-zone"/>
                <mandatory value="true"/>
                <description>
                  <text>ID of an OSPF domain.</text>
                </description>
              </leaf>
              <leaf name="type">
                <type name="domain-id-type"/>
                <default value="0x0005"/>
                <description>
                  <text>Type of an OSPF domain ID.</text>
                </description>
              </leaf>
              <leaf name="value">
                <type name="pub-type:hex-binary">
                  <length value="1..6"/>
                  <pattern value="((0x|0X)?[0-9a-fA-F]{0,4})"/>
                </type>
                <default value="0x0"/>
                <description>
                  <text>Value of an OSPF domain ID type.</text>
                </description>
              </leaf>
            </container>
            <container name="domain-id-secondarys">
              <when condition="../domain-id"/>
              <description>
                <text>List of ID of a secondary domain.</text>
              </description>
              <list name="domain-id-secondary">
                <key value="domain-id type value"/>
                <description>
                  <text>Configure ID of a secondary domain.</text>
                </description>
                <leaf name="domain-id">
                  <type name="inet:ipv4-address-no-zone"/>
                  <description>
                    <text>ID of an OSPF domain.</text>
                  </description>
                </leaf>
                <leaf name="type">
                  <type name="domain-id-type"/>
                  <description>
                    <text>Type of an OSPF domain ID.</text>
                  </description>
                </leaf>
                <leaf name="value">
                  <type name="pub-type:hex-binary">
                    <length value="1..6"/>
                    <pattern value="((0x|0X)?[0-9a-fA-F]{0,4})"/>
                  </type>
                  <description>
                    <text>Value of an OSPF domain ID type.</text>
                  </description>
                </leaf>
              </list>
            </container>
          </container>
          <container name="host-name">
            <description>
              <text>Configure OSPF host name.</text>
            </description>
            <leaf name="enable">
              <type name="boolean"/>
              <must condition="../enable='false' or ../../opaque='true'"/>
              <default value="false"/>
              <description>
                <text>Enable/disable host name.</text>
              </description>
            </leaf>
            <leaf name="value">
              <when condition="../enable='true'"/>
              <type name="string">
                <length value="1..255"/>
              </type>
              <description>
                <text>Set host name.</text>
              </description>
            </leaf>
          </container>
          <container name="bandwidth">
            <description>
              <text>Configure bandwidth.</text>
            </description>
            <leaf name="config-bw-enable">
              <type name="boolean"/>
              <default value="false"/>
              <description>
                <text>Enable/disable bandwidth config.</text>
              </description>
            </leaf>
            <leaf name="reference-value">
              <type name="uint32">
                <range value="1..2147483648"/>
              </type>
              <default value="100"/>
              <description>
                <text>Reference to the calculation of link costs.</text>
              </description>
            </leaf>
          </container>
          <container name="timer">
            <description>
              <text>Configure timer.</text>
            </description>
            <container name="lsa-arrival">
              <description>
                <text>Configure interval for receiving LSAs either through an intelligent timer or timer.</text>
              </description>
              <choice name="timer-type">
                <default value="intelligent-timer"/>
                <description>
                  <text>Set a value for the LDP and OSPF synchronization timer.</text>
                </description>
                <case name="timer">
                  <description>
                    <text>Interval for receiving LSAs.</text>
                  </description>
                  <leaf name="interval">
                    <type name="uint32">
                      <range value="0..10000"/>
                    </type>
                    <description>
                      <text>Interval for receiving LSAs.</text>
                    </description>
                  </leaf>
                </case>
                <case name="intelligent-timer">
                  <description>
                    <text>Specify an intelligent timer.</text>
                  </description>
                  <leaf name="max-interval">
                    <type name="uint32">
                      <range value="1..300000"/>
                    </type>
                    <must condition="../start-interval&lt;=../max-interval and ../hold-interval&lt;=../max-interval"/>
                    <default value="1000"/>
                    <description>
                      <text>Maximum interval for receiving OSPF LSAs.</text>
                    </description>
                  </leaf>
                  <leaf name="start-interval">
                    <type name="uint32">
                      <range value="0..60000"/>
                    </type>
                    <must condition="../start-interval&lt;=../max-interval"/>
                    <default value="500"/>
                    <description>
                      <text>Initial interval for receiving OSPF LSAs.</text>
                    </description>
                  </leaf>
                  <leaf name="hold-interval">
                    <type name="uint32">
                      <range value="1..60000"/>
                    </type>
                    <must condition="../hold-interval&lt;=../max-interval"/>
                    <default value="500"/>
                    <description>
                      <text>Hold interval for receiving OSPF LSAs.</text>
                    </description>
                  </leaf>
                </case>
              </choice>
            </container>
            <container name="lsa-arr-supp-flap">
              <description>
                <text>Configure the delay timer for LSA acceptance.</text>
              </description>
              <leaf name="interval">
                <type name="uint32">
                  <range value="0..600"/>
                </type>
                <units name="s"/>
                <default value="10"/>
                <description>
                  <text>Specifies an interval for suppressing LSA advertising in case of route flapping. The value 0 indicates that LSA advertising is not suppressed.</text>
                </description>
              </leaf>
              <leaf name="threshold-count">
                <type name="uint32">
                  <range value="3..100"/>
                </type>
                <default value="30"/>
                <description>
                  <text>Value of the threshold of LSAs to be suppressed.</text>
                </description>
              </leaf>
            </container>
            <container name="lsa-originate">
              <description>
                <text>Configure interval for receiving LSAs either through an intelligent timer or timer.</text>
              </description>
              <leaf name="other-type-interval">
                <type name="uint32">
                  <range value="0..10"/>
                </type>
                <default value="5"/>
                <description>
                  <text>Specify an updating/originating interval for type3, type4 and type10 LSAs. Interval for updating the LSAs except OSPF router LSAs and network LSAs.</text>
                </description>
              </leaf>
              <choice name="timer-type">
                <default value="intelligent-timer"/>
                <description>
                  <text>Specify an updating/originating interval for type3, type4 and type10 LSAs.</text>
                </description>
                <case name="intelligent-timer">
                  <description>
                    <text>Specify an intelligent timer for router LSA, network LSA, external LSA and NSSA LSA.</text>
                  </description>
                  <leaf name="max-interval">
                    <type name="uint32">
                      <range value="1..300000"/>
                    </type>
                    <must condition="../start-interval&lt;=../max-interval and ../hold-interval&lt;=../max-interval"/>
                    <default value="5000"/>
                    <description>
                      <text>Maximum interval for updating OSPF LSAs.</text>
                    </description>
                  </leaf>
                  <leaf name="start-interval">
                    <type name="uint32">
                      <range value="0..60000"/>
                    </type>
                    <must condition="../start-interval&lt;=../max-interval"/>
                    <default value="500"/>
                    <description>
                      <text>Initial interval for updating OSPF LSAs.</text>
                    </description>
                  </leaf>
                  <leaf name="hold-interval">
                    <type name="uint32">
                      <range value="1..60000"/>
                    </type>
                    <must condition="../hold-interval&lt;=../max-interval"/>
                    <default value="1000"/>
                    <description>
                      <text>Hold interval for updating OSPF LSAs.</text>
                    </description>
                  </leaf>
                </case>
              </choice>
            </container>
            <container name="lsa-org-supp-flap">
              <description>
                <text>Configure the delay timer for LSA generation.</text>
              </description>
              <leaf name="interval">
                <type name="uint32">
                  <range value="0..600"/>
                </type>
                <units name="s"/>
                <default value="10"/>
                <description>
                  <text>Specifies an interval for suppressing LSA advertising in case of route flapping. The value 0 indicates that LSA advertising is not suppressed.</text>
                </description>
              </leaf>
              <leaf name="threshold-count">
                <type name="uint32">
                  <range value="3..100"/>
                </type>
                <default value="30"/>
                <description>
                  <text>Value of the threshold of LSAs to be suppressed.</text>
                </description>
              </leaf>
            </container>
            <container name="spf-schedule">
              <description>
                <text>Configure the interval for SPF calculation.</text>
              </description>
              <choice name="schedule-type">
                <default value="intelligent-timer"/>
                <description>
                  <text>SPF schedule interval.</text>
                </description>
                <case name="timer">
                  <description>
                    <text>Specify an updating/originating interval for type3, type4 and type10 LSAs.</text>
                  </description>
                  <leaf name="interval">
                    <type name="uint32">
                      <range value="1..10"/>
                    </type>
                    <units name="s"/>
                    <description>
                      <text>Interval for OSPF SPF calculation. By adjusting the interval for SPF calculation, you can prevent the occupation of too many bandwidth and router resources caused by frequent network changes.</text>
                    </description>
                  </leaf>
                </case>
                <case name="intelligent-timer">
                  <description>
                    <text>Specify an intelligent timer.</text>
                  </description>
                  <leaf name="max-interval">
                    <type name="uint32">
                      <range value="1..300000"/>
                    </type>
                    <units name="ms"/>
                    <must condition="../start-interval&lt;=../max-interval and ../hold-interval&lt;=../max-interval"/>
                    <default value="5000"/>
                    <description>
                      <text>Maximum interval for OSPF SPF calculation set through an intelligent timer. By adjusting the interval for SPF calculation, you can prevent the occupation of too many bandwidths and device resources caused by frequent network changes.</text>
                    </description>
                  </leaf>
                  <leaf name="start-interval">
                    <type name="uint32">
                      <range value="1..60000"/>
                    </type>
                    <units name="ms"/>
                    <must condition="../start-interval&lt;=../max-interval"/>
                    <default value="50"/>
                    <description>
                      <text>Initial interval for OSPF SPF calculation set through an intelligent timer. By adjusting the interval for SPF calculation, you can prevent the occupation of too many bandwidth and router resources caused by frequent network changes.</text>
                    </description>
                  </leaf>
                  <leaf name="hold-interval">
                    <type name="uint32">
                      <range value="1..60000"/>
                    </type>
                    <units name="ms"/>
                    <must condition="../hold-interval&lt;=../max-interval"/>
                    <default value="200"/>
                    <description>
                      <text>Hold interval for OSPF SPF calculation set through an intelligent timer. By adjusting the interval for calculating SPF, you can prevent the occupation of too many bandwidth and router resources caused by frequent network changes.</text>
                    </description>
                  </leaf>
                  <leaf name="conserv-flag">
                    <type name="boolean"/>
                    <default value="false"/>
                    <description>
                      <text>Enable/disable conservative mode.</text>
                    </description>
                  </leaf>
                </case>
                <case name="millisecond">
                  <description>
                    <text>Specify an interval.</text>
                  </description>
                  <leaf name="millisecond">
                    <type name="uint32">
                      <range value="1..10000"/>
                    </type>
                    <units name="ms"/>
                    <description>
                      <text>Interval for OSPF SPF calculation. By adjusting the interval for SPF calculation, you can prevent the occupation of too many bandwidths and device resources caused by frequent network changes.</text>
                    </description>
                  </leaf>
                </case>
              </choice>
            </container>
          </container>
          <container name="bfd">
            <description>
              <text>Configure BFD in an OSPF process.</text>
            </description>
            <leaf name="enable">
              <type name="boolean"/>
              <default value="false"/>
              <description>
                <text>Enable/disable BFD in an OSPF process.</text>
              </description>
            </leaf>
            <leaf name="min-rx">
              <type name="uint32">
                <range value="3..20000"/>
              </type>
              <units name="ms"/>
              <description>
                <text>Minimum interval for receiving BFD packets from the peer end. The range and default are based on PAF.</text>
              </description>
            </leaf>
            <leaf name="min-tx">
              <type name="uint32">
                <range value="3..20000"/>
              </type>
              <units name="ms"/>
              <description>
                <text>Minimum interval for sending BFD packets to the peer end. The range and default are based on PAF.</text>
              </description>
            </leaf>
            <leaf name="detect-multiplier">
              <type name="uint32">
                <range value="3..50"/>
              </type>
              <default value="3"/>
              <description>
                <text>Local detection multiplier.</text>
              </description>
            </leaf>
            <leaf name="incr-cost">
              <type name="uint32">
                <range value="1..65535"/>
              </type>
              <description>
                <text>BFD sync cost.</text>
              </description>
            </leaf>
            <leaf name="frr-binding">
              <type name="boolean"/>
              <default value="false"/>
              <description>
                <text>Enable/disable bind FRR with BFD.</text>
              </description>
            </leaf>
          </container>
          <container name="metric-delay">
            <when condition="../opaque='true'"/>
            <description>
              <text>Configure metric delay in an OSPF process.</text>
            </description>
            <leaf name="adv-enable">
              <type name="boolean"/>
              <default value="false"/>
              <description>
                <text>Enable/disable metric delay advertisement.</text>
              </description>
            </leaf>
            <leaf name="suppress-timer">
              <type name="uint32">
                <range value="0..600"/>
              </type>
              <units name="s"/>
              <default value="120"/>
              <description>
                <text>Metric delay suppress timer.</text>
              </description>
            </leaf>
            <leaf name="suppress-percent">
              <type name="uint8">
                <range value="0..100"/>
              </type>
              <default value="10"/>
              <description>
                <text>Metric delay suppress percent threshold.</text>
              </description>
            </leaf>
            <leaf name="suppress-absolute">
              <type name="uint32">
                <range value="0..10000"/>
              </type>
              <default value="1000"/>
              <description>
                <text>Metric delay suppress absolute threshold.</text>
              </description>
            </leaf>
          </container>
          <container name="bier">
            <when condition="../opaque='true' and ../../../../ni:name='_public_'"/>
            <description>
              <text>Configure the bier.</text>
            </description>
            <leaf name="enable">
              <type name="boolean"/>
              <default value="false"/>
              <description>
                <text>Enable/disable the bier.</text>
              </description>
            </leaf>
          </container>
          <container name="stub-router">
            <description>
              <text>Configure stub router is used to configure the router as stub router.</text>
            </description>
            <leaf name="flag">
              <type name="stub-router-flag"/>
              <default value="no-config"/>
              <description>
                <text>Stub router. The router configured with the stub router command notifies other OSPF routers of not using the stub router to forward data by increasing the metric greater than 65535 of the links in the LSA that is generated by the router.</text>
              </description>
            </leaf>
            <leaf name="interval">
              <when condition="../flag='stub-on-boot'"/>
              <type name="uint32">
                <range value="5..65535"/>
              </type>
              <default value="500"/>
              <description>
                <text>Interval for remaining as the stub router during the active/standby switchover of a router.</text>
              </description>
            </leaf>
            <leaf name="include-stub">
              <when condition="../flag!='no-config'"/>
              <type name="boolean"/>
              <default value="false"/>
              <description>
                <text>Enable/disable set maximum cost for router lsa stub link.</text>
              </description>
            </leaf>
            <leaf name="external-lsa">
              <when condition="../flag!='no-config'"/>
              <type name="boolean"/>
              <default value="false"/>
              <description>
                <text>Enable/disable set maximum cost for external lsa.</text>
              </description>
            </leaf>
            <leaf name="external-cost">
              <when condition="../external-lsa='true'"/>
              <type name="uint32">
                <range value="1..16777215"/>
              </type>
              <default value="16711680"/>
              <description>
                <text>Advertise external LSAs with configured/default overriding metric.</text>
              </description>
            </leaf>
            <leaf name="summary-lsa">
              <when condition="../flag!='no-config'"/>
              <type name="boolean"/>
              <default value="false"/>
              <description>
                <text>Enable/disable set maximum cost for summary lsa.</text>
              </description>
            </leaf>
            <leaf name="summary-cost">
              <when condition="../summary-lsa='true'"/>
              <type name="uint32">
                <range value="1..16777215"/>
              </type>
              <default value="16711680"/>
              <description>
                <text>Advertise summary LSAs with configured/default overriding metric.</text>
              </description>
            </leaf>
          </container>
          <container name="bgp-ls">
            <description>
              <text>Configure BGP LS.</text>
            </description>
            <leaf name="enable">
              <type name="boolean"/>
              <must condition="../enable='false' or ../../../../../ni:name='_public_'"/>
              <default value="false"/>
              <description>
                <text>Enable/disable BGP LS.</text>
              </description>
            </leaf>
            <leaf name="identifier">
              <when condition="../enable='true'"/>
              <type name="uint32">
                <range value="2..65535"/>
              </type>
              <description>
                <text>Topo identifier.</text>
              </description>
            </leaf>
          </container>
          <container name="import-limit">
            <presence value="Enable import-limit"/>
            <description>
              <text>Configure limits the maximum number of routes to be imported.</text>
            </description>
            <leaf name="max-number">
              <type name="uint32">
                <range value="1..4294967295"/>
              </type>
              <mandatory value="true"/>
              <description>
                <text>Maximum number of external routes that can be imported.</text>
              </description>
            </leaf>
            <leaf name="threshold-upper">
              <type name="uint32">
                <range value="1..100"/>
              </type>
              <must condition="../threshold-upper&gt;=../threshold-lower"/>
              <default value="80"/>
              <description>
                <text>Upper threshold of the maximum number of routes that can be imported.</text>
              </description>
            </leaf>
            <leaf name="threshold-lower">
              <type name="uint32">
                <range value="1..100"/>
              </type>
              <must condition="../threshold-upper&gt;=../threshold-lower"/>
              <default value="70"/>
              <description>
                <text>Lower threshold of the maximum number of routes that can be imported.</text>
              </description>
            </leaf>
          </container>
          <container name="spf-control">
            <description>
              <text>Configure SPF calculation control.</text>
            </description>
            <leaf name="max-load-balancing">
              <type name="uint32">
                <range value="1..128"/>
              </type>
              <ext:dynamic-default/>
              <description>
                <text>Maximum number of the equal cost routes of the destination address in the routing table. The range and default are based on PAF.</text>
              </description>
            </leaf>
          </container>
          <container name="default-para">
            <description>
              <text>Configure default parameter.</text>
            </description>
            <choice name="cost">
              <default value="cost-value"/>
              <description>
                <text>Cost assigned to the ASE or NSSA LSA generated when an external route is imported.</text>
              </description>
              <case name="cost-value">
                <description>
                  <text>Default metric of the external routes imported by OSPF.</text>
                </description>
                <leaf name="cost-value">
                  <type name="uint32">
                    <range value="0..16777214"/>
                  </type>
                  <default value="1"/>
                  <description>
                    <text>Default metric of the external routes imported by OSPF.</text>
                  </description>
                </leaf>
              </case>
              <case name="cost-inherit-metric">
                <description>
                  <text>Cost of the imported route is the cost carried in the route. If the cost is not specified, the default cost can be set through the default command.</text>
                </description>
                <leaf name="cost-inherit-metric">
                  <type name="empty"/>
                  <description>
                    <text>Cost of the imported route is the cost carried in the route. If the cost is not specified, the default cost can be set through the default command.</text>
                  </description>
                </leaf>
              </case>
            </choice>
            <leaf name="tag">
              <type name="uint32">
                <range value="0..4294967295"/>
              </type>
              <default value="1"/>
              <description>
                <text>Tag of an external route.</text>
              </description>
            </leaf>
            <leaf name="type">
              <type name="external-route-type"/>
              <default value="type2"/>
              <description>
                <text>Type of an external route.</text>
              </description>
            </leaf>
          </container>
          <container name="avoid-micro-loop">
            <description>
              <text>Configure OSPF avoid microloop.</text>
            </description>
            <leaf name="frr-enable">
              <type name="boolean"/>
              <default value="false"/>
              <description>
                <text>Enable/disable avoid microloop.</text>
              </description>
            </leaf>
            <leaf name="frr-rib-update-delay">
              <type name="uint32">
                <range value="100..5000"/>
              </type>
              <units name="ms"/>
              <default value="100"/>
              <description>
                <text>Set the maximum interval of delay timer.</text>
              </description>
            </leaf>
            <leaf name="te-tunnel-enable">
              <type name="boolean"/>
              <default value="false"/>
              <description>
                <text>Enable/disable TE shortcut tunnel avoid microloop.</text>
              </description>
            </leaf>
            <leaf name="te-tunnel-rib-update-delay">
              <type name="uint32">
                <range value="100..30000"/>
              </type>
              <units name="ms"/>
              <default value="1000"/>
              <description>
                <text>Set the route delivery delay for TE shortcut tunnel.</text>
              </description>
            </leaf>
          </container>
          <container name="asbr-summary-prefixs">
            <description>
              <text>List of aggregate addresses for OSPF (only for ASBR).</text>
            </description>
            <list name="asbr-summary-prefix">
              <key value="prefix network-mask"/>
              <description>
                <text>Configure aggregate addresses for OSPF (only for ASBR).</text>
              </description>
              <leaf name="prefix">
                <type name="inet:ipv4-address-no-zone"/>
                <description>
                  <text>IP address. Prefix is the address after mask.</text>
                </description>
              </leaf>
              <leaf name="network-mask">
                <type name="inet:ipv4-address-no-zone"/>
                <description>
                  <text>IP address mask.</text>
                </description>
              </leaf>
              <leaf name="cost">
                <type name="uint32">
                  <range value="0..16777214"/>
                </type>
                <description>
                  <text>Specifies the cost of the aggregated route.</text>
                </description>
              </leaf>
              <leaf name="distribute-delay">
                <type name="uint32">
                  <range value="1..65535"/>
                </type>
                <units name="s"/>
                <description>
                  <text>Distribute delay interval.</text>
                </description>
              </leaf>
              <leaf name="tag">
                <type name="uint32">
                  <range value="0..4294967295"/>
                </type>
                <description>
                  <text>Specifies that the tag is mainly used to control advertisement of routes through the routing policy.</text>
                </description>
              </leaf>
              <leaf name="advertise">
                <type name="boolean"/>
                <default value="true"/>
                <description>
                  <text>Enable/disable the aggregated route is advertised.</text>
                </description>
              </leaf>
              <leaf name="generate-null-zero">
                <when condition="../advertise = 'true'"/>
                <type name="boolean"/>
                <default value="false"/>
                <description>
                  <text>Enable/disable generate black hole route.</text>
                </description>
              </leaf>
            </list>
          </container>
          <choice name="default-route">
            <description>
              <text>Notification of default routes to the OSPF routing area.</text>
            </description>
            <case name="summary">
              <description>
                <text>Summary.</text>
              </description>
              <container name="summary">
                <when condition="../../../../ni:name!='_public_'"/>
                <presence value="Enable default-route-advertise summary."/>
                <description>
                  <text>Configure advertises the type3 summary LSAs of the specified default route.</text>
                </description>
                <leaf name="cost">
                  <type name="uint32">
                    <range value="0..16777214"/>
                  </type>
                  <mandatory value="true"/>
                  <description>
                    <text>Metric of an ASE LSA.</text>
                  </description>
                </leaf>
              </container>
            </case>
            <case name="no-summary">
              <description>
                <text>No summary.</text>
              </description>
              <container name="no-summary">
                <presence value="Enable default-route-advertise no-summary."/>
                <description>
                  <text>Configure default routes to the OSPF routing area.</text>
                </description>
                <leaf name="enable">
                  <type name="empty"/>
                  <status value="deprecated"/>
                  <description>
                    <text>Default Route Enable. The node enable is deprecated. You are advised to use the node no-summary.</text>
                  </description>
                </leaf>
                <leaf name="cost">
                  <type name="uint32">
                    <range value="0..16777214"/>
                  </type>
                  <description>
                    <text>Metric of an ASE LSA.</text>
                  </description>
                </leaf>
                <leaf name="tag">
                  <type name="uint32">
                    <range value="0..4294967295"/>
                  </type>
                  <description>
                    <text>Route tag for default imported routes.</text>
                  </description>
                </leaf>
                <leaf name="type">
                  <type name="external-route-type"/>
                  <description>
                    <text>Metric type of an ASE LSA for the default routes.</text>
                  </description>
                </leaf>
                <leaf name="distribute-delay">
                  <type name="uint32">
                    <range value="1..65535"/>
                  </type>
                  <description>
                    <text>Distribute delay timer used for the local router to calculate the default routes advertised by other routers.</text>
                  </description>
                </leaf>
                <choice name="mode">
                  <description>
                    <text>Mode for default route.</text>
                  </description>
                  <case name="permit-calculate-other">
                    <description>
                      <text>Set permit calculate other.</text>
                    </description>
                    <leaf name="permit-calculate-other">
                      <type name="empty"/>
                      <description>
                        <text>To generate and advertise an ASE LSA that describes the default route only when there are valid default routes of other protocols in the routing table of the host. The router configured with permit calculate other still calculates the default routes of other routers.</text>
                      </description>
                    </leaf>
                    <leaf name="permit-ibgp">
                      <type name="empty"/>
                      <description>
                        <text>Match IBGP default route.</text>
                      </description>
                    </leaf>
                  </case>
                  <case name="always">
                    <description>
                      <text>Set always.</text>
                    </description>
                    <leaf name="always">
                      <type name="empty"/>
                      <description>
                        <text>The router configured with the always parameter does not calculate the default routes of other routers.</text>
                      </description>
                    </leaf>
                  </case>
                </choice>
                <choice name="policy-type">
                  <description>
                    <text>Mode of the policy used for default route.</text>
                  </description>
                  <case name="route-policy-name">
                    <description>
                      <text>Set a route policy name.</text>
                    </description>
                    <leaf name="route-policy-name">
                      <type name="leafref">
                        <path value="/rtp:routing-policy/rtp:policy-definitions/rtp:policy-definition/rtp:name"/>
                      </type>
                      <description>
                        <text>Name of a routing policy. Through the routing policy, the device matches the default routing entry generated by the device that does not run OSPF in the routing table, and advertises the default route according to the parameters set through the routing policy.</text>
                      </description>
                    </leaf>
                  </case>
                  <case name="route-filter-name">
                    <description>
                      <text>Set a route filter name.</text>
                    </description>
                    <leaf name="route-filter-name">
                      <type name="leafref">
                        <path value="/xpl:xpl/xpl:route-filters/xpl:route-filter/xpl:name"/>
                      </type>
                      <description>
                        <text>Name of a routing Filter.</text>
                      </description>
                    </leaf>
                  </case>
                </choice>
              </container>
            </case>
          </choice>
          <container name="route-preferences">
            <description>
              <text>List of preference of an OSPF route.</text>
            </description>
            <list name="route-preference">
              <key value="route-type"/>
              <max-elements value="4"/>
              <ext:generated-by value="system">
                <ext:filter value="route-type='default' or route-type='ase'"/>
              </ext:generated-by>
              <description>
                <text>Configure preference of an OSPF route.</text>
              </description>
              <leaf name="route-type">
                <type name="ospf-route-type"/>
                <description>
                  <text>Router type.</text>
                </description>
              </leaf>
              <leaf name="preference">
                <type name="uint32">
                  <range value="1..255"/>
                </type>
                <ext:dynamic-default>
                  <ext:default-value value="10">
                    <when condition="../route-type != 'ase'"/>
                    <description>
                      <text>The default value is 10 when route type is not ase.</text>
                    </description>
                  </ext:default-value>
                  <ext:default-value value="150">
                    <when condition="../route-type = 'ase'"/>
                    <description>
                      <text>The default value is 150 when route type is ase.</text>
                    </description>
                  </ext:default-value>
                </ext:dynamic-default>
                <description>
                  <text>Preference of an OSPF route. The default of intra and inter area is 10.And The default of external is 150.</text>
                </description>
              </leaf>
              <choice name="policy-type">
                <description>
                  <text>Type of a filtering rule.</text>
                </description>
                <case name="route-policy-name">
                  <description>
                    <text>Name of a routing policy.</text>
                  </description>
                  <leaf name="route-policy-name">
                    <type name="leafref">
                      <path value="/rtp:routing-policy/rtp:policy-definitions/rtp:policy-definition/rtp:name"/>
                    </type>
                    <description>
                      <text>Name of a routing policy.</text>
                    </description>
                  </leaf>
                </case>
                <case name="route-filter-name">
                  <description>
                    <text>Name of a routing Filter.</text>
                  </description>
                  <leaf name="route-filter-name">
                    <type name="leafref">
                      <path value="/xpl:xpl/xpl:route-filters/xpl:route-filter/xpl:name"/>
                    </type>
                    <description>
                      <text>Name of a routing Filter.</text>
                    </description>
                  </leaf>
                </case>
              </choice>
            </list>
          </container>
          <container name="nexthop-weights">
            <description>
              <text>List of preference of an equal cost route.</text>
            </description>
            <list name="nexthop-weight">
              <key value="ip-address"/>
              <description>
                <text>Configure preference of an equal cost route.</text>
              </description>
              <leaf name="ip-address">
                <type name="inet:ipv4-address-no-zone"/>
                <description>
                  <text>IP address of the next hop.</text>
                </description>
              </leaf>
              <leaf name="weight">
                <type name="uint32">
                  <range value="1..254"/>
                </type>
                <mandatory value="true"/>
                <description>
                  <text>Weight of the next hop. After OSPF calculates the equal cost routes, the next hop is chosen from these equal cost routes based on the value of weight. The smaller the value is, the higher the preference is. By default, the weight is 255. This indicates that load balancing is carried out among equal cost routes and packets are forwarded at the same time without the preference being set.</text>
                </description>
              </leaf>
            </list>
          </container>
          <container name="prefix-prioritys">
            <description>
              <text>List of priority of OSPF route.</text>
            </description>
            <list name="prefix-priority">
              <key value="priority"/>
              <description>
                <text>Configure priority of OSPF route.</text>
              </description>
              <leaf name="priority">
                <type name="prefix-priority"/>
                <description>
                  <text>Convergence priority of OSPF routes.</text>
                </description>
              </leaf>
              <leaf name="ip-prefix">
                <type name="leafref">
                  <path value="/rtp:routing-policy/rtp:ipv4-prefix-filters/rtp:ipv4-prefix-filter/rtp:name"/>
                </type>
                <mandatory value="true"/>
                <description>
                  <text>Name of the IP prefix list. The value is a string of 1 to 169 characters without spaces. The prefix priority command sets the convergence priority of OSPF routes according to the specified IP prefix list name, and takes effect on the public network only.
By default, the convergence priority of public OSPF host routes is medium, the convergence priority of directly connected routes is high, the convergence priority of static routes is medium, and the convergence priority of the routes of other protocols (such as BGP and RIP) is low. On the private network, OSPF 32 bit host routes are globally identified as medium.</text>
                </description>
              </leaf>
            </list>
          </container>
          <container name="ecmp-prefer">
            <description>
              <text>Configure equal cost multipath preference of OSPF routes.</text>
            </description>
            <leaf name="te-tunnel">
              <type name="boolean"/>
              <must condition="../te-tunnel='false' or ../intact='false'"/>
              <default value="false"/>
              <description>
                <text>Enable/disable TE tunnel interface nexthops.</text>
              </description>
            </leaf>
            <leaf name="intact">
              <type name="boolean"/>
              <must condition="../intact='false' or ../te-tunnel='false'"/>
              <default value="false"/>
              <description>
                <text>Enable/disable IGP native nexthop.</text>
              </description>
            </leaf>
          </container>
          <container name="frr">
            <description>
              <text>Configure OSPF fast rerouting.</text>
            </description>
            <leaf name="enable">
              <type name="boolean"/>
              <default value="false"/>
              <description>
                <text>Enable/disable FRR for this particular process.</text>
              </description>
            </leaf>
            <leaf name="lfa-enable">
              <type name="boolean"/>
              <must condition="../lfa-enable='false' or ../enable='true'"/>
              <default value="false"/>
              <description>
                <text>Enable/disable loop free alternate.</text>
              </description>
            </leaf>
            <leaf name="rlfa-enable">
              <type name="boolean"/>
              <must condition="../rlfa-enable='false' or ../lfa-enable='true' and ../../../../../ni:name='_public_'"/>
              <default value="false"/>
              <description>
                <text>Enable/disable remote lfa.</text>
              </description>
            </leaf>
            <leaf name="rlfa-max-cost">
              <when condition="../rlfa-enable='true'"/>
              <type name="uint32">
                <range value="1..4294967295"/>
              </type>
              <default value="4294967295"/>
              <description>
                <text>Remote lfa tunnel ldp maximum-reachable-cost.</text>
              </description>
            </leaf>
            <leaf name="tilfa-enable">
              <type name="boolean"/>
              <must condition="../tilfa-enable='false' or ../lfa-enable='true' and ../../../../../ni:name='_public_'"/>
              <default value="false"/>
              <description>
                <text>Enable/disable TILFA.</text>
              </description>
            </leaf>
            <container name="tiebreaker">
              <when condition="../lfa-enable='true'"/>
              <must condition="node-protect!=lowest-cost"/>
              <must condition="node-protect!=ldp-sync"/>
              <must condition="ldp-sync!=lowest-cost"/>
              <must condition="node-protect!=srlg-disjoint"/>
              <must condition="lowest-cost!=srlg-disjoint"/>
              <must condition="ldp-sync!=srlg-disjoint"/>
              <description>
                <text>Configure tiebreaker for multiple backups.</text>
              </description>
              <leaf name="node-protect">
                <type name="uint8">
                  <range value="1..255"/>
                </type>
                <default value="40"/>
                <description>
                  <text>Set the value of node protect preference.</text>
                </description>
              </leaf>
              <leaf name="lowest-cost">
                <type name="uint8">
                  <range value="1..255"/>
                </type>
                <default value="20"/>
                <description>
                  <text>Set the value of lowcost preference.</text>
                </description>
              </leaf>
              <leaf name="ldp-sync">
                <type name="uint8">
                  <range value="1..255"/>
                </type>
                <default value="10"/>
                <description>
                  <text>Prefer backup nexthop with ldp sync.</text>
                </description>
              </leaf>
              <leaf name="srlg-disjoint">
                <type name="uint8">
                  <range value="1..255"/>
                </type>
                <default value="5"/>
                <description>
                  <text>Set the value of srlg-disjoint preference.</text>
                </description>
              </leaf>
            </container>
            <container name="route-policy">
              <when condition="../enable='true'"/>
              <description>
                <text>Configure FRR route policy.</text>
              </description>
              <choice name="policy-type">
                <description>
                  <text>Type of a filtering rule.</text>
                </description>
                <case name="route-policy-name">
                  <description>
                    <text>Name of a routing policy.</text>
                  </description>
                  <leaf name="route-policy-name">
                    <type name="leafref">
                      <path value="/rtp:routing-policy/rtp:policy-definitions/rtp:policy-definition/rtp:name"/>
                    </type>
                    <description>
                      <text>Name of a routing policy.</text>
                    </description>
                  </leaf>
                </case>
                <case name="route-filter-name">
                  <description>
                    <text>Name of a routing filter.</text>
                  </description>
                  <leaf name="route-filter-name">
                    <type name="leafref">
                      <path value="/xpl:xpl/xpl:route-filters/xpl:route-filter/xpl:name"/>
                    </type>
                    <description>
                      <text>Name of a routing filter.</text>
                    </description>
                  </leaf>
                </case>
              </choice>
            </container>
            <container name="rlfa-tunnel">
              <when condition="../enable='true' and ../../../../../ni:name='_public_'"/>
              <description>
                <text>Configure remote LFA ip prefix.</text>
              </description>
              <choice name="policy-type">
                <description>
                  <text>Name of a routing policy.</text>
                </description>
                <case name="ip-prefix-name">
                  <description>
                    <text>Set a route policy name.</text>
                  </description>
                  <leaf name="ip-prefix-name">
                    <type name="leafref">
                      <path value="/rtp:routing-policy/rtp:ipv4-prefix-filters/rtp:ipv4-prefix-filter/rtp:name"/>
                    </type>
                    <description>
                      <text>To filter PQ node. Those nodes which don't pass filter can't be PQ node.</text>
                    </description>
                  </leaf>
                </case>
              </choice>
            </container>
          </container>
          <container name="local-mt-filter">
            <when condition="../../../../ni:name='_public_'"/>
            <description>
              <text>Configure OSPF local multicast topology filtering in routing updates.</text>
            </description>
            <choice name="filter-policy">
              <description>
                <text>Type of a filtering rule.</text>
              </description>
              <case name="acl-name-or-num">
                <description>
                  <text>ACL name or ACL number. ACL name: specify the ACL name. ACL num: apply basic ACL.</text>
                </description>
                <leaf name="acl-name-or-num">
                  <type name="leafref">
                    <path value="/acl:acl/acl:groups/acl:group/acl:identity"/>
                  </type>
                  <must condition="/acl:acl/acl:groups/acl:group[acl:identity=current()]/acl:type='basic'"/>
                  <description>
                    <text>ACL name or ACL number. ACL name: specify the ACL name. ACL num: apply basic ACL.</text>
                  </description>
                </leaf>
              </case>
              <case name="ip-prefix">
                <description>
                  <text>Ip prefix name.</text>
                </description>
                <leaf name="ip-prefix">
                  <type name="leafref">
                    <path value="/rtp:routing-policy/rtp:ipv4-prefix-filters/rtp:ipv4-prefix-filter/rtp:name"/>
                  </type>
                  <description>
                    <text>Ip prefix name.</text>
                  </description>
                </leaf>
              </case>
              <case name="route-policy-name">
                <description>
                  <text>Name of a routing policy.</text>
                </description>
                <leaf name="route-policy-name">
                  <type name="leafref">
                    <path value="/rtp:routing-policy/rtp:policy-definitions/rtp:policy-definition/rtp:name"/>
                  </type>
                  <description>
                    <text>Name of a routing policy.</text>
                  </description>
                </leaf>
              </case>
              <case name="route-filter-name">
                <description>
                  <text>Name of a routing filter.</text>
                </description>
                <leaf name="route-filter-name">
                  <type name="leafref">
                    <path value="/xpl:xpl/xpl:route-filters/xpl:route-filter/xpl:name"/>
                  </type>
                  <description>
                    <text>Name of a routing filter.</text>
                  </description>
                </leaf>
              </case>
            </choice>
          </container>
          <container name="peers">
            <description>
              <text>List of the IP address and DR priority of the neighbor router on the NBMA network.</text>
            </description>
            <list name="peer">
              <key value="ip-address"/>
              <description>
                <text>Configure the IP address and DR priority of the neighbor router on the NBMA network.</text>
              </description>
              <leaf name="ip-address">
                <type name="inet:ipv4-address-no-zone"/>
                <description>
                  <text>IP address of the neighbor router.</text>
                </description>
              </leaf>
              <leaf name="priority">
                <type name="uint8">
                  <range value="0..255"/>
                </type>
                <default value="1"/>
                <description>
                  <text>To set the IP address and DR priority of the neighbor router on the NBMA network.</text>
                </description>
              </leaf>
            </list>
          </container>
          <container name="filter-lsa-outs">
            <description>
              <text>List of LSA filter policy.</text>
            </description>
            <list name="filter-lsa-out">
              <key value="peer-ip-addr"/>
              <max-elements value="512"/>
              <description>
                <text>Configure LSA filter policy.</text>
              </description>
              <leaf name="peer-ip-addr">
                <type name="inet:ipv4-address-no-zone"/>
                <description>
                  <text>IP address of the P2MP neighbor.</text>
                </description>
              </leaf>
              <choice name="type">
                <mandatory value="true"/>
                <description>
                  <text>Set filter lsa out type.</text>
                </description>
                <case name="total">
                  <description>
                    <text>Filter total types of LSAs.</text>
                  </description>
                  <leaf name="all-flag">
                    <type name="empty"/>
                    <description>
                      <text>Filter all types of LSAs.</text>
                    </description>
                  </leaf>
                </case>
                <case name="other">
                  <description>
                    <text>Filter other types of LSAs.</text>
                  </description>
                  <leaf name="sum-flag">
                    <type name="empty"/>
                    <description>
                      <text>Filter type 3 Summary LSAs.</text>
                    </description>
                  </leaf>
                  <choice name="sum-filter">
                    <description>
                      <text>Type of a filtering rule.</text>
                    </description>
                    <case name="sum-acl-name">
                      <description>
                        <text>ACL Name or ACL Number. ACL Name: specify the ACL name. ACL Num: apply basic ACL.</text>
                      </description>
                      <leaf name="sum-acl-name">
                        <when condition="../sum-flag"/>
                        <type name="leafref">
                          <path value="/acl:acl/acl:groups/acl:group/acl:identity"/>
                        </type>
                        <must condition="/acl:acl/acl:groups/acl:group[acl:identity=current()]/acl:type='basic'"/>
                        <description>
                          <text>ACL Name or ACL Number. ACL Name: specify the ACL name. ACL Num: apply basic ACL.</text>
                        </description>
                      </leaf>
                    </case>
                  </choice>
                  <leaf name="ase-flag">
                    <type name="empty"/>
                    <description>
                      <text>Filter type-5 ASE LSAs.</text>
                    </description>
                  </leaf>
                  <choice name="ase-filter">
                    <description>
                      <text>Type of a filtering rule.</text>
                    </description>
                    <case name="ase-acl-name">
                      <description>
                        <text>ACL Name or ACL Number. ACL Name: specify the ACL name. ACL Num: apply basic ACL.</text>
                      </description>
                      <leaf name="ase-acl-name">
                        <when condition="../ase-flag"/>
                        <type name="leafref">
                          <path value="/acl:acl/acl:groups/acl:group/acl:identity"/>
                        </type>
                        <must condition="/acl:acl/acl:groups/acl:group[acl:identity=current()]/acl:type='basic'"/>
                        <description>
                          <text>ACL Name or ACL Number. ACL Name: specify the ACL name. ACL Num: apply basic ACL.</text>
                        </description>
                      </leaf>
                    </case>
                  </choice>
                  <leaf name="nssa-flag">
                    <type name="empty"/>
                    <description>
                      <text>Filter type7 NSSA LSAs.</text>
                    </description>
                  </leaf>
                  <choice name="nssa-filter">
                    <description>
                      <text>Type of a filtering rule.</text>
                    </description>
                    <case name="nssa-acl-name">
                      <description>
                        <text>ACL Name or ACL Number. ACL Name: specify the ACL name. ACL Num: apply basic ACL.</text>
                      </description>
                      <leaf name="nssa-acl-name">
                        <when condition="../nssa-flag"/>
                        <type name="leafref">
                          <path value="/acl:acl/acl:groups/acl:group/acl:identity"/>
                        </type>
                        <must condition="/acl:acl/acl:groups/acl:group[acl:identity=current()]/acl:type='basic'"/>
                        <description>
                          <text>ACL Name or ACL Number. ACL Name: specify the ACL name. ACL Num: apply basic ACL.</text>
                        </description>
                      </leaf>
                    </case>
                  </choice>
                </case>
              </choice>
            </list>
          </container>
          <choice name="graceful-restart">
            <description>
              <text>Information about OSPF GR.</text>
            </description>
            <case name="gr-ietf">
              <description>
                <text>Information about OSPF IETF GR.</text>
              </description>
              <container name="gr-ietf">
                <when condition="../opaque='true'"/>
                <presence value="Enable OSPF IETF GR."/>
                <description>
                  <text>Configure OSPF IETF GR.</text>
                </description>
                <choice name="helper-role-mode">
                  <description>
                    <text>Information about OSPF IETF GR helper role.</text>
                  </description>
                  <case name="never">
                    <description>
                      <text>Forbid to support enable mode.</text>
                    </description>
                    <leaf name="never">
                      <type name="empty"/>
                      <description>
                        <text>Forbid to support enable mode.</text>
                      </description>
                    </leaf>
                  </case>
                  <case name="work-mode">
                    <description>
                      <text>Information about OSPF IETF GR.</text>
                    </description>
                    <leaf name="planned-only">
                      <type name="empty"/>
                      <description>
                        <text>To support only planned GR on routers.</text>
                      </description>
                    </leaf>
                    <leaf name="ignore-external-lsa">
                      <type name="empty"/>
                      <description>
                        <text>Ignore external lsa check (type5 and type7 LSAs).</text>
                      </description>
                    </leaf>
                    <choice name="match-mode">
                      <description>
                        <text>Name of a routing policy.</text>
                      </description>
                      <case name="acl-name-or-num">
                        <description>
                          <text>Specify the basic ACL number or ACL name for filtering peer.</text>
                        </description>
                        <leaf name="acl-name-or-num">
                          <type name="leafref">
                            <path value="/acl:acl/acl:groups/acl:group/acl:identity"/>
                          </type>
                          <must condition="/acl:acl/acl:groups/acl:group[acl:identity=current()]/acl:type='basic'"/>
                          <description>
                            <text>Specify the basic ACL number or ACL name for filtering peer.</text>
                          </description>
                        </leaf>
                      </case>
                      <case name="ip-prefix-name">
                        <description>
                          <text>Specify the IP prefix name for filtering peers.</text>
                        </description>
                        <leaf name="ip-prefix-name">
                          <type name="leafref">
                            <path value="/rtp:routing-policy/rtp:ipv4-prefix-filters/rtp:ipv4-prefix-filter/rtp:name"/>
                          </type>
                          <description>
                            <text>Specify the IP prefix name for filtering peers.</text>
                          </description>
                        </leaf>
                      </case>
                    </choice>
                  </case>
                </choice>
              </container>
            </case>
            <case name="gr-non-ietf">
              <description>
                <text>Information about OSPF NON IETF GR.</text>
              </description>
              <container name="gr-non-ietf">
                <when condition="../opaque='true'"/>
                <presence value="Enable OSPF NON IETF GR."/>
                <description>
                  <text>Configure OSPF NON IETF GR.</text>
                </description>
              </container>
            </case>
          </choice>
          <container name="flood-control">
            <presence value="Enable flood-control"/>
            <description>
              <text>Configure OSPF flooding control.</text>
            </description>
            <leaf name="number">
              <type name="uint32">
                <range value="1..1000"/>
              </type>
              <default value="50"/>
              <description>
                <text>Specify the update packet number to send.</text>
              </description>
            </leaf>
            <leaf name="interval">
              <type name="uint32">
                <range value="30..100000"/>
              </type>
              <default value="30"/>
              <description>
                <text>Specify the update packet timer interval to send.</text>
              </description>
            </leaf>
          </container>
          <container name="dn-bit">
            <when condition="../../../../ni:name!='_public_'"/>
            <description>
              <text>Configure VPN optionA feature will help PEs in inter AS exchange routes through OSPF. VPN optionA provide two ways to suppress DN bit so that so that inter AS can exchange routes through OSPF. First way dose not set DN bit while import routes from BGP, second way dose not check DN bit while learning the routes from neighbor router. In both the scenario PE will be able to learn route and propagate the route in the new VPN cloud.</text>
            </description>
            <leaf name="set-ase">
              <type name="boolean"/>
              <default value="false"/>
              <description>
                <text>Enable/disable setting of DN bit in all ASE LSA, applicable in private network.</text>
              </description>
            </leaf>
            <leaf name="set-nssa">
              <type name="boolean"/>
              <default value="false"/>
              <description>
                <text>Enable/disable setting of DN bit in all NSSA LSA, applicable in private network.</text>
              </description>
            </leaf>
            <leaf name="set-summary">
              <type name="boolean"/>
              <default value="false"/>
              <description>
                <text>Enable/disable setting of DN bit in all summary LSA, applicable in private network.</text>
              </description>
            </leaf>
            <leaf name="check-ase">
              <type name="boolean"/>
              <default value="false"/>
              <description>
                <text>Enable/disable checking of DN bit in all ASE LSA, applicable in private network.</text>
              </description>
            </leaf>
            <leaf name="check-nssa">
              <type name="boolean"/>
              <default value="false"/>
              <description>
                <text>Enable/disable checking of DN bit in all NSSA LSA, applicable in private network.</text>
              </description>
            </leaf>
            <leaf name="check-summary">
              <type name="boolean"/>
              <default value="false"/>
              <description>
                <text>Enable/disable checking of DN bit in all summary LSA, applicable in private network.</text>
              </description>
            </leaf>
            <container name="summary-router-ids">
              <when condition="../check-summary='false'"/>
              <description>
                <text>List of router ID for which checking for DN bit summary LSA is disabled, applicable in private network.</text>
              </description>
              <list name="summary-router-id">
                <key value="router-id"/>
                <description>
                  <text>Configure router ID for which checking for DN bit summary LSA is disabled, applicable in private network.</text>
                </description>
                <leaf name="router-id">
                  <type name="inet:ipv4-address-no-zone"/>
                  <description>
                    <text>Router ID for which checking for DN bit summary LSA is disabled, applicable in private network.</text>
                  </description>
                </leaf>
              </list>
            </container>
          </container>
          <container name="filter-import">
            <presence value="filter policy import"/>
            <description>
              <text>Configure the filter policy import command. You can set the rules for filtering intra area, inter area, and AS external routes of an OSPF process.</text>
            </description>
            <choice name="policy-type">
              <mandatory value="true"/>
              <description>
                <text>Type of a filtering rule.</text>
              </description>
              <case name="basic-acl">
                <description>
                  <text>Apply basic ACL name or ACL number.</text>
                </description>
                <leaf name="name-or-num">
                  <type name="leafref">
                    <path value="/acl:acl/acl:groups/acl:group/acl:identity"/>
                  </type>
                  <must condition="/acl:acl/acl:groups/acl:group[acl:identity=current()]/acl:type='basic'"/>
                  <description>
                    <text>Apply basic ACL name or ACL number.</text>
                  </description>
                </leaf>
              </case>
              <case name="ip-prefix">
                <description>
                  <text>Set the name of an IP prefix list.</text>
                </description>
                <leaf name="ip-prefix">
                  <type name="leafref">
                    <path value="/rtp:routing-policy/rtp:ipv4-prefix-filters/rtp:ipv4-prefix-filter/rtp:name"/>
                  </type>
                  <description>
                    <text>Set the name of an IP prefix list.</text>
                  </description>
                </leaf>
              </case>
              <case name="route-policy">
                <description>
                  <text>Set a route policy name.</text>
                </description>
                <leaf name="route-policy">
                  <type name="leafref">
                    <path value="/rtp:routing-policy/rtp:policy-definitions/rtp:policy-definition/rtp:name"/>
                  </type>
                  <description>
                    <text>Set a route policy name.</text>
                  </description>
                </leaf>
              </case>
              <case name="route-filter">
                <description>
                  <text>Set a route filter name.</text>
                </description>
                <leaf name="route-filter">
                  <type name="leafref">
                    <path value="/xpl:xpl/xpl:route-filters/xpl:route-filter/xpl:name"/>
                  </type>
                  <description>
                    <text>Set a route filter name.</text>
                  </description>
                </leaf>
              </case>
            </choice>
            <leaf name="secondary">
              <type name="boolean"/>
              <must condition="../secondary='false' or ../route-policy"/>
              <default value="false"/>
              <description>
                <text>Enable/disable secondary flag.</text>
              </description>
            </leaf>
          </container>
          <container name="filter-exports">
            <description>
              <text>List of the filter policy export command. You can set the rules for filtering the routes imported through the import route (OSPF) command when the routes are advertised in type5 LSAs to the OSPF AS.</text>
            </description>
            <list name="filter-export">
              <key value="protocol protocol-id"/>
              <description>
                <text>Configure the filter policy export command. You can set the rules for filtering the routes imported through the import route (OSPF) command when the routes are advertised in type5 LSAs to the OSPF AS.</text>
              </description>
              <leaf name="protocol">
                <type name="filter-rt-protocol"/>
                <description>
                  <text>Protocol that advertises routes.</text>
                </description>
              </leaf>
              <leaf name="protocol-id">
                <type name="uint32">
                  <range value="1..4294967295"/>
                </type>
                <must condition="../protocol-id=1 or ../protocol='ospf' or ../protocol='isis' or ../protocol='rip'"/>
                <description>
                  <text>Process ID when the advertised protocol is RIP, ISIS, or OSPF.</text>
                </description>
              </leaf>
              <choice name="policy-type">
                <mandatory value="true"/>
                <description>
                  <text>Type of a filtering rule.</text>
                </description>
                <case name="basic-acl">
                  <description>
                    <text>Apply basic ACL name or ACL number.</text>
                  </description>
                  <leaf name="name-or-num">
                    <type name="leafref">
                      <path value="/acl:acl/acl:groups/acl:group/acl:identity"/>
                    </type>
                    <must condition="/acl:acl/acl:groups/acl:group[acl:identity=current()]/acl:type='basic'"/>
                    <description>
                      <text>Apply basic ACL name or ACL number.</text>
                    </description>
                  </leaf>
                </case>
                <case name="ip-prefix">
                  <description>
                    <text>Set the name of an IP prefix list.</text>
                  </description>
                  <leaf name="ip-prefix">
                    <type name="leafref">
                      <path value="/rtp:routing-policy/rtp:ipv4-prefix-filters/rtp:ipv4-prefix-filter/rtp:name"/>
                    </type>
                    <description>
                      <text>Set the name of an IP prefix list.</text>
                    </description>
                  </leaf>
                </case>
                <case name="route-policy">
                  <description>
                    <text>Set a route policy name.</text>
                  </description>
                  <leaf name="route-policy">
                    <type name="leafref">
                      <path value="/rtp:routing-policy/rtp:policy-definitions/rtp:policy-definition/rtp:name"/>
                    </type>
                    <description>
                      <text>Set a route policy name.</text>
                    </description>
                  </leaf>
                </case>
              </choice>
            </list>
          </container>
          <container name="import-routes">
            <description>
              <text>List of import the routes learned from other routing protocols.</text>
            </description>
            <list name="import-route">
              <key value="protocol process-id"/>
              <description>
                <text>Configure import the routes learned from other routing protocols.</text>
              </description>
              <leaf name="protocol">
                <type name="import-rt-protocol"/>
                <description>
                  <text>Routing protocol whose routes are imported.</text>
                </description>
              </leaf>
              <leaf name="process-id">
                <type name="uint32">
                  <range value="0..4294967295"/>
                </type>
                <must condition="(../process-id=0 and (../protocol='direct' or ../protocol='static' or ../protocol='bgp' or ../protocol='unr' or ../protocol='opr')) or (../process-id&gt;0 and (../protocol='ospf' or ../protocol='isis' or ../protocol='rip'))"/>
                <description>
                  <text>Process ID of the routing protocol whose routes are imported. It needs to be set only when the protocol is RIP, OSPF, or ISIS.</text>
                </description>
              </leaf>
              <leaf name="cost">
                <type name="uint32">
                  <range value="0..16777214"/>
                </type>
                <description>
                  <text>Route cost.</text>
                </description>
              </leaf>
              <leaf name="tag">
                <type name="uint32">
                  <range value="0..4294967295"/>
                </type>
                <description>
                  <text>Route tag of an external LSA.</text>
                </description>
              </leaf>
              <leaf name="type">
                <type name="external-route-type"/>
                <description>
                  <text>Metric type of an external LSA.</text>
                </description>
              </leaf>
              <leaf name="permit-ibgp">
                <type name="boolean"/>
                <must condition="../permit-ibgp='false' or ../protocol='bgp'"/>
                <default value="false"/>
                <description>
                  <text>Enable/disable to import IBGP routes.</text>
                </description>
              </leaf>
              <choice name="policy-type">
                <description>
                  <text>Type of a filtering rule.</text>
                </description>
                <case name="route-policy-name">
                  <description>
                    <text>Name of a routing policy.</text>
                  </description>
                  <leaf name="route-policy-name">
                    <type name="leafref">
                      <path value="/rtp:routing-policy/rtp:policy-definitions/rtp:policy-definition/rtp:name"/>
                    </type>
                    <description>
                      <text>Name of a routing policy.</text>
                    </description>
                  </leaf>
                </case>
              </choice>
            </list>
          </container>
          <container name="silent-interfaces">
            <when condition="../silent-all-interface='false'"/>
            <description>
              <text>List of interface. To prevent the OSPF routing information from being received by routers in a network segment and forbid the local router to receive the routing update information that is advertised by other routers in the network segment, you can use the silent interface command to suppress the interface to receive or send the OSPF packets.</text>
            </description>
            <list name="silent-interface">
              <key value="if-name"/>
              <description>
                <text>Configure interface. To prevent the OSPF routing information from being received by routers in a network segment and forbid the local router to receive the routing update information that is advertised by other routers in the network segment, you can use the silent interface command to suppress the interface to receive or send the OSPF packets.</text>
              </description>
              <leaf name="if-name">
                <type name="leafref">
                  <path value="/ifm:ifm/ifm:interfaces/ifm:interface/ifm:name"/>
                </type>
                <description>
                  <text>Interface name.</text>
                </description>
              </leaf>
            </list>
          </container>
          <container name="undo-silent-interfaces">
            <when condition="../silent-all-interface='true'"/>
            <description>
              <text>List of interface. Enables sending and receiving packet on this interface when true. This information is useful when sending and receiving packet is suppressed on all interface on process level and user want it to be enabled on certain interface.</text>
            </description>
            <list name="undo-silent-interface">
              <key value="if-name"/>
              <description>
                <text>Configure interface. Enables sending and receiving packet on this interface when true. This information is useful when sending and receiving packet is suppressed on all interface on process level and user want it to be enabled on certain interface.</text>
              </description>
              <leaf name="if-name">
                <type name="leafref">
                  <path value="/ifm:ifm/ifm:interfaces/ifm:interface/ifm:name"/>
                </type>
                <description>
                  <text>Interface name.</text>
                </description>
              </leaf>
            </list>
          </container>
          <container name="cost-bindings">
            <description>
              <text>List of cost-binding. Indicate cost of routing from indicated router source ID binding interface.</text>
            </description>
            <list name="cost-binding">
              <key value="router-id if-name"/>
              <description>
                <text>Configure cost-binding. Indicate cost of routing from indicated router source ID binding interface.</text>
              </description>
              <leaf name="router-id">
                <type name="inet:ipv4-address-no-zone"/>
                <description>
                  <text>Source router ID.</text>
                </description>
              </leaf>
              <leaf name="if-name">
                <type name="leafref">
                  <path value="/ifm:ifm/ifm:interfaces/ifm:interface/ifm:name"/>
                </type>
                <description>
                  <text>Interface name.</text>
                </description>
              </leaf>
            </list>
          </container>
          <container name="areas">
            <description>
              <text>List of OSPF area.</text>
            </description>
            <list name="area">
              <key value="area-id"/>
              <description>
                <text>Configure OSPF area.</text>
              </description>
              <leaf name="area-id">
                <type name="inet:ipv4-address-no-zone"/>
                <description>
                  <text>Area ID.</text>
                </description>
              </leaf>
              <leaf name="area-type">
                <type name="area-type"/>
                <default value="normal"/>
                <description>
                  <text>Area type.</text>
                </description>
              </leaf>
              <leaf name="description">
                <type name="string">
                  <length value="1..80"/>
                  <pattern value="([^?]*)"/>
                </type>
                <description>
                  <text>Description of an OSPF area.</text>
                </description>
              </leaf>
              <container name="networks">
                <description>
                  <text>List of the interface that runs OSPF and the area to which the interface belongs to.</text>
                </description>
                <list name="network">
                  <key value="ip-address mask"/>
                  <description>
                    <text>Configure the interface that runs OSPF and the area to which the interface belongs to.</text>
                  </description>
                  <leaf name="ip-address">
                    <type name="inet:ipv4-address-no-zone"/>
                    <description>
                      <text>Specifies the address of the network segment where the interface resides.</text>
                    </description>
                  </leaf>
                  <leaf name="mask">
                    <type name="inet:ipv4-address-no-zone"/>
                    <description>
                      <text>Specifies the wildcard mask of IP address.</text>
                    </description>
                  </leaf>
                  <leaf name="description">
                    <type name="string">
                      <length value="1..80"/>
                      <pattern value="([^?]*)"/>
                    </type>
                    <description>
                      <text>Specifies the description of the specified OSPF network segment.</text>
                    </description>
                  </leaf>
                </list>
              </container>
              <container name="stub">
                <when condition="../area-type='stub'"/>
                <description>
                  <text>Configure stub area.</text>
                </description>
                <leaf name="backbone-peer-ignore">
                  <type name="boolean"/>
                  <default value="false"/>
                  <description>
                    <text>Enable/disable ignore backbone peer for Stub area.</text>
                  </description>
                </leaf>
                <leaf name="no-summary">
                  <type name="boolean"/>
                  <default value="false"/>
                  <description>
                    <text>Enable/disable forbid an ABR to Send Summary LSAs to the Stub Area.</text>
                  </description>
                </leaf>
                <leaf name="default-cost">
                  <type name="int32">
                    <range value="0..16777214"/>
                  </type>
                  <default value="1"/>
                  <description>
                    <text>Cost of the default Type3 routes that are sent by an OSPF process to a stub area or an NSSA area. The value is an integer ranging from 1 to 16777214. This parameter takes effect only when it is configured on the ABR connected to a stub area or an NSSA area. The default is 1.</text>
                  </description>
                </leaf>
              </container>
              <container name="nssa">
                <when condition="../area-type='nssa'"/>
                <description>
                  <text>Configure nssa area.</text>
                </description>
                <leaf name="no-summary">
                  <type name="boolean"/>
                  <default value="false"/>
                  <description>
                    <text>Enable/disable forbid an ABR to Send Summary LSAs to the NSSA Area.</text>
                  </description>
                </leaf>
                <leaf name="translator-always">
                  <type name="boolean"/>
                  <default value="false"/>
                  <description>
                    <text>Enable/disable the router to act as translator always for NSSA Area.</text>
                  </description>
                </leaf>
                <leaf name="default-route-advertise">
                  <type name="boolean"/>
                  <default value="false"/>
                  <description>
                    <text>Enable/disable originate Type 7 default into NSSA area.</text>
                  </description>
                </leaf>
                <leaf name="backbone-peer-ignore">
                  <when condition="../default-route-advertise='true'"/>
                  <type name="boolean"/>
                  <default value="false"/>
                  <description>
                    <text>Enable/disable ignore backbone peer for Nssa area.</text>
                  </description>
                </leaf>
                <leaf name="no-import-route">
                  <type name="boolean"/>
                  <default value="false"/>
                  <description>
                    <text>Enable/disable redistribute type 7 LSA into this area.</text>
                  </description>
                </leaf>
                <leaf name="set-n-bit">
                  <type name="boolean"/>
                  <default value="false"/>
                  <description>
                    <text>Enable/disable setting of 'N' bit in the DD packet.</text>
                  </description>
                </leaf>
                <leaf name="suppress-fa">
                  <type name="boolean"/>
                  <default value="false"/>
                  <description>
                    <text>Enable/disable the forwarding address as 0 for all translated type 5 LSAs.</text>
                  </description>
                </leaf>
                <leaf name="zero-address-forwarding">
                  <type name="boolean"/>
                  <default value="false"/>
                  <description>
                    <text>Enable/disable allow zero forwarding address for redistributed type 7 LSA.</text>
                  </description>
                </leaf>
                <leaf name="translator-interval">
                  <type name="uint32">
                    <range value="1..120"/>
                  </type>
                  <default value="40"/>
                  <description>
                    <text>Translator interval.</text>
                  </description>
                </leaf>
                <leaf name="default-cost">
                  <type name="int32">
                    <range value="0..16777214"/>
                  </type>
                  <default value="1"/>
                  <description>
                    <text>Cost of the default Type3 routes that are sent by an OSPF process to a stub area or an NSSA area. This parameter takes effect only when it is configured on the ABR connected to a stub area or an NSSA area.</text>
                  </description>
                </leaf>
              </container>
              <container name="ldp-sync">
                <description>
                  <text>Configure timer ldp sync.</text>
                </description>
                <leaf name="enable">
                  <type name="boolean"/>
                  <default value="false"/>
                  <description>
                    <text>Enable/disable LDP and OSPF synchronization on area.</text>
                  </description>
                </leaf>
                <choice name="hold-cost-max">
                  <default value="infinite"/>
                  <description>
                    <text>Set timer ldp sync.</text>
                  </description>
                  <case name="timer">
                    <description>
                      <text>Set timer ldp sync.</text>
                    </description>
                    <leaf name="timer">
                      <type name="uint32">
                        <range value="0..65535"/>
                      </type>
                      <units name="s"/>
                      <description>
                        <text>The interval for sending OSPF LSAs to advertise the maximum metric on the local device.</text>
                      </description>
                    </leaf>
                  </case>
                  <case name="infinite">
                    <description>
                      <text>Always advertise the maximum cost.</text>
                    </description>
                    <leaf name="infinite">
                      <type name="boolean"/>
                      <must condition="../infinite='true'"/>
                      <default value="true"/>
                      <description>
                        <text>Enable/disable always advertise the maximum cost.</text>
                      </description>
                    </leaf>
                  </case>
                </choice>
              </container>
              <container name="mpls-te">
                <when condition="../../../../../../ni:name!='__dcn_vpn__'"/>
                <description>
                  <text>Configure OSPF processes enabled with the TE capability.</text>
                </description>
                <leaf name="enable">
                  <type name="boolean"/>
                  <must condition="../../../../opaque='true' or ../enable='false'"/>
                  <default value="false"/>
                  <description>
                    <text>Enable/disable TE capability of the OSPF process in the specified area.</text>
                  </description>
                </leaf>
                <leaf name="standard-complying">
                  <type name="boolean"/>
                  <must condition="../standard-complying='false' or ../enable='true'"/>
                  <default value="false"/>
                  <description>
                    <text>Enable/disable accept the LSAs in standard format only.</text>
                  </description>
                </leaf>
              </container>
              <container name="authentication">
                <must condition="(key-id and auth-text-md5) or (not(key-id) and not(auth-text-md5))"/>
                <description>
                  <text>Configure authentication. The encryption algorithm MD5 has a low security, which may bring security risks. Using more secure authentication is recommended.</text>
                </description>
                <leaf name="mode">
                  <type name="area-authen-mode"/>
                  <default value="none"/>
                  <description>
                    <text>Set authentication type.</text>
                  </description>
                </leaf>
                <leaf name="auth-text-simple">
                  <when condition="../mode='simple'"/>
                  <type name="pub-type:password"/>
                  <description>
                    <text>Simple authentication key. The password is a string ranging from 1 to 8 characters for a unencrypted password and 24 to 128 characters for a encrypted password.</text>
                  </description>
                </leaf>
                <leaf name="key-id">
                  <when condition="../mode='hmac-md5' or ../mode='hmac-sha256' or ../mode='md5'"/>
                  <type name="uint32">
                    <range value="1..255"/>
                  </type>
                  <description>
                    <text>Authentication identifier.</text>
                  </description>
                </leaf>
                <leaf name="auth-text-md5">
                  <when condition="../mode='hmac-md5' or ../mode='hmac-sha256' or ../mode='md5'"/>
                  <type name="pub-type:password-extend"/>
                  <description>
                    <text>Encrypted authentication password.</text>
                  </description>
                </leaf>
                <leaf name="key-chain-name">
                  <when condition="../mode='keychain'"/>
                  <type name="string">
                    <length value="1..47"/>
                    <pattern value="[^A-Z]+"/>
                  </type>
                  <ext:case-sensitivity value="lower-only"/>
                  <mandatory value="true"/>
                  <description>
                    <text>Keychain name.</text>
                  </description>
                </leaf>
              </container>
              <container name="abr-summary-prefixs">
                <description>
                  <text>List of ABR route aggregation method.</text>
                </description>
                <list name="abr-summary-prefix">
                  <key value="prefix network-mask"/>
                  <description>
                    <text>Configure ABR route aggregation method.</text>
                  </description>
                  <leaf name="prefix">
                    <type name="inet:ipv4-address-no-zone"/>
                    <description>
                      <text>Specifies the IP address in dotted decimal notation.</text>
                    </description>
                  </leaf>
                  <leaf name="network-mask">
                    <type name="inet:ipv4-address-no-zone"/>
                    <description>
                      <text>Specifies the mask of the IP address. The mask address is in dotted decimal notation.</text>
                    </description>
                  </leaf>
                  <choice name="cost">
                    <description>
                      <text>Set the cost.</text>
                    </description>
                    <case name="cost-value">
                      <description>
                        <text>Specifies the cost of the aggregated route. By default, the maximal cost in that of all the routes which are aggregated serves as the cost of the aggregated route.</text>
                      </description>
                      <leaf name="cost-value">
                        <type name="uint32">
                          <range value="0..16777214"/>
                        </type>
                        <description>
                          <text>Specifies the cost of the aggregated route. By default, the maximal cost in that of all the routes which are aggregated serves as the cost of the aggregated route.</text>
                        </description>
                      </leaf>
                    </case>
                    <case name="cost-inherit-min">
                      <description>
                        <text>Indicates if minimum cost should be inherited. By default, maximum cost is inherited.</text>
                      </description>
                      <leaf name="cost-inherit-min">
                        <when condition="../../../../../../../../ni:name!='_public_'"/>
                        <type name="empty"/>
                        <description>
                          <text>Indicates if minimum cost should be inherited. By default, maximum cost is inherited.</text>
                        </description>
                      </leaf>
                    </case>
                  </choice>
                  <leaf name="hold-max-cost">
                    <type name="uint16">
                      <range value="10..1800"/>
                    </type>
                    <description>
                      <text>Hold max cost duration.</text>
                    </description>
                  </leaf>
                  <leaf name="advertise">
                    <type name="boolean"/>
                    <default value="true"/>
                    <description>
                      <text>Enable/disable the aggregated route is advertised.</text>
                    </description>
                  </leaf>
                  <leaf name="generate-null-zero">
                    <when condition="../advertise='true'"/>
                    <type name="boolean"/>
                    <default value="false"/>
                    <description>
                      <text>Enable/disable generate black hole route.</text>
                    </description>
                  </leaf>
                </list>
              </container>
              <container name="vlink-peers">
                <when condition="../area-type='normal'"/>
                <description>
                  <text>List of virtual link and its parameters.</text>
                </description>
                <list name="vlink-peer">
                  <key value="router-id"/>
                  <description>
                    <text>Configure virtual link and its parameters.</text>
                  </description>
                  <leaf name="router-id">
                    <type name="inet:ipv4-address-no-zone"/>
                    <description>
                      <text>Neighbor Router ID.</text>
                    </description>
                  </leaf>
                  <leaf name="retransmit-interval">
                    <type name="uint32">
                      <range value="1..3600"/>
                    </type>
                    <default value="5"/>
                    <description>
                      <text>Interval for retransmitting LSAs.</text>
                    </description>
                  </leaf>
                  <leaf name="smart-discover">
                    <type name="boolean"/>
                    <default value="false"/>
                    <description>
                      <text>Enable/disable smart discover feature.</text>
                    </description>
                  </leaf>
                  <container name="timer">
                    <must condition="hello-interval&lt;dead-interval"/>
                    <description>
                      <text>Configure timer.</text>
                    </description>
                    <leaf name="hello-interval">
                      <type name="uint32">
                        <range value="1..65535"/>
                      </type>
                      <default value="10"/>
                      <description>
                        <text>Time between Hello packets.</text>
                      </description>
                    </leaf>
                    <leaf name="dead-interval">
                      <type name="uint32">
                        <range value="1..235926000"/>
                      </type>
                      <default value="40"/>
                      <description>
                        <text>Interval after which a neighbor is declared dead.</text>
                      </description>
                    </leaf>
                    <leaf name="trans-delay-interval">
                      <type name="uint32">
                        <range value="1..3600"/>
                      </type>
                      <default value="1"/>
                      <description>
                        <text>Delay for transmitting LSAs.</text>
                      </description>
                    </leaf>
                  </container>
                  <container name="authentication">
                    <description>
                      <text>Configure authentication. The encryption algorithm MD5 has a low security, which may bring security risks. Using more secure authentication is recommended.</text>
                    </description>
                    <leaf name="mode">
                      <type name="intf-authen-mode"/>
                      <default value="none"/>
                      <description>
                        <text>Set authentication type: Simple authentication type, Md5/HMAC MD5/HMAC SHA256 authentication type.</text>
                      </description>
                    </leaf>
                    <leaf name="auth-text-simple">
                      <when condition="../mode='simple'"/>
                      <type name="pub-type:password"/>
                      <description>
                        <text>Simple authentication key. The password is a string ranging from 1 to 8 characters for a unencrypted password and 24 to 128 characters for a encrypted password.</text>
                      </description>
                    </leaf>
                    <leaf name="key-id">
                      <when condition="../mode='hmac-sha256' or ../mode='md5' or ../mode='hmac-md5'"/>
                      <type name="uint32">
                        <range value="1..255"/>
                      </type>
                      <must condition="../key-id and ../auth-text-md5"/>
                      <description>
                        <text>Authentication identifier.</text>
                      </description>
                    </leaf>
                    <leaf name="auth-text-md5">
                      <when condition="../mode='hmac-sha256' or ../mode='md5' or ../mode='hmac-md5'"/>
                      <type name="pub-type:password-extend">
                        <length value="1..432"/>
                      </type>
                      <must condition="../key-id and ../auth-text-md5"/>
                      <description>
                        <text>MD5/HMAC MD5/HMAC SHA256 authentication password.</text>
                      </description>
                    </leaf>
                    <leaf name="key-chain-name">
                      <when condition="../mode='keychain'"/>
                      <type name="string">
                        <length value="1..47"/>
                        <pattern value="[^A-Z]+"/>
                      </type>
                      <ext:case-sensitivity value="lower-only"/>
                      <mandatory value="true"/>
                      <description>
                        <text>Keychain name.</text>
                      </description>
                    </leaf>
                  </container>
                  <container name="attribute">
                    <config value="false"/>
                    <description>
                      <text>Statistics of vlink peer.</text>
                    </description>
                    <leaf name="intf-state">
                      <type name="intf-state"/>
                      <description>
                        <text>OSPF virtual interface states.</text>
                      </description>
                    </leaf>
                    <leaf name="intf-ip-addr">
                      <type name="inet:ipv4-address-no-zone"/>
                      <description>
                        <text>Self or source IP address of the out going interface.</text>
                      </description>
                    </leaf>
                    <leaf name="gr-state">
                      <type name="nbr-gr-state"/>
                      <description>
                        <text>Status of GR of an interface.</text>
                      </description>
                    </leaf>
                    <leaf name="intf-cost">
                      <type name="uint32"/>
                      <description>
                        <text>Cost to reach the end point of this virtual link or cost to reach the virtual neighbor.</text>
                      </description>
                    </leaf>
                    <leaf name="intf-type">
                      <type name="network-type"/>
                      <description>
                        <text>Interface type.</text>
                      </description>
                    </leaf>
                    <leaf name="if-events">
                      <type name="uint32"/>
                      <description>
                        <text>Virtual interface events.</text>
                      </description>
                    </leaf>
                    <leaf name="if-status">
                      <type name="if-row-status"/>
                      <description>
                        <text>Virtual interface status.</text>
                      </description>
                    </leaf>
                    <leaf name="if-lsa-count">
                      <type name="uint32"/>
                      <description>
                        <text>Virtual interface LSA count.</text>
                      </description>
                    </leaf>
                    <leaf name="if-lsa-checksum">
                      <type name="uint32"/>
                      <description>
                        <text>Virtual interface LSA checksum.</text>
                      </description>
                    </leaf>
                  </container>
                </list>
              </container>
              <container name="sham-links">
                <when condition="../../../../../../ni:name!='_public_'"/>
                <description>
                  <text>List of sham link and its parameters.</text>
                </description>
                <list name="sham-link">
                  <key value="src-addr dest-addr"/>
                  <description>
                    <text>Configure sham link and its parameters.</text>
                  </description>
                  <leaf name="src-addr">
                    <type name="inet:ipv4-address-no-zone"/>
                    <description>
                      <text>Source address.</text>
                    </description>
                  </leaf>
                  <leaf name="dest-addr">
                    <type name="inet:ipv4-address-no-zone"/>
                    <must condition="../dest-addr!=../src-addr"/>
                    <description>
                      <text>Destination address.</text>
                    </description>
                  </leaf>
                  <leaf name="cost">
                    <type name="uint32">
                      <range value="1..65535"/>
                    </type>
                    <default value="1"/>
                    <description>
                      <text>Cost.</text>
                    </description>
                  </leaf>
                  <leaf name="retransmit-interval">
                    <type name="uint32">
                      <range value="1..3600"/>
                    </type>
                    <default value="5"/>
                    <description>
                      <text>Interval for retransmitting LSAs.</text>
                    </description>
                  </leaf>
                  <leaf name="smart-discover">
                    <type name="boolean"/>
                    <default value="false"/>
                    <description>
                      <text>Enable/disable smart discover feature.</text>
                    </description>
                  </leaf>
                  <container name="timer">
                    <must condition="hello-interval&lt;dead-interval"/>
                    <description>
                      <text>Configure timer.</text>
                    </description>
                    <leaf name="hello-interval">
                      <type name="uint32">
                        <range value="1..65535"/>
                      </type>
                      <default value="10"/>
                      <description>
                        <text>Time between Hello packets.</text>
                      </description>
                    </leaf>
                    <leaf name="dead-interval">
                      <type name="uint32">
                        <range value="1..235926000"/>
                      </type>
                      <default value="40"/>
                      <description>
                        <text>Interval after which a neighbor is declared dead.</text>
                      </description>
                    </leaf>
                    <leaf name="trans-delay-interval">
                      <type name="uint32">
                        <range value="1..3600"/>
                      </type>
                      <default value="1"/>
                      <description>
                        <text>Delay for transmitting LSAs.</text>
                      </description>
                    </leaf>
                  </container>
                  <container name="authentication">
                    <description>
                      <text>Configure authentication. The encryption algorithm MD5 has a low security, which may bring security risks. Using more secure authentication is recommended.</text>
                    </description>
                    <leaf name="mode">
                      <type name="intf-authen-mode"/>
                      <default value="none"/>
                      <description>
                        <text>Set authentication type: Simple authentication type, Md5/HMAC MD5/HMAC SHA256 authentication type.</text>
                      </description>
                    </leaf>
                    <leaf name="auth-text-simple">
                      <when condition="../mode='simple'"/>
                      <type name="pub-type:password"/>
                      <description>
                        <text>Simple authentication key. The password is a string ranging from 1 to 8 characters for a unencrypted password and 24 to 128 characters for a encrypted password.</text>
                      </description>
                    </leaf>
                    <leaf name="key-id">
                      <when condition="../mode='hmac-sha256' or ../mode='md5' or ../mode='hmac-md5'"/>
                      <type name="uint32">
                        <range value="1..255"/>
                      </type>
                      <must condition="../key-id and ../auth-text-md5"/>
                      <description>
                        <text>Authentication identifier.</text>
                      </description>
                    </leaf>
                    <leaf name="auth-text-md5">
                      <when condition="../mode='hmac-sha256' or ../mode='md5' or ../mode='hmac-md5'"/>
                      <type name="pub-type:password-extend">
                        <length value="1..432"/>
                      </type>
                      <must condition="../key-id and ../auth-text-md5"/>
                      <description>
                        <text>MD5/HMAC MD5/HMAC SHA256 authentication password.</text>
                      </description>
                    </leaf>
                    <leaf name="key-chain-name">
                      <when condition="../mode='keychain'"/>
                      <type name="string">
                        <length value="1..47"/>
                        <pattern value="[^A-Z]+"/>
                      </type>
                      <ext:case-sensitivity value="lower-only"/>
                      <mandatory value="true"/>
                      <description>
                        <text>Keychain name.</text>
                      </description>
                    </leaf>
                  </container>
                  <container name="attribute">
                    <config value="false"/>
                    <description>
                      <text>Statistics of sham link.</text>
                    </description>
                    <leaf name="state">
                      <type name="intf-state"/>
                      <description>
                        <text>OSPF sham interface states.</text>
                      </description>
                    </leaf>
                    <leaf name="gr-state">
                      <type name="nbr-gr-state"/>
                      <description>
                        <text>Status of GR of the interface.</text>
                      </description>
                    </leaf>
                    <leaf name="nbr-state">
                      <type name="nbr-state"/>
                      <description>
                        <text>OSPF neighbor states.</text>
                      </description>
                    </leaf>
                    <leaf name="nbr-id">
                      <type name="inet:ipv4-address-no-zone"/>
                      <description>
                        <text>Neighbor router ID.</text>
                      </description>
                    </leaf>
                  </container>
                </list>
              </container>
              <container name="filters">
                <description>
                  <text>List of ABR to filter outgoing or incoming summary LSAs of the local area.</text>
                </description>
                <list name="filter">
                  <key value="mode"/>
                  <description>
                    <text>Configure ABR to filter outgoing or incoming summary LSAs of the local area.</text>
                  </description>
                  <leaf name="mode">
                    <type name="area-filter-mode"/>
                    <description>
                      <text>Using the filter export command, you can configure an ABR to filter outgoing summary LSAs of the local area. Using the filter import command, you can configure an ABR to filter incoming summary LSAs of the local area.</text>
                    </description>
                  </leaf>
                  <uses name="policy-type"/>
                  <leaf name="include-abr-summary">
                    <when condition="../mode='filter-import'"/>
                    <type name="boolean"/>
                    <default value="false"/>
                    <description>
                      <text>Enable/disable include abr summary flag.</text>
                    </description>
                  </leaf>
                </list>
              </container>
              <container name="interfaces">
                <description>
                  <text>List of the interface enabled with OSPF.</text>
                </description>
                <list name="interface">
                  <key value="name"/>
                  <description>
                    <text>Configure the interface enabled with OSPF.</text>
                  </description>
                  <leaf name="name">
                    <type name="leafref">
                      <path value="/ifm:ifm/ifm:interfaces/ifm:interface/ifm:name"/>
                    </type>
                    <must condition="../../../../../../../../ni:name=/ifm:ifm/ifm:interfaces/ifm:interface[ifm:name=current()]/ifm:vrf-name"/>
                    <description>
                      <text>Interface name. OSPF does not support the configuration of a NULL/LMP/MTUNNEL/RPR/STACKPORT/SIP/ATMBUNDLE/IMAGROUP/Remote-Ap/VBridge interface types.</text>
                    </description>
                  </leaf>
                  <leaf name="network-type">
                    <type name="network-type"/>
                    <ext:dynamic-default/>
                    <description>
                      <text>Network type of an OSPF interface. By default, the network type of an interface is determined by the physical interface.
In the same network segment, if only two routers run OSPF, you can change the network type of an interface to P2P.</text>
                    </description>
                  </leaf>
                  <leaf name="peer-ip-ignore">
                    <type name="peer-ip-ignore-type"/>
                    <must condition="../peer-ip-ignore='enable' and ../network-type='p2p' or ../peer-ip-ignore='disable'"/>
                    <default value="disable"/>
                    <description>
                      <text>Ignores the subnet match check in multi access interface when network type P2P is configured along with this option.</text>
                    </description>
                  </leaf>
                  <leaf name="transmit-delay">
                    <type name="int32">
                      <range value="1..500"/>
                    </type>
                    <default value="1"/>
                    <description>
                      <text>Delay for transmitting LSAs on an interface.</text>
                    </description>
                  </leaf>
                  <leaf name="mtu-enable">
                    <type name="boolean"/>
                    <default value="false"/>
                    <description>
                      <text>Enable/disable MTU filled in a DD packet when the packet is sent. The default MTU in the DD packet is 0. Using the command, you can manually configure the interface to fill in the MTU field (actual MTU of the interface) when the interface sends DD packets.
After a virtual connection is established through the virtual template or tunnel, different vendors may use different MTUs as default settings. To ensure consistency, the MTU is set to 0 by default when the interface sends DD packets. Thus, the ospf mtu-enable command usually does not need to be configured.
OSPF does not support the preceding configuration on a null interface.</text>
                    </description>
                  </leaf>
                  <leaf name="dr-priority">
                    <type name="int32">
                      <range value="0..255"/>
                    </type>
                    <default value="1"/>
                    <description>
                      <text>Priority of the interface that participates in the DR and BDR election. The greater the value is, the higher the priority is. The router whose priority is 0 cannot be elected as a DR or a BDR.</text>
                    </description>
                  </leaf>
                  <leaf name="cost">
                    <type name="uint32">
                      <range value="1..65535"/>
                    </type>
                    <description>
                      <text>To set the cost of an interface. The default value is 1. But the default of Loopback is 0.</text>
                    </description>
                  </leaf>
                  <leaf name="smart-discover">
                    <type name="boolean"/>
                    <default value="false"/>
                    <description>
                      <text>Enable/disable smart discover on an interface. When the neighbor status of a router or a DR or BDR on the multi address network of the broadcast or NBMA type changes, the router can send hello packets to the neighbor actively rather than wait for the expiration of the hello timer.</text>
                    </description>
                  </leaf>
                  <leaf name="p2mp-mask-ignore">
                    <type name="boolean"/>
                    <default value="false"/>
                    <description>
                      <text>Enable/disable to ignore the check of network masks on a P2MP network. On a P2MP network, when the lengths of device masks are inconsistent, the check of network masks in hello packets can be ignored through this command. In this case, the OSPF neighbor relationship can be normally established.</text>
                    </description>
                  </leaf>
                  <leaf name="ldp-sync-block">
                    <type name="boolean"/>
                    <must condition="../ldp-sync-block='false' or ../ldp-sync-enable='false'"/>
                    <default value="false"/>
                    <description>
                      <text>Enable/disable LDP sync on this interface.</text>
                    </description>
                  </leaf>
                  <leaf name="ldp-sync-enable">
                    <type name="boolean"/>
                    <must condition="../ldp-sync-enable='false' or ../ldp-sync-block='false'"/>
                    <default value="false"/>
                    <description>
                      <text>Enable/disable LDP and OSPF synchronization on an interface.</text>
                    </description>
                  </leaf>
                  <leaf name="link-cost">
                    <type name="uint32">
                      <range value="1..65535"/>
                    </type>
                    <description>
                      <text>Link cost to be incremented when link quality is low. The default of Loopback is 0,others is 1.</text>
                    </description>
                  </leaf>
                  <leaf name="suppress-reachability">
                    <type name="suppr-reach"/>
                    <default value="not-configured"/>
                    <description>
                      <text>Suppress its own prefixes and ignore the suppressed prefixes.</text>
                    </description>
                  </leaf>
                  <leaf name="mpls-ldp-auto-flag">
                    <type name="if-auto-cfg-state"/>
                    <default value="none"/>
                    <description>
                      <text>Block MPLS LDP auto configuration on the interface.</text>
                    </description>
                  </leaf>
                  <leaf name="dcn-opq-blk-enable">
                    <type name="boolean"/>
                    <default value="false"/>
                    <description>
                      <text>Enable/disable filter type-10 dcn opaque LSAs.</text>
                    </description>
                  </leaf>
                  <leaf name="peer-hold-max-timer">
                    <type name="uint32">
                      <range value="100..1000000"/>
                    </type>
                    <units name="ms"/>
                    <description>
                      <text>Config peer hold max cost interval.</text>
                    </description>
                  </leaf>
                  <leaf name="fallback-cost">
                    <when condition="/ifm:ifm/ifm:interfaces/ifm:interface[ifm:name=current()/../name]/ifm:type='Eth-Trunk'"/>
                    <type name="uint32">
                      <range value="1..65535"/>
                    </type>
                    <description>
                      <text>Fallback cost.</text>
                    </description>
                  </leaf>
                  <leaf name="fallback-bw">
                    <when condition="../fallback-cost"/>
                    <type name="uint32">
                      <range value="1..4294967295"/>
                    </type>
                    <units name="Mbit/s"/>
                    <mandatory value="true"/>
                    <description>
                      <text>Fallback bandwidth threshold.</text>
                    </description>
                  </leaf>
                  <leaf name="source-sub-ip-address">
                    <type name="inet:ipv4-address-no-zone"/>
                    <must condition="/ifm:ifm/ifm:interfaces/ifm:interface[ifm:name=current()/../name]/ifm:type='Vlanif' or /ifm:ifm/ifm:interfaces/ifm:interface[ifm:name=current()/../name]/ifm:type='Vbdif'"/>
                    <description>
                      <text>Source address indicating a subordinate IP address.</text>
                    </description>
                  </leaf>
                  <container name="frr">
                    <description>
                      <text>Configure FRR.</text>
                    </description>
                    <leaf name="frr-disable">
                      <type name="boolean"/>
                      <default value="false"/>
                      <description>
                        <text>Enable/disable not use this interface for FRR calculation.</text>
                      </description>
                    </leaf>
                    <leaf name="rlfa-disable">
                      <type name="boolean"/>
                      <default value="false"/>
                      <description>
                        <text>Enable/disable remote LFA of the Interface.</text>
                      </description>
                    </leaf>
                    <container name="ti-lfa">
                      <description>
                        <text>Configure topology independent loop free alternate (TILFA) computation using segment routing.</text>
                      </description>
                      <leaf name="disable">
                        <type name="boolean"/>
                        <default value="false"/>
                        <description>
                          <text>Enable/disable topology independent LFA on the interface except Loopback.</text>
                        </description>
                      </leaf>
                    </container>
                  </container>
                  <container name="timer">
                    <description>
                      <text>Configure timer.</text>
                    </description>
                    <leaf name="hello-interval">
                      <type name="int32">
                        <range value="1..65535"/>
                      </type>
                      <ext:dynamic-default>
                        <ext:default-value value="10">
                          <when condition="../../network-type = 'p2p' or ../../network-type = 'broadcast'"/>
                          <description>
                            <text>The default value is 10 when network-type is p2p or broadcast.</text>
                          </description>
                        </ext:default-value>
                        <ext:default-value value="30">
                          <when condition="../../network-type = 'p2mp' or ../../network-type = 'nbma'"/>
                          <description>
                            <text>The default value is 30 when network-type is p2mp or nbma.</text>
                          </description>
                        </ext:default-value>
                      </ext:dynamic-default>
                      <description>
                        <text>Interval for sending hello packets on an interface.</text>
                      </description>
                    </leaf>
                    <leaf name="hello-conser">
                      <type name="boolean"/>
                      <default value="false"/>
                      <description>
                        <text>Enable/disable the compensation mechanism of controling the deadtimer to enable.</text>
                      </description>
                    </leaf>
                    <leaf name="dead-interval">
                      <type name="int32">
                        <range value="1..235926000"/>
                      </type>
                      <must condition="../hello-interval&lt;../dead-interval"/>
                      <ext:dynamic-default>
                        <ext:default-value value="4*(../hello-interval)">
                          <description>
                            <text>The default value of dead interval is 4 times that of hello interval.</text>
                          </description>
                        </ext:default-value>
                      </ext:dynamic-default>
                      <description>
                        <text>Dead interval of an OSPF neighbor.</text>
                      </description>
                    </leaf>
                    <leaf name="wait-interval">
                      <type name="uint32">
                        <range value="1..235926000"/>
                      </type>
                      <ext:dynamic-default>
                        <ext:default-value value="../dead-interval">
                          <description>
                            <text>The default value of wait interval is equal to dead interval.</text>
                          </description>
                        </ext:default-value>
                      </ext:dynamic-default>
                      <description>
                        <text>Wait time.</text>
                      </description>
                    </leaf>
                    <leaf name="poll-interval">
                      <type name="int32">
                        <range value="1..3600"/>
                      </type>
                      <default value="120"/>
                      <description>
                        <text>Poll interval for sending hello packets. On the NBMA network, after the neighbor is invalid, the router sends hello packets periodically according to the interval set through the ospf timer poll command. The poll interval should be at least four times of the interval for sending hello packets.</text>
                      </description>
                    </leaf>
                    <leaf name="retransmit-interval">
                      <type name="int32">
                        <range value="1..3600"/>
                      </type>
                      <default value="5"/>
                      <description>
                        <text>Interval for retransmitting LSAs on an interface.</text>
                      </description>
                    </leaf>
                  </container>
                  <container name="ldp-sync">
                    <description>
                      <text>Configure timer ldp sync.</text>
                    </description>
                    <leaf name="hold-down-timer">
                      <type name="uint16">
                        <range value="0..65535"/>
                      </type>
                      <units name="s"/>
                      <default value="10"/>
                      <description>
                        <text>Hold down interval.</text>
                      </description>
                    </leaf>
                    <choice name="hold-cost-max">
                      <description>
                        <text>Set timer ldp sync.</text>
                      </description>
                      <case name="timer">
                        <description>
                          <text>Set max cost interval.</text>
                        </description>
                        <leaf name="timer">
                          <type name="uint16">
                            <range value="0..65535"/>
                          </type>
                          <units name="s"/>
                          <description>
                            <text>Max cost interval range.</text>
                          </description>
                        </leaf>
                      </case>
                      <case name="infinite">
                        <description>
                          <text>Always advertise the maximum cost.</text>
                        </description>
                        <leaf name="infinite">
                          <type name="empty"/>
                          <description>
                            <text>Always advertise the maximum cost.</text>
                          </description>
                        </leaf>
                      </case>
                    </choice>
                  </container>
                  <container name="bfd">
                    <description>
                      <text>Configure BFD.</text>
                    </description>
                    <leaf name="enable">
                      <type name="boolean"/>
                      <must condition="../enable='false' or ../block='false'"/>
                      <default value="false"/>
                      <description>
                        <text>Enable/disable BFD.</text>
                      </description>
                    </leaf>
                    <leaf name="block">
                      <type name="boolean"/>
                      <must condition="../block='false' or ../enable='false'"/>
                      <default value="false"/>
                      <description>
                        <text>Enable/disable to prevent the dynamic establishment of a BFD session on an interface.</text>
                      </description>
                    </leaf>
                    <leaf name="detect-multiplier">
                      <type name="uint32">
                        <range value="3..50"/>
                      </type>
                      <default value="3"/>
                      <description>
                        <text>Local detection multiplier.</text>
                      </description>
                    </leaf>
                    <leaf name="min-tx">
                      <type name="uint32">
                        <range value="3..20000"/>
                      </type>
                      <units name="ms"/>
                      <description>
                        <text>Minimum interval for sending BFD packets to the peer end. The ranges and the default value is determined by the PAF.</text>
                      </description>
                    </leaf>
                    <leaf name="min-rx">
                      <type name="uint32">
                        <range value="3..20000"/>
                      </type>
                      <units name="ms"/>
                      <description>
                        <text>Minimum interval for receiving BFD packets from the peer end. The ranges and the default value is determined by the PAF.</text>
                      </description>
                    </leaf>
                    <leaf name="frr-binding">
                      <type name="boolean"/>
                      <default value="false"/>
                      <description>
                        <text>Enable/disable bind FRR with BFD.</text>
                      </description>
                    </leaf>
                    <choice name="incr-cost">
                      <description>
                        <text>Interface BFD sync cost.</text>
                      </description>
                      <case name="incr-cost-block">
                        <description>
                          <text>Interface BFD sync cost block.</text>
                        </description>
                        <leaf name="incr-cost-block">
                          <type name="empty"/>
                          <description>
                            <text>Interface BFD sync cost block.</text>
                          </description>
                        </leaf>
                      </case>
                      <case name="incr-cost-value">
                        <description>
                          <text>Interface BFD sync cost.</text>
                        </description>
                        <leaf name="incr-cost-value">
                          <type name="uint32">
                            <range value="1..65535"/>
                          </type>
                          <description>
                            <text>Interface BFD sync cost.</text>
                          </description>
                        </leaf>
                      </case>
                    </choice>
                    <leaf name="per-link-flag">
                      <type name="boolean"/>
                      <must condition="../one-arm-echo-flag=../per-link-flag and (../per-link-flag='false' or ../enable='true')"/>
                      <default value="false"/>
                      <description>
                        <text>Enable/disable BFD session per link, support only VLANif interface.</text>
                      </description>
                    </leaf>
                    <leaf name="one-arm-echo-flag">
                      <type name="boolean"/>
                      <must condition="../one-arm-echo-flag=../per-link-flag and (../one-arm-echo-flag='false' or /ifm:ifm/ifm:interfaces/ifm:interface[ifm:name=current()/../../name]/ifm:type='Vlanif')"/>
                      <default value="false"/>
                      <description>
                        <text>Enable/disable echo detection mode, support only VLANif interface.</text>
                      </description>
                    </leaf>
                  </container>
                  <container name="authentication">
                    <description>
                      <text>Configure authentication. The encryption algorithm MD5 has a low security, which may bring security risks. Using more secure authentication is recommended.</text>
                    </description>
                    <leaf name="mode">
                      <type name="intf-authen-mode"/>
                      <default value="none"/>
                      <description>
                        <text>Set Authentication type.</text>
                      </description>
                    </leaf>
                    <leaf name="auth-text-simple">
                      <when condition="../mode='simple'"/>
                      <type name="pub-type:password"/>
                      <description>
                        <text>Simple authentication key. The password is a string ranging from 1 to 8 characters for a unencrypted password and 24 to 128 characters for a encrypted password.</text>
                      </description>
                    </leaf>
                    <leaf name="key-id">
                      <when condition="../mode='hmac-md5' or ../mode='hmac-sha256' or ../mode='md5'"/>
                      <type name="uint32">
                        <range value="1..255"/>
                      </type>
                      <must condition="../key-id and ../auth-text-md5"/>
                      <description>
                        <text>Authentication identifier.</text>
                      </description>
                    </leaf>
                    <leaf name="auth-text-md5">
                      <when condition="../mode='hmac-md5' or ../mode='hmac-sha256' or ../mode='md5'"/>
                      <type name="pub-type:password-extend"/>
                      <must condition="../key-id and ../auth-text-md5"/>
                      <description>
                        <text>Encrypted authentication password.</text>
                      </description>
                    </leaf>
                    <leaf name="key-chain-name">
                      <when condition="../mode='keychain'"/>
                      <type name="string">
                        <length value="1..47"/>
                        <pattern value="[^A-Z]+"/>
                      </type>
                      <ext:case-sensitivity value="lower-only"/>
                      <mandatory value="true"/>
                      <description>
                        <text>Keychain name.</text>
                      </description>
                    </leaf>
                  </container>
                  <container name="filter-lsa-out">
                    <description>
                      <text>Configure filter lsa out.</text>
                    </description>
                    <choice name="type">
                      <description>
                        <text>Set filter lsa out type.</text>
                      </description>
                      <case name="total">
                        <description>
                          <text>Filter total types of LSAs.</text>
                        </description>
                        <leaf name="all-flag">
                          <type name="empty"/>
                          <description>
                            <text>Filter all types of LSAs.</text>
                          </description>
                        </leaf>
                      </case>
                      <case name="other">
                        <description>
                          <text>Filter other types of LSAs.</text>
                        </description>
                        <leaf name="sum-flag">
                          <type name="empty"/>
                          <description>
                            <text>Filter type3 summary LSAs.</text>
                          </description>
                        </leaf>
                        <choice name="sum-filter">
                          <description>
                            <text>Type of a filtering rule.</text>
                          </description>
                          <case name="acl">
                            <description>
                              <text>Basic ACL name or ACL number.</text>
                            </description>
                            <leaf name="sum-name">
                              <when condition="../sum-flag"/>
                              <type name="leafref">
                                <path value="/acl:acl/acl:groups/acl:group/acl:identity"/>
                              </type>
                              <must condition="/acl:acl/acl:groups/acl:group[acl:identity=current()]/acl:type='basic'"/>
                              <description>
                                <text>Basic ACL name or ACL number.</text>
                              </description>
                            </leaf>
                          </case>
                        </choice>
                        <leaf name="ase-flag">
                          <type name="empty"/>
                          <description>
                            <text>Filter type5 ASE LSAs.</text>
                          </description>
                        </leaf>
                        <choice name="ase-filter">
                          <description>
                            <text>Type of a filtering rule.</text>
                          </description>
                          <case name="acl">
                            <description>
                              <text>Basic ACL name or ACL number.</text>
                            </description>
                            <leaf name="ase-name">
                              <when condition="../ase-flag"/>
                              <type name="leafref">
                                <path value="/acl:acl/acl:groups/acl:group/acl:identity"/>
                              </type>
                              <must condition="/acl:acl/acl:groups/acl:group[acl:identity=current()]/acl:type='basic'"/>
                              <description>
                                <text>Basic ACL name or ACL number.</text>
                              </description>
                            </leaf>
                          </case>
                        </choice>
                        <leaf name="nssa-flag">
                          <type name="empty"/>
                          <description>
                            <text>Filter type7 NSSA LSAs.</text>
                          </description>
                        </leaf>
                        <choice name="nssa-filter">
                          <description>
                            <text>Type of a filtering rule.</text>
                          </description>
                          <case name="acl">
                            <description>
                              <text>Basic ACL name or ACL number.</text>
                            </description>
                            <leaf name="nssa-name">
                              <when condition="../nssa-flag"/>
                              <type name="leafref">
                                <path value="/acl:acl/acl:groups/acl:group/acl:identity"/>
                              </type>
                              <must condition="/acl:acl/acl:groups/acl:group[acl:identity=current()]/acl:type='basic'"/>
                              <description>
                                <text>Basic ACL name or ACL number.</text>
                              </description>
                            </leaf>
                          </case>
                        </choice>
                      </case>
                    </choice>
                  </container>
                  <container name="suppress-flapping">
                    <description>
                      <text>Configure flapping suppression.</text>
                    </description>
                    <container name="peer">
                      <must condition="resume-interval&gt;detect-interval"/>
                      <description>
                        <text>Configure flapping suppression peer. Not support Loopback.</text>
                      </description>
                      <leaf name="enable">
                        <type name="boolean"/>
                        <default value="true"/>
                        <description>
                          <text>Enable/disable interface peer flapping suppress.</text>
                        </description>
                      </leaf>
                      <leaf name="detect-interval">
                        <type name="uint32">
                          <range value="1..300"/>
                        </type>
                        <default value="60"/>
                        <description>
                          <text>Detecting interval.</text>
                        </description>
                      </leaf>
                      <leaf name="threshold">
                        <type name="uint32">
                          <range value="1..1000"/>
                        </type>
                        <default value="10"/>
                        <description>
                          <text>Threshold.</text>
                        </description>
                      </leaf>
                      <leaf name="resume-interval">
                        <type name="uint32">
                          <range value="2..1000"/>
                        </type>
                        <default value="120"/>
                        <description>
                          <text>Resume interval.</text>
                        </description>
                      </leaf>
                      <leaf name="hold-down-interval">
                        <type name="uint32">
                          <range value="1..86400"/>
                        </type>
                        <description>
                          <text>Hold down interval.</text>
                        </description>
                      </leaf>
                      <leaf name="hold-max-cost">
                        <type name="boolean"/>
                        <default value="true"/>
                        <description>
                          <text>Enable/disable hold max cost.</text>
                        </description>
                      </leaf>
                    </container>
                  </container>
                  <container name="flush-source-trace">
                    <description>
                      <text>Configure OSPF flush source trace function.</text>
                    </description>
                    <leaf name="block">
                      <type name="boolean"/>
                      <default value="false"/>
                      <description>
                        <text>Enable/disable OSPF flush source trace.</text>
                      </description>
                    </leaf>
                  </container>
                  <container name="mpls-te-peers">
                    <description>
                      <text>List of OSPF passive peer.</text>
                    </description>
                    <list name="mpls-te-peer">
                      <key value="id router-id intf-ip"/>
                      <description>
                        <text>Configure OSPF passive peer.</text>
                      </description>
                      <leaf name="id">
                        <type name="uint32">
                          <range value="1..255"/>
                        </type>
                        <description>
                          <text>Neighbor ID.</text>
                        </description>
                      </leaf>
                      <leaf name="router-id">
                        <type name="inet:ipv4-address-no-zone"/>
                        <description>
                          <text>Configure OSPF passive peer router ID.</text>
                        </description>
                      </leaf>
                      <leaf name="intf-ip">
                        <type name="inet:ipv4-address-no-zone"/>
                        <description>
                          <text>Configure interface address of passive peer.</text>
                        </description>
                      </leaf>
                      <container name="procs">
                        <description>
                          <text>List of OSPF passive peers area.</text>
                        </description>
                        <list name="proc">
                          <key value="pid aid"/>
                          <max-elements value="1"/>
                          <description>
                            <text>Configure OSPF passive peers area.</text>
                          </description>
                          <leaf name="pid">
                            <type name="uint32">
                              <range value="1..4294967295"/>
                            </type>
                            <description>
                              <text>Process ID.</text>
                            </description>
                          </leaf>
                          <leaf name="aid">
                            <type name="inet:ipv4-address-no-zone"/>
                            <description>
                              <text>Area ID.</text>
                            </description>
                          </leaf>
                          <leaf name="cost">
                            <type name="uint32">
                              <range value="1..65535"/>
                            </type>
                            <default value="1"/>
                            <description>
                              <text>Configuration cost value.</text>
                            </description>
                          </leaf>
                        </list>
                      </container>
                    </list>
                  </container>
                  <container name="multi-areas">
                    <description>
                      <text>List of the information about the multi-area interface enabled with OSPF.</text>
                    </description>
                    <list name="multi-area">
                      <key value="area-id"/>
                      <description>
                        <text>Configure the interface enabled with OSPF.</text>
                      </description>
                      <leaf name="area-id">
                        <type name="leafref">
                          <path value="/ni:network-instance/ni:instances/ni:instance/ospfv2:ospfv2/ospfv2:sites/ospfv2:site/ospfv2:areas/ospfv2:area/ospfv2:area-id"/>
                        </type>
                        <must condition="../area-id!=../../../../../area-id"/>
                        <description>
                          <text>Multi area id.</text>
                        </description>
                      </leaf>
                      <leaf name="transmit-delay">
                        <type name="int32">
                          <range value="1..500"/>
                        </type>
                        <units name="s"/>
                        <default value="1"/>
                        <description>
                          <text>Delay for transmitting LSAs on an interface.</text>
                        </description>
                      </leaf>
                      <leaf name="mtu-enable">
                        <type name="boolean"/>
                        <default value="false"/>
                        <description>
                          <text>Enable/disable MTU.Configure the interface to fill in the MTU field (actual MTU of the interface) when the interface sends DD packets.
After a virtual connection is established through the virtual template or tunnel, different vendors may use different MTUs as default settings. To ensure consistency, the MTU is set to 0 by default when the interface sends DD packets. Thus, the ospf mtu-enable command usually does not need to be configured. OSPF does not support the preceding configuration on a null interface.</text>
                        </description>
                      </leaf>
                      <leaf name="cost">
                        <type name="uint32">
                          <range value="1..65535"/>
                        </type>
                        <description>
                          <text>To set the cost of an interface. The default value is 1. But the default of Loopback is 0.</text>
                        </description>
                      </leaf>
                      <leaf name="smart-discover">
                        <type name="boolean"/>
                        <default value="false"/>
                        <description>
                          <text>Enable/disable smart discover on an interface. When the neighbor status of a router or a DR or BDR on the multi-address network of the broadcast or NBMA type changes, the router can send Hello packets to the neighbor actively rather than wait for the expiration of the Hello timer.</text>
                        </description>
                      </leaf>
                      <leaf name="ldp-sync-block">
                        <type name="boolean"/>
                        <must condition="../ldp-sync-block='false' or ../ldp-sync-enable='false'"/>
                        <default value="false"/>
                        <description>
                          <text>Enable/disable ldp sync on this interface.</text>
                        </description>
                      </leaf>
                      <leaf name="ldp-sync-enable">
                        <type name="boolean"/>
                        <must condition="../ldp-sync-enable='false' or ../ldp-sync-block='false'"/>
                        <default value="false"/>
                        <description>
                          <text>Enable/disable LDP and OSPF synchronization on an interface.</text>
                        </description>
                      </leaf>
                      <leaf name="fallback-cost">
                        <when condition="/ifm:ifm/ifm:interfaces/ifm:interface[ifm:name=current()/../../../name]/ifm:type='Eth-Trunk'"/>
                        <type name="uint16">
                          <range value="1..65535"/>
                        </type>
                        <description>
                          <text>Fallback cost.</text>
                        </description>
                      </leaf>
                      <leaf name="fallback-bw">
                        <when condition="../fallback-cost"/>
                        <type name="uint32">
                          <range value="1..4294967295"/>
                        </type>
                        <units name="Mbit/s"/>
                        <mandatory value="true"/>
                        <description>
                          <text>Fallback bandwidth threshold.</text>
                        </description>
                      </leaf>
                      <container name="frr">
                        <description>
                          <text>Configure frr.</text>
                        </description>
                        <leaf name="frr-disable">
                          <type name="boolean"/>
                          <default value="false"/>
                          <description>
                            <text>Enable/disable use this interface for FRR calculation.</text>
                          </description>
                        </leaf>
                        <leaf name="rlfa-disable">
                          <type name="boolean"/>
                          <default value="false"/>
                          <description>
                            <text>Enable/disable remote LFA of the Interface.</text>
                          </description>
                        </leaf>
                        <leaf name="ti-lfa-disable">
                          <type name="boolean"/>
                          <default value="false"/>
                          <description>
                            <text>Enable/disable Topology Independent LFA on the interface except Loopback.</text>
                          </description>
                        </leaf>
                      </container>
                      <container name="timer">
                        <description>
                          <text>Configure timer.</text>
                        </description>
                        <leaf name="hello-interval">
                          <type name="int32">
                            <range value="1..65535"/>
                          </type>
                          <units name="s"/>
                          <ext:dynamic-default>
                            <ext:default-value value="10">
                              <when condition="../../../../network-type = 'p2p' or ../../../../network-type = 'broadcast'"/>
                              <description>
                                <text>The default value is 10 when network-type is p2p or broadcast.</text>
                              </description>
                            </ext:default-value>
                            <ext:default-value value="30">
                              <when condition="../../../../network-type = 'p2mp' or ../../../../network-type = 'nbma'"/>
                              <description>
                                <text>The default value is 30 when network-type is p2mp or nbma.</text>
                              </description>
                            </ext:default-value>
                          </ext:dynamic-default>
                          <description>
                            <text>Interval for sending Hello packets on an interface. The default value of p2p and broadcast is 10.The default of p2mp and NBMA is 30.</text>
                          </description>
                        </leaf>
                        <leaf name="dead-interval">
                          <type name="int32">
                            <range value="1..235926000"/>
                          </type>
                          <units name="s"/>
                          <must condition="../hello-interval&lt;../dead-interval"/>
                          <ext:dynamic-default>
                            <ext:default-value value="4*(../hello-interval)">
                              <description>
                                <text>The default value of dead interval is 4 times that of hello interval.</text>
                              </description>
                            </ext:default-value>
                          </ext:dynamic-default>
                          <description>
                            <text>Dead interval of an OSPF neighbor. The default value of p2p and broadcast is 40.The default of p2mp and NBMA is 120.</text>
                          </description>
                        </leaf>
                        <leaf name="retransmit-interval">
                          <type name="int32">
                            <range value="1..3600"/>
                          </type>
                          <default value="5"/>
                          <description>
                            <text>Interval for Retransmitting LSAs on an Interface.</text>
                          </description>
                        </leaf>
                      </container>
                      <container name="ldp-sync">
                        <description>
                          <text>Configure timer ldp sync.</text>
                        </description>
                        <leaf name="hold-down-timer">
                          <type name="uint16">
                            <range value="0..65535"/>
                          </type>
                          <units name="s"/>
                          <default value="10"/>
                          <description>
                            <text>Hold down interval range.</text>
                          </description>
                        </leaf>
                        <choice name="hold-cost-max">
                          <description>
                            <text>Set timer ldp-sync.</text>
                          </description>
                          <case name="timer">
                            <description>
                              <text>Set max cost interval.</text>
                            </description>
                            <leaf name="timer">
                              <type name="uint16">
                                <range value="0..65535"/>
                              </type>
                              <units name="s"/>
                              <description>
                                <text>Max cost interval range.</text>
                              </description>
                            </leaf>
                          </case>
                          <case name="infinite">
                            <description>
                              <text>Always advertise the maximum cost.</text>
                            </description>
                            <leaf name="infinite">
                              <type name="empty"/>
                              <description>
                                <text>Always advertise the maximum cost.</text>
                              </description>
                            </leaf>
                          </case>
                        </choice>
                      </container>
                      <container name="bfd">
                        <description>
                          <text>Configure BFD.</text>
                        </description>
                        <leaf name="block">
                          <type name="boolean"/>
                          <default value="false"/>
                          <description>
                            <text>Enable/disable prevent the dynamic establishment of a BFD session on an interface.</text>
                          </description>
                        </leaf>
                      </container>
                      <container name="authentication">
                        <must condition="(key-id and auth-text-md5) or (not(key-id) and not(auth-text-md5))"/>
                        <description>
                          <text>Configure authentication. The encryption algorithm MD5 has a low security, which may bring security risks. Using more secure authentication is recommended.</text>
                        </description>
                        <leaf name="mode">
                          <type name="intf-authen-mode"/>
                          <default value="none"/>
                          <description>
                            <text>Set Authentication type.</text>
                          </description>
                        </leaf>
                        <leaf name="auth-text-simple">
                          <when condition="../mode='simple'"/>
                          <type name="pub-type:password"/>
                          <description>
                            <text>Simple authentication key. The password is a string ranging from 1 to 8 characters for a unencrypted password and 24 to 128 characters for a encrypted password.</text>
                          </description>
                        </leaf>
                        <leaf name="key-id">
                          <when condition="../mode='hmac-md5' or ../mode='hmac-sha256' or ../mode='md5'"/>
                          <type name="uint32">
                            <range value="1..255"/>
                          </type>
                          <description>
                            <text>Authentication identifier.</text>
                          </description>
                        </leaf>
                        <leaf name="auth-text-md5">
                          <when condition="../mode='hmac-md5' or ../mode='hmac-sha256' or ../mode='md5'"/>
                          <type name="pub-type:password-extend"/>
                          <description>
                            <text>MD5/HMAC-MD5/HMAC-SHA256 Authentication Password.</text>
                          </description>
                        </leaf>
                        <leaf name="key-chain-name">
                          <when condition="../mode='keychain'"/>
                          <type name="string">
                            <length value="1..47"/>
                            <pattern value="[^A-Z]+"/>
                          </type>
                          <ext:case-sensitivity value="lower-only"/>
                          <mandatory value="true"/>
                          <description>
                            <text>Keychain Name.</text>
                          </description>
                        </leaf>
