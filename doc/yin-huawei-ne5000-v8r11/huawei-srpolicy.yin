  <ext:task-name value="srpolicy"/>
  <grouping name="bfd">
    <description>
      <text>SR Policy BFD configuration.</text>
    </description>
    <container name="bfd">
      <description>
        <text>Configure SR Policy BFD.</text>
      </description>
      <leaf name="enable">
        <type name="bfd-enable-type"/>
        <default value="disable"/>
        <description>
          <text>Enable seamless-bfd for all SR Policies.</text>
        </description>
      </leaf>
      <leaf name="min-tx">
        <type name="uint32">
          <range value="1..4294967295"/>
        </type>
        <units name="ms"/>
        <default value="10"/>
        <description>
          <text>Minimum interval for sending seamless-bfd packets to the peer end. The range and default are based on PAF.</text>
        </description>
      </leaf>
      <leaf name="min-rx">
        <type name="uint32">
          <range value="1..4294967295"/>
        </type>
        <units name="ms"/>
        <default value="10"/>
        <description>
          <text>Minimum interval for receiving seamless-bfd packets from the peer end. The range and default are based on PAF.</text>
        </description>
      </leaf>
      <leaf name="multi-num">
        <type name="uint32">
          <range value="3..50"/>
        </type>
        <default value="3"/>
        <description>
          <text>Local detection multiplier.</text>
        </description>
      </leaf>
    </container>
  </grouping>
  <grouping name="global-attributes">
    <description>
      <text>SR Policy global configuration.</text>
    </description>
    <leaf name="switch-delay">
      <type name="uint32">
        <range value="0..600000"/>
      </type>
      <units name="ms"/>
      <default value="5000"/>
      <description>
        <text>MBB switch delay time.</text>
      </description>
    </leaf>
    <leaf name="delete-delay">
      <type name="uint32">
        <range value="0..600000"/>
      </type>
      <units name="ms"/>
      <default value="20000"/>
      <description>
        <text>MBB delete delay time.</text>
      </description>
    </leaf>
    <leaf name="hsb-enable">
      <type name="boolean"/>
      <default value="false"/>
      <description>
        <text>Enable/disable hot standby.</text>
      </description>
    </leaf>
    <leaf name="traffic-statistics-enable">
      <type name="boolean"/>
      <default value="false"/>
      <description>
        <text>Enable/disable traffic statistics collection.</text>
      </description>
    </leaf>
    <leaf name="bgp-ls-enable">
      <type name="boolean"/>
      <default value="false"/>
      <description>
        <text>Enable/disable BGP-LS.</text>
      </description>
    </leaf>
    <leaf name="path-verification">
      <type name="boolean"/>
      <default value="false"/>
      <description>
        <text>Enable/disable path verification.</text>
      </description>
    </leaf>
    <leaf name="suppress-flapping">
      <type name="boolean"/>
      <default value="true"/>
      <description>
        <text>Enable/disable suppress flapping.</text>
      </description>
    </leaf>
    <leaf name="policy-threshold-upper">
      <type name="uint32">
        <range value="2..100"/>
      </type>
      <must condition="(../policy-threshold-upper&gt;../policy-threshold-lower)"/>
      <default value="80"/>
      <description>
        <text>SR Policy threshold upper limit.</text>
      </description>
    </leaf>
    <leaf name="policy-threshold-lower">
      <type name="uint32">
        <range value="1..99"/>
      </type>
      <default value="75"/>
      <description>
        <text>SR Policy threshold lower limit.</text>
      </description>
    </leaf>
    <leaf name="list-threshold-upper">
      <type name="uint32">
        <range value="2..100"/>
      </type>
      <must condition="(../list-threshold-upper&gt;../list-threshold-lower)"/>
      <default value="80"/>
      <description>
        <text>Segment List threshold upper limit.</text>
      </description>
    </leaf>
    <leaf name="list-threshold-lower">
      <type name="uint32">
        <range value="1..99"/>
      </type>
      <default value="75"/>
      <description>
        <text>Segment List threshold lower limit.</text>
      </description>
    </leaf>
    <leaf name="group-threshold-upper">
      <type name="uint32">
        <range value="2..100"/>
      </type>
      <must condition="(../group-threshold-upper&gt;../group-threshold-lower)"/>
      <default value="80"/>
      <description>
        <text>Upper threshold for the number of SR Policy groups.</text>
      </description>
    </leaf>
    <leaf name="group-threshold-lower">
      <type name="uint32">
        <range value="1..99"/>
      </type>
      <default value="75"/>
      <description>
        <text>Lower threshold for the number of SR Policy groups.</text>
      </description>
    </leaf>
    <uses name="bfd"/>
  </grouping>
  <grouping name="srv4-segment-lists">
    <description>
      <text>Global segment lists.</text>
    </description>
    <container name="segment-lists">
      <description>
        <text>List of global segment lists.</text>
      </description>
      <list name="segment-list">
        <key value="name"/>
        <description>
          <text>Configure a global segment list.</text>
        </description>
        <leaf name="name">
          <type name="string">
            <length value="1..31"/>
            <pattern value="[^ \?]*"/>
          </type>
          <description>
            <text>Global segment list name.</text>
          </description>
        </leaf>
        <container name="segments">
          <description>
            <text>List of segments in a global segment list.</text>
          </description>
          <list name="segment">
            <key value="index"/>
            <max-elements value="10"/>
            <description>
              <text>Configure a segment in a global segment list.</text>
            </description>
            <leaf name="index">
              <type name="uint32">
                <range value="0..4294967295"/>
              </type>
              <description>
                <text>The segment index.</text>
              </description>
            </leaf>
            <choice name="type">
              <mandatory value="true"/>
              <description>
                <text>The segment type.</text>
              </description>
              <case name="segment-type-1">
                <description>
                  <text>The segment is declared by MPLS label.</text>
                </description>
                <leaf name="sid-value-1">
                  <type name="uint32">
                    <range value="16..1048575"/>
                  </type>
                  <description>
                    <text>MPLS label value.</text>
                  </description>
                </leaf>
              </case>
            </choice>
          </list>
        </container>
      </list>
    </container>
  </grouping>
  <grouping name="srv6-segment-lists">
    <description>
      <text>Global segment lists.</text>
    </description>
    <container name="segment-lists">
      <description>
        <text>List of global segment lists.</text>
      </description>
      <list name="segment-list">
        <key value="name"/>
        <description>
          <text>Configure a global segment list.</text>
        </description>
        <leaf name="name">
          <type name="string">
            <length value="1..31"/>
            <pattern value="[^ \?]*"/>
          </type>
          <description>
            <text>Global segment list name.</text>
          </description>
        </leaf>
        <container name="segments">
          <description>
            <text>List of segments in a global segment list.</text>
          </description>
          <list name="segment">
            <key value="index"/>
            <max-elements value="10"/>
            <description>
              <text>Configure a segment in a global segment list.</text>
            </description>
            <leaf name="index">
              <type name="uint32">
                <range value="0..4294967295"/>
              </type>
              <description>
                <text>The segment index.</text>
              </description>
            </leaf>
            <choice name="type">
              <mandatory value="true"/>
              <description>
                <text>The segment type.</text>
              </description>
              <case name="segment-type-2">
                <description>
                  <text>The segment is declared by SRv6 SID value.</text>
                </description>
                <leaf name="sid-value-2">
                  <type name="inet:ipv6-address-no-zone"/>
                  <description>
                    <text>SRv6 sid value.</text>
                  </description>
                </leaf>
              </case>
            </choice>
          </list>
        </container>
      </list>
    </container>
  </grouping>
  <grouping name="srv6-policy-attributes">
    <description>
      <text>Grouping containing attributes applicable to all SR policies.</text>
    </description>
    <container name="policy-attributes">
      <description>
        <text>Configure attributes for all SR policies.</text>
      </description>
      <uses name="global-attributes"/>
      <leaf name="locator">
        <ext:operation-exclude value="update"/>
        <type name="leafref">
          <path value="/ni:network-instance/ni:instances/ni:instance/srv6:srv6/srv6:locators/srv6:locator/srv6:name"/>
        </type>
        <description>
          <text>Locator Name.</text>
        </description>
      </leaf>
      <uses name="srv6-segment-lists"/>
    </container>
  </grouping>
  <grouping name="srv4-policy-attributes">
    <description>
      <text>Grouping containing attributes applicable to all SR policies.</text>
    </description>
    <container name="policy-attributes">
      <description>
        <text>Configure attributes for all SR policies.</text>
      </description>
      <uses name="global-attributes"/>
      <uses name="srv4-segment-lists"/>
    </container>
  </grouping>
  <grouping name="candidate-paths">
    <description>
      <text>Grouping containing candidate paths to all SR policies.</text>
    </description>
    <container name="candidate-paths">
      <description>
        <text>List of candidate paths in an SR Policy.</text>
      </description>
      <list name="candidate-path">
        <must condition="(discriminator=preference)"/>
        <key value="protocol-origin originator discriminator"/>
        <max-elements value="64"/>
        <description>
          <text>Configure a candidate path.</text>
        </description>
        <leaf name="protocol-origin">
          <type name="protocol-origin-type"/>
          <must condition="(../protocol-origin='configuration')"/>
          <description>
            <text>Candidate path protocol origin.</text>
          </description>
        </leaf>
        <leaf name="originator">
          <type name="string">
            <length value="1..60"/>
          </type>
          <must condition="(../originator='0,0.0.0.0')"/>
          <description>
            <text>Candidate path originator.</text>
          </description>
        </leaf>
        <leaf name="discriminator">
          <type name="uint32">
            <range value="1..65535"/>
          </type>
          <description>
            <text>Candidate path discriminator.</text>
          </description>
        </leaf>
        <leaf name="preference">
          <type name="uint32">
            <range value="1..65535"/>
          </type>
          <mandatory value="true"/>
          <description>
            <text>Candidate path preference.</text>
          </description>
        </leaf>
        <choice name="type">
          <description>
            <text>Type of candidate paths.</text>
          </description>
          <case name="explicit">
            <description>
              <text>Candidate path with explicitly defined set/s of segment-lists.</text>
            </description>
            <container name="segment-lists">
              <description>
                <text>List of segment lists in a candidate path.</text>
              </description>
              <list name="segment-list">
                <key value="name"/>
                <max-elements value="64"/>
                <description>
                  <text>Configure a segment list for a candidate path.</text>
                </description>
                <leaf name="name">
                  <type name="leafref">
                    <path value="../../../../../../../policy-attributes/segment-lists/segment-list/name"/>
                  </type>
                  <description>
                    <text>Segment list name.</text>
                  </description>
                </leaf>
                <leaf name="weight">
                  <type name="uint32">
                    <range value="1..4294967295"/>
                  </type>
                  <default value="1"/>
                  <description>
                    <text>Segment-list weighted loadshare.</text>
                  </description>
                </leaf>
                <leaf name="path-mtu">
                  <type name="uint32">
                    <range value="1280..9600"/>
                  </type>
                  <description>
                    <text>Segment-list path-mtu.</text>
                  </description>
                </leaf>
              </list>
            </container>
          </case>
        </choice>
      </list>
    </container>
  </grouping>
  <grouping name="srv4-policys">
    <description>
      <text>SR policy grouping.</text>
    </description>
    <container name="policys">
      <description>
        <text>List of SR Policies.</text>
      </description>
      <list name="policy">
        <key value="endpoint color"/>
        <unique tag="name"/>
        <unique tag="binding-sid/value"/>
        <description>
          <text>Configure an SR Policy.</text>
        </description>
        <leaf name="endpoint">
          <type name="inet:ip-address"/>
          <description>
            <text>SR Policy endpoint address.</text>
          </description>
        </leaf>
        <leaf name="color">
          <type name="uint32">
            <range value="0..4294967295"/>
          </type>
          <description>
            <text>SR Policy color.</text>
          </description>
        </leaf>
        <leaf name="name">
          <type name="string">
            <length value="1..31"/>
            <pattern value="[^ \?]*"/>
          </type>
          <mandatory value="true"/>
          <description>
            <text>SR Policy name.</text>
          </description>
        </leaf>
        <container name="binding-sid">
          <description>
            <text>Configure a binding SID.</text>
          </description>
          <leaf name="dataplane">
            <type name="dataplane-type"/>
            <must condition="(../dataplane='mpls')"/>
            <mandatory value="true"/>
            <description>
              <text>Binding sid dataplane type.</text>
            </description>
          </leaf>
          <leaf name="value">
            <type name="sid-value-type"/>
            <mandatory value="true"/>
            <description>
              <text>Binding sid value.</text>
            </description>
          </leaf>
        </container>
        <leaf name="admin-state">
          <type name="admin-state-type"/>
          <default value="up"/>
          <description>
            <text>Close the function of SR Policy.</text>
          </description>
        </leaf>
        <leaf name="mtu">
          <type name="uint32">
            <range value="512..9600"/>
          </type>
          <description>
            <text>SR Policy mtu value.</text>
          </description>
        </leaf>
        <leaf name="hsb-enable">
          <type name="enable-3types"/>
          <default value="not-configured"/>
          <description>
            <text>SR Policy hot standby.</text>
          </description>
        </leaf>
        <leaf name="bfd-enable">
          <type name="policy-bfd-enable-type"/>
          <default value="not-configured"/>
          <description>
            <text>SR Policy BFD.</text>
          </description>
        </leaf>
        <leaf name="traffic-statistics-enable">
          <type name="enable-3types"/>
          <default value="not-configured"/>
          <description>
            <text>SR Policy traffic statistics.</text>
          </description>
        </leaf>
        <container name="diffserv-mode">
          <description>
            <text>Configure the DiffServ mode in an SR Policy.</text>
          </description>
          <leaf name="pipe-mode">
            <type name="pipe-mode-type"/>
            <description>
              <text>Specifies the pipe mode of SR Policy.</text>
            </description>
          </leaf>
          <leaf name="service-class">
            <when condition="../pipe-mode='pipe'"/>
            <type name="serv-class-types"/>
            <mandatory value="true"/>
            <description>
              <text>Service class of SR Policy.</text>
            </description>
          </leaf>
          <leaf name="service-color">
            <when condition="../pipe-mode='pipe'"/>
            <type name="serv-color-types"/>
            <mandatory value="true"/>
            <description>
              <text>Service color of SR Policy.</text>
            </description>
          </leaf>
        </container>
        <uses name="candidate-paths"/>
      </list>
    </container>
  </grouping>
