  <container name="pcc">
    <presence value="Enter pce-client view."/>
    <description>
      <text>Path Computation Client (PCC).</text>
    </description>
    <container name="attribute">
      <description>
        <text>Configure attribute of a PCE client.</text>
      </description>
      <leaf name="sr-capability">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable segment routing capability.</text>
        </description>
      </leaf>
      <leaf name="srv6-capability">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable segment routing IPv6 capability.</text>
        </description>
      </leaf>
      <leaf name="multi-delegate">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable Multi-delegate support.</text>
        </description>
      </leaf>
      <leaf name="stitch-label-capability">
        <when condition="../sr-capability='true'"/>
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable Label Stitch capability.</text>
        </description>
      </leaf>
      <leaf name="initiated-capability">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable PCE initiated capability.</text>
        </description>
      </leaf>
      <leaf name="association-type-enable">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable New Association based on IETF.</text>
        </description>
      </leaf>
    </container>
    <container name="timer">
      <must condition="hold-time&gt;=keepalive-time"/>
      <description>
        <text>Configure timer of PCE client.</text>
      </description>
      <leaf name="hold-time">
        <type name="uint16">
          <range value="0..255"/>
        </type>
        <units name="s"/>
        <default value="120"/>
        <description>
          <text>Time to declare PCE session dead.</text>
        </description>
      </leaf>
      <leaf name="keepalive-time">
        <type name="uint16">
          <range value="0..255"/>
        </type>
        <units name="s"/>
        <default value="30"/>
        <description>
          <text>Time to send Keepalive messages on active PCE sessions.</text>
        </description>
      </leaf>
      <leaf name="delegate-hold-time">
        <type name="uint16">
          <range value="30..255"/>
        </type>
        <units name="s"/>
        <default value="30"/>
        <description>
          <text>LSP delegate timer sets the LSP delegate keep
time when the connection with server is break or when the higher preference server session is Up.</text>
        </description>
      </leaf>
      <leaf name="state-timeout-time">
        <type name="uint16">
          <range value="0..1800"/>
        </type>
        <units name="s"/>
        <default value="30"/>
        <description>
          <text>Time to declare PCE learnt LSP state as invalid.</text>
        </description>
      </leaf>
    </container>
    <container name="connect-servers">
      <description>
        <text>List of connect-servers of a PCE client.</text>
      </description>
      <list name="connect-server">
        <key value="ip-address"/>
        <description>
          <text>Configure connect-server of a PCE client.</text>
        </description>
        <leaf name="ip-address">
          <type name="inet:ip-address"/>
          <description>
            <text>IP address of a PCE.</text>
          </description>
        </leaf>
        <leaf name="preference">
          <type name="uint32">
            <range value="0..7"/>
          </type>
          <default value="0"/>
          <description>
            <text>Configure preference of connect-server.</text>
          </description>
        </leaf>
        <leaf name="dscp">
          <type name="uint32">
            <range value="0..63"/>
          </type>
          <must condition="contains(../ip-address,':') or (not(contains(../ip-address,':')) and ../dscp&gt;=0 and ../dscp&lt;=7)"/>
          <default value="6"/>
          <description>
            <text>DSCP.</text>
          </description>
        </leaf>
        <container name="authentication">
          <description>
            <text>Configure authentication on PCE client.</text>
          </description>
          <choice name="auth-type-selection">
            <description>
              <text>Options for expressing authentication setting.</text>
            </description>
            <case name="auth-key-chain">
              <description>
                <text>Auth key chain.</text>
              </description>
              <leaf name="keychain-name">
                <type name="string">
                  <length value="1..47"/>
                  <pattern value="[^A-Z]+"/>
                </type>
                <ext:case-sensitivity value="lower-only"/>
                <description>
                  <text>Keychain name.</text>
                </description>
              </leaf>
            </case>
          </choice>
        </container>
      </list>
    </container>
    <container name="threshold-alarm">
      <description>
        <text>Configure alarm threshold.</text>
      </description>
      <container name="pce-initiated-lsp-number">
        <must condition="upper-value&gt;lower-value"/>
        <description>
          <text>Configure alarm threshold for PCEP-initiated LSPs.</text>
        </description>
        <leaf name="upper-value">
          <type name="uint32">
            <range value="2..100"/>
          </type>
          <default value="85"/>
          <description>
            <text>Upper threshold.</text>
          </description>
        </leaf>
        <leaf name="lower-value">
          <type name="uint32">
            <range value="1..99"/>
          </type>
          <default value="75"/>
          <description>
            <text>Lower threshold.</text>
          </description>
        </leaf>
      </container>
    </container>
    <container name="lsps">
      <config value="false"/>
      <description>
        <text>List of PCE LSP information on client.</text>
      </description>
      <list name="lsp">
        <key value="symbolic-path-name plsp-id"/>
        <description>
          <text>Operational data of PCE LSP information on client.</text>
        </description>
        <leaf name="symbolic-path-name">
          <type name="string">
            <length value="1..256"/>
          </type>
          <description>
            <text>Symbolic path name.</text>
          </description>
        </leaf>
        <leaf name="plsp-id">
          <type name="uint32">
            <range value="1..1048574"/>
          </type>
          <description>
            <text>PLSP ID.</text>
          </description>
        </leaf>
        <leaf name="tunnel-id">
          <type name="uint16"/>
          <description>
            <text>Tunnel ID.</text>
          </description>
        </leaf>
        <leaf name="lsp-id">
          <type name="uint16"/>
          <description>
            <text>LSP ID.</text>
          </description>
        </leaf>
        <leaf name="state">
          <type name="pce-client-lsp-state-type"/>
          <description>
            <text>LSP state.</text>
          </description>
        </leaf>
        <leaf name="initiated-type">
          <type name="pce-client-lsp-init-type"/>
          <description>
            <text>LSP init type.</text>
          </description>
        </leaf>
        <leaf name="ingress-address">
          <type name="inet:ip-address"/>
          <description>
            <text>Ingress IP address.</text>
          </description>
        </leaf>
        <leaf name="egress-address">
          <type name="inet:ip-address"/>
          <description>
            <text>Egress IP address.</text>
          </description>
        </leaf>
        <leaf name="delegate-flag">
          <type name="pce-client-lsp-delegate-state"/>
          <description>
            <text>Delegation state.</text>
          </description>
        </leaf>
      </list>
    </container>
    <container name="sessions">
      <config value="false"/>
      <description>
        <text>List of PCE session information on client.</text>
      </description>
      <list name="session">
        <key value="ip-address"/>
        <description>
          <text>Operational data of PCE session information on client.</text>
        </description>
        <leaf name="ip-address">
          <type name="inet:ip-address-no-zone"/>
          <description>
            <text>IP address of a PCE session.</text>
          </description>
        </leaf>
        <leaf name="msd">
          <type name="uint8"/>
          <description>
            <text>Maximum segment depth.</text>
          </description>
        </leaf>
        <leaf name="state">
          <type name="pce-client-fsm-state"/>
          <description>
            <text>PCE session status.</text>
          </description>
        </leaf>
        <leaf name="pce-status">
          <type name="pce-server-status-type"/>
          <description>
            <text>PCE server status.</text>
          </description>
        </leaf>
        <leaf name="session-id">
          <type name="uint8"/>
          <description>
            <text>ID of a PCE session.</text>
          </description>
        </leaf>
        <leaf name="ssl-policy-name">
          <type name="string">
            <length value="1..23"/>
          </type>
          <description>
            <text>Ssl policy name.</text>
          </description>
        </leaf>
        <leaf name="is-preferred">
          <type name="boolean"/>
          <description>
            <text>Preferred session.</text>
          </description>
        </leaf>
        <leaf name="hold-time">
          <type name="uint8"/>
          <units name="s"/>
          <description>
            <text>Hold time of PCE sessions.</text>
          </description>
        </leaf>
        <leaf name="keepalive-time">
          <type name="uint8"/>
          <units name="s"/>
          <description>
            <text>Keepalive time of PCE sessions.</text>
          </description>
        </leaf>
        <leaf name="peer-hold-time">
          <type name="uint8"/>
          <units name="s"/>
          <description>
            <text>Hold time of PCE sessions on peer.</text>
          </description>
        </leaf>
        <leaf name="peer-keepalive-time">
          <type name="uint8"/>
          <units name="s"/>
          <description>
            <text>Keepalive time of PCE sessions on peer.</text>
          </description>
        </leaf>
        <leaf name="session-active-time">
          <type name="yang:date-and-time"/>
          <description>
            <text>Time when session became active.</text>
          </description>
        </leaf>
        <leaf name="session-preference">
          <type name="uint8"/>
          <description>
            <text>Session preference value.</text>
          </description>
        </leaf>
        <leaf name="stitch-label-base">
          <type name="uint32"/>
          <description>
            <text>Label base value.</text>
          </description>
        </leaf>
        <leaf name="stitch-label-size">
          <type name="uint32"/>
          <description>
            <text>Number of stitch labels.</text>
          </description>
        </leaf>
        <leaf name="last-down-reason">
          <type name="pce-client-ssn-down-reason-type"/>
          <description>
            <text>Reason of PCE session down.</text>
          </description>
        </leaf>
        <container name="statistics">
          <description>
            <text>Statistics of PCE sessions.</text>
          </description>
          <leaf name="open-msg-recv-count">
            <type name="uint32"/>
            <description>
              <text>Number of open messages received.</text>
            </description>
          </leaf>
          <leaf name="open-msg-sent-count">
            <type name="uint32"/>
            <description>
              <text>Number of open messages sent.</text>
            </description>
          </leaf>
          <leaf name="keepalive-msg-recv-count">
            <type name="uint32"/>
            <description>
              <text>Number of keepalive messages received.</text>
            </description>
          </leaf>
          <leaf name="keepalive-msg-sent-count">
            <type name="uint32"/>
            <description>
              <text>Number of keepalive messages sent.</text>
            </description>
          </leaf>
          <leaf name="lsp-update-recv-count">
            <type name="uint32"/>
            <description>
              <text>Number of LSP update messages received.</text>
            </description>
          </leaf>
          <leaf name="lsp-update-sent-count">
            <type name="uint32"/>
            <description>
              <text>Number of LSP update messages sent.</text>
            </description>
          </leaf>
          <leaf name="lsp-report-recv-count">
            <type name="uint32"/>
            <description>
              <text>Number of LSP report messages received.</text>
            </description>
          </leaf>
          <leaf name="lsp-report-sent-count">
            <type name="uint32"/>
            <description>
              <text>Number of LSP report messages sent.</text>
            </description>
          </leaf>
          <leaf name="lsp-init-recv-count">
            <type name="uint32"/>
            <description>
              <text>Number of LSP initiate messages received.</text>
            </description>
          </leaf>
          <leaf name="lsp-init-sent-count">
            <type name="uint32"/>
            <description>
              <text>Number of LSP initiate messages sent.</text>
            </description>
          </leaf>
          <leaf name="pce-error-msg-recv-count">
            <type name="uint32"/>
            <description>
              <text>Number of path computation error messages received.</text>
            </description>
          </leaf>
          <leaf name="pce-error-msg-sent-count">
            <type name="uint32"/>
