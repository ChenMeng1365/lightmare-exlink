  <feature name="strict-policy">
    <description>
      <text>Strict policy.</text>
    </description>
  </feature>
  <container name="configuration">
    <description>
      <text>Route-map's configuration.</text>
    </description>
    <container name="route-map">
      <description>
        <text>Routing policy's module.</text>
      </description>
      <container name="rmaps">
        <description>
          <text>Routing policy's list.</text>
        </description>
        <list name="rmap">
          <description>
            <text>Routing policy's list.</text>
          </description>
          <key value="rmap-name rseq-no"/>
          <leaf name="rmap-name">
            <description>
              <text>Routing Policy's name.</text>
            </description>
            <type name="string">
              <length value="1..200">
                <description>
                  <text>The length of rmap name is from 1 to 200.</text>
                </description>
              </length>
            </type>
          </leaf>
          <leaf name="rseq-no">
            <description>
              <text>Routing Policy's sequence.</text>
            </description>
            <type name="uint16">
              <range value="0..65535">
                <description>
                  <text>The range of routemap-sequence is from 0 to 65535</text>
                </description>
              </range>
            </type>
          </leaf>
          <leaf name="deny-permit">
            <description>
              <text>Deny  : Route map denies set operations and specify packets to reject.
Permit: Route map permits set operations and specify packets to forward.</text>
            </description>
            <type name="enumeration">
              <enum name="deny">
                <value value="1"/>
                <description>
                  <text>Deny  : Route map denies set operations and specify packets to reject.</text>
                </description>
              </enum>
              <enum name="permit">
                <value value="0"/>
                <description>
                  <text>Permit: Route map permits set operations and specify packets to forward</text>
                </description>
              </enum>
            </type>
            <default value="permit"/>
          </leaf>
          <container name="description">
            <description>
              <text>Description of the route map (1-199).</text>
            </description>
            <presence value="description"/>
            <leaf name="description">
              <description>
                <text>Description of the route map (1-199).</text>
              </description>
              <type name="ncd:string-with-blank">
                <length value="1..199"/>
              </type>
              <mandatory value="true"/>
            </leaf>
            <ncd:revise-date date="2018-11-27"/>
          </container>
          <container name="match">
            <description>
              <text>Routing Policy's condition.</text>
            </description>
            <presence value="match"/>
            <container name="as-paths">
              <description>
                <text>Match BGP as-path list.</text>
              </description>
              <list name="as-path">
                <description>
                  <text>Match BGP as-path list.</text>
                </description>
                <max-elements value="10"/>
                <key value="name"/>
                <leaf name="name">
                  <description>
                    <text>Match BGP's as-path list.</text>
                  </description>
                  <type name="uint16">
                    <range value="1..256">
                      <description>
                        <text>The range of as-path number is from 0 to 256.</text>
                      </description>
                    </range>
                  </type>
                </leaf>
              </list>
            </container>
            <container name="as-path-names">
              <description>
                <text>Match BGP as-path list name.</text>
              </description>
              <list name="as-path-name">
                <description>
                  <text>Match BGP as-path list name.</text>
                </description>
                <max-elements value="10"/>
                <key value="name"/>
                <leaf name="name">
                  <description>
                    <text>As-path list name.</text>
                  </description>
                  <type name="string">
                    <length value="1..31"/>
                  </type>
                </leaf>
              </list>
              <ncd:revise-date date="2018-02-09"/>
            </container>
            <container name="as-path-len">
              <description>
                <text>Match BGP as-path-length.</text>
              </description>
              <presence value="as-path-length is not Mandatory"/>
              <leaf name="alg">
                <description>
                  <text>Eq:Equal as-path length to be matched.
Ge:Minimum as-path length to be matched.
Le:Maximun as-path length to be matched.</text>
                </description>
                <type name="enumeration">
