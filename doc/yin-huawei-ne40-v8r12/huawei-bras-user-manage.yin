    </description>
    <input>
      <leaf name="start-ip-address">
        <type name="inet:ipv4-address-no-zone"/>
        <mandatory value="true"/>
        <description>
          <text>Start IP address.</text>
        </description>
      </leaf>
      <leaf name="end-ip-address">
        <type name="inet:ipv4-address-no-zone"/>
        <description>
          <text>End IP address.</text>
        </description>
      </leaf>
      <leaf name="vpn-name">
        <type name="string">
          <length value="1..31"/>
        </type>
        <description>
          <text>Name of a specified VPN instance.</text>
        </description>
      </leaf>
    </input>
  </rpc>
  <rpc name="cut-access-user-by-condition">
    <description>
