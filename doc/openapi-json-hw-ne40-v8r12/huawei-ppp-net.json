    "definitions": {
        "ppp-net_schema": {
            "description": "PPP net.",
            "properties": {
                "ppp-dampening": {
                    "description": "Enable/disable type of PPP damp. If dampen-level is set to manual, the settings of the other parameters need to meet the following requirement: suppress-value/1000 <= (reuse-value/1000) * 2^ (max-suppress-time/half-life-period).",
                    "properties": {
                        "dampen-level": {
                            "type": "string",
                            "enum": [
                                "light",
                                "middle",
                                "heavy",
                                "manual"
                            ],
                            "description": "PPP protocol status damping level."
                        },
                        "half-life-period": {
                            "type": "string",
                            "description": "PPP protocol status damping half life time (seconds)"
                        },
                        "suppress-value": {
                            "type": "string",
                            "description": "1000 times of PPP protocol status damping suppress threshold."
                        },
                        "reuse-value": {
                            "type": "string",
                            "description": "1000 times of PPP protocol status damping reuse threshold."
                        },
                        "max-suppress-time": {
                            "type": "string",
                            "description": "PPP protocol status damping max suppress time."
                        }
                    }
