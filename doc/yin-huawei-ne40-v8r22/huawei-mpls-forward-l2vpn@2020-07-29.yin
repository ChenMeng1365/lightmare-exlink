<?xml version="1.0" encoding="UTF-8"?>
<module name="huawei-mpls-forward-l2vpn"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:mpls-forward-l2vpn="urn:huawei:yang:huawei-mpls-forward-l2vpn"
        xmlns:l2vpn="urn:huawei:yang:huawei-l2vpn"
        xmlns:ext="urn:huawei:yang:huawei-extension">
  <yang-version value="1"/>
  <namespace uri="urn:huawei:yang:huawei-mpls-forward-l2vpn"/>
  <prefix value="mpls-forward-l2vpn"/>
  <import module="huawei-l2vpn">
    <prefix value="l2vpn"/>
  </import>
  <import module="huawei-extension">
    <prefix value="ext"/>
  </import>
  <organization>
    <text>Huawei Technologies Co., Ltd.</text>
  </organization>
  <contact>
    <text>Huawei Industrial Base
Bantian, Longgang
Shenzhen 518129
People's Republic of China
Website: http://www.huawei.com
Email: support@huawei.com</text>
  </contact>
  <description>
    <text>MPLS forward for L2VPN.</text>
  </description>
  <revision date="2020-07-29">
    <description>
      <text>Initial revision.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <augment target-node="/l2vpn:l2vpn/l2vpn:instances/l2vpn:instance/l2vpn:vpls/l2vpn:ipmsi/l2vpn:leaf">
    <description>
      <text>Configure a MPLS P2MP loopback board.</text>
    </description>
    <container name="mpls-p2mp-slot">
      <description>
        <text>Configure a MPLS P2MP loopback board.</text>
      </description>
      <leaf name="slotid">
        <type name="string">
          <length value="1..32"/>
        </type>
        <description>
          <text>Slot ID.</text>
        </description>
      </leaf>
    </container>
  </augment>
  <ext:task-name value="forwarding"/>
</module>
