  <container name="configuration">
    <description>
      <text>EFM configuration</text>
    </description>
    <container name="efm">
      <description>
        <text>Ethernet in the first mile</text>
      </description>
      <container name="global">
        <description>
          <text>EFM global configuration</text>
        </description>
        <leaf name="adminstate">
          <description>
            <text>EFM global enable switch</text>
          </description>
          <type name="enumeration">
            <enum name="enable">
              <value value="1"/>
              <description>
                <text>Enable Ethernet OAM global function</text>
              </description>
            </enum>
            <enum name="disable">
              <value value="2"/>
              <description>
                <text>Disable Ethernet OAM global function</text>
              </description>
            </enum>
          </type>
          <default value="disable"/>
        </leaf>
        <leaf name="rlbtimeout">
          <description>
            <text>EFM loopback time-out,range from 1 to 10,unit:second</text>
          </description>
          <type name="uint8">
            <range value="1..10">
              <description>
                <text>Range from 1 to 10</text>
              </description>
            </range>
          </type>
          <units name="second"/>
          <default value="3"/>
        </leaf>
        <leaf name="oui">
          <description>
            <text>EFM Organizationally Unique Identifier</text>
          </description>
          <type name="string">
            <length value="1..3">
              <description>
                <text>Range from 1 to 3</text>
              </description>
            </length>
          </type>
          <default value="ZTE"/>
        </leaf>
      </container>
      <container name="base-interfaces">
        <description>
          <text>EFM base interface configuration</text>
        </description>
        <list name="base-interface">
          <description>
            <text>EFM base interface configuration</text>
          </description>
          <key value="if-name"/>
          <leaf name="if-name">
            <description>
              <text>Interface name</text>
            </description>
            <type name="ncd:ifname"/>
          </leaf>
          <leaf name="ifenable">
            <description>
              <text>Enable interface EFM function</text>
            </description>
            <type name="enumeration">
              <enum name="enable">
                <value value="1"/>
                <description>
                  <text>Enable interface Ethernet OAM function</text>
                </description>
              </enum>
              <enum name="disable">
                <value value="2"/>
                <description>
                  <text>Disable interface Ethernet OAM function</text>
                </description>
              </enum>
            </type>
            <default value="disable"/>
          </leaf>
          <leaf name="ifoammode">
            <description>
              <text>Configure EFM OAM mode</text>
            </description>
            <type name="enumeration">
              <enum name="active">
                <value value="1"/>
                <description>
                  <text>Set Ethernet OAM active mode</text>
                </description>
              </enum>
              <enum name="passive">
                <value value="2"/>
                <description>
                  <text>Set Ethernet OAM passive mode</text>
                </description>
              </enum>
            </type>
            <default value="active"/>
          </leaf>
          <leaf name="ifperiod">
            <description>
              <text>EFM PDU transmitting period, range from 1 to 10, unit:100ms</text>
            </description>
            <type name="uint8">
              <range value="1..10">
                <description>
                  <text>Range from 1 to 10</text>
                </description>
              </range>
            </type>
            <units name="100ms"/>
            <default value="10"/>
          </leaf>
          <leaf name="iflktimeoutfast">
            <description>
              <text>Configure fast link timeout,range from 3 to 200, unit:100ms</text>
            </description>
            <type name="uint8">
              <range value="3..200">
                <description>
                  <text>Range from 3 to 200</text>
                </description>
              </range>
            </type>
            <units name="100ms"/>
            <default value="50"/>
          </leaf>
          <leaf name="orgspectlv">
            <description>
              <text>Configure EFM capability of carrying Organization Specific Information TLV</text>
            </description>
            <type name="enumeration">
              <enum name="enable">
                <value value="1"/>
                <description>
                  <text>Enable Organization Specific TLV sending</text>
                </description>
              </enum>
              <enum name="disable">
                <value value="2"/>
                <description>
                  <text>Disable Organization Specific TLV sending</text>
                </description>
              </enum>
            </type>
            <default value="enable"/>
          </leaf>
          <leaf name="rmtlbmode">
            <description>
              <text>Configure EFM capability of responding to remote loopback request</text>
            </description>
            <type name="enumeration">
              <enum name="enable">
                <value value="1"/>
                <description>
                  <text>Enable Ethernet OAM be in remote loopback mode</text>
                </description>
              </enum>
              <enum name="disable">
                <value value="2"/>
                <description>
                  <text>Disable Ethernet OAM be in remote loopback mode</text>
                </description>
              </enum>
            </type>
            <default value="enable"/>
          </leaf>
          <leaf name="rmtlbguard">
            <description>
              <text>Configure EFM guard mode of disabling remote loopback mode from attack</text>
            </description>
            <type name="enumeration">
              <enum name="enable">
                <value value="1"/>
                <description>
                  <text>Enable guard mode of recoverying from LB to FWD</text>
                </description>
              </enum>
              <enum name="disable">
                <value value="2"/>
                <description>
                  <text>Disable guard mode of recoverying from LB to FWD</text>
                </description>
              </enum>
            </type>
            <default value="disable"/>
          </leaf>
          <leaf name="vabretrieval">
            <description>
              <text>Configure EFM capability of responding to variable request</text>
            </description>
            <type name="enumeration">
              <enum name="enable">
                <value value="1"/>
                <description>
                  <text>Enable variable-response function</text>
                </description>
              </enum>
              <enum name="disable">
                <value value="2"/>
                <description>
                  <text>Disable variable-response function</text>
                </description>
              </enum>
            </type>
            <default value="enable"/>
          </leaf>
        </list>
      </container>
      <container name="linkmonitor-intfs">
        <description>
          <text>Configure EFM parameters of link monitor</text>
        </description>
        <list name="linkmonitor-intf">
          <description>
            <text>Configure EFM parameters of link monitor</text>
          </description>
          <key value="if-name"/>
          <leaf name="if-name">
            <description>
              <text>Interface name</text>
            </description>
            <type name="ncd:ifname"/>
          </leaf>
          <leaf name="iflmenable">
            <description>
              <text>Configure EFM linkmonitor enable/disable</text>
            </description>
            <type name="enumeration">
              <enum name="enable">
                <value value="1"/>
                <description>
                  <text>Enable Ethernet OAM link monitor function</text>
                </description>
              </enum>
              <enum name="disable">
                <value value="2"/>
                <description>
                  <text>Disable Ethernet OAM link monitor function</text>
                </description>
              </enum>
            </type>
            <default value="disable"/>
          </leaf>
          <leaf name="errsymthr">
            <description>
              <text>Configure symbol period threshold,units:symbol</text>
            </description>
            <type name="uint32">
              <range value="1..65535">
                <description>
                  <text>Range from 1 to 65535</text>
                </description>
              </range>
            </type>
            <units name="symbol"/>
            <default value="1"/>
          </leaf>
          <leaf name="errsymwin">
            <description>
              <text>Configure symbol period window,unit:million symbols</text>
            </description>
            <type name="uint32">
              <range value="1..65535">
                <description>
                  <text>Range from 1 to 65535</text>
                </description>
              </range>
            </type>
            <units name="million symbols"/>
            <default value="1"/>
          </leaf>
          <leaf name="errframethr">
            <description>
              <text>Configure frame event threshold,unit:frame</text>
            </description>
            <type name="uint32">
              <range value="1..65535">
                <description>
                  <text>Range from 1 to 65535</text>
                </description>
              </range>
            </type>
            <units name="frame"/>
            <default value="1"/>
          </leaf>
          <leaf name="errframewin">
            <description>
              <text>Configure frame event window,unit:second</text>
            </description>
            <type name="uint32">
              <range value="1..60">
                <description>
                  <text>Range from 1 to 60</text>
                </description>
              </range>
            </type>
            <units name="second"/>
            <default value="1"/>
          </leaf>
          <leaf name="errframeperthr">
            <description>
              <text>Configure frame period event threshold,unit:frame</text>
            </description>
            <type name="uint32">
              <range value="1..65535">
                <description>
                  <text>Range from 1 to 65535</text>
                </description>
              </range>
            </type>
            <units name="frame"/>
            <default value="1"/>
          </leaf>
          <leaf name="errframeperwin">
            <description>
              <text>Configure frame period event window,unit:ten thousand frames</text>
            </description>
            <type name="uint32">
              <range value="1..65535">
                <description>
                  <text>Range from 1 to 65535</text>
                </description>
              </range>
            </type>
            <units name="ten thousand frames"/>
            <default value="1"/>
          </leaf>
          <leaf name="errframesecthr">
            <description>
              <text>Configure frame second event threshold,unit:second</text>
            </description>
            <type name="uint32">
              <range value="1..900">
                <description>
                  <text>Range from 1 to 900</text>
                </description>
              </range>
            </type>
            <units name="second"/>
            <default value="1"/>
          </leaf>
          <leaf name="errframesecwin">
            <description>
              <text>Configure frame second event window,unit:second</text>
            </description>
            <type name="uint32">
              <range value="1..900">
                <description>
                  <text>Range from 1 to 900</text>
                </description>
              </range>
            </type>
            <units name="second"/>
            <default value="1"/>
          </leaf>
        </list>
      </container>
    </container>
  </container>
  <container name="operation">
    <description>
