              <text>Action performed to the yellow packets.</text>
            </description>
          </leaf>
          <leaf name="yellow-service-class">
            <when condition="../yellow-action='pass'"/>
            <type name="qos-service-class"/>
            <description>
              <text>Service class marked to the yellow packets.</text>
            </description>
          </leaf>
          <leaf name="yellow-color">
            <when condition="../yellow-service-class"/>
            <type name="qos-color"/>
            <mandatory value="true"/>
            <description>
              <text>Color marked to the yellow packets.</text>
            </description>
          </leaf>
          <leaf name="red-action">
