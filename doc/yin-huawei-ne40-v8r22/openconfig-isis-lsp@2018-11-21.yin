  <grouping name="lsp-state">
    <description>
      <text>This grouping defines ISIS LSP state information.</text>
    </description>
    <leaf name="lsp-id">
      <type name="leafref">
        <path value="../state/lsp-id"/>
      </type>
      <description>
        <text>A reference to the Link State PDU ID.</text>
      </description>
    </leaf>
    <container name="state">
      <description>
        <text>State parameters of Link State PDU.</text>
      </description>
      <leaf name="lsp-id">
        <type name="oc-isis-types:lsp-id"/>
        <description>
          <text>LSP ID of the LSP.</text>
        </description>
      </leaf>
      <leaf name="maximum-area-addresses">
        <type name="uint8"/>
        <description>
          <text>Number of area addresses permitted for this ISs area. 0
indicates the IS only supports three area addresses (by
default). Any number inclusive of 1 and 254 indicates the
number of areas allowed.</text>
        </description>
      </leaf>
      <leaf name="version">
        <type name="uint8"/>
        <default value="1"/>
        <description>
          <text>PDU version. This is set to 1.</text>
        </description>
      </leaf>
      <leaf name="version2">
        <type name="uint8"/>
        <default value="1"/>
        <description>
          <text>PDU version2. This is set to 1</text>
        </description>
      </leaf>
      <leaf name="id-length">
        <type name="uint8"/>
        <description>
          <text>Length of the ID field of NSAP addresses and NETs used in
this routing domain.</text>
        </description>
      </leaf>
      <leaf name="pdu-type">
        <type name="enumeration">
          <enum name="LEVEL_1">
            <description>
              <text>This enum describes ISIS level 1 PDU.</text>
            </description>
          </enum>
          <enum name="LEVEL_2">
            <description>
              <text>This enum describes ISIS level 2 PDU.</text>
            </description>
          </enum>
        </type>
        <description>
          <text>Link State PDU type.</text>
        </description>
      </leaf>
      <leaf name="remaining-lifetime">
        <type name="uint16"/>
        <units name="seconds"/>
        <description>
          <text>Remaining lifetime in seconds before the LSP expiration.</text>
        </description>
      </leaf>
      <leaf name="sequence-number">
        <type name="uint32"/>
        <description>
          <text>Sequence number of the LSP.</text>
        </description>
      </leaf>
      <leaf name="checksum">
        <type name="uint16"/>
        <description>
          <text>Checksum of the LSP.</text>
        </description>
      </leaf>
      <leaf name="pdu-length">
        <type name="uint16"/>
        <description>
          <text>Total length of the LSP.</text>
        </description>
      </leaf>
      <leaf-list name="flags">
        <type name="enumeration">
          <enum name="PARTITION_REPAIR">
            <description>
              <text>When set, the originator supports partition
repair.</text>
            </description>
          </enum>
          <enum name="ATTACHED_ERROR">
            <description>
              <text>When set, the originator is attached to another
area using the referred metric.</text>
            </description>
          </enum>
          <enum name="ATTACHED_EXPENSE">
            <description>
              <text>When set, the originator is attached to another
area using the referred metric.</text>
            </description>
          </enum>
          <enum name="ATTACHED_DELAY">
            <description>
              <text>When set, the originator is attached to another
area using the referred metric.</text>
            </description>
          </enum>
          <enum name="ATTACHED_DEFAULT">
            <description>
              <text>When set, the originator is attached to another
area using the referred metric.</text>
            </description>
          </enum>
          <enum name="OVERLOAD">
            <description>
              <text>When set, the originator is overloaded, and must
be avoided in path calculation.</text>
            </description>
          </enum>
        </type>
        <description>
          <text>LSP Type-Block flags.</text>
        </description>
      </leaf-list>
      <leaf name="is-type">
        <type name="oc-isis-types:level-number"/>
        <description>
          <text>Type of neighboring system.</text>
        </description>
      </leaf>
    </container>
    <container name="tlvs">
      <description>
        <text>This container defines Link State PDU State TLVs.</text>
      </description>
      <list name="tlv">
        <key value="type"/>
        <description>
          <text>List of TLV types in the LSDB for the specified LSP.</text>
        </description>
        <leaf name="type">
          <type name="leafref">
            <path value="../state/type"/>
          </type>
          <description>
            <text>Reference to the TLV's type.</text>
          </description>
        </leaf>
        <container name="state">
          <config value="false"/>
          <description>
            <text>Operational state parameters relating to the specified
LSP</text>
          </description>
          <uses name="isis-lsdb-tlv-type-state"/>
        </container>
        <container name="area-address">
          <when condition="../state/type = 'oc-isis-lsdb-types:AREA_ADDRESSES'">
            <description>
              <text>Include area address parameters only when the TLV type
is TLV 1.</text>
            </description>
          </when>
          <description>
            <text>This container defines TLV 1.</text>
          </description>
          <container name="state">
            <description>
              <text>State parameters of ISIS TLV 1.</text>
            </description>
            <leaf-list name="address">
              <type name="oc-isis-types:area-address"/>
              <description>
                <text>Area adress(es) of the IS. Set of manual area
addresses of this IS.</text>
              </description>
              <reference>
                <text>ISO 10589 Intermediate System to Intermediate System
Intra- Domain Routeing Exchange Protocol for use in
Conjunction with the Protocol for Providing the
Connectionless-mode Network Service (ISO 8473 )
International Standard 10589: 2002, Second Edition,
2002. TLV 1.</text>
              </reference>
            </leaf-list>
          </container>
        </container>
        <container name="lsp-buffer-size">
          <when condition="../state/type = 'oc-isis-lsdb-types:LSP_BUFFER_SIZE'">
            <description>
              <text>Include the LSP buffer size parameters only when the
TLV type is TLV 14.</text>
            </description>
          </when>
          <description>
            <text>This container defines TLV 14 - the LSP Buffer Size
TLV.</text>
          </description>
          <container name="state">
            <description>
              <text>State parameters of TLV 14.</text>
            </description>
            <leaf name="size">
              <type name="uint16"/>
              <units name="bytes"/>
              <description>
                <text>The maximum MTU that the advertising system can
receive, expressed in bytes.</text>
              </description>
              <reference>
                <text>ISO 10589 Intermediate System to Intermediate System
Intra- Domain Routeing Exchange Protocol for use in
Conjunction with the Protocol for Providing the
Connectionless-mode Network Service (ISO 8473 )
International Standard 10589: 2002, Second Edition,
2002. TLV 14.</text>
              </reference>
            </leaf>
          </container>
        </container>
        <container name="nlpid">
          <when condition="../state/type = 'oc-isis-lsdb-types:NLPID'">
            <description>
              <text>Include NLPID specification only when the TLV type is
TLV 129.</text>
            </description>
          </when>
          <description>
            <text>This container defines TLV 129.</text>
          </description>
          <container name="state">
            <description>
              <text>State parameters of ISIS TLV 129.</text>
            </description>
            <uses name="isis-lsdb-tlv-nlpid-state"/>
          </container>
        </container>
        <container name="hostname">
          <when condition="../state/type = 'oc-isis-lsdb-types:DYNAMIC_NAME'">
            <description>
              <text>Include the dynamic hostname TLV only when the TLV is
type 137.</text>
            </description>
          </when>
          <description>
            <text>This container defines TLV 137.</text>
          </description>
          <container name="state">
            <description>
              <text>State parameters of ISIS TLV 137.</text>
            </description>
            <leaf-list name="hostname">
              <type name="string"/>
              <description>
                <text>Name of the node.</text>
              </description>
              <reference>
                <text>RFC6233: IS-IS Registry Extension for Purges, RFC
5301: Dynamic Hostname Exchange Mechanism for IS-IS.
TLV 137</text>
              </reference>
            </leaf-list>
          </container>
        </container>
        <container name="ipv4-interface-addresses">
          <when condition="../state/type = 'oc-isis-lsdb-types:IPV4_INTERFACE_ADDRESSES'">
            <description>
              <text>Include the IPv4 interface addresses TLV only when the
TLV is type 132.</text>
            </description>
          </when>
          <description>
            <text>This container defines TLV 132.</text>
          </description>
          <container name="state">
            <description>
              <text>State parameters of ISIS TLV 132.</text>
            </description>
            <leaf-list name="address">
              <type name="inet:ipv4-address"/>
              <description>
                <text>IPv4 address(es) of the interface corresponding to
the SNPA over which this PDU is to be transmitted.</text>
              </description>
              <reference>
                <text>RFC1195: Use of OSI IS-IS for Routing in TCP/IP and
Dual Environments. TLV 132.</text>
              </reference>
            </leaf-list>
          </container>
        </container>
        <container name="ipv6-interface-addresses">
          <when condition="../state/type = 'oc-isis-lsdb-types:IPV6_INTERFACE_ADDRESSES'">
            <description>
              <text>Include the IPv6 interface addresses TLV only when the
TLV is type 232.</text>
            </description>
          </when>
          <description>
            <text>This container defines TLV 232.</text>
          </description>
          <container name="state">
            <description>
              <text>State parameters of ISIS TLV 232.</text>
            </description>
            <leaf-list name="address">
              <type name="inet:ipv6-address"/>
              <description>
                <text>IPv6 interface addresses of the node.  MUST contain
only the non-link-local IPv6 addresses assigned to the
IS.</text>
              </description>
              <reference>
                <text>RFC5308: Routing IPv6 with IS-IS. TLV 232.</text>
              </reference>
            </leaf-list>
          </container>
        </container>
        <container name="ipv4-te-router-id">
          <when condition="../state/type = 'oc-isis-lsdb-types:IPV4_TE_ROUTER_ID'">
            <description>
              <text>Include the IPv4 traffic engineering router ID TLV only
when the TLV is type 134.</text>
            </description>
          </when>
          <description>
            <text>This container defines TLV 134.</text>
          </description>
          <container name="state">
            <description>
              <text>State parameters of ISIS TLV 134.</text>
            </description>
            <leaf-list name="router-id">
              <type name="inet:ipv4-address"/>
              <description>
                <text>IPv4 Traffic Engineering router ID of the node. For
traffic engineering, it guarantees that we have a
single stable address that can always be referenced in
a path that will be reachable from multiple hops away,
regardless of the state of the node's interfaces.</text>
              </description>
              <reference>
                <text>RFC5305: IS-IS Extensions for Traffic Engineering. TLV
134.</text>
              </reference>
            </leaf-list>
          </container>
        </container>
        <container name="ipv6-te-router-id">
          <when condition="../state/type = 'oc-isis-lsdb-types:IPV6_TE_ROUTER_ID'">
            <description>
              <text>Include the IPv6 traffic engineering router ID TLV only
when the TLV is type 140.</text>
            </description>
          </when>
          <description>
            <text>This container defines TLV 140.</text>
          </description>
          <container name="state">
            <description>
              <text>State parameters of ISIS TLV 140.</text>
            </description>
            <leaf-list name="router-id">
              <type name="inet:ipv6-address"/>
              <description>
                <text>IPv6 Traffic Engineering router ID of the node. For
traffic engineering, it guarantees that we have a
single stable address that can always be referenced in
a path that will be reachable from multiple hops away,
regardless of the state of the node's interfaces.</text>
              </description>
              <reference>
                <text>RFC6119: IPv6 Traffic Engineering in IS-IS. TLV
140.</text>
              </reference>
            </leaf-list>
          </container>
        </container>
        <container name="instance-ids">
          <when condition="../state/type = 'oc-isis-lsdb-types:INSTANCE_ID'">
            <description>
              <text>Include the ISIS Instance Identifier TLV only when the
TLV is type 7.</text>
            </description>
          </when>
          <description>
            <text>This container defines ISIS Instance Identifier TLV.</text>
          </description>
          <reference>
            <text>RFC6822: IS-IS Multi-Instance. TLV 7.</text>
          </reference>
          <list name="instance-id">
            <key value="instance-id"/>
            <description>
              <text>A list of instance IDs received within TLV 7 within an
IS-IS LSP. In the case that more than one instance of
TLV 7 is included in the LSP, the instance IDs specified
within the instances are concatenated within this
list.</text>
            </description>
            <leaf name="instance-id">
              <type name="leafref">
                <path value="../state/instance-id"/>
              </type>
              <description>
                <text>Reference to the unique instance ID.</text>
              </description>
            </leaf>
            <container name="state">
              <description>
                <text>State parameters of ISIS TLV 7.</text>
              </description>
              <leaf name="instance-id">
                <type name="uint16"/>
                <description>
                  <text>An Instance Identifier (IID) to uniquely identify
an IS-IS instance. When the IID = 0, the list of
supported ITIDs MUST NOT be present. An IID-TLV with
IID = 0 MUST NOT appear in an SNP or LSP. When the
TLV appears (with a non-zero IID) in an SNP or LSP,
exactly one ITID. MUST be present indicating the
topology with which the PDU is associated. If no
ITIDs or multiple ITIDs are present or the IID is
zero, then the PDU MUST be ignored.</text>
                </description>
              </leaf>
              <leaf-list name="topology-id">
                <type name="uint16"/>
                <description>
                  <text>Instance-Specific Topology Identifiers (ITIDs).</text>
                </description>
              </leaf-list>
            </container>
          </list>
        </container>
        <container name="ipv4-srlgs">
          <when condition="../state/type = 'oc-isis-lsdb-types:IPV4_SRLG'">
            <description>
              <text>Include the IPv4 SRLG TLV only when the TLV is type
138.</text>
            </description>
          </when>
          <description>
            <text>This container defines ISIS SRLG TLV 138.</text>
          </description>
          <reference>
            <text>RFC5307: IS-IS Extensions in Support of Generalized
Multi-Protocol Label Switching (GMPLS). TLV 138.</text>
          </reference>
          <list name="ipv4-srlg">
            <key value="instance-number"/>
            <description>
              <text>Instance of the IPv4 SRLG TLV</text>
            </description>
            <leaf name="instance-number">
              <type name="leafref">
                <path value="../state/instance-number"/>
              </type>
              <description>
                <text>Reference to the instance number of TLV 138.</text>
              </description>
            </leaf>
            <container name="state">
              <description>
                <text>State parameters of TLV 138.</text>
              </description>
              <leaf name="instance-number">
                <type name="uint32"/>
                <description>
                  <text>An arbitrary unsigned 32-bit integer used to
disambiguate the instance of TLV 138. The instance
identifier is synthesised by the system
and may be renumbered for the same SRLG definition
in subsequent advertised LSPs if (and only if) the
entire list of SRLGs is replaced.</text>
                </description>
              </leaf>
              <leaf name="system-id">
                <type name="oc-isis-types:system-id"/>
                <description>
                  <text>Neighbor system ID.</text>
                </description>
              </leaf>
              <leaf name="psn-number">
                <type name="uint8"/>
                <description>
                  <text>Pseudonode number if the neighbor is on a LAN
interface.</text>
                </description>
              </leaf>
              <leaf-list name="flags">
                <type name="enumeration">
                  <enum name="NUMBERED">
                    <description>
                      <text>When set, the interface is numbered, whereas if
unset indicates that the interface is
unnumbered.</text>
                    </description>
                  </enum>
                </type>
                <description>
                  <text>SRLG flags.</text>
                </description>
              </leaf-list>
              <leaf name="ipv4-interface-address">
                <type name="inet:ipv4-address"/>
                <description>
                  <text>IPv4 interface address.</text>
                </description>
              </leaf>
              <leaf name="ipv4-neighbor-address">
                <type name="inet:ipv4-address"/>
                <description>
                  <text>IPv4 neighbor address.</text>
                </description>
              </leaf>
              <leaf-list name="srlg-value">
                <type name="uint32"/>
                <description>
                  <text>List of SRLG values.</text>
                </description>
              </leaf-list>
            </container>
          </list>
        </container>
        <container name="ipv6-srlgs">
          <when condition="../state/type = 'oc-isis-lsdb-types:IPV6_SRLG'">
            <description>
              <text>Include the IPv6 SRLG TLV only when the TLV is type
139.</text>
            </description>
          </when>
          <description>
            <text>This container defines ISIS SRLG TLV.</text>
          </description>
          <reference>
            <text>RFC6119: IPv6 Traffic Engineering in IS-IS. TLV 139.</text>
          </reference>
          <list name="ipv6-srlg">
            <key value="instance-number"/>
            <description>
              <text>Instance of the IPv6 SRLG TLV.</text>
            </description>
            <leaf name="instance-number">
              <type name="leafref">
                <path value="../state/instance-number"/>
              </type>
              <description>
                <text>Reference to the instance number of the IPv6 Shared
Risk Link Group (SRLG) TLV.</text>
              </description>
            </leaf>
            <container name="state">
              <description>
                <text>State parameters of TLV 139.</text>
              </description>
              <leaf name="instance-number">
                <type name="uint32"/>
                <description>
                  <text>An arbitrary unsigned 32-bit integer used to
disambiguate the instance of TLV 138. The instance
identifier is synthesised by the system
and may be renumbered for the same SRLG definition
in subsequent advertised LSPs if (and only if) the
entire list of SRLGs is replaced.</text>
                </description>
              </leaf>
              <leaf name="system-id">
                <type name="oc-isis-types:system-id"/>
                <description>
                  <text>Neighbor system ID.</text>
                </description>
              </leaf>
              <leaf name="psn-number">
                <type name="uint8"/>
                <description>
                  <text>Pseudonode number if the neighbor is on a LAN
interface.</text>
                </description>
              </leaf>
              <leaf-list name="flags">
                <type name="enumeration">
                  <enum name="NA">
                    <description>
                      <text>When set, the IPv6 neighbour address is
included, whereas if unset, it is omitted</text>
                    </description>
                  </enum>
                </type>
                <description>
                  <text>IPv6 SRLG flags.</text>
                </description>
              </leaf-list>
              <leaf name="ipv6-interface-address">
                <type name="inet:ipv6-address"/>
                <description>
                  <text>IPv6 interface address or Link Local Identifier.</text>
                </description>
              </leaf>
              <leaf name="ipv6-neighbor-address">
                <type name="inet:ipv6-address"/>
                <description>
                  <text>IPv6 neighbor address or Link Remote Identifier.</text>
                </description>
              </leaf>
              <leaf-list name="srlg-value">
                <type name="uint32"/>
                <description>
                  <text>SRLG values.</text>
                </description>
              </leaf-list>
            </container>
          </list>
        </container>
        <container name="purge-oi">
          <when condition="../state/type = 'oc-isis-lsdb-types:PURGE_OI'">
            <description>
              <text>Only include the purge originator identitication TLV
when the TLV type is 13.</text>
            </description>
          </when>
          <description>
            <text>This container defines ISIS purge TLV.</text>
          </description>
          <reference>
            <text>RFC6232: Purge Originator Identification TLV for IS-IS.
TLV 13.</text>
          </reference>
          <container name="state">
            <description>
              <text>State parameters of TLV 13.</text>
            </description>
            <leaf name="system-id-count">
              <type name="uint8"/>
              <description>
                <text>Number of system IDs carried in this TLV.</text>
              </description>
            </leaf>
            <leaf name="source-system-id">
              <type name="oc-isis-types:system-id"/>
              <description>
                <text>System ID of the Intermediate System that inserted
this TLV.</text>
              </description>
            </leaf>
            <leaf name="received-system-id">
              <type name="oc-isis-types:system-id"/>
              <description>
                <text>System ID of the Intermediate System from which the
purge was received.</text>
              </description>
            </leaf>
          </container>
        </container>
        <container name="router-capabilities">
          <when condition="../state/type = 'oc-isis-lsdb-types:ROUTER_CAPABILITY'">
            <description>
              <text>Only include the router capability TLV when the TLV is
type 242.</text>
            </description>
          </when>
          <description>
            <text>This container defines router capabilities.</text>
          </description>
          <list name="capability">
            <key value="instance-number"/>
            <description>
              <text>This list describes IS Router capabilities.</text>
            </description>
            <reference>
              <text>RFC4971: Intermediate System to Intermediate System
(IS-IS) Extensions for Advertising Router Information.
TLV 242.</text>
            </reference>
            <leaf name="instance-number">
              <type name="leafref">
                <path value="../state/instance-number"/>
              </type>
              <description>
                <text>Reference to the instance number of the router
capability TLV.</text>
              </description>
            </leaf>
            <container name="state">
              <description>
                <text>State parameters of TLV 242.</text>
              </description>
              <leaf name="instance-number">
                <type name="uint32"/>
                <description>
                  <text>A unique instance number for the instance of the
router capabilities TLV. The instance number should
be autogenerated by the producer of the data and may
be renumbered if the entire LSP contents are
replaced in subsequent advertisements.</text>
                </description>
              </leaf>
              <leaf name="router-id">
                <type name="inet:ipv4-address"/>
                <description>
                  <text>IPv4 router-id.</text>
                </description>
              </leaf>
              <leaf-list name="flags">
                <type name="enumeration">
                  <enum name="FLOOD">
                    <description>
                      <text>When the S bit is set(1), the IS - IS Router
CAPABILITY TLV MUST be flooded across the entire
routing domain. When the S bit is not set(0),
the TLV MUST NOT be leaked between levels . This
bit MUST NOT be altered during the TLV
leaking.</text>
                    </description>
                  </enum>
                  <enum name="DOWN">
                    <description>
                      <text>When the IS-IS Router CAPABILITY TLV is leaked
from level - 2 to level-1, the Down bit MUST be
set. Otherwise, this bit MUST be clear. IS - IS
Router capability TLVs with the Down bit set
MUST NOT be leaked from level - 1 to level-2.
This is to prevent TLV looping.</text>
                    </description>
                  </enum>
                </type>
                <description>
                  <text>Router capability flags.</text>
                </description>
              </leaf-list>
            </container>
            <container name="subtlvs">
              <description>
                <text>This container describes router capability TLV
sub-TLVs</text>
              </description>
              <list name="subtlv">
                <key value="type"/>
                <description>
                  <text>List of subTLV types in the LSDB for the specified
TLV</text>
                </description>
                <leaf name="type">
                  <type name="leafref">
                    <path value="../state/type"/>
                  </type>
                  <description>
                    <text>Reference to the sub-TLV type</text>
                  </description>
                </leaf>
                <container name="state">
                  <description>
                    <text>State parameters of IS Router Capabilities</text>
                  </description>
                  <uses name="isis-lsdb-subtlv-type-state"/>
                </container>
                <container name="segment-routing-algorithms">
                  <when condition="../state/type = 'oc-isis-lsdb-types:ROUTER_CAPABILITY_SR_ALGORITHM'">
                    <description>
                      <text>Only include segment routing algorithm when the
sub-TLV is type 19.</text>
                    </description>
                  </when>
                  <description>
                    <text>This container defines SR algorithm sub-TLV 19.</text>
                  </description>
                  <reference>
                    <text>draft-ietf-isis-segment-routing-extensions.
TLV 242, sub-TLV 19</text>
                  </reference>
                  <container name="state">
                    <description>
                      <text>State parameters of sub-TLV 19 - Segment
Routing Algorithm.</text>
                    </description>
                    <leaf-list name="algorithm">
                      <type name="enumeration">
                        <enum name="SPF">
                          <value value="0"/>
                          <description>
                            <text>Shortest Path First (SPF) algorithm
based on link metric.  This is the
well-known shortest path algorithm as
computed by the IS-IS Decision process.
Consistent with the deployed practice
for link-state protocols, algorithm 0
permits any node to overwrite the SPF
path with a different path based on
local policy.</text>
                          </description>
                        </enum>
                        <enum name="STRICT_SPF">
                          <value value="1"/>
                          <description>
                            <text>Strict Shortest Path First (SPF)
algorithm based on link metric. The
algorithm is identical to algorithm 0
but algorithm 1 requires that all nodes
along the path will honor the SPF
routing decision. Local policy MUST NOT
alter the forwarding decision computed
by algorithm 1 at the node claiming to
support algorithm 1.</text>
                          </description>
                        </enum>
                      </type>
                      <description>
                        <text>The Segment Routing algorithm that is
described by the TLV.</text>
                      </description>
                    </leaf-list>
                  </container>
                </container>
                <container name="segment-routing-capability">
                  <when condition="../state/type = 'oc-isis-lsdb-types:ROUTER_CAPABILITY_SR_CAPABILITY'">
                    <description>
                      <text>Only include the SR capability sub-TLV when
the sub-TLV type is 2.</text>
                    </description>
                  </when>
                  <description>
                    <text>This container defines SR Capability sub-TLV 2.</text>
                  </description>
                  <reference>
                    <text>draft-ietf-isis-segment-routing-extensions. TLV
242, sub-TLV 2.</text>
                  </reference>
                  <container name="state">
                    <description>
                      <text>State parameters of IS SR Router Capability</text>
                    </description>
                    <leaf-list name="flags">
                      <type name="enumeration">
                        <enum name="IPV4_MPLS">
                          <description>
                            <text>When set, the router is capable of
processing SR MPLS encapsulated IPv4
packets on all interfaces.</text>
                          </description>
                        </enum>
                        <enum name="IPV6_MPLS">
                          <description>
                            <text>When set, the router is capable of
processing SR MPLS encapsulated IPv6
packets on all interfaces.</text>
                          </description>
                        </enum>
                        <enum name="IPV6_SR">
                          <description>
                            <text>When set, the router is capable of
processing the IPv6 Segment Routing Header
on all interfaces.</text>
                          </description>
                        </enum>
                      </type>
                      <description>
                        <text>Segment Routing Capability Flags.</text>
                      </description>
                    </leaf-list>
                  </container>
                  <container name="srgb-descriptors">
                    <description>
                      <text>SRGB Descriptors included within the SR
capability sub-TLV</text>
                    </description>
                    <list name="srgb-descriptor">
                      <key value="range"/>
                      <description>
                        <text>Descriptor entry within the SR capabilty
sub-TLV</text>
                      </description>
                      <leaf name="range">
                        <type name="leafref">
                          <path value="../state/range"/>
                        </type>
                        <description>
                          <text>Reference to unique SRGB Descriptor.</text>
                        </description>
                      </leaf>
                      <container name="state">
                        <description>
                          <text>State parameters of the SR range</text>
                        </description>
                        <leaf name="range">
                          <type name="uint32"/>
                          <description>
                            <text>Number of SRGB elements. The range
value MUST be greater than 0.</text>
                          </description>
                        </leaf>
                        <leaf name="label">
                          <type name="oc-mplst:mpls-label"/>
                          <description>
                            <text>The first value of the SRGB when
expressed as an MPLS label.</text>
                          </description>
                        </leaf>
                      </container>
                    </list>
                  </container>
                </container>
              </list>
            </container>
            <uses name="isis-lsdb-undefined-subtlv"/>
          </list>
        </container>
        <container name="is-reachability">
          <when condition="../state/type = 'oc-isis-lsdb-types:IIS_NEIGHBORS'">
            <description>
              <text>Include IIS_NEIGHBORS sub-TLV when the TLV type is 2.</text>
            </description>
          </when>
          <description>
            <text>This container describes list of ISIS neighbors and
attributes.</text>
          </description>
          <reference>
            <text>ISO 10589, Intermediate System to Intermediate System
Intra- Domain Routeing Exchange Protocol for use in
Conjunction with the Protocol for Providing the
Connectionless-mode Network Service (ISO 8473),
International Standard 10589: 2002, Second Edition,
2002. TLV 2.</text>
          </reference>
          <container name="neighbors">
            <description>
              <text>This container describes IS neighbors.</text>
            </description>
            <list name="neighbor">
              <key value="system-id"/>
              <description>
                <text>IS reachability neighbor attributes.</text>
              </description>
              <leaf name="system-id">
                <type name="leafref">
                  <path value="../state/system-id"/>
                </type>
                <description>
                  <text>Reference to the system ID of the neighbor.</text>
                </description>
              </leaf>
              <uses name="isis-lsdb-neighbor"/>
            </list>
          </container>
        </container>
        <container name="ipv4-internal-reachability">
          <when condition="../state/type = 'oc-isis-lsdb-types:IPV4_INTERNAL_REACHABILITY'">
            <description>
              <text>Include IPv4 internal reachability TLV when the TLV
type is specified as 128.</text>
            </description>
          </when>
          <description>
            <text>This container defines list of IPv4 internal reachability
information.</text>
          </description>
          <reference>
            <text>RFC1195: OSI ISIS for IP and Dual Environments. RFC5302:
Domain-Wide Prefix Distribution with Two-Level IS-IS. TLV
128</text>
          </reference>
          <container name="prefixes">
            <description>
              <text>This container describes IS prefixes.</text>
            </description>
            <list name="prefix">
              <key value="prefix"/>
              <description>
                <text>IPv4 prefixes and internal reachability attributes.</text>
              </description>
              <leaf name="prefix">
                <type name="leafref">
                  <path value="../state/prefix"/>
                </type>
                <description>
                  <text>Reference to the IPv4 prefix</text>
                </description>
              </leaf>
              <uses name="ipv4-prefix-attributes-state"/>
            </list>
          </container>
        </container>
        <container name="ipv4-external-reachability">
          <when condition="../state/type = 'oc-isis-lsdb-types:IPV4_EXTERNAL_REACHABILITY'">
            <description>
              <text>Include IPv4 external reachability when the TLV type
is set to 130.</text>
            </description>
          </when>
          <description>
            <text>This container defines list of IPv4 external reachability
information.</text>
          </description>
          <reference>
            <text>RFC1195: OSI ISIS for IP and Dual Environments.  RFC5302:
Domain-Wide Prefix Distribution with Two-Level IS-IS. TLV
130</text>
          </reference>
          <container name="prefixes">
            <description>
              <text>This container describes IS neighbors.</text>
            </description>
            <list name="prefix">
              <key value="prefix"/>
              <description>
                <text>IPv4 external prefixes and reachability attributes.</text>
              </description>
              <leaf name="prefix">
                <type name="leafref">
                  <path value="../state/prefix"/>
                </type>
                <description>
                  <text>Reference to the IPv4 prefix.</text>
                </description>
              </leaf>
              <uses name="ipv4-prefix-attributes-state"/>
            </list>
          </container>
        </container>
        <container name="authentication">
          <when condition="../state/type = 'oc-isis-lsdb-types:AUTHENTICATION'">
            <description>
              <text>Only include the authentication TLV when the TLV is
type 10.</text>
            </description>
          </when>
          <description>
            <text>This container defines authentication information of the
node.</text>
          </description>
          <reference>
            <text>ISO 10589 Intermediate System to Intermediate System
Intra- Domain Routeing Exchange Protocol for use in
Conjunction with the Protocol for Providing the
Connectionless-mode Network Service (ISO 8473)
International Standard 10589: 2002, Second Edition, 2002.
TLV 10.</text>
          </reference>
          <container name="state">
            <description>
              <text>State parameters of TLV 10.</text>
            </description>
            <leaf name="crypto-type">
              <type name="enumeration">
                <enum name="HMAC_MD5">
                  <description>
                    <text>HMAC-MD5 Authentication type.</text>
                  </description>
                </enum>
                <enum name="CLEARTEXT">
                  <description>
                    <text>Cleartext Authentication type.</text>
                  </description>
                </enum>
              </type>
              <description>
                <text>Authentication type to be used.</text>
              </description>
            </leaf>
            <leaf name="authentication-key">
              <type name="string"/>
              <description>
                <text>Authentication key to be used.</text>
              </description>
            </leaf>
          </container>
        </container>
        <container name="extended-is-reachability">
          <when condition="../state/type = 'oc-isis-lsdb-types:EXTENDED_IS_REACHABILITY'">
            <description>
              <text>Only included the extended IS reachability TLV when the
TLV is type 22.</text>
            </description>
          </when>
          <description>
            <text>This container defines list of ISIS extended reachability
neighbors.</text>
          </description>
          <reference>
            <text>RFC5305: IS-IS Extensions for Traffic Engineering. TLV
22.</text>
          </reference>
          <uses name="isis-lsdb-common-extisreach-neighbors"/>
        </container>
        <container name="extended-ipv4-reachability">
          <when condition="../state/type = 'oc-isis-lsdb-types:EXTENDED_IPV4_REACHABILITY'">
            <description>
              <text>Only include the extended IPv4 reachability container
when the TLV type is 135.</text>
            </description>
          </when>
          <description>
            <text>This container defines list of IPv4 extended reachability
information.</text>
          </description>
          <reference>
            <text>RFC5305: IS-IS Extensions for Traffic Engineering. TLV
135</text>
          </reference>
          <container name="prefixes">
            <description>
              <text>This container describes IS prefixes.</text>
            </description>
            <list name="prefix">
              <key value="prefix"/>
              <description>
                <text>This list describes IPv4 extended prefixes and
attributes.</text>
              </description>
              <leaf name="prefix">
                <type name="leafref">
                  <path value="../state/prefix"/>
                </type>
                <description>
                  <text>Reference to the IPv4 prefix that the TLV describes
the attributes of.</text>
                </description>
              </leaf>
              <uses name="ipv4-prefix-extended-state"/>
            </list>
          </container>
        </container>
        <container name="ipv6-reachability">
          <when condition="../state/type = 'oc-isis-lsdb-types:IPV6_REACHABILITY'">
            <description>
              <text>Only include the IPv6 reachability container when the
TLV type is 236.</text>
            </description>
          </when>
          <description>
            <text>This container defines list of IPv6 reachability
information.</text>
          </description>
          <reference>
            <text>RFC5308: Routing IPv6 with IS-IS. TLV 236</text>
          </reference>
          <container name="prefixes">
            <description>
              <text>This container describes IS prefixes.</text>
            </description>
            <list name="prefix">
              <key value="prefix"/>
              <description>
                <text>This list defines IPv6 extended prefix attributes.</text>
              </description>
              <leaf name="prefix">
                <type name="leafref">
                  <path value="../state/prefix"/>
                </type>
                <description>
                  <text>Reference to the IPv6 prefix that the TLV
corresponds to.</text>
                </description>
              </leaf>
              <uses name="ipv6-prefix-extended-state"/>
            </list>
          </container>
        </container>
        <container name="multi-topology">
          <when condition="../state/type = 'oc-isis-lsdb-types:MULTI_TOPOLOGY'">
            <description>
              <text>Only include the multi-topology container when the TLV
is type 229.</text>
            </description>
          </when>
          <description>
            <text>This container defines the topology supported.</text>
          </description>
          <reference>
            <text>RFC5120: M-ISIS: Multi Topology (MT) Routing in
Intermediate System to Intermediate Systems (IS-ISs). TLV
229</text>
          </reference>
          <container name="topologies">
            <description>
              <text>This container describes IS topologies.</text>
            </description>
            <list name="topology">
              <key value="mt-id"/>
              <description>
                <text>This list describes a topology.</text>
              </description>
              <leaf name="mt-id">
                <type name="leafref">
                  <path value="../state/mt-id"/>
                </type>
                <description>
                  <text>Reference to the multi-topology ID being described
by the list entry.</text>
                </description>
              </leaf>
              <container name="state">
                <description>
                  <text>State parameters of IS multi-topology TLV 229.</text>
                </description>
                <leaf name="mt-id">
                  <type name="uint16">
                    <range value="0 .. 4095"/>
                  </type>
                  <description>
                    <text>Multi-topology ID.</text>
                  </description>
                </leaf>
                <leaf name="attributes">
                  <type name="enumeration">
                    <enum name="OVERLOAD">
                      <description>
                        <text>When set, node is overloaded, still part of
the topology but cannot be used for transit.</text>
                      </description>
                    </enum>
                    <enum name="ATTACHED">
                      <description>
                        <text>When set, node is attached to another area
using the referred metric and can be used as
default gateway.</text>
                      </description>
                    </enum>
                  </type>
                  <description>
                    <text>Attributes of the LSP for the associated
topology.</text>
                  </description>
                </leaf>
              </container>
            </list>
          </container>
        </container>
        <container name="isis-neighbor-attribute">
          <when condition="../state/type = 'oc-isis-lsdb-types:IS_NEIGHBOR_ATTRIBUTE'">
            <description>
              <text>Only include the neighbor attribute container when the
TLV is type 23.</text>
            </description>
          </when>
          <description>
            <text>This container defines list of ISIS topology neighbors
for extended ISIS LSP (multiple system IDs). </text>
          </description>
          <reference>
            <text>RFC5311: Simplified Extension of Link State PDU (LSP)
Space for IS-IS. TLV 23. It is identical in format  to the
extended IS reachability TLV 22.</text>
          </reference>
          <uses name="isis-lsdb-common-extisreach-neighbors"/>
        </container>
        <container name="is-alias-id">
          <when condition="../state/type = 'oc-isis-lsdb-types:ISIS_ALIAS_ID'">
            <description>
              <text>Only include the ISIS alias ID container when the TLV
is type 24.</text>
            </description>
          </when>
          <description>
            <text>This container defines the IS-Alias TLV which allows
extension-capable ISs to recognize the Originating System
of an Extended LSP set. It identifies the Normal system-
id of the Originating System.</text>
          </description>
          <reference>
            <text>RFC5311: Simplified Extension of Link State PDU (LSP)
Space for IS-IS TLV 24.</text>
          </reference>
          <container name="state">
            <config value="false"/>
            <description>
              <text>State parameters of alias ID.</text>
            </description>
            <leaf name="alias-id">
              <type name="oc-isis-types:system-id"/>
              <description>
                <text>List of alias ID(s).</text>
              </description>
            </leaf>
          </container>
        </container>
        <container name="mt-isn">
          <when condition="../state/type = 'oc-isis-lsdb-types:MT_ISN'">
            <description>
              <text>Only include the MT ISN container when the TLV is type
222.</text>
            </description>
          </when>
          <description>
            <text>This container defines list of ISIS multi-topology
neighbors.</text>
          </description>
          <reference>
            <text>RFC5120: M-ISIS: Multi Topology (MT) Routing in
Intermediate System to Intermediate Systems (IS-ISs). TLV
222.</text>
          </reference>
          <uses name="isis-lsdb-mtis-common"/>
        </container>
        <container name="mt-isis-neighbor-attribute">
          <when condition="../state/type = 'oc-isis-lsdb-types:MT_IS_NEIGHBOR_ATTRIBUTE'">
            <description>
              <text>Only include the MT ISIS neighbor attribute container
when the TLV is type 223.</text>
            </description>
          </when>
          <description>
            <text>This container defines list of ISIS multi-topology
neighbors.</text>
          </description>
          <reference>
            <text>RFC5311: Simplified Extension of Link State PDU (LSP)
Space for IS-IS. TLV 223. It is identical in format to the
MT-ISN TLV 222.</text>
          </reference>
          <uses name="isis-lsdb-mtis-common"/>
        </container>
        <container name="mt-ipv4-reachability">
          <when condition="../state/type = 'oc-isis-lsdb-types:MT_IPV4_REACHABILITY'">
            <description>
              <text>Only include the multi-topology IPv4 reachability
container when the TLV is type 235.</text>
            </description>
          </when>
          <description>
            <text>This container defines list of IPv4 reachability
Information in multi-topology environment.</text>
          </description>
          <reference>
            <text>RFC5120: M-ISIS: Multi Topology (MT) Routing in
Intermediate System to Intermediate Systems (IS-ISs). TLV
235.</text>
          </reference>
          <container name="prefixes">
            <description>
              <text>This container describes IS prefixes.</text>
            </description>
            <list name="prefix">
              <key value="mt-id prefix"/>
              <leaf name="mt-id">
                <type name="leafref">
                  <path value="../state/mt-id"/>
                </type>
                <description>
                  <text>Reference to the topology ID of the topology that
the prefix is within.</text>
                </description>
              </leaf>
              <leaf name="prefix">
                <type name="leafref">
                  <path value="../state/prefix"/>
                </type>
                <description>
                  <text>Reference to the prefix to which reachability is
being advertised.</text>
                </description>
              </leaf>
              <description>
                <text>IPv4 prefixes that are contained within MT
reachability TLV.</text>
              </description>
              <uses name="ipv4-mt-prefix-extended-state"/>
            </list>
          </container>
        </container>
        <container name="mt-ipv6-reachability">
          <when condition="../state/type = 'oc-isis-lsdb-types:MT_IPV6_REACHABILITY'">
            <description>
              <text>Only include the multi-topology IPv6 reachability
container when the TLV is type 237.</text>
            </description>
          </when>
          <description>
            <text>This container defines list of IPv6 reachability
information in multi - topology environment.</text>
          </description>
          <reference>
            <text>RFC5120: M-ISIS: Multi Topology (MT) Routing in
Intermediate System to Intermediate Systems (IS-ISs). TLV
237.</text>
          </reference>
          <container name="prefixes">
            <description>
              <text>This container describes IS prefixes.</text>
            </description>
            <list name="prefix">
              <key value="prefix mt-id"/>
              <description>
                <text>List of IPv6 prefixes contained within MT
reachability TLV.</text>
              </description>
              <leaf name="prefix">
                <type name="leafref">
                  <path value="../state/prefix"/>
                </type>
                <description>
                  <text>Reference to the IPv6 prefix described by the
TLV.</text>
                </description>
              </leaf>
              <leaf name="mt-id">
                <type name="leafref">
                  <path value="../state/mt-id"/>
                </type>
                <description>
                  <text>Reference to the multi-topology ID.</text>
                </description>
              </leaf>
              <uses name="ipv6-mt-prefix-extended-state"/>
            </list>
          </container>
        </container>
      </list>
    </container>
    <uses name="isis-lsdb-undefined-tlv"/>
  </grouping>
</submodule>
