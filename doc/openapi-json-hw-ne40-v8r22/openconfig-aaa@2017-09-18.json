{
    "swagger": "2.0",
    "info": {
        "description": "openconfig-aaa API generated from openconfig-aaa@2017-09-18.yang",
        "version": "1.0.0",
        "title": "openconfig-aaa API"
    },
    "host": "localhost:8080",
    "basePath": "/restconf",
    "schemes": [
        "http"
    ],
    "paths": {},
    "definitions": {
        "aaa-servergroup-common-config": {
            "description": "Configuration data for AAA server groups",
            "properties": {
                "name": {
                    "type": "string",
                    "description": "Name for the server group"
                },
                "type": {
                    "type": "string",
                    "description": "AAA server type -- all servers in the group must be of this\ntype"
                }
            }
        },
        "aaa-servergroup-common-state": {
            "description": "Operational state data for AAA server groups"
        },
        "aaa-servergroup-common-top": {
            "description": "Top-level grouping for AAA server groups",
            "properties": {
                "server-groups": {
                    "description": "Enclosing container for AAA server groups",
                    "properties": {
                        "server-group": {
                            "description": "List of AAA server groups.  All servers in a group\nmust have the same type as indicated by the server\ntype.",
                            "type": "array",
                            "items": {
                                "allOf": [
                                    {
                                        "$ref": "#/definitions/aaa-server-top"
                                    },
                                    {
                                        "properties": {
                                            "name": {
                                                "type": "string",
                                                "x-path": "../config/name",
                                                "description": "Reference to configured name of the server group"
                                            },
                                            "config": {
                                                "description": "Configuration data for each server group",
                                                "$ref": "#/definitions/aaa-servergroup-common-config"
                                            },
                                            "state": {
                                                "description": "Operational state data for each server group",
                                                "$ref": "#/definitions/aaa-servergroup-common-state"
                                            }
                                        }
                                    }
                                ]
                            },
                            "x-key": "name"
                        }
                    }
                }
            }
        },
        "aaa-server-config": {
            "description": "Common configuration data for AAA servers",
            "properties": {
                "name": {
                    "type": "string",
                    "description": "Name assigned to the server"
                },
                "address": {
                    "type": "string",
                    "description": "Address of the authentication server"
                },
                "timeout": {
                    "type": "string",
                    "description": "Set the timeout in seconds on responses from the AAA\nserver"
                }
            }
        },
        "aaa-server-state": {
            "description": "Common operational state data for AAA servers",
            "properties": {
                "connection-opens": {
                    "type": "string",
                    "description": "Number of new connection requests sent to the server, e.g.\nsocket open"
                },
                "connection-closes": {
                    "type": "string",
                    "description": "Number of connection close requests sent to the server, e.g.\nsocket close"
                },
                "connection-aborts": {
                    "type": "string",
                    "description": "Number of aborted connections to the server.  These do\nnot include connections that are close gracefully."
                },
                "connection-failures": {
                    "type": "string",
                    "description": "Number of connection failures to the server"
                },
                "connection-timeouts": {
                    "type": "string",
                    "description": "Number of connection timeouts to the server"
                },
                "messages-sent": {
                    "type": "string",
                    "description": "Number of messages sent to the server"
                },
                "messages-received": {
                    "type": "string",
                    "description": "Number of messages received by the server"
                },
                "errors-received": {
                    "type": "string",
                    "description": "Number of error messages received from the server"
                }
            }
        },
        "aaa-server-top": {
            "description": "Top-level grouping for list of AAA servers",
            "properties": {
                "servers": {
                    "description": "Enclosing container the list of servers",
                    "properties": {
                        "server": {
                            "description": "List of AAA servers",
                            "type": "array",
                            "items": {
                                "allOf": [
                                    {
                                        "$ref": "#/definitions/aaa-tacacs-server-top"
                                    },
                                    {
                                        "$ref": "#/definitions/aaa-radius-server-top"
                                    },
                                    {
                                        "properties": {
                                            "address": {
                                                "type": "string",
                                                "x-path": "../config/address",
                                                "description": "Reference to the configured address of the AAA server"
                                            },
                                            "config": {
                                                "description": "Configuration data ",
                                                "$ref": "#/definitions/aaa-server-config"
                                            },
                                            "state": {
                                                "description": "Operational state data ",
                                                "$ref": "#/definitions/aaa-server-state"
                                            }
                                        }
                                    }
                                ]
                            },
                            "x-key": "address"
                        }
                    }
                }
            }
        },
        "aaa-admin-config": {
            "description": "Configuration data for the system built-in\nadministrator / root user account",
            "properties": {
                "admin-password": {
                    "type": "string",
                    "description": "The admin/root password, supplied as a cleartext string.\nThe system should hash and only store the password as a\nhashed value."
                },
                "admin-password-hashed": {
                    "type": "string",
                    "description": "The admin/root password, supplied as a hashed value\nusing the notation described in the definition of the\ncrypt-password-type."
                }
            }
        },
        "aaa-admin-state": {
            "description": "Operational state data for the root user",
            "properties": {
                "admin-username": {
                    "type": "string",
                    "description": "Name of the administrator user account, e.g., admin, root,\netc."
                }
            }
        },
        "aaa-authentication-admin-top": {
            "description": "Top-level grouping for root user configuration and state\ndata",
            "properties": {
                "admin-user": {
                    "description": "Top-level container for the system root or admin user\nconfiguration and operational state",
                    "properties": {
                        "config": {
                            "description": "Configuration data for the root user account",
                            "$ref": "#/definitions/aaa-admin-config"
                        },
                        "state": {
                            "description": "Operational state data for the root user account",
                            "$ref": "#/definitions/aaa-admin-state"
                        }
                    }
                }
            }
        },
        "aaa-authentication-user-config": {
            "description": "Configuration data for local users",
            "properties": {
                "username": {
                    "type": "string",
                    "description": "Assigned username for this user"
                },
                "password": {
                    "type": "string",
                    "description": "The user password, supplied as cleartext.  The system\nmust hash the value and only store the hashed value."
                },
                "password-hashed": {
                    "type": "string",
                    "description": "The user password, supplied as a hashed value\nusing the notation described in the definition of the\ncrypt-password-type."
                },
                "ssh-key": {
                    "type": "string",
                    "description": "SSH public key for the user (RSA or DSA)"
                },
                "role": {
                    "type": "string",
                    "description": "Role assigned to the user.  The role may be supplied\nas a string or a role defined by the SYSTEM_DEFINED_ROLES\nidentity."
                }
            }
        },
        "aaa-authentication-user-state": {
            "description": "Operational state data for local users"
        },
        "aaa-authentication-user-top": {
            "description": "Top-level grouping for local users",
            "properties": {
                "users": {
                    "description": "Enclosing container list of local users",
                    "properties": {
                        "user": {
                            "description": "List of local users on the system",
                            "type": "array",
                            "items": {
                                "properties": {
                                    "username": {
                                        "type": "string",
                                        "x-path": "../config/username",
                                        "description": "References the configured username for the user"
                                    },
                                    "config": {
                                        "description": "Configuration data for local users",
                                        "$ref": "#/definitions/aaa-authentication-user-config"
                                    },
                                    "state": {
                                        "description": "Operational state data for local users",
                                        "$ref": "#/definitions/aaa-authentication-user-state"
                                    }
                                }
                            },
                            "x-key": "username"
                        }
                    }
                }
            }
        },
        "aaa-accounting-methods-common": {
            "description": "Common definitions for accounting methods",
            "properties": {
                "accounting-method": {
                    "type": "array",
                    "items": {
                        "type": "string",
                        "description": "The method used for AAA accounting for this event\ntype.  The method is defined by the destination for\naccounting data, which may be specified as the group of\nall TACACS+/RADIUS servers, a defined server group, or\nthe local system."
                    }
                }
            }
        },
        "aaa-accounting-events-config": {
            "description": "Configuration data for AAA accounting events",
            "properties": {
                "event-type": {
                    "type": "string",
                    "description": "The type of activity to record at the AAA accounting\nserver"
                },
                "record": {
                    "type": "string",
                    "enum": [
                        "START_STOP",
                        "STOP"
                    ],
                    "description": "Type of record to send to the accounting server for this\nactivity type"
                }
            }
        },
        "aaa-accounting-events-state": {
            "description": "Operational state data for accounting events"
        },
        "aaa-accounting-events-top": {
            "description": "Top-level grouping for accounting events",
            "properties": {
                "events": {
                    "description": "Enclosing container for defining handling of events\nfor accounting",
                    "properties": {
                        "event": {
                            "description": "List of events subject to accounting",
                            "type": "array",
                            "items": {
                                "properties": {
                                    "event-type": {
                                        "type": "string",
                                        "x-path": "../config/event-type",
                                        "description": "Reference to the event-type being logged at the\naccounting server"
                                    },
                                    "config": {
                                        "description": "Configuration data for accounting events",
                                        "$ref": "#/definitions/aaa-accounting-events-config"
                                    },
                                    "state": {
                                        "description": "Operational state data for accounting events",
                                        "$ref": "#/definitions/aaa-accounting-events-state"
                                    }
                                }
                            },
                            "x-key": "event-type"
                        }
                    }
                }
            }
        },
        "aaa-accounting-config": {
            "description": "Configuration data for event accounting",
            "$ref": "#/definitions/aaa-accounting-methods-common"
        },
        "aaa-accounting-state": {
            "description": "Operational state data for event accounting services"
        },
        "aaa-accounting-top": {
            "description": "Top-level grouping for user activity accounting",
            "properties": {
                "accounting": {
                    "description": "Top-level container for AAA accounting",
                    "allOf": [
                        {
                            "$ref": "#/definitions/aaa-accounting-events-top"
                        },
                        {
                            "properties": {
                                "config": {
                                    "description": "Configuration data for user activity accounting.",
                                    "$ref": "#/definitions/aaa-accounting-config"
                                },
                                "state": {
                                    "description": "Operational state data for user accounting.",
                                    "$ref": "#/definitions/aaa-accounting-state"
                                }
                            }
                        }
                    ]
                }
            }
        },
        "aaa-authorization-methods-config": {
            "description": "Common definitions for authorization methods for global\nand per-event type",
            "properties": {
                "authorization-method": {
                    "type": "array",
                    "items": {
                        "type": "string",
                        "description": "Ordered list of methods for authorizing commands.  The first\nmethod that provides a response (positive or negative) should\nbe used.  The list may contain a well-defined method such\nas the set of all TACACS or RADIUS servers, or the name of\na defined AAA server group.  The system must validate\nthat the named server group exists."
                    }
                }
            }
        },
        "aaa-authorization-events-config": {
            "description": "Configuration data for AAA authorization events",
            "properties": {
                "event-type": {
                    "type": "string",
                    "description": "The type of event to record at the AAA authorization\nserver"
                }
            }
        },
        "aaa-authorization-events-state": {
            "description": "Operational state data for AAA authorization events"
        },
        "aaa-authorization-events-top": {
            "description": "Top-level grouping for authorization events",
            "properties": {
                "events": {
                    "description": "Enclosing container for the set of events subject\nto authorization",
                    "properties": {
                        "event": {
                            "description": "List of events subject to AAA authorization",
                            "type": "array",
                            "items": {
                                "properties": {
                                    "event-type": {
                                        "type": "string",
                                        "x-path": "../config/event-type",
                                        "description": "Reference to the event-type list key"
                                    },
                                    "config": {
                                        "description": "Configuration data for each authorized event",
                                        "$ref": "#/definitions/aaa-authorization-events-config"
                                    },
                                    "state": {
                                        "description": "Operational state data for each authorized activity",
                                        "$ref": "#/definitions/aaa-authorization-events-state"
                                    }
                                }
                            },
                            "x-key": "event-type"
                        }
                    }
                }
            }
        },
        "aaa-authorization-config": {
            "description": "Configuration data for AAA authorization",
            "$ref": "#/definitions/aaa-authorization-methods-config"
        },
        "aaa-authorization-state": {
            "description": "Operational state data for AAA authorization"
        },
        "aaa-authorization-top": {
            "description": "Top-level grouping for AAA authorization",
            "properties": {
                "authorization": {
                    "description": "Top-level container for AAA authorization configuration\nand operational state data",
                    "allOf": [
                        {
                            "$ref": "#/definitions/aaa-authorization-events-top"
                        },
                        {
                            "properties": {
                                "config": {
                                    "description": "Configuration data for authorization based on AAA\nmethods",
                                    "$ref": "#/definitions/aaa-authorization-config"
                                },
                                "state": {
                                    "description": "Operational state data for authorization based on AAA",
                                    "$ref": "#/definitions/aaa-authorization-state"
                                }
                            }
                        }
                    ]
                }
            }
        },
        "aaa-authentication-config": {
            "description": "Configuration data for global authentication",
            "properties": {
                "authentication-method": {
                    "type": "array",
                    "items": {
                        "type": "string",
                        "description": "Ordered list of authentication methods for users.  This\ncan be either a reference to a server group, or a well-\ndefined designation in the AAA_METHOD_TYPE identity.  If\nauthentication fails with one method, the next defined\nmethod is tried -- failure of all methods results in the\nuser being denied access."
                    }
                }
            }
        },
        "aaa-authentication-state": {
            "description": "Operational state data for global authentication"
        },
        "aaa-authentication-top": {
            "description": "Top-level grouping for top-level authentication",
            "properties": {
                "authentication": {
                    "description": "Top-level container for global authentication data",
                    "allOf": [
                        {
                            "$ref": "#/definitions/aaa-authentication-admin-top"
                        },
                        {
                            "$ref": "#/definitions/aaa-authentication-user-top"
                        },
                        {
                            "properties": {
                                "config": {
                                    "description": "Configuration data for global authentication services",
                                    "$ref": "#/definitions/aaa-authentication-config"
                                },
                                "state": {
                                    "description": "Operational state data for global authentication\nservices",
                                    "$ref": "#/definitions/aaa-authentication-state"
                                }
                            }
                        }
                    ]
                }
            }
        },
        "aaa-config": {
            "description": "Configuration data for top level AAA"
        },
        "aaa-state": {
            "description": "Operational state data for top level AAA"
        },
        "aaa-top": {
            "description": "Top-level grouping for AAA services",
            "properties": {
                "aaa": {
                    "description": "Top-level container for AAA services",
                    "allOf": [
                        {
                            "$ref": "#/definitions/aaa-authentication-top"
                        },
                        {
                            "$ref": "#/definitions/aaa-authorization-top"
                        },
                        {
                            "$ref": "#/definitions/aaa-accounting-top"
                        },
                        {
                            "$ref": "#/definitions/aaa-servergroup-common-top"
                        },
                        {
                            "properties": {
                                "config": {
                                    "description": "Configuration data for top level AAA services",
                                    "$ref": "#/definitions/aaa-config"
                                },
                                "state": {
                                    "description": "Operational state data for top level AAA services ",
                                    "$ref": "#/definitions/aaa-state"
                                }
                            }
                        }
                    ]
                }
            }
        },
        "aaa-tacacs-server-config": {
            "description": "Configuration data for a TACACS+ server",
            "properties": {
                "port": {
                    "type": "string",
                    "description": "The port number on which to contact the TACACS server"
                },
                "secret-key": {
                    "type": "string",
                    "description": "The unencrypted shared key used between the authentication\nserver and the device."
                },
                "source-address": {
                    "type": "string",
                    "description": "Source IP address to use in messages to the TACACS server"
                }
            }
        },
        "aaa-tacacs-server-state": {
            "description": "Operational state data for a TACACS+ server"
        },
        "aaa-tacacs-server-top": {
            "description": "Top-level grouping for TACACS+ sever data",
            "properties": {
                "tacacs": {
                    "description": "Top-level container for TACACS+ server data",
                    "properties": {
                        "config": {
                            "description": "Configuration data for TACACS+ server",
                            "$ref": "#/definitions/aaa-tacacs-server-config"
                        },
                        "state": {
                            "description": "Operational state data for TACACS+ server",
                            "$ref": "#/definitions/aaa-tacacs-server-state"
                        }
                    }
                }
            }
        },
        "aaa-radius-server-config": {
            "description": "Configuration data for a RADIUS server",
            "properties": {
                "auth-port": {
                    "type": "string",
                    "description": "Port number for authentication requests"
                },
                "acct-port": {
                    "type": "string",
                    "description": "Port number for accounting requests"
                },
                "secret-key": {
                    "type": "string",
                    "description": "The unencrypted shared key used between the authentication\nserver and the device."
                },
                "source-address": {
                    "type": "string",
                    "description": "Source IP address to use in messages to the RADIUS server"
                },
                "retransmit-attempts": {
                    "type": "string",
                    "description": "Number of times the system may resend a request to the\nRADIUS server when it is unresponsive"
                }
            }
        },
        "aaa-radius-server-state": {
            "description": "Operational state data for a RADIUS server",
            "properties": {
                "counters": {
                    "description": "A collection of RADIUS related state objects.",
                    "properties": {
                        "retried-access-requests": {
                            "type": "string",
                            "description": "Retransmitted Access-Request messages."
                        },
                        "access-accepts": {
                            "type": "string",
                            "description": "Received Access-Accept messages."
                        },
                        "access-rejects": {
                            "type": "string",
                            "description": "Received Access-Reject messages."
                        },
                        "timeout-access-requests": {
                            "type": "string",
                            "description": "Access-Request messages that have timed-out,\nrequiring retransmission."
                        }
                    }
                }
            }
        },
        "aaa-radius-server-top": {
            "description": "Top-level grouping for RADIUS server data",
            "properties": {
                "radius": {
                    "description": "Top-level container for RADIUS server data",
                    "properties": {
                        "config": {
                            "description": "Configuration data for RADIUS servers",
                            "$ref": "#/definitions/aaa-radius-server-config"
                        },
                        "state": {
                            "description": "Operational state data for RADIUS servers",
                            "$ref": "#/definitions/aaa-radius-server-state"
                        }
                    }
                }
            }
        }
    }
}