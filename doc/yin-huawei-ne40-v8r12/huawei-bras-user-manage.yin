  <augment target-node="/ifm:ifm/ifm:interfaces/ifm:interface/bras-basic-access:bras">
    <description>
      <text>Bras interface.</text>
    </description>
    <container name="bras-user-manage">
      <description>
        <text>Configure interface.</text>
      </description>
      <container name="port-access-limit">
        <when condition="(../../../ifm:type='GigabitEthernet' or&#10;../../../ifm:type='10GE' or&#10;../../../ifm:type='XGigabitEthernet' or&#10;../../../ifm:type='100GE' or&#10;../../../ifm:type='50GE' or&#10;../../../ifm:type='50|100GE' or&#10;../../../ifm:type='FlexE' or&#10;../../../ifm:type='25GE' or&#10;../../../ifm:type='Eth-Trunk' or&#10;../../../ifm:type='Virtual-Ethernet')"/>
        <description>
          <text>Configure the number of access users on a physical port.</text>
        </description>
        <leaf name="number">
          <type name="uint32">
            <range value="0..65536"/>
          </type>
          <must condition="../../../../ifm:class!='sub-interface'"/>
          <must condition="not((../../../../ifm:name)=(/ifm:ifm/ifm:interfaces/ifm:interface/ifm-trunk:trunk/ifm-trunk:members/ifm-trunk:member/ifm-trunk:name))"/>
          <must condition="not(../../../../ethernet:ethernet/ethernet:main-interface/ethernet:l2-attribute)"/>
          <description>
            <text>Specifies the maximum number of access users on the physical port.</text>
          </description>
        </leaf>
      </container>
    </container>
  </augment>
  <augment target-node="/ifm:ifm/ifm:interfaces/ifm:interface/bras-basic-access:bas/bras-basic-access:access-type/bras-basic-access:layer2-subscriber/bras-basic-access:layer2-subscriber">
    <description>
      <text>Bras user manage in bas layer2-subscriber.</text>
    </description>
    <uses name="bras-user-manage:bas-view-group"/>
    <uses name="default-password-template"/>
    <uses name="block-interface"/>
    <uses name="default-user-name-template"/>
  </augment>
  <augment target-node="/ifm:ifm/ifm:interfaces/ifm:interface/bras-basic-access:bas/bras-basic-access:access-type/bras-basic-access:layer2-leaseline/bras-basic-access:layer2-leaseline">
    <description>
      <text>Bras user manage in bas layer2-leaseline.</text>
    </description>
    <uses name="default-user-name-template"/>
  </augment>
  <augment target-node="/aaa:aaa/aaa:accounting-schemes/aaa:accounting-scheme">
    <description>
      <text>Bras user manage in accounting scheme.</text>
    </description>
    <container name="bras-user-manage">
      <description>
        <text>Configure a accounting scheme.</text>
      </description>
      <container name="real-time-accounting-policy">
        <description>
          <text>Configure an interval for real-time accounting and the conditions for sending real-time accounting packets and hashes real-time accounting packets and send the interim accounting packet immediately after receiving the accounting start response.</text>
        </description>
        <leaf name="accounting-interval">
          <type name="uint32">
            <range value="0..65535"/>
          </type>
          <default value="0"/>
          <description>
            <text>Specifies the real-time accounting interval.</text>
          </description>
        </leaf>
        <leaf name="accounting-unit">
          <type name="bras-user-manage-acct-unit"/>
          <default value="minute"/>
          <description>
            <text>Specifies the unit of accounting interval.</text>
          </description>
        </leaf>
        <leaf name="traffic">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable interim accounting packet when increment of the user traffic is not zero.</text>
          </description>
        </leaf>
        <leaf name="hash">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable the real-time accounting packets are hashed.</text>
          </description>
        </leaf>
        <leaf name="accounting-send-update">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable the router to send the interim accounting packet immediately after receiving the accounting start response.</text>
          </description>
        </leaf>
      </container>
      <container name="accounting-fail-policy">
        <description>
          <text>Configure a policy for real-time accounting failures or an accounting start failure.</text>
        </description>
        <leaf name="max-times">
          <type name="uint8">
            <range value="1..255"/>
          </type>
          <default value="3"/>
          <description>
            <text>Specifies the maximum number of real-time accounting failures.</text>
          </description>
        </leaf>
        <leaf name="interim-fail-policy">
          <type name="enumeration">
            <enum name="online">
              <value value="1"/>
              <description>
                <text>Users keep online after accounting fail.</text>
              </description>
            </enum>
            <enum name="offline">
              <value value="2"/>
              <description>
                <text>Users cannot go online after accounting fail.</text>
              </description>
            </enum>
          </type>
          <default value="online"/>
          <description>
            <text>Configures a policy for real-time accounting failures.</text>
          </description>
        </leaf>
        <leaf name="start-fail-policy">
          <type name="enumeration">
            <enum name="online">
              <value value="1"/>
              <description>
                <text>Users keep online after accounting fail.</text>
              </description>
            </enum>
            <enum name="offline">
              <value value="2"/>
              <description>
                <text>Users cannot go online after accounting fail.</text>
              </description>
            </enum>
          </type>
          <default value="offline"/>
          <description>
            <text>Configures a policy for an accounting start failure.</text>
          </description>
        </leaf>
        <leaf name="keep-accounting">
          <when condition="../start-fail-policy='online'"/>
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable a device stays in the accounting state and does not immediately send accounting stop packets.</text>
          </description>
        </leaf>
      </container>
    </container>
  </augment>
  <augment target-node="/aaa:aaa/aaa:authentication-schemes/aaa:authentication-scheme">
    <description>
      <text>Bras user manage in authentication scheme.</text>
    </description>
    <container name="bras-user-manage">
      <description>
        <text>Configure authentication scheme.</text>
      </description>
      <container name="authentication-fail-policy">
        <must condition="(authen-fail-remain-domain and (user-authen-fail-policy='online') ) or ((not(authen-fail-remain-domain) and (user-authen-fail-policy='offline') ))"/>
        <description>
          <text>Configure a policy to be adopted after user authentication fails.</text>
        </description>
        <leaf name="user-authen-fail-policy">
          <type name="enumeration">
            <enum name="offline">
              <value value="0"/>
              <description>
                <text>Forces the users failing authentication to go offline.</text>
              </description>
            </enum>
            <enum name="online">
              <value value="1"/>
              <description>
                <text>Allows users failing authentication to remain online.</text>
              </description>
            </enum>
          </type>
          <default value="offline"/>
          <description>
            <text>Forces the users failing authentication to go offline or allows users failing authentication to remain online.</text>
          </description>
        </leaf>
        <leaf name="authen-fail-remain-domain">
          <type name="leafref">
            <path value="/aaa:aaa/aaa:domains/aaa:domain/aaa:name"/>
          </type>
          <description>
            <text>Specifies a domain that is used for controlling the access right of a user who fails authentication but remains online.</text>
          </description>
        </leaf>
      </container>
      <container name="authentication-redirect-domain-policy">
        <must condition="(radius-no-response-domain and (radius-no-response-policy='online') ) or ((not(radius-no-response-domain) and (radius-no-response-policy='offline') ))"/>
        <description>
          <text>Configure redirect domain policy.</text>
        </description>
        <leaf name="quota-out-redirect-enable">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable the function of redirecting a user to a specified domain when the quota of the user equal to zero.</text>
          </description>
        </leaf>
        <leaf name="redirect-domain">
          <type name="leafref">
            <path value="/aaa:aaa/aaa:domains/aaa:domain/aaa:name"/>
          </type>
          <description>
            <text>Configures a redirection domain when the user quota is used up.</text>
          </description>
        </leaf>
        <leaf name="radius-no-response-policy">
          <type name="enumeration">
            <enum name="online">
              <value value="1"/>
              <description>
                <text>Indicates that users stay online.</text>
              </description>
            </enum>
            <enum name="offline">
              <value value="2"/>
              <description>
                <text>Indicates that users go offline.</text>
              </description>
            </enum>
          </type>
          <default value="offline"/>
          <description>
            <text>Configures a policy when users' RADIUS authentication packets are not responded.</text>
          </description>
        </leaf>
        <leaf name="radius-no-response-domain">
          <type name="leafref">
            <path value="/aaa:aaa/aaa:domains/aaa:domain/aaa:name"/>
          </type>
          <description>
            <text>Indicates the domain in which users stay online if their RADIUS authentication packets are not responded.</text>
          </description>
        </leaf>
      </container>
    </container>
  </augment>
  <augment target-node="/aaa:aaa/aaa:domains/aaa:domain/bras-basic-access:bas">
    <description>
      <text>Bras user manage in domain.</text>
    </description>
    <container name="bras-user-manage">
      <description>
        <text>Configure the bras user manage in domain.</text>
      </description>
      <container name="flow-statistic">
        <description>
          <text>Configure the system to collect the statistics about the volume of the upstream or downstream traffic of a user in a domain.</text>
        </description>
        <leaf name="user-up-flow-statistics">
          <type name="boolean"/>
          <default value="true"/>
          <description>
            <text>Enable/disable the statistics about the volume of the upstream traffic of a user in a domain.</text>
          </description>
        </leaf>
        <leaf name="user-down-flow-statistics">
          <type name="boolean"/>
          <default value="true"/>
          <description>
            <text>Enable/disable the statistics about the volume of the downstream traffic of a user in a domain.</text>
          </description>
        </leaf>
      </container>
      <container name="flow-bill">
        <description>
          <text>Configure the system to collect the statistics about the total traffic volume of a domain.</text>
        </description>
        <leaf name="enable">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable the system to collect the statistics about the total traffic volume of a domain.</text>
          </description>
        </leaf>
      </container>
      <container name="access-trigger-loose">
        <description>
          <text>Configure online in loose mode after the system restarts due to an exception reduces client restarts.</text>
        </description>
        <leaf name="infinite-lease">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable infinite of lease.</text>
          </description>
        </leaf>
        <leaf name="loose-time">
          <when condition="../all-time='false'"/>
          <type name="uint16">
            <range value="0..1440"/>
          </type>
          <units name="min"/>
          <default value="0"/>
          <description>
            <text>Period during which users can get online in loose mode after the system restarts. 0 indicates that users cannot get online in loose mode after the system restarts.</text>
          </description>
        </leaf>
        <leaf name="all-time">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable loose mode for getting online after the system restarts.</text>
          </description>
        </leaf>
      </container>
      <container name="cpe-ip">
        <description>
          <text>Configure an IP address of the CPE in the domain.</text>
        </description>
        <leaf name="ip-address">
          <type name="inet:ipv4-address-no-zone"/>
          <description>
            <text>Configure an IP address for the CPE in the domain.</text>
          </description>
        </leaf>
      </container>
      <container name="trunk-downstream-load-balance">
        <description>
          <text>Configure load balancing of downstream Eth-Trunk traffic.</text>
        </description>
        <leaf name="enable">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable load balancing of downstream Eth-Trunk traffic.</text>
          </description>
        </leaf>
      </container>
      <container name="accounting-copy-group">
        <description>
          <text>Configure an accounting copy group in the domain.</text>
        </description>
        <leaf name="name">
          <type name="leafref">
            <path value="/bras-user-manage:bras-user-manage/bras-user-manage:accounting-copy-groups/bras-user-manage:accounting-copy-group/bras-user-manage:name"/>
          </type>
          <description>
            <text>Configure the name of an accounting copy group.</text>
          </description>
        </leaf>
      </container>
    </container>
  </augment>
  <augment target-node="/devm:devm/devm:lpu-boards/devm:lpu-board/bras-basic-access:bras">
    <description>
