<?xml version="1.0" encoding="UTF-8"?>
<submodule name="huawei-mpls-type"
           xmlns="urn:ietf:params:xml:ns:yang:yin:1"
           xmlns:mpls="urn:huawei:yang:huawei-mpls">
  <belongs-to module="huawei-mpls">
    <prefix value="mpls"/>
  </belongs-to>
  <organization>
    <text>Huawei Technologies Co., Ltd.</text>
  </organization>
  <contact>
    <text>Huawei Industrial Base
Bantian, Longgang
Shenzhen 518129
People's Republic of China
Website: http://www.huawei.com
Email: support@huawei.com</text>
  </contact>
  <description>
    <text>Specifies the multi-protocol label switching protocol.</text>
  </description>
  <revision date="2020-03-17">
    <description>
      <text>Modified some description.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <revision date="2020-02-12">
    <description>
      <text>Added the sr-exp-mode-type.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <revision date="2019-03-30">
    <description>
      <text>Initial revision.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <typedef name="label-advertise">
    <type name="enumeration">
      <enum name="explicit-null">
        <value value="0"/>
        <description>
          <text>Allocates the label 0 to the penultimate hop by the end node.</text>
        </description>
      </enum>
      <enum name="implicit-null">
        <value value="1"/>
        <description>
          <text>Allocates the label 3 to the penultimate hop by the end node.</text>
        </description>
      </enum>
      <enum name="non-null">
        <value value="2"/>
        <description>
          <text>Indicates that the end node does not allocate a null label to the penultimate hop.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Label Advertise.</text>
    </description>
  </typedef>
  <typedef name="mpls-ttl-mode-type">
    <type name="enumeration">
      <enum name="uniform">
        <value value="1"/>
        <description>
          <text>Uniform Mode.</text>
        </description>
      </enum>
      <enum name="pipe">
        <value value="2"/>
        <description>
          <text>Pipe Mode.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>MPLS TTL Mode Type.</text>
    </description>
  </typedef>
  <typedef name="diff-serv-mode-type">
    <type name="enumeration">
      <enum name="uniform">
        <value value="2"/>
        <description>
          <text>Uniform Mode.</text>
        </description>
      </enum>
      <enum name="pipe">
        <value value="1"/>
        <description>
          <text>Pipe Mode.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>MPLS DiffServ Mode Type.</text>
    </description>
  </typedef>
  <typedef name="sr-exp-mode-type">
    <type name="enumeration">
      <enum name="uniform">
        <value value="3"/>
        <description>
          <text>Uniform Mode.</text>
        </description>
      </enum>
      <enum name="pipe">
        <value value="1"/>
        <description>
          <text>Pipe Mode.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>MPLS SR EXP Mode Type.</text>
    </description>
  </typedef>
  <typedef name="mpls-lsp-type">
    <type name="enumeration">
      <enum name="static-lsp">
        <value value="0"/>
        <description>
          <text>Static LSP.</text>
        </description>
      </enum>
      <enum name="static-crlsp">
        <value value="1"/>
        <description>
          <text>Static CR-LSP.</text>
        </description>
      </enum>
      <enum name="ldp-lsp">
        <value value="2"/>
        <description>
          <text>Label Distribution Protocol (LDP) label switched path (LSP).</text>
        </description>
      </enum>
      <enum name="rsvp-crlsp">
        <value value="3"/>
        <description>
          <text>RSVP CR-LSP.</text>
        </description>
      </enum>
      <enum name="bgp-lsp">
        <value value="4"/>
        <description>
          <text>Border Gateway Protocol (BGP) label switched path (LSP).</text>
        </description>
      </enum>
      <enum name="asbr-lsp">
        <value value="5"/>
        <description>
          <text>Autonomous system boundary router (ASBR) label switched path (LSP).</text>
        </description>
      </enum>
      <enum name="bgp-ipv6-lsp">
        <value value="6"/>
        <description>
          <text>Border Gateway Protocol (BGP) IPv6label switched path (LSP).</text>
        </description>
      </enum>
      <enum name="l3vpn-ipv6-lsp">
        <value value="7"/>
        <description>
          <text>Layer 3 virtual private network (L3VPN) IPv6 label switched path (LSP).</text>
        </description>
      </enum>
      <enum name="lsp">
        <value value="8"/>
        <description>
          <text>Label switched path (LSP).</text>
        </description>
      </enum>
      <enum name="crlsp">
        <value value="9"/>
        <description>
          <text>Constraint-based routed label switched path (CR-LSP).</text>
        </description>
      </enum>
    </type>
    <description>
      <text>MPLS LSP Type.</text>
    </description>
  </typedef>
  <typedef name="outgoing-tunnel-type">
    <type name="enumeration">
      <enum name="bgp">
        <value value="2"/>
        <description>
          <text>BGP Tunnel.</text>
        </description>
      </enum>
      <enum name="srbe">
        <value value="41"/>
        <description>
          <text>SRBE Tunnel.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Stick Tunnel Type.</text>
    </description>
  </typedef>
</submodule>
