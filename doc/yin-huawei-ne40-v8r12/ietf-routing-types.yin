  <typedef name="timer-value-seconds16">
    <type name="union">
      <type name="uint16">
        <range value="1..65535"/>
      </type>
      <type name="enumeration">
        <enum name="infinity">
          <description>
            <text>The timer is set to infinity.</text>
          </description>
        </enum>
        <enum name="not-set">
          <description>
            <text>The timer is not set.</text>
          </description>
        </enum>
      </type>
    </type>
    <units name="seconds"/>
    <description>
      <text>Timer value type, in seconds (16-bit range).</text>
    </description>
  </typedef>
  <typedef name="timer-value-seconds32">
    <type name="union">
      <type name="uint32">
        <range value="1..4294967295"/>
      </type>
      <type name="enumeration">
        <enum name="infinity">
          <description>
            <text>The timer is set to infinity.</text>
          </description>
        </enum>
        <enum name="not-set">
          <description>
            <text>The timer is not set.</text>
          </description>
        </enum>
      </type>
    </type>
    <units name="seconds"/>
    <description>
      <text>Timer value type, in seconds (32-bit range).</text>
