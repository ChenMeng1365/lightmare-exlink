                                        "type": "string",
                                        "enum": [
                                            "disable",
                                            "enable"
                                        ]
                                    },
                                    "localport": {
                                        "description": "Configure local port.",
                                        "properties": {
                                            "local-port": {
                                                "description": "Local port bind the section",
                                                "type": "string"
                                            }
                                        }
                                    },
                                    "peerip": {
                                        "description": "Configure peer IP.",
                                        "properties": {
                                            "peer-ip": {
