    type enumeration {
      enum unknown {
        value 0 ;
        description 'The switch flag of port group is unknown.'
                    ;
      }
      enum switch {
        value 1 ;
        description 'The switch flag of port group is switch.'
                    ;
      }
      enum no-switch {
        value 2 ;
        description 'The switch flag of port group is no switch.'
                    ;
      }
      enum both-send-working-receive {
        value 3 ;
        description 'The switch flag of port group is both send working receive.'
                    ;
      }
      enum both-send-protect-receive {
        value 4 ;
        description 'The switch flag of port group is both send protect receive.'
