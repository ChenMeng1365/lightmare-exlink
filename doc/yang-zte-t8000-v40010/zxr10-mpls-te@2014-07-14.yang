                  value 1 ;
                }
                enum slave {
                  value 2 ;
                }
              }
            }
            leaf min-tx-interval {
              description 'The tunnel BFD min transmit interval.' ;
              type uint32 {
                range 3..990 ;
              }
              units ms ;
              mandatory true ;
            }
            leaf min-rx-interval {
              description 'The tunnel BFD min receive interval.' ;
              type uint32 {
                range 3..990 ;
              }
              units ms ;
              mandatory true ;
            }
            leaf detect-multiple {
              description 'The tunnel BFD detecting multiple.' ;
              type uint16 {
                range 3..50 ;
              }
              mandatory true ;
            }
          }
        }
      }
      container access-tunnel {
