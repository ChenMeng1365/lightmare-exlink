                <description>
                  <text>The name of split-horizon scope(&lt;A-Z&gt;).</text>
                </description>
                <type name="string">
                  <length value="1">
                    <description>
                      <text>The length of name is 1.</text>
                    </description>
                  </length>
                </type>
                <mandatory value="true"/>
              </leaf>
              <ncd:revise-date date="2018-12-04"/>
            </container>
            <container name="l2-equivalent-vni-label">
              <description>
                <text>Set L2 equivalent VNI label.</text>
              </description>
              <presence value="l2-equivalent-vni-label"/>
              <leaf name="equivalent-vni-label">
                <description>
                  <text>Set equivalent VNI label.</text>
                </description>
                <type name="empty"/>
                <mandatory value="true"/>
              </leaf>
              <ncd:revise-date date="2018-12-04"/>
            </container>
            <container name="l3-equivalent-vni-label">
              <description>
                <text>Set L3 equivalent VNI label.</text>
              </description>
              <presence value="l3-equivalent-vni-label"/>
              <leaf name="equivalent-vni-label">
                <description>
                  <text>Set equivalent VNI label.</text>
                </description>
                <type name="empty"/>
                <mandatory value="true"/>
              </leaf>
              <ncd:revise-date date="2018-12-04"/>
            </container>
            <container name="color">
              <description>
                <text>Set BGP color.</text>
              </description>
              <presence value="color"/>
              <choice name="color">
                <description>
                  <text>Set BGP color, value and list-name and remove can't co-exist in one sequence.</text>
                </description>
                <mandatory value="true"/>
                <case name="value">
                  <description>
                    <text>The color value.</text>
                  </description>
                  <leaf name="value">
                    <description>
                      <text>The color value.</text>
                    </description>
                    <type name="uint32">
                      <range value="1..4294967295">
                        <description>
                          <text>The range of color is from 1 to 4294967295.</text>
                        </description>
                      </range>
                    </type>
                    <mandatory value="true"/>
                  </leaf>
                </case>
                <case name="clear-color">
                  <description>
                    <text>Clear color flag.</text>
                  </description>
                  <leaf name="clear-color">
                    <description>
                      <text>Clear color flag.</text>
                    </description>
                    <type name="empty"/>
                    <mandatory value="true"/>
                    <ncd:revise-date date="2019-03-21"/>
                  </leaf>
                </case>
                <case name="list-name">
                  <description>
                    <text>The color list name.</text>
                  </description>
                  <leaf name="name">
                    <description>
                      <text>The color list name.</text>
                    </description>
                    <type name="string">
                      <length value="1..31"/>
                    </type>
                    <mandatory value="true"/>
                  </leaf>
                  <ncd:revise-date date="2019-05-31"/>
                </case>
              </choice>
