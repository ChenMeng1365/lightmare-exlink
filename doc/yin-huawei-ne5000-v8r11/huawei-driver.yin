<?xml version="1.0" encoding="UTF-8"?>
<module name="huawei-driver"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:driver="urn:huawei:yang:huawei-driver"
        xmlns:pub-type="urn:huawei:yang:huawei-pub-type"
        xmlns:ext="urn:huawei:yang:huawei-extension"
        xmlns:devm="urn:huawei:yang:huawei-devm"
        xmlns:hardware-type="urn:huawei:yang:huawei-hardware-type"
        xmlns:ifm="urn:huawei:yang:huawei-ifm">
  <yang-version value="1"/>
  <namespace uri="urn:huawei:yang:huawei-driver"/>
  <prefix value="driver"/>
  <import module="huawei-pub-type">
    <prefix value="pub-type"/>
  </import>
  <import module="huawei-extension">
    <prefix value="ext"/>
  </import>
  <import module="huawei-devm">
    <prefix value="devm"/>
  </import>
  <import module="huawei-hardware-type">
    <prefix value="hardware-type"/>
  </import>
  <import module="huawei-ifm">
    <prefix value="ifm"/>
  </import>
  <include module="huawei-driver-type"/>
  <organization>
    <text>Huawei Technologies Co., Ltd.</text>
  </organization>
  <contact>
    <text>Huawei Industrial Base
Bantian, Longgang
Shenzhen 518129
People's Republic of China
Website: http://www.huawei.com
Email: support@huawei.com</text>
  </contact>
  <description>
    <text>Configuration of the driver file.</text>
  </description>
  <revision date="2020-04-02">
    <description>
      <text>Init revision.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <ext:task-name value="device-mgr"/>
  <grouping name="led-flash-time">
    <description>
      <text>Indicator blinking periods.</text>
    </description>
    <leaf name="flash-time">
      <type name="uint32">
        <range value="1..5"/>
      </type>
      <units name="s"/>
      <default value="3"/>
      <description>
        <text>Set the duration in which an indicator blinks.</text>
      </description>
    </leaf>
    <leaf name="alarm-status-time">
      <type name="uint32">
        <range value="1..5"/>
      </type>
      <units name="s"/>
      <default value="3"/>
      <description>
        <text>Set the duration in which an indicator does not blink temporarily.</text>
      </description>
    </leaf>
    <leaf name="cancel-time">
      <type name="uint32">
        <range value="5..60"/>
      </type>
      <units name="min"/>
      <default value="10"/>
      <description>
        <text>Set the total duration after which an indicator stops blinking.</text>
      </description>
    </leaf>
  </grouping>
  <container name="driver">
    <description>
      <text>Configuration of the driver.</text>
    </description>
    <container name="global-attribute">
      <description>
        <text>Configure global attribute.</text>
      </description>
      <leaf name="board-power-on-delay">
        <type name="uint32">
          <range value="0..65535"/>
        </type>
        <units name="min"/>
        <default value="0"/>
        <description>
          <text>Board Power-on-delay (0 stands for cancel operation).</text>
        </description>
      </leaf>
      <leaf name="card-power-on-delay">
        <type name="uint32">
          <range value="0..65535"/>
        </type>
        <units name="min"/>
        <default value="0"/>
        <description>
          <text>Pic Power-on-delay (0 stands for cancel operation).</text>
        </description>
      </leaf>
      <leaf name="dustproof-check">
        <type name="driver-bool"/>
        <default value="yes"/>
        <description>
          <text>Dustproof Gate Check.</text>
        </description>
      </leaf>
      <leaf name="energy-mode">
        <type name="energy-mode-type"/>
        <default value="basic"/>
        <description>
          <text>Energy-saving Mode (which includes basic, standard, deep, optimal. The default value is basic).</text>
        </description>
      </leaf>
      <leaf name="energy-saving-pm">
        <ext:dynamic-default/>
        <type name="energy-saving-pm-type"/>
        <must condition="not(../energy-mode='standard' and ../energy-saving-pm='enable')"/>
        <description>
          <text>PM Energy-saving Mode (which includes disable, enable. The default value is disable).</text>
        </description>
      </leaf>
      <leaf name="entity-fault-board-isolate">
        <type name="driver-entity-bool"/>
        <default value="enable"/>
        <description>
          <text>Enable Board Abnormal Reset Detection.</text>
        </description>
      </leaf>
      <leaf name="entity-fault-card-isolate">
        <type name="driver-entity-bool"/>
        <default value="enable"/>
        <description>
          <text>Enable Card Abnormal Reset Detection.</text>
        </description>
      </leaf>
      <leaf name="fault-strategy">
        <type name="global-card-fault-process-mode"/>
        <default value="reset"/>
        <description>
          <text>Global Card-Fault Process-Mode.</text>
        </description>
      </leaf>
      <leaf name="high-temp-switch">
        <type name="driver-bool"/>
        <default value="yes"/>
        <description>
          <text>High Temperature Switch.</text>
        </description>
      </leaf>
      <leaf name="interface-match-check">
        <type name="driver-interface-match-check-flag"/>
        <default value="disable"/>
        <description>
          <text>Used to configure interface matching check functionality.</text>
        </description>
      </leaf>
      <leaf name="least-sfu-number">
        <type name="uint32">
          <range value="1..8"/>
        </type>
        <default value="3"/>
        <description>
          <text>Least number of sfu (Dynamic parameter range, which may be smaller).</text>
        </description>
      </leaf>
      <leaf name="offline-function">
        <type name="offline-function-type"/>
        <default value="enable"/>
        <description>
          <text>Offline function mask (which includes disable, enable. The default value is enable).</text>
        </description>
      </leaf>
      <leaf name="optional-entity-fault-isolate">
        <type name="entity-fault-isolate-type"/>
        <default value="enable"/>
        <description>
          <text>EntFaultIso mode.</text>
        </description>
      </leaf>
      <leaf name="pic-reload-cycle">
        <type name="reload-time"/>
        <default value="60min"/>
        <description>
          <text>Set the pic reload cycle after the pic failed to register 10 times.</text>
        </description>
      </leaf>
      <leaf name="sampling-period">
        <type name="sampling-period-type"/>
        <default value="one-hour"/>
        <description>
          <text>Power sampling period.</text>
        </description>
      </leaf>
      <leaf name="sfu-switch-mode">
        <type name="sfu-switch-mode-type"/>
        <default value="disable"/>
        <description>
          <text>SfuSwitchMode.</text>
        </description>
      </leaf>
      <leaf name="system-mac-address">
        <type name="pub-type:mac-address">
          <pattern value="[0-9a-f][02468ace][0-9a-f]{2}(-[0-9a-f]{4}){2}"/>
        </type>
        <description>
          <text>The system MAC address, with the value being a unicast MAC address that is not composed of all 0s. It will take effect only after the system is rebooted.</text>
        </description>
      </leaf>
      <leaf name="system-mac-number">
        <when condition="../system-mac-address"/>
        <type name="uint32">
          <range value="1..32"/>
        </type>
        <default value="8"/>
        <description>
          <text>The number of system MAC address.</text>
        </description>
      </leaf>
      <leaf name="system-trap">
        <type name="trap-type"/>
        <default value="base-trap"/>
        <description>
          <text>Switch trapType between base-trap and entity-trap.</text>
        </description>
      </leaf>
      <leaf name="temp-query-mode">
        <type name="mib-query-mode-type"/>
        <default value="default"/>
        <description>
          <text>MIB Temperatue Brief Query Mode.</text>
        </description>
      </leaf>
      <leaf name="vlt-query-mode">
        <type name="mib-query-mode-type"/>
        <default value="default"/>
        <description>
          <text>MIB Voltage Brief Query Mode.</text>
        </description>
      </leaf>
      <leaf name="cur-forward-mode">
        <type name="forwarding-mode"/>
        <default value="compatible"/>
        <config value="false"/>
        <description>
          <text>Current forward mode, compatible or enhance.</text>
        </description>
      </leaf>
      <leaf name="next-forward-mode">
        <type name="forwarding-mode"/>
        <default value="compatible"/>
        <description>
          <text>Next forward mode, compatible or enhance, which some boards can not support (warning:system will restart after success, and then take effect).</text>
        </description>
      </leaf>
      <leaf name="cur-work-mode">
        <type name="driver-system-mode"/>
        <config value="false"/>
        <description>
          <text>Cur work mode, 1+1 or 1:1.</text>
        </description>
      </leaf>
      <leaf name="next-work-mode">
        <type name="driver-system-mode"/>
        <config value="false"/>
        <description>
          <text>Next work mode,1+1 or 1:1. The work mode cannot be switched on specific device models or in specific scenarios (warning:system will restart after success, and then take effect).</text>
        </description>
      </leaf>
      <leaf name="warm-backup-states">
        <type name="warm-backup-status"/>
        <default value="off"/>
        <config value="false"/>
        <description>
          <text>Cur warm-backup status.</text>
        </description>
      </leaf>
      <leaf name="warm-backup-mode">
        <type name="warm-backup-mode"/>
        <must condition="not((../energy-mode='deep' or ../energy-mode='optimal') and (../warm-backup-mode='enable'))"/>
        <default value="disable"/>
        <description>
          <text>Cur warm-backup mode.</text>
        </description>
      </leaf>
      <leaf name="cur-device-work-mode">
        <type name="driver-work-mode"/>
        <config value="false"/>
        <description>
          <text>Cur device work mode, base or ptn-iot.</text>
        </description>
      </leaf>
      <leaf name="next-device-work-mode">
        <type name="driver-work-mode"/>
        <config value="false"/>
        <description>
          <text>Next device work mode, base or ptn-iot.</text>
        </description>
      </leaf>
      <leaf name="cur-system-mode">
        <type name="driver-system-mode-type"/>
        <default value="classic"/>
        <config value="false"/>
        <description>
          <text>Cur System mode, classic or ls.</text>
        </description>
      </leaf>
      <leaf name="next-system-mode">
        <type name="driver-system-mode-type"/>
        <default value="classic"/>
        <config value="false"/>
        <description>
          <text>Next System mode, classic or ls (waring:system will restart after success, and then take effect).</text>
        </description>
      </leaf>
      <leaf name="auto-upgrade-model">
        <type name="driver-upgrade-mode-type"/>
        <default value="open"/>
        <config value="false"/>
        <description>
          <text>Auto Upgrade Model.</text>
        </description>
      </leaf>
      <leaf name="chassis-last-change-time">
        <type name="pub-type:time-tick"/>
        <config value="false"/>
        <description>
          <text>Time when a chassis or an entity is last added or removed, or time when chassis status (administration status, running status, or active/standby status) is last changed. The time is in UTC time.</text>
        </description>
      </leaf>
      <leaf name="cur-chassis-number">
        <type name="uint32"/>
        <config value="false"/>
        <description>
          <text>Current number of chassis.</text>
        </description>
      </leaf>
      <leaf name="max-chassis-number">
        <type name="uint32">
          <range value="1..255"/>
        </type>
        <config value="false"/>
        <description>
          <text>Maximum number of chassis.</text>
        </description>
      </leaf>
      <leaf name="power-consumption">
        <type name="string">
          <length value="1..255"/>
        </type>
        <config value="false"/>
        <description>
          <text>Power Consumption.</text>
        </description>
      </leaf>
      <leaf name="split-resume">
        <type name="driver-split-resume-mode-type"/>
        <config value="false"/>
        <description>
          <text>Slave chassis reset mode.</text>
        </description>
      </leaf>
      <leaf name="switch-port-mode">
        <type name="string">
          <length value="1..128"/>
        </type>
        <config value="false"/>
        <description>
          <text>Switch-port Work-mode.</text>
        </description>
      </leaf>
      <leaf name="system-service-type">
        <type name="string">
          <length value="1..128"/>
        </type>
        <config value="false"/>
        <description>
          <text>System Service Type.</text>
        </description>
      </leaf>
      <leaf name="usb-uart-status">
        <type name="usb-uart-status"/>
        <config value="false"/>
        <description>
          <text>Usb uart status, open or closed.</text>
        </description>
      </leaf>
      <leaf name="cur-system-mac-address">
        <type name="pub-type:mac-address"/>
        <config value="false"/>
        <description>
          <text>Current system MAC address.</text>
        </description>
      </leaf>
      <leaf name="board-channel-check-status">
        <type name="board-channel-check-type"/>
        <config value="false"/>
        <description>
          <text>The board-channel-check status.</text>
        </description>
      </leaf>
      <leaf name="energy-saving-pm-status">
        <type name="energy-saving-pm-type"/>
        <config value="false"/>
        <description>
          <text>PM energy-saving mode status.</text>
        </description>
      </leaf>
    </container>
    <container name="io-inspect-states">
      <config value="false"/>
      <description>
        <text>List of IO inspect state.</text>
      </description>
      <list name="io-inspect-state">
        <key value="position index"/>
        <description>
          <text>IO inspect state.</text>
        </description>
        <leaf name="position">
          <type name="string">
            <length value="1..32"/>
          </type>
          <description>
            <text>Position. The position is expressed in the format of slotID. For example, 1. Here 1 is an example. The value may vary with devices.</text>
          </description>
        </leaf>
        <leaf name="index">
          <type name="uint32"/>
          <description>
            <text>Index.</text>
          </description>
        </leaf>
        <leaf name="states">
          <type name="driver-inspect-state"/>
          <description>
            <text>Input status.</text>
          </description>
        </leaf>
        <leaf name="default-states">
          <type name="driver-inspect-state"/>
          <description>
            <text>Default status.</text>
          </description>
        </leaf>
        <leaf name="alarm-name">
          <type name="string">
            <length value="1..16"/>
          </type>
          <description>
            <text>Alarm name.</text>
          </description>
        </leaf>
        <leaf name="alarm-status">
          <type name="driver-inspect-alarm-state"/>
          <description>
            <text>Alarm status.</text>
          </description>
        </leaf>
      </list>
    </container>
    <container name="io-inspect-attributes">
      <description>
        <text>List of IO inspect attributes.</text>
      </description>
      <list name="io-inspect-attribute">
        <key value="position index"/>
        <description>
          <text>Configure IO inspect attribute.</text>
        </description>
        <leaf name="position">
          <type name="string">
            <length value="1..32"/>
          </type>
          <description>
            <text>Position. The position is expressed in the format of slotID. For example, 1. Here 1 is an example. The value may vary with devices.</text>
          </description>
        </leaf>
        <leaf name="index">
          <type name="uint32"/>
          <description>
            <text>Index.</text>
          </description>
        </leaf>
        <leaf name="default-states">
          <type name="driver-default-states"/>
          <mandatory value="true"/>
          <description>
            <text>Default status.</text>
          </description>
        </leaf>
        <leaf name="alarm-name">
          <ext:operation-exclude value="update|delete"/>
          <type name="string">
            <length value="1..16"/>
          </type>
          <description>
            <text>Alarm name.</text>
          </description>
        </leaf>
      </list>
    </container>
    <container name="temperatures">
      <description>
        <text>List of temperature information about a board.</text>
      </description>
      <list name="temperature">
        <ext:generated-by value="system"/>
        <ext:operation-exclude value="create"/>
        <key value="position i2c address channel"/>
        <description>
          <text>Configure temperature information about a board.</text>
        </description>
        <leaf name="position">
          <type name="string">
            <length value="1..50"/>
          </type>
          <description>
            <text>Location of a board. The position is expressed in the format of slotID. For example, 1. Here 1 is an example. The value may vary with devices.</text>
          </description>
        </leaf>
        <leaf name="i2c">
          <type name="uint32"/>
          <description>
            <text>I2C bus number of a board.</text>
          </description>
        </leaf>
        <leaf name="address">
          <type name="string">
            <length value="1..50"/>
          </type>
          <description>
            <text>Sensor address of a board. The address is expressed in the format of a number. For example, 0. Here 0 is an example. The value may vary with devices.</text>
          </description>
        </leaf>
        <leaf name="channel">
          <type name="uint32"/>
          <description>
            <text>ID of the sensor channel of a board.</text>
          </description>
        </leaf>
        <leaf name="minor">
          <type name="uint32">
            <range value="80..120"/>
          </type>
          <must condition="(../minor&lt;=../major and ../minor&lt;=../fatal)"/>
          <description>
            <text>Minor high temperature alarm threshold of a board.</text>
          </description>
        </leaf>
        <leaf name="major">
          <type name="uint32">
            <range value="80..120"/>
          </type>
          <must condition="(../major&gt;=../minor and ../major&lt;=../fatal)"/>
          <description>
            <text>Major high temperature alarm threshold of a board.</text>
          </description>
        </leaf>
        <leaf name="fatal">
          <type name="uint32">
            <range value="80..120"/>
          </type>
          <must condition="(../fatal&gt;=../minor and ../fatal&gt;=../major)"/>
          <description>
            <text>Fatal high temperature alarm threshold of a board.</text>
          </description>
        </leaf>
        <leaf name="fan-tmax">
          <ext:dynamic-default/>
          <when condition="../minor and ../major and ../fatal"/>
          <type name="int32">
            <range value="-50..127"/>
          </type>
          <must condition="(../fan-tmax&gt;=../fan-tmin)"/>
          <description>
            <text>Fan Speed Adjust High Threshold.[-50,127]for get operation,[-50,127]for edit operation. Each board has a different default fan-tmax value.</text>
          </description>
        </leaf>
        <leaf name="fan-tmin">
          <ext:dynamic-default/>
          <when condition="../minor and ../major and ../fatal"/>
          <type name="int32">
            <range value="-50..127"/>
          </type>
          <must condition="(../fan-tmin&lt;=../fan-tmax)"/>
          <description>
            <text>Fan Speed Adjust Low Threshold.[-50,127]for get operation,[-50,127]for edit operation. Each board has a different default fan-tmin value.</text>
          </description>
        </leaf>
        <leaf name="pcb-name">
          <type name="string">
            <length value="1..50"/>
          </type>
          <config value="false"/>
          <description>
            <text>Type of a board.</text>
          </description>
        </leaf>
        <leaf name="entity-index">
          <type name="uint32"/>
          <config value="false"/>
          <description>
            <text>Index of a board.</text>
          </description>
        </leaf>
        <leaf name="state">
          <type name="driver-sensor-state"/>
          <config value="false"/>
          <description>
            <text>Sensor status of a board.</text>
          </description>
        </leaf>
        <leaf name="current-temperature">
          <type name="int32"/>
          <config value="false"/>
          <description>
            <text>Current temperature of a board.</text>
          </description>
        </leaf>
        <leaf name="minor-alarm-threshold">
          <type name="int32"/>
          <config value="false"/>
          <description>
            <text>Lower temperature threshold of a board.</text>
          </description>
        </leaf>
        <leaf name="major-alarm-threshold">
          <type name="int32"/>
          <config value="false"/>
          <description>
            <text>Upper temperature threshold of a board.</text>
          </description>
        </leaf>
        <leaf name="fatal-alarm-threshold">
          <type name="int32"/>
          <config value="false"/>
          <description>
            <text>Upper limit of the temperature of a board.</text>
          </description>
        </leaf>
        <leaf name="fan-tmax-states">
          <type name="int32">
            <range value="-50..127"/>
          </type>
          <config value="false"/>
          <description>
            <text>Fan Speed Adjust High Threshold.</text>
          </description>
        </leaf>
        <leaf name="fan-tmin-states">
          <type name="int32">
            <range value="-50..127"/>
          </type>
          <config value="false"/>
          <description>
            <text>Fan Speed Adjust Low Threshold.</text>
          </description>
        </leaf>
      </list>
    </container>
    <container name="voltageinfos">
      <description>
        <text>List of voltage information about a board.</text>
      </description>
      <list name="voltageinfo">
        <ext:generated-by value="system"/>
        <ext:operation-exclude value="create"/>
        <key value="position i2c address channel"/>
        <description>
          <text>Configure voltage information about a board.</text>
        </description>
        <leaf name="position">
          <type name="string">
            <length value="1..50"/>
          </type>
          <description>
            <text>Location of a board. The position is expressed in the format of slotID. For example, 1. Here 1 is an example. The value may vary with devices.</text>
          </description>
        </leaf>
        <leaf name="i2c">
          <type name="uint32"/>
          <description>
            <text>I2C bus number of a board.</text>
          </description>
        </leaf>
        <leaf name="address">
          <type name="string">
            <length value="1..50"/>
          </type>
          <description>
            <text>Sensor address of a board. The address is expressed in the format of a number. For example, 0. Here 0 is an example. The value may vary with devices.</text>
          </description>
        </leaf>
        <leaf name="channel">
          <type name="uint32"/>
          <description>
            <text>ID of the sensor channel of a board.</text>
          </description>
        </leaf>
        <leaf name="major-voltage-threshold-low">
          <type name="uint32">
            <range value="80..120"/>
          </type>
          <must condition="(../major-voltage-threshold-low&lt;=../major-voltage-threshold-high and ../major-voltage-threshold-low&gt;=../fatal-voltage-threshold-low and ../major-voltage-threshold-low&lt;=../fatal-voltage-threshold-high)"/>
          <description>
            <text>The percentage of the major voltage threshold below the default voltage.</text>
          </description>
        </leaf>
        <leaf name="major-voltage-threshold-high">
          <type name="uint32">
            <range value="80..120"/>
          </type>
          <must condition="(../major-voltage-threshold-high&gt;=../major-voltage-threshold-low and ../major-voltage-threshold-high&gt;=../fatal-voltage-threshold-low and ../major-voltage-threshold-high&lt;=../fatal-voltage-threshold-high)"/>
          <description>
            <text>The percentage of the major voltage threshold above the default voltage.</text>
          </description>
        </leaf>
        <leaf name="fatal-voltage-threshold-low">
          <type name="uint32">
            <range value="80..120"/>
          </type>
          <must condition="(../fatal-voltage-threshold-low&lt;=../major-voltage-threshold-low and ../fatal-voltage-threshold-low&lt;=../major-voltage-threshold-high and ../fatal-voltage-threshold-low&lt;=../fatal-voltage-threshold-high)"/>
          <description>
            <text>The percentage of the fatal voltage threshold below the default voltage.</text>
          </description>
        </leaf>
        <leaf name="fatal-voltage-threshold-high">
          <type name="uint32">
            <range value="80..120"/>
          </type>
          <must condition="(../fatal-voltage-threshold-high&gt;=../major-voltage-threshold-low and ../fatal-voltage-threshold-high&gt;=../major-voltage-threshold-high and ../fatal-voltage-threshold-high&gt;=../fatal-voltage-threshold-low)"/>
          <description>
            <text>The percentage of the fatal voltage threshold above the default voltage.</text>
          </description>
        </leaf>
        <leaf name="pcb-name">
          <type name="string">
            <length value="1..50"/>
          </type>
          <config value="false"/>
          <description>
            <text>Type of a board.</text>
          </description>
        </leaf>
        <leaf name="entity-index">
          <type name="uint32"/>
          <config value="false"/>
          <description>
            <text>Index of a board.</text>
