  <ext:task-name value="qos"/>
  <typedef name="policy-match-type">
    <type name="enumeration">
      <enum name="ip-layer-push">
        <value value="0"/>
        <description>
          <text>MF traffic classification based only on IP Layer (Layer 3) information for incoming traffic on the public network.</text>
        </description>
      </enum>
      <enum name="ip-layer-pop">
        <value value="7"/>
        <description>
          <text>MF traffic classification based only on IP Layer (Layer 3) information for outgoing traffic on the public network.</text>
        </description>
      </enum>
      <enum name="mpls-layer-push">
        <value value="3"/>
        <description>
          <text>MF classification based on both IP and MPLS information for incoming traffic on the public network.</text>
        </description>
      </enum>
      <enum name="mpls-layer-pop">
        <value value="6"/>
        <description>
          <text>MF classification based on both IP and MPLS information for outgoing traffic on the public network.</text>
        </description>
      </enum>
      <enum name="mpls-layer-l2-inbound">
        <value value="4"/>
        <description>
          <text>MF traffic classification based on MPLS information for outgoing traffic on the public network in Layer 2 forwarding scenarios.</text>
        </description>
      </enum>
      <enum name="ipv6-address-rule-mask">
        <value value="1"/>
        <description>
          <text>MF traffic classification based on the mask of IPv6 address.</text>
        </description>
      </enum>
      <enum name="protocol-mc-reserved">
        <value value="2"/>
        <description>
          <text>Match Layer 3 protocol packets with reserved multicast addresses against ACLs.</text>
        </description>
      </enum>
      <enum name="rule-packet">
        <value value="5"/>
        <description>
          <text>Enable packet type-based filtering for MF classification.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Policy math type.</text>
    </description>
  </typedef>
  <typedef name="pq-schedule-priority-type">
    <type name="enumeration">
      <enum name="high">
        <value value="0"/>
        <description>
          <text>High.</text>
        </description>
      </enum>
      <enum name="low">
        <value value="1"/>
        <description>
          <text>Low.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>PQ schedule priority mode.</text>
    </description>
  </typedef>
  <typedef name="channel-alarm-type">
    <type name="enumeration">
      <enum name="bandwidth">
        <value value="1"/>
        <description>
          <text>Bandwidth.</text>
        </description>
      </enum>
      <enum name="quantity">
        <value value="0"/>
        <description>
          <text>Quantity.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Channel alarm type.</text>
    </description>
  </typedef>
  <typedef name="channel-alarm-pool-type">
    <type name="enumeration">
      <enum name="pool-0">
        <value value="1"/>
        <description>
          <text>Pool 0.</text>
        </description>
      </enum>
      <enum name="pool-1">
        <value value="2"/>
        <description>
          <text>Pool 1.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Pool ID.</text>
    </description>
  </typedef>
  <typedef name="channel-profile-precision-type">
    <type name="enumeration">
      <enum name="up">
        <value value="1"/>
        <description>
          <text>Up.</text>
        </description>
      </enum>
      <enum name="down">
        <value value="2"/>
        <description>
          <text>Down.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Precision type.</text>
    </description>
  </typedef>
  <typedef name="multicast-priority-type">
    <type name="enumeration">
      <enum name="both">
        <value value="2"/>
        <description>
          <text>Both.</text>
        </description>
      </enum>
      <enum name="high">
        <value value="1"/>
        <description>
          <text>High.</text>
        </description>
      </enum>
      <enum name="low">
        <value value="0"/>
        <description>
          <text>Low.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Multicast priority type.</text>
    </description>
  </typedef>
  <typedef name="match-type">
    <type name="enumeration">
      <enum name="destination-user">
        <value value="1"/>
        <description>
          <text>Destination user.</text>
        </description>
      </enum>
      <enum name="interface-acl">
        <value value="2"/>
        <description>
          <text>Interface ACL.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Match type.</text>
    </description>
  </typedef>
  <typedef name="limit-type">
    <type name="enumeration">
      <enum name="unlimited">
        <value value="1"/>
        <description>
          <text>Unlimited bandwidth.</text>
        </description>
      </enum>
      <enum name="limited">
        <value value="0"/>
        <description>
          <text>Limited bandwidth.</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Is bandwidth limited.</text>
    </description>
  </typedef>
  <augment target-node="/devm-vnf:devm-vnf/devm-vnf:vnf-boards/devm-vnf:vnf-board">
    <description>
      <text>Adds QoS configuration to the vnf board.</text>
    </description>
    <container name="qos">
      <description>
        <text>Configure QoS application.</text>
      </description>
      <container name="traffic-policy-matchs">
        <description>
          <text>List of traffic policy match.</text>
        </description>
        <list name="traffic-policy-match">
          <key value="type"/>
          <max-elements value="3"/>
          <description>
            <text>Configure traffic policy match. Some boards do not support this object.</text>
          </description>
          <leaf name="type">
            <type name="policy-match-type"/>
            <description>
              <text>Traffic policy match type.</text>
            </description>
          </leaf>
        </list>
      </container>
      <container name="traffic-policy-match-type">
        <description>
          <text>Configure policy UCL rule match.</text>
        </description>
        <container name="destination-user">
          <presence value="create destination-user"/>
          <description>
            <text>Enable/disable traffic policy match type destination user.</text>
          </description>
        </container>
        <container name="interface-acl">
          <presence value="create interface-acl"/>
          <description>
            <text>Enable/disable traffic policy match type interface acl.</text>
          </description>
        </container>
      </container>
    </container>
  </augment>
  <augment target-node="/devm:devm/devm:mpu-boards/devm:mpu-board">
    <description>
      <text>Adds QoS configuration to the board.</text>
    </description>
    <container name="qos">
      <description>
        <text>Configure QoS application.</text>
      </description>
      <container name="traffic-policy-matchs">
        <description>
          <text>List of traffic policy match.</text>
        </description>
        <list name="traffic-policy-match">
          <must condition="(((type='ip-layer-push' or type='ip-layer-pop' or type='rule-packet') and not(../traffic-policy-match[type='mpls-layer-push']) and not(../traffic-policy-match[type='mpls-layer-pop'])) or ((type='mpls-layer-push' or type='mpls-layer-pop') and not(../traffic-policy-match[type='ip-layer-push']) and not(../traffic-policy-match[type='ip-layer-pop']) and not(../traffic-policy-match[type='rule-packet'])) or (type='mpls-layer-l2-inbound' or type='protocol-mc-reserved'))"/>
          <key value="type"/>
          <max-elements value="5"/>
          <description>
            <text>Configure traffic policy match. Some boards do not support this object.</text>
          </description>
          <leaf name="type">
            <type name="policy-match-type"/>
            <description>
              <text>Traffic policy match type.</text>
            </description>
          </leaf>
        </list>
      </container>
      <container name="pq-schedule-prioritys">
        <description>
          <text>List of PQ schedule priority.</text>
        </description>
        <list name="pq-schedule-priority">
          <key value="direction"/>
          <max-elements value="2"/>
          <description>
            <text>Configure PQ schedule priority.</text>
          </description>
          <ext:generated-by value="system">
            <description>
              <text>The all instances are generated automatically.</text>
            </description>
          </ext:generated-by>
          <ext:operation-exclude value="create|delete">
            <description>
              <text>The all instances cannot be created or deleted.</text>
            </description>
          </ext:operation-exclude>
          <leaf name="direction">
            <type name="qos:qos-direction-type"/>
            <description>
              <text>Direction.</text>
            </description>
          </leaf>
          <leaf name="mode">
            <type name="pq-schedule-priority-type"/>
            <mandatory value="true"/>
            <description>
              <text>PQ schedule mode.</text>
            </description>
          </leaf>
        </list>
      </container>
      <container name="pq-schedule-priority-on-cards">
        <description>
          <text>List of PQ schedule priority.</text>
        </description>
        <list name="pq-schedule-priority-on-card">
          <key value="card-id"/>
          <description>
            <text>Configure PQ schedule priority. This configuration takes effect only on eTM subcards.</text>
          </description>
          <leaf name="card-id">
            <type name="string">
              <length value="1..31"/>
            </type>
            <description>
              <text>Card ID.</text>
            </description>
          </leaf>
          <leaf name="mode">
            <type name="pq-schedule-priority-type"/>
            <mandatory value="true"/>
            <description>
              <text>PQ schedule priority mode.</text>
            </description>
          </leaf>
        </list>
      </container>
      <container name="multicast-port-queues">
        <description>
          <text>List of port queue schedule in outbound direction on the MTunnel interface of the distributed multicast VPN.</text>
        </description>
        <list name="multicast-port-queue">
          <key value="service-class"/>
          <max-elements value="8"/>
          <description>
            <text>Configure port queue schedule in outbound direction on the MTunnel interface of the distributed multicast VPN.</text>
          </description>
          <ext:generated-by value="system">
            <description>
              <text>The all instances are generated automatically.</text>
            </description>
          </ext:generated-by>
          <ext:operation-exclude value="create|delete">
            <description>
              <text>The all instances cannot be created or deleted.</text>
            </description>
          </ext:operation-exclude>
          <leaf name="service-class">
            <type name="qos:qos-service-class"/>
            <description>
              <text>Class of service.</text>
            </description>
          </leaf>
          <leaf name="queue-schedule-mode">
            <type name="qos:queue-schedule-mode"/>
            <mandatory value="true"/>
            <description>
              <text>Schedule Mode.</text>
            </description>
          </leaf>
          <leaf name="weight">
            <when condition="../queue-schedule-mode='wfq'"/>
            <type name="uint8">
              <range value="1..100"/>
            </type>
            <mandatory value="true"/>
            <description>
              <text>Weight of wfq scheduling strategy.</text>
            </description>
          </leaf>
          <choice name="shaping-parameters">
            <description>
              <text>Specify shaping parameters.</text>
            </description>
            <case name="shaping">
              <description>
                <text>Specify shaping parameters choice case as shaping value.</text>
              </description>
              <leaf name="shaping-value">
                <type name="uint32">
                  <range value="0..4294967294"/>
                </type>
                <units name="Mbit/s"/>
                <description>
                  <text>Shaping value.</text>
                </description>
              </leaf>
            </case>
            <case name="shaping-percent">
              <description>
                <text>Specify shaping parameters choice case as shaping percentage value.</text>
              </description>
              <leaf name="shaping-percentage">
                <type name="uint8">
                  <range value="0..100"/>
                </type>
                <units name="%"/>
                <description>
                  <text>Shaping percentage.</text>
                </description>
              </leaf>
            </case>
          </choice>
          <leaf name="port-wred-name">
            <type name="leafref">
              <path value="/qos:qos/qos:wred-template/qos:port-wreds/qos:port-wred/qos:name"/>
            </type>
            <description>
              <text>Port wred name.</text>
            </description>
          </leaf>
        </list>
      </container>
      <container name="multicast-port-shaping">
        <description>
          <text>Configure a peak rate at which the MTunnel interface in the distributed multicast VPN can send data.</text>
        </description>
        <choice name="shaping-parameters">
          <description>
            <text>Specify shaping parameters.</text>
          </description>
          <case name="shaping-type-mode">
            <description>
              <text>Specify shaping parameters choice case as shaping type.</text>
            </description>
            <leaf name="shaping-type">
              <type name="qos:shaping-value-type"/>
              <description>
                <text>Shaping value type.</text>
              </description>
            </leaf>
          </case>
          <case name="shaping-value-mode">
            <description>
              <text>Specify shaping parameters choice case as shaping value.</text>
            </description>
            <leaf name="shaping-value">
              <type name="uint32">
                <range value="1..4294967294"/>
              </type>
              <units name="Mbit/s"/>
              <description>
                <text>Shaping value.</text>
              </description>
            </leaf>
          </case>
        </choice>
      </container>
      <container name="user-queue-pir-precision">
        <presence value="create user-queue-pir-precision"/>
        <description>
          <text>Enable/disable user queue pir precision.</text>
        </description>
        <ext:generated-by value="system"/>
        <ext:operation-exclude value="create|delete"/>
        <leaf name="precision">
          <type name="uint8">
            <range value="1..100"/>
          </type>
          <units name="%"/>
          <default value="1"/>
          <description>
            <text>Precision.</text>
          </description>
        </leaf>
      </container>
      <container name="exp-phb-disable">
        <description>
          <text>Configure disable the private network tag priority in a DS domain from being marked for the downstream traffic on the PW interface in the VLL networking.</text>
        </description>
        <leaf name="forward-type">
          <type name="qos:forward-type"/>
          <description>
            <text>Forward type.</text>
          </description>
        </leaf>
      </container>
      <container name="packet-block-thresholds">
        <description>
          <text>List of the packet block threshold size.</text>
        </description>
        <list name="packet-block-threshold">
          <key value="service-class"/>
          <description>
            <text>Configure the packet block threshold size.</text>
          </description>
          <leaf name="service-class">
            <type name="qos:qos-service-class"/>
            <description>
              <text>Class of service.</text>
            </description>
          </leaf>
          <leaf name="size">
            <type name="uint8">
              <range value="0..5"/>
            </type>
            <units name="kB"/>
            <mandatory value="true"/>
            <description>
              <text>The packet block threshold size.</text>
            </description>
          </leaf>
        </list>
      </container>
      <container name="packet-block-threshold-all">
        <description>
          <text>Configure the packet block threshold size for all queues.</text>
        </description>
        <leaf name="size">
          <type name="uint8">
            <range value="0..5"/>
          </type>
          <units name="kB"/>
          <description>
            <text>The packet block threshold size.</text>
          </description>
        </leaf>
      </container>
      <container name="channel-alarms">
        <description>
          <text>List of channel alarm.</text>
        </description>
        <list name="channel-alarm">
          <key value="alarm-type pool"/>
          <description>
            <text>Configure channel alarm.</text>
          </description>
          <leaf name="alarm-type">
            <type name="channel-alarm-type"/>
            <description>
              <text>Alarm type.</text>
            </description>
          </leaf>
          <leaf name="pool">
            <type name="channel-alarm-pool-type"/>
            <description>
              <text>Pool.</text>
            </description>
          </leaf>
          <leaf name="threshold">
            <type name="uint8">
              <range value="1..100"/>
            </type>
            <units name="%"/>
            <mandatory value="true"/>
            <description>
              <text>Threshold value.</text>
            </description>
          </leaf>
        </list>
      </container>
      <container name="channel-profile-precision">
        <description>
          <text>Configure the bandwidth allocation mode.</text>
        </description>
        <leaf name="type">
          <type name="channel-profile-precision-type"/>
          <description>
            <text>Precision type.</text>
          </description>
        </leaf>
      </container>
      <container name="channel-profile-apply">
        <description>
          <text>Configure a channel profile to a board. Apply channel profile template may affect scheduling. The configuration will work after the board restart.</text>
        </description>
        <leaf name="name">
          <type name="leafref">
            <path value="/qos:qos/qos:queue-scheduler-template/qos:channel-profiles/qos:channel-profile/qos:name"/>
          </type>
          <description>
            <text>Channel profile name.</text>
          </description>
        </leaf>
      </container>
      <container name="multicast-shaping">
        <description>
          <text>Configure an upper rate limit for multicast traffic on a specified board.</text>
        </description>
        <leaf name="shaping-value">
          <type name="uint32">
            <range value="1..1000000"/>
          </type>
          <units name="pps"/>
          <description>
            <text>Shaping value.</text>
          </description>
        </leaf>
      </container>
      <container name="port-queue-alarms">
        <description>
          <text>List of port queue buffer alarm.</text>
        </description>
        <list name="port-queue-alarm">
          <key value="service-class"/>
          <max-elements value="8"/>
          <description>
            <text>Configure port queue buffer alarm.</text>
          </description>
          <leaf name="service-class">
            <type name="qos:qos-service-class"/>
            <description>
              <text>Class of service.</text>
            </description>
          </leaf>
          <leaf name="buffer-percentage">
            <type name="uint8">
              <range value="0..100"/>
            </type>
            <units name="%"/>
            <mandatory value="true"/>
            <description>
              <text>Buffer percentage.</text>
            </description>
          </leaf>
        </list>
      </container>
      <container name="user-queue-recovery-disable">
        <presence value="create user-queue-recovery-disable"/>
        <description>
          <text>Enable/disable automatic switchback from CAR to user queue.</text>
        </description>
      </container>
      <container name="access-user-board-schedule-enable">
        <presence value="create access-user-board-schedule-enable"/>
        <description>
          <text>Enable/disable downstream HQoS scheduling.</text>
        </description>
      </container>
      <container name="convergent-precedence-highs">
        <description>
          <text>List of configuration specific low-priority queues on a board as high-priority queues.</text>
        </description>
        <list name="convergent-precedence-high">
          <key value="service-class-value"/>
          <max-elements value="5"/>
          <description>
            <text>Configure specific low-priority queues on a board as high-priority queues.</text>
          </description>
          <leaf name="service-class-value">
            <type name="uint8">
              <range value="0..4"/>
            </type>
            <description>
              <text>The value of service class.</text>
            </description>
          </leaf>
        </list>
      </container>
      <container name="queue-discard-alarms">
        <description>
          <text>List of queue discard alarm.</text>
        </description>
        <list name="queue-discard-alarm">
          <key value="service-class"/>
          <max-elements value="8"/>
          <description>
            <text>Configure queue discard alarm.</text>
          </description>
          <leaf name="service-class">
            <type name="qos:qos-service-class"/>
            <description>
              <text>Class of service.</text>
            </description>
          </leaf>
          <choice name="discard-type">
            <mandatory value="true"/>
            <description>
              <text>Specify discard type.</text>
            </description>
            <case name="bytes">
              <description>
                <text>Specify discard type choice case as discard-byte.</text>
              </description>
              <leaf name="bytes">
                <type name="uint32">
                  <range value="1..4294967295"/>
                </type>
                <units name="Byte"/>
                <mandatory value="true"/>
                <description>
                  <text>The discarded bytes.</text>
                </description>
              </leaf>
            </case>
            <case name="packets">
              <description>
                <text>Specify discard type choice case as discard packet.</text>
              </description>
              <leaf name="packets">
                <type name="uint32">
                  <range value="1..4294967295"/>
                </type>
                <units name="packet"/>
                <mandatory value="true"/>
                <description>
                  <text>The discarded packets.</text>
                </description>
              </leaf>
            </case>
            <case name="packet-ratio">
              <description>
                <text>Specify discard type choice case as discard packet ratio.</text>
              </description>
              <leaf name="packet-coefficient">
                <type name="uint8">
                  <range value="1..9"/>
                </type>
                <mandatory value="true"/>
                <description>
                  <text>Ratio coefficient of the discard packets.</text>
                </description>
              </leaf>
              <leaf name="packet-exponent">
                <type name="uint8">
                  <range value="1..5"/>
                </type>
                <mandatory value="true"/>
                <description>
                  <text>Ratio exponent of the discard packets.</text>
                </description>
              </leaf>
            </case>
          </choice>
          <leaf name="interval-time">
            <type name="uint16">
              <range value="15..3600"/>
            </type>
            <units name="s"/>
            <default value="30"/>
            <description>
              <text>Monitoring time of the queue discard alarm.</text>
