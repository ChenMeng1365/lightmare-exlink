            <description>
              <text>Specifies a DHCPv4 option code.</text>
            </description>
          </leaf>
          <leaf name="dhcpv6-code-value">
            <type name="uint32">
              <range value="15..17|21..22|27..36|39..65535"/>
            </type>
            <description>
              <text>Specifies a DHCPv6 option code.</text>
            </description>
          </leaf>
        </container>
        <container name="vendor-attribute-multiple">
          <description>
            <text>Configure the NE40E to carry multiple proprietary attributes in RADIUS attribute 26 Vendor-Specific.</text>
          </description>
          <leaf name="huawei">
            <type name="boolean"/>
            <default value="true"/>
            <description>
              <text>Enable/disable the Huawei proprietary attribute.</text>
            </description>
          </leaf>
          <leaf name="microsoft">
