<?xml version="1.0" encoding="UTF-8"?>
<module name="zxr10-mim"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:mim="http://www.zte.com.cn/zxr10/netconf/schema/rosng/mim"
        xmlns:ncd="http://www.zte.com.cn/zxr10/netconf/schema/rosng/netconf-defs">
  <namespace uri="http://www.zte.com.cn/zxr10/netconf/schema/rosng/mim"/>
  <prefix value="mim"/>
  <yang-version value="1"/>
  <import module="zxr10-netconf-defs">
    <prefix value="ncd"/>
  </import>
  <organization>
    <text>ZTE Corporation</text>
  </organization>
  <contact>
    <text>ZTE Corporation  
   Nanjing Institute of ZTE Corporation 
   No.68 Zijinghua Rd. Yuhuatai District, Nanjing, China 
   Tel: +86-25-52870000</text>
  </contact>
  <description>
    <text>mim Module</text>
  </description>
  <revision date="2019-02-21">
    <description>
      <text>Add session-type(lmt,otnqx,pct) to configuration-change notification content session-type.
Change configuration-change notification content ipv4-addr and ipv6-addr type to chars.</text>
    </description>
  </revision>
  <revision date="2017-09-04">
    <description>
      <text>Enable or disable sending snmp trap about configuration-data changed.</text>
    </description>
  </revision>
  <revision date="2017-04-04">
    <description>
      <text>add query configuration state. 1.query loaded file(zdb or startrun.dat) created time. 2.query zdb or startrun.dat last save time. 3.query configuration last changed time.</text>
    </description>
  </revision>
  <revision date="2015-09-17">
    <description>
      <text>Create Date</text>
    </description>
  </revision>
  <feature name="config-variation-trap">
    <description>
      <text>The ability of generating snmp trap when configuration-data is changed.</text>
    </description>
  </feature>
  <container name="state">
    <config value="false"/>
    <container name="config-state">
      <container name="loaded-data-time">
        <description>
          <text>create time of loaded file</text>
        </description>
        <leaf name="zdb">
          <description>
            <text>create time of loaded zdb file</text>
          </description>
          <type name="ncd:datetime"/>
        </leaf>
        <leaf name="txt">
          <description>
            <text>create time of loaded startrun.dat file</text>
          </description>
          <type name="ncd:datetime"/>
        </leaf>
      </container>
      <container name="last-saved-time">
        <description>
          <text>last saved time of configuration file after device start</text>
        </description>
        <leaf name="zdb">
          <description>
            <text>last saved time of configuration file(zdb) after device start</text>
          </description>
          <type name="ncd:datetime"/>
        </leaf>
        <leaf name="txt">
          <description>
            <text>last saved time of configuration file(startrun.dat) after device start</text>
          </description>
          <type name="ncd:datetime"/>
        </leaf>
      </container>
      <leaf name="changed-time">
        <description>
          <text>configuration last changed time</text>
        </description>
        <type name="ncd:datetime"/>
      </leaf>
    </container>
    <ncd:revise-date date="2017-04-04"/>
  </container>
  <container name="configuration">
    <container name="mim">
      <description>
        <text>The configuration about management-info-maintance of system.</text>
      </description>
      <container name="global">
        <description>
          <text>Global configuration.</text>
        </description>
        <container name="config-variation-trap-switch">
          <description>
            <text>Enable or disable sending snmp trap about configuration-data changed.</text>
          </description>
          <leaf name="trap-switch">
            <description>
              <text>The snmp trap is generated or not when configuration-data changed is detceted.</text>
            </description>
            <type name="enumeration">
              <enum name="disable">
                <value value="1"/>
                <description>
                  <text>System won't send a trap when configuration-data changed is detceted.</text>
                </description>
              </enum>
              <enum name="enable">
                <value value="2"/>
                <description>
                  <text>System will send a trap when configuration-data changed is detceted.</text>
                </description>
              </enum>
            </type>
            <default value="enable"/>
          </leaf>
        </container>
      </container>
    </container>
    <ncd:revise-date date="2017-09-04"/>
  </container>
  <notification name="configuration-change">
    <description>
      <text>Report notification when configuration changed</text>
    </description>
    <container name="notification">
      <description>
        <text>Container name which make notification more understandable</text>
      </description>
      <list name="content">
        <description>
          <text>notification content</text>
        </description>
        <key value="session-type"/>
        <leaf name="session-type">
          <description>
            <text>Session Type</text>
          </description>
          <type name="enumeration">
            <enum name="CLI">
              <value value="1"/>
              <description>
                <text>CLI Session</text>
              </description>
            </enum>
            <enum name="SNMP">
              <value value="2"/>
              <description>
                <text>SNMP Session</text>
              </description>
            </enum>
            <enum name="WEB">
              <value value="3"/>
              <description>
                <text>WEB Session</text>
              </description>
            </enum>
            <enum name="NETCONF">
              <value value="4"/>
              <description>
                <text>NETCONF Session</text>
              </description>
            </enum>
            <enum name="QX">
              <value value="5"/>
              <description>
                <text>QX Session</text>
              </description>
            </enum>
            <enum name="TR069">
              <value value="6"/>
              <description>
                <text>TR069 Session</text>
              </description>
            </enum>
            <enum name="LMT">
              <value value="7"/>
              <description>
                <text>LMT Session</text>
              </description>
            </enum>
            <enum name="OTNQX">
              <value value="8"/>
              <description>
                <text>OTNQX Session</text>
              </description>
            </enum>
            <enum name="PCT">
              <value value="9"/>
              <description>
                <text>PCT Session</text>
              </description>
            </enum>
          </type>
        </leaf>
        <leaf name="ip-type">
          <description>
            <text>ipAddr type</text>
          </description>
          <type name="enumeration">
            <enum name="NoIpAddr">
              <value value="0"/>
              <description>
                <text>No Ip Address</text>
              </description>
            </enum>
            <enum name="IpV4Addr">
              <value value="4"/>
              <description>
                <text>IpV4 Address</text>
              </description>
            </enum>
            <enum name="IpV6Addr">
              <value value="6"/>
              <description>
                <text>IpV6 Address</text>
              </description>
            </enum>
          </type>
        </leaf>
        <leaf name="ipv4-addr">
          <description>
            <text>Ipv4 Address</text>
          </description>
          <type name="ncd:string-all"/>
        </leaf>
        <leaf name="ipv6-addr">
          <description>
            <text>Ipv6 Address</text>
          </description>
          <type name="ncd:string-all"/>
        </leaf>
      </list>
    </container>
  </notification>
</module>
