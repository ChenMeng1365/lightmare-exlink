            </container>
            <container name="evpn-router-mac">
              <description>
                <text>Set EVPN router's MAC extcommunity attribute.</text>
              </description>
              <presence value="evpn-routr-mac"/>
              <leaf name="intf-name">
                <description>
                  <text>Set EVPN router's MAC interface.</text>
                </description>
                <type name="ncd:ifname">
                  <length value="1..31"/>
                </type>
                <mandatory value="true"/>
              </leaf>
              <ncd:revise-date date="2017-02-28"/>
            </container>
            <container name="l2-vni-label">
              <description>
                <text>Set l2-VNI label.</text>
              </description>
              <presence value="l2-vni-label"/>
              <leaf name="label">
                <description>
                  <text>Set the label value.</text>
                </description>
                <type name="uint32">
                  <range value="1..16777215">
                    <description>
                      <text>The range of l2-vni label is from 1 to 16777215.</text>
                    </description>
                  </range>
                </type>
                <mandatory value="true"/>
              </leaf>
              <ncd:revise-date date="2016-11-17"/>
            </container>
            <container name="policy-route-id">
              <description>
                <text>Set policy route id.</text>
              </description>
              <presence value="policy-route-id"/>
              <leaf name="pid">
                <description>
                  <text>Policy route id.</text>
                </description>
                <type name="uint8">
                  <range value="1..63"/>
                </type>
                <mandatory value="true"/>
              </leaf>
              <ncd:revise-date date="2017-04-28"/>
            </container>
            <container name="router-mac">
              <description>
                <text>Set router MAC address.</text>
              </description>
              <presence value="router-mac"/>
              <leaf name="mac-addr">
                <description>
                  <text>Set router MAC address.</text>
                </description>
                <type name="ncd:mac-address"/>
