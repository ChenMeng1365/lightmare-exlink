  <ext:task-name value="soc"/>
  <notification name="l2-loop-detect-alarm">
    <description>
      <text>L2 loop detect alarmed, a loop occurs on an interface.</text>
    </description>
    <leaf name="chassis-id">
      <type name="uint32"/>
      <description>
        <text>Chassis ID of the interface where a loop occurs.</text>
      </description>
    </leaf>
    <leaf name="slot">
      <type name="uint32"/>
      <description>
        <text>Slot ID of the interface where a loop occurs. When a device works in 1:1 backup mode, the slot ID is displayed as 0.</text>
      </description>
    </leaf>
    <leaf name="interface">
      <type name="string">
        <length value="1..63"/>
      </type>
      <description>
        <text>Name of the interface where a loop occurs.</text>
      </description>
    </leaf>
    <leaf name="vlan-id">
      <type name="uint32"/>
      <description>
        <text>ID of the VLAN that the interface joins. If the interface is a member of a VLANIF interface, the VLANID value ranges from 1 to 4094. If the interface is not a member of a VLANIF interface, the VLANID value is 0.</text>
      </description>
    </leaf>
  </notification>
  <notification name="l2-loop-detect-alarm-clear">
    <description>
      <text>L2 loop detect cleared, the loop on an interface has cleared.</text>
    </description>
    <leaf name="chassis-id">
      <type name="uint32"/>
      <description>
        <text>Chassis ID of the interface where a loop occurs.</text>
      </description>
    </leaf>
    <leaf name="slot">
      <type name="uint32"/>
      <description>
        <text>Slot ID of the interface where a loop occurs. When a device works in 1:1 backup mode, the slot ID is displayed as 0.</text>
      </description>
    </leaf>
    <leaf name="interface">
      <type name="string">
        <length value="1..63"/>
      </type>
      <description>
        <text>Name of the interface where a loop occurs.</text>
      </description>
    </leaf>
    <leaf name="vlan-id">
      <type name="uint32"/>
      <description>
        <text>ID of the VLAN that the interface joins. If the interface is a member of a VLANIF interface, the VLANID value ranges from 1 to 4094. If the interface is not a member of a VLANIF interface, the VLANID value is 0.</text>
      </description>
    </leaf>
  </notification>
  <notification name="socattack-alarm">
    <description>
      <text>Security Operation Center detected one attack.</text>
    </description>
    <leaf name="sequenece-number">
      <type name="uint32"/>
      <description>
        <text>Sequence number.</text>
      </description>
    </leaf>
    <leaf name="possibility">
      <type name="string">
        <length value="1..63"/>
      </type>
      <description>
        <text>Possibility of being attacked.</text>
      </description>
    </leaf>
    <leaf name="reason">
      <type name="string">
        <length value="1..128"/>
      </type>
      <description>
        <text>Reason for an attack.</text>
      </description>
    </leaf>
    <leaf name="interface">
      <type name="string">
        <length value="1..63"/>
      </type>
      <description>
        <text>Name of a main interface that is being attacked.</text>
      </description>
    </leaf>
    <leaf name="sub-interface">
      <type name="string">
        <length value="1..63"/>
      </type>
      <description>
        <text>Name of a logic interface that is being attacked.</text>
      </description>
    </leaf>
    <leaf name="pe-vlan">
      <type name="uint32"/>
      <description>
        <text>Outer VLAN ID.</text>
      </description>
    </leaf>
    <leaf name="ce-vlan">
      <type name="uint32"/>
      <description>
        <text>Inner VLAN tag.</text>
      </description>
    </leaf>
    <leaf name="mac-address">
      <type name="pub-type:mac-address"/>
      <description>
        <text>MAC address of an attack source.</text>
      </description>
    </leaf>
    <leaf name="ip-address">
      <type name="inet:ipv4-address-no-zone"/>
      <description>
        <text>IP address of an attack source.</text>
      </description>
    </leaf>
    <leaf name="ipv6-address">
      <type name="inet:ipv6-address-no-zone"/>
      <description>
        <text>IPv6 address of an attack source.</text>
      </description>
    </leaf>
    <leaf name="vni-index">
      <type name="uint32"/>
      <description>
        <text>Index of a VNI that is being attacked.</text>
      </description>
    </leaf>
  </notification>
