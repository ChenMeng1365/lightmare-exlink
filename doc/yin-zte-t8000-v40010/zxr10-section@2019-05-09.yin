<?xml version="1.0" encoding="UTF-8"?>
<module name="zxr10-section"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:section="http://www.zte.com.cn/zxr10/netconf/schema/rosng/section"
        xmlns:ncd="http://www.zte.com.cn/zxr10/netconf/schema/rosng/netconf-defs">
  <namespace uri="http://www.zte.com.cn/zxr10/netconf/schema/rosng/section"/>
  <prefix value="section"/>
  <yang-version value="1"/>
  <import module="zxr10-netconf-defs">
    <prefix value="ncd"/>
  </import>
  <organization>
    <text>ZTE Corporation</text>
  </organization>
  <contact>
    <text>ZTE Corporation  
   Nanjing Institute of ZTE Corporation 
   No.68 Zijinghua Rd. Yuhuatai District, Nanjing, China 
   Tel: +86-25-52870000</text>
  </contact>
  <description>
    <text>The netconf module to describe the section.
Netconfig can get and set section config info,which include sectionid ,localport,peerip,sd status,by config module.
section state info also can be get by state module.</text>
  </description>
  <revision date="2019-05-09">
    <description>
      <text>1 add /configuration/sections/section/peeripv6</text>
    </description>
  </revision>
  <revision date="2016-11-18">
    <description>
      <text>1,Change the maxmum value of /configuration/sections/section/section-id from 4000 to performance ID 67305544.
2,Change the maxmum value of /state/sections/section/section-id from 4000 to performance ID 67305544.</text>
    </description>
  </revision>
  <revision date="2013-12-02">
    <description>
      <text>1,add /configuration/sections/section/localport/local-port if filter.</text>
    </description>
  </revision>
  <revision date="2013-10-27">
    <description>
      <text>1,add /configuration/sections/section/localport
2,add /configuration/sections/section/peerip
3,move /configuration/sections/section/local-port to /configuration/sections/section/localport/local-port
4,move  /configuration/sections/section/peer-ip to  /configuration/sections/section/peerip/peer-ip</text>
    </description>
  </revision>
  <revision date="2013-08-27">
    <description>
      <text>Initial version.</text>
    </description>
  </revision>
  <container name="configuration">
    <description>
      <text>Configuration model information of section.</text>
    </description>
    <container name="sections">
      <description>
        <text>The configuration information of section.</text>
      </description>
      <list name="section">
        <description>
          <text>The configration table in the instance of section</text>
        </description>
        <key value="section-id"/>
        <leaf name="section-id">
          <description>
            <text>Sectionid used to identify the section entity</text>
          </description>
          <type name="uint32">
            <range value="1..4000"/>
          </type>
        </leaf>
        <leaf name="sd-status">
          <description>
            <text>Section layer sd enable flag:0 disable,1 enable,default value is disable.</text>
          </description>
          <type name="enumeration">
            <enum name="disable">
              <value value="0"/>
              <description>
                <text>0:disable</text>
              </description>
            </enum>
            <enum name="enable">
              <value value="1"/>
              <description>
                <text>1:enable</text>
              </description>
            </enum>
          </type>
          <default value="disable"/>
        </leaf>
        <container name="localport">
          <description>
            <text>Configure local port.</text>
          </description>
          <presence value="Configure local port"/>
          <leaf name="local-port">
            <description>
              <text>Local port bind the section</text>
            </description>
            <type name="ncd:ifname"/>
            <mandatory value="true"/>
          </leaf>
        </container>
        <container name="peerip">
          <description>
            <text>Configure peer IP.</text>
          </description>
          <presence value="Configure peer IP"/>
          <leaf name="peer-ip">
            <description>
              <text>peerip bind the section</text>
            </description>
            <type name="ncd:ipv4-address"/>
            <mandatory value="true"/>
          </leaf>
        </container>
        <container name="peeripv6">
          <description>
            <text>Configure peer IPv6.</text>
          </description>
          <presence value="Configure peer IPv6"/>
          <leaf name="peer-ipv6">
            <description>
              <text>peeripv6 bind the section</text>
            </description>
            <type name="ncd:ipv6-address"/>
            <mandatory value="true"/>
          </leaf>
          <ncd:revise-date date="2019-05-09"/>
        </container>
      </list>
    </container>
  </container>
  <container name="state">
    <config value="false"/>
    <description>
      <text>State model information of section.</text>
    </description>
    <container name="sections">
      <description>
        <text>State data of section.</text>
      </description>
      <list name="section">
        <description>
          <text>The state table of the instance of section</text>
        </description>
        <key value="section-id"/>
        <leaf name="section-id">
          <description>
            <text>Sectionid used to identify the section entity</text>
          </description>
          <type name="uint32">
            <range value="1..4000"/>
          </type>
        </leaf>
        <leaf name="section-state">
          <description>
            <text>the state of section</text>
          </description>
          <type name="enumeration">
            <enum name="OK">
              <value value="0"/>
              <description>
                <text>section state is ok</text>
              </description>
            </enum>
            <enum name="SF">
              <value value="1"/>
              <description>
                <text>section state is sf</text>
              </description>
            </enum>
            <enum name="SD">
              <value value="2"/>
              <description>
                <text>section state is sd</text>
              </description>
            </enum>
          </type>
        </leaf>
      </list>
    </container>
  </container>
</module>
