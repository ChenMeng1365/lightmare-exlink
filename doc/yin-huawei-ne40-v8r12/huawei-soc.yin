  <grouping name="devm-group">
    <description>
      <text>Configure policies for global management and service plane protection.</text>
    </description>
    <container name="soc">
      <description>
        <text>Configure policies for global management and service plane protection.</text>
      </description>
      <container name="l2-loop-detect">
        <description>
          <text>Configure layer 2 loop detection.</text>
        </description>
        <container name="slot-l2loopdet">
          <description>
            <text>Configure layer 2 loop detection.</text>
          </description>
          <leaf name="enable">
            <type name="boolean"/>
            <default value="true"/>
            <description>
              <text>Enable/disable layer 2 loop detection.</text>
            </description>
          </leaf>
          <leaf name="packets-drop-threshold">
            <type name="uint32">
              <range value="1..65535000"/>
            </type>
            <description>
              <text>Detect threshold.</text>
            </description>
          </leaf>
        </container>
        <container name="action">
          <description>
            <text>Configure the CPU in response to layer 2 loops after the system detects an existing or a potential loop on an interface, after layer 2 loop detection is enabled.</text>
          </description>
          <container name="shutdown">
            <presence value="The initial interval between when the interface shuts down and when it goes to the Up state."/>
            <description>
              <text>Enable/disable shut down the interface after detecting an existing layer 2 loop on the interface.</text>
            </description>
            <leaf name="up-times">
              <type name="uint16">
                <range value="1..10"/>
              </type>
              <default value="5"/>
              <description>
                <text>The maximum number of consecutive times that the system allows the Down interface to go Up in an interval.</text>
              </description>
            </leaf>
            <leaf name="up-interval">
              <type name="uint16">
                <range value="1..10"/>
              </type>
              <units name="min"/>
              <default value="5"/>
              <description>
                <text>The initial interval between when the interface shuts down and when it goes to the Up state.</text>
              </description>
            </leaf>
          </container>
          <container name="trap">
            <description>
              <text>Configure a trap after detecting an existing or a potential layer 2 loop.</text>
            </description>
            <leaf name="enable">
              <type name="boolean"/>
              <default value="true"/>
              <description>
                <text>Enable/disable the system from sending a trap after the system detects a layer 2 loop.</text>
              </description>
            </leaf>
          </container>
        </container>
        <container name="loop-level-thresholds">
          <description>
            <text>List of configure the loop level threshold on a detected main interface or sub-interface.</text>
          </description>
          <list name="loop-level-threshold">
            <ext:generated-by value="system"/>
            <ext:operation-exclude value="create"/>
            <must condition="((./determined&gt;./suspect) and (./suspect&gt;./notification))"/>
            <key value="interface-type"/>
            <description>
              <text>Configure the loop level threshold on a detected main interface or sub-interface.</text>
            </description>
            <leaf name="interface-type">
              <type name="interface-type"/>
              <description>
                <text>Interface type, main interface or sub-interface.</text>
              </description>
            </leaf>
            <leaf name="determined">
              <type name="uint32">
                <range value="3..6000"/>
              </type>
              <units name="packet"/>
              <ext:dynamic-default>
                <ext:default-value value="3000">
                  <when condition="../interface-type = 'main-interface'"/>
                  <description>
                    <text>The default value is 3000 when interface-type is main-interface.</text>
                  </description>
                </ext:default-value>
                <ext:default-value value="300">
                  <when condition="../interface-type = 'sub-interface'"/>
                  <description>
                    <text>The default value is 300 when interface-type is sub-interface.</text>
                  </description>
                </ext:default-value>
              </ext:dynamic-default>
              <description>
                <text>The determined loop threshold.</text>
              </description>
            </leaf>
            <leaf name="suspect">
              <type name="uint32">
                <range value="2..5999"/>
              </type>
              <units name="packet"/>
              <ext:dynamic-default>
                <ext:default-value value="2000">
                  <when condition="../interface-type = 'main-interface'"/>
                  <description>
                    <text>The default value is 2000 when interface-type is main-interface.</text>
                  </description>
                </ext:default-value>
                <ext:default-value value="200">
                  <when condition="../interface-type = 'sub-interface'"/>
                  <description>
                    <text>The default value is 200 when interface-type is sub-interface.</text>
                  </description>
                </ext:default-value>
              </ext:dynamic-default>
              <description>
                <text>The suspect loop threshold.</text>
              </description>
            </leaf>
            <leaf name="notification">
              <type name="uint32">
                <range value="1..5998"/>
              </type>
              <units name="packet"/>
              <ext:dynamic-default>
                <ext:default-value value="1000">
                  <when condition="../interface-type = 'main-interface'"/>
                  <description>
                    <text>The default value is 1000 when interface-type is main-interface.</text>
                  </description>
                </ext:default-value>
                <ext:default-value value="100">
                  <when condition="../interface-type = 'sub-interface'"/>
                  <description>
                    <text>The default value is 100 when interface-type is sub-interface.</text>
                  </description>
                </ext:default-value>
              </ext:dynamic-default>
              <description>
                <text>The notification loop threshold.</text>
              </description>
            </leaf>
          </list>
        </container>
      </container>
    </container>
  </grouping>
  <ext:task-name value="soc"/>
  <container name="soc">
    <description>
      <text>Security operation center.</text>
    </description>
    <container name="security-operation-center">
      <presence value="Enables SOC"/>
      <description>
        <text>Enable/disable security operation center.</text>
      </description>
      <container name="attack-defend">
        <description>
          <text>Configure attack defense.</text>
        </description>
        <leaf name="enable">
          <type name="boolean"/>
          <default value="true"/>
          <description>
            <text>Enable/disable the SOC's attack defense function.</text>
          </description>
        </leaf>
        <container name="user-disable-groups">
          <presence value="Enables user-disable-group"/>
          <description>
            <text>List of a user-defined group for which attack defense is disabled. Enable/disable the user-defined group for which attack defense is disabled.</text>
          </description>
          <list name="user-disable-group">
            <key value="protocol-name"/>
            <description>
              <text>Configure a user-defined group for which attack defense is disabled.</text>
            </description>
            <leaf name="protocol-name">
              <type name="attack-defend-protocol"/>
              <description>
                <text>Specifys specific protocol packets for the user-defined group.</text>
              </description>
            </leaf>
          </list>
        </container>
        <container name="user-enable-groups">
          <presence value="Enables user-enable-group"/>
          <description>
            <text>List of a user-defined group for which attack defense is enabled. Enable/disable the user-defined group for which attack defense is enabled.</text>
          </description>
          <list name="user-enable-group">
            <key value="protocol-name"/>
            <description>
              <text>Configure a user-defined group for which attack defense is enabled.</text>
            </description>
            <leaf name="protocol-name">
              <type name="attack-defend-protocol"/>
              <description>
                <text>Specifys specific protocol packets for the user-defined group.</text>
              </description>
            </leaf>
          </list>
        </container>
      </container>
      <container name="attack-detect">
        <description>
          <text>Configure attack detection.</text>
        </description>
        <container name="threshold">
          <description>
            <text>Configure the threshold for attack detection.</text>
          </description>
          <container name="cpu-usages">
            <description>
              <text>Configure the CPU usage threshold for attack detection.</text>
            </description>
            <leaf name="cpu-usage">
              <type name="uint32">
                <range value="1..99"/>
              </type>
              <units name="%"/>
              <default value="15"/>
              <description>
                <text>The CPU usage threshold for attack detection.</text>
              </description>
            </leaf>
          </container>
          <container name="protocol-cars">
            <description>
              <text>List of set the rate threshold for sending protocol packets to the CPU and the packet loss percentage threshold for attack detection.</text>
            </description>
            <list name="protocol-car">
              <ext:generated-by value="system"/>
              <ext:operation-exclude value="create"/>
              <key value="protocol-name"/>
              <description>
                <text>Configure the rate threshold for sending protocol packets to the CPU and the packet loss percentage threshold for attack detection.</text>
              </description>
              <leaf name="protocol-name">
                <type name="protocol-name"/>
                <description>
                  <text>The name of a protocol that supports CAR.</text>
                </description>
              </leaf>
              <leaf name="min-rate">
                <type name="uint32">
                  <range value="20..4000"/>
                </type>
                <units name="pps"/>
                <ext:dynamic-default>
                  <ext:default-value value="300">
                    <when condition="../protocol-name = 'icmp'"/>
                    <description>
                      <text>The default value is 300 when protocol-name is ICMP.</text>
                    </description>
                  </ext:default-value>
                  <ext:default-value value="500">
                    <description>
                      <text>The default value is 500 when protocol-name is others.</text>
                    </description>
                  </ext:default-value>
                </ext:dynamic-default>
                <description>
                  <text>A rate threshold for CP-CAR.</text>
                </description>
              </leaf>
              <leaf name="drop-packet-percent">
                <type name="uint32">
                  <range value="0..99"/>
                </type>
                <units name="%"/>
                <ext:dynamic-default>
                  <ext:default-value value="20">
                    <when condition="../protocol-name = 'dhcp'"/>
                    <description>
                      <text>The default value is 20 when protocol-name is DHCP.</text>
                    </description>
                  </ext:default-value>
                  <ext:default-value value="20">
                    <when condition="../protocol-name = 'pppoe'"/>
                    <description>
                      <text>The default value is 20 when protocol-name is PPPOE.</text>
                    </description>
                  </ext:default-value>
                  <ext:default-value value="30">
                    <description>
                      <text>The default value is 30 when protocol-name is others.</text>
                    </description>
                  </ext:default-value>
                </ext:dynamic-default>
                <description>
                  <text>The packet loss percentage threshold for CP-CAR.</text>
                </description>
              </leaf>
            </list>
          </container>
        </container>
      </container>
      <container name="attack-trace">
        <description>
          <text>Configure attack tracing.</text>
        </description>
        <container name="attack-reason">
          <description>
            <text>Configure the threshold for determining the cause of an attack event.</text>
          </description>
          <leaf name="broadcast-flood-percent">
            <type name="uint8">
              <range value="30..80"/>
            </type>
            <units name="%"/>
            <default value="50"/>
            <description>
              <text>Specifys the threshold for the percentage of broadcast packets to the total number of sampled packets.</text>
            </description>
          </leaf>
          <leaf name="change-source-packet-percent">
            <type name="uint8">
              <range value="2..10"/>
            </type>
            <units name="%"/>
            <default value="5"/>
            <description>
              <text>Specifys the threshold for the percentage of packets with varied source addresses to the total number of sampled packets.</text>
            </description>
          </leaf>
          <leaf name="app-packet-percent">
            <type name="uint8">
              <range value="20..80"/>
            </type>
            <units name="%"/>
            <default value="30"/>
            <description>
              <text>Specifys the threshold for the percentage of the specified protocol packets to the total number of sampled packets.</text>
            </description>
          </leaf>
        </container>
        <container name="attack-location-type">
          <description>
            <text>Configure the threshold for determining the location of an attack event.</text>
          </description>
          <leaf name="interface-percent">
            <type name="uint8">
              <range value="15..99"/>
            </type>
            <units name="%"/>
            <default value="20"/>
            <description>
              <text>Specifys the threshold for the percentage of the number of packets received on a physical interface to the total number of sampled packets.</text>
            </description>
          </leaf>
          <leaf name="sub-interface-percent">
            <type name="uint8">
              <range value="10..99"/>
            </type>
            <units name="%"/>
            <default value="10"/>
            <description>
              <text>Specifys the threshold for the percentage of the number of packets received on a logical interface to the total number of sampled packets.</text>
            </description>
          </leaf>
          <leaf name="vlan-percent">
            <type name="uint8">
              <range value="15..50"/>
            </type>
            <units name="%"/>
            <default value="20"/>
            <description>
              <text>Specifys the threshold for the percentage of the number of single-tagged VLAN packets with a specified VLAN ID to the total number of sampled packets.</text>
            </description>
          </leaf>
          <leaf name="source-mac-percent">
            <type name="uint8">
              <range value="5..50"/>
            </type>
            <units name="%"/>
            <default value="10"/>
            <description>
              <text>Specifys the threshold for the percentage of the number of packets with a specified source MAC address to the total number of sampled packets.</text>
            </description>
          </leaf>
          <leaf name="source-ip-percent">
            <type name="uint8">
              <range value="5..50"/>
            </type>
            <units name="%"/>
            <default value="10"/>
            <description>
              <text>Specifys the threshold for the percentage of the number of packets with a specified source IP address to the total number of sampled packets.</text>
            </description>
          </leaf>
          <leaf name="qinq-percent">
            <type name="uint8">
              <range value="3..15"/>
            </type>
            <units name="%"/>
            <default value="10"/>
            <description>
              <text>Specifys the threshold for the percentage of the number of double-tagged VLAN packets with specified inner and outer VLAN IDs to the total number of sampled packets.</text>
            </description>
          </leaf>
          <leaf name="vni-percent">
            <type name="uint8">
              <range value="15..50"/>
            </type>
            <units name="%"/>
            <default value="20"/>
            <description>
              <text>Specifys the threshold for the percentage of the number of packets with a specified VNI to the total number of sampled packets.</text>
            </description>
          </leaf>
        </container>
        <container name="attack-probabilitys">
          <description>
            <text>List of Configure the threshold for determining the probability of an attack event.</text>
          </description>
          <list name="attack-probability">
            <ext:generated-by value="system"/>
            <ext:operation-exclude value="create"/>
            <key value="determine-object"/>
            <description>
              <text>Configure the threshold for determining the probability of an attack event.</text>
            </description>
            <leaf name="determine-object">
              <type name="determine-probability-object"/>
              <description>
                <text>Determining object of probability of attack event.</text>
              </description>
            </leaf>
            <leaf name="determined-threshold-value">
              <type name="uint8"/>
              <ext:dynamic-default>
                <ext:default-value value="90">
                  <when condition="../determine-object = 'broadcast-flood'"/>
                  <description>
                    <text>The default value is 90 when determine-object is broadcast-flood.</text>
                  </description>
                </ext:default-value>
                <ext:default-value value="90">
                  <when condition="../determine-object = 'app-error-percent'"/>
                  <description>
                    <text>The default value is 90 when determine-object is app-error-percent.</text>
                  </description>
                </ext:default-value>
                <ext:default-value value="80">
                  <description>
                    <text>The default value is 80 when determine-object is others.</text>
                  </description>
                </ext:default-value>
              </ext:dynamic-default>
              <must condition="((../determine-object = 'top5-user' or ../determine-object = 'top5-source-mac' or ../determine-object = 'top5-source-ip') and (../determined-threshold-value&gt;=80 and ../determined-threshold-value&lt;=99)) or ((../determine-object = 'broadcast-flood' or ../determine-object = 'app-error-percent') and (../determined-threshold-value&gt;=90 and ../determined-threshold-value&lt;=99))"/>
              <description>
                <text>Specifys the threshold for the percentage of the number of packets with specified characteristics to the total number of sampled packets. BROADCAST[90,99], APP_ERR[90,99], others[80,99]. determine-object with different value have different default determined-threshold-value values.</text>
              </description>
            </leaf>
            <leaf name="suspicion-threshold-value">
              <type name="uint8"/>
              <ext:dynamic-default>
                <ext:default-value value="70">
                  <when condition="../determine-object = 'broadcast-flood'"/>
                  <description>
                    <text>The default value is 70 when determine-object is broadcast-flood.</text>
                  </description>
                </ext:default-value>
                <ext:default-value value="75">
                  <when condition="../determine-object = 'app-error-percent'"/>
                  <description>
                    <text>The default value is 75 when determine-object is app-error-percent.</text>
                  </description>
                </ext:default-value>
                <ext:default-value value="60">
                  <description>
                    <text>The default value is 60 when determine-object is others.</text>
                  </description>
                </ext:default-value>
              </ext:dynamic-default>
              <must condition="((../determine-object = 'top5-user' or ../determine-object = 'top5-source-mac' or ../determine-object = 'top5-source-ip') and (../suspicion-threshold-value&gt;=60 and ../suspicion-threshold-value&lt;=80)) or (../determine-object = 'broadcast-flood' and ../suspicion-threshold-value&gt;=70 and ../suspicion-threshold-value&lt;=90) or (../determine-object = 'app-error-percent' and ../suspicion-threshold-value&gt;=75 and ../suspicion-threshold-value&lt;=90)"/>
              <description>
                <text>Specifys the suspicion threshold for the percentage of the number of packets with specified characteristics to the total number of sampled packets. BROADCAST[70,90], APP_ERR[75,90], others[60,80]. determine-object with different value have different default suspicion-threshold-value values.</text>
              </description>
            </leaf>
            <leaf name="notification-threshold-value">
              <type name="uint8"/>
              <ext:dynamic-default>
                <ext:default-value value="50">
                  <when condition="../determine-object = 'broadcast-flood'"/>
                  <description>
                    <text>The default value is 50 when determine-object is broadcast-flood.</text>
                  </description>
                </ext:default-value>
                <ext:default-value value="60">
                  <when condition="../determine-object = 'app-error-percent'"/>
                  <description>
                    <text>The default value is 60 when determine-object is app-error-percent.</text>
                  </description>
                </ext:default-value>
                <ext:default-value value="40">
                  <description>
                    <text>The default value is 40 when determine-object is others.</text>
                  </description>
                </ext:default-value>
              </ext:dynamic-default>
              <must condition="((../determine-object = 'top5-user' or ../determine-object = 'top5-source-mac' or ../determine-object = 'top5-source-ip') and (../notification-threshold-value&gt;=40 and ../notification-threshold-value&lt;=60)) or (../determine-object = 'broadcast-flood' and ../notification-threshold-value&gt;=50 and ../notification-threshold-value&lt;=70) or (../determine-object = 'app-error-percent' and ../notification-threshold-value&gt;=60 and ../notification-threshold-value&lt;=75)"/>
              <description>
                <text>Specifys the notification threshold for the percentage of the number of packets with specified characteristics to the total number of sampled packets. BROADCAST[50,70], APP_ERR[60,75], others[40,60]. determine-object with different value have different default notification-threshold-value.</text>
              </description>
            </leaf>
          </list>
        </container>
