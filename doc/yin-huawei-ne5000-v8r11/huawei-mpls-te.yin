  <ext:task-name value="mpls-te"/>
  <augment target-node="/mpls:mpls">
    <description>
      <text>MPLS TE Configuration information augment to module MPLS.</text>
    </description>
    <container name="te">
      <description>
        <text>Configure MPLS TE parameters.</text>
      </description>
      <leaf name="enable">
        <when condition="/mpls:mpls/mpls:common/mpls:global/mpls:mpls-enable='true'"/>
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable MPLS TE.</text>
        </description>
      </leaf>
      <container name="global">
        <when condition="../enable='true'"/>
        <must condition="(p2mp-leaf-switch-delay-time=0 and p2mp-leaf-delete-delay-time=0) or (not(p2mp-leaf-switch-delay-time) and not(p2mp-leaf-delete-delay-time))"/>
        <description>
          <text>Configure global MPLS TE basic parameters.</text>
        </description>
        <leaf name="switch-delay-time">
          <type name="uint32">
            <range value="0..600000"/>
          </type>
          <units name="ms"/>
          <default value="5000"/>
          <description>
            <text>Delay time for switching the TE traffic from a Primary CR-LSP to a Modified CR-LSP.</text>
          </description>
        </leaf>
        <leaf name="delete-delay-time">
          <type name="uint32">
            <range value="0..600000"/>
          </type>
          <units name="ms"/>
          <default value="20000"/>
          <description>
            <text>Delay time for deleting the Primary CR-LSP after the TE traffic is switched to the Modified CR-LSP.</text>
          </description>
        </leaf>
        <leaf name="frr-timer-weight">
          <type name="uint32">
            <range value="0..604800"/>
          </type>
          <default value="300"/>
          <description>
            <text>FRR binding upgrade weight. If the value is 0, stop the FRR timer. The FRR timer interval depends on the value and the number of LSPs.</text>
          </description>
        </leaf>
        <leaf name="frr-switch-delay-time">
          <type name="uint32">
            <range value="100..1800000"/>
          </type>
          <units name="ms"/>
          <description>
            <text>When hot-standby and frr exist at the same time, delay time for switching the TE traffic from a Primary CR-LSP to FRR.</text>
          </description>
        </leaf>
        <leaf name="pce-delegate">
          <type name="delegate-type"/>
          <default value="no"/>
          <description>
            <text>Specifies the enabling state of PCE delegation capability.</text>
          </description>
        </leaf>
        <leaf name="pce-inter-layer-delegate">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable PCE inter-layer delegation.</text>
          </description>
        </leaf>
        <leaf name="pce-cleanup-lsp-state">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable PCE clean lsp state.</text>
          </description>
        </leaf>
        <leaf name="pce-cleanup-initiated-lsp">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable PCE cleanup initiated lsp.</text>
          </description>
        </leaf>
        <leaf name="pce-clean-stitch-label">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable PCE cleanup stitch label.</text>
          </description>
        </leaf>
        <leaf name="path-selection">
          <type name="path-selection-mode"/>
          <description>
            <text>Enable path selection and set mode as overload.</text>
          </description>
        </leaf>
        <leaf name="lsp-loopback-clear-time">
          <type name="uint32">
            <range value="1..2880"/>
          </type>
          <default value="5"/>
          <description>
            <text>LSP loopback auto clear time.</text>
          </description>
        </leaf>
        <leaf name="ip-prefix-delete-time">
          <type name="uint32">
            <range value="30..604800"/>
          </type>
          <default value="300"/>
          <description>
            <text>Auto Primary Tunnel Delete Time.</text>
          </description>
        </leaf>
        <leaf name="slsp-deduct-rsvp-bandwidth">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable rsvp bandwidth deduction.</text>
          </description>
        </leaf>
        <leaf name="reoptimization-link-disable">
          <type name="boolean"/>
          <default value="true"/>
          <description>
            <text>Enable/disable global interface reoptimization capability.</text>
          </description>
        </leaf>
        <leaf name="rsvp-entropy-label">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable RSVP P2P tunnel entropy label.</text>
          </description>
        </leaf>
        <leaf name="bandwidth-auto-config-disable">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable automatic bandwidth configuration.</text>
          </description>
        </leaf>
        <leaf name="suppress-flapping-disable">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable the TE LSP flapping suppression.</text>
          </description>
        </leaf>
        <leaf name="path-verification">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable MPLS TE tunnel path verify.</text>
          </description>
        </leaf>
        <leaf name="soft-preemption">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable soft preemption.</text>
          </description>
        </leaf>
        <leaf name="explicit-path-deletion-down">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable SR LSP explicit path deletion down.</text>
          </description>
        </leaf>
        <leaf name="self-ping">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable self-ping capability.</text>
          </description>
        </leaf>
        <leaf name="self-ping-duration">
          <type name="uint32">
            <range value="60..65535"/>
          </type>
          <units name="s"/>
          <default value="1800"/>
          <description>
            <text>Global self-ping timeout period. Value 65535 indicates no limit on the detection time.</text>
          </description>
        </leaf>
        <leaf name="p2mp-enable">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable P2MP TE capability.</text>
          </description>
        </leaf>
        <leaf name="p2mp-auto-frr">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable P2MP TE auto-frr capability.</text>
          </description>
        </leaf>
        <leaf name="p2mp-frr-mbb-type">
          <type name="mpls-p2mp-te-frr-mbb-type"/>
          <default value="sub-lsp"/>
          <description>
            <text>P2MP FRR MBB Mode.</text>
          </description>
        </leaf>
        <leaf name="p2mp-leaf-switch-delay-time">
          <type name="uint16"/>
          <units name="ms"/>
          <description>
            <text>P2MP leaf switch delay time.</text>
          </description>
        </leaf>
        <leaf name="p2mp-leaf-delete-delay-time">
          <type name="uint16"/>
          <units name="ms"/>
          <description>
            <text>P2MP leaf delete delay time.</text>
          </description>
        </leaf>
        <leaf name="auto-frr-enable">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable auto FRR capability.</text>
          </description>
        </leaf>
        <leaf name="auto-frr-self-adapting">
          <when condition="../auto-frr-enable='true'"/>
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable auto FRR self-adapting.</text>
          </description>
        </leaf>
        <leaf name="auto-frr-reoptimization">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable the enabling state of auto bypass Reoptimization.</text>
          </description>
        </leaf>
        <leaf name="auto-frr-frequency">
          <when condition="../auto-frr-reoptimization='true'"/>
          <type name="uint32">
            <range value="60..604800"/>
          </type>
          <default value="3600"/>
          <description>
            <text>Specifies the frequency of auto bypass Reoptimization.</text>
          </description>
        </leaf>
        <leaf name="auto-bandwidth-enable">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable the tunnel's traffic rate sampling switch.</text>
          </description>
        </leaf>
        <leaf name="auto-bandwidth-interval">
          <when condition="../auto-bandwidth-enable='true'"/>
          <type name="uint32">
            <range value="1..604800"/>
          </type>
          <units name="s"/>
          <default value="300"/>
          <description>
            <text>Set interval of Tunnel traffic rate sampling.</text>
          </description>
        </leaf>
        <leaf name="flood-bandwidth-enable">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable flooding TE link bandwidth periodically capability. When TE LSP reserves or releases bandwidth with the changing rate not reaching the flooding threshold of TE link, it will not flood bandwidth to network. In order to flood bandwidth to network as soon as possible, and to avoid waste of network resource caused by frequent flooding, it will flood changed bandwidth to network periodically.</text>
          </description>
        </leaf>
        <leaf name="flood-bandwidth-interval">
          <when condition="../flood-bandwidth-enable='true'"/>
          <type name="uint32">
            <range value="10..43200"/>
          </type>
          <units name="s"/>
          <default value="30"/>
          <description>
            <text>Interval at which a TE interface floods bandwidth over the network. The bandwidth that a TE LSP reserves or releases on an interface may be only a small part compared with the reservable bandwidth of the interface, which does not reach the flooding threshold of the interface. To flood the interface bandwidth over the network and avoid frequent flooding, the TE NE is configured to flood bandwidth over the network periodically.</text>
          </description>
        </leaf>
        <leaf name="ds-te-mode">
          <type name="ds-te-mode-type"/>
          <must condition="../ds-te-mode='non-ietf' or (../pce-delegate='no' and ../soft-preemption='false' and ../p2mp-enable='false')"/>
          <default value="non-ietf"/>
          <description>
            <text>DS-TE mode.</text>
          </description>
        </leaf>
        <container name="bcm-model">
          <when condition="../ds-te-mode='ietf'"/>
          <description>
            <text>Configure DS-TE bcm.</text>
          </description>
          <leaf name="bcm-mode">
            <type name="ds-te-bcm-type"/>
            <default value="rdm"/>
            <description>
              <text>DS-TE bandwidth constraints model.</text>
            </description>
          </leaf>
        </container>
      </container>
      <container name="tunnel-locks">
        <description>
          <text>List of Tunnel locks.</text>
        </description>
        <list name="tunnel-lock">
          <key value="name"/>
          <description>
            <text>Configure Tunnel lock.</text>
          </description>
          <leaf name="name">
            <type name="leafref">
              <path value="/ifm:ifm/ifm:interfaces/ifm:interface/ifm:name"/>
            </type>
            <must condition="/ifm:ifm/ifm:interfaces/ifm:interface[ifm:name=current()]/ifm:type='Tunnel' and (/ifm:ifm/ifm:interfaces/ifm:interface[ifm:name=current()]/tnlm:tunnel-protocol/mpls-te:te-tunnel/mpls-te:common-attributes/mpls-te:signal-protocol='rsvp-te' or /ifm:ifm/ifm:interfaces/ifm:interface[ifm:name=current()]/tnlm:tunnel-protocol/mpls-te:te-tunnel/mpls-te:common-attributes/mpls-te:signal-protocol='segment-routing')"/>
            <description>
              <text>Specifies the name of a TE tunnel interface. The name is case-insensitive.</text>
            </description>
          </leaf>
        </list>
      </container>
      <container name="rsvp">
        <description>
          <text>Configure MPLS RSVP-TE parameters.</text>
        </description>
        <leaf name="enable">
          <when condition="../../enable='true'"/>
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable RSVP TE.</text>
          </description>
        </leaf>
        <container name="global">
          <when condition="../enable='true'"/>
          <description>
            <text>Configure global MPLS RSVP-TE.</text>
          </description>
          <leaf name="hello-enable">
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable Hello enabling state.</text>
            </description>
          </leaf>
          <leaf name="max-hello-miss-times">
            <when condition="../hello-enable='true'"/>
            <type name="uint32">
              <range value="3..10"/>
            </type>
            <default value="3"/>
            <description>
              <text>Maximum number of Hello packet loss times. When the number of Hello packet refreshing (from the NE) times exceeds the configured maximum number, and the NE does not receive a response from its peer end, the NE considers that the session with the peer end is lost.</text>
            </description>
          </leaf>
          <leaf name="hello-interval">
            <when condition="../hello-enable='true'"/>
            <type name="uint32">
              <range value="1..25"/>
            </type>
            <units name="s"/>
            <default value="3"/>
            <description>
              <text>Interval for refreshing Hello packets. It is the interval of refreshing a Hello packet from an NE to its peer after a Hello session is established.</text>
            </description>
          </leaf>
          <leaf name="support-gr-enable">
            <when condition="../hello-enable='true'"/>
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable support GR.</text>
            </description>
          </leaf>
          <leaf name="keep-multiplier">
            <type name="uint16">
              <range value="3..255"/>
            </type>
            <default value="3"/>
            <description>
              <text>Timeout multiplier of soft state PSB or RSB. If the local NE does not receive a packet for refreshing the TE LSP soft state from its upstream or downstream neighbor in the period (that is a specified multiplier of the refreshing cycle), the NE considers that the soft state times out and will delete the TE LSP.</text>
            </description>
          </leaf>
          <leaf name="refresh-interval">
            <type name="uint32">
              <range value="10..65535"/>
            </type>
            <units name="s"/>
            <default value="30"/>
            <description>
              <text>Cycle of refreshing a PATH or a RESV message (for soft state maintenance).</text>
            </description>
          </leaf>
          <leaf name="resv-confirm-enable">
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable reservation confirmation capability. After the reservation confirmation capability is configured on an NE, all starting nodes of TE tunnels whose destinations are the NE will send source reservation confirmation messages to the NE after receiving the RESV messages from the NE. This capability is not applicable for single-direction LSPs. Enabling reservation confirmation capability in the single-direction LSP scenario is not recommended.</text>
            </description>
          </leaf>
          <leaf name="srefresh-enable">
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable summary refresh.</text>
            </description>
          </leaf>
          <leaf name="reliable-delivery-enable">
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable reliable delivery.</text>
            </description>
          </leaf>
          <leaf name="retransmission-interval">
            <type name="uint32">
              <range value="500..5000"/>
            </type>
            <units name="ms"/>
            <default value="5000"/>
            <description>
              <text>Interval at which interface summary messages are retransmitted.</text>
            </description>
          </leaf>
          <leaf name="retransmission-increment-value">
            <type name="uint32">
              <range value="1..10"/>
            </type>
            <units name="ms"/>
            <default value="1"/>
            <description>
              <text>Incremental value for retransmitting interface summary. If an NE does not receive a response from the peer end after sending a message to the peer end, the NE will start the retransmission mechanism. The retransmission interval increases with the increase of the retransmission incremental value. The formula is as following: retransmission interval = last retransmission interval * (1 + retransmission incremental value).</text>
            </description>
          </leaf>
          <leaf name="challenge-retransmission-interval">
            <type name="uint32">
              <range value="500..10000"/>
            </type>
            <units name="ms"/>
            <default value="1000"/>
            <description>
              <text>Challenge message retransmission interval. If the NE does not receive a response message from the peer end after sending a Challenge message for handshake authentication to the peer end, the NE will retransmit the Challenge message.</text>
            </description>
          </leaf>
          <leaf name="max-challenge-miss-times">
            <type name="uint32">
              <range value="1..10"/>
            </type>
            <default value="3"/>
            <description>
              <text>Maximum challenge loss times. If the times of a retransmitting a Challenge message from the NE to its neighbor exceed the maximum challenge loss times, the NE considers that the handshake cannot be established and will not send a Challenge message again.</text>
            </description>
          </leaf>
          <leaf name="is-send-down-reason">
            <type name="boolean"/>
            <default value="true"/>
            <description>
              <text>Enable/disable RSVP send down reason capability. When this command is configured on a non-ingress node, RSVP can send the down reasons to the ingress through path error message and resv tear message.</text>
            </description>
          </leaf>
          <leaf name="transit-rro1">
            <type name="transit-rro-type"/>
            <must condition="not(../transit-rro1='disable')"/>
            <default value="incoming-with-label"/>
            <description>
              <text>Transit-rro1.
Special explanation:
Constraint: For transit RRO types: The value of the inbound interface type can be incoming or incoming-with-label. The value of the outbound interface type can be outgoing or outgoing-with-label. The value of the router ID type can be router-id or router-id-with-label. The value cannot be set to disable for transit-rro1. If transit-rro1 is configured, one of the inbound interface type, outbound interface type, and router ID type can only be selected form transit RRO types. The transit RRO type of transit-rro1, transit-rro2, and transit-rro3 nodes must be different from one another.</text>
            </description>
          </leaf>
          <leaf name="transit-rro2">
            <type name="transit-rro-type"/>
            <must condition="(((../transit-rro1='incoming' or ../transit-rro1='incoming-with-label') and not(../transit-rro2='incoming' or ../transit-rro2='incoming-with-label')) or ((../transit-rro1='router-id' or ../transit-rro1='router-id-with-label') and not(../transit-rro2='router-id' or ../transit-rro2='router-id-with-label')) or ((../transit-rro1='outgoing' or ../transit-rro1='outgoing-with-label') and not(../transit-rro2='outgoing' or ../transit-rro2='outgoing-with-label')))"/>
            <default value="router-id-with-label"/>
            <description>
              <text>Transit-rro2.
Special explanation:
Constraint: The value if disable type can be disable. For transit RRO types: The value of the inbound interface type can be incoming or incoming-with-label. The value of the outbound interface type can be outgoing or outgoing-with-label. The value of the router ID type can be router-id or router-id-with-label. If transit-rro2 is configured, this leaf can be set to disable. Or one of the inbound interface type, outbound interface type, and router ID type form transit RRO types can only be selected for transit-rro2 node, and the transit RRO type of transit-rro1, transit-rro2, and transit-rro3 nodes must be different from one another.</text>
            </description>
          </leaf>
          <leaf name="transit-rro3">
            <type name="transit-rro-type"/>
            <must condition="../transit-rro3='disable' or (((../transit-rro1='incoming' or ../transit-rro1='incoming-with-label') and (((../transit-rro2='outgoing' or ../transit-rro2='outgoing-with-label') and (../transit-rro3='router-id' or ../transit-rro3='router-id-with-label')) or ((../transit-rro2='router-id' or ../transit-rro2='router-id-with-label') and (../transit-rro3='outgoing' or ../transit-rro3='outgoing-with-label')))) or ((../transit-rro1='outgoing' or ../transit-rro1='outgoing-with-label') and (((../transit-rro2='incoming' or ../transit-rro2='incoming-with-label') and (../transit-rro3='router-id' or ../transit-rro3='router-id-with-label')) or ((../transit-rro2='router-id' or ../transit-rro2='router-id-with-label') and (../transit-rro3='incoming' or ../transit-rro3='incoming-with-label')))) or ((../transit-rro1='router-id' or ../transit-rro1='router-id-with-label') and (((../transit-rro2='incoming' or ../transit-rro2='incoming-with-label') and (../transit-rro3='outgoing' or ../transit-rro3='outgoing-with-label')) or ((../transit-rro2='outgoing' or ../transit-rro2='outgoing-with-label') and (../transit-rro3='incoming' or ../transit-rro3='incoming-with-label')))))"/>
            <default value="outgoing"/>
            <description>
              <text>Transit-rro3.
Special explanation:
Constraint: The value if disable type can be disable. For transit RRO types: The value of the inbound interface type can be incoming or incoming-with-label. The value of the outbound interface type can be outgoing or outgoing-with-label. The value of the router ID type can be router-id or router-id-with-label. If transit-rro2 is set to disable, transit-rro3 can only be set to disable, too. Or one of the inbound interface type, outbound interface type, and router ID type can only be selected for transit-rro3 node form transit RRO types, and the transit RRO type of transit-rro1, transit-rro2, and transit-rro3 nodes must be different from one another.</text>
            </description>
          </leaf>
          <leaf name="egress-rro1">
            <type name="egress-rro-type"/>
            <must condition="not(../egress-rro1='disable')"/>
            <default value="incoming-with-label"/>
            <description>
              <text>Egress-rro1.
Special explanation:
Constraint: For egress RRO types: For egress RRO types: The value of the inbound interface type can be incoming or incoming-with-label. The value of the router ID type can be router-id or router-id-with-label. The value if disable type can be disable. The value cannot be set to disable for egress-rro1. If egress-rro1 is delivered, either the inbound interface type or router ID type can be selected form egress RRO types. The egress RRO type of egress-rro1 must be differ from that of egress-rro2.</text>
            </description>
          </leaf>
          <leaf name="egress-rro2">
            <type name="egress-rro-type"/>
            <must condition="((../egress-rro1='incoming' or ../egress-rro1='incoming-with-label') and not(../egress-rro2='incoming' or ../egress-rro2='incoming-with-label')) or ((../egress-rro1='router-id' or ../egress-rro1='router-id-with-label') and not(../egress-rro2='router-id' or ../egress-rro2='router-id-with-label'))"/>
            <default value="router-id-with-label"/>
            <description>
              <text>Egress-rro2.
Special explanation:
Constraint: For egress RRO types: The value of the inbound interface type can be incoming or incoming-with-label. The value of the router ID type can be router-id or router-id-with-label. The value if disable type can be disable. If egress-rro2 is configured, this leaf can be set to disable. Or either the inbound interface type or router ID type can be selected form egress RRO types, and the egress RRO type of egress-rro1 must be differ from that of egress-rro2.</text>
            </description>
          </leaf>
          <leaf name="frr-bw-compatible">
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable fast-reroute bandwidth storage in memory, and the default mode is float.</text>
            </description>
          </leaf>
          <leaf name="without-affinity">
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable the session-attribute object in RSVP message without affinity at ingress.</text>
            </description>
          </leaf>
          <leaf name="soft-preempt-interval">
            <type name="uint32">
              <range value="30..300"/>
            </type>
            <units name="s"/>
            <default value="30"/>
            <description>
              <text>Interval for soft preempt timer.</text>
            </description>
          </leaf>
          <leaf name="graceful-shutdown-enable">
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable graceful shutdown for system.</text>
            </description>
          </leaf>
          <leaf name="graceful-shutdown-time">
            <type name="uint16">
              <range value="30..1200"/>
            </type>
            <units name="s"/>
            <description>
              <text>Delayed deletion period of Graceful Shutdown.</text>
            </description>
          </leaf>
          <leaf-list name="nodeid-session">
            <when condition="../hello-enable='true'"/>
            <type name="inet:ip-address"/>
            <max-elements value="128"/>
            <description>
              <text>Configure a list of establishes hello sessions with a single-hop or multi-hop neighbor of a specified IP address.</text>
            </description>
          </leaf-list>
        </container>
        <container name="peer-infos">
          <config value="false"/>
          <description>
            <text>List of MPLS RSVP-TE neighbor information.</text>
          </description>
          <list name="peer-info">
            <key value="type ip-address"/>
            <description>
              <text>Operational state of MPLS RSVP-TE neighbor query information.</text>
            </description>
            <leaf name="type">
              <type name="neighbor-address-type"/>
              <description>
                <text>Type of a neighbor.</text>
              </description>
            </leaf>
            <leaf name="ip-address">
              <type name="inet:ipv4-address-no-zone"/>
              <description>
                <text>IP address of a neighbor.</text>
              </description>
            </leaf>
            <leaf name="local-interface-name">
              <type name="pub-type:if-name"/>
              <description>
                <text>Local interface name for normal neighbor.</text>
              </description>
            </leaf>
            <leaf name="src-instance">
              <type name="pub-type:hex-binary"/>
              <description>
                <text>Src instance of a neighbor.</text>
              </description>
            </leaf>
            <leaf name="neighbor-src-instance">
              <type name="pub-type:hex-binary"/>
              <description>
                <text>Neighbor src instance of a neighbor.</text>
              </description>
            </leaf>
            <leaf name="psb-count">
              <type name="uint32"/>
              <description>
                <text>Psb count of a neighbor.</text>
              </description>
            </leaf>
            <leaf name="rsb-count">
              <type name="uint32"/>
              <description>
                <text>Rsb count of a neighbor.</text>
              </description>
            </leaf>
            <leaf name="hello-type-sent">
              <type name="neighbor-hello-sent-type"/>
              <description>
                <text>Hello type sent of a neighbor.</text>
              </description>
            </leaf>
            <leaf name="hello-extension-enable">
              <type name="boolean"/>
              <description>
                <text>Hello extension of a neighbor.</text>
              </description>
            </leaf>
            <leaf name="srefresh-enable">
              <type name="boolean"/>
              <description>
                <text>Srefresh enable state of a neighbor.</text>
              </description>
            </leaf>
            <leaf name="authentication-enable">
              <type name="boolean"/>
              <description>
                <text>Authentication enable state of a neighbor.</text>
              </description>
            </leaf>
            <leaf name="authentication-type">
              <type name="neighbor-authentication-type"/>
              <description>
                <text>Authentication type of a neighbor.</text>
              </description>
            </leaf>
            <leaf name="keychain-name">
              <type name="string">
                <length value="1..47"/>
              </type>
              <description>
                <text>Keychain name of a neighbor.</text>
              </description>
            </leaf>
            <leaf name="window-size">
              <type name="uint8">
                <range value="1..255"/>
              </type>
              <description>
                <text>Window size of a neighbor.</text>
              </description>
            </leaf>
            <leaf name="last-valid-sequence-h">
              <type name="uint32"/>
              <description>
                <text>Upper 32 bits of last valid sequence number.</text>
              </description>
            </leaf>
            <leaf name="last-valid-sequence-l">
              <type name="uint32"/>
              <description>
                <text>Lower 32 bits of last valid sequence number.</text>
              </description>
            </leaf>
          </list>
        </container>
        <container name="neighbors">
          <when condition="../enable='true'"/>
          <description>
            <text>List of MPLS RSVP-TE neighbors.</text>
          </description>
          <list name="neighbor">
            <key value="peer-ip-address"/>
            <description>
              <text>Configure RSVP-TE neighbor information.</text>
            </description>
            <leaf name="peer-ip-address">
              <type name="inet:ipv4-address-no-zone"/>
              <description>
                <text>IP address of a neighbor.</text>
              </description>
            </leaf>
            <container name="authentication">
              <description>
                <text>Configure Neighbor configuration of RSVP TE authentication.</text>
              </description>
              <leaf name="enable">
                <type name="boolean"/>
                <default value="false"/>
                <description>
                  <text>Enable/disable neighbor authentication.</text>
                </description>
              </leaf>
              <leaf name="type">
                <when condition="../enable='true'"/>
                <type name="rsvp-authentication-type"/>
                <mandatory value="true"/>
                <description>
                  <text>Authentication type on an RSVP-TE neighbor.</text>
                </description>
              </leaf>
              <leaf name="keychain-name">
                <when condition="../enable='true' and ../type='keychain'"/>
                <type name="string">
                  <length value="1..47"/>
                  <pattern value="[^A-Z]+"/>
                </type>
                <mandatory value="true"/>
                <ext:case-sensitivity value="lower-only"/>
                <description>
                  <text>Keychain name. The name is case insensitive.</text>
                </description>
              </leaf>
              <leaf name="md5-password">
                <when condition="../enable='true' and ../type!='keychain'"/>
                <type name="pub-type:password-extend">
                  <pattern value="[^ ]+"/>
                </type>
                <mandatory value="true"/>
                <description>
                  <text>Authentication key of a RSVP TE neighbor. The two NEs that configured with neighbor authentication authenticate each other by sending and receiving RSVP packets. The packets that are sent from the local NE to its neighbor carry the authentication information that are calculated based on the authentication key by using the MD5 algorithm. The packets received from the neighbor are also authenticated based on the authentication key. The password is a string ranging from 1 to 255 characters for a plaintext password and 20 to 432 characters for a ciphertext password.</text>
                </description>
              </leaf>
              <leaf name="life-time">
                <when condition="../enable='true'"/>
                <type name="pub-type:time-tick">
                  <range value="1..86399"/>
                </type>
                <units name="s"/>
                <default value="1800"/>
                <description>
                  <text>Authentication lifetime. During the authentication lifetime, RSVP authentication lifetime will be reset after a RSVP packet is received. If no RSVP packet is received when the authentication lifetime times out, the RSVP neighbor will delete the authentication relationship to prevent persistent authentication.</text>
                </description>
              </leaf>
              <leaf name="handshake-enable">
                <when condition="../enable='true'"/>
                <type name="boolean"/>
                <default value="false"/>
                <description>
                  <text>Enable/disable the handshake authentication. When RSVP packet authentication is out of order, a handshake will be initiated and authentication will be re-negotiated.</text>
                </description>
              </leaf>
              <leaf name="window-size">
                <when condition="../enable='true' and ../handshake-enable='true'"/>
                <type name="uint32">
                  <range value="1..64"/>
                </type>
                <default value="1"/>
                <description>
                  <text>Size of an authentication window that is set to prevent information loss caused by packet disorder. If the packet sequence number exceeds the window size, the packet is considered to be out of order.</text>
                </description>
              </leaf>
            </container>
          </list>
        </container>
        <container name="distribute-instances">
          <when condition="../enable='true'"/>
          <description>
            <text>List of RSVP distributed instance.</text>
          </description>
          <list name="distribute-instance">
            <key value="instance-name"/>
            <max-elements value="16"/>
            <description>
              <text>Configure RSVP distributed instance.</text>
            </description>
            <leaf name="instance-name">
              <type name="string">
                <length value="1..31"/>
                <pattern value="[a-z,A-Z][a-z,A-Z,_,\-,0-9]*"/>
              </type>
              <description>
                <text>Name of RSVP distributed instance.</text>
              </description>
            </leaf>
            <leaf name="os-group-name">
              <type name="string">
                <length value="1..31"/>
              </type>
              <mandatory value="true"/>
              <description>
                <text>Name of RSVP distributed instance os-group.</text>
              </description>
            </leaf>
          </list>
        </container>
      </container>
      <container name="cspf">
        <when condition="../enable='true'"/>
        <description>
          <text>Configure CSPF parameters.</text>
        </description>
        <leaf name="enable">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable CSPF capability.</text>
          </description>
        </leaf>
        <leaf name="preferred-igp">
          <when condition="../enable='true'"/>
          <type name="cspf-igp-type"/>
          <default value="ospf"/>
          <description>
            <text>Preferred IGP TEDB in CSPF to calculate path.</text>
          </description>
        </leaf>
        <leaf name="multi-instances-short-path-mode">
          <when condition="../enable='true'"/>
          <type name="cspf-multiple-area-shortpath-mode"/>
          <default value="disable"/>
          <description>
            <text>Multiple IGP instances Shortest path Mode.</text>
          </description>
        </leaf>
        <leaf name="tie-breaking">
          <type name="tebase-tie-breaking"/>
          <default value="random"/>
          <description>
            <text>Rules for selecting multiple equal-cost routes. In path computation, if multiple paths meet constraints, a device uses this rule to select a path.
Random: randomly selects a path.
Least Fill: selects a link with the lowest proportion of used bandwidth to the maximum reservable bandwidth.
Most Fill: selects a link with the highest proportion of used bandwidth to the maximum reservable bandwidth.</text>
          </description>
        </leaf>
        <leaf name="path-metric-type">
          <type name="global-path-metric-type"/>
          <default value="te"/>
          <description>
            <text>Referenced metric type of one link for calculating path when creating TE tunnels. The available options IGP and TE, default is TE.</text>
          </description>
        </leaf>
        <leaf name="srlg-path-calculate-mode">
          <type name="mpls-te-cspf-srlg-path-calculation-mode"/>
          <default value="default"/>
          <description>
            <text>SRLG path calculation mode. It is configured on the ingress of a tunnel so that a TE HSB path can be calculated based on the SRLG if TE HSB is enabled.
In strict mode, the ingress keeps using the SRLG as a constraint. In preferred mode, SRLG will be considered as a constraint only when the ingress computes a backup CR-LSP at the first time. If the path computation fails, the ingress will no longer uses the SRLG as a constraint.</text>
          </description>
        </leaf>
        <leaf name="fast-notice">
          <when condition="../enable='true'"/>
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable CSPF tedb fast notice.</text>
          </description>
        </leaf>
        <leaf name="disable-optimize-mode">
          <when condition="../enable='true'"/>
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable CSPF algorithm optimization mode.</text>
          </description>
        </leaf>
        <container name="tedbs">
          <config value="false"/>
          <description>
            <text>List of TEDB records of MPLS-TE.</text>
          </description>
          <list name="tedb">
            <key value="igp-type"/>
            <description>
              <text>Statistics of TEDB record of MPLS-TE.</text>
            </description>
            <leaf name="igp-type">
              <type name="tedb-srdb-igp-type"/>
              <description>
                <text>IGP type of the node.</text>
              </description>
            </leaf>
            <leaf name="process-id">
              <type name="uint32"/>
              <description>
                <text>Process ID of the IGP protocol.</text>
              </description>
            </leaf>
            <leaf name="area-id">
              <type name="string">
                <length value="1..31"/>
              </type>
              <description>
                <text>ID of the area where the node resides.</text>
              </description>
            </leaf>
            <leaf name="router-id">
              <type name="inet:ip-address"/>
              <description>
                <text>Router ID of the node.</text>
              </description>
            </leaf>
            <leaf name="link-count">
              <type name="uint32"/>
              <description>
                <text>Number of TE interfaces of the node.</text>
              </description>
            </leaf>
          </list>
        </container>
        <container name="srdbs">
          <config value="false"/>
          <description>
            <text>List of SRDB records of MPLS-TE.</text>
          </description>
          <list name="srdb">
            <key value="process-id area-id igp-type router-id ipv4-node-sid"/>
            <description>
              <text>Statistics of SRDB record of MPLS-TE.</text>
            </description>
            <leaf name="process-id">
              <type name="uint32"/>
              <description>
                <text>Process ID of the IGP protocol.</text>
              </description>
            </leaf>
            <leaf name="area-id">
              <type name="uint32"/>
              <description>
                <text>ID of the area where the node resides.</text>
              </description>
            </leaf>
            <leaf name="igp-type">
              <type name="tedb-srdb-igp-type"/>
              <description>
                <text>IGP type of the node.</text>
              </description>
            </leaf>
            <leaf name="router-id">
              <type name="inet:ip-address"/>
              <description>
                <text>Router ID of the node.</text>
              </description>
            </leaf>
            <leaf name="ipv4-node-sid">
              <type name="uint32">
                <range value="0..65534"/>
              </type>
              <description>
                <text>SID of the node.</text>
              </description>
            </leaf>
            <leaf name="adjacent-sid-count">
              <type name="uint32"/>
              <description>
                <text>Number of adjacent SID.</text>
              </description>
            </leaf>
          </list>
        </container>
      </container>
      <container name="p2mp-leaf-lists">
        <when condition="../enable='true'"/>
        <description>
          <text>List of p2mp leaf list configuration.</text>
        </description>
        <list name="p2mp-leaf-list">
          <key value="name"/>
          <description>
            <text>Configure p2mp leaf list.</text>
          </description>
          <leaf name="name">
            <type name="string">
              <length value="1..31"/>
              <pattern value="[^ \?]*"/>
            </type>
            <description>
              <text>Name of leaf list of P2mp.</text>
            </description>
          </leaf>
          <container name="leafs">
            <description>
              <text>List of leaf configuration.</text>
            </description>
            <list name="leaf">
              <key value="destination"/>
              <max-elements value="1024"/>
              <description>
                <text>Configure leaf.</text>
              </description>
              <leaf name="destination">
                <type name="inet:ipv4-address-no-zone"/>
                <description>
                  <text>Leaf destination ip address.</text>
                </description>
              </leaf>
              <leaf name="explicit-path-name">
                <type name="leafref">
                  <path value="/te:te/te:explicit-paths/te:explicit-path/te:name"/>
                </type>
                <description>
                  <text>Explicit path name of a leaflist. The name is case-insensitive.</text>
                </description>
              </leaf>
            </list>
          </container>
        </list>
      </container>
      <container name="p2mp-templates">
        <when condition="../enable='true'"/>
        <description>
          <text>List of P2MP template configuration.</text>
        </description>
        <list name="p2mp-template">
          <must condition="((not(include-all-affinity-name) and not(include-any-affinity-name) and not(exclude-affinity-name)) or (include-any='0x0' and exclude-any='0x0'))"/>
          <must condition="((./bandwidth&gt;=./frr/bandwidth) or (./bandwidth and not(./frr/bandwidth)) or (not(./bandwidth) and not(./frr/bandwidth)))"/>
          <key value="name"/>
          <description>
            <text>Configure P2MP template.</text>
          </description>
          <leaf name="name">
            <type name="string">
              <length value="1..31"/>
              <pattern value="[^ ]*"/>
            </type>
            <description>
              <text>Name of p2mp template.</text>
            </description>
          </leaf>
          <leaf name="cspf-disable">
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable CSPF of LSP.</text>
            </description>
          </leaf>
          <leaf name="record-route">
            <type name="record-route-mode"/>
            <default value="disable"/>
            <description>
              <text>Route record mode.</text>
            </description>
          </leaf>
          <leaf name="resv-style">
            <type name="resource-reservation-style"/>
            <must condition="../resv-style='se' or ../reoptimization='false'"/>
            <default value="se"/>
            <description>
              <text>P2MP Template reservation styles.</text>
            </description>
          </leaf>
          <leaf name="setup-priority">
            <type name="uint8">
              <range value="0..7"/>
            </type>
            <must condition="../frr/setup-priority&gt;=../setup-priority and ../setup-priority&gt;=../hold-priority"/>
            <default value="7"/>
            <description>
              <text>Specifies a p2mp template setup priority. The smaller the value, the higher the setup priority. 0 is the highest priority.
Special explanation:
Constraint: The setup priority must be equal to or smaller than its holding priority.</text>
            </description>
          </leaf>
          <leaf name="hold-priority">
            <type name="uint8">
              <range value="0..7"/>
            </type>
            <must condition="../frr/hold-priority&gt;=../hold-priority and ../setup-priority&gt;=../hold-priority"/>
            <default value="7"/>
            <description>
              <text>Specifies a p2mp template holding priority. The smaller the value, the higher the holding priority. 0 is the highest priority.
Special explanation:
Constraint: The holding priority must be equal to or larger than its setup priority.</text>
            </description>
          </leaf>
          <leaf name="bandwidth">
            <type name="uint32">
              <range value="1..4000000000"/>
            </type>
            <units name="kbit/s"/>
            <description>
              <text>Specifies a tunnel bandwidth.</text>
            </description>
          </leaf>
          <leaf name="reoptimization">
            <type name="boolean"/>
            <must condition="../reoptimization='false' or ../resv-style='se'"/>
            <default value="false"/>
            <description>
              <text>Enable/disable auto reoptimization of P2MP Template.</text>
            </description>
          </leaf>
          <leaf name="reoptimization-frequency">
            <type name="uint32">
              <range value="60..604800"/>
            </type>
            <units name="s"/>
            <must condition="../reoptimization-frequency=3600 or ../reoptimization='true'"/>
            <default value="3600"/>
            <description>
              <text>Frequency of auto reoptimization.</text>
            </description>
          </leaf>
          <leaf name="lsp-tp-outbound">
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable traffic policing capability.</text>
            </description>
          </leaf>
          <leaf name="path-metric-type">
            <type name="tunnel-path-metric-type"/>
            <default value="none"/>
            <description>
              <text>Referenced metric type of one link for calculating path when creating TE tunnels. The available options are DEFAULT, IGP and TE, default is inheriting from global configuration.</text>
            </description>
          </leaf>
          <leaf name="tie-breaking">
            <type name="p2p-tunnel-tie-breaking"/>
            <default value="default"/>
            <description>
              <text>Routing rules for a p2mp template with multiple equal-cost routes. Random: Select a link randomly. least fill: Select the link with smallest bandwidth usage. most fill: Select the link with biggest bandwidth usage. By default, routing rules are inherited from the global MPLS TE routing rules. If multiple paths meet certain limits, a path will be selected based on the preceding rules.</text>
            </description>
          </leaf>
          <leaf name="hop-limit">
            <type name="uint32">
              <range value="1..32"/>
            </type>
            <default value="32"/>
            <description>
              <text>Hop limit on a TE LSP.</text>
            </description>
          </leaf>
          <leaf name="include-any">
            <type name="pub-type:hex-binary"/>
            <default value="0x0"/>
            <description>
              <text>Administrative group attribute of an LSP (includeAny).</text>
            </description>
          </leaf>
          <leaf name="exclude-any">
            <type name="pub-type:hex-binary"/>
            <default value="0x0"/>
            <description>
              <text>Administrative group attribute of an LSP (excludeAny).</text>
            </description>
          </leaf>
          <leaf name="include-all">
            <type name="pub-type:hex-binary"/>
            <config value="false"/>
            <description>
              <text>Administrative group attribute of an LSP (includeAll).</text>
            </description>
          </leaf>
          <leaf name="leaf-list-name">
            <type name="leafref">
              <path value="../../../p2mp-leaf-lists/p2mp-leaf-list/name"/>
            </type>
            <description>
              <text>Specifies the leaf-list.</text>
            </description>
          </leaf>
          <leaf-list name="include-any-affinity-name">
            <type name="leafref">
              <path value="/te:te/te:affinity-define/te:affinity-mappings/te:affinity-mapping/te:attribute-name"/>
            </type>
            <must condition="not(../include-all-affinity-name)"/>
            <max-elements value="32"/>
            <description>
              <text>Configure affinity name that can be refer to a tunnel to include any.</text>
            </description>
          </leaf-list>
          <leaf-list name="include-all-affinity-name">
            <type name="leafref">
              <path value="/te:te/te:affinity-define/te:affinity-mappings/te:affinity-mapping/te:attribute-name"/>
            </type>
            <must condition="not(../include-any-affinity-name)"/>
            <max-elements value="32"/>
            <description>
              <text>Configure affinity name that can be refer to a tunnel to include all.</text>
            </description>
          </leaf-list>
          <leaf-list name="exclude-affinity-name">
            <type name="leafref">
              <path value="/te:te/te:affinity-define/te:affinity-mappings/te:affinity-mapping/te:attribute-name"/>
            </type>
            <max-elements value="32"/>
            <description>
              <text>Configure tunnel path affinity attribute: exclude.</text>
            </description>
          </leaf-list>
          <leaf name="refer-number">
            <type name="uint32"/>
            <config value="false"/>
            <description>
              <text>Refer number for p2mp template.</text>
            </description>
          </leaf>
          <container name="frr">
            <description>
              <text>Configure fast reroute attribute of P2MP template.</text>
            </description>
            <leaf name="enable">
              <type name="boolean"/>
              <must condition="../enable='false' or (../../record-route='record-label' and ../../resv-style='se')"/>
              <default value="false"/>
              <description>
                <text>Enable/disable fast reroute capability.</text>
              </description>
            </leaf>
            <leaf name="bandwidth-protection-enable">
              <when condition="../enable='true'"/>
              <type name="boolean"/>
              <default value="false"/>
              <description>
                <text>Enable/disable the tunnel with fast reroute capability requests bandwidth protection.</text>
              </description>
            </leaf>
            <leaf name="bandwidth">
              <when condition="../bandwidth-protection-enable='true'"/>
              <type name="uint32">
                <range value="1..4000000000"/>
              </type>
              <units name="kbit/s"/>
              <description>
                <text>FRR-protection bandwidth (kbit/s) requested by an active tunnel. The value cannot exceed the bandwidth of the active tunnel.</text>
              </description>
            </leaf>
            <leaf name="setup-priority">
              <type name="uint8">
                <range value="0..7"/>
              </type>
              <must condition="../setup-priority&gt;=../../setup-priority and ../setup-priority&gt;=../hold-priority and ((../setup-priority=7) or ../bandwidth-protection-enable='true')"/>
              <default value="7"/>
              <description>
                <text>Setup priority of FRR-protection tunnels. The smaller the value, the higher the setup priority. 0 is the highest priority.
Special explanation:
Constraint: The protection tunnel setup priority cannot exceed the setup priority of the active tunnel.</text>
              </description>
            </leaf>
            <leaf name="hold-priority">
              <type name="uint8">
                <range value="0..7"/>
              </type>
              <must condition="../setup-priority&gt;=../hold-priority and ../hold-priority&gt;=../../hold-priority and ((../hold-priority=7) or ../bandwidth-protection-enable='true')"/>
              <default value="7"/>
              <description>
                <text>Holding priority of FRR protection tunnels. The smaller the value, the higher the priority. The value 0 is the highest priority.
Special explanation:
Constraint: The protection tunnel holding priority cannot exceed the active tunnel holding priority.</text>
              </description>
            </leaf>
          </container>
        </list>
      </container>
      <container name="p2p-templates">
        <when condition="../enable='true'"/>
        <description>
          <text>List of P2P template configuration.</text>
        </description>
        <list name="p2p-template">
          <must condition="auto-bandwidth-max-bandwidth&gt;=auto-bandwidth-min-bandwidth"/>
          <must condition="((./bandwidth&gt;=./frr/bandwidth) or (./bandwidth and not(./frr/bandwidth)) or (not(./bandwidth) and not(./frr/bandwidth)) or ./frr/bandwidth='0')"/>
          <key value="name"/>
          <description>
            <text>Configure P2P template.</text>
          </description>
          <leaf name="name">
            <type name="string">
              <length value="1..31"/>
              <pattern value="[^ ]*"/>
            </type>
            <description>
              <text>Name of p2p template.</text>
            </description>
          </leaf>
          <leaf name="record-route">
            <type name="record-route-mode"/>
            <default value="disable"/>
            <description>
              <text>Route record mode. The available options are not record, recording routes only, and recording routes and labels.</text>
            </description>
          </leaf>
          <leaf name="resv-style">
            <type name="resource-reservation-style"/>
            <must condition="../resv-style='se' or ../reoptimization='false'"/>
            <default value="se"/>
            <description>
              <text>P2P Template reservation styles. SE style: shared explicit style; FF: fixed filter style. The default tunnel reservation style is SE.</text>
            </description>
          </leaf>
          <leaf name="setup-priority">
            <type name="uint8">
              <range value="0..7"/>
            </type>
            <must condition="../frr/setup-priority&gt;=../setup-priority and ../setup-priority&gt;=../hold-priority"/>
            <default value="7"/>
            <description>
              <text>Specifies a p2p template setup priority. The smaller the value, the higher the setup priority. 0 is the highest priority.
Special explanation:
Constraint: The setup priority must be equal to or smaller than its holding priority.</text>
            </description>
          </leaf>
          <leaf name="hold-priority">
            <type name="uint8">
              <range value="0..7"/>
            </type>
            <must condition="../frr/hold-priority&gt;=../hold-priority and ../setup-priority&gt;=../hold-priority"/>
            <default value="7"/>
            <description>
              <text>Specifies a p2p template holding priority. The smaller the value, the higher the holding priority. 0 is the highest priority.
Special explanation:
Constraint: The holding priority must be equal to or larger than its setup priority.</text>
            </description>
          </leaf>
          <leaf name="bandwidth">
            <type name="uint32">
              <range value="1..4000000000"/>
            </type>
            <units name="kbit/s"/>
            <description>
              <text>Specifies a tunnel bandwidth.</text>
            </description>
          </leaf>
          <leaf name="reoptimization">
            <type name="boolean"/>
            <must condition="../reoptimization='false' or ../resv-style='se'"/>
            <default value="false"/>
            <description>
              <text>Enable/disable auto reoptimization enabling state of P2P Template.</text>
            </description>
          </leaf>
          <leaf name="reoptimization-frequency">
            <type name="uint32">
              <range value="60..604800"/>
            </type>
            <units name="s"/>
            <must condition="../reoptimization-frequency=3600 or ../reoptimization='true'"/>
            <default value="3600"/>
            <description>
              <text>Frequency of auto reoptimization.</text>
            </description>
          </leaf>
          <leaf name="lsp-tp-outbound">
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable traffic policing enabling state.</text>
            </description>
          </leaf>
          <leaf name="path-metric-type">
            <type name="tunnel-path-metric-type"/>
            <default value="none"/>
            <description>
              <text>Referenced metric type of one link for calculating path when creating TE tunnels. The available options are DEFAULT, IGP and TE, default is inheriting from global configuration.</text>
            </description>
          </leaf>
          <leaf name="tie-breaking">
            <type name="p2p-tunnel-tie-breaking"/>
            <default value="default"/>
            <description>
              <text>Routing rules for a p2p template with multiple equal-cost routes. Random: Select a link randomly. least fill: Select the link with smallest bandwidth usage. most fill: Select the link with biggest bandwidth usage. By default, routing rules are inherited from the global MPLS TE routing rules. If multiple paths meet certain limits, a path will be selected based on the preceding rules.</text>
            </description>
          </leaf>
          <leaf name="auto-bandwidth-mode">
            <type name="auto-bandwidth-type"/>
            <default value="none"/>
            <description>
              <text>Auto bandwidth mode.</text>
            </description>
          </leaf>
          <leaf name="auto-bandwidth-frequency">
            <when condition="../auto-bandwidth-mode!='none'"/>
            <type name="uint32">
              <range value="300..604800"/>
            </type>
            <default value="86400"/>
            <description>
              <text>Set frequency of auto bandwidth.</text>
            </description>
          </leaf>
          <leaf name="auto-bandwidth-max-bandwidth">
            <when condition="../auto-bandwidth-mode!='none'"/>
            <type name="uint32">
              <range value="0..4000000000"/>
            </type>
            <default value="4000000000"/>
            <description>
              <text>Set maximum bandwidth.</text>
            </description>
          </leaf>
          <leaf name="auto-bandwidth-min-bandwidth">
            <when condition="../auto-bandwidth-mode!='none'"/>
            <type name="uint32">
              <range value="0..4000000000"/>
            </type>
            <default value="0"/>
            <description>
              <text>Set minimum bandwidth.</text>
            </description>
          </leaf>
          <leaf name="auto-bandwidth-threshold">
            <when condition="../auto-bandwidth-mode='adjustment'"/>
            <type name="uint32">
              <range value="0..100"/>
            </type>
            <default value="0"/>
            <description>
              <text>Set the threshold of the adjustment bandwidth.</text>
            </description>
          </leaf>
          <leaf name="auto-bandwidth-threshold-absolute">
            <when condition="../auto-bandwidth-mode='adjustment'"/>
            <type name="uint64">
              <range value="0..4000000000"/>
            </type>
            <default value="0"/>
            <description>
              <text>Set the absolute minimum bandwidth of the adjustment bandwidth.</text>
            </description>
          </leaf>
          <leaf name="hotstandby-enable">
            <type name="boolean"/>
            <must condition="../hotstandby-enable='false' or (../record-route!='disable' and ../resv-style='se')"/>
            <default value="false"/>
            <description>
              <text>Enable/disable hot standby for protecting TE tunnels. When an active LSP is set up successfully, a standby LSP that meets certain limits will be set up to protect the active LSP. When the active LSP fails, the traffic on the active LSP will be switched to the standby LSP.</text>
            </description>
          </leaf>
          <leaf name="hotstandby-revertive-mode">
            <type name="tunnel-hotstandby-revert-mode"/>
            <must condition="../hotstandby-revertive-mode='revertive' or ../hotstandby-enable='true'"/>
            <default value="revertive"/>
            <description>
              <text>Hot-standby revertive.</text>
            </description>
          </leaf>
          <leaf name="hotstandby-wtr">
            <type name="uint32">
              <range value="0..2592000"/>
            </type>
            <units name="s"/>
            <must condition="../hotstandby-wtr=10 or (../hotstandby-revertive-mode='revertive' and ../hotstandby-enable='true')"/>
            <default value="10"/>
            <description>
              <text>Time of waiting recovering back to primary LSP. When hot-standby backup is in use, after primary LSP restores, the traffic will switch to primary LSP after waiting some time instead of switching to primary LSP immediately. This is to avoid frequent switching between primary LSP and backup LSP caused by network flapping.</text>
            </description>
          </leaf>
          <leaf name="hotstandby-path-overlap">
            <type name="boolean"/>
            <must condition="../hotstandby-path-overlap='false' or ../hotstandby-enable='true'"/>
            <default value="false"/>
            <description>
              <text>Enable/disable hot-standby LSP can overlap primary LSP. If there is no other path to choose, the hot-standby LSP can overlap the primary LSP.</text>
            </description>
          </leaf>
          <leaf name="hotstandby-dynamic-bandwidth">
            <type name="boolean"/>
            <must condition="../hotstandby-dynamic-bandwidth='false' or ../hotstandby-enable='true'"/>
            <default value="false"/>
            <description>
              <text>Enable/disable hot-standby dynamic-bandwidth.</text>
            </description>
          </leaf>
          <leaf name="statistic-enable">
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable Statistic.</text>
            </description>
          </leaf>
          <leaf name="entropy-label">
            <type name="entropy-label-enable"/>
            <default value="default"/>
            <description>
              <text>Entropy Label Enable.</text>
            </description>
          </leaf>
          <leaf name="refer-number">
            <type name="uint32"/>
            <config value="false"/>
            <description>
              <text>Refer number for p2p template.</text>
            </description>
          </leaf>
          <container name="frr">
            <description>
              <text>Configure fast reroute attribute of p2p template.</text>
            </description>
            <leaf name="enable">
              <type name="boolean"/>
              <must condition="../enable='false' or (../../record-route='record-label' and ../../resv-style='se')"/>
              <default value="false"/>
              <description>
                <text>Enable/disable fast reroute capability.</text>
              </description>
            </leaf>
            <leaf name="bandwidth-protection-enable">
              <when condition="../enable='true'"/>
              <type name="boolean"/>
              <default value="false"/>
              <description>
                <text>Enable/disable the tunnel with fast reroute capability requests bandwidth protection.</text>
              </description>
            </leaf>
            <leaf name="bandwidth">
              <when condition="../bandwidth-protection-enable='true'"/>
              <type name="uint32">
                <range value="0..4000000000"/>
              </type>
              <units name="kbit/s"/>
              <description>
                <text>FRR-protection bandwidth (kbits/s) requested by an active tunnel. The value cannot exceed the bandwidth of the active tunnel.</text>
              </description>
            </leaf>
            <leaf name="setup-priority">
              <type name="uint8">
                <range value="0..7"/>
              </type>
              <must condition="../setup-priority&gt;=../../setup-priority and ../setup-priority&gt;=../hold-priority and ((../setup-priority=7) or ../bandwidth-protection-enable='true')"/>
              <default value="7"/>
              <description>
                <text>Setup priority of FRR-protection tunnels. The smaller the value, the higher the setup priority. 0 is the highest priority.
Special explanation:
Constraint: The protection tunnel setup priority cannot exceed the setup priority of the active tunnel.</text>
              </description>
            </leaf>
            <leaf name="hold-priority">
              <type name="uint8">
                <range value="0..7"/>
              </type>
              <must condition="../setup-priority&gt;=../hold-priority and ../hold-priority&gt;=../../hold-priority and ((../hold-priority=7) or ../bandwidth-protection-enable='true')"/>
              <default value="7"/>
              <description>
                <text>Holding priority of FRR protection tunnels. The smaller the value, the higher the priority. The value 0 is the highest priority.
Special explanation:
Constraint: The protection tunnel holding priority cannot exceed the active tunnel holding priority.</text>
              </description>
            </leaf>
          </container>
          <container name="lsp-paths">
            <description>
              <text>List of LSP attribute parameter.</text>
            </description>
            <list name="lsp-path">
              <must condition="((not(include-all-affinity-name) and not(include-any-affinity-name) and not(exclude-affinity-name)) or (include-any='0x0' and exclude-any='0x0'))"/>
              <key value="path-type"/>
              <max-elements value="1"/>
              <description>
                <text>Configure LSP attribute parameter.</text>
              </description>
              <leaf name="path-type">
                <type name="lsp-path-type"/>
                <must condition="../path-type='primary'"/>
                <description>
                  <text>Set LSP type. Only support primary type.</text>
                </description>
              </leaf>
              <leaf name="hop-limit">
                <type name="uint32">
                  <range value="1..32"/>
                </type>
                <default value="32"/>
                <description>
                  <text>Hop limit on a TE LSP.</text>
                </description>
              </leaf>
              <leaf name="include-all">
                <type name="pub-type:hex-binary"/>
                <config value="false"/>
                <description>
                  <text>Administrative group attribute of an LSP (includeAll).</text>
                </description>
              </leaf>
              <leaf name="include-any">
                <type name="pub-type:hex-binary"/>
                <default value="0x0"/>
                <description>
                  <text>Administrative group attribute of an LSP (includeAny).</text>
                </description>
              </leaf>
              <leaf name="exclude-any">
                <type name="pub-type:hex-binary"/>
                <default value="0x0"/>
                <description>
                  <text>Administrative group attribute of an LSP (excludeAny).</text>
                </description>
              </leaf>
              <leaf-list name="include-any-affinity-name">
                <type name="leafref">
                  <path value="/te:te/te:affinity-define/te:affinity-mappings/te:affinity-mapping/te:attribute-name"/>
                </type>
                <must condition="not(../include-all-affinity-name)"/>
                <max-elements value="32"/>
                <description>
                  <text>Configure affinity name that can be refer to a tunnel to include any.</text>
                </description>
              </leaf-list>
              <leaf-list name="include-all-affinity-name">
                <type name="leafref">
                  <path value="/te:te/te:affinity-define/te:affinity-mappings/te:affinity-mapping/te:attribute-name"/>
                </type>
                <must condition="not(../include-any-affinity-name)"/>
                <max-elements value="32"/>
                <description>
                  <text>Configure affinity name that can be refer to a tunnel to include all.</text>
                </description>
              </leaf-list>
              <leaf-list name="exclude-affinity-name">
                <type name="leafref">
                  <path value="/te:te/te:affinity-define/te:affinity-mappings/te:affinity-mapping/te:attribute-name"/>
                </type>
                <max-elements value="32"/>
                <description>
                  <text>Configure tunnel path affinity attribute: exclude.</text>
                </description>
              </leaf-list>
            </list>
          </container>
        </list>
      </container>
      <container name="ip-prefixs">
        <when condition="../enable='true'"/>
        <description>
          <text>List of IP prefix auto tunnel.</text>
        </description>
        <list name="ip-prefix">
          <key value="name"/>
          <description>
            <text>Configure IP prefix auto tunnel.</text>
          </description>
          <leaf name="name">
            <type name="leafref">
              <path value="/rtp:routing-policy/rtp:ipv4-prefix-filters/rtp:ipv4-prefix-filter/rtp:name"/>
            </type>
            <description>
              <text>Ip prefix name of ip prefix auto tunnel.</text>
            </description>
          </leaf>
          <leaf name="template-name">
            <type name="leafref">
              <path value="../../../p2p-templates/p2p-template/name"/>
            </type>
            <description>
              <text>Template name of ip prefix auto tunnel.</text>
            </description>
          </leaf>
        </list>
      </container>
      <container name="te-class-mapping">
        <when condition="../enable='true'"/>
        <presence value="Enter te-class-mapping view."/>
        <description>
          <text>Enable/disable TE class mapping.</text>
        </description>
        <container name="te-classes">
          <description>
            <text>List of TE class configuration.</text>
          </description>
          <list name="te-class">
            <key value="te-class-mode"/>
            <unique tag="te-class-type priority"/>
            <max-elements value="8"/>
            <description>
              <text>Configure TE class.</text>
            </description>
            <leaf name="te-class-mode">
              <type name="te-class-mode-type"/>
              <description>
                <text>TE-Class only configured from te-class0 to te-class7.</text>
              </description>
            </leaf>
            <leaf name="te-class-type">
              <type name="te-class-type"/>
              <mandatory value="true"/>
              <description>
                <text>Class-Type 0 to 7.</text>
              </description>
            </leaf>
            <leaf name="priority">
              <type name="uint8">
                <range value="0..7"/>
              </type>
              <mandatory value="true"/>
              <description>
                <text>Class type priority.</text>
              </description>
            </leaf>
            <leaf name="description">
              <type name="string">
                <length value="1..31"/>
              </type>
              <description>
                <text>Set TE-Class description.</text>
              </description>
            </leaf>
          </list>
        </container>
      </container>
      <container name="associate-groups">
        <when condition="../enable='true'"/>
        <description>
          <text>List of associated tunnel groups.</text>
        </description>
        <list name="associate-group">
          <key value="group-id"/>
          <max-elements value="1024"/>
          <description>
            <text>Configure associated tunnel group.</text>
          </description>
          <leaf name="group-id">
            <type name="uint32">
              <range value="1..4294967295"/>
            </type>
            <description>
              <text>Associated tunnel group id.</text>
            </description>
          </leaf>
          <leaf name="backup-type">
            <type name="backup-type"/>
            <default value="load-balance"/>
            <description>
              <text>Backup type of a tunnel association group.</text>
            </description>
          </leaf>
          <container name="original-tunnels">
            <description>
              <text>List of original tunnels.</text>
            </description>
            <list name="original-tunnel">
              <must condition="(/ifm:ifm/ifm:interfaces/ifm:interface[ifm:name=current()/name]/tnlm:tunnel-protocol/mpls-te:te-tunnel/mpls-te:rsvp-te/mpls-te:split-tunnel='false' and /ifm:ifm/ifm:interfaces/ifm:interface[ifm:name=current()/name]/tnlm:tunnel-protocol/mpls-te:te-tunnel/mpls-te:rsvp-te/mpls-te:ordinary-enable='false' and /ifm:ifm/ifm:interfaces/ifm:interface[ifm:name=current()/name]/tnlm:tunnel-protocol/mpls-te:te-tunnel/mpls-te:rsvp-te/mpls-te:best-effort-enable='false' and /ifm:ifm/ifm:interfaces/ifm:interface[ifm:name=current()/name]/tnlm:tunnel-protocol/mpls-te:te-tunnel/mpls-te:rsvp-te/mpls-te:bypass/mpls-te:enable='false' and /ifm:ifm/ifm:interfaces/ifm:interface[ifm:name=current()/name]/tnlm:tunnel-protocol/mpls-te:te-tunnel/mpls-te:rsvp-te/mpls-te:fast-reroute/mpls-te:mode!='detour-backup' and /ifm:ifm/ifm:interfaces/ifm:interface[ifm:name=current()/name]/tnlm:tunnel-protocol/mpls-te:te-tunnel/mpls-te:rsvp-te/mpls-te:fast-reroute/mpls-te:enable='false') or (/ifm:ifm/ifm:interfaces/ifm:interface[ifm:name=current()/name]/tnlm:tunnel-protocol/mpls-te:te-tunnel/mpls-te:sr-te/mpls-te:split-tunnel='false')"/>
              <key value="name"/>
              <max-elements value="1"/>
              <description>
                <text>Configure original tunnel.</text>
              </description>
              <leaf name="name">
                <type name="leafref">
                  <path value="/ifm:ifm/ifm:interfaces/ifm:interface/ifm:name"/>
                </type>
                <must condition="/ifm:ifm/ifm:interfaces/ifm:interface[ifm:name=current()]/ifm:type='Tunnel' and /ifm:ifm/ifm:interfaces/ifm:interface[ifm:name=current()]/tnlm:tunnel-protocol/mpls-te:te-tunnel/mpls-te:common-attributes/mpls-te:egress-lsr-id and /ifm:ifm/ifm:interfaces/ifm:interface[ifm:name=current()]/tnlm:tunnel-protocol/mpls-te:te-tunnel/mpls-te:common-attributes/mpls-te:tunnel-id"/>
                <must condition="../../../backup-type='load-balance' or (/ifm:ifm/ifm:interfaces/ifm:interface[ifm:name=current()]/tnlm:tunnel-protocol/mpls-te:te-tunnel/mpls-te:common-attributes/mpls-te:signal-protocol='rsvp-te')"/>
                <description>
                  <text>Original tunnel name.</text>
                </description>
              </leaf>
              <leaf name="state">
                <type name="associate-tunnel-group-state"/>
                <config value="false"/>
                <description>
                  <text>Original tunnel status.</text>
                </description>
              </leaf>
              <container name="split-tunnels">
                <description>
                  <text>List of split tunnels.</text>
                </description>
                <list name="split-tunnel">
                  <must condition="(/ifm:ifm/ifm:interfaces/ifm:interface[ifm:name=current()/name]/tnlm:tunnel-protocol/mpls-te:te-tunnel/mpls-te:common-attributes/mpls-te:signal-protocol='rsvp-te' and /ifm:ifm/ifm:interfaces/ifm:interface[ifm:name=current()/name]/tnlm:tunnel-protocol/mpls-te:te-tunnel/mpls-te:rsvp-te/mpls-te:split-tunnel='true') or (/ifm:ifm/ifm:interfaces/ifm:interface[ifm:name=current()/name]/tnlm:tunnel-protocol/mpls-te:te-tunnel/mpls-te:common-attributes/mpls-te:signal-protocol='segment-routing' and /ifm:ifm/ifm:interfaces/ifm:interface[ifm:name=current()/name]/tnlm:tunnel-protocol/mpls-te:te-tunnel/mpls-te:sr-te/mpls-te:split-tunnel='true')"/>
                  <must condition="../../name!=name"/>
                  <must condition="/ifm:ifm/ifm:interfaces/ifm:interface[ifm:name=current()/../../name]/tnlm:tunnel-protocol/mpls-te:te-tunnel/mpls-te:common-attributes/mpls-te:egress-lsr-id=/ifm:ifm/ifm:interfaces/ifm:interface[ifm:name=current()/name]/tnlm:tunnel-protocol/mpls-te:te-tunnel/mpls-te:common-attributes/mpls-te:egress-lsr-id"/>
                  <key value="name"/>
                  <max-elements value="63"/>
                  <description>
                    <text>Configure split tunnel.</text>
                  </description>
                  <leaf name="name">
                    <type name="leafref">
                      <path value="/ifm:ifm/ifm:interfaces/ifm:interface/ifm:name"/>
                    </type>
                    <must condition="/ifm:ifm/ifm:interfaces/ifm:interface[ifm:name=current()]/ifm:type='Tunnel'"/>
                    <must condition="../../../../../backup-type='load-balance' or (/ifm:ifm/ifm:interfaces/ifm:interface[ifm:name=current()]/tnlm:tunnel-protocol/mpls-te:te-tunnel/mpls-te:common-attributes/mpls-te:signal-protocol='rsvp-te')"/>
                    <description>
                      <text>Split tunnel name.</text>
                    </description>
                  </leaf>
                  <leaf name="state">
                    <type name="associate-tunnel-group-state"/>
                    <config value="false"/>
                    <description>
                      <text>Split tunnel status.</text>
                    </description>
                  </leaf>
                </list>
              </container>
            </list>
          </container>
        </list>
      </container>
      <container name="tunnel-statistic">
        <config value="false"/>
        <description>
          <text>Statistics of tunnel about quantities and status.</text>
        </description>
        <leaf name="ingress-tunnel">
          <type name="uint32"/>
          <description>
            <text>Number of tunnels of the ingress.</text>
          </description>
        </leaf>
        <leaf name="ingress-tunnel-up">
          <type name="uint32"/>
          <description>
            <text>Number of up tunnels of the ingress.</text>
          </description>
        </leaf>
        <leaf name="ingress-cr-lsps-up">
          <type name="uint32"/>
          <description>
            <text>Number of up CR-LSPs of the ingress.</text>
          </description>
        </leaf>
        <leaf name="ingress-tunnel-modified">
          <type name="uint32"/>
          <description>
            <text>Number of modified tunnels of the ingress.</text>
          </description>
        </leaf>
        <leaf name="ingress-tunnel-in-progress">
          <type name="uint32"/>
          <description>
            <text>Number of being-created tunnels of the ingress.</text>
          </description>
        </leaf>
        <leaf name="ingress-tunnel-failed">
          <type name="uint32"/>
          <description>
            <text>Number of down tunnels of the ingress.</text>
          </description>
        </leaf>
        <leaf name="transit-tunnel-up">
          <type name="uint32"/>
          <description>
            <text>Number of up tunnels of the transit node.</text>
          </description>
        </leaf>
        <leaf name="egress-tunnel-up">
          <type name="uint32"/>
          <description>
            <text>Number of up tunnels of the egress.</text>
          </description>
        </leaf>
        <leaf name="tunnel-group-count">
          <type name="uint32"/>
          <description>
            <text>Tunnel group statistic.</text>
          </description>
        </leaf>
        <leaf name="p2mp-tunnel-count">
          <type name="uint32"/>
          <description>
            <text>Total number of p2mp tunnel.</text>
          </description>
        </leaf>
      </container>
    </container>
  </augment>
  <augment target-node="/ni:network-instance/ni:instances/ni:instance/mpls:mpls">
    <description>
      <text>MPLS TE Configuration information augment to network instance.</text>
    </description>
    <container name="te">
      <when condition="/mpls:mpls/mpls-te:te/mpls-te:enable='true'"/>
      <description>
        <text>Configure MPLS TE parameters.</text>
      </description>
    </container>
  </augment>
  <augment target-node="/ifm:ifm/ifm:interfaces/ifm:interface/tnlm:tunnel-protocol/tnlm:type">
    <description>
      <text>MPLS TE Tunnel Configuration information augment to tnlm.</text>
    </description>
    <case name="mpls-te">
      <when condition="/mpls:mpls/mpls-te:te/mpls-te:enable='true'"/>
      <description>
        <text>Tunnel protocol MPLS te.</text>
      </description>
      <container name="te-tunnel">
        <presence value="Specifies tunnel-protocol MPLS TE."/>
        <description>
          <text>Enable/disable MPLS TE tunnel.</text>
        </description>
        <container name="common-attributes">
          <description>
            <text>Configure Te tunnel common attributes.</text>
          </description>
          <leaf name="signal-protocol">
            <type name="te-tunnel-signal-protocol"/>
            <default value="rsvp-te"/>
            <description>
              <text>MPLS-TE tunnel signal-protocol.</text>
            </description>
          </leaf>
          <leaf name="tunnel-id">
            <type name="uint16">
              <range value="1..32768"/>
            </type>
            <description>
              <text>Session ID of a tunnel.
Special explanation:
Range: The min value of MPLSTunnelIndex leaf is 1, and max value is dynamic.</text>
            </description>
          </leaf>
          <leaf name="egress-lsr-id">
            <type name="inet:ipv4-address-no-zone"/>
            <description>
              <text>Specifies egress LSR ID of the tunnel.</text>
            </description>
          </leaf>
          <leaf name="reserved-for-binding">
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable the tunnel can transmit only the service of the specified VPN.</text>
            </description>
          </leaf>
          <leaf name="lsp-tp-outbound">
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable traffic policing enabling state.</text>
            </description>
          </leaf>
          <leaf name="statistic-enable">
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable TE tunnel interface statistic enabling state.</text>
            </description>
          </leaf>
        </container>
        <container name="rsvp-te">
          <when condition="../common-attributes/signal-protocol='rsvp-te'"/>
          <description>
            <text>Configure TE E2E Dynamic unicast tunnel.</text>
          </description>
          <leaf name="signalled-tunnel-name">
            <type name="string">
              <length value="1..63"/>
              <pattern value="[a-z,A-Z,_][^ /]*"/>
            </type>
            <description>
              <text>User Defined el Name, use tunnel interface name as default. The first character cannot be numbers. And cannot use physical interface name.</text>
            </description>
          </leaf>
          <leaf name="traffic-enable">
            <type name="boolean"/>
            <default value="true"/>
            <description>
              <text>Enable/disable traffic for a tunnel.</text>
            </description>
          </leaf>
          <leaf name="bandwidth">
            <type name="uint32">
              <range value="0..4000000000"/>
            </type>
            <units name="kbit/s"/>
            <must condition="(../fast-reroute/bypass-attributes/frr-bandwidth and ../bandwidth&gt;=../fast-reroute/bypass-attributes/frr-bandwidth and ../class-type='ct0') or not(../fast-reroute/bypass-attributes/frr-bandwidth)"/>
            <default value="0"/>
            <description>
              <text>Specifies a tunnel bandwidth.</text>
            </description>
          </leaf>
          <leaf name="class-type">
            <when condition="/mpls:mpls/mpls-te:te/mpls-te:global/mpls-te:ds-te-mode='ietf'"/>
            <type name="te-class-type"/>
            <must condition="../class-type='ct0' or ../bandwidth!=0"/>
            <default value="ct0"/>
            <description>
              <text>The class type for IETF mode.</text>
            </description>
          </leaf>
          <leaf name="setup-priority">
            <type name="uint8">
              <range value="0..7"/>
            </type>
            <must condition="((../fast-reroute/bypass-attributes/setup-priority&gt;=../setup-priority) or not(../fast-reroute/bypass-attributes/setup-priority)) and ((../setup-priority&gt;=../hold-priority) or (../setup-priority='7' and not(../hold-priority)))"/>
            <default value="7"/>
            <description>
              <text>Specifies a tunnel setup priority. The smaller the value, the higher the setup priority. 0 is the highest priority.</text>
            </description>
          </leaf>
          <leaf name="hold-priority">
            <type name="uint8">
              <range value="0..7"/>
            </type>
            <must condition="((../fast-reroute/bypass-attributes/hold-priority&gt;=../hold-priority) or not(../fast-reroute/bypass-attributes/hold-priority)) and ((../setup-priority&gt;=../hold-priority) or not(../setup-priority))"/>
            <default value="7"/>
            <description>
              <text>Specifies a tunnel holding priority. The smaller the value, the higher the holding priority. 0 is the highest priority.</text>
            </description>
          </leaf>
          <leaf name="ordinary-enable">
            <type name="boolean"/>
            <must condition="(../ordinary-enable='false') or (../ordinary-enable='true' and ../record-route!='disable')"/>
            <default value="false"/>
            <description>
              <text>Enable/disable a tunnel ordinary backup protection capability. When it is enabled, and the primary LSP fails, a backup LSP that meets certain limits will be set up. Then, the traffic on the primary LSP will be switched to the backup LSP.</text>
            </description>
          </leaf>
          <leaf name="best-effort-enable">
            <type name="boolean"/>
            <must condition="(../best-effort-enable='false') or (../best-effort-enable='true' and ../record-route!='disable')"/>
            <default value="false"/>
            <description>
              <text>Enable/disable best-effort path protection of tunnels. When best-effort path is enabled for a TE tunnel, and both active and standby LSP fail, an LSP will be set up in the best effort method.</text>
            </description>
          </leaf>
          <leaf name="backup-frr-inuse">
            <when condition="../ordinary-enable='true' or ../hotstandby"/>
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable the capability of creating backup lsp during frr in use. When configured fast-reroute and backup protection, and the tunnel is down, it will switch to frr-in-use, and it will try to restore the primary LSP, if you configure this capability, the same time, it will try to create backup LSP. This capability depends on configuration of enabling hot-standby or ordinary.</text>
            </description>
          </leaf>
          <leaf name="path-disjoint-calculate">
            <type name="boolean"/>
            <must condition="(../path-disjoint-calculate='false') or (../path-disjoint-calculate='true' and ../hotstandby)"/>
            <default value="false"/>
            <description>
              <text>Enable/disable the capability of calculate fully disjoint path for main and hot-standby.</text>
            </description>
          </leaf>
          <leaf name="soft-preempt-enable">
            <type name="boolean"/>
            <must condition="(../reserve-style='se' and ../soft-preempt-enable='true') or (../soft-preempt-enable='false')"/>
            <default value="false"/>
            <description>
              <text>Enable/disable soft preempt.</text>
            </description>
          </leaf>
          <leaf name="disable-cspf">
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable Cspf of a Tunnel.</text>
            </description>
          </leaf>
          <leaf name="reserve-style">
            <type name="resource-reservation-style"/>
            <must condition="(../reserve-style='ff' and ../best-effort-enable='false' and ../ordinary-enable='false' and ../fast-reroute/enable='false' and ../reoptimization='false' and ../soft-preempt-enable='false') or (../reserve-style='se')"/>
            <default value="se"/>
            <description>
              <text>Tunnel reservation styles.</text>
            </description>
          </leaf>
          <leaf name="record-route">
            <type name="record-route-mode"/>
            <default value="disable"/>
            <description>
              <text>Route record mode of a tunnel.</text>
            </description>
          </leaf>
          <leaf name="reoptimization">
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable auto reoptimization enabling state of TE Tunnel.</text>
            </description>
          </leaf>
          <leaf name="reoptimization-frequency">
            <type name="uint32">
              <range value="60..604800"/>
            </type>
            <units name="s"/>
            <must condition="../reoptimization-frequency=3600 or ../reoptimization='true'"/>
            <default value="3600"/>
            <description>
              <text>Frequency of auto reoptimization.</text>
            </description>
          </leaf>
          <leaf name="inter-area-reoptimization">
            <type name="boolean"/>
            <must condition="../inter-area-reoptimization='false' or ../reoptimization='true'"/>
            <default value="false"/>
            <description>
              <text>Enable/disable inter-area reoptimization enabling state of TE Tunnel.</text>
            </description>
          </leaf>
          <leaf name="tie-breaking">
            <type name="p2p-tunnel-tie-breaking"/>
            <default value="default"/>
            <description>
              <text>Routing rules for a tunnel with multiple equal-cost routes. Random: Select a link randomly. least fill: Select the link with smallest bandwidth usage. most fill: Select the link with biggest bandwidth usage. By default, routing rules are inherited from the global MPLS TE routing rules. If multiple paths meet certain limits, a path will be selected based on the preceding rules.</text>
            </description>
          </leaf>
          <leaf name="path-metric-type">
            <type name="tunnel-path-metric-type"/>
            <default value="none"/>
            <description>
              <text>Referenced metric type of one link for calculating path when creating TE tunnels. The available options are DEFAULT, IGP and TE, default is inheriting from global configuration.</text>
            </description>
          </leaf>
          <leaf name="metric-inherit-igp">
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable the TE tunnel metric inherits the IGP metric.</text>
            </description>
          </leaf>
          <leaf name="pce-delegate">
            <type name="delegate-type"/>
            <default value="no"/>
            <description>
              <text>Specifies the enabling state of PCE delegation capability.</text>
            </description>
          </leaf>
          <leaf name="entropy-label">
            <type name="entropy-label-enable"/>
            <default value="default"/>
            <description>
              <text>Entropy Label Enable.</text>
            </description>
          </leaf>
          <leaf name="split-tunnel">
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable the RSVP-TE tunnel as a split tunnel.</text>
            </description>
          </leaf>
          <leaf name="soft-preempt-block">
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable block soft preempt.</text>
            </description>
          </leaf>
          <leaf name="disable-reroute">
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable MPLS te rerouting function.</text>
            </description>
          </leaf>
          <leaf name="self-ping">
            <type name="tunnel-self-ping-type"/>
            <default value="disable"/>
            <description>
              <text>Self-ping enabling state of an RSVP-TE tunnel.</text>
            </description>
          </leaf>
          <leaf name="self-ping-duration">
            <type name="uint32">
              <range value="60..65535"/>
            </type>
            <units name="s"/>
            <default value="1800"/>
            <description>
              <text>Self-ping timeout period of an RSVP-TE tunnel. Value 65535 indicates no limit on the detection time.</text>
            </description>
          </leaf>
          <container name="fast-reroute">
            <must condition="(enable='true' and mode and mode='facility-backup') or (enable='false' and not(mode='facility-backup'))"/>
            <must condition="not(../disable-reroute='true' and enable='true')"/>
            <description>
              <text>Configure fast reroute attribute of a Tunnel.</text>
            </description>
            <leaf name="enable">
              <type name="boolean"/>
              <must condition="(../../record-route='record-label' and ../enable='true') or (../enable='false')"/>
              <default value="false"/>
              <description>
                <text>Enable/disable fast reroute capability.</text>
              </description>
            </leaf>
            <leaf name="mode">
              <type name="tunnel-fast-reroute-type"/>
              <description>
                <text>The tunnel with fast reroute mode.</text>
              </description>
            </leaf>
            <leaf name="bandwidth-protection-enable">
              <when condition="../enable='true'"/>
              <type name="boolean"/>
              <default value="false"/>
              <description>
                <text>Enable/disable the tunnel with fast reroute capability requests bandwidth protection.</text>
              </description>
            </leaf>
            <leaf name="frr-switch-degrade">
              <when condition="../enable='true'"/>
              <type name="boolean"/>
              <default value="false"/>
              <description>
                <text>Enable/disable fast reroute tunnel degrade capability.</text>
              </description>
            </leaf>
            <container name="bypass-attributes">
              <when condition="../enable='true'"/>
              <must condition="((not(include-all-affinity-name) and not(include-any-affinity-name) and not(exclude-affinity-name)) or (include-any='0x0' and exclude-any='0x0'))"/>
              <description>
                <text>Configure fast reroute attributes.</text>
              </description>
              <leaf name="frr-bandwidth">
                <when condition="../../bandwidth-protection-enable='true'"/>
                <type name="uint32">
                  <range value="0..4000000000"/>
                </type>
                <units name="kbit/s"/>
                <must condition="../../../bandwidth&gt;=../frr-bandwidth"/>
                <description>
                  <text>FRR-protection bandwidth (kbits/s) requested by an active tunnel. The value cannot exceed the bandwidth of the active tunnel.</text>
                </description>
              </leaf>
              <leaf name="setup-priority">
                <type name="uint32">
                  <range value="0..7"/>
                </type>
                <must condition="((../setup-priority&gt;=../../../setup-priority) or (../setup-priority='7' and not(../../../setup-priority))) and ((../setup-priority&gt;=../hold-priority) or (../setup-priority='7' and not(../hold-priority)))"/>
                <default value="7"/>
                <description>
                  <text>Setup priority of FRR-protection tunnels. The smaller the value, the higher the setup priority. 0 is the highest priority.
Special explanation:
Constraint: The protection tunnel setup priority cannot exceed the setup priority of the active tunnel.</text>
                </description>
              </leaf>
              <leaf name="hold-priority">
                <type name="uint32">
                  <range value="0..7"/>
                </type>
                <must condition="((../hold-priority&gt;=../../../hold-priority) or (../hold-priority='7' and not(../../../hold-priority))) and ((../setup-priority&gt;=../hold-priority) or (not(../setup-priority)))"/>
                <default value="7"/>
                <description>
                  <text>Holding priority of FRR protection tunnels. The value 0 is the highest priority.
Special explanation:
Constraint: The protection tunnel holding priority cannot exceed the active tunnel holding priority.</text>
                </description>
              </leaf>
              <leaf name="include-any">
                <type name="pub-type:hex-binary"/>
                <default value="0x0"/>
                <description>
                  <text>Tunnel path management attribute of FRR protection tunnel: Include-any. The attribute is a 32-bit vector. If the management attribute of a link contain any bit in the Include-any attribute field of an LSP, the link can be a candidate LSP link.</text>
                </description>
              </leaf>
              <leaf name="exclude-any">
                <type name="pub-type:hex-binary"/>
                <default value="0x0"/>
                <description>
                  <text>Tunnel path management attribute of FRR protection tunnel: Exclude-any. The management attribute is a 32-bit vector. If the management attribute of a link contains any bit in the Exclude-any attribute field of an LSP, the link cannot be a candidate LSP link.</text>
                </description>
              </leaf>
              <leaf name="include-all">
                <type name="pub-type:hex-binary"/>
                <config value="false"/>
                <description>
                  <text>Tunnel path management attribute of FRR protection tunnel: Include-all. The management attribute is a 32-bit vector. The management attribute of a selected link must contain the entire Include-all attribute field of the tunnel.</text>
                </description>
              </leaf>
              <leaf-list name="include-any-affinity-name">
                <type name="leafref">
                  <path value="/te:te/te:affinity-define/te:affinity-mappings/te:affinity-mapping/te:attribute-name"/>
                </type>
                <max-elements value="32"/>
                <description>
                  <text>Configure affinity name that can be refer to a tunnel to include any.</text>
                </description>
              </leaf-list>
              <leaf-list name="include-all-affinity-name">
                <type name="leafref">
                  <path value="/te:te/te:affinity-define/te:affinity-mappings/te:affinity-mapping/te:attribute-name"/>
                </type>
                <max-elements value="32"/>
                <description>
                  <text>Configure affinity name that can be refer to a tunnel to include all.</text>
                </description>
              </leaf-list>
              <leaf-list name="exclude-affinity-name">
                <type name="leafref">
                  <path value="/te:te/te:affinity-define/te:affinity-mappings/te:affinity-mapping/te:attribute-name"/>
                </type>
                <max-elements value="32"/>
                <description>
                  <text>Configure tunnel path affinity attribute: exclude.</text>
                </description>
              </leaf-list>
            </container>
          </container>
          <container name="hotstandby">
            <must condition="../record-route!='disable' and ../ordinary-enable='false'"/>
            <presence value="Indicates hotstandby-enable a node is configured."/>
            <description>
              <text>Enable/disable hot standby for protecting TE tunnels. When an active LSP is set up successfully, a standby LSP that meets certain limits will be set up to protect the active LSP. When the active LSP fails, the traffic on the active LSP will be switched to the standby LSP.</text>
            </description>
            <leaf name="wtr">
              <when condition="../revertive-mode='revertive'"/>
              <type name="uint32">
                <range value="0..2592000"/>
              </type>
              <units name="s"/>
              <default value="10"/>
              <description>
                <text>Time of waiting recovering back to primary LSP. When hot-standby backup is in use, after primary LSP restores, the traffic will switch to primary LSP after waiting some time instead of switching to primary LSP immediately. This is to avoid frequent switching between primary LSP and backup LSP caused by network flapping.</text>
              </description>
            </leaf>
            <leaf name="revertive-mode">
              <type name="tunnel-hotstandby-revert-mode"/>
              <default value="revertive"/>
              <description>
                <text>Hot-standby revertive. There are two revert modes, e.g. revertive and non-revertive. The default mode is revertive.</text>
              </description>
            </leaf>
            <leaf name="path-overlap">
              <type name="boolean"/>
              <default value="false"/>
              <description>
                <text>Enable/disable hot-standby LSP can overlap primary LSP. If there is no other path to choose, the hot-standby LSP can overlap the primary LSP.</text>
              </description>
            </leaf>
            <leaf name="dynamic-bandwidth">
              <type name="boolean"/>
              <default value="false"/>
              <description>
                <text>Enable/disable hot-standby dynamic bandwidth.</text>
              </description>
            </leaf>
          </container>
          <container name="lsp-paths">
            <description>
              <text>List of LSP path of a tunnel.</text>
            </description>
            <list name="lsp-path">
              <must condition="((not(include-all-affinity-name) and not(include-any-affinity-name) and not(exclude-affinity-name)) or (include-any='0x0' and exclude-any='0x0'))"/>
              <key value="path-type"/>
              <max-elements value="4"/>
              <description>
                <text>Configure LSP path of a tunnel. Four roles are available.</text>
              </description>
              <leaf name="path-type">
                <type name="te-tunnel-path-type"/>
                <description>
                  <text>Path role of a tunnel. The available options are primary (used by primary LSP), hot-standby (used by hot-standby backup LSP), ordinary (used by ordinary backup LSP), and best-effort (used by best-effort LSP).</text>
                </description>
              </leaf>
              <leaf name="explicit-path-name">
                <when condition="not(../path-type='best-effort')"/>
                <type name="leafref">
                  <path value="/te:te/te:explicit-paths/te:explicit-path/te:name"/>
                </type>
                <description>
                  <text>Referenced explicit path by the tunnel. The name is case-insensitive.</text>
                </description>
              </leaf>
              <leaf name="hop-limit">
                <type name="uint32">
                  <range value="1..32"/>
                </type>
                <default value="32"/>
                <description>
                  <text>Hop limit on a TE LSP.</text>
                </description>
              </leaf>
              <leaf name="lsp-id">
                <type name="uint32">
                  <range value="0..65535"/>
                </type>
                <config value="false"/>
                <description>
                  <text>LSP ID of a tunnel path.</text>
                </description>
              </leaf>
              <leaf name="lsp-state">
                <type name="mpls-te-lsp-status-type"/>
                <config value="false"/>
                <description>
                  <text>The state of LSP.</text>
                </description>
              </leaf>
              <leaf name="modify-lsp-id">
                <type name="uint32">
                  <range value="0..65535"/>
                </type>
                <config value="false"/>
                <description>
                  <text>Modified LSP ID of a tunnel path.</text>
                </description>
              </leaf>
              <leaf name="include-all">
                <type name="pub-type:hex-binary"/>
                <config value="false"/>
                <description>
                  <text>Tunnel path management attribute: Include-all. The management attribute is a 32-bit vector. The management attribute of a selected link must contain the entire Include-all attribute field of the tunnel.</text>
                </description>
              </leaf>
              <leaf name="include-any">
                <type name="pub-type:hex-binary"/>
                <default value="0x0"/>
                <description>
                  <text>Tunnel path management attribute: Include-any. The attribute is a 32-bit vector. If the management attribute of a link contain any bit in the Include-any attribute field of an LSP, the link can be a candidate LSP link.</text>
                </description>
              </leaf>
              <leaf name="exclude-any">
                <type name="pub-type:hex-binary"/>
                <default value="0x0"/>
                <description>
                  <text>Tunnel path management attribute: Exclude-any. The management attribute is a 32-bit vector. If the management attribute of a link contains any bit in the Exclude-any attribute field of an LSP, the link cannot be a candidate LSP link.</text>
                </description>
              </leaf>
              <leaf-list name="include-any-affinity-name">
                <type name="leafref">
                  <path value="/te:te/te:affinity-define/te:affinity-mappings/te:affinity-mapping/te:attribute-name"/>
                </type>
                <max-elements value="32"/>
                <description>
                  <text>Configure affinity name that can be refer to a tunnel to include any.</text>
                </description>
              </leaf-list>
              <leaf-list name="include-all-affinity-name">
                <type name="leafref">
                  <path value="/te:te/te:affinity-define/te:affinity-mappings/te:affinity-mapping/te:attribute-name"/>
                </type>
                <max-elements value="32"/>
                <description>
                  <text>Configure affinity name that can be refer to a tunnel to include all.</text>
                </description>
              </leaf-list>
              <leaf-list name="exclude-affinity-name">
                <type name="leafref">
                  <path value="/te:te/te:affinity-define/te:affinity-mappings/te:affinity-mapping/te:attribute-name"/>
                </type>
                <max-elements value="32"/>
                <description>
                  <text>Configure tunnel path affinity attribute: exclude.</text>
                </description>
              </leaf-list>
            </list>
          </container>
