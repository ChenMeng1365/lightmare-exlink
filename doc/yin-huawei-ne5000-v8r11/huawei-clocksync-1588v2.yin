          <leaf name="port-discard-resp">
            <type name="uint32"/>
            <units name="packet"/>
            <default value="0"/>
            <description>
              <text>Number of Response packets discarded on a PTP port.</text>
            </description>
          </leaf>
          <leaf name="port-discard-pdelayresp">
            <type name="uint32"/>
            <units name="packet"/>
            <default value="0"/>
            <description>
