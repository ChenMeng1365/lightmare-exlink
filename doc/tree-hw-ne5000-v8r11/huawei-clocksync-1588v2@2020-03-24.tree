     |        |  +--rw accuracy?        uint32
     |        |  +--rw clk-class?       uint32
     |        |  +--rw priority1?       uint32
     |        |  +--rw priority2?       uint32
     |        |  +--rw gm-clockid?      string
     |        |  +--rw offset-scaled?   uint32
     |        +--ro ptp-bits-bmc-learn
     |           +--ro time-source?     ptp-time-source
