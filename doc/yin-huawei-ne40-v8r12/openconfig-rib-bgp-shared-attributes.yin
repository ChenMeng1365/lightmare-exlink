<?xml version="1.0" encoding="UTF-8"?>
<submodule name="openconfig-rib-bgp-shared-attributes"
           xmlns="urn:ietf:params:xml:ns:yang:yin:1"
           xmlns:oc-rib-bgp="http://openconfig.net/yang/rib/bgp"
           xmlns:oc-ext="http://openconfig.net/yang/openconfig-ext">
  <belongs-to module="openconfig-rib-bgp">
    <prefix value="oc-rib-bgp"/>
  </belongs-to>
  <import module="openconfig-extensions">
    <prefix value="oc-ext"/>
  </import>
  <include module="openconfig-rib-bgp-attributes"/>
  <organization>
    <text>OpenConfig working group</text>
  </organization>
  <contact>
    <text>OpenConfig working group
www.openconfig.net</text>
  </contact>
  <description>
    <text>This submodule contains structural data definitions for
attribute sets shared across routes.</text>
  </description>
  <oc-ext:openconfig-version semver="0.6.0"/>
  <revision date="2019-04-25">
    <description>
      <text>Update last-modified timestamp to be expressed as nanoseconds
since the Unix epoch.</text>
    </description>
    <reference>
      <text>0.6.0</text>
    </reference>
  </revision>
  <revision date="2019-04-16">
    <description>
      <text>Rename the top-level BGP RIB container's name
to RIB.</text>
    </description>
    <reference>
      <text>0.5.0</text>
    </reference>
  </revision>
  <revision date="2019-02-27">
    <description>
      <text>Remove top-level BGP RIB container, and update list
names to be compatible with path compression.</text>
    </description>
    <reference>
      <text>0.4.0</text>
    </reference>
  </revision>
  <revision date="2018-11-21">
    <description>
      <text>Add OpenConfig module metadata extensions.</text>
    </description>
    <reference>
      <text>0.3.1</text>
    </reference>
  </revision>
  <revision date="2016-10-17">
    <description>
      <text>OpenConfig BGP RIB refactor</text>
    </description>
    <reference>
      <text>0.3.0</text>
    </reference>
  </revision>
  <grouping name="attribute-sets-top">
    <description>
      <text>Top level grouping for list of common attribute sets</text>
    </description>
    <container name="attr-sets">
      <description>
        <text>Enclosing container for the list of path attribute sets</text>
      </description>
      <list name="attr-set">
        <key value="index"/>
        <description>
          <text>List of path attributes that may be in use by multiple
routes in the table</text>
        </description>
        <leaf name="index">
          <type name="leafref">
            <path value="../state/index"/>
          </type>
          <description>
            <text>Reference to list key</text>
          </description>
        </leaf>
        <container name="state">
          <config value="false"/>
          <description>
            <text>Operational state for common path attributes</text>
          </description>
          <leaf name="index">
            <type name="uint64"/>
            <description>
              <text>System generated index for each attribute set.  The
index is used to reference an attribute set from a
specific path.  Multiple paths may reference the same
attribute set.</text>
            </description>
          </leaf>
          <uses name="bgp-shared-common-attr-state"/>
        </container>
        <uses name="bgp-aggregator-attr-top"/>
        <uses name="bgp-as-path-attr-top"/>
        <uses name="bgp-as4-path-attr-top"/>
        <uses name="bgp-tunnel-encapsulation-attr-top"/>
      </list>
    </container>
  </grouping>
  <grouping name="community-sets-top">
    <description>
      <text>Top level grouping for list of shared community attribute
sets</text>
    </description>
    <container name="communities">
      <description>
        <text>Enclosing container for the list of community attribute
sets</text>
      </description>
      <list name="community">
        <key value="index"/>
        <description>
          <text>List of path attributes that may be in use by multiple
routes in the table</text>
        </description>
        <leaf name="index">
          <type name="leafref">
            <path value="../state/index"/>
          </type>
          <description>
            <text>Reference to the list key</text>
          </description>
        </leaf>
        <container name="state">
          <config value="false"/>
          <description>
            <text>Operational state for shared BGP community attribute</text>
          </description>
          <leaf name="index">
            <type name="uint64"/>
            <description>
              <text>System generated index for each attribute set.  The
index is used to reference an attribute set from a
specific path.  Multiple paths may reference the same
attribute set.</text>
            </description>
          </leaf>
          <uses name="bgp-community-attr-state"/>
        </container>
      </list>
    </container>
  </grouping>
  <grouping name="ext-community-sets-top">
    <description>
      <text>Top level grouping for list of extended community attribute
sets</text>
    </description>
    <container name="ext-communities">
      <description>
        <text>Enclosing container for the list of extended community
attribute sets</text>
      </description>
      <list name="ext-community">
        <key value="index"/>
        <description>
          <text>List of path attributes that may be in use by multiple
routes in the table</text>
        </description>
        <leaf name="index">
          <type name="leafref">
            <path value="../state/index"/>
          </type>
          <description>
            <text>Reference to the list key</text>
          </description>
        </leaf>
        <container name="state">
          <config value="false"/>
          <description>
            <text>Operational state for shared BGP extended community
attribute</text>
          </description>
          <leaf name="index">
            <type name="uint64"/>
            <description>
              <text>System generated index for each attribute set.  The
index is used to reference an attribute set from a
specific path.  Multiple paths may reference the same
attribute set.</text>
            </description>
          </leaf>
          <uses name="bgp-extended-community-attr-state"/>
        </container>
      </list>
    </container>
  </grouping>
</submodule>
