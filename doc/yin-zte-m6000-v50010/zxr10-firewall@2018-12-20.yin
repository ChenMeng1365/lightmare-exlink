      </container>
      <container name="white-lists">
        <description>
          <text>Configure whitelist list</text>
        </description>
        <container name="white-ip-lists">
          <description>
            <text>Whitelist that key is IP</text>
          </description>
          <list name="white-ip-list">
            <description>
              <text>Whitelist that key is IP</text>
            </description>
            <key value="ip"/>
            <leaf name="ip">
              <description>
