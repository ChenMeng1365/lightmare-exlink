          <list name="af">
            <key value="address-family"/>
            <description>
              <text>Statistics of BGP route discarded in the address family.</text>
            </description>
            <leaf name="address-family">
              <type name="bgp:af-type"/>
              <description>
                <text>Address family type.</text>
