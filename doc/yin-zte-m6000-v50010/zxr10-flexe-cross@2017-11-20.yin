<?xml version="1.0" encoding="UTF-8"?>
<module name="zxr10-flexe-cross"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:flexe-cross="http://www.zte.com.cn/zxr10/netconf/schema/rosng/flexe-cross"
        xmlns:ncd="http://www.zte.com.cn/zxr10/netconf/schema/rosng/netconf-defs">
  <namespace uri="http://www.zte.com.cn/zxr10/netconf/schema/rosng/flexe-cross"/>
  <prefix value="flexe-cross"/>
  <import module="zxr10-netconf-defs">
    <prefix value="ncd"/>
  </import>
  <organization>
    <text>ZTE Corporation</text>
  </organization>
  <contact>
    <text>ZTE Corporation  
   Nanjing Institute of ZTE Corporation 
   No.68 Zijinghua Rd. Yuhuatai District, Nanjing, China 
   Tel: +86-25-52870000</text>
  </contact>
  <description>
    <text>The netconf module to describe the flexE cross relationships between two flexE client interfaces.</text>
  </description>
  <revision date="2017-11-20">
    <description>
      <text>The initial version</text>
    </description>
  </revision>
  <container name="configuration">
    <description>
      <text>Describe the configuration of the flexE cross relationships between two flexE client interfaces.</text>
    </description>
    <container name="flexe-crosses">
      <description>
        <text>Describe the parameters of the flexE cross relationships between two flexE client interfaces.</text>
      </description>
      <list name="flexe-cross">
        <description>
          <text>The list of the flexE cross relationships between two flexE client interfaces.</text>
        </description>
        <key value="flexe-client-x"/>
        <leaf name="flexe-client-x">
          <description>
            <text>The flexE client interface x of the flexE cross relationship</text>
          </description>
          <type name="ncd:ifname"/>
        </leaf>
        <leaf name="flexe-client-y">
          <description>
            <text>The flexE client interface y of the flexE cross relationship</text>
          </description>
          <type name="ncd:ifname"/>
          <mandatory value="true"/>
        </leaf>
      </list>
    </container>
  </container>
</module>
