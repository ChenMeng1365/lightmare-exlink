    "definitions": {
        "dhcpv6-server-property": {
            "description": "Configure a status (Up/Down) switchover for a DHCPv6 server.",
            "properties": {
                "dead-count": {
                    "type": "string",
                    "description": "Specify the maximum number of times at which the DHCPv6 server does not send response packets."
                },
                "time-out": {
                    "type": "string",
                    "description": "Specify a timeout period. If a device receives no response from the DHCPv6 server within the timeout period, it re-sends request packets."
                },
                "dead-time": {
                    "type": "string",
                    "description": "Specify a period within which the DHCPv6 server is down. After the dead-time, the DHCPv6 server is set to up."
                }
            }
        },
