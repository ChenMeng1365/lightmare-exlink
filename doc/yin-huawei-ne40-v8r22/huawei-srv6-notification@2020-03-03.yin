<?xml version="1.0" encoding="UTF-8"?>
<module name="huawei-srv6-notification"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:srv6-notification="urn:huawei:yang:huawei-srv6-notification"
        xmlns:inet="urn:ietf:params:xml:ns:yang:ietf-inet-types"
        xmlns:ext="urn:huawei:yang:huawei-extension">
  <yang-version value="1"/>
  <namespace uri="urn:huawei:yang:huawei-srv6-notification"/>
  <prefix value="srv6-notification"/>
  <import module="ietf-inet-types">
    <prefix value="inet"/>
  </import>
  <import module="huawei-extension">
    <prefix value="ext"/>
  </import>
  <organization>
    <text>Huawei Technologies Co., Ltd.</text>
  </organization>
  <contact>
    <text>Huawei Industrial Base
Bantian, Longgang
Shenzhen 518129
People's Republic of China
Website: http://www.huawei.com
Email: support@huawei.com</text>
  </contact>
  <description>
    <text>Notification of segment routing IPv6.</text>
  </description>
  <revision date="2020-03-03">
    <description>
      <text>Init revision.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <ext:task-name value="segr"/>
  <notification name="local-sid-route-conflict">
    <description>
      <text>Conflict between an SRv6 local SID and a route.</text>
    </description>
    <leaf name="local-sid">
      <type name="inet:ipv6-address-no-zone"/>
      <description>
        <text>Local SID.</text>
      </description>
    </leaf>
    <leaf name="prefix-length">
      <type name="uint32"/>
      <description>
        <text>Mask length of local SID.</text>
      </description>
    </leaf>
  </notification>
  <notification name="local-sid-route-conflict-resume">
    <description>
      <text>The conflict between the SRv6 local SID and route was removed.</text>
    </description>
    <leaf name="local-sid">
      <type name="inet:ipv6-address-no-zone"/>
      <description>
        <text>Local SID.</text>
      </description>
    </leaf>
    <leaf name="prefix-length">
      <type name="uint32"/>
      <description>
        <text>Mask length of local SID.</text>
      </description>
    </leaf>
  </notification>
</module>
