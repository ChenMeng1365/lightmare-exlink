<?xml version="1.0" encoding="UTF-8"?>
<submodule name="huawei-nqa-test-flow"
           xmlns="urn:ietf:params:xml:ns:yang:yin:1"
           xmlns:nqa="urn:huawei:yang:huawei-nqa"
           xmlns:pub-type="urn:huawei:yang:huawei-pub-type"
           xmlns:inet="urn:ietf:params:xml:ns:yang:ietf-inet-types">
  <yang-version value="1"/>
  <belongs-to module="huawei-nqa">
    <prefix value="nqa"/>
  </belongs-to>
  <import module="huawei-pub-type">
    <prefix value="pub-type"/>
  </import>
  <import module="ietf-inet-types">
    <prefix value="inet"/>
  </import>
  <organization>
    <text>Huawei Technologies Co., Ltd.</text>
  </organization>
  <contact>
    <text>Huawei Industrial Base
Bantian, Longgang
Shenzhen 518129
People's Republic of China
Website: http://www.huawei.com
Email: support@huawei.com</text>
  </contact>
  <description>
    <text>Description of submodule nqa-instance.</text>
  </description>
  <revision date="2021-04-29">
    <description>
      <text>Init revision.</text>
    </description>
    <reference>
      <text>Huawei private.</text>
    </reference>
  </revision>
  <grouping name="test-flows-type">
    <description>
      <text>Test flow.</text>
    </description>
    <list name="test-flow">
      <must condition="(ce-vid and pe-vid) or (not(ce-vid) and not(pe-vid))"/>
      <must condition="(color-aware-green-8021p and color-aware-yellow-8021p) or (not(color-aware-green-8021p) and not(color-aware-yellow-8021p))"/>
      <must condition="(color-aware-green-dscp and color-aware-yellow-dscp) or (not(color-aware-green-dscp) and not(color-aware-yellow-dscp))"/>
      <must condition="(end-dest-port and (end-dest-port - dest-port) &gt; 0 and (end-dest-port - dest-port) &lt; 128) or not(end-dest-port)"/>
      <must condition="(end-src-port  and (end-src-port  - src-port)  &gt; 0 and (end-src-port  - src-port)  &lt; 128) or not(end-src-port)"/>
      <must condition="(end-vlan-id   and (end-vlan-id   - vlan-id)   &gt; 0 and (end-vlan-id   - vlan-id)   &lt; 128) or not(end-vlan-id)"/>
      <must condition="(end-ce-vid    and (end-ce-vid    - ce-vid)    &gt; 0 and (end-ce-vid    - ce-vid)    &lt; 128) or not(end-ce-vid)"/>
      <must condition="(end-color-aware-green-8021p  and end-color-aware-green-8021p  &gt; color-aware-green-8021p)  or not(end-color-aware-green-8021p)"/>
      <must condition="(end-color-aware-yellow-8021p and end-color-aware-yellow-8021p &gt; color-aware-yellow-8021p) or not(end-color-aware-yellow-8021p)"/>
      <must condition="(end-color-aware-green-dscp   and end-color-aware-green-dscp   &gt; color-aware-green-dscp)   or not(end-color-aware-green-dscp)"/>
      <must condition="(end-color-aware-yellow-dscp  and end-color-aware-yellow-dscp  &gt; color-aware-yellow-dscp)  or not(end-color-aware-yellow-dscp)"/>
      <must condition="(end-color-blind-8021p        and end-color-blind-8021p        &gt; color-blind-8021p)        or not(end-color-blind-8021p)"/>
      <must condition="(end-color-blind-dscp         and end-color-blind-dscp         &gt; color-blind-dscp)         or not(end-color-blind-dscp)"/>
      <must condition="(flr and ftd and fdv) or (not(flr) and not(ftd) and not(fdv))"/>
      <key value="test-flow-id"/>
      <description>
        <text>Configure NQA test flow.</text>
      </description>
      <leaf name="test-flow-id">
        <type name="uint16">
          <range value="1..65535"/>
        </type>
        <description>
          <text>ID of a test flow.</text>
        </description>
      </leaf>
      <leaf name="dest-mac">
        <type name="pub-type:mac-address"/>
        <default value="0000-0000-0000"/>
        <description>
          <text>Start destination MAC address.</text>
        </description>
      </leaf>
      <leaf name="src-mac">
        <type name="pub-type:mac-address"/>
        <default value="0000-0000-0000"/>
        <description>
          <text>Start source MAC address.</text>
        </description>
      </leaf>
      <leaf name="dest-ipv4">
        <type name="inet:ipv4-address-no-zone"/>
        <default value="0.0.0.0"/>
        <description>
          <text>Start destination IPv4 address.</text>
        </description>
      </leaf>
      <leaf name="src-ipv4">
        <type name="inet:ipv4-address-no-zone"/>
        <default value="0.0.0.0"/>
        <description>
          <text>Start source IP address.</text>
        </description>
      </leaf>
      <leaf name="dest-port">
        <type name="uint16">
          <range value="7|1025..65535"/>
        </type>
        <default value="7"/>
        <description>
          <text>Start destination UDP port.</text>
        </description>
      </leaf>
      <leaf name="src-port">
        <type name="uint16">
          <range value="1025..65535"/>
        </type>
        <default value="49184"/>
        <description>
          <text>Start source UDP port.</text>
        </description>
      </leaf>
      <choice name="vlan-tag-config">
        <description>
          <text>VLAN configuration selection. One VLAN tag or two VLAN tags can be configured.</text>
        </description>
        <case name="one-vlan-tag">
          <description>
            <text>One VLAN tag can be configured.</text>
          </description>
          <leaf name="vlan-id">
            <type name="uint16">
              <range value="1..4094"/>
            </type>
            <description>
              <text>Begin VLAN ID.</text>
            </description>
          </leaf>
        </case>
        <case name="two-vlan-tag">
          <description>
            <text>Two VLAN tags can be configured.</text>
          </description>
          <leaf name="ce-vid">
            <type name="uint16">
              <range value="1..4094"/>
            </type>
            <description>
              <text>Begin CE VLAN ID.</text>
            </description>
          </leaf>
          <leaf name="pe-vid">
            <type name="uint16">
              <range value="1..4094"/>
            </type>
            <description>
              <text>PE VLAN ID.</text>
            </description>
          </leaf>
        </case>
      </choice>
      <choice name="color-mode">
        <description>
          <text>Color mode of a test flow.</text>
        </description>
        <case name="color-blind">
          <description>
            <text>The color mode is color blind.</text>
          </description>
          <choice name="blind-priority-type">
            <description>
              <text>The priority type when color mode is color-blind.</text>
            </description>
            <case name="blind-priority-8021p">
              <description>
                <text>The priority type is 802.1p.</text>
              </description>
              <leaf name="color-blind-8021p">
                <type name="uint8">
                  <range value="0..7"/>
                </type>
                <description>
                  <text>The start 802.1p priority when color mode is color-blind.</text>
                </description>
              </leaf>
            </case>
            <case name="blind-priority-dscp">
              <description>
                <text>The priority type is DSCP.</text>
              </description>
              <leaf name="color-blind-dscp">
                <type name="uint8">
                  <range value="0..63"/>
                </type>
                <description>
                  <text>The start DSCP priority when color mode is color-blind.</text>
                </description>
              </leaf>
            </case>
          </choice>
        </case>
        <case name="color-aware">
          <description>
            <text>The color mode is color aware.</text>
          </description>
          <choice name="aware-priority-type">
            <description>
              <text>The priority type when color mode is color-aware.</text>
            </description>
            <case name="aware-priority-8021p">
              <description>
                <text>The priority type is 802.1p.</text>
              </description>
              <leaf name="color-aware-green-8021p">
                <type name="uint8">
                  <range value="0..7"/>
                </type>
                <description>
                  <text>The start 802.1p priority of green packets when the color mode is color-aware.</text>
                </description>
              </leaf>
              <leaf name="color-aware-yellow-8021p">
                <type name="uint8">
                  <range value="0..7"/>
                </type>
                <description>
                  <text>The start 802.1p priority of yellow packets when the color mode is color-aware.</text>
                </description>
              </leaf>
            </case>
            <case name="aware-priority-dscp">
              <description>
                <text>The priority type is DSCP.</text>
              </description>
              <leaf name="color-aware-green-dscp">
                <type name="uint8">
                  <range value="0..63"/>
                </type>
                <description>
                  <text>The start DSCP priority of green packets when the color mode is color-aware.</text>
                </description>
              </leaf>
              <leaf name="color-aware-yellow-dscp">
                <type name="uint8">
                  <range value="0..63"/>
                </type>
                <description>
                  <text>The start DSCP priority of yellow packets when the color mode is color-aware.</text>
                </description>
              </leaf>
            </case>
          </choice>
        </case>
      </choice>
      <choice name="range-type">
        <description>
          <text>Range type.</text>
        </description>
        <case name="dest-mac">
          <description>
            <text>The destination MAC address will be set to a range.</text>
          </description>
          <choice name="dest-mac-range-type">
            <description>
              <text>Destination MAC range type.</text>
            </description>
            <case name="end-dest-mac">
              <description>
                <text>End destination MAC address.</text>
              </description>
              <leaf name="end-dest-mac">
                <type name="pub-type:mac-address"/>
                <description>
                  <text>End destination MAC address.</text>
                </description>
              </leaf>
            </case>
            <case name="increment-dest-mac">
              <description>
                <text>The incremental range of destination MAC addresses.</text>
              </description>
              <leaf name="dest-mac-mask">
                <type name="pub-type:mac-address"/>
                <description>
                  <text>The mask of destination MAC address. After conversion to binary, the positions of 1s must be consecutive, and the number of consecutive 1s cannot be less than 1 or greater than 32.</text>
                </description>
              </leaf>
              <leaf name="dest-mac-increment-step">
                <type name="uint8">
                  <range value="1..128"/>
                </type>
                <description>
                  <text>Increment step.</text>
                </description>
              </leaf>
              <leaf name="dest-mac-increment-count">
                <type name="uint8">
                  <range value="1..128"/>
                </type>
                <description>
                  <text>Increment count.</text>
                </description>
              </leaf>
            </case>
          </choice>
        </case>
        <case name="source-mac">
          <description>
            <text>The source MAC address will be set to a range.</text>
          </description>
          <choice name="src-mac-range-type">
            <description>
              <text>Source MAC range type.</text>
            </description>
            <case name="end-src-mac">
              <description>
                <text>End source MAC address.</text>
              </description>
              <leaf name="end-src-mac">
                <type name="pub-type:mac-address"/>
                <description>
                  <text>End source MAC address.</text>
                </description>
              </leaf>
            </case>
            <case name="increment-src-mac">
              <description>
                <text>The incremental range of source MAC addresses.</text>
              </description>
              <leaf name="src-mac-mask">
                <type name="pub-type:mac-address"/>
                <description>
                  <text>The mask of source MAC address. After conversion to binary, the positions of 1s must be consecutive, and the number of consecutive 1s cannot be less than 1 or greater than 32.</text>
                </description>
              </leaf>
              <leaf name="src-mac-increment-step">
                <type name="uint8">
                  <range value="1..128"/>
                </type>
                <description>
                  <text>Increment step.</text>
                </description>
              </leaf>
              <leaf name="src-mac-increment-count">
                <type name="uint8">
                  <range value="1..128"/>
                </type>
                <description>
                  <text>Increment count.</text>
                </description>
              </leaf>
            </case>
          </choice>
        </case>
        <case name="dest-ipv4">
          <description>
            <text>The destination IPv4 address will be set to a range.</text>
          </description>
          <choice name="dest-ipv4-range-type">
            <description>
              <text>Destination IPv4 range type.</text>
            </description>
            <case name="end-dest-ipv4">
              <description>
                <text>End destination IPv4 address.</text>
              </description>
              <leaf name="end-dest-ipv4">
                <type name="inet:ipv4-address-no-zone"/>
                <description>
                  <text>End destination IPv4 address.</text>
                </description>
              </leaf>
            </case>
            <case name="increment-dest-ipv4">
              <description>
                <text>The incremental range of destination IPv4 addresses.</text>
              </description>
              <leaf name="dest-ipv4-mask">
                <type name="inet:ipv4-address-no-zone"/>
                <description>
                  <text>The mask of destination IPv4 address. After conversion to binary, the positions of 1s must be consecutive, and the number of consecutive 1s cannot be less than 1 or greater than 32.</text>
                </description>
              </leaf>
              <leaf name="dest-ipv4-increment-step">
                <type name="uint8">
                  <range value="1..128"/>
                </type>
                <description>
                  <text>Increment step.</text>
                </description>
              </leaf>
              <leaf name="dest-ipv4-increment-count">
                <type name="uint8">
                  <range value="1..128"/>
                </type>
                <description>
                  <text>Increment count.</text>
                </description>
              </leaf>
            </case>
          </choice>
        </case>
        <case name="source-ipv4">
          <description>
            <text>The source IPv4 address will be set to a range.</text>
          </description>
          <choice name="src-ipv4-range-type">
            <description>
              <text>Source IPv4 range type.</text>
            </description>
            <case name="end-src-ipv4">
              <description>
                <text>End source IPv4 address.</text>
              </description>
              <leaf name="end-src-ipv4">
                <type name="inet:ipv4-address-no-zone"/>
                <description>
                  <text>End source IPv4 address.</text>
                </description>
              </leaf>
            </case>
            <case name="increment-src-ipv4">
              <description>
                <text>The incremental range of source IPv4 addresses.</text>
              </description>
              <leaf name="src-ipv4-mask">
                <type name="inet:ipv4-address-no-zone"/>
                <description>
                  <text>The mask of source IPv4 address. After conversion to binary, the positions of 1s must be consecutive, and the number of consecutive 1s cannot be less than 1 or greater than 32.</text>
                </description>
              </leaf>
              <leaf name="src-ipv4-increment-step">
                <type name="uint8">
                  <range value="1..128"/>
                </type>
                <description>
                  <text>Increment step.</text>
                </description>
              </leaf>
              <leaf name="src-ipv4-increment-count">
                <type name="uint8">
                  <range value="1..128"/>
                </type>
                <description>
                  <text>Increment count.</text>
                </description>
              </leaf>
            </case>
          </choice>
        </case>
        <case name="dest-port">
          <description>
            <text>The destination UDP port will be set to a range.</text>
          </description>
          <leaf name="end-dest-port">
            <type name="uint16">
              <range value="1025..65535"/>
            </type>
            <description>
              <text>End destination UDP port.</text>
            </description>
          </leaf>
        </case>
        <case name="source-port">
          <description>
            <text>The source UDP port will be set to a range.</text>
          </description>
          <leaf name="end-src-port">
            <type name="uint16">
              <range value="1025..65535"/>
            </type>
            <description>
              <text>End source UDP port.</text>
            </description>
          </leaf>
        </case>
        <case name="vlan-id">
          <description>
            <text>The VLAN ID will be set to a range.</text>
          </description>
          <leaf name="end-vlan-id">
            <when condition="../vlan-id"/>
            <type name="uint16">
              <range value="1..4094"/>
            </type>
            <description>
              <text>End CE VLAN ID.</text>
            </description>
          </leaf>
        </case>
        <case name="ce-vid">
          <description>
            <text>The CE VLAN ID will be set to a range.</text>
          </description>
          <leaf name="end-ce-vid">
            <when condition="../ce-vid"/>
            <type name="uint16">
              <range value="1..4094"/>
            </type>
            <description>
              <text>End CE VLAN ID.</text>
            </description>
          </leaf>
        </case>
        <case name="color-blind-8021p">
          <description>
            <text>The 802.1p priority value in color-blind mode will be set to a range.</text>
          </description>
          <leaf name="end-color-blind-8021p">
            <when condition="../color-blind-8021p"/>
            <type name="uint8">
              <range value="0..7"/>
            </type>
            <description>
              <text>The end 802.1p priority when color mode is color-blind.</text>
            </description>
          </leaf>
        </case>
        <case name="color-blind-dscp">
          <description>
            <text>The DSCP priority value in color-blind mode will be set to a range.</text>
          </description>
          <leaf name="end-color-blind-dscp">
            <when condition="../color-blind-dscp"/>
            <type name="uint8">
              <range value="0..63"/>
            </type>
            <description>
              <text>The end dscp priority when color mode is color-blind.</text>
            </description>
          </leaf>
        </case>
        <case name="color-aware-8021p">
          <description>
            <text>The 802.1p priority value in color-aware mode will be set to a range.</text>
          </description>
          <leaf name="end-color-aware-green-8021p">
            <when condition="../color-aware-green-8021p"/>
            <type name="uint8">
              <range value="0..7"/>
            </type>
            <description>
              <text>The end 802.1p priority of green packets when color mode is color-aware.</text>
            </description>
          </leaf>
          <leaf name="end-color-aware-yellow-8021p">
            <when condition="../color-aware-yellow-8021p"/>
            <type name="uint8">
              <range value="0..7"/>
            </type>
            <description>
              <text>The end 802.1p priority of yellow packets when color mode is color-aware.</text>
            </description>
          </leaf>
        </case>
        <case name="color-aware-dscp">
          <description>
            <text>The DSCP priority value in color-aware mode will be set to a range.</text>
          </description>
          <leaf name="end-color-aware-green-dscp">
            <when condition="../color-aware-green-dscp"/>
            <type name="uint8">
              <range value="0..63"/>
            </type>
            <description>
              <text>The end DSCP priority of green packets when color mode is color-aware.</text>
            </description>
          </leaf>
          <leaf name="end-color-aware-yellow-dscp">
            <when condition="../color-aware-yellow-dscp"/>
            <type name="uint8">
              <range value="0..63"/>
            </type>
            <description>
              <text>The end DSCP priority of yellow packets when color mode is color-aware.</text>
            </description>
          </leaf>
        </case>
      </choice>
      <leaf name="cir">
        <type name="uint32">
          <range value="8..10000000"/>
        </type>
        <units name="kbit/s"/>
        <description>
          <text>Committed information rate.</text>
        </description>
      </leaf>
      <leaf name="eir">
        <when condition="../cir"/>
        <type name="uint32">
          <range value="1..10000000"/>
        </type>
        <units name="kbit/s"/>
        <description>
          <text>Excess information rate.</text>
        </description>
      </leaf>
      <leaf name="flr">
        <type name="uint32">
          <range value="0..100000"/>
        </type>
        <units name="0.001 %"/>
        <description>
          <text>Value of frame loss rate in per 100000.</text>
        </description>
      </leaf>
      <leaf name="ftd">
        <type name="uint32">
          <range value="1..10000000"/>
        </type>
        <units name="us"/>
        <description>
          <text>Frame transmission delay.</text>
        </description>
      </leaf>
      <leaf name="fdv">
        <type name="uint32">
          <range value="1..10000000"/>
        </type>
        <units name="us"/>
        <description>
          <text>Frame delay variation.</text>
        </description>
      </leaf>
      <leaf name="cir-simple-test">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable a simple CIR test.</text>
        </description>
      </leaf>
      <leaf name="traffic-policing-test">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable traffic policing test.</text>
        </description>
      </leaf>
      <leaf name="description">
        <type name="string">
          <length value="1..127"/>
        </type>
        <description>
          <text>Description of a test flow.</text>
        </description>
      </leaf>
    </list>
  </grouping>
</submodule>
