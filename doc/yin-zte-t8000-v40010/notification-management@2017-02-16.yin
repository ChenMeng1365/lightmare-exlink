<?xml version="1.0" encoding="UTF-8"?>
<module name="notification-management"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:manageEvent="urn:ietf:params:xml:ns:netmod:notification">
  <namespace uri="urn:ietf:params:xml:ns:netmod:notification"/>
  <prefix value="manageEvent"/>
  <organization>
    <text>IETF NETCONF WG</text>
  </organization>
  <contact>
    <text>netconf@ietf.org</text>
  </contact>
  <description>
    <text>Conversion of the 'manageEvent' XSD in the NETCONF
Notifications RFC.</text>
  </description>
  <reference>
    <text>RFC 5277</text>
  </reference>
  <revision date="2017-02-16">
    <description>
      <text>RFC 5277 version.</text>
    </description>
  </revision>
  <container name="netconf">
    <description>
      <text>Top-level element in the notification namespace</text>
    </description>
    <config value="false"/>
    <container name="streams">
      <description>
        <text>The list of event streams supported by the system. When a query is issued, the returned set of streams is determined based on user privileges.</text>
      </description>
      <list name="stream">
        <description>
          <text>Stream name, description and other information.</text>
        </description>
        <key value="name"/>
        <min-elements value="1"/>
        <leaf name="name">
          <description>
            <text>The name of the event stream. If this is the default NETCONF stream, this must have the value 'NETCONF'.</text>
          </description>
          <type name="string">
            <length value="1..255"/>
          </type>
        </leaf>
        <leaf name="description">
          <description>
            <text>A description of the event stream, including such information as the type of events that are sent over this stream.</text>
          </description>
          <type name="string">
            <length value="1..255"/>
          </type>
          <mandatory value="true"/>
        </leaf>
        <leaf name="replaySupport">
          <description>
            <text>An indication of whether or not event replay is available on this stream.</text>
          </description>
          <type name="boolean"/>
          <default value="false"/>
        </leaf>
      </list>
    </container>
  </container>
</module>
