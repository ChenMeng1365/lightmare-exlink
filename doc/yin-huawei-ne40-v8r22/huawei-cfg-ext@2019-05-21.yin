<?xml version="1.0" encoding="UTF-8"?>
<module name="huawei-cfg-ext"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:cfg-ext="urn:huawei:yang:huawei-cfg-ext"
        xmlns:ext="urn:huawei:yang:huawei-extension">
  <yang-version value="1.1"/>
  <namespace uri="urn:huawei:yang:huawei-cfg-ext"/>
  <prefix value="cfg-ext"/>
  <import module="huawei-extension">
    <prefix value="ext"/>
  </import>
  <organization>
    <text>Huawei Technologies Co., Ltd.</text>
  </organization>
  <contact>
    <text>Huawei Industrial Base
Bantian, Longgang
Shenzhen 518129
People's Republic of China
Website: http://www.huawei.com
Email: support@huawei.com</text>
  </contact>
  <description>
    <text>Display the changes information in the candidate datastore.</text>
  </description>
  <revision date="2019-05-21">
    <description>
      <text>Init revision.</text>
    </description>
    <reference>
      <text>RFC 6241, Section 8.3.</text>
    </reference>
  </revision>
  <ext:task-name value="config"/>
  <rpc name="get-candidate-change">
    <description>
      <text>Configuration change information.</text>
    </description>
    <output>
      <anydata name="effective-changes">
        <description>
          <text>Configuration change information recorded in the candidate datastore.</text>
        </description>
      </anydata>
      <anydata name="ineffective-changes">
        <description>
          <text>Changes in configurations that have not taken effect in the candidate datastore.</text>
        </description>
      </anydata>
    </output>
  </rpc>
  <rpc name="get-configuration-change">
    <description>
      <text>Get configuration change information.</text>
    </description>
    <input>
      <container name="source">
        <description>
          <text>The source configuration.</text>
        </description>
        <choice name="config-source">
          <description>
            <text>Particular source configuration to compare from.</text>
          </description>
          <case name="checkpoint">
            <description>
              <text>Configuration checkpoints.</text>
            </description>
            <leaf name="commit-id">
              <type name="string">
                <length value="1..20"/>
              </type>
              <description>
                <text>Uniquely identify a configuration checkpoint.</text>
              </description>
            </leaf>
          </case>
          <case name="running">
            <description>
              <text>Running configuration.</text>
            </description>
            <leaf name="running">
              <type name="empty"/>
              <description>
                <text>Running configuration.</text>
              </description>
            </leaf>
          </case>
          <case name="url">
            <description>
              <text>The URL-based configuration.</text>
            </description>
            <leaf name="url">
              <type name="string">
                <length value="1..256"/>
              </type>
              <description>
                <text>The URL-based configuration.</text>
              </description>
            </leaf>
          </case>
        </choice>
      </container>
      <container name="target">
        <description>
          <text>The target configuration.</text>
        </description>
        <choice name="config-target">
          <description>
            <text>Particular target configuration to compare to.</text>
          </description>
          <case name="checkpoint">
            <description>
              <text>Configuration checkpoints.</text>
            </description>
            <leaf name="commit-id">
              <type name="string">
                <length value="1..20"/>
              </type>
              <description>
                <text>Uniquely identify a configuration checkpoint.</text>
              </description>
            </leaf>
          </case>
          <case name="running">
            <description>
              <text>Running configuration.</text>
            </description>
            <leaf name="running">
              <type name="empty"/>
              <description>
                <text>Running configuration.</text>
              </description>
            </leaf>
          </case>
          <case name="url">
            <description>
              <text>The URL-based configuration.</text>
            </description>
            <leaf name="url">
              <type name="string">
                <length value="1..256"/>
              </type>
              <description>
                <text>The URL-based configuration.</text>
              </description>
            </leaf>
          </case>
        </choice>
      </container>
      <leaf name="rollback-tag">
        <type name="empty"/>
        <description>
          <text>With rollback tag.</text>
        </description>
      </leaf>
    </input>
    <output>
      <anydata name="changes">
        <description>
          <text>Configuration changes.</text>
        </description>
      </anydata>
    </output>
  </rpc>
  <rpc name="load-configuration-change">
    <description>
      <text>After configuration difference loading, users can get to know the result of configuration difference loading through load-configuration-result node.</text>
    </description>
    <input>
      <anydata name="changes">
        <description>
          <text>Configuration change to be loaded.</text>
        </description>
      </anydata>
    </input>
  </rpc>
  <rpc name="load-configuration-result">
    <description>
      <text>The result of load configuration changes.</text>
    </description>
    <output>
      <anydata name="result">
        <description>
          <text>Configuration loading result.</text>
        </description>
      </anydata>
    </output>
  </rpc>
</module>
