  <ext:task-name value="mpls-ldp"/>
  <grouping name="session-car-grp">
    <description>
      <text>Session CAR group.</text>
    </description>
    <leaf name="cir">
      <type name="uint32">
        <range value="1..1000000"/>
      </type>
      <units name="kbit/s"/>
      <description>
        <text>Session CAR CIR value.</text>
      </description>
    </leaf>
    <leaf name="cbs">
      <type name="uint32">
        <range value="1..9000000"/>
      </type>
      <units name="Byte"/>
      <description>
        <text>Session CAR CBS value.</text>
      </description>
    </leaf>
    <leaf name="pir">
      <type name="uint32">
        <range value="1..1000000"/>
      </type>
      <units name="kbit/s"/>
      <description>
        <text>Session CAR PIR value.</text>
      </description>
    </leaf>
    <leaf name="pbs">
      <type name="uint32">
        <range value="1..9000000"/>
      </type>
      <units name="Byte"/>
      <description>
        <text>Session CAR PBS value.</text>
      </description>
    </leaf>
  </grouping>
  <rpc name="reset-ldp-session">
    <description>
      <text>Reset LDP Session.</text>
    </description>
    <input>
      <leaf name="peer-id">
        <type name="inet:ipv4-address-no-zone"/>
        <description>
          <text>Peer ID of an LDP session to be restarted.</text>
        </description>
      </leaf>
      <leaf name="graceful">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>GR flag.</text>
        </description>
      </leaf>
      <leaf name="vrfname">
        <type name="string">
          <length value="1..31"/>
        </type>
        <description>
          <text>Name of an LDP instance.</text>
        </description>
      </leaf>
    </input>
  </rpc>
  <rpc name="reset-all-ldp">
    <description>
      <text>Resets LDP.</text>
    </description>
    <input>
      <leaf name="graceful">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Keep uninterrupted forwarding.</text>
        </description>
      </leaf>
      <leaf name="vrfname">
        <type name="string">
          <length value="1..31"/>
        </type>
        <description>
          <text>Name of an LDP instance.</text>
        </description>
      </leaf>
    </input>
  </rpc>
  <rpc name="reset-ldp-traffic-statistics">
    <description>
      <text>Reset LDP traffic statistics.</text>
    </description>
    <input>
      <leaf name="ipv4-dest-address">
        <type name="inet:ipv4-address-no-zone"/>
        <description>
          <text>Prefix.</text>
        </description>
      </leaf>
      <leaf name="ipv4-masklen">
        <type name="uint32"/>
        <description>
          <text>Length of mask.</text>
        </description>
      </leaf>
    </input>
  </rpc>
  <rpc name="reset-mldp-traffic-statistics">
    <description>
      <text>Reset MLDP P2MP traffic statistics.</text>
    </description>
    <input>
      <leaf name="root-ip">
        <type name="inet:ipv4-address-no-zone"/>
        <description>
          <text>MLDP P2MP lsp traffic statistics root IP address.</text>
        </description>
      </leaf>
      <leaf name="lsp-id">
        <type name="uint32"/>
        <description>
          <text>MLDP P2MP LSP ID.</text>
        </description>
      </leaf>
    </input>
  </rpc>
  <rpc name="reset-mldp-all-traffic-statistics">
    <description>
      <text>Reset all MLDP P2MP traffic statistics.</text>
    </description>
    <input>
      <leaf name="is-all">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Reset all MLDP P2MP traffic statistics.</text>
        </description>
      </leaf>
    </input>
  </rpc>
  <rpc name="diagnose-lsp-establishment">
    <description>
      <text>Diagnose LSP establishment.</text>
    </description>
    <input>
      <leaf name="destination-address">
        <type name="inet:ipv4-address-no-zone"/>
        <mandatory value="true"/>
        <description>
          <text>Destination address of an LSP.</text>
        </description>
      </leaf>
      <leaf name="prefix-length">
        <type name="uint32">
          <range value="1..32"/>
        </type>
        <mandatory value="true"/>
        <description>
          <text>Prefix length of the destination address of an LSP.</text>
        </description>
      </leaf>
      <leaf name="vrfname">
        <type name="string">
          <length value="1..31"/>
        </type>
        <description>
          <text>Name of an LDP instance.</text>
        </description>
      </leaf>
    </input>
    <output>
      <leaf name="ingress-state">
        <type name="ldp-lsp-state"/>
        <description>
          <text>Ingress LSP state.</text>
        </description>
      </leaf>
      <leaf name="ingress-check-route">
        <type name="check-lsp-establishment"/>
        <description>
          <text>Route check for an ingress LSP.</text>
        </description>
      </leaf>
      <leaf name="ingress-check-policy">
        <type name="check-lsp-establishment"/>
        <description>
          <text>Policy check for an ingress LSP.</text>
        </description>
      </leaf>
      <leaf name="ingress-check-downstream-session">
        <type name="check-lsp-establishment"/>
        <description>
          <text>Downstream session check for an ingress LSP.</text>
        </description>
      </leaf>
      <leaf name="ingress-check-downstream-mapping">
        <type name="check-lsp-establishment"/>
        <description>
          <text>Downstream mapping check for an ingress LSP.</text>
        </description>
      </leaf>
      <leaf name="ingress-check-limit">
        <type name="check-lsp-establishment"/>
        <description>
          <text>Limit check for an ingress LSP.</text>
        </description>
      </leaf>
      <leaf name="ingress-check-resource">
        <type name="check-lsp-establishment"/>
        <description>
          <text>Resource check for an ingress LSP.</text>
        </description>
      </leaf>
      <leaf name="transit-state">
        <type name="ldp-lsp-state"/>
        <description>
          <text>Transit LSP state.</text>
        </description>
      </leaf>
      <leaf name="transit-check-route">
        <type name="check-lsp-establishment"/>
        <description>
          <text>Route check for a transit LSP.</text>
        </description>
      </leaf>
      <leaf name="transit-check-policy">
        <type name="check-lsp-establishment"/>
        <description>
          <text>Policy check for a transit LSP.</text>
        </description>
      </leaf>
      <leaf name="transit-check-downstream-session">
        <type name="check-lsp-establishment"/>
        <description>
          <text>Downstream session check for a transit LSP.</text>
        </description>
      </leaf>
      <leaf name="transit-check-downstream-mapping">
        <type name="check-lsp-establishment"/>
        <description>
          <text>Downstream mapping check for a transit LSP.</text>
        </description>
      </leaf>
      <leaf name="transit-check-upstream-request">
        <type name="check-lsp-establishment"/>
        <description>
          <text>Upstream request check for a transit LSP.</text>
        </description>
      </leaf>
      <leaf name="transit-check-limit">
        <type name="check-lsp-establishment"/>
        <description>
          <text>Limit check for a transit LSP.</text>
        </description>
      </leaf>
      <leaf name="transit-check-resource">
        <type name="check-lsp-establishment"/>
        <description>
          <text>Resource check for a transit LSP.</text>
        </description>
      </leaf>
      <leaf name="egress-state">
        <type name="ldp-lsp-state"/>
        <description>
          <text>Egress LSP state.</text>
        </description>
      </leaf>
      <leaf name="egress-check-route">
        <type name="check-lsp-establishment"/>
        <description>
          <text>Route check for an egress LSP.</text>
        </description>
      </leaf>
      <leaf name="egress-check-policy">
        <type name="check-lsp-establishment"/>
        <description>
          <text>Policy check for an egress LSP.</text>
        </description>
      </leaf>
      <leaf name="egress-check-upstream-request">
        <type name="check-lsp-establishment"/>
        <description>
          <text>Upstream request check for an egress LSP.</text>
        </description>
      </leaf>
      <leaf name="egress-check-limit">
        <type name="check-lsp-establishment"/>
        <description>
          <text>Limit check for an egress LSP.</text>
        </description>
      </leaf>
      <leaf name="egress-check-resource">
        <type name="check-lsp-establishment"/>
        <description>
          <text>Resource check for an egress LSP.</text>
        </description>
      </leaf>
    </output>
  </rpc>
  <rpc name="reset-ldp-event-records">
    <description>
      <text>Reset LDP event records.</text>
    </description>
    <input>
      <leaf name="session-down">
        <type name="boolean"/>
        <description>
          <text>Reset the down events of LDP sessions.</text>
        </description>
      </leaf>
      <leaf name="adjacency-down">
        <type name="boolean"/>
        <description>
          <text>Reset the down events of LDP adjacencys.</text>
        </description>
      </leaf>
    </input>
  </rpc>
  <rpc name="reset-ldp-error-packet">
    <description>
      <text>Reset LDP error packets.</text>
    </description>
    <input>
      <leaf name="tcp-error-packet">
        <type name="boolean"/>
        <description>
          <text>Reset TCP error packets.</text>
        </description>
      </leaf>
      <leaf name="udp-error-packet">
        <type name="boolean"/>
        <description>
          <text>Reset UDP error packets.</text>
        </description>
      </leaf>
      <leaf name="all-error-packet">
        <type name="boolean"/>
        <description>
          <text>Reset all error packets.</text>
        </description>
      </leaf>
    </input>
  </rpc>
  <augment target-node="/mpls:mpls">
    <description>
      <text>MPLS LDP Configuration information augment to module MPLS.</text>
    </description>
    <container name="ldp">
      <when condition="/mpls:mpls/mpls:common/mpls:global/mpls:mpls-enable='true'"/>
      <description>
        <text>Configure LDP, query and modify LDP Configuration information.</text>
      </description>
      <leaf name="enable">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable LDP.</text>
        </description>
      </leaf>
      <leaf name="lsp-trigger-mode">
        <type name="ldp-trigger-type"/>
        <must condition="(current()='ip-prefix' and ../lsp-ip-prefix-name) or (current()!='ip-prefix' and not(../lsp-ip-prefix-name))"/>
        <default value="host"/>
        <description>
          <text>Specifies a triggering mode for establishing LSPs by LDP. The value can be: 1. All mode (in which LDP is triggered by all static routes and IGP routes to establish LSPs); 2. Host mode (in which LDP is triggered by 32- or 128-bit IP routes to establish LSPs); 3. None mode (in which LDP is not triggered to establish LSPs); 4. IP-prefix mode (in which LDP is triggered by the IP address prefix list to establish LSPs.</text>
        </description>
      </leaf>
      <leaf name="lsp-ip-prefix-name">
        <type name="leafref">
          <path value="/rtp:routing-policy/rtp:ipv4-prefix-filters/rtp:ipv4-prefix-filter/rtp:name"/>
        </type>
        <must condition="../lsp-trigger-mode='ip-prefix'"/>
        <description>
          <text>Specifies or clear the IP prefix triggering mode, LDP is triggered to establish or to clear LSPs by IP address prefixes.</text>
        </description>
      </leaf>
      <leaf name="proxy-egress-lsp-disable">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable establish proxy LDP LSP.</text>
        </description>
      </leaf>
      <leaf name="bgp-label">
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable of triggering LDP alloc label for BGP label route.</text>
        </description>
      </leaf>
      <leaf name="bgp-ip-prefix-name">
        <when condition="../bgp-label='true'"/>
        <type name="leafref">
          <path value="/rtp:routing-policy/rtp:ipv4-prefix-filters/rtp:ipv4-prefix-filter/rtp:name"/>
        </type>
        <description>
          <text>Trigger policy when LDP alloc label for BGP label route.</text>
        </description>
      </leaf>
      <leaf name="bgp-not-only-host">
        <when condition="../bgp-label='true'"/>
        <type name="boolean"/>
        <default value="false"/>
        <description>
          <text>Enable/disable LDP to allocate labels for labeled BGP routes with non-32-bit masks.</text>
        </description>
      </leaf>
      <leaf name="sr-lsp-trigger-mode">
        <type name="sr-lsp-trigger-type"/>
        <default value="none"/>
        <description>
          <text>SR LSP Triggering Mode.</text>
        </description>
      </leaf>
      <container name="global">
        <when condition="../enable='true'"/>
        <description>
          <text>Configure global LDP. Query and modify global LDP parameters.</text>
        </description>
        <leaf name="mtu-signaling-mode">
          <type name="mtu-signaling-type"/>
          <default value="mtu-signaling"/>
          <description>
            <text>The MTU signaling.</text>
          </description>
        </leaf>
        <leaf name="gr-enable">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable LDP GR capability. Enabling or disabling of GR will cause the re-establishment of all LDP sessions.</text>
          </description>
        </leaf>
        <leaf name="reconnect-time">
          <when condition="../gr-enable='true'"/>
          <type name="uint32">
            <range value="3..259200"/>
          </type>
          <units name="s"/>
          <default value="300"/>
          <description>
            <text>Specifies the value of the LDP session reconnection timer.</text>
          </description>
        </leaf>
        <leaf name="recovery-time">
          <when condition="../gr-enable='true'"/>
          <type name="uint32">
            <range value="3..259200"/>
          </type>
          <units name="s"/>
          <default value="300"/>
          <description>
            <text>Specifies the value of the LSP recovery timer.</text>
          </description>
        </leaf>
        <leaf name="peer-live-time">
          <when condition="../gr-enable='true'"/>
          <type name="uint32">
            <range value="3..259200"/>
          </type>
          <units name="s"/>
          <default value="600"/>
          <description>
            <text>Specifies the value of the neighbor keeplive timer.</text>
          </description>
        </leaf>
        <leaf name="trap-pub-session">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable trap LDP session.</text>
          </description>
        </leaf>
        <leaf name="trap-session-rlfa-disable">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable a device from generating traps when LDP sessions go down due to RLFA route deletion.</text>
          </description>
        </leaf>
        <leaf name="gr-no-delete-flag">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable the modification of GR parameters, it will not affect established sessions, but only affect new created sessions.</text>
          </description>
        </leaf>
        <leaf name="announcement-enable">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable dynamic announcement for LDP.</text>
          </description>
        </leaf>
        <leaf name="back-off-init-time">
          <type name="uint32">
            <range value="5..2147483"/>
          </type>
          <units name="s"/>
          <default value="15"/>
          <description>
            <text>Specifies the init value of the exponential backoff timer.</text>
          </description>
        </leaf>
        <leaf name="back-off-max-time">
          <type name="uint32">
            <range value="5..2147483"/>
          </type>
          <units name="s"/>
          <default value="120"/>
          <description>
            <text>Specifies the maximum value of the exponential backoff timer.</text>
          </description>
        </leaf>
        <leaf name="entropy-label-enable">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable LDP entropy label capability.</text>
          </description>
        </leaf>
        <leaf name="prot-packet-suppress-disable">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable LDP packet suppress capability.</text>
          </description>
        </leaf>
        <leaf name="ssn-suppress-disable">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable LDP session suppress capability.</text>
          </description>
        </leaf>
        <leaf name="disable-unknown-tlv">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable unknowntlv.</text>
          </description>
        </leaf>
        <leaf name="ecmp-frr-coexist-enable">
          <type name="boolean"/>
          <default value="false"/>
          <description>
            <text>Enable/disable ECMP And FRR Coexist Capability.</text>
          </description>
        </leaf>
        <leaf name="bit-err-detect-level">
          <type name="leafref">
            <path value="/mpls:mpls/mpls:common/mpls:bit-err-levels/mpls:bit-err-level/mpls:level-number"/>
          </type>
          <description>
            <text>Config LDP bit error level.</text>
          </description>
        </leaf>
      </container>
      <container name="mldp">
        <when condition="../enable='true'"/>
        <description>
          <text>Configure MLDP. Query and modify MLDP parameters.</text>
        </description>
        <container name="p2mp">
          <description>
            <text>Configure MLDP P2MP. Query and modify MLDP P2MP parameters.</text>
          </description>
          <leaf name="enable">
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable P2MP for MLDP.</text>
            </description>
          </leaf>
          <leaf name="traffic-statistic-enable">
            <when condition="/mpls:mpls/mpls:common/mpls:global/mpls:traffic-statistic-enable='true'"/>
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable MLDP traffic statistic.</text>
            </description>
          </leaf>
          <container name="attributes">
            <when condition="../enable='true'"/>
            <description>
              <text>Configure MLDP attributes.</text>
            </description>
            <leaf name="make-before-break-enable">
              <type name="boolean"/>
              <default value="false"/>
              <description>
                <text>Enable/disable MBB capability for MLDP.</text>
              </description>
            </leaf>
            <leaf name="mbb-traffic-detect">
              <when condition="../make-before-break-enable='true'"/>
              <type name="boolean"/>
              <default value="false"/>
              <description>
                <text>Enable/disable MLDP MBB P2MP traffic detect.</text>
              </description>
            </leaf>
            <leaf name="mbb-switch-delay-time">
              <when condition="../make-before-break-enable='true'"/>
              <type name="uint32">
                <range value="100..600000"/>
              </type>
              <units name="ms"/>
              <default value="100"/>
              <description>
                <text>MLDP MBB Switch Delay Time.</text>
              </description>
            </leaf>
            <leaf name="mbb-wait-ack-time">
              <when condition="../make-before-break-enable='true'"/>
              <type name="uint32">
                <range value="1..600"/>
              </type>
              <units name="s"/>
              <default value="60"/>
              <description>
                <text>MLDP MBB ACK Notification Wait Time.</text>
              </description>
            </leaf>
            <leaf name="recursive-fec">
              <type name="boolean"/>
              <default value="false"/>
              <description>
                <text>Enable/disable Recursive FEC for MLDP.</text>
              </description>
            </leaf>
            <leaf name="p2mp-link-frr-enable">
              <type name="boolean"/>
              <default value="false"/>
              <description>
                <text>Enable/disable MLDP P2MP Frr Link Proteciton.</text>
              </description>
            </leaf>
            <leaf name="p2mp-best-effort">
