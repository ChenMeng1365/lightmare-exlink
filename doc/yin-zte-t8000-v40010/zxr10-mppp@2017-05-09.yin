<?xml version="1.0" encoding="UTF-8"?>
<module name="zxr10-mppp"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:mppp="http://www.zte.com.cn/zxr10/netconf/schema/rosng/mppp"
        xmlns:ncd="http://www.zte.com.cn/zxr10/netconf/schema/rosng/netconf-defs">
  <namespace uri="http://www.zte.com.cn/zxr10/netconf/schema/rosng/mppp"/>
  <prefix value="mppp"/>
  <yang-version value="1"/>
  <import module="zxr10-netconf-defs">
    <prefix value="ncd"/>
  </import>
  <organization>
    <text>ZTE Corporation</text>
  </organization>
  <contact>
    <text>ZTE Corporation  
   Nanjing Institute of ZTE Corporation 
   No.68 Zijinghua Rd. Yuhuatai District, Nanjing, China 
   Tel: +86-25-52870000</text>
  </contact>
  <description>
    <text>The netconf module to describe Multilink Point TO Point Protocol(PPP)</text>
  </description>
  <revision date="2017-05-09">
    <description>
      <text>Change the range of leaf epd(state/mppp/mppp-instance/mppp/epd) from 0-31 to 0-32.
Change the range of leaf epd(state/mppp/mppp-instance/mcc/epd) from 0-31 to 0-32.</text>
    </description>
  </revision>
  <revision date="2016-08-12">
    <description>
      <text>Add a container of epd(configuration/mppp/epd)
Add a container of mrru(configuration/mppp/mrru)
Add a feature of mppp-mrru</text>
    </description>
  </revision>
  <revision date="2015-03-17">
    <description>
      <text>Add a container of ssnhf(configuration/mppp/ssnhf)</text>
    </description>
  </revision>
  <revision date="2015-03-16">
    <description>
      <text>Add a container of neighborroute(configuration/mppp/neighborroute)
Chang some case of proper nouns,such as MPPP,MCC,MPLSCP</text>
    </description>
  </revision>
  <revision date="2014-02-20">
    <description>
      <text>The initial version</text>
    </description>
  </revision>
  <feature name="mppp-mrru">
    <description>
      <text>Set MPPP MRRU feature</text>
    </description>
  </feature>
  <container name="configuration">
    <description>
      <text>Describe the configuration of MPPP</text>
    </description>
    <container name="mppp">
      <description>
        <text>Configuration data of MPPP</text>
      </description>
      <container name="basic">
        <description>
          <text>MPPP basic correlation configuration</text>
        </description>
        <list name="fragment">
          <description>
            <text>Configure multilink fragmentation</text>
          </description>
          <key value="ifname"/>
          <leaf name="ifname">
            <description>
              <text>This value identifies the MPPP interface name.This attribute is created by Interface Management</text>
            </description>
            <type name="ncd:ifname"/>
          </leaf>
          <leaf name="flag">
            <description>
              <text>MPPP multilink fragmentation</text>
            </description>
            <type name="uint8">
              <range value="0..1">
                <description>
                  <text>0: no mppp multilink fragmentation
1: mppp multilink fragmentation</text>
                </description>
              </range>
            </type>
            <default value="0"/>
          </leaf>
        </list>
      </container>
      <container name="policy">
        <description>
          <text>MPPP policy correlation configuration</text>
        </description>
        <list name="loadbalance">
          <description>
            <text>Configure  MPPP load balance</text>
          </description>
          <key value="ifname"/>
          <leaf name="ifname">
            <description>
              <text>This value identifies the MPPP interface name.This attribute is created by Interface Management</text>
            </description>
            <type name="ncd:ifname"/>
          </leaf>
          <leaf name="flag">
            <description>
              <text>Configure  MPPP load balance</text>
            </description>
            <type name="enumeration">
              <enum name="per-destination">
                <value value="0"/>
                <description>
                  <text>MPPP load balance by destination</text>
                </description>
              </enum>
              <enum name="per-packet">
                <value value="1"/>
                <description>
                  <text>MPPP load balance by packet</text>
                </description>
              </enum>
            </type>
            <default value="per-destination"/>
          </leaf>
        </list>
      </container>
      <container name="mplscp">
        <description>
          <text>MPPP MPLSCP correlation configuration</text>
        </description>
        <list name="mplsenable">
          <description>
            <text>Configure MPLSCP parameters</text>
          </description>
          <key value="ifname"/>
          <leaf name="ifname">
            <description>
              <text>This value identifies the MPPP interface name.This attribute is created by Interface Management</text>
            </description>
            <type name="ncd:ifname"/>
          </leaf>
          <leaf name="enable">
            <description>
              <text>MPPP MPLSCP enable</text>
            </description>
            <type name="enumeration">
              <enum name="disable">
                <value value="0"/>
                <description>
                  <text>Disable MPLSCP</text>
                </description>
              </enum>
              <enum name="enable">
                <value value="1"/>
                <description>
                  <text>Enable MPLSCP</text>
                </description>
              </enum>
            </type>
            <default value="disable"/>
          </leaf>
        </list>
      </container>
      <container name="header">
        <description>
          <text>MPPP header correlation configuration</text>
        </description>
        <list name="headerenable">
          <description>
            <text>Configure MPPP header parameters</text>
          </description>
          <key value="ifname"/>
          <leaf name="ifname">
            <description>
              <text>This value identifies the MPPP interface name.This attribute is created by Interface Management</text>
            </description>
            <type name="ncd:ifname"/>
          </leaf>
          <leaf name="enable">
            <description>
              <text>MPPP header enable</text>
            </description>
            <type name="enumeration">
              <enum name="disable">
                <value value="0"/>
                <description>
                  <text>MPPP header disable</text>
                </description>
              </enum>
              <enum name="enable">
                <value value="1"/>
                <description>
                  <text>MPPP header enable</text>
                </description>
              </enum>
            </type>
            <default value="disable"/>
          </leaf>
        </list>
      </container>
      <container name="mrru">
        <description>
          <text>MPPP MRRU correlation configuration</text>
        </description>
        <list name="mpppmrru">
          <description>
            <text>Set MPPP MRRU parameters</text>
          </description>
          <key value="ifname"/>
          <leaf name="ifname">
            <description>
              <text>This value identifies the MPPP interface name.This attribute is created by Interface Management</text>
            </description>
            <type name="ncd:ifname"/>
          </leaf>
          <leaf name="value">
            <description>
              <text>Configure multilink MRRU value</text>
            </description>
            <type name="uint32">
              <range value="1..65535"/>
            </type>
            <default value="1500"/>
          </leaf>
        </list>
        <ncd:revise-date date="2016-08-12"/>
      </container>
      <container name="ssnhf">
        <description>
          <text>MPPP SSNHF(Short Sequence Number Header Format) correlation configuration</text>
        </description>
        <container name="ssnhfrequest">
          <description>
            <text>MPPP SSNHF request correlation configuration</text>
          </description>
          <list name="request">
            <description>
              <text>Set MPPP SSNHF request parameters</text>
            </description>
            <key value="ifname"/>
            <leaf name="ifname">
              <description>
                <text>This value identifies the MPPP interface name.This attribute is created by Interface Management</text>
              </description>
              <type name="ncd:ifname"/>
            </leaf>
            <leaf name="status">
              <description>
                <text>The enable status of MPPP SSNHF request</text>
              </description>
              <type name="enumeration">
                <enum name="disable">
                  <value value="0"/>
                  <description>
                    <text>Disable MPPP SSNHF request</text>
                  </description>
                </enum>
                <enum name="enable">
                  <value value="1"/>
                  <description>
                    <text>Enable MPPP SSNHF request</text>
                  </description>
                </enum>
              </type>
              <default value="disable"/>
            </leaf>
          </list>
        </container>
        <container name="ssnhfaccept">
          <description>
            <text>MPPP SSNHF accept correlation configuration</text>
          </description>
          <list name="accept">
            <description>
              <text>Set MPPP SSNHF accept parameters</text>
            </description>
            <key value="ifname"/>
            <leaf name="ifname">
              <description>
                <text>This value identifies the MPPP interface name.This attribute is created by Interface Management</text>
              </description>
              <type name="ncd:ifname"/>
            </leaf>
            <leaf name="status">
              <description>
                <text>The enable status of MPPP SSNHF accept</text>
              </description>
              <type name="enumeration">
                <enum name="disable">
                  <value value="0"/>
                  <description>
                    <text>Disable MPPP SSNHF accept</text>
                  </description>
                </enum>
                <enum name="enable">
                  <value value="1"/>
                  <description>
                    <text>Enable MPPP SSNHF accept</text>
                  </description>
                </enum>
              </type>
              <default value="disable"/>
            </leaf>
          </list>
        </container>
      </container>
      <container name="neighborroute">
        <description>
          <text>MPPP IPCP neighbor-route correlation configuration</text>
        </description>
        <list name="ipcpneighborroute">
          <description>
            <text>Set MPPP IPCP neighbor-route parameters</text>
          </description>
          <key value="ifname"/>
          <leaf name="ifname">
            <description>
              <text>This value identifies the MPPP interface name.This attribute is created by Interface Management</text>
            </description>
            <type name="ncd:ifname"/>
          </leaf>
          <leaf name="status">
            <description>
              <text>The enable status of MPPP IPCP neighbor-route</text>
            </description>
            <type name="enumeration">
              <enum name="enable">
                <value value="0"/>
                <description>
                  <text>Enable MPPP IPCP neighbor-route</text>
                </description>
              </enum>
              <enum name="disable">
                <value value="1"/>
                <description>
                  <text>Disable MPPP IPCP neighbor-route</text>
                </description>
              </enum>
            </type>
            <default value="enable"/>
          </leaf>
        </list>
      </container>
      <container name="epd">
        <description>
          <text>MPPP EPD(Endpoint Discriminator) correlation configuration</text>
        </description>
        <list name="epdstatus">
          <description>
            <text>Set MPPP EPD parameters</text>
          </description>
          <key value="ifname"/>
          <leaf name="ifname">
            <description>
              <text>This value identifies the MPPP interface name.This attribute is created by Interface Management</text>
            </description>
            <type name="ncd:ifname"/>
          </leaf>
          <leaf name="status">
            <description>
              <text>The enable status of MPPP EPD</text>
            </description>
            <type name="enumeration">
              <enum name="enable">
                <value value="0"/>
                <description>
                  <text>MPPP EPD enable</text>
                </description>
              </enum>
              <enum name="disable">
                <value value="1"/>
                <description>
                  <text>MPPP EPD disable</text>
                </description>
              </enum>
            </type>
            <default value="enable"/>
          </leaf>
        </list>
        <ncd:revise-date date="2016-08-12"/>
      </container>
    </container>
  </container>
  <container name="state">
    <config value="false"/>
    <description>
      <text>Describe the state of MPPP</text>
    </description>
    <container name="mppp">
      <description>
        <text>State data of MPPP</text>
      </description>
      <container name="mppp-instance">
        <description>
          <text>MPPP correlation state</text>
        </description>
        <list name="mppp">
          <description>
            <text>Show multilink PPP bundle information</text>
          </description>
          <key value="mpppid"/>
          <leaf name="mpppid">
            <description>
              <text>multilink group id</text>
            </description>
            <type name="uint32">
              <range value="1..256"/>
            </type>
          </leaf>
          <leaf name="mpppname">
            <description>
              <text>multilink  name</text>
            </description>
            <type name="string">
              <length value="0..31"/>
            </type>
            <mandatory value="true"/>
          </leaf>
          <leaf name="epd">
            <description>
              <text>Endpoint Discriminator</text>
            </description>
            <type name="string">
              <length value="0..32"/>
            </type>
            <mandatory value="true"/>
          </leaf>
          <leaf name="policy">
            <description>
              <text>Load Balance</text>
            </description>
            <type name="enumeration">
              <enum name="per-destination">
                <value value="0"/>
                <description>
                  <text>per-destination</text>
                </description>
              </enum>
              <enum name="per-packet">
                <value value="1"/>
                <description>
                  <text>per-packet</text>
                </description>
              </enum>
            </type>
            <mandatory value="true"/>
          </leaf>
          <leaf name="activenum">
            <description>
              <text>active member number</text>
            </description>
            <type name="uint8"/>
            <mandatory value="true"/>
          </leaf>
          <leaf name="inactivenum">
            <description>
              <text>inactive member number</text>
            </description>
            <type name="uint8"/>
            <mandatory value="true"/>
          </leaf>
          <leaf name="membername">
            <description>
              <text>member interface name</text>
            </description>
            <type name="string">
              <length value="1..31"/>
            </type>
            <mandatory value="true"/>
          </leaf>
          <leaf name="memberstatus">
            <description>
              <text>member protocol status</text>
            </description>
            <type name="string">
              <length value="1..1024"/>
            </type>
            <mandatory value="true"/>
          </leaf>
        </list>
        <list name="mcc">
          <description>
            <text>Show MCC PPP bundle information</text>
          </description>
          <key value="mccid"/>
          <leaf name="mccid">
            <description>
              <text>MCC group id</text>
            </description>
            <type name="uint32">
              <range value="1..256"/>
            </type>
          </leaf>
          <leaf name="mccname">
            <description>
              <text>multilink  name</text>
            </description>
            <type name="string">
              <length value="0..31"/>
            </type>
            <mandatory value="true"/>
          </leaf>
          <leaf name="epd">
            <description>
              <text>Endpoint Discriminator</text>
            </description>
            <type name="string">
              <length value="0..32"/>
            </type>
            <mandatory value="true"/>
          </leaf>
          <leaf name="policy">
            <description>
              <text>Load Balance</text>
            </description>
            <type name="enumeration">
              <enum name="per-destination">
                <value value="0"/>
                <description>
                  <text>per-destination</text>
                </description>
              </enum>
              <enum name="per-packet">
                <value value="1"/>
                <description>
                  <text>per-packet</text>
                </description>
              </enum>
            </type>
            <mandatory value="true"/>
          </leaf>
          <leaf name="activenum">
            <description>
              <text>active member number</text>
            </description>
            <type name="uint8"/>
            <mandatory value="true"/>
          </leaf>
          <leaf name="inactivenum">
            <description>
              <text>inactive member number</text>
            </description>
            <type name="uint8"/>
            <mandatory value="true"/>
          </leaf>
          <leaf name="membername">
            <description>
              <text>member interface name</text>
            </description>
            <type name="string">
              <length value="1..31"/>
            </type>
            <mandatory value="true"/>
          </leaf>
          <leaf name="memberstatus">
            <description>
              <text>member protocol status</text>
            </description>
            <type name="string">
              <length value="0..31"/>
            </type>
            <mandatory value="true"/>
          </leaf>
        </list>
      </container>
    </container>
  </container>
</module>
