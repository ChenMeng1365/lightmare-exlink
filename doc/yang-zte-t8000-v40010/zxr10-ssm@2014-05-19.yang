  container configuration {
    description 'The configuration of SSM.' ;
    container ssm {
      description 'The node of SSM.' ;
      container global {
        description 'The node of SSM global configuration.' ;
        leaf usessm {
          description 'The switch of SSM algorithm for clock selection,enable/d'
                    + 'isable.'
                      ;
          type enumeration {
            enum enable {
              value 1 ;
              description 'Enable the SSM algorithm.' ;
            }
            enum disable {
              value 2 ;
              description 'Disable the SSM algorithm.' ;
            }
          }
          default disable ;
        }
        leaf ssm-mode {
          description 'The synchronization clock selection algorithm,ssm/ex-ssm'
                    + '.'
                      ;
          type enumeration {
            enum ssm {
              value 1 ;
              description 'Standard SSM algorithm of ITU.' ;
            }
            enum expssm {
              value 2 ;
              description 'Expand SSM algorithm of ZTE.' ;
            }
          }
          default ssm ;
        }
        leaf station {
          description 'The switch of NE station,yes/no.' ;
          type enumeration {
            enum no {
              value 0 ;
              description 'Set NE station.' ;
            }
            enum yes {
              value 1 ;
              description 'Cancel NE station.' ;
            }
          }
          default no ;
        }
        leaf station-id {
          description 'ID of NE station,the range is 1-7.' ;
          type uint8 {
            range 1..7 ;
          }
          default 7 ;
        }
        leaf id-protect {
          description 'The switch of ID protection,open/close.' ;
          type enumeration {
            enum open {
              value 1 ;
              description 'Open the ID protection.' ;
            }
            enum close {
              value 2 ;
              description 'Close the ID protection.' ;
            }
          }
          default close ;
        }
        leaf quality-unknown {
          description 'The switch of QL unknown mode,open/close.' ;
          type enumeration {
            enum open {
              value 1 ;
              description 'Open QL unknown.' ;
            }
            enum close {
              value 2 ;
              description 'Close QL unknown.' ;
            }
          }
          default close ;
        }
        leaf sec-quality {
          description 'The QL of inner clock.' ;
          type enumeration {
            enum QL-UNK {
              value 0 ;
              description 'The QL is unknown.' ;
            }
            enum QL-PRC {
              value 2 ;
              description 'The QL is PRC.' ;
            }
            enum QL-SSU-A {
              value 4 ;
              description 'The QL is SSU-A.' ;
            }
            enum QL-SSU-B {
              value 8 ;
              description 'The QL is SSU-B.' ;
            }
            enum QL-SEC {
              value 11 ;
              description 'The QL is SEC.' ;
            }
          }
          default QL-SEC ;
        }
        leaf wtr-time {
          description 'The wait time to restore.the range is 0-12 minutes.' ;
          type uint8 {
            range 0..12 ;
          }
          units minute ;
          default 5 ;
        }
        leaf quality-limit {
          description 'The switch of local clock QL limit,enable/disable.' ;
          type enumeration {
            enum enable {
              value 1 ;
              description 'Enable the QL limit.' ;
            }
            enum disable {
              value 2 ;
              description 'Disable the QL limit.' ;
            }
          }
          default disable ;
        }
        leaf lock-bits-send-dnu {
          description 'The switch of sending DNU when system locks bits clock,e'
                    + 'nable/disable.'
                      ;
          type enumeration {
            enum enable {
              value 1 ;
              description 'Enable switch.' ;
            }
            enum disable {
              value 2 ;
              description 'Disable switch.' ;
            }
          }
          default disable ;
        }
      }
      container sync-clocks {
        description 'The node of synchronization clock configuration.' ;
        list sync-clock {
          description 'The synchronization clock list.' ;
          key 'if-name clock-type' ;
          leaf if-name {
            description 'The interface name of clock,inner-clk or PTP1588-clk i'
                      + 's NULL.'
                        ;
            type string {
              length 0..31 ;
            }
          }
          leaf clock-type {
            description 'The clock type.' ;
            type enumeration {
              enum external-clk {
                value 1 ;
                description 'External clock.' ;
              }
              enum line-clk {
                value 2 ;
                description 'Line clock.' ;
              }
              enum tributeline-clk {
                value 5 ;
                description 'Tribute line clock.' ;
              }
              enum gps-clk {
                value 7 ;
                description 'GPS clock.' ;
              }
              enum stm-clk {
                value 8 ;
                description 'Stm-n clock.' ;
              }
            }
          }
          leaf priority {
            description 'The clock priority,the range is 1-255.' ;
            type uint8 {
              range 1..255 {
                description 'The value of DIS is 255.' ;
              }
            }
            default 255 ;
          }
          leaf rcv-quality-mode {
            description 'The mode of receive clock QL,auto/manual.' ;
            type enumeration {
              enum auto {
                value 1 ;
                description 'Automatically receive SSM mode.' ;
              }
              enum manual {
                value 2 ;
                description 'Manually receive SSM mode.' ;
              }
            }
            default auto ;
          }
          leaf manual-rcv-quality {
            description 'The value of receive clock QL by manual.' ;
            type enumeration {
              enum QL-UNK {
                value 0 ;
                description 'The QL is unknown.' ;
              }
              enum QL-PRC {
                value 2 ;
                description 'The QL is PRC.' ;
              }
              enum QL-SSU-A {
                value 4 ;
                description 'The QL is SSU-A.' ;
              }
              enum QL-SSU-B {
                value 8 ;
                description 'The QL is SSU-B.' ;
              }
              enum QL-SEC {
                value 11 ;
                description 'The QL is SEC.' ;
              }
              enum QL-DNU {
                value 15 ;
                description 'The QL is DNU.' ;
              }
            }
            default QL-DNU ;
          }
          leaf frame-mode {
            description 'The frame mode for bits interface,unframe/frame.' ;
            type enumeration {
              enum frame {
                value 1 ;
                description 'Frame mode.' ;
              }
              enum unframe {
                value 2 ;
                description 'Unframe mode.' ;
              }
              enum invaild {
                value 254 ;
                description 'Property is invaild.' ;
              }
            }
            default invaild ;
          }
        }
        list sync-unif-clock {
          description 'The synchronization clock list.' ;
          key clock-type ;
          leaf clock-type {
            description 'The clock type.' ;
            type enumeration {
              enum inner-clk {
                value 3 ;
                description 'Internal clock.' ;
              }
              enum PTP1588-clk {
                value 6 ;
                description 'PTP clock.' ;
              }
            }
          }
          leaf priority {
            description 'The clock priority,the range is 1-255.' ;
            type uint8 {
              range 1..255 {
                description 'The value of DIS is 255.' ;
              }
            }
            default 255 ;
          }
          leaf rcv-quality-mode {
            description 'The mode of receive clock QL,auto/manual.' ;
            type enumeration {
              enum auto {
                value 1 ;
                description 'Automatically receive SSM mode' ;
              }
              enum manual {
                value 2 ;
                description 'Manually receive SSM mode' ;
              }
            }
            default manual ;
          }
          leaf manual-rcv-quality {
            description 'The value of receive clock QL by manual.' ;
            type enumeration {
              enum QL-UNK {
                value 0 ;
                description 'The QL is unknown.' ;
              }
              enum QL-PRC {
                value 2 ;
                description 'The QL is PRC.' ;
              }
              enum QL-SSU-A {
                value 4 ;
                description 'The QL is SSU-A.' ;
              }
              enum QL-SSU-B {
                value 8 ;
                description 'The QL is SSU-B.' ;
              }
              enum QL-SEC {
                value 11 ;
                description 'The QL is SEC.' ;
              }
              enum QL-DNU {
                value 15 ;
                description 'The QL is DNU.' ;
              }
            }
            default QL-DNU ;
          }
          leaf frame-mode {
            description 'The frame mode for bits interface,unframe/frame.' ;
            type enumeration {
              enum frame {
                value 1 ;
                description 'Frame mode.' ;
              }
              enum unframe {
                value 2 ;
                description 'Unframe mode.' ;
              }
              enum invaild {
                value 254 ;
                description 'Property is invaild.' ;
              }
            }
            default invaild ;
          }
        }
        list sync-external-cmd {
          description 'The external commands for synchronization clock.' ;
          key 'if-name clock-type' ;
          leaf if-name {
            description 'The interface name of clock,inner-clk or PTP1588-clk i'
                      + 's NULL.'
                        ;
            type string {
              length 0..31 ;
            }
          }
          leaf clock-type {
            description 'The clock type.' ;
            type enumeration {
              enum external-clk {
                value 1 ;
                description 'External clock.' ;
              }
              enum line-clk {
                value 2 ;
                description 'Line clock.' ;
              }
              enum tributeline-clk {
                value 5 ;
                description 'Tribute line clock.' ;
              }
              enum gps-clk {
                value 7 ;
                description 'GPS clock.' ;
              }
              enum stm-clk {
                value 8 ;
                description 'Stm-n clock.' ;
              }
            }
          }
          leaf lock-out {
            description 'The lockout state of synchronization clock,on/off.' ;
            type enumeration {
              enum on {
                value 1 ;
                description 'Open lockout.' ;
              }
              enum off {
                value 2 ;
                description 'Close lockout.' ;
              }
            }
            default off ;
          }
          leaf switch-mode {
            description 'The switch mode of synchronization clock:normal/force/'
                      + 'manual,there is only one synchronization clock in forc'
                      + 'e/manual switch mode.'
                        ;
            type enumeration {
              enum normal {
                value 1 ;
                description 'Normal mode.' ;
              }
              enum force {
                value 2 ;
                description 'Force mode.' ;
              }
              enum manual {
                value 3 ;
                description 'Manual mode.' ;
              }
            }
            default normal ;
          }
        }
        list sync-unif-external-cmd {
          description 'The external commands for synchronization clock.' ;
          key clock-type ;
          leaf clock-type {
            description 'The clock type.' ;
            type enumeration {
              enum inner-clk {
                value 3 ;
                description 'Internal clock.' ;
              }
              enum PTP1588-clk {
                value 6 ;
                description 'PTP clock.' ;
              }
            }
          }
          leaf lock-out {
            description 'The lockout state of synchronization clock,on/off.' ;
            type enumeration {
              enum on {
                value 1 ;
                description 'Open lockout.' ;
              }
              enum off {
                value 2 ;
                description 'Close lockout.' ;
              }
            }
            default off ;
          }
          leaf switch-mode {
            description 'The switch mode of synchronization clock:normal/force/'
                      + 'manual,there is only one synchronization clock in forc'
                      + 'e/manual switch mode.'
                        ;
            type enumeration {
              enum normal {
                value 1 ;
                description 'Normal mode.' ;
              }
              enum force {
                value 2 ;
                description 'Force mode.' ;
              }
              enum manual {
                value 3 ;
                description 'Manual mode.' ;
              }
            }
            default normal ;
          }
        }
      }
      container extimer-clocks {
        description 'The node of extimer clock configuration.' ;
        list out-clock {
          description 'The configuration of out clock.' ;
          key if-name ;
          leaf if-name {
            description 'The extimer clock interface name.' ;
            type string {
              length 0..31 ;
            }
          }
          leaf if-attribute {
            description 'The port attribute.' ;
            type enumeration {
              enum 2Mbits {
                value 1 ;
                description '2Mbits of port attribute.' ;
              }
              enum 2Mhz {
                value 2 ;
                description '2Mhz of port attribute.' ;
              }
            }
            default 2Mbits ;
          }
          leaf quality-limit {
            description 'The minimum limit qulaity of out clock port.' ;
            type enumeration {
              enum QL-UNK {
                value 0 ;
                description 'The QL is unknown.' ;
              }
              enum QL-PRC {
                value 2 ;
                description 'The QL is PRC.' ;
              }
              enum QL-SSU-A {
                value 4 ;
                description 'The QL is SSU-A.' ;
              }
              enum QL-SSU-B {
                value 8 ;
                description 'The QL is SSU-B.' ;
              }
              enum QL-SEC {
                value 11 ;
                description 'The QL is SEC.' ;
              }
            }
            default QL-SEC ;
          }
          leaf out-mode {
            description 'The selection algorithm of out clock port:priority/qua'
                      + 'lity.'
                        ;
            type enumeration {
              enum priority {
                value 1 ;
                description 'According to priority' ;
              }
              enum quality {
                value 2 ;
                description 'According to quality-level' ;
              }
            }
            default priority ;
          }
          list out-clock-list {
            description 'The out clock source configuration list.' ;
            key 'bind-if-name clock-type' ;
            leaf bind-if-name {
              description 'The interface name of out clock source.' ;
              type string {
                length 0..31 ;
              }
            }
            leaf clock-type {
              description 'The clock type of out clock source.' ;
              type enumeration {
                enum external-clk {
                  value 1 ;
                  description 'External clock.' ;
                }
                enum line-clk {
                  value 2 ;
                  description 'Line clock.' ;
                }
                enum tributeline-clk {
                  value 5 ;
                  description 'Tribute line clock.' ;
                }
                enum gps-clk {
                  value 7 ;
                  description 'GPS clock.' ;
                }
                enum stm-clk {
                  value 8 ;
                  description 'Stm-n clock.' ;
                }
              }
            }
            leaf priority {
              description 'The clock priority,the range is 1-255.' ;
              type uint8 {
                range 1..255 ;
              }
              default 255 ;
            }
          }
          list out-clock-unif-list {
            description 'The out clock source configuration list.' ;
            key clock-type ;
            leaf clock-type {
              description 'The clock type of out clock source.' ;
              type enumeration {
                enum PTP1588-clk {
                  value 6 ;
                  description 'PTP clock.' ;
                }
                enum current-clock {
                  value 19 ;
                  description 'Current system clock.' ;
                }
              }
            }
            leaf priority {
              description 'The clock priority,the range is 1-255.' ;
              type uint8 {
                range 1..255 ;
              }
              default 255 ;
            }
          }
        }
      }
      container sa-position {
        description sa-position. ;
        list sa-position-list {
          description 'The configuration of sa position.' ;
          key if-name ;
          leaf if-name {
            description 'The extimer clock interface name.' ;
            type string {
              length 0..31 ;
            }
          }
          leaf sa-receive-position {
            description 'The sa position of receiving SSM,the range is 4-8.' ;
            type uint8 {
              range 4..8 ;
            }
            default 4 ;
          }
          leaf sa-transmit-position {
            description 'The sa position of transmitting SSM,the range is 4-8.' ;
            type uint8 {
              range 4..8 ;
            }
            default 4 ;
          }
        }
      }
      container ssm-transmit {
        description 'The node of clock transmit configuration.' ;
        list transmit-disable {
          description 'The ethernet port list that is disabled to transmit SSM.' ;
          key 'if-name clock-type' ;
          leaf if-name {
            description 'The interface name of clock.' ;
            type string {
              length 0..31 ;
            }
          }
          leaf clock-type {
            description 'The clock type.' ;
            type enumeration {
              enum line-clk {
                value 2 ;
                description 'Line clock.' ;
              }
            }
          }
        }
        list trans-ssm {
          description 'The list of setting transmit clock QL.' ;
          key 'if-name clock-type' ;
          leaf if-name {
            description 'The interface name of clock.' ;
            type string {
              length 0..31 ;
            }
          }
          leaf clock-type {
            description 'The clock type.' ;
            type enumeration {
              enum external-clk {
                value 1 ;
                description 'External clock.' ;
              }
              enum line-clk {
                value 2 ;
                description 'Line clock.' ;
              }
              enum stm-clk {
                value 8 ;
                description 'Stm-n clock.' ;
              }
            }
          }
          leaf transmit-quality {
            description 'The value of transmitting clock QL by manual.' ;
            type enumeration {
              enum QL-UNK {
                value 0 ;
                description 'The QL is unknown.' ;
              }
              enum QL-PRC {
                value 2 ;
                description 'The QL is PRC.' ;
              }
              enum QL-SSU-A {
                value 4 ;
                description 'The QL is SSU-A.' ;
              }
              enum QL-SSU-B {
                value 8 ;
                description 'The QL is SSU-B.' ;
              }
              enum QL-SEC {
                value 11 ;
                description 'The QL is SEC.' ;
              }
              enum QL-DNU {
                value 15 ;
                description 'The QL is DNU.' ;
              }
            }
            default QL-DNU ;
          }
        }
      }
      container edge-link {
        description 'The edge compatibility of clock port.' ;
        list edgelink {
          description 'The edge link clock list.' ;
          key 'if-name clock-type' ;
          leaf if-name {
            description 'The interface name of clock.' ;
            type string {
              length 0..31 ;
            }
          }
          leaf clock-type {
            description 'The clock type.' ;
            type enumeration {
              enum line-clk {
                value 2 ;
                description 'Line clock.' ;
              }
              enum stm-clk {
                value 8 ;
                description 'Stm-n clock.' ;
              }
            }
          }
        }
      }
    }
  }
  container state {
    description 'The state of SSM.' ;
    container ssm {
      description 'The node of SSM.' ;
      container current-clock {
        description 'SSM current system clock information.' ;
        leaf if-name {
          description 'SSM current interface name of clock,inner-clk,hold-off o'
                    + 'r PTP1588-clk is NULL.'
                      ;
          type string {
            length 0..31 ;
          }
        }
        leaf clock-type {
          description 'SSM current clock type.' ;
          type enumeration {
            enum external-clk {
              value 1 ;
              description 'External clock.' ;
            }
            enum line-clk {
              value 2 ;
              description 'Line clock.' ;
            }
            enum inner-clk {
