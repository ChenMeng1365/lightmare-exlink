              <leaf name="zero-holdtime-num">
                <type name="uint32"/>
                <description>
                  <text>Number of LSPs with the lifetime zero.</text>
                </description>
              </leaf>
              <leaf name="zero-frag-holdtime-num">
                <type name="uint32"/>
                <description>
                  <text>Number of non-pseudo node LSPs with the fragment number zero and lifetime zero.</text>
                </description>
              </leaf>
              <leaf name="zero-sequence-num">
                <type name="uint32"/>
                <description>
                  <text>Number of LSPs with the sequence number zero.</text>
                </description>
              </leaf>
