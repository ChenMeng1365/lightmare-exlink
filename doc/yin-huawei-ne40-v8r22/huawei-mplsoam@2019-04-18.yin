          <leaf name="oam-active-status">
            <type name="active-type"/>
            <default value="deactive"/>
            <description>
              <text>Whether MPLS OAM detection is enabled.</text>
            </description>
          </leaf>
          <leaf name="two-dm-send-enable">
            <type name="boolean"/>
            <default value="false"/>
            <description>
              <text>Enable/disable a device sends packets for proactive two-way delay measurement.</text>
            </description>
          </leaf>
          <leaf name="two-dm-packet-size">
            <when condition="../two-dm-send-enable='true'"/>
            <type name="uint32">
              <range value="64..1500"/>
            </type>
            <default value="64"/>
            <description>
              <text>Configure the packet size of proactive two-way delay measurement.</text>
            </description>
          </leaf>
          <leaf name="two-dm-pad-value">
            <when condition="../two-dm-send-enable='true'"/>
            <type name="padding-value"/>
            <default value="0"/>
            <description>
              <text>Configure the padding value of proactive two-way delay measurement.</text>
            </description>
          </leaf>
          <leaf name="two-dm-exp">
            <when condition="../two-dm-send-enable='true'"/>
            <type name="uint8">
              <range value="0..7"/>
            </type>
            <default value="7"/>
            <description>
              <text>Configure the exp value of proactive two-way delay measurement.</text>
            </description>
          </leaf>
          <leaf name="description">
            <type name="string">
              <length value="0..64"/>
            </type>
            <config value="false"/>
            <description>
              <text>Description of a tunnel.</text>
            </description>
          </leaf>
          <leaf name="lsp-type">
            <type name="lsp-establish-type"/>
            <config value="false"/>
            <description>
              <text>LSP type.</text>
            </description>
          </leaf>
          <uses name="session-status"/>
        </list>
      </container>
    </container>
    <container name="vll-pws">
      <when condition="../global/oam-enable='true'"/>
      <description>
        <text>List of MPLS OAM monitors VLL PWs.</text>
      </description>
      <list name="vll-pw">
        <must condition="(detect-mode='auto' and send-enable='true' and receive-enable='true') or detect-mode='manual'"/>
        <key value="l2vpn-name l2vpn-type l2vpn-role"/>
        <description>
          <text>Configure MPLS OAM to monitor a VLL PW.</text>
        </description>
        <leaf name="l2vpn-name">
          <type name="leafref">
            <path value="/l2vpn:l2vpn/l2vpn:instances/l2vpn:instance/l2vpn:name"/>
          </type>
          <description>
            <text>Configure L2VPN name of MPLS OAM binded static PW.</text>
          </description>
        </leaf>
        <leaf name="l2vpn-type">
          <type name="leafref">
            <path value="/l2vpn:l2vpn/l2vpn:instances/l2vpn:instance[l2vpn:name=current()/../l2vpn-name]/l2vpn:type"/>
          </type>
          <description>
            <text>Configure L2VPN type of MPLS OAM binded static PW.</text>
          </description>
        </leaf>
        <leaf name="l2vpn-role">
          <type name="leafref">
            <path value="/l2vpn:l2vpn/l2vpn:instances/l2vpn:instance[l2vpn:name = current()/../l2vpn-name][l2vpn:type = current()/../l2vpn-type]/l2vpn:vpws-ldp/l2vpn:pws/l2vpn:pw/l2vpn:role"/>
          </type>
          <description>
            <text>Configure PW role of MPLS OAM binded static PW.</text>
          </description>
        </leaf>
        <uses name="l2vpn-pw"/>
        <uses name="session-status"/>
      </list>
    </container>
    <container name="vsi-pws">
      <when condition="../global/oam-enable='true'"/>
      <description>
        <text>List of MPLS OAM monitors VSI PWs.</text>
      </description>
      <list name="vsi-pw">
        <must condition="(detect-mode='auto' and send-enable='true' and receive-enable='true') or detect-mode='manual'"/>
        <key value="peer-ip vc-id vc-type"/>
        <description>
          <text>Configure MPLS OAM to monitor a VSI PW.</text>
        </description>
        <leaf name="peer-ip">
          <type name="leafref">
            <path value="/l2vpn:l2vpn/l2vpn:instances/l2vpn:instance/l2vpn:vpls/l2vpn:ldp-signaling/l2vpn:pws/l2vpn:pw/l2vpn:peer-ip"/>
          </type>
          <description>
            <text>Configure peer IP of MPLS OAM binded static PW.</text>
          </description>
        </leaf>
        <leaf name="vc-id">
          <type name="leafref">
            <path value="/l2vpn:l2vpn/l2vpn:instances/l2vpn:instance/l2vpn:vpls/l2vpn:ldp-signaling/l2vpn:pws/l2vpn:pw[l2vpn:peer-ip=current()/../peer-ip]/l2vpn:negotiation-vc-id"/>
          </type>
          <description>
            <text>Configure VC ID of MPLS OAM binded static PW.</text>
          </description>
        </leaf>
        <leaf name="vc-type">
          <type name="vpls-pw-encap-type"/>
          <description>
            <text>Configure remote VC type of MPLS OAM binded static PW.</text>
          </description>
        </leaf>
        <uses name="l2vpn-pw">
          <refine target-node="remote-lsr-id">
            <ext:dynamic-default>
              <ext:default-value value="../peer-ip"/>
            </ext:dynamic-default>
            <description>
              <text>Configure a remote LSR ID.</text>
            </description>
          </refine>
          <refine target-node="remote-vc-id">
            <ext:dynamic-default>
              <ext:default-value value="../vc-id"/>
            </ext:dynamic-default>
            <description>
              <text>Configure a remote VC ID of a static PW to which MPLS OAM is bound.</text>
            </description>
          </refine>
          <refine target-node="remote-vc-type">
            <ext:dynamic-default>
              <ext:default-value value="../vc-type"/>
            </ext:dynamic-default>
            <description>
              <text>Configure a remote VC type of a static PW to which MPLS OAM is bound.</text>
            </description>
          </refine>
        </uses>
        <uses name="session-status"/>
      </list>
    </container>
  </container>
