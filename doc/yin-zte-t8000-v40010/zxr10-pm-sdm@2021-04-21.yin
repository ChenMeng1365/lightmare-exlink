            <text>Software number.</text>
          </description>
          <type name="uint32"/>
        </leaf>
        <list name="software-information">
          <key value="name"/>
          <leaf name="name">
            <description>
              <text>Software version name.</text>
            </description>
            <type name="string"/>
          </leaf>
          <leaf name="status">
            <description>
              <text>Software status.</text>
            </description>
            <type name="enumeration">
              <enum name="ADD">
                <value value="1"/>
              </enum>
              <enum name="ACTIVATED">
                <value value="2"/>
              </enum>
              <enum name="COMMITED">
                <value value="3"/>
              </enum>
            </type>
          </leaf>
          <leaf name="source">
            <description>
              <text>Source software filename.</text>
            </description>
            <type name="string"/>
          </leaf>
          <leaf name="source-type">
            <description>
              <text>Source type.</text>
            </description>
            <type name="enumeration">
              <enum name="COMMON">
                <value value="0"/>
              </enum>
              <enum name="BASE">
                <value value="1"/>
              </enum>
              <enum name="FIRMWARE">
                <value value="2"/>
              </enum>
              <enum name="PATCH">
                <value value="3"/>
              </enum>
              <enum name="SERVICE">
                <value value="4"/>
              </enum>
              <enum name="COMPLETE">
                <value value="5"/>
              </enum>
              <enum name="INCREMENT">
                <value value="6"/>
              </enum>
              <enum name="PATCHPKG">
                <value value="7"/>
              </enum>
              <enum name="PATCHPKG_ALL">
                <value value="8"/>
              </enum>
              <enum name="IMAGE">
                <value value="9"/>
              </enum>
              <enum name="BLUEPRINT">
                <value value="10"/>
              </enum>
              <enum name="PATCHPKG_COMPOSITE">
                <value value="11"/>
              </enum>
              <enum name="PTACH_PNP">
                <value value="12"/>
              </enum>
              <enum name="COMPONENT">
                <value value="13"/>
              </enum>
            </type>
            <ncd:revise-date date="2021-04-21"/>
          </leaf>
        </list>
      </container>
