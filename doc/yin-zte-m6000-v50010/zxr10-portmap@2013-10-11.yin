<?xml version="1.0" encoding="UTF-8"?>
<module name="zxr10-portmap"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:portmap="http://www.zte.com.cn/zxr10/netconf/schema/rosng/portmap"
        xmlns:ncd="http://www.zte.com.cn/zxr10/netconf/schema/rosng/netconf-defs">
  <namespace uri="http://www.zte.com.cn/zxr10/netconf/schema/rosng/portmap"/>
  <prefix value="portmap"/>
  <import module="zxr10-netconf-defs">
    <prefix value="ncd"/>
  </import>
  <organization>
    <text>ZTE Corporation</text>
  </organization>
  <contact>
    <text>ZTE Corporation

   Nanjing Institute of ZTE Corporation

   No.68 Zijinghua Rd. Yuhuatai District, Nanjing, China

   Tel: +86-25-52870000</text>
  </contact>
  <description>
    <text>The netconf module to describe the Interface Relation.Like the mapping relation between pos interface and ulei interface.</text>
  </description>
  <revision date="2013-10-11">
    <description>
      <text>The initial version</text>
    </description>
  </revision>
  <container name="configuration">
    <description>
      <text>Configure the mapping relation between interfaces</text>
    </description>
    <container name="portmap">
      <description>
        <text>Describe the parameters of interface mapping relation to be configured</text>
      </description>
      <container name="portmap-configuration">
        <description>
          <text>Describe the parameters of interface mapping relation to be configured</text>
        </description>
        <list name="interface">
          <description>
            <text>The list of interfaces for configuring parameters</text>
          </description>
          <key value="src-interface"/>
          <leaf name="src-interface">
            <description>
              <text>Source interface name</text>
            </description>
            <type name="ncd:ifname"/>
          </leaf>
          <leaf name="dst-interface">
            <description>
              <text>Destination interface name</text>
            </description>
            <type name="ncd:ifname"/>
            <mandatory value="true"/>
          </leaf>
        </list>
      </container>
    </container>
  </container>
</module>
