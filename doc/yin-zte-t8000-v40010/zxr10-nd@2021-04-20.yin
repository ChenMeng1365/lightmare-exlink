  <container name="configuration">
    <description>
      <text>Configuration model info.</text>
    </description>
    <container name="nd">
      <description>
        <text>Neighbor Discovery Protocol configuration.</text>
      </description>
      <container name="ipv6-nd-attribute">
        <description>
          <text>Configure IPv6 ND attributes.</text>
        </description>
        <list name="nd-attribute">
          <description>
            <text>Configure ND cache entry attributes.</text>
          </description>
          <key value="ifname"/>
          <leaf name="ifname">
            <description>
              <text>The name of the interface.</text>
            </description>
            <type name="ncd:ifname"/>
          </leaf>
          <leaf name="dad-attempts">
            <description>
              <text>Configure the DAD attempts number. Its default value which is between 1-10 and generally 3, depends on product specification.</text>
            </description>
            <type name="uint8">
              <range value="0..10"/>
            </type>
          </leaf>
          <leaf name="man-cfg-flag">
            <description>
              <text>The flag denotes the hosts should use DHCP(Dynamic Host Configuration Protocol) for address configuration.</text>
            </description>
            <type name="enumeration">
              <enum name="reset">
                <value value="1"/>
                <description>
                  <text>Reset the flag.</text>
                </description>
              </enum>
              <enum name="set">
                <value value="2"/>
                <description>
                  <text>Set the flag.</text>
                </description>
              </enum>
            </type>
            <default value="reset"/>
          </leaf>
          <leaf name="other-cfg-flag">
            <description>
              <text>The flag denotes the hosts should use DHCP for configuration except for address.</text>
            </description>
            <type name="enumeration">
              <enum name="reset">
                <value value="1"/>
                <description>
                  <text>Reset the flag.</text>
                </description>
              </enum>
              <enum name="set">
                <value value="2"/>
                <description>
                  <text>Set the flag.</text>
                </description>
              </enum>
            </type>
            <default value="reset"/>
          </leaf>
          <leaf name="nud-switch">
            <description>
              <text>IPv6 ND NUD(Neighbour Unreachable Detect) switch.</text>
            </description>
            <type name="enumeration">
              <enum name="off">
                <value value="1"/>
                <description>
                  <text>Turn off the switch.</text>
                </description>
              </enum>
              <enum name="on">
                <value value="2"/>
                <description>
                  <text>Turn on the switch.</text>
                </description>
              </enum>
            </type>
            <default value="on"/>
          </leaf>
          <leaf name="stale-switch">
            <description>
              <text>IPv6 ND stale switch.</text>
            </description>
            <type name="enumeration">
              <enum name="off">
                <value value="1"/>
                <description>
                  <text>Turn off the switch.</text>
                </description>
              </enum>
              <enum name="on">
                <value value="2"/>
                <description>
                  <text>Turn on the switch.</text>
                </description>
              </enum>
            </type>
            <default value="off"/>
          </leaf>
          <leaf name="ra-curhoplimit">
            <description>
              <text>Set IPv6 router advertisement current hoplimit.</text>
            </description>
            <type name="uint8">
              <range value="0..255"/>
            </type>
            <default value="64"/>
          </leaf>
          <leaf name="ra-interval">
            <description>
              <text>Set IPv6 router advertisement interval(second).</text>
            </description>
            <type name="uint16">
              <range value="3..1800"/>
            </type>
            <units name="second"/>
            <default value="600"/>
          </leaf>
          <leaf name="ra-lifetime">
            <description>
              <text>Set IPv6 router advertisement lifetime(second).</text>
            </description>
            <type name="uint16">
              <range value="0..9000"/>
            </type>
            <units name="second"/>
            <default value="1800"/>
          </leaf>
          <leaf name="na-parse">
            <description>
              <text>Parse IPv6 multicast neighbor advertisements.</text>
            </description>
            <type name="enumeration">
              <enum name="off">
                <value value="1"/>
                <description>
                  <text>Turn off the switch.</text>
                </description>
              </enum>
              <enum name="on">
                <value value="2"/>
                <description>
                  <text>Turn on the switch.</text>
                </description>
              </enum>
            </type>
            <default value="off"/>
          </leaf>
          <leaf name="staled-time">
            <description>
              <text>IPv6 ND entry staled time(minute).</text>
            </description>
            <type name="uint32">
              <range value="1..14400"/>
            </type>
            <units name="minute"/>
          </leaf>
          <leaf name="redirect">
            <description>
              <text>Control IPv6 router ND redirect.</text>
            </description>
            <type name="enumeration">
              <enum name="off">
                <value value="1"/>
                <description>
                  <text>Turn off the switch.</text>
                </description>
              </enum>
              <enum name="on">
                <value value="2"/>
                <description>
                  <text>Turn on the switch.</text>
                </description>
              </enum>
            </type>
            <default value="off"/>
          </leaf>
          <leaf name="ra-linkmtu">
            <description>
              <text>Set IPv6 router advertisement linkmtu.Its maximum which is between 1500-9586 and generally 1500, depends on pruduct specification.</text>
            </description>
            <type name="uint16">
              <range value="0..9586"/>
            </type>
            <units name="byte"/>
            <default value="1500"/>
          </leaf>
          <leaf name="proxy-switch">
            <description>
              <text>The ND proxy switch between the interfaces.</text>
            </description>
            <type name="enumeration">
              <enum name="off">
                <value value="1"/>
                <description>
                  <text>Turn off the switch.</text>
                </description>
              </enum>
              <enum name="on">
                <value value="2"/>
                <description>
                  <text>Turn on the switch.</text>
                </description>
              </enum>
            </type>
            <default value="off"/>
          </leaf>
          <leaf name="innervlan-proxy-switch">
            <description>
              <text>The ND proxy switch in the same vlan of the interface.</text>
            </description>
            <type name="enumeration">
              <enum name="off">
                <value value="1"/>
                <description>
                  <text>Turn off the switch.</text>
                </description>
              </enum>
              <enum name="on">
                <value value="2"/>
                <description>
                  <text>Turn on the switch.</text>
                </description>
              </enum>
            </type>
            <default value="off"/>
          </leaf>
          <leaf name="intervlan-proxy-switch">
            <description>
              <text>The ND proxy switch in the different vlan of the interface.</text>
            </description>
            <type name="enumeration">
              <enum name="off">
                <value value="1"/>
                <description>
                  <text>Turn off the switch.</text>
                </description>
              </enum>
              <enum name="on">
                <value value="2"/>
                <description>
                  <text>Turn on the switch.</text>
                </description>
              </enum>
            </type>
            <default value="off"/>
          </leaf>
          <ncd:revise-date date="2015-10-13"/>
        </list>
      </container>
      <container name="ipv6-nd-prefix">
        <description>
          <text>Configure IPv6 ND prefix.</text>
        </description>
        <list name="prefix">
          <description>
            <text>Configure IPv6 ND prefix.</text>
          </description>
          <key value="ifname prefix"/>
          <leaf name="ifname">
            <description>
              <text>The name of the interface.</text>
            </description>
            <type name="ncd:ifname"/>
          </leaf>
          <leaf name="prefix">
            <description>
              <text>IPv6 ND prefix.</text>
            </description>
            <type name="ncd:ipv6-prefix"/>
            <ncd:revise-date date="2015-12-31"/>
          </leaf>
          <container name="lifetime">
            <description>
              <text>Set the lifetimes of the prefix.</text>
            </description>
            <presence value="lifetime"/>
            <leaf name="valid-lifetime">
              <description>
                <text>Set the valid lifetime of the prefix (second).</text>
              </description>
              <type name="uint32">
                <range value="0..4294967295"/>
              </type>
              <units name="second"/>
              <mandatory value="true"/>
            </leaf>
            <leaf name="preferred-lifetime">
              <description>
                <text>Set the preferred lifetime of the prefix(second).</text>
              </description>
              <type name="uint32">
                <range value="0..4294967295"/>
              </type>
              <units name="second"/>
              <mandatory value="true"/>
            </leaf>
          </container>
          <leaf name="no-autoconfig">
            <description>
              <text>Set the flag indicates that can't generate automatically the address using the prefix.</text>
            </description>
            <type name="enumeration">
              <enum name="reset">
                <value value="1"/>
                <description>
                  <text>Reset the flag.</text>
                </description>
              </enum>
              <enum name="set">
                <value value="2"/>
                <description>
                  <text>Set the flag.</text>
                </description>
              </enum>
            </type>
            <default value="reset"/>
          </leaf>
          <leaf name="off-link">
            <description>
              <text>Set the flag indicates that can't use prefix for onlink determination.</text>
            </description>
            <type name="enumeration">
              <enum name="reset">
                <value value="1"/>
                <description>
                  <text>Reset the flag.</text>
                </description>
              </enum>
              <enum name="set">
                <value value="2"/>
                <description>
                  <text>Set the flag.</text>
                </description>
              </enum>
            </type>
            <default value="reset"/>
          </leaf>
        </list>
      </container>
      <container name="static-nd">
        <description>
          <text>Configure static ND cache entry.</text>
        </description>
        <container name="no-supervlan">
          <description>
            <text>Configure static ND cache entry on non-supervlan interface.</text>
          </description>
          <list name="nd-config">
            <description>
              <text>Configure  relevant attributes static ND cache entry.</text>
            </description>
            <key value="ifname next-hop"/>
            <leaf name="ifname">
              <description>
                <text>The name of the interface.</text>
              </description>
              <type name="ncd:ifname"/>
            </leaf>
            <leaf name="next-hop">
              <description>
                <text>The next hop IPv6 address.</text>
              </description>
              <type name="ncd:ipv6-address"/>
            </leaf>
            <leaf name="mac">
              <description>
                <text>MAC(Media Access Control) address.</text>
              </description>
              <type name="ncd:mac-address"/>
              <mandatory value="true"/>
            </leaf>
            <container name="vlan-id">
              <description>
                <text>Configure VLAN ID.</text>
              </description>
              <presence value="vlan-id"/>
              <leaf name="external-vlan-id">
                <description>
                  <text>External VLAN ID.</text>
                </description>
                <type name="uint16">
                  <range value="1..4094"/>
                </type>
                <mandatory value="true"/>
              </leaf>
              <leaf name="internal-vlan-id">
                <description>
                  <text>Internal VLAN ID.</text>
                </description>
                <type name="uint16">
                  <range value="1..4094"/>
                </type>
              </leaf>
            </container>
          </list>
        </container>
        <container name="supervlan">
          <description>
            <text>Configure static ND cache entry on supervlan interface.</text>
          </description>
          <list name="nd-config">
            <description>
              <text>Configure relevant attributes static ND cache entry.</text>
            </description>
            <key value="ifname next-hop"/>
            <leaf name="ifname">
              <description>
                <text>The name of the interface.</text>
              </description>
              <type name="ncd:ifname"/>
            </leaf>
            <leaf name="next-hop">
              <description>
                <text>The next hop IPv6 address.</text>
              </description>
              <type name="ncd:ipv6-address"/>
            </leaf>
            <leaf name="mac">
              <description>
                <text>Mac address.</text>
              </description>
              <type name="ncd:mac-address"/>
              <mandatory value="true"/>
            </leaf>
            <leaf name="subvlan-ifname">
              <description>
                <text>The subvlan interface name.</text>
              </description>
              <type name="ncd:ifname"/>
              <mandatory value="true"/>
            </leaf>
            <container name="vlan-id">
              <description>
                <text>Configure VLAN ID.</text>
              </description>
              <presence value="vlan-id"/>
              <leaf name="external-vlan-id">
                <description>
                  <text>External VLAN ID.</text>
                </description>
                <type name="uint16">
                  <range value="1..4094"/>
                </type>
                <mandatory value="true"/>
              </leaf>
              <leaf name="internal-vlan-id">
                <description>
                  <text>Internal VLAN ID.</text>
                </description>
                <type name="uint16">
                  <range value="1..4094"/>
                </type>
              </leaf>
            </container>
          </list>
        </container>
      </container>
