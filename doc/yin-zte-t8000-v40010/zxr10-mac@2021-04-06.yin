  <container name="state">
    <description>
      <text>MAC state info.</text>
    </description>
    <config value="false"/>
    <container name="mac">
      <description>
        <text>MAC state info.</text>
      </description>
      <container name="mac-vpls-tables">
        <description>
          <text>MAC table of VPLS instance.</text>
        </description>
        <list name="mac-vpls-table">
          <description>
            <text>MAC table of VPLS instance.</text>
          </description>
          <key value="instance-name vlan-id mac-address"/>
          <ncd:select fields="instance-name"/>
          <ncd:select fields="vlan-id"/>
          <ncd:select fields="mac-address"/>
          <ncd:select fields="if-name"/>
          <ncd:select fields="multiflag"/>
          <ncd:select fields="instance-name vlan-id mac-address"/>
          <leaf name="instance-name">
            <description>
              <text>VPLS instance name.</text>
            </description>
            <type name="string">
              <length value="1..32">
                <description>
                  <text>The length of VPLS instance name.</text>
                </description>
              </length>
            </type>
          </leaf>
          <leaf name="vlan-id">
            <description>
              <text>VLAN ID.0 means VLAN is invalid.</text>
            </description>
            <type name="uint16">
              <range value="0..4094">
                <description>
                  <text>VLAN ID must be in this range.</text>
                </description>
              </range>
            </type>
          </leaf>
          <leaf name="mac-address">
            <description>
              <text>MAC address.</text>
            </description>
            <type name="ncd:mac-address"/>
          </leaf>
          <leaf name="if-name">
            <description>
              <text>Interface name.</text>
            </description>
            <type name="ncd:ifname">
              <length value="1..31">
                <description>
                  <text>The length of interface name.</text>
                </description>
              </length>
            </type>
          </leaf>
          <leaf name="multiflag">
            <description>
              <text>The type of MAC address.</text>
            </description>
            <type name="enumeration">
              <enum name="dynamic">
                <value value="1"/>
                <description>
                  <text>Dynamic MAC address.</text>
                </description>
              </enum>
              <enum name="static">
                <value value="2"/>
                <description>
                  <text>Static MAC address.</text>
                </description>
              </enum>
              <enum name="permanent">
                <value value="3"/>
                <description>
                  <text>Permanent MAC address.</text>
                </description>
              </enum>
              <enum name="source-filter">
                <value value="4"/>
                <description>
                  <text>Source-filter MAC address.</text>
                </description>
              </enum>
              <enum name="dst-filter">
                <value value="5"/>
                <description>
                  <text>Destination-filter MAC address.</text>
                </description>
              </enum>
              <enum name="both-filter">
                <value value="6"/>
                <description>
                  <text>Both-filter MAC address.</text>
                </description>
              </enum>
              <enum name="to-static">
                <value value="7"/>
                <description>
                  <text>To-static MAC address.</text>
                </description>
              </enum>
              <enum name="to-permanent">
                <value value="8"/>
                <description>
                  <text>To-permanent MAC address.</text>
                </description>
              </enum>
            </type>
          </leaf>
          <leaf name="evpn-tunnel-id">
            <description>
              <text>EVPN tunnel ID.</text>
            </description>
            <type name="uint32"/>
          </leaf>
          <leaf name="vni">
            <description>
              <text>VNI.</text>
            </description>
            <type name="uint32">
              <range value="1..16777215"/>
            </type>
          </leaf>
          <leaf name="out-label">
            <description>
              <text>MPLS EVPN out-label.</text>
            </description>
            <type name="uint32"/>
          </leaf>
          <leaf name="sid">
            <description>
              <text>SRV6 EVPN SID.</text>
            </description>
            <type name="ncd:ipv6-address"/>
          </leaf>
          <container name="next-hop">
            <description>
              <text>Nexthop IP address.</text>
            </description>
            <choice name="next-hop-ip">
              <description>
                <text>Nexthop IP address.</text>
              </description>
              <case name="ipv4">
                <description>
                  <text>IPv4 address.</text>
                </description>
                <leaf name="ipv4-address">
                  <description>
                    <text>Nexthop IPv4 address.</text>
                  </description>
                  <type name="ncd:ipv4-address"/>
                </leaf>
              </case>
              <case name="ipv6">
                <description>
                  <text>IPv6 address.</text>
                </description>
                <leaf name="ipv6-address">
                  <description>
                    <text>Nexthop IPv6 address.</text>
                  </description>
                  <type name="ncd:ipv6-address"/>
                </leaf>
              </case>
            </choice>
          </container>
          <leaf name="esi">
            <description>
              <text>ESI of MAC address.</text>
            </description>
            <type name="string">
              <length value="1..51">
                <description>
                  <text>The length of ESI.</text>
                </description>
              </length>
            </type>
          </leaf>
        </list>
      </container>
      <container name="mac-tables">
        <description>
          <text>MAC table.</text>
        </description>
        <list name="mac-table">
          <description>
            <text>MAC table.</text>
          </description>
          <key value="vlan-id mac-address"/>
          <ncd:select fields="vlan-id"/>
          <ncd:select fields="mac-address"/>
          <ncd:select fields="multiflag"/>
          <ncd:select fields="if-name"/>
          <ncd:select fields="nickname"/>
          <ncd:select fields="vlan-id mac-address"/>
          <leaf name="vlan-id">
            <description>
              <text>VLAN ID.</text>
            </description>
            <type name="uint16">
              <range value="1..4094">
                <description>
                  <text>VLAN ID must be in this range.</text>
                </description>
              </range>
            </type>
          </leaf>
          <leaf name="mac-address">
            <description>
              <text>MAC address.</text>
            </description>
            <type name="ncd:mac-address"/>
          </leaf>
          <leaf name="if-name">
            <description>
              <text>Interface name.</text>
            </description>
            <type name="ncd:ifname">
              <length value="1..31">
                <description>
                  <text>The length of interface name.</text>
                </description>
              </length>
            </type>
          </leaf>
          <leaf name="multiflag">
            <description>
