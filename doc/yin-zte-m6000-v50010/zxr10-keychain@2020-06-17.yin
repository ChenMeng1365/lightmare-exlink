  <feature name="gm">
    <description>
      <text>The specifiction of GM.</text>
    </description>
  </feature>
  <typedef name="periodicType">
    <type name="enumeration">
      <enum name="daily">
        <value value="1"/>
        <description>
          <text>daily</text>
        </description>
      </enum>
      <enum name="weekly">
        <value value="2"/>
        <description>
          <text>Weekly</text>
        </description>
      </enum>
      <enum name="monthly">
        <value value="3"/>
        <description>
          <text>Monthly</text>
        </description>
      </enum>
      <enum name="yearly">
        <value value="4"/>
        <description>
          <text>Yearly</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Periodic type</text>
    </description>
  </typedef>
  <typedef name="toleranceType">
    <type name="enumeration">
      <enum name="time">
        <value value="1"/>
        <description>
          <text>Receive tolerance value</text>
        </description>
      </enum>
      <enum name="infinite">
        <value value="2"/>
        <description>
          <text>Receive tolerance infinite</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Tolerance type</text>
    </description>
  </typedef>
  <typedef name="lifetimeMode">
    <type name="enumeration">
      <enum name="periodic">
        <value value="2"/>
      </enum>
      <enum name="absolute">
        <value value="1"/>
      </enum>
    </type>
    <description>
      <text>Lifetime mode</text>
    </description>
  </typedef>
